INSERT INTO sentence [{"id":{"d":540,"n":1,"t":4},"speaker":"A","text":"今までに大きなけがをしたことはありますか？"},{"id":{"d":540,"n":2,"t":4},"speaker":"B","text":"足を複雑骨折したことがあります。"},{"id":{"d":540,"n":3,"t":4},"speaker":"A","text":"何歳の頃ですか？"},{"id":{"d":540,"n":4,"t":4},"speaker":"B","text":"10歳の時です。"},{"id":{"d":540,"n":5,"t":4},"speaker":"A","text":"その時は、松葉杖と車椅子のどちらを使いましたか？"},{"id":{"d":540,"n":6,"t":4},"speaker":"B","text":"車椅子でした。"},{"id":{"d":540,"n":7,"t":4},"speaker":"A","text":"それは大変でしたね。学校生活に支障があったのではありませんか？"},{"id":{"d":540,"n":8,"t":4},"speaker":"B","text":"そうですね。母に付き添ってもらったり、友達にいろいろと助けてもらったりしました。"}];
INSERT INTO sentence [{"id":{"d":541,"n":1,"t":4},"speaker":"A","text":"すみません、診察券を忘れてしまいました。"},{"id":{"d":541,"n":2,"t":4},"speaker":"B","text":"なくしてしまったのですか？"},{"id":{"d":541,"n":3,"t":4},"speaker":"A","text":"いいえ、自宅に置いてきてしまいました。"},{"id":{"d":541,"n":4,"t":4},"speaker":"B","text":"それでは、こちらの用紙にお名前と連絡先を記入してください。"},{"id":{"d":541,"n":5,"t":4},"speaker":"A","text":"はい、書きました。"},{"id":{"d":541,"n":6,"t":4},"speaker":"B","text":"健康保険証はお持ちですか？"},{"id":{"d":541,"n":7,"t":4},"speaker":"A","text":"はい、持っています。これです。"},{"id":{"d":541,"n":8,"t":4},"speaker":"B","text":"お預かりしますね。受診履歴を確認しますので、おかけになってお待ちください。"},{"id":{"d":541,"n":9,"t":4},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":542,"n":1,"t":4},"speaker":"A","text":"マッサージをお願いします。"},{"id":{"d":542,"n":2,"t":4},"speaker":"B","text":"承知しました。お時間はどのくらいにしますか？"},{"id":{"d":542,"n":3,"t":4},"speaker":"A","text":"30分間でお願いします。"},{"id":{"d":542,"n":4,"t":4},"speaker":"B","text":"担当者のご希望はありますか？"},{"id":{"d":542,"n":5,"t":4},"speaker":"A","text":"どなたでも構いません。"},{"id":{"d":542,"n":6,"t":4},"speaker":"B","text":"かしこまりました。10分後にご案内できるのですが、よろしいでしょうか？"},{"id":{"d":542,"n":7,"t":4},"speaker":"A","text":"はい、お願いします。"},{"id":{"d":542,"n":8,"t":4},"speaker":"B","text":"準備ができましたらお呼びしますので、ソファに座ってお待ちください。"}];
INSERT INTO sentence [{"id":{"d":543,"n":1,"t":4},"speaker":"A","text":"久本さん、3番へお越しください。"},{"id":{"d":543,"n":2,"t":4},"speaker":"B","text":"はい、久本です。"},{"id":{"d":543,"n":3,"t":4},"speaker":"A","text":"こちらが今回処方されたお薬です。"},{"id":{"d":543,"n":4,"t":4},"speaker":"B","text":"それぞれ、飲み方が違いますか？"},{"id":{"d":543,"n":5,"t":4},"speaker":"A","text":"はい。錠剤は、1日3回、食後に服用してください。"},{"id":{"d":543,"n":6,"t":4},"speaker":"B","text":"粉薬の方はいつ飲めばよいですか？"},{"id":{"d":543,"n":7,"t":4},"speaker":"A","text":"こちらは、1日1回、寝る前に服用してください。"},{"id":{"d":543,"n":8,"t":4},"speaker":"B","text":"分かりました。飲み忘れないように気を付けます。"}];
INSERT INTO sentence [{"id":{"d":544,"n":1,"t":4},"speaker":"A","text":"昨日は欠席でしたが、体調が悪かったのですか？"},{"id":{"d":544,"n":2,"t":4},"speaker":"B","text":"朝から熱があったのです。"},{"id":{"d":544,"n":3,"t":4},"speaker":"A","text":"大変でしたね。"},{"id":{"d":544,"n":4,"t":4},"speaker":"B","text":"それほど高熱ではありませんでしたが、一日中寝ていました。"},{"id":{"d":544,"n":5,"t":4},"speaker":"A","text":"今日は、もう大丈夫なのですか？"},{"id":{"d":544,"n":6,"t":4},"speaker":"B","text":"おかげさまで、平熱に戻ったので大丈夫です。"},{"id":{"d":544,"n":7,"t":4},"speaker":"A","text":"無理はしないでくださいね。"},{"id":{"d":544,"n":8,"t":4},"speaker":"B","text":"お気遣いいただき、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":545,"n":1,"t":4},"speaker":"A","text":"私が生まれた時、ママは大変だったでしょうね。"},{"id":{"d":545,"n":2,"t":4},"speaker":"B","text":"大変でしたが、医者の先生からは安産だと言われましたよ。"},{"id":{"d":545,"n":3,"t":4},"speaker":"A","text":"生まれるまで、どのくらい時間がかかったのですか？"},{"id":{"d":545,"n":4,"t":4},"speaker":"B","text":"3時間くらいです。"},{"id":{"d":545,"n":5,"t":4},"speaker":"A","text":"それでも安産だったのですか？"},{"id":{"d":545,"n":6,"t":4},"speaker":"B","text":"あなたのお兄ちゃんの時は、10時間以上かかりましたよ。"},{"id":{"d":545,"n":7,"t":4},"speaker":"A","text":"それは長いですね。"},{"id":{"d":545,"n":8,"t":4},"speaker":"B","text":"ようやく生まれた時は、心底ほっとしたのを覚えています。"}];
INSERT INTO sentence [{"id":{"d":546,"n":1,"t":4},"speaker":"A","text":"夜のスキンケアをしていますか？"},{"id":{"d":546,"n":2,"t":4},"speaker":"B","text":"はい。簡単なものですが、一応しています。"},{"id":{"d":546,"n":3,"t":4},"speaker":"A","text":"どのようなことをしていますか？"},{"id":{"d":546,"n":4,"t":4},"speaker":"B","text":"お風呂の後、フェイスパックをしたり美容液を使ったりしています。"},{"id":{"d":546,"n":5,"t":4},"speaker":"A","text":"道理で、お肌がきれいです。私はお風呂に入るとすぐに眠くなってしまいます。"},{"id":{"d":546,"n":6,"t":4},"speaker":"B","text":"私も、パックをしながらうとうとすることがよくあります。"},{"id":{"d":546,"n":7,"t":4},"speaker":"A","text":"それでもきちんとしているので偉いですよ。見習いたいです。"},{"id":{"d":546,"n":8,"t":4},"speaker":"B","text":"私もあなたくらい若かった時は、何もしていませんでしたよ。"}];
INSERT INTO sentence [{"id":{"d":547,"n":1,"t":4},"speaker":"A","text":"今度、私の家で一緒にお昼を食べませんか？"},{"id":{"d":547,"n":2,"t":4},"speaker":"B","text":"ぜひ、ご一緒したいです。"},{"id":{"d":547,"n":3,"t":4},"speaker":"A","text":"私が料理を作りますが、何か食べられないものはありますか？"},{"id":{"d":547,"n":4,"t":4},"speaker":"B","text":"牛乳にアレルギーがあるので、避けていただきたいです。"},{"id":{"d":547,"n":5,"t":4},"speaker":"A","text":"そうなのですか。事前に聞いておいてよかったです。"},{"id":{"d":547,"n":6,"t":4},"speaker":"B","text":"ひどいアレルギーではありませんが、できるだけ摂取しないようにしています。"},{"id":{"d":547,"n":7,"t":4},"speaker":"A","text":"外食の時などは、神経を使いますね。"},{"id":{"d":547,"n":8,"t":4},"speaker":"B","text":"栄養成分表示を確認することが習慣になっています。"}];
INSERT INTO sentence [{"id":{"d":548,"n":1,"t":4},"speaker":"A","text":"梅雨に入ってからジメジメしていますね。"},{"id":{"d":548,"n":2,"t":4},"speaker":"B","text":"食中毒に気を付けなければいけない時期ですね。"},{"id":{"d":548,"n":3,"t":4},"speaker":"A","text":"隣の市のお店で買ったお弁当を食べた人が、食中毒になったというニュースを見ました。"},{"id":{"d":548,"n":4,"t":4},"speaker":"B","text":"怖いですね。"},{"id":{"d":548,"n":5,"t":4},"speaker":"A","text":"家庭でも、気を付けたいですね。"},{"id":{"d":548,"n":6,"t":4},"speaker":"B","text":"子どものお弁当を作る時も、神経を使います。"},{"id":{"d":548,"n":7,"t":4},"speaker":"A","text":"私はお弁当箱と保冷剤を保冷バッグに入れて持たせています。"},{"id":{"d":548,"n":8,"t":4},"speaker":"B","text":"それは良い方法ですね。"}];
INSERT INTO sentence [{"id":{"d":549,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":549,"n":2,"t":4},"speaker":"B","text":"右の奥歯が痛みます。"},{"id":{"d":549,"n":3,"t":4},"speaker":"A","text":"上の歯ですか？"},{"id":{"d":549,"n":4,"t":4},"speaker":"B","text":"はい。上の一番奥です。"},{"id":{"d":549,"n":5,"t":4},"speaker":"A","text":"痛みはいつからありますか？"},{"id":{"d":549,"n":6,"t":4},"speaker":"B","text":"3日前からです。"},{"id":{"d":549,"n":7,"t":4},"speaker":"A","text":"虫歯の可能性があるので、レントゲン写真を撮りましょう。"},{"id":{"d":549,"n":8,"t":4},"speaker":"B","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":550,"n":1,"t":4},"speaker":"A","text":"4階に行きましょう。"},{"id":{"d":550,"n":2,"t":4},"speaker":"B","text":"あそこにエレベーターがあります。"},{"id":{"d":550,"n":3,"t":4},"speaker":"A","text":"私は階段を使います。"},{"id":{"d":550,"n":4,"t":4},"speaker":"B","text":"健康のためですか？"},{"id":{"d":550,"n":5,"t":4},"speaker":"A","text":"はい。なるべく階段を利用するようにしています。"},{"id":{"d":550,"n":6,"t":4},"speaker":"B","text":"それは素晴らしいですね。"},{"id":{"d":550,"n":7,"t":4},"speaker":"A","text":"他に運動する習慣がないのです。どうか私のことは気にしないで、エレベーターで行ってください。"},{"id":{"d":550,"n":8,"t":4},"speaker":"B","text":"それでは、4階で落ち合いましょう。"}];
INSERT INTO sentence [{"id":{"d":551,"n":1,"t":4},"speaker":"A","text":"断食が流行しているそうですよ。"},{"id":{"d":551,"n":2,"t":4},"speaker":"B","text":"本当ですか？"},{"id":{"d":551,"n":3,"t":4},"speaker":"A","text":"はい。ダイエットの方法として注目されているようです。"},{"id":{"d":551,"n":4,"t":4},"speaker":"B","text":"私は食べることが大好きなので、断食は無理です。"},{"id":{"d":551,"n":5,"t":4},"speaker":"A","text":"私の友人は、週に1日だけ断食するダイエットをしています。"},{"id":{"d":551,"n":6,"t":4},"speaker":"B","text":"断食する曜日を決めるというイメージでしょうか？"},{"id":{"d":551,"n":7,"t":4},"speaker":"A","text":"そうだと思います。友人は、仕事が休みの土曜日に断食しているそうです。"},{"id":{"d":551,"n":8,"t":4},"speaker":"B","text":"週に1日でも、私にはできそうにありません。"}];
INSERT INTO sentence [{"id":{"d":552,"n":1,"t":4},"speaker":"A","text":"保険の見直しをすることにしました。"},{"id":{"d":552,"n":2,"t":4},"speaker":"B","text":"何かきっかけがあったのですか？"},{"id":{"d":552,"n":3,"t":4},"speaker":"A","text":"いとこから、保険の見直しをしたら保険料が安くなったと聞いたのです。"},{"id":{"d":552,"n":4,"t":4},"speaker":"B","text":"今はどのような保険に入っているのですか？"},{"id":{"d":552,"n":5,"t":4},"speaker":"A","text":"医療保険と個人年金保険ですが、月々の保険料が負担になっています。"},{"id":{"d":552,"n":6,"t":4},"speaker":"B","text":"私は妻に任せ切りで、保険の内容を把握していません。"},{"id":{"d":552,"n":7,"t":4},"speaker":"A","text":"補償内容をしっかり調べたら、見直しの必要があると感じるかもしれませんよ。"},{"id":{"d":552,"n":8,"t":4},"speaker":"B","text":"そうですね。妻とも話し合ってみます。"}];
INSERT INTO sentence [{"id":{"d":553,"n":1,"t":4},"speaker":"A","text":"明日は日曜日ですが、部活はありますか？"},{"id":{"d":553,"n":2,"t":4},"speaker":"B","text":"午後に練習があります。"},{"id":{"d":553,"n":3,"t":4},"speaker":"A","text":"そうですか。暑くなりそうなので、熱中症が心配です。"},{"id":{"d":553,"n":4,"t":4},"speaker":"B","text":"小まめに休憩するように、顧問の先生からも指導がありました。"},{"id":{"d":553,"n":5,"t":4},"speaker":"A","text":"休憩の時は、水分補給もしてくださいね。"},{"id":{"d":553,"n":6,"t":4},"speaker":"B","text":"はい。水筒を忘れずに持って行きます。"},{"id":{"d":553,"n":7,"t":4},"speaker":"A","text":"足りなくなるといけないので、明日は大きめの水筒を準備しましょう。"},{"id":{"d":553,"n":8,"t":4},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":554,"n":1,"t":4},"speaker":"A","text":"日本人の平均寿命は長いですね。"},{"id":{"d":554,"n":2,"t":4},"speaker":"B","text":"そうなのですね。何歳くらいなのですか？"},{"id":{"d":554,"n":3,"t":4},"speaker":"A","text":"男性が80歳で、女性が87歳くらいだったと思います。"},{"id":{"d":554,"n":4,"t":4},"speaker":"B","text":"男女でかなり差がありますね。"},{"id":{"d":554,"n":5,"t":4},"speaker":"A","text":"どうやら、女性の方が長寿なのは世界的に見られる傾向らしいです。"},{"id":{"d":554,"n":6,"t":4},"speaker":"B","text":"日本に限ったことではないのですね。"},{"id":{"d":554,"n":7,"t":4},"speaker":"A","text":"はい。できれば健康で長生きしたいですね。"},{"id":{"d":554,"n":8,"t":4},"speaker":"B","text":"世界共通の願いでしょうね。"}];
INSERT INTO sentence [{"id":{"d":555,"n":1,"t":4},"speaker":"A","text":"運転免許の更新のお知らせが届きましたよ。"},{"id":{"d":555,"n":2,"t":4},"speaker":"B","text":"もう更新はしないで、免許証を返納しようと考えています。"},{"id":{"d":555,"n":3,"t":4},"speaker":"A","text":"運転に不安があるのですか？"},{"id":{"d":555,"n":4,"t":4},"speaker":"B","text":"まだ不安はありませんが、不安を感じる前に返納すべきだと思うのです。"},{"id":{"d":555,"n":5,"t":4},"speaker":"A","text":"免許を返納したら、今乗っている車はどうしますか？"},{"id":{"d":555,"n":6,"t":4},"speaker":"B","text":"家族で他に運転する人がいないので、処分することになります。"},{"id":{"d":555,"n":7,"t":4},"speaker":"A","text":"長く乗っている車なので、寂しいです。"},{"id":{"d":555,"n":8,"t":4},"speaker":"B","text":"私も同じ気持ちですが、仕方ありませんね。"}];
INSERT INTO sentence [{"id":{"d":556,"n":1,"t":4},"speaker":"A","text":"ここ数年は、健康寿命という言葉を耳にしますね。"},{"id":{"d":556,"n":2,"t":4},"speaker":"B","text":"そうですね。平均寿命とどう違うのでしょうか？"},{"id":{"d":556,"n":3,"t":4},"speaker":"A","text":"健康寿命とは、健康的に生活できる期間を指すようです。"},{"id":{"d":556,"n":4,"t":4},"speaker":"B","text":"なるほど。健康寿命が長い方が望ましいですね。"},{"id":{"d":556,"n":5,"t":4},"speaker":"A","text":"日本人は、平均寿命だけではなく健康寿命も長いようですよ。"},{"id":{"d":556,"n":6,"t":4},"speaker":"B","text":"それは誇らしいですね。"},{"id":{"d":556,"n":7,"t":4},"speaker":"A","text":"私たちも、健康を保ちながら長生きしたいですね。"},{"id":{"d":556,"n":8,"t":4},"speaker":"B","text":"同感です。"}];
INSERT INTO sentence [{"id":{"d":557,"n":1,"t":4},"speaker":"A","text":"将来はどのような職業に就きたいですか？"},{"id":{"d":557,"n":2,"t":4},"speaker":"B","text":"介護に関わる仕事がしたいです。"},{"id":{"d":557,"n":3,"t":4},"speaker":"A","text":"立派な目標ですね。何かきっかけがあったのですか？"},{"id":{"d":557,"n":4,"t":4},"speaker":"B","text":"祖父母が介護サービスを受けていて、スタッフの方に憧れるようになったのです。"},{"id":{"d":557,"n":5,"t":4},"speaker":"A","text":"どのようなところに憧れているのですか？"},{"id":{"d":557,"n":6,"t":4},"speaker":"B","text":"常に穏やかで優しくて、祖父母のために一生懸命取り組んでくれるところです。"},{"id":{"d":557,"n":7,"t":4},"speaker":"A","text":"素晴らしい方々ですね。"},{"id":{"d":557,"n":8,"t":4},"speaker":"B","text":"自分もそうなれるように努力したいです。"}];
INSERT INTO sentence [{"id":{"d":558,"n":1,"t":4},"speaker":"A","text":"おばあちゃん、今日のデイケアはどうでしたか？"},{"id":{"d":558,"n":2,"t":4},"speaker":"B","text":"楽しく過ごせましたよ。"},{"id":{"d":558,"n":3,"t":4},"speaker":"A","text":"今日は何をしたのですか？"},{"id":{"d":558,"n":4,"t":4},"speaker":"B","text":"体操をして、その後塗り絵をしました。"},{"id":{"d":558,"n":5,"t":4},"speaker":"A","text":"どのような塗り絵ですか？"},{"id":{"d":558,"n":6,"t":4},"speaker":"B","text":"持ち帰ってきたので、見せましょうか？"},{"id":{"d":558,"n":7,"t":4},"speaker":"A","text":"ぜひ見せてください。"},{"id":{"d":558,"n":8,"t":4},"speaker":"B","text":"これです。"},{"id":{"d":558,"n":9,"t":4},"speaker":"A","text":"きれいなお花ですね。上手に塗れていますよ。"}];
INSERT INTO sentence [{"id":{"d":559,"n":1,"t":4},"speaker":"A","text":"お風呂に入浴剤を入れてよいですか？"},{"id":{"d":559,"n":2,"t":4},"speaker":"B","text":"よいですよ。何か効果があるのですか？"},{"id":{"d":559,"n":3,"t":4},"speaker":"A","text":"リラックス効果があると書いてあります。"},{"id":{"d":559,"n":4,"t":4},"speaker":"B","text":"それはいいですね。香りはありますか？"},{"id":{"d":559,"n":5,"t":4},"speaker":"A","text":"ラベンダーの香りです。"},{"id":{"d":559,"n":6,"t":4},"speaker":"B","text":"私の大好きな香りです。お風呂に入るのが楽しみになりました。"},{"id":{"d":559,"n":7,"t":4},"speaker":"A","text":"すみませんが、先に入ってもよいですか？"},{"id":{"d":559,"n":8,"t":4},"speaker":"B","text":"もちろん。お先にどうぞ。"}];
INSERT INTO sentence [{"id":{"d":560,"n":1,"t":4},"speaker":"A","text":"パパ、昨夜はいびきがひどかったですよ。"},{"id":{"d":560,"n":2,"t":4},"speaker":"B","text":"隣の部屋で寝ているのに、どうして分かったのですか？"},{"id":{"d":560,"n":3,"t":4},"speaker":"A","text":"あまりに大きないびきなので、隣の部屋にも聞こえてきたのです。"},{"id":{"d":560,"n":4,"t":4},"speaker":"B","text":"それほどひどかったのですか？"},{"id":{"d":560,"n":5,"t":4},"speaker":"A","text":"夜中に何度も目が覚めてしまいました。"},{"id":{"d":560,"n":6,"t":4},"speaker":"B","text":"それは悪かったですね。"},{"id":{"d":560,"n":7,"t":4},"speaker":"A","text":"眠っている間のことなので、パパにもどうしようもありませんよね。"},{"id":{"d":560,"n":8,"t":4},"speaker":"B","text":"そうなのです。困ったものです。"}];
INSERT INTO sentence [{"id":{"d":561,"n":1,"t":4},"speaker":"A","text":"3日前に、足をくじいてしまいました。"},{"id":{"d":561,"n":2,"t":4},"speaker":"B","text":"どちらの足ですか？"},{"id":{"d":561,"n":3,"t":4},"speaker":"A","text":"右足です。"},{"id":{"d":561,"n":4,"t":4},"speaker":"B","text":"どうしてくじいてしまったのですか？"},{"id":{"d":561,"n":5,"t":4},"speaker":"A","text":"段差のある所で、足を踏み外してしまいました。"},{"id":{"d":561,"n":6,"t":4},"speaker":"B","text":"今も少し腫れているようですね。"},{"id":{"d":561,"n":7,"t":4},"speaker":"A","text":"3日前はもっと腫れていたのですよ。これでもだいぶ良くなりました。"},{"id":{"d":561,"n":8,"t":4},"speaker":"B","text":"時間とともに良くなっているのですね。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":562,"n":1,"t":4},"speaker":"A","text":"私は毎日、1万歩以上歩いています。"},{"id":{"d":562,"n":2,"t":4},"speaker":"B","text":"歩数を計っているのですか？"},{"id":{"d":562,"n":3,"t":4},"speaker":"A","text":"歩数を計測するスマートフォンのアプリケーションを使っています。"},{"id":{"d":562,"n":4,"t":4},"speaker":"B","text":"1万歩以上歩くには、かなり時間がかかるのではありませんか？"},{"id":{"d":562,"n":5,"t":4},"speaker":"A","text":"そうですね。ただ、私は仕事中もよく歩くので、気が付くとたくさん歩いているのです。"},{"id":{"d":562,"n":6,"t":4},"speaker":"B","text":"外回りの仕事なのですか？"},{"id":{"d":562,"n":7,"t":4},"speaker":"A","text":"いいえ、看護師をしています。"},{"id":{"d":562,"n":8,"t":4},"speaker":"B","text":"それはたくさん歩くでしょうね。"}];
INSERT INTO sentence [{"id":{"d":563,"n":1,"t":4},"speaker":"A","text":"パパ、コロの具合が悪いようです。"},{"id":{"d":563,"n":2,"t":4},"speaker":"B","text":"どうしてそう思うのですか？"},{"id":{"d":563,"n":3,"t":4},"speaker":"A","text":"朝からずっと横になっていて、散歩にも行きたがらないためです。"},{"id":{"d":563,"n":4,"t":4},"speaker":"B","text":"餌は食べましたか？"},{"id":{"d":563,"n":5,"t":4},"speaker":"A","text":"あげましたが、ほとんど食べませんでした。"},{"id":{"d":563,"n":6,"t":4},"speaker":"B","text":"それは心配ですね。昨日はいつも通りでしたよね。"},{"id":{"d":563,"n":7,"t":4},"speaker":"A","text":"はい。昨日は餌も全部食べていました。"},{"id":{"d":563,"n":8,"t":4},"speaker":"B","text":"心配ですね。様子を見に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":564,"n":1,"t":4},"speaker":"A","text":"少し前から、つわりがひどいです。"},{"id":{"d":564,"n":2,"t":4},"speaker":"B","text":"何か食べると、気分が悪くなるのですか？"},{"id":{"d":564,"n":3,"t":4},"speaker":"A","text":"はい。食べ物も飲み物も受け付けません。"},{"id":{"d":564,"n":4,"t":4},"speaker":"B","text":"私もつわりはつらかったので、つらさは分かります。"},{"id":{"d":564,"n":5,"t":4},"speaker":"A","text":"覚悟はしていましたが、これほどまでに苦しいとは思いませんでした。"},{"id":{"d":564,"n":6,"t":4},"speaker":"B","text":"無理をせず、体をいたわってくださいね。"},{"id":{"d":564,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。産まれてくる赤ちゃんのためにも頑張ります。"},{"id":{"d":564,"n":8,"t":4},"speaker":"B","text":"早くつわりの時期が終わるといいですね。"}];
INSERT INTO sentence [{"id":{"d":565,"n":1,"t":4},"speaker":"A","text":"お茶をどうぞ。"},{"id":{"d":565,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。すみませんが、猫舌なので後ほどいただきます。"},{"id":{"d":565,"n":3,"t":4},"speaker":"A","text":"そうでしたか。こちらこそすみません。"},{"id":{"d":565,"n":4,"t":4},"speaker":"B","text":"とんでもありません。お恥ずかしいです。"},{"id":{"d":565,"n":5,"t":4},"speaker":"A","text":"体質のようなものですから、仕方ないことですよ。"},{"id":{"d":565,"n":6,"t":4},"speaker":"B","text":"大人になったら平気になるのではないかと期待していたのですが、変わりませんでした。"},{"id":{"d":565,"n":7,"t":4},"speaker":"A","text":"私の母も猫舌なので分かりますよ。"},{"id":{"d":565,"n":8,"t":4},"speaker":"B","text":"大人で猫舌の人も、少なくないのですね。"}];
INSERT INTO sentence [{"id":{"d":566,"n":1,"t":4},"speaker":"A","text":"このサウナはものすごく暑いですね。"},{"id":{"d":566,"n":2,"t":4},"speaker":"B","text":"そうですね。私がいつも行くサウナより、だいぶ暑いです。"},{"id":{"d":566,"n":3,"t":4},"speaker":"A","text":"入ってからどのくらい経ったでしょうか？"},{"id":{"d":566,"n":4,"t":4},"speaker":"B","text":"まだ5分くらいです。"},{"id":{"d":566,"n":5,"t":4},"speaker":"A","text":"そろそろ限界が近づいてきました。"},{"id":{"d":566,"n":6,"t":4},"speaker":"B","text":"私も、かなり苦しいです。"},{"id":{"d":566,"n":7,"t":4},"speaker":"A","text":"いったん出て、水風呂に入りましょうか？"},{"id":{"d":566,"n":8,"t":4},"speaker":"B","text":"そうですね。我慢できなくなってきました。"}];
INSERT INTO sentence [{"id":{"d":567,"n":1,"t":4},"speaker":"A","text":"バランスボールを買いました。"},{"id":{"d":567,"n":2,"t":4},"speaker":"B","text":"どのようにして使うのですか？"},{"id":{"d":567,"n":3,"t":4},"speaker":"A","text":"ただ座るだけでもいいですし、ボールを使ったエクササイズがたくさんあります。"},{"id":{"d":567,"n":4,"t":4},"speaker":"B","text":"楽しそうですね。"},{"id":{"d":567,"n":5,"t":4},"speaker":"A","text":"楽しいです。それに、体幹が鍛えられるといった効果が期待できます。"},{"id":{"d":567,"n":6,"t":4},"speaker":"B","text":"座っても、破裂したりしないでしょうか？"},{"id":{"d":567,"n":7,"t":4},"speaker":"A","text":"丈夫に作られているので心配ありませんよ。"},{"id":{"d":567,"n":8,"t":4},"speaker":"B","text":"試しに座ってみてもよいですか？"},{"id":{"d":567,"n":9,"t":4},"speaker":"A","text":"どうぞ、どうぞ。"}];
INSERT INTO sentence [{"id":{"d":568,"n":1,"t":4},"speaker":"A","text":"ダイエットのために運動を始めました。"},{"id":{"d":568,"n":2,"t":4},"speaker":"B","text":"どのような運動ですか？"},{"id":{"d":568,"n":3,"t":4},"speaker":"A","text":"水中ウォーキングです。"},{"id":{"d":568,"n":4,"t":4},"speaker":"B","text":"プールで歩いているのですか？"},{"id":{"d":568,"n":5,"t":4},"speaker":"A","text":"はい。昨日は1時間以上歩きました。"},{"id":{"d":568,"n":6,"t":4},"speaker":"B","text":"相当な運動量になるでしょうね。"},{"id":{"d":568,"n":7,"t":4},"speaker":"A","text":"それを期待しています。"},{"id":{"d":568,"n":8,"t":4},"speaker":"B","text":"ダイエットの効果は表れていますか？"},{"id":{"d":568,"n":9,"t":4},"speaker":"A","text":"はい。何とか1キログラムほど体重が減りました。"}];
INSERT INTO sentence [{"id":{"d":569,"n":1,"t":4},"speaker":"A","text":"ランニングマシンはどれがお薦めですか？"},{"id":{"d":569,"n":2,"t":4},"speaker":"B","text":"今、一番売れているのはこちらの商品です。"},{"id":{"d":569,"n":3,"t":4},"speaker":"A","text":"何か特徴があるのですか？"},{"id":{"d":569,"n":4,"t":4},"speaker":"B","text":"使用時の音がとても小さいです。"},{"id":{"d":569,"n":5,"t":4},"speaker":"A","text":"確かに、静かですね。"},{"id":{"d":569,"n":6,"t":4},"speaker":"B","text":"また、使わない時は非常にコンパクトに折りたためます。"},{"id":{"d":569,"n":7,"t":4},"speaker":"A","text":"収納に便利ですね。"},{"id":{"d":569,"n":8,"t":4},"speaker":"B","text":"性能も充実していますので、お薦めの商品ですよ。"},{"id":{"d":569,"n":9,"t":4},"speaker":"A","text":"試しに使うことはできますか？"},{"id":{"d":569,"n":10,"t":4},"speaker":"B","text":"はい、可能です。ぜひ、使ってみてください。"}];
INSERT INTO sentence [{"id":{"d":570,"n":1,"t":4},"speaker":"A","text":"足がつってしまいました。"},{"id":{"d":570,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":570,"n":3,"t":4},"speaker":"A","text":"痛いです。"},{"id":{"d":570,"n":4,"t":4},"speaker":"B","text":"どうしたらよいのでしょうか？"},{"id":{"d":570,"n":5,"t":4},"speaker":"A","text":"マッサージをしてみます。"},{"id":{"d":570,"n":6,"t":4},"speaker":"B","text":"それが良さそうですね。楽な姿勢でマッサージしてください。"},{"id":{"d":570,"n":7,"t":4},"speaker":"A","text":"私はなぜかよく足がつるのです。"},{"id":{"d":570,"n":8,"t":4},"speaker":"B","text":"困りものですね。"}];
INSERT INTO sentence [{"id":{"d":571,"n":1,"t":4},"speaker":"A","text":"いつもと声が違いますね。"},{"id":{"d":571,"n":2,"t":4},"speaker":"B","text":"昨日、カラオケで歌い過ぎて、声がかれてしまいました。"},{"id":{"d":571,"n":3,"t":4},"speaker":"A","text":"それほどたくさん歌ったのですか？"},{"id":{"d":571,"n":4,"t":4},"speaker":"B","text":"友達と2人で3時間ずっと歌っていました。"},{"id":{"d":571,"n":5,"t":4},"speaker":"A","text":"それは喉に負担がかかるでしょうね。"},{"id":{"d":571,"n":6,"t":4},"speaker":"B","text":"久しぶりだったせいか、とても盛り上がり、長時間の歌唱になってしまいました。"},{"id":{"d":571,"n":7,"t":4},"speaker":"A","text":"楽しかったのなら、何よりです。のど飴がありますが、いかがですか？"},{"id":{"d":571,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。いただきます。"}];
INSERT INTO sentence [{"id":{"d":572,"n":1,"t":4},"speaker":"A","text":"赤ちゃんには離乳食を1日に何回あげていますか？"},{"id":{"d":572,"n":2,"t":4},"speaker":"B","text":"1日3回です。"},{"id":{"d":572,"n":3,"t":4},"speaker":"A","text":"どのようなものをあげていますか？"},{"id":{"d":572,"n":4,"t":4},"speaker":"B","text":"おかゆとスープと果物をあげることが多いです。"},{"id":{"d":572,"n":5,"t":4},"speaker":"A","text":"赤ちゃんはよく食べてくれますか？"},{"id":{"d":572,"n":6,"t":4},"speaker":"B","text":"果物は喜んで食べますが、スープの中の野菜は食べないことがあります。"},{"id":{"d":572,"n":7,"t":4},"speaker":"A","text":"離乳食を作るのは大変だと思うので、時々市販品も活用するといいですよ。"},{"id":{"d":572,"n":8,"t":4},"speaker":"B","text":"はい。今でも時々利用しています。"}];
INSERT INTO sentence [{"id":{"d":573,"n":1,"t":4},"speaker":"A","text":"赤ちゃんのお尻の皮膚が赤くなってしまいました。"},{"id":{"d":573,"n":2,"t":4},"speaker":"B","text":"少し見せてください。"},{"id":{"d":573,"n":3,"t":4},"speaker":"A","text":"ここです。だいぶ赤くて、痛そうです。"},{"id":{"d":573,"n":4,"t":4},"speaker":"B","text":"これは恐らくオムツかぶれです。"},{"id":{"d":573,"n":5,"t":4},"speaker":"A","text":"どうすればよいでしょうか？"},{"id":{"d":573,"n":6,"t":4},"speaker":"B","text":"小児科で診てもらえば、きっと塗り薬をもらえますよ。"},{"id":{"d":573,"n":7,"t":4},"speaker":"A","text":"早速、明日行ってきます。"},{"id":{"d":573,"n":8,"t":4},"speaker":"B","text":"早く治るといいですね。"}];
INSERT INTO sentence [{"id":{"d":574,"n":1,"t":4},"speaker":"A","text":"くしゃみが止まりません。"},{"id":{"d":574,"n":2,"t":4},"speaker":"B","text":"風邪ですか？"},{"id":{"d":574,"n":3,"t":4},"speaker":"A","text":"いいえ。アレルギーだと思います。"},{"id":{"d":574,"n":4,"t":4},"speaker":"B","text":"何かアレルギーがあるのですか？"},{"id":{"d":574,"n":5,"t":4},"speaker":"A","text":"猫アレルギーなのです。"},{"id":{"d":574,"n":6,"t":4},"speaker":"B","text":"それでは、うちの猫が原因ですね。すみません。別の部屋に連れて行きます。"},{"id":{"d":574,"n":7,"t":4},"speaker":"A","text":"こちらこそ、すみません。"},{"id":{"d":574,"n":8,"t":4},"speaker":"B","text":"いえいえ。早く症状が治まるといいですね。"}];
INSERT INTO sentence [{"id":{"d":575,"n":1,"t":4},"speaker":"A","text":"私の若い頃の写真が出てきました。"},{"id":{"d":575,"n":2,"t":4},"speaker":"B","text":"見せてください。あら、今と随分違いますね。"},{"id":{"d":575,"n":3,"t":4},"speaker":"A","text":"この頃は、我ながら不健康な生活をしていたので、元気がなさそうです。"},{"id":{"d":575,"n":4,"t":4},"speaker":"B","text":"今の方が若々しくて元気に見えますよ。"},{"id":{"d":575,"n":5,"t":4},"speaker":"A","text":"今は健康に気を付けていますし、体を鍛えていますからね。"},{"id":{"d":575,"n":6,"t":4},"speaker":"B","text":"生活を見直したのですね。"},{"id":{"d":575,"n":7,"t":4},"speaker":"A","text":"はい。このままでは良くないと思って、健康に気を配るようになりました。"},{"id":{"d":575,"n":8,"t":4},"speaker":"B","text":"私も見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":576,"n":1,"t":4},"speaker":"A","text":"お土産にシュークリームをいただいたので、一緒に食べませんか？"},{"id":{"d":576,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。でも、ダイエット中なので、やめておきます。"},{"id":{"d":576,"n":3,"t":4},"speaker":"A","text":"あなたにはダイエットの必要はないと思いますよ。"},{"id":{"d":576,"n":4,"t":4},"speaker":"B","text":"最近、少し体形が気になっているのです。"},{"id":{"d":576,"n":5,"t":4},"speaker":"A","text":"あなたは標準的な体形だと思います。"},{"id":{"d":576,"n":6,"t":4},"speaker":"B","text":"そうでしょうか？鏡で自分の姿を見ると、あれこれ気になってしまいます。"},{"id":{"d":576,"n":7,"t":4},"speaker":"A","text":"気持ちは分かりますが、あなたがダイエットをすべきだとは全然思いませんよ。"},{"id":{"d":576,"n":8,"t":4},"speaker":"B","text":"私のクラスにはスリムな子が多いので、標準的な体形でも、太っているように感じてしまうのかもしれません。"},{"id":{"d":576,"n":9,"t":4},"speaker":"A","text":"健康であることが一番大事です。あまり気にし過ぎないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":577,"n":1,"t":4},"speaker":"A","text":"今日はどのようにしますか？"},{"id":{"d":577,"n":2,"t":4},"speaker":"B","text":"カットとカラーをお願いします。"},{"id":{"d":577,"n":3,"t":4},"speaker":"A","text":"カラーをすると髪が傷みやすいので、ご一緒にトリートメントはいかがですか？"},{"id":{"d":577,"n":4,"t":4},"speaker":"B","text":"やはりカラーは、髪が傷むのですね。"},{"id":{"d":577,"n":5,"t":4},"speaker":"A","text":"そうですね。薬剤を使いますので、髪へのダメージは避けられません。"},{"id":{"d":577,"n":6,"t":4},"speaker":"B","text":"トリートメントをすると、時間がかかりますか？"},{"id":{"d":577,"n":7,"t":4},"speaker":"A","text":"洗い流さないタイプのものを使いますので、時間はそれほどかかりません。"},{"id":{"d":577,"n":8,"t":4},"speaker":"B","text":"では、トリートメントもお願いします。"}];
INSERT INTO sentence [{"id":{"d":578,"n":1,"t":4},"speaker":"A","text":"こちらのエアコンはお薦めですよ。"},{"id":{"d":578,"n":2,"t":4},"speaker":"B","text":"どのような機能があるのですか？"},{"id":{"d":578,"n":3,"t":4},"speaker":"A","text":"フィルターが花粉を通さないので、お部屋に花粉が入ってきません。"},{"id":{"d":578,"n":4,"t":4},"speaker":"B","text":"私は花粉症なので、助かります。"},{"id":{"d":578,"n":5,"t":4},"speaker":"A","text":"さらにフィルターには、ウイルスの侵入を防ぐ効果もあります。"},{"id":{"d":578,"n":6,"t":4},"speaker":"B","text":"魅力的ですね。フィルターの掃除は大変ではありませんか？"},{"id":{"d":578,"n":7,"t":4},"speaker":"A","text":"自動で掃除される機能が付いていますので、ご安心ください。"},{"id":{"d":578,"n":8,"t":4},"speaker":"B","text":"いいですね。そのエアコンにします。"}];
INSERT INTO sentence [{"id":{"d":579,"n":1,"t":4},"speaker":"A","text":"デッドボールです。"},{"id":{"d":579,"n":2,"t":4},"speaker":"B","text":"打者が倒れてしまいましたね。"},{"id":{"d":579,"n":3,"t":4},"speaker":"A","text":"どうやら、ボールが頭に当たったようです。"},{"id":{"d":579,"n":4,"t":4},"speaker":"B","text":"大丈夫でしょうか？"},{"id":{"d":579,"n":5,"t":4},"speaker":"A","text":"倒れたまま、立ち上がりませんね。"},{"id":{"d":579,"n":6,"t":4},"speaker":"B","text":"担架が来ました。このまま病院へ運ぶようです。"},{"id":{"d":579,"n":7,"t":4},"speaker":"A","text":"今シーズンに活躍している選手なので、大事に至らないといいですね。"},{"id":{"d":579,"n":8,"t":4},"speaker":"B","text":"私が一番応援している選手なので、とても心配です。"}];
INSERT INTO sentence [{"id":{"d":580,"n":1,"t":4},"speaker":"A","text":"明日はいよいよ退院ですね。"},{"id":{"d":580,"n":2,"t":4},"speaker":"B","text":"お世話になりました。先生、退院後は、通常の生活をしても大丈夫でしょうか？"},{"id":{"d":580,"n":3,"t":4},"speaker":"A","text":"しばらくの間は運動を控えて、安静に過ごしてください。"},{"id":{"d":580,"n":4,"t":4},"speaker":"B","text":"分かりました。仕事にはいつ頃復帰できるでしょうか？"},{"id":{"d":580,"n":5,"t":4},"speaker":"A","text":"今月いっぱいはお休みしておいた方が無難ですよ。"},{"id":{"d":580,"n":6,"t":4},"speaker":"B","text":"仕事仲間に迷惑をかけているので、なるべく早く職場復帰したいです。"},{"id":{"d":580,"n":7,"t":4},"speaker":"A","text":"お気持ちは分かりますが、焦りは禁物ですよ。体がしっかり回復してからにしましょう。"},{"id":{"d":580,"n":8,"t":4},"speaker":"B","text":"わかりました。"}];
INSERT INTO sentence [{"id":{"d":581,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":581,"n":2,"t":4},"speaker":"B","text":"バイクで走っていて、転んでしまいました。"},{"id":{"d":581,"n":3,"t":4},"speaker":"A","text":"一番痛いところはどこですか？"},{"id":{"d":581,"n":4,"t":4},"speaker":"B","text":"右の足首です。"},{"id":{"d":581,"n":5,"t":4},"speaker":"A","text":"だいぶ腫れていますね。レントゲン写真を撮りましょう。"},{"id":{"d":581,"n":6,"t":4},"speaker":"B","text":"骨折しているでしょうか？"},{"id":{"d":581,"n":7,"t":4},"speaker":"A","text":"レントゲン撮影の結果を見ないと何とも言えませんが、骨折している可能性は低いと思いますよ。"},{"id":{"d":581,"n":8,"t":4},"speaker":"B","text":"そうですか。少し安心しました。"}];
INSERT INTO sentence [{"id":{"d":582,"n":1,"t":4},"speaker":"A","text":"先月は、ぎっくり腰になって大変でした。"},{"id":{"d":582,"n":2,"t":4},"speaker":"B","text":"ぎっくり腰はつらいと聞きます。"},{"id":{"d":582,"n":3,"t":4},"speaker":"A","text":"本当につらかったです。全く動けなくなりました。"},{"id":{"d":582,"n":4,"t":4},"speaker":"B","text":"寝込んでいたのですか？"},{"id":{"d":582,"n":5,"t":4},"speaker":"A","text":"はい。2日ほど寝込みました。"},{"id":{"d":582,"n":6,"t":4},"speaker":"B","text":"今は完全に回復しましたか？"},{"id":{"d":582,"n":7,"t":4},"speaker":"A","text":"ほぼ元通りですが、念のためにコルセットをしています。"},{"id":{"d":582,"n":8,"t":4},"speaker":"B","text":"ぎっくり腰は繰り返すことが多いそうなので、心配ですね。"}];
INSERT INTO sentence [{"id":{"d":583,"n":1,"t":4},"speaker":"A","text":"今年に入ってから禁煙しています。"},{"id":{"d":583,"n":2,"t":4},"speaker":"B","text":"あれほどヘビースモーカーだったのに、どういう風の吹き回しですか？"},{"id":{"d":583,"n":3,"t":4},"speaker":"A","text":"会社内が完全に禁煙になったのです。"},{"id":{"d":583,"n":4,"t":4},"speaker":"B","text":"そういう会社が増えていますよね。"},{"id":{"d":583,"n":5,"t":4},"speaker":"A","text":"時代の流れには逆らえません。"},{"id":{"d":583,"n":6,"t":4},"speaker":"B","text":"喫煙者も減りましたよね。"},{"id":{"d":583,"n":7,"t":4},"speaker":"A","text":"私の会社でも、喫煙者はごく少数になりました。"},{"id":{"d":583,"n":8,"t":4},"speaker":"B","text":"きっかけはどうあれ、禁煙は健康に良いのでこのまま続くといいですね。"}];
INSERT INTO sentence [{"id":{"d":584,"n":1,"t":4},"speaker":"A","text":"お風呂上がりにお茶を飲むなんて、珍しいですね。"},{"id":{"d":584,"n":2,"t":4},"speaker":"B","text":"いつもはビールですが、今日は休肝日なのです。"},{"id":{"d":584,"n":3,"t":4},"speaker":"A","text":"休肝日を決めているのですか？"},{"id":{"d":584,"n":4,"t":4},"speaker":"B","text":"はい。毎週火曜日はアルコールを取らないことにしています。"},{"id":{"d":584,"n":5,"t":4},"speaker":"A","text":"お風呂上がりのビールは習慣になっているようですし、つらくありませんか？"},{"id":{"d":584,"n":6,"t":4},"speaker":"B","text":"週に1日だけなので、思いのほかつらくはありません。"},{"id":{"d":584,"n":7,"t":4},"speaker":"A","text":"あなたが頑張っているので、私も今日はお酒はやめておくことにします。"},{"id":{"d":584,"n":8,"t":4},"speaker":"B","text":"気を使わせてすみません。もし飲みたくなったら、遠慮なく飲んでください。"}];
INSERT INTO sentence [{"id":{"d":585,"n":1,"t":4},"speaker":"A","text":"私はほこりアレルギーがあります。"},{"id":{"d":585,"n":2,"t":4},"speaker":"B","text":"どういう症状があるのですか？"},{"id":{"d":585,"n":3,"t":4},"speaker":"A","text":"ほこりの多い所にいると、くしゃみが止まらなくなります。"},{"id":{"d":585,"n":4,"t":4},"speaker":"B","text":"それでは、自宅はいつもきれいにしておかないといけませんね。"},{"id":{"d":585,"n":5,"t":4},"speaker":"A","text":"そうなのですが、掃除をするとほこりが舞って、そのせいでくしゃみが出てしまうのです。"},{"id":{"d":585,"n":6,"t":4},"speaker":"B","text":"掃除もやりにくいというわけですか。大変ですね。"},{"id":{"d":585,"n":7,"t":4},"speaker":"A","text":"掃除をする時は、マスクをしてほこりを避けるように気を付けています。"},{"id":{"d":585,"n":8,"t":4},"speaker":"B","text":"いろいろと苦労があるのですね。"}];
INSERT INTO sentence [{"id":{"d":586,"n":1,"t":4},"speaker":"A","text":"ママは結婚指輪をしているのに、パパはしていませんね。"},{"id":{"d":586,"n":2,"t":4},"speaker":"B","text":"パパは金属アレルギーなので、指輪ができないのです。"},{"id":{"d":586,"n":3,"t":4},"speaker":"A","text":"もし付けたら、どういう症状が出るのですか？"},{"id":{"d":586,"n":4,"t":4},"speaker":"B","text":"皮膚が赤くなったり、かゆくなったりします。"},{"id":{"d":586,"n":5,"t":4},"speaker":"A","text":"そうなのですか。腕時計は付けても大丈夫ですか？"},{"id":{"d":586,"n":6,"t":4},"speaker":"B","text":"金属アレルギーが起こりにくい素材の時計を選んで買っているので、大丈夫ですよ。"},{"id":{"d":586,"n":7,"t":4},"speaker":"A","text":"種類によって、症状が出たり出なかったりするのですね。"},{"id":{"d":586,"n":8,"t":4},"speaker":"B","text":"はい。結婚指輪は素材ではなくデザインで選んだところ、アレルギー症状が出てしまいました。"}];
INSERT INTO sentence [{"id":{"d":587,"n":1,"t":4},"speaker":"A","text":"私は汗かきで、夏がつらいです。"},{"id":{"d":587,"n":2,"t":4},"speaker":"B","text":"パパも汗かきなので、遺伝したのかもしれませんね。"},{"id":{"d":587,"n":3,"t":4},"speaker":"A","text":"パパも夏はつらいですか？"},{"id":{"d":587,"n":4,"t":4},"speaker":"B","text":"そうでもありません。夏にたくさん汗をかいて、シャワーを浴びてさっぱりすると、気分がいいです。"},{"id":{"d":587,"n":5,"t":4},"speaker":"A","text":"私は汗でベタベタするのは嫌いです。できれば、あまり汗をかかないママに似たかったです。"},{"id":{"d":587,"n":6,"t":4},"speaker":"B","text":"遺伝するところが選べたら良かったですね。"}];
INSERT INTO sentence [{"id":{"d":588,"n":1,"t":4},"speaker":"A","text":"薬を飲みたいので、お水をいただけますか？"},{"id":{"d":588,"n":2,"t":4},"speaker":"B","text":"はい、どうぞ。何の薬ですか？"},{"id":{"d":588,"n":3,"t":4},"speaker":"A","text":"胃薬です。"},{"id":{"d":588,"n":4,"t":4},"speaker":"B","text":"どうかされたのですか？"},{"id":{"d":588,"n":5,"t":4},"speaker":"A","text":"胃が痛むことが多くて病院に行ったら、ストレス性の胃炎と診断されて薬を処方されたのです。"},{"id":{"d":588,"n":6,"t":4},"speaker":"B","text":"しばらく前から、仕事が忙しくて休みがないと話していましたよね。そういうこともストレスの原因でしょうね。"},{"id":{"d":588,"n":7,"t":4},"speaker":"A","text":"はい。忙し過ぎて、心が休まる時間が取れません。"},{"id":{"d":588,"n":8,"t":4},"speaker":"B","text":"仕事の状況を改善してもらうよう、職場の人に相談した方がいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":589,"n":1,"t":4},"speaker":"A","text":"禁煙をしたいのですが、なかなかうまくいきません。"},{"id":{"d":589,"n":2,"t":4},"speaker":"B","text":"挑戦したことはあるのですか？"},{"id":{"d":589,"n":3,"t":4},"speaker":"A","text":"何度もありますが、いつも挫折してしまいます。"},{"id":{"d":589,"n":4,"t":4},"speaker":"B","text":"私の同僚は、病院に通って禁煙に成功したと話していましたよ。"},{"id":{"d":589,"n":5,"t":4},"speaker":"A","text":"禁煙外来ですね。どのようなことをするのかが不安で、敬遠してしまいます。"},{"id":{"d":589,"n":6,"t":4},"speaker":"B","text":"同僚は、ニコチンパッチを使用して、それほど苦労せずに乗り越えたと言っていましたよ。"},{"id":{"d":589,"n":7,"t":4},"speaker":"A","text":"そうですか。実際に体験した人の話は参考になります。"},{"id":{"d":589,"n":8,"t":4},"speaker":"B","text":"勇気を出して、病院に予約を入れてみてはどうですか？"},{"id":{"d":589,"n":9,"t":4},"speaker":"A","text":"前向きに検討します。"}];
INSERT INTO sentence [{"id":{"d":590,"n":1,"t":4},"speaker":"A","text":"赤ちゃんの首に湿疹ができてしまいました。"},{"id":{"d":590,"n":2,"t":4},"speaker":"B","text":"あせもではありませんか？"},{"id":{"d":590,"n":3,"t":4},"speaker":"A","text":"なるほど。夏ですし、そうかもしれません。"},{"id":{"d":590,"n":4,"t":4},"speaker":"B","text":"赤ちゃんはたくさん汗をかくので、あせもができやすいですよね。"},{"id":{"d":590,"n":5,"t":4},"speaker":"A","text":"この子は特に汗をかきやすいようです。"},{"id":{"d":590,"n":6,"t":4},"speaker":"B","text":"小まめにシャワーを浴びて汗を洗い流した方がいいですね。"},{"id":{"d":590,"n":7,"t":4},"speaker":"A","text":"でも、出かけていてシャワーを浴びられないことも多いです。"},{"id":{"d":590,"n":8,"t":4},"speaker":"B","text":"そのような時は、ぬれたタオルで汗を拭いてあげるだけでも効果があると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":591,"n":1,"t":4},"speaker":"A","text":"深爪をしてしまいました。"},{"id":{"d":591,"n":2,"t":4},"speaker":"B","text":"深爪は痛いですよね。"},{"id":{"d":591,"n":3,"t":4},"speaker":"A","text":"はい。それに、物がつかみにくいです。"},{"id":{"d":591,"n":4,"t":4},"speaker":"B","text":"机の上の紙などが取りにくいですよね。"},{"id":{"d":591,"n":5,"t":4},"speaker":"A","text":"はい。早く爪が伸びてほしいです。"},{"id":{"d":591,"n":6,"t":4},"speaker":"B","text":"そうですね。でも、そう思っている時ほど、なかなか伸びない気がするものです。"},{"id":{"d":591,"n":7,"t":4},"speaker":"A","text":"ええ。普段はすぐに伸びてしまうように感じるのに、待っていると遅いのですよね。"},{"id":{"d":591,"n":8,"t":4},"speaker":"B","text":"何より、これからは爪を切り過ぎないように気を付けてくださいね。"},{"id":{"d":591,"n":9,"t":4},"speaker":"A","text":"そうですよね。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":592,"n":1,"t":4},"speaker":"A","text":"今度、胃カメラ検査を受けます。"},{"id":{"d":592,"n":2,"t":4},"speaker":"B","text":"胃が悪いのですか？"},{"id":{"d":592,"n":3,"t":4},"speaker":"A","text":"健康診断を受けたら、胃の検査が必要だという結果が届いたのです。"},{"id":{"d":592,"n":4,"t":4},"speaker":"B","text":"胃カメラは苦しいとよくいわれていますね。"},{"id":{"d":592,"n":5,"t":4},"speaker":"A","text":"はい。とても怖いです。"},{"id":{"d":592,"n":6,"t":4},"speaker":"B","text":"麻酔を使って受けることもできると聞いたことがあります。"},{"id":{"d":592,"n":7,"t":4},"speaker":"A","text":"もし使えるなら、絶対に麻酔を使いたいです。"},{"id":{"d":592,"n":8,"t":4},"speaker":"B","text":"あらかじめ問い合わせをして、麻酔を受けられる病院に予約したらどうでしょうか？"}];
INSERT INTO sentence [{"id":{"d":593,"n":1,"t":4},"speaker":"A","text":"医師国家試験に合格したそうですね。おめでとうございます。"},{"id":{"d":593,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。合格できて本当にうれしいです。"},{"id":{"d":593,"n":3,"t":4},"speaker":"A","text":"ようやく、医者になれるのですね。"},{"id":{"d":593,"n":4,"t":4},"speaker":"B","text":"本格的な医者になる前に、まずは研修医として働きます。"},{"id":{"d":593,"n":5,"t":4},"speaker":"A","text":"そうでした。研修医として働く病院は決まりましたか？"},{"id":{"d":593,"n":6,"t":4},"speaker":"B","text":"まだ決まっていません。"},{"id":{"d":593,"n":7,"t":4},"speaker":"A","text":"研修医になったら、きっと今とは比べものにならないほど忙しくなるのでしょうね。"},{"id":{"d":593,"n":8,"t":4},"speaker":"B","text":"それは覚悟しています。"}];
INSERT INTO sentence [{"id":{"d":594,"n":1,"t":4},"speaker":"A","text":"枕を買い替えました。"},{"id":{"d":594,"n":2,"t":4},"speaker":"B","text":"どのような枕を買ったのですか？"},{"id":{"d":594,"n":3,"t":4},"speaker":"A","text":"低反発で、睡眠の質を高めてくれると評判になっているものです。"},{"id":{"d":594,"n":4,"t":4},"speaker":"B","text":"それなら知っています。私も欲しいと思っていました。"},{"id":{"d":594,"n":5,"t":4},"speaker":"A","text":"今までは眠りが浅かったのですが、枕を新しくしたことで改善したような気がします。"},{"id":{"d":594,"n":6,"t":4},"speaker":"B","text":"評判通りということですね。"},{"id":{"d":594,"n":7,"t":4},"speaker":"A","text":"高かったのですが、買って正解でした。"},{"id":{"d":594,"n":8,"t":4},"speaker":"B","text":"いくらだったのですか？"},{"id":{"d":594,"n":9,"t":4},"speaker":"A","text":"約1万円です。"},{"id":{"d":594,"n":10,"t":4},"speaker":"B","text":"それは高額ですね。私は買うのをためらってしまいます。"}];
INSERT INTO sentence [{"id":{"d":595,"n":1,"t":4},"speaker":"A","text":"お化粧を落とさないまま寝てしまいました。"},{"id":{"d":595,"n":2,"t":4},"speaker":"B","text":"それは肌が荒れてしまいそうですね。"},{"id":{"d":595,"n":3,"t":4},"speaker":"A","text":"ええ。今朝は肌の状態がひどかったです。"},{"id":{"d":595,"n":4,"t":4},"speaker":"B","text":"昨日は何があったのですか？"},{"id":{"d":595,"n":5,"t":4},"speaker":"A","text":"飲み会があって深夜に帰宅して、着替えたりお風呂に入ったりもせず、そのまま朝まで眠ってしまいました。"},{"id":{"d":595,"n":6,"t":4},"speaker":"B","text":"帰宅して、安心してしまったのかもしれませんね。"},{"id":{"d":595,"n":7,"t":4},"speaker":"A","text":"朝に目が覚めて前の晩のままだと気が付いた時は、かなりショックでした。"},{"id":{"d":595,"n":8,"t":4},"speaker":"B","text":"私も同じ失敗をしたことがあります。"}];
INSERT INTO sentence [{"id":{"d":596,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。何かお探しですか？"},{"id":{"d":596,"n":2,"t":4},"speaker":"B","text":"ファンデーションが欲しいのですが、どれがいいのか分からず悩んでいます。"},{"id":{"d":596,"n":3,"t":4},"speaker":"A","text":"今はどのようなものをお使いですか？"},{"id":{"d":596,"n":4,"t":4},"speaker":"B","text":"ごく普通のものですが、敏感肌のようで、肌荒れしてしまうのです。"},{"id":{"d":596,"n":5,"t":4},"speaker":"A","text":"それでは、こちらの商品はいかがでしょうか？敏感肌用のものになっております。"},{"id":{"d":596,"n":6,"t":4},"speaker":"B","text":"試しに使ってみることはできますか？"},{"id":{"d":596,"n":7,"t":4},"speaker":"A","text":"もちろんです。あちらにメークができる場所がありますので、よろしければどうぞ。"},{"id":{"d":596,"n":8,"t":4},"speaker":"B","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":597,"n":1,"t":4},"speaker":"A","text":"すみません。こちらのジムの体験コースに申し込んだ大友です。"},{"id":{"d":597,"n":2,"t":4},"speaker":"B","text":"大友様、お待ちしておりました。最初にお着替えをお願いします。"},{"id":{"d":597,"n":3,"t":4},"speaker":"A","text":"手ぶらで来てしまったのですが、大丈夫ですか？"},{"id":{"d":597,"n":4,"t":4},"speaker":"B","text":"こちらでご用意しておりますので、ご安心ください。サイズはS、M、Lがありますが、どれがよろしいですか？"},{"id":{"d":597,"n":5,"t":4},"speaker":"A","text":"Mでお願いします。"},{"id":{"d":597,"n":6,"t":4},"speaker":"B","text":"こちらがMサイズです。右手奥にロッカールームがありますので、ご利用ください。"},{"id":{"d":597,"n":7,"t":4},"speaker":"A","text":"着替えが終わったら、どうすればよいですか？"},{"id":{"d":597,"n":8,"t":4},"speaker":"B","text":"ご案内しますので、こちらのカウンターにお越しください。"}];
INSERT INTO sentence [{"id":{"d":598,"n":1,"t":4},"speaker":"A","text":"新しく買ったチークを使ってみました。"},{"id":{"d":598,"n":2,"t":4},"speaker":"B","text":"随分印象が変わりますね。"},{"id":{"d":598,"n":3,"t":4},"speaker":"A","text":"少し派手でしょうか？"},{"id":{"d":598,"n":4,"t":4},"speaker":"B","text":"そのようなことはありませんよ。"},{"id":{"d":598,"n":5,"t":4},"speaker":"A","text":"今まで使ったことのない色味なので、似合っているか不安です。"},{"id":{"d":598,"n":6,"t":4},"speaker":"B","text":"とてもよく似合っていますよ。顔色が明るく見えます。"},{"id":{"d":598,"n":7,"t":4},"speaker":"A","text":"良かったです。安心しました。"},{"id":{"d":598,"n":8,"t":4},"speaker":"B","text":"化粧品を変えると、気分が変わっていいですね。"}];
INSERT INTO sentence [{"id":{"d":599,"n":1,"t":4},"speaker":"A","text":"今日はいつもと何か違いますね。"},{"id":{"d":599,"n":2,"t":4},"speaker":"B","text":"カラーコンタクトをしているのです。"},{"id":{"d":599,"n":3,"t":4},"speaker":"A","text":"本当ですね。雰囲気が違います。"},{"id":{"d":599,"n":4,"t":4},"speaker":"B","text":"おかしいでしょうか？"},{"id":{"d":599,"n":5,"t":4},"speaker":"A","text":"いえいえ。とてもお似合いですよ。"},{"id":{"d":599,"n":6,"t":4},"speaker":"B","text":"色の種類がたくさんあったのですが、悩んだ末にこの色にしました。"},{"id":{"d":599,"n":7,"t":4},"speaker":"A","text":"すてきです。何という色ですか？"},{"id":{"d":599,"n":8,"t":4},"speaker":"B","text":"ライトブラウンです。"}];
INSERT INTO sentence [{"id":{"d":600,"n":1,"t":4},"speaker":"A","text":"行ってきます。"},{"id":{"d":600,"n":2,"t":4},"speaker":"B","text":"いつもよりだいぶ早い出発ですね。"},{"id":{"d":600,"n":3,"t":4},"speaker":"A","text":"会社の最寄り駅の2駅前で降りて、そこから歩いて行くことにしたのです。"},{"id":{"d":600,"n":4,"t":4},"speaker":"B","text":"歩くと何分くらいかかりますか？"},{"id":{"d":600,"n":5,"t":4},"speaker":"A","text":"速足で30分くらいです。"},{"id":{"d":600,"n":6,"t":4},"speaker":"B","text":"いい運動になりますね。"},{"id":{"d":600,"n":7,"t":4},"speaker":"A","text":"はい。これからは時々歩こうと思っています。"},{"id":{"d":600,"n":8,"t":4},"speaker":"B","text":"気を付けて行ってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":601,"n":1,"t":4},"speaker":"A","text":"注文は決まりましたか？私はチキンカレーを頼みます。"},{"id":{"d":601,"n":2,"t":4},"speaker":"B","text":"私はカツカレーにします。辛さはどうしますか？"},{"id":{"d":601,"n":3,"t":4},"speaker":"A","text":"大辛にします。"},{"id":{"d":601,"n":4,"t":4},"speaker":"B","text":"一番辛いレベルですよ。大丈夫ですか？"},{"id":{"d":601,"n":5,"t":4},"speaker":"A","text":"辛いものが大好きなので、大丈夫です。"},{"id":{"d":601,"n":6,"t":4},"speaker":"B","text":"カレー以外でも、辛さに強いのですか？"},{"id":{"d":601,"n":7,"t":4},"speaker":"A","text":"そうですね。自宅で料理しても、唐辛子をたくさんかけることが多いです。"},{"id":{"d":601,"n":8,"t":4},"speaker":"B","text":"それはすごいですね。私は無難に中辛にします。"}];
INSERT INTO sentence [{"id":{"d":602,"n":1,"t":4},"speaker":"A","text":"妊婦健診はどうでしたか？"},{"id":{"d":602,"n":2,"t":4},"speaker":"B","text":"体重が急に増えたので、気を付けるように言われました。"},{"id":{"d":602,"n":3,"t":4},"speaker":"A","text":"そうでしたか。でも、妊婦さんがダイエットをするわけにもいきませんし、難しいですね。"},{"id":{"d":602,"n":4,"t":4},"speaker":"B","text":"とりあえず、間食を控えることにします。"},{"id":{"d":602,"n":5,"t":4},"speaker":"A","text":"そうですね。おなかの赤ちゃんの方は、問題ありませんでしたか？"},{"id":{"d":602,"n":6,"t":4},"speaker":"B","text":"はい。順調に成長しているようです。"},{"id":{"d":602,"n":7,"t":4},"speaker":"A","text":"それは安心ですね。性別はまだ分からないのですね。"},{"id":{"d":602,"n":8,"t":4},"speaker":"B","text":"はい。次の検診で分かるのではないか、と先生が話していました。"}];
INSERT INTO sentence [{"id":{"d":603,"n":1,"t":4},"speaker":"A","text":"もしもし、美紀です。お父さんですか？"},{"id":{"d":603,"n":2,"t":4},"speaker":"B","text":"はい、お父さんです。どうしたのですか？"},{"id":{"d":603,"n":3,"t":4},"speaker":"A","text":"これから病院に面会に行きますが、何か欲しいものはありますか？"},{"id":{"d":603,"n":4,"t":4},"speaker":"B","text":"果物の入ったゼリーが欲しいです。"},{"id":{"d":603,"n":5,"t":4},"speaker":"A","text":"分かりました。他にはありますか？"},{"id":{"d":603,"n":6,"t":4},"speaker":"B","text":"お父さんが毎週読んでいる雑誌は分かりますか？"},{"id":{"d":603,"n":7,"t":4},"speaker":"A","text":"はい。経済の雑誌ですよね。"},{"id":{"d":603,"n":8,"t":4},"speaker":"B","text":"そうです。その最新刊が読みたいです。"},{"id":{"d":603,"n":9,"t":4},"speaker":"A","text":"それでは、ゼリーと雑誌を買ってから病院に行きますね。"}];
INSERT INTO sentence [{"id":{"d":604,"n":1,"t":4},"speaker":"A","text":"半年くらい前から、ホットフラッシュに悩まされています。"},{"id":{"d":604,"n":2,"t":4},"speaker":"B","text":"どういう症状があるのですか？"},{"id":{"d":604,"n":3,"t":4},"speaker":"A","text":"急に顔がカーっと熱くなって、汗が噴き出してくるのです。"},{"id":{"d":604,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。"},{"id":{"d":604,"n":5,"t":4},"speaker":"A","text":"仕事中にお客様の前で症状が出てしまうと、とても困ります。"},{"id":{"d":604,"n":6,"t":4},"speaker":"B","text":"病院には行きましたか？"},{"id":{"d":604,"n":7,"t":4},"speaker":"A","text":"はい。更年期障害だろうということで、漢方薬を処方されました。"},{"id":{"d":604,"n":8,"t":4},"speaker":"B","text":"少しでも改善されるといいですね。"}];
INSERT INTO sentence [{"id":{"d":605,"n":1,"t":4},"speaker":"A","text":"夏の会社は少し過ごしづらいです。"},{"id":{"d":605,"n":2,"t":4},"speaker":"B","text":"どうしてですか？"},{"id":{"d":605,"n":3,"t":4},"speaker":"A","text":"私は冷え性なのですが、会社が夏なのに寒いくらいなのです。"},{"id":{"d":605,"n":4,"t":4},"speaker":"B","text":"エアコンの設定温度が低いのでしょうね。"},{"id":{"d":605,"n":5,"t":4},"speaker":"A","text":"そうなのです。私は事務仕事で座ったままなので、じっとしていると本当に寒くてつらいです。"},{"id":{"d":605,"n":6,"t":4},"speaker":"B","text":"会社の人に相談してはどうですか？"},{"id":{"d":605,"n":7,"t":4},"speaker":"A","text":"上司に話したのですが、営業で外回りをしている人が多くて、その人たちからは夏にエアコンを効かせてほしいと要望されていると言われてしまいました。"},{"id":{"d":605,"n":8,"t":4},"speaker":"B","text":"そうでしたか。では、厚着したり、温かい飲み物を飲んだりして、自分で対策するしかなさそうですね。"}];
INSERT INTO sentence [{"id":{"d":606,"n":1,"t":4},"speaker":"A","text":"ヘアカラーをお願いします。"},{"id":{"d":606,"n":2,"t":4},"speaker":"B","text":"どのような色にしますか？"},{"id":{"d":606,"n":3,"t":4},"speaker":"A","text":"私の髪は真っ黒で重いイメージなので雰囲気を変えたいのですが、どのような色がいいでしょうか？"},{"id":{"d":606,"n":4,"t":4},"speaker":"B","text":"こちらに色の見本があります。明るい茶色はどうですか？"},{"id":{"d":606,"n":5,"t":4},"speaker":"A","text":"この栗色もきれいですね。"},{"id":{"d":606,"n":6,"t":4},"speaker":"B","text":"栗色もお似合いだと思いますよ。"},{"id":{"d":606,"n":7,"t":4},"speaker":"A","text":"では、この栗色でお願いします。"},{"id":{"d":606,"n":8,"t":4},"speaker":"B","text":"はい。最初に髪を洗いますので、シャンプー台へ移動をお願いします。"}];
INSERT INTO sentence [{"id":{"d":607,"n":1,"t":4},"speaker":"A","text":"ニキビがなかなか治りません。"},{"id":{"d":607,"n":2,"t":4},"speaker":"B","text":"洗顔はしっかりしていますか？"},{"id":{"d":607,"n":3,"t":4},"speaker":"A","text":"もちろんです。朝と夜、きちんと洗っています。"},{"id":{"d":607,"n":4,"t":4},"speaker":"B","text":"気になるでしょうが、手でニキビを触らない方がいいですよ。"},{"id":{"d":607,"n":5,"t":4},"speaker":"A","text":"分かっているのですが、どうしても触ってしまいます。"},{"id":{"d":607,"n":6,"t":4},"speaker":"B","text":"手で触ると、治りが遅くなってしまいます。"},{"id":{"d":607,"n":7,"t":4},"speaker":"A","text":"そうですよね。気を付けます。"},{"id":{"d":607,"n":8,"t":4},"speaker":"B","text":"皮膚科で薬をもらうという方法もありますよ。"},{"id":{"d":607,"n":9,"t":4},"speaker":"A","text":"このまましばらく治らないようなら、皮膚科に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":608,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":608,"n":2,"t":4},"speaker":"B","text":"うちの猫が、食欲がありません。"},{"id":{"d":608,"n":3,"t":4},"speaker":"A","text":"いつ頃からですか？"},{"id":{"d":608,"n":4,"t":4},"speaker":"B","text":"先週くらいからです。"},{"id":{"d":608,"n":5,"t":4},"speaker":"A","text":"その他に症状はありませんか？"},{"id":{"d":608,"n":6,"t":4},"speaker":"B","text":"そういえば、便が軟らかかったかもしれません。"},{"id":{"d":608,"n":7,"t":4},"speaker":"A","text":"わかりました。診察しますので、猫を診察台に乗せてください。"},{"id":{"d":608,"n":8,"t":4},"speaker":"B","text":"はい。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":609,"n":1,"t":4},"speaker":"A","text":"堀さん、診察室へどうぞ。"},{"id":{"d":609,"n":2,"t":4},"speaker":"B","text":"失礼します。"},{"id":{"d":609,"n":3,"t":4},"speaker":"A","text":"どうぞ。堀さん、骨折していた手首の調子はどうですか？"},{"id":{"d":609,"n":4,"t":4},"speaker":"B","text":"もう痛みもありませんし、骨折する前と変わらないと思います。"},{"id":{"d":609,"n":5,"t":4},"speaker":"A","text":"それは良かったです。治療はこれで終わりですが、これからはしばらくリハビリに通ってください。"},{"id":{"d":609,"n":6,"t":4},"speaker":"B","text":"骨折が治っても、リハビリが必要なのですか？"},{"id":{"d":609,"n":7,"t":4},"speaker":"A","text":"ずっと動かしていなかったために筋肉が衰えていますから、リハビリで筋力を回復しましょう。"},{"id":{"d":609,"n":8,"t":4},"speaker":"B","text":"そうなのですね。早く元に戻るように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":610,"n":1,"t":4},"speaker":"A","text":"今までに大きな病気をしたことはありますか？"},{"id":{"d":610,"n":2,"t":4},"speaker":"B","text":"結核になったことがあります。"},{"id":{"d":610,"n":3,"t":4},"speaker":"A","text":"何年前ですか？"},{"id":{"d":610,"n":4,"t":4},"speaker":"B","text":"大学4年生の時なので、3年前です。"},{"id":{"d":610,"n":5,"t":4},"speaker":"A","text":"他にはありますか？"},{"id":{"d":610,"n":6,"t":4},"speaker":"B","text":"気管支喘息でした。"},{"id":{"d":610,"n":7,"t":4},"speaker":"A","text":"それはいつ頃ですか？"},{"id":{"d":610,"n":8,"t":4},"speaker":"B","text":"子どもの頃です。"}];
INSERT INTO sentence [{"id":{"d":611,"n":1,"t":4},"speaker":"A","text":"退院おめでとうございます。"},{"id":{"d":611,"n":2,"t":4},"speaker":"B","text":"ようやく退院できて、本当にうれしいです。"},{"id":{"d":611,"n":3,"t":4},"speaker":"A","text":"入院中につらかったことは何ですか？"},{"id":{"d":611,"n":4,"t":4},"speaker":"B","text":"つらいというほどではありませんが、お風呂が週に3回だったことです。毎日入りたかったです。"},{"id":{"d":611,"n":5,"t":4},"speaker":"A","text":"それは私も同じ気持ちになると思います。"},{"id":{"d":611,"n":6,"t":4},"speaker":"B","text":"それから、回復してくるとすることがなくて退屈でした。"},{"id":{"d":611,"n":7,"t":4},"speaker":"A","text":"時間が経つのが遅く感じそうですね。"},{"id":{"d":611,"n":8,"t":4},"speaker":"B","text":"本当にその通りです。時間を持て余していました。"}];
INSERT INTO sentence [{"id":{"d":612,"n":1,"t":4},"speaker":"A","text":"交通事故に遭ったと聞きましたが、大丈夫ですか？"},{"id":{"d":612,"n":2,"t":4},"speaker":"B","text":"大丈夫です。ご心配をおかけしてすみません。"},{"id":{"d":612,"n":3,"t":4},"speaker":"A","text":"どのような事故だったのですか？"},{"id":{"d":612,"n":4,"t":4},"speaker":"B","text":"私はバイクに乗っていて、信号のない交差点で車と接触してしまいました。"},{"id":{"d":612,"n":5,"t":4},"speaker":"A","text":"けがはありませんでしたか？"},{"id":{"d":612,"n":6,"t":4},"speaker":"B","text":"擦り傷と打撲で済みました。"},{"id":{"d":612,"n":7,"t":4},"speaker":"A","text":"大けがにならなくて良かったです。"},{"id":{"d":612,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。これからはもっと慎重に運転することを心掛けます。"}];
INSERT INTO sentence [{"id":{"d":613,"n":1,"t":4},"speaker":"A","text":"私は救急車に乗ったことがありません。"},{"id":{"d":613,"n":2,"t":4},"speaker":"B","text":"乗ったことがない方がいいですよ。"},{"id":{"d":613,"n":3,"t":4},"speaker":"A","text":"あなたはありますか？"},{"id":{"d":613,"n":4,"t":4},"speaker":"B","text":"2年前に熱中症になって救急車で運ばれました。"},{"id":{"d":613,"n":5,"t":4},"speaker":"A","text":"どのような感じでしたか？"},{"id":{"d":613,"n":6,"t":4},"speaker":"B","text":"意識がもうろうとしていたので、よく覚えていません。"},{"id":{"d":613,"n":7,"t":4},"speaker":"A","text":"熱中症の症状は大丈夫でしたか？"},{"id":{"d":613,"n":8,"t":4},"speaker":"B","text":"念のために1日だけ入院しましたが、大事には至りませんでした。"}];
INSERT INTO sentence [{"id":{"d":614,"n":1,"t":4},"speaker":"A","text":"あなたはずっと髪を伸ばしていますね。"},{"id":{"d":614,"n":2,"t":4},"speaker":"B","text":"髪を寄付したいので伸ばしているのです。"},{"id":{"d":614,"n":3,"t":4},"speaker":"A","text":"髪を寄付できるのですか？"},{"id":{"d":614,"n":4,"t":4},"speaker":"B","text":"はい。寄付した髪で、病気で髪を失ってしまった人のためのウィッグを作ることができるのです。"},{"id":{"d":614,"n":5,"t":4},"speaker":"A","text":"それは素晴らしいですね。"},{"id":{"d":614,"n":6,"t":4},"speaker":"B","text":"寄付するには髪の長さが31㎝以上必要なので、まだ伸ばす必要があります。"},{"id":{"d":614,"n":7,"t":4},"speaker":"A","text":"あと10㎝くらいですね。"},{"id":{"d":614,"n":8,"t":4},"speaker":"B","text":"はい。寄付したら、ショートヘアにしてイメージチェンジするつもりです。"}];
INSERT INTO sentence [{"id":{"d":615,"n":1,"t":4},"speaker":"A","text":"真夏なのに、この部屋は寒いです。"},{"id":{"d":615,"n":2,"t":4},"speaker":"B","text":"そうですか？"},{"id":{"d":615,"n":3,"t":4},"speaker":"A","text":"エアコンの設定温度は何度ですか？"},{"id":{"d":615,"n":4,"t":4},"speaker":"B","text":"18度です。"},{"id":{"d":615,"n":5,"t":4},"speaker":"A","text":"それは低過ぎます。体に良くありませんよ。"},{"id":{"d":615,"n":6,"t":4},"speaker":"B","text":"私にはちょうどいい温度です。"},{"id":{"d":615,"n":7,"t":4},"speaker":"A","text":"すみませんが、私には寒過ぎるので、もう少し高い温度に変えてください。"},{"id":{"d":615,"n":8,"t":4},"speaker":"B","text":"分かりました。風量も弱くしますね。"}];
INSERT INTO sentence [{"id":{"d":616,"n":1,"t":4},"speaker":"A","text":"おはようございます。うっかり二度寝してしまいました。"},{"id":{"d":616,"n":2,"t":4},"speaker":"B","text":"おはようございます。いつもよりだいぶ遅いですね。会社に間に合いますか？"},{"id":{"d":616,"n":3,"t":4},"speaker":"A","text":"急いで身支度すれば間に合うと思います。"},{"id":{"d":616,"n":4,"t":4},"speaker":"B","text":"朝ご飯はどうしますか？"},{"id":{"d":616,"n":5,"t":4},"speaker":"A","text":"食べている時間がありません。"},{"id":{"d":616,"n":6,"t":4},"speaker":"B","text":"朝ご飯を抜くと健康に良くありませんよ。"},{"id":{"d":616,"n":7,"t":4},"speaker":"A","text":"分かってはいますが、食べていたら遅刻してしまいます。"},{"id":{"d":616,"n":8,"t":4},"speaker":"B","text":"バナナだけでも食べませんか？"},{"id":{"d":616,"n":9,"t":4},"speaker":"A","text":"バナナならすぐに食べられるので、いただきます。"}];
INSERT INTO sentence [{"id":{"d":617,"n":1,"t":4},"speaker":"A","text":"緒方さんはいつもペットボトルを机に置いていますね。"},{"id":{"d":617,"n":2,"t":4},"speaker":"B","text":"毎日水を2リットル飲むようにしているので、いつでも飲めるよう机に置いているのです。"},{"id":{"d":617,"n":3,"t":4},"speaker":"A","text":"2リットルですか。かなりの量ですね。"},{"id":{"d":617,"n":4,"t":4},"speaker":"B","text":"そうですね。水を2リットル飲むようにしていると、他の飲み物を飲みたいと思うことがなくなりますよ。"},{"id":{"d":617,"n":5,"t":4},"speaker":"A","text":"そうなるでしょうね。以前は緒方さんがコーヒーを飲む姿をよく見かけましたが、今は見なくなりました。"},{"id":{"d":617,"n":6,"t":4},"speaker":"B","text":"ええ。最近は1日に1杯もコーヒーを飲まない日の方が多いと思います。"},{"id":{"d":617,"n":7,"t":4},"speaker":"A","text":"水は常温で飲んでいるのですか？"},{"id":{"d":617,"n":8,"t":4},"speaker":"B","text":"昼間は常温で飲みますが、家では白湯もよく飲みます。"}];
INSERT INTO sentence [{"id":{"d":618,"n":1,"t":4},"speaker":"A","text":"お風呂に浸かり過ぎてのぼせてしまいました。"},{"id":{"d":618,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":618,"n":3,"t":4},"speaker":"A","text":"少し、くらくらします。"},{"id":{"d":618,"n":4,"t":4},"speaker":"B","text":"ソファに横になってください。"},{"id":{"d":618,"n":5,"t":4},"speaker":"A","text":"はい。すみませんが、水をください。"},{"id":{"d":618,"n":6,"t":4},"speaker":"B","text":"わかりました。冷たい方がいいですよね。"},{"id":{"d":618,"n":7,"t":4},"speaker":"A","text":"はい。氷水にしてほしいです。"},{"id":{"d":618,"n":8,"t":4},"speaker":"B","text":"分かりました。すぐに持ってきます。"}];
INSERT INTO sentence [{"id":{"d":619,"n":1,"t":4},"speaker":"A","text":"おはようございます。"},{"id":{"d":619,"n":2,"t":4},"speaker":"B","text":"おはようございます。寝癖が付いていますよ。"},{"id":{"d":619,"n":3,"t":4},"speaker":"A","text":"どこですか？"},{"id":{"d":619,"n":4,"t":4},"speaker":"B","text":"後頭部です。鏡を見て直した方がいいですよ。"},{"id":{"d":619,"n":5,"t":4},"speaker":"A","text":"そうします。洗面所に行ってきます。"},{"id":{"d":619,"n":6,"t":4},"speaker":"B","text":"急がないと遅刻しますよ。"},{"id":{"d":619,"n":7,"t":4},"speaker":"A","text":"はい。手早く直します。"},{"id":{"d":619,"n":8,"t":4},"speaker":"B","text":"髪に水を付けてからブラシでとかすと直りやすいです。"},{"id":{"d":619,"n":9,"t":4},"speaker":"A","text":"やってみます。"}];
INSERT INTO sentence [{"id":{"d":620,"n":1,"t":4},"speaker":"A","text":"歯医者に行ったら、歯列矯正を勧められました。"},{"id":{"d":620,"n":2,"t":4},"speaker":"B","text":"歯並びが悪いようには見えませんよ。"},{"id":{"d":620,"n":3,"t":4},"speaker":"A","text":"上の前歯が前に出ているので、押さえた方がいいそうです。"},{"id":{"d":620,"n":4,"t":4},"speaker":"B","text":"費用はどのくらいですか？"},{"id":{"d":620,"n":5,"t":4},"speaker":"A","text":"総額で30万から40万円だそうです。"},{"id":{"d":620,"n":6,"t":4},"speaker":"B","text":"かなり高額ですね。"},{"id":{"d":620,"n":7,"t":4},"speaker":"A","text":"はい。矯正するかどうか迷っています。"},{"id":{"d":620,"n":8,"t":4},"speaker":"B","text":"すぐには決められませんよね。"}];
INSERT INTO sentence [{"id":{"d":621,"n":1,"t":4},"speaker":"A","text":"頭がガンガンとして、気分が悪いです。"},{"id":{"d":621,"n":2,"t":4},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":621,"n":3,"t":4},"speaker":"A","text":"二日酔いです。"},{"id":{"d":621,"n":4,"t":4},"speaker":"B","text":"昨夜はそれほど飲んだのですか？"},{"id":{"d":621,"n":5,"t":4},"speaker":"A","text":"大学の友達との久しぶりの飲み会が盛り上がり、つい飲み過ぎてしまいました。"},{"id":{"d":621,"n":6,"t":4},"speaker":"B","text":"何時まで飲んでいたのですか？"},{"id":{"d":621,"n":7,"t":4},"speaker":"A","text":"よく覚えていませんが、終電にはギリギリ間に合いました。"},{"id":{"d":621,"n":8,"t":4},"speaker":"B","text":"お水を持ってきましょうか？"},{"id":{"d":621,"n":9,"t":4},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":622,"n":1,"t":4},"speaker":"A","text":"そろそろ花粉症のシーズンですね。"},{"id":{"d":622,"n":2,"t":4},"speaker":"B","text":"そうなのですね。私は花粉症ではないので、よく分かりません。"},{"id":{"d":622,"n":3,"t":4},"speaker":"A","text":"うらやましいです。"},{"id":{"d":622,"n":4,"t":4},"speaker":"B","text":"あなたは花粉症がひどいのですか？"},{"id":{"d":622,"n":5,"t":4},"speaker":"A","text":"とてもひどいです。仕事を休むこともあるくらいです。"},{"id":{"d":622,"n":6,"t":4},"speaker":"B","text":"それは大変ですね。薬は飲まないのですか？"},{"id":{"d":622,"n":7,"t":4},"speaker":"A","text":"飲みますが、それでも症状が治まらない日もあります。"},{"id":{"d":622,"n":8,"t":4},"speaker":"B","text":"それほどまでにひどいこともあるのですね。今年は軽く済むといいですね。"}];
INSERT INTO sentence [{"id":{"d":623,"n":1,"t":4},"speaker":"A","text":"お母さん、どうしたのですか？"},{"id":{"d":623,"n":2,"t":4},"speaker":"B","text":"微熱があって、体がだるいのです。"},{"id":{"d":623,"n":3,"t":4},"speaker":"A","text":"ベッドで寝た方がいいですよ。"},{"id":{"d":623,"n":4,"t":4},"speaker":"B","text":"でも、まだ夕食の準備をしていないし、洗濯物も取り込んでいません。"},{"id":{"d":623,"n":5,"t":4},"speaker":"A","text":"それは私と弟で協力してやりますから、気にせず寝てください。"},{"id":{"d":623,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。それではお願いして、寝室に行きますね。"},{"id":{"d":623,"n":7,"t":4},"speaker":"A","text":"何か必要なものがあったら言ってくださいね。"},{"id":{"d":623,"n":8,"t":4},"speaker":"B","text":"分かりました。本当に助かります。"}];
INSERT INTO sentence [{"id":{"d":624,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。今日はどうされましたか？"},{"id":{"d":624,"n":2,"t":4},"speaker":"B","text":"漢方薬を処方していただきたいです。"},{"id":{"d":624,"n":3,"t":4},"speaker":"A","text":"どのような症状がありますか？"},{"id":{"d":624,"n":4,"t":4},"speaker":"B","text":"ひどい冷え性で困っています。"},{"id":{"d":624,"n":5,"t":4},"speaker":"A","text":"それはつらいですね。特に冷えるのはどこでしょうか？"},{"id":{"d":624,"n":6,"t":4},"speaker":"B","text":"足先です。座って仕事をしていると冷えてくるので、靴下を重ね履きしています。"},{"id":{"d":624,"n":7,"t":4},"speaker":"A","text":"それでは血行を促進する効果のある漢方薬を処方しますね。"},{"id":{"d":624,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。効果を感じられるまでには時間がかかるでしょうか？"},{"id":{"d":624,"n":9,"t":4},"speaker":"A","text":"そうですね、即効性はありません。でも、飲み続けるとしっかりと効いてくると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":625,"n":1,"t":4},"speaker":"A","text":"今までどのようなダイエットをしたことがありますか？"},{"id":{"d":625,"n":2,"t":4},"speaker":"B","text":"いろいろとしましたよ。例えばリンゴダイエットというものをしたことがあります。"},{"id":{"d":625,"n":3,"t":4},"speaker":"A","text":"聞いたことがありません。どのようなダイエットですか？"},{"id":{"d":625,"n":4,"t":4},"speaker":"B","text":"1日3食のうち1食をリンゴだけにするのです。"},{"id":{"d":625,"n":5,"t":4},"speaker":"A","text":"お昼ご飯に決めたら、お昼はリンゴだけ食べるということですか？"},{"id":{"d":625,"n":6,"t":4},"speaker":"B","text":"そうです。その代わりに、リンゴなら何個食べてもいいのです。"},{"id":{"d":625,"n":7,"t":4},"speaker":"A","text":"効果があるのでしょうか？"},{"id":{"d":625,"n":8,"t":4},"speaker":"B","text":"私は2週間続けましたが、体重はほぼ変化しませんでした。"}];
INSERT INTO sentence [{"id":{"d":626,"n":1,"t":4},"speaker":"A","text":"夫が糖尿病になってしまいました。"},{"id":{"d":626,"n":2,"t":4},"speaker":"B","text":"それでは、食事に気を使いますね。"},{"id":{"d":626,"n":3,"t":4},"speaker":"A","text":"そうなのです。料理が得意ではないので、負担が大きいです。"},{"id":{"d":626,"n":4,"t":4},"speaker":"B","text":"カロリー計算もしているのですか？"},{"id":{"d":626,"n":5,"t":4},"speaker":"A","text":"大まかにですが、一応計算しています。"},{"id":{"d":626,"n":6,"t":4},"speaker":"B","text":"毎食となると、本当に大変ですね。"},{"id":{"d":626,"n":7,"t":4},"speaker":"A","text":"はい。外食も控えなければいけないので、毎日料理に追われています。"},{"id":{"d":626,"n":8,"t":4},"speaker":"B","text":"旦那さんにも料理を分担してもらってはどうでしょうか？"},{"id":{"d":626,"n":9,"t":4},"speaker":"A","text":"そうですね。少しずつ料理を覚えてほしいです。"}];
INSERT INTO sentence [{"id":{"d":627,"n":1,"t":4},"speaker":"A","text":"実家の母が入院しました。"},{"id":{"d":627,"n":2,"t":4},"speaker":"B","text":"病気ですか？"},{"id":{"d":627,"n":3,"t":4},"speaker":"A","text":"お風呂場で転んで骨折してしまったのです。"},{"id":{"d":627,"n":4,"t":4},"speaker":"B","text":"それはお気の毒ですね。"},{"id":{"d":627,"n":5,"t":4},"speaker":"A","text":"高齢なので、長く入院すると回復にも時間がかかるようで心配です。"},{"id":{"d":627,"n":6,"t":4},"speaker":"B","text":"そうですね。リハビリなども長期化しそうです。"},{"id":{"d":627,"n":7,"t":4},"speaker":"A","text":"母がふさぎ込まずに治療にも前向きなのが救いです。"},{"id":{"d":627,"n":8,"t":4},"speaker":"B","text":"それは良かったですね。"}];
INSERT INTO sentence [{"id":{"d":628,"n":1,"t":4},"speaker":"A","text":"お風呂に入ってきます。しばらく出てきませんが、心配しないでください。"},{"id":{"d":628,"n":2,"t":4},"speaker":"B","text":"お風呂で何をするのですか？"},{"id":{"d":628,"n":3,"t":4},"speaker":"A","text":"半身浴です。長時間すると体にいいと友達に教えてもらいました。"},{"id":{"d":628,"n":4,"t":4},"speaker":"B","text":"長時間というと、どのくらいですか？"},{"id":{"d":628,"n":5,"t":4},"speaker":"A","text":"1時間は続けようと思っています。"},{"id":{"d":628,"n":6,"t":4},"speaker":"B","text":"ずっと半身浴をしているだけでは、退屈しそうですね。"},{"id":{"d":628,"n":7,"t":4},"speaker":"A","text":"本を読みながら湯船に浸かるつもりです。"},{"id":{"d":628,"n":8,"t":4},"speaker":"B","text":"浴槽に本を落とさないように気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":629,"n":1,"t":4},"speaker":"A","text":"夕食を食べに行きませんか？"},{"id":{"d":629,"n":2,"t":4},"speaker":"B","text":"外食を控えているので、やめておきます。"},{"id":{"d":629,"n":3,"t":4},"speaker":"A","text":"家で自炊しているのですか？"},{"id":{"d":629,"n":4,"t":4},"speaker":"B","text":"はい。外食ばかりしていると栄養が偏ってしまうので、自炊することにしました。"},{"id":{"d":629,"n":5,"t":4},"speaker":"A","text":"1人分を作るのは手間ではありませんか？"},{"id":{"d":629,"n":6,"t":4},"speaker":"B","text":"一度に3人から4人分くらいの料理を作って、数日間は同じものを食べています。"},{"id":{"d":629,"n":7,"t":4},"speaker":"A","text":"飽きてしまいそうですね。"},{"id":{"d":629,"n":8,"t":4},"speaker":"B","text":"私は毎日が同じ料理でも気にならないので、全く問題ありませんよ。"}];
INSERT INTO sentence [{"id":{"d":630,"n":1,"t":4},"speaker":"A","text":"お子さんは何歳になりましたか？"},{"id":{"d":630,"n":2,"t":4},"speaker":"B","text":"2歳半です。"},{"id":{"d":630,"n":3,"t":4},"speaker":"A","text":"もうおしゃべりをしますか？"},{"id":{"d":630,"n":4,"t":4},"speaker":"B","text":"いいえ。まだ何もしゃべらないので少し心配です。"},{"id":{"d":630,"n":5,"t":4},"speaker":"A","text":"成長には個人差があるので気にしない方がいいですよ。"},{"id":{"d":630,"n":6,"t":4},"speaker":"B","text":"そうなのですが、同じ年齢のお友達が上手におしゃべりしているのと見ると、不安になります。"},{"id":{"d":630,"n":7,"t":4},"speaker":"A","text":"私の息子は3歳を過ぎてから、ようやく話すようになりました。焦りは禁物ですよ。"},{"id":{"d":630,"n":8,"t":4},"speaker":"B","text":"分かりました。ゆっくり成長を見守ります。"}];
INSERT INTO sentence [{"id":{"d":631,"n":1,"t":4},"speaker":"A","text":"お兄さんは今日、学校へ行かないのですか？"},{"id":{"d":631,"n":2,"t":4},"speaker":"B","text":"熱があるのでお休みします。"},{"id":{"d":631,"n":3,"t":4},"speaker":"A","text":"昨日は元気だったのに、急ですね。"},{"id":{"d":631,"n":4,"t":4},"speaker":"B","text":"夜中に突然に熱が出たようです。"},{"id":{"d":631,"n":5,"t":4},"speaker":"A","text":"風邪でしょうか？"},{"id":{"d":631,"n":6,"t":4},"speaker":"B","text":"分かりません。後で病院に行きます。"},{"id":{"d":631,"n":7,"t":4},"speaker":"A","text":"お兄さんに、「ゆっくり休んでくださいね」と伝えてください。"},{"id":{"d":631,"n":8,"t":4},"speaker":"B","text":"分かりました。行ってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":632,"n":1,"t":4},"speaker":"A","text":"もしもし、涼平ですか？"},{"id":{"d":632,"n":2,"t":4},"speaker":"B","text":"もしもし、お父さん、どうしたのですか？"},{"id":{"d":632,"n":3,"t":4},"speaker":"A","text":"病院に寄って帰るので、今日は遅くなります。"},{"id":{"d":632,"n":4,"t":4},"speaker":"B","text":"病院ですか？けがでもしたのですか？"},{"id":{"d":632,"n":5,"t":4},"speaker":"A","text":"会社で転倒して、腰を痛めてしまいました。"},{"id":{"d":632,"n":6,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":632,"n":7,"t":4},"speaker":"A","text":"念のため、病院で検査してもらいますが、恐らく大丈夫です。"},{"id":{"d":632,"n":8,"t":4},"speaker":"B","text":"心配なので、検査結果が分かったらすぐに教えてください。"},{"id":{"d":632,"n":9,"t":4},"speaker":"A","text":"分かりました。結果が分かったらまた電話しますね。"}];
INSERT INTO sentence [{"id":{"d":633,"n":1,"t":4},"speaker":"A","text":"最近、玄米がはやっていますね。"},{"id":{"d":633,"n":2,"t":4},"speaker":"B","text":"ええ。健康に良いらしいですね。"},{"id":{"d":633,"n":3,"t":4},"speaker":"A","text":"味は良いのでしょうか？"},{"id":{"d":633,"n":4,"t":4},"speaker":"B","text":"多くの人が食べているので、おいしいのではないでしょうか？"},{"id":{"d":633,"n":5,"t":4},"speaker":"A","text":"うちも試してみますか？"},{"id":{"d":633,"n":6,"t":4},"speaker":"B","text":"食べてみたいです。"},{"id":{"d":633,"n":7,"t":4},"speaker":"A","text":"後で買い物に行くので、買ってきます。"},{"id":{"d":633,"n":8,"t":4},"speaker":"B","text":"うれしいです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":634,"n":1,"t":4},"speaker":"A","text":"あなたは歯並びが良いですね。"},{"id":{"d":634,"n":2,"t":4},"speaker":"B","text":"歯列矯正をしたのです。"},{"id":{"d":634,"n":3,"t":4},"speaker":"A","text":"何歳くらいのことですか？"},{"id":{"d":634,"n":4,"t":4},"speaker":"B","text":"高校生の頃なので、16歳か17歳の時です。"},{"id":{"d":634,"n":5,"t":4},"speaker":"A","text":"歯並びはだいぶ変わりましたか？"},{"id":{"d":634,"n":6,"t":4},"speaker":"B","text":"そうですね。歯列矯正をする前は歯並びがかなり悪かったです。"},{"id":{"d":634,"n":7,"t":4},"speaker":"A","text":"歯列矯正をして正解でしたね。"},{"id":{"d":634,"n":8,"t":4},"speaker":"B","text":"はい。費用は高かったですが、やって良かったです。"}];
INSERT INTO sentence [{"id":{"d":635,"n":1,"t":4},"speaker":"A","text":"4年2組が学級閉鎖になりました。"},{"id":{"d":635,"n":2,"t":4},"speaker":"B","text":"もしかして、インフルエンザで欠席が多いからですか？"},{"id":{"d":635,"n":3,"t":4},"speaker":"A","text":"そうです。よく分かりましたね。"},{"id":{"d":635,"n":4,"t":4},"speaker":"B","text":"先ほどスーパーで知り合いに会った時、隣の小学校でインフルエンザでの学級閉鎖が相次いでいると聞いたのです。"},{"id":{"d":635,"n":5,"t":4},"speaker":"A","text":"隣の小学校でもインフルエンザが流行しているのですね。"},{"id":{"d":635,"n":6,"t":4},"speaker":"B","text":"そのようです。あなたの息子さんのクラスではどうですか？"},{"id":{"d":635,"n":7,"t":4},"speaker":"A","text":"息子のクラスでは、今日はインフルエンザのために2人欠席でした。"},{"id":{"d":635,"n":8,"t":4},"speaker":"B","text":"そうですか。私たちも感染しないように気を付けなければなりませんね。"}];
INSERT INTO sentence [{"id":{"d":636,"n":1,"t":4},"speaker":"A","text":"寝癖が直りません。"},{"id":{"d":636,"n":2,"t":4},"speaker":"B","text":"どこですか？"},{"id":{"d":636,"n":3,"t":4},"speaker":"A","text":"この襟足の辺りです。"},{"id":{"d":636,"n":4,"t":4},"speaker":"B","text":"確かに少し髪がはねていますね。"},{"id":{"d":636,"n":5,"t":4},"speaker":"A","text":"恥ずかしいので、直したいです。"},{"id":{"d":636,"n":6,"t":4},"speaker":"B","text":"少し濡らしてからドライヤーでセットしたら直ると思いますよ。"},{"id":{"d":636,"n":7,"t":4},"speaker":"A","text":"先ほど試したのですが、あまり変わりませんでした。"},{"id":{"d":636,"n":8,"t":4},"speaker":"B","text":"それなら、今日は髪を結んでみてはどうですか？"}];
INSERT INTO sentence [{"id":{"d":637,"n":1,"t":4},"speaker":"A","text":"今日は、会社で会議中にウトウトしてしまいました。"},{"id":{"d":637,"n":2,"t":4},"speaker":"B","text":"寝不足なのではありませんか？"},{"id":{"d":637,"n":3,"t":4},"speaker":"A","text":"そうかもしれません。家に帰るとついオンラインゲームをしていまい、気が付くと何時間も経っているのです。"},{"id":{"d":637,"n":4,"t":4},"speaker":"B","text":"それで夜更かししてしまうのですね。"},{"id":{"d":637,"n":5,"t":4},"speaker":"A","text":"はい。とても面白いゲームで、なかなかやめられません。"},{"id":{"d":637,"n":6,"t":4},"speaker":"B","text":"でも、仕事に支障が出るのは問題ですし、健康にも良くありませんよ。"},{"id":{"d":637,"n":7,"t":4},"speaker":"A","text":"そうですよね。反省しています。"},{"id":{"d":637,"n":8,"t":4},"speaker":"B","text":"タイマーをセットするなどして、終わりの時間を決めてやってはどうでしょうか？"},{"id":{"d":637,"n":9,"t":4},"speaker":"A","text":"そうですね、今日はそうしてみます。"}];
INSERT INTO sentence [{"id":{"d":638,"n":1,"t":4},"speaker":"A","text":"肌の調子が悪いです。"},{"id":{"d":638,"n":2,"t":4},"speaker":"B","text":"どのように調子が悪いのですか？"},{"id":{"d":638,"n":3,"t":4},"speaker":"A","text":"乾燥してしまっているのか、顔が全体的にカサカサしています。"},{"id":{"d":638,"n":4,"t":4},"speaker":"B","text":"保湿が必要ですね。"},{"id":{"d":638,"n":5,"t":4},"speaker":"A","text":"フェイスパックをしたいのですが、今は持っていないので明日買いに行きます。"},{"id":{"d":638,"n":6,"t":4},"speaker":"B","text":"私が持っているものでよければ、使ってください。"},{"id":{"d":638,"n":7,"t":4},"speaker":"A","text":"頂いてしまってよいのですか？"},{"id":{"d":638,"n":8,"t":4},"speaker":"B","text":"たくさんあるので、構いませんよ。"}];
INSERT INTO sentence [{"id":{"d":639,"n":1,"t":4},"speaker":"A","text":"眼鏡を作りたいのですが、どのフレームがいいか分かりません。"},{"id":{"d":639,"n":2,"t":4},"speaker":"B","text":"フレームの形は、丸型と四角いものとではどちらがお好みですか？"},{"id":{"d":639,"n":3,"t":4},"speaker":"A","text":"四角い方がいいです。"},{"id":{"d":639,"n":4,"t":4},"speaker":"B","text":"材質がいろいろとあるのですが、何かご希望はありますか？"},{"id":{"d":639,"n":5,"t":4},"speaker":"A","text":"なるべく軽いものがいいですね。"},{"id":{"d":639,"n":6,"t":4},"speaker":"B","text":"それでは、こちらに軽量フレームがございます。"},{"id":{"d":639,"n":7,"t":4},"speaker":"A","text":"かけてみてもよいですか？"},{"id":{"d":639,"n":8,"t":4},"speaker":"B","text":"もちろんです。こちらに鏡がありますのでお使いください。"}];
INSERT INTO sentence [{"id":{"d":640,"n":1,"t":4},"speaker":"A","text":"おなかを壊してしまいました。"},{"id":{"d":640,"n":2,"t":4},"speaker":"B","text":"おなかが冷えたのでしょうか？"},{"id":{"d":640,"n":3,"t":4},"speaker":"A","text":"昨日、アイスを食べ過ぎてしまったせいだと思います。"},{"id":{"d":640,"n":4,"t":4},"speaker":"B","text":"薬を飲みますか？"},{"id":{"d":640,"n":5,"t":4},"speaker":"A","text":"もう飲みました。何か食べたいのですが、普通の食事は取れそうにありません。"},{"id":{"d":640,"n":6,"t":4},"speaker":"B","text":"おかゆはどうですか？"},{"id":{"d":640,"n":7,"t":4},"speaker":"A","text":"おかゆなら食べられそうです。"},{"id":{"d":640,"n":8,"t":4},"speaker":"B","text":"すぐに作りますね。"},{"id":{"d":640,"n":9,"t":4},"speaker":"A","text":"ご親切にありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":641,"n":1,"t":4},"speaker":"A","text":"里奈、姿勢が悪いですよ。"},{"id":{"d":641,"n":2,"t":4},"speaker":"B","text":"そうですか？"},{"id":{"d":641,"n":3,"t":4},"speaker":"A","text":"背筋を伸ばして座りなさい。"},{"id":{"d":641,"n":4,"t":4},"speaker":"B","text":"こうですか？"},{"id":{"d":641,"n":5,"t":4},"speaker":"A","text":"そうです。その姿勢を保ってください。"},{"id":{"d":641,"n":6,"t":4},"speaker":"B","text":"このままでは疲れてしまいます。"},{"id":{"d":641,"n":7,"t":4},"speaker":"A","text":"いつもやっていれば、すぐに慣れますよ。"},{"id":{"d":641,"n":8,"t":4},"speaker":"B","text":"分かりました。慣れるまで姿勢を保てるよう気を付けます。"}];
INSERT INTO sentence [{"id":{"d":642,"n":1,"t":4},"speaker":"A","text":"もしもし、三日月旅館です。"},{"id":{"d":642,"n":2,"t":4},"speaker":"B","text":"もしもし、そちらに予約した大塚と申します。"},{"id":{"d":642,"n":3,"t":4},"speaker":"A","text":"大塚様ですね。ご予約は承っております。何かございましたか？"},{"id":{"d":642,"n":4,"t":4},"speaker":"B","text":"予約したメンバーの中に、卵アレルギーの人がいることが分かったのです。"},{"id":{"d":642,"n":5,"t":4},"speaker":"A","text":"そうでしたか。それではアレルギー対応のお食事にする必要がありますね。"},{"id":{"d":642,"n":6,"t":4},"speaker":"B","text":"はい。そのような食事を用意していただくことは可能ですか？"},{"id":{"d":642,"n":7,"t":4},"speaker":"A","text":"もちろん可能です。ただ、その方の宿泊代に追加料金がかかってしまいますが、よろしいでしょうか？"},{"id":{"d":642,"n":8,"t":4},"speaker":"B","text":"はい、お支払いしますので大丈夫です。それではよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":643,"n":1,"t":4},"speaker":"A","text":"暑くて寝苦しいですね。"},{"id":{"d":643,"n":2,"t":4},"speaker":"B","text":"そうですね。エアコンをつけましょうか？"},{"id":{"d":643,"n":3,"t":4},"speaker":"A","text":"つけたままだと体調が悪くなりそうなので、タイマーを設定してください。"},{"id":{"d":643,"n":4,"t":4},"speaker":"B","text":"どのくらいで止まるように設定しますか？"},{"id":{"d":643,"n":5,"t":4},"speaker":"A","text":"2時間でお願いします。"},{"id":{"d":643,"n":6,"t":4},"speaker":"B","text":"分かりました。"},{"id":{"d":643,"n":7,"t":4},"speaker":"A","text":"部屋の空気が循環するように、扇風機も使いましょう。"},{"id":{"d":643,"n":8,"t":4},"speaker":"B","text":"そうですね。リビングから持ってきます。"}];
INSERT INTO sentence [{"id":{"d":644,"n":1,"t":4},"speaker":"A","text":"視力検査をします。機械をのぞき込んでください。丸い輪の途切れている方向を答えてくださいね。"},{"id":{"d":644,"n":2,"t":4},"speaker":"B","text":"分かりました。お願いします。"},{"id":{"d":644,"n":3,"t":4},"speaker":"A","text":"これはどうですか？"},{"id":{"d":644,"n":4,"t":4},"speaker":"B","text":"右下です。"},{"id":{"d":644,"n":5,"t":4},"speaker":"A","text":"次にこれはどうでしょう？"},{"id":{"d":644,"n":6,"t":4},"speaker":"B","text":"分かりません。"},{"id":{"d":644,"n":7,"t":4},"speaker":"A","text":"去年よりかなり視力が落ちていますよ。"},{"id":{"d":644,"n":8,"t":4},"speaker":"B","text":"自分でもそのような気がしていました。"}];
INSERT INTO sentence [{"id":{"d":645,"n":1,"t":4},"speaker":"A","text":"大相撲の横綱が明日から休場だそうです。"},{"id":{"d":645,"n":2,"t":4},"speaker":"B","text":"何かあったのでしょうか？"},{"id":{"d":645,"n":3,"t":4},"speaker":"A","text":"今日の取組でけがをしてしまったようです。"},{"id":{"d":645,"n":4,"t":4},"speaker":"B","text":"先場所も膝をけがしていましたよね。"},{"id":{"d":645,"n":5,"t":4},"speaker":"A","text":"そうですね。ようやく完治して今場所に臨んだのに、またけがとは無念でしょうね。"},{"id":{"d":645,"n":6,"t":4},"speaker":"B","text":"今度はどこをけがしたのでしょうか？"},{"id":{"d":645,"n":7,"t":4},"speaker":"A","text":"肩です。転倒した時に打ってしまったらしいです。"},{"id":{"d":645,"n":8,"t":4},"speaker":"B","text":"今場所はまだ無敗だったのに、残念ですね。"}];
INSERT INTO sentence [{"id":{"d":646,"n":1,"t":4},"speaker":"A","text":"私は毎朝納豆を食べます。"},{"id":{"d":646,"n":2,"t":4},"speaker":"B","text":"納豆が好きなのですね。"},{"id":{"d":646,"n":3,"t":4},"speaker":"A","text":"好きですが、毎朝食べるのは健康のためです。"},{"id":{"d":646,"n":4,"t":4},"speaker":"B","text":"納豆は健康に良いのですか？"},{"id":{"d":646,"n":5,"t":4},"speaker":"A","text":"多くの栄養素が含まれていて、とても体に良いです。"},{"id":{"d":646,"n":6,"t":4},"speaker":"B","text":"そうなのですね。でも毎朝食べると飽きてしまいそうです。"},{"id":{"d":646,"n":7,"t":4},"speaker":"A","text":"調味料を変えたり、のりやしらすを加えたりと工夫しているので、大丈夫です。"},{"id":{"d":646,"n":8,"t":4},"speaker":"B","text":"なるほど。私も積極的に納豆を食べるように心掛けます。"}];
INSERT INTO sentence [{"id":{"d":647,"n":1,"t":4},"speaker":"A","text":"手荒れがひどいです。"},{"id":{"d":647,"n":2,"t":4},"speaker":"B","text":"冬は空気が乾燥しているので、そのせいでしょうね。"},{"id":{"d":647,"n":3,"t":4},"speaker":"A","text":"そうですね。毎年冬は手荒れに悩まされます。"},{"id":{"d":647,"n":4,"t":4},"speaker":"B","text":"ハンドクリームは塗っていますか？"},{"id":{"d":647,"n":5,"t":4},"speaker":"A","text":"塗ろうと思って毎年買ってはいるのですが、つい塗り忘れてしまうことが多いです。"},{"id":{"d":647,"n":6,"t":4},"speaker":"B","text":"せっかく持っているのなら、小まめに塗った方がいいですよ。"},{"id":{"d":647,"n":7,"t":4},"speaker":"A","text":"そうですよね。すぐに取れる所に置いておきます。"},{"id":{"d":647,"n":8,"t":4},"speaker":"B","text":"それがいいですね。習慣になると面倒ではなくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":648,"n":1,"t":4},"speaker":"A","text":"そろそろ犬の散歩の時間ですよ。"},{"id":{"d":648,"n":2,"t":4},"speaker":"B","text":"そうなのですが、まだ外が暑いので犬の体調が心配です。"},{"id":{"d":648,"n":3,"t":4},"speaker":"A","text":"確かに、日差しが強くて暑そうですね。"},{"id":{"d":648,"n":4,"t":4},"speaker":"B","text":"気温が30度を超えていると思います。犬も熱中症になってしまうかもしれません。"},{"id":{"d":648,"n":5,"t":4},"speaker":"A","text":"もう少し涼しくなってからにした方がいいですね。"},{"id":{"d":648,"n":6,"t":4},"speaker":"B","text":"はい。1時間くらい待ってみます。"},{"id":{"d":648,"n":7,"t":4},"speaker":"A","text":"真夏は散歩する時間にも気を使いますね。"},{"id":{"d":648,"n":8,"t":4},"speaker":"B","text":"本当ですね。早く秋になってほしいです。"}];
INSERT INTO sentence [{"id":{"d":649,"n":1,"t":4},"speaker":"A","text":"まだ寝ているのですか？会社に遅れますよ。"},{"id":{"d":649,"n":2,"t":4},"speaker":"B","text":"それが、熱があるようなのです。"},{"id":{"d":649,"n":3,"t":4},"speaker":"A","text":"それは大変です。体温計を持ってきますね。"},{"id":{"d":649,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。今日は会社へは行けそうにありません。"},{"id":{"d":649,"n":5,"t":4},"speaker":"A","text":"私から、会社へ連絡しておきましょうか？"},{"id":{"d":649,"n":6,"t":4},"speaker":"B","text":"いいえ、自分で電話できます。すみませんが、私の携帯電話を取ってくれますか？"},{"id":{"d":649,"n":7,"t":4},"speaker":"A","text":"分かりました。体温計と一緒に持ってきます。"},{"id":{"d":649,"n":8,"t":4},"speaker":"B","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":650,"n":1,"t":4},"speaker":"A","text":"ボクシングの桃園選手が引退するそうです。"},{"id":{"d":650,"n":2,"t":4},"speaker":"B","text":"そうなのですか？理由を知っていますか？"},{"id":{"d":650,"n":3,"t":4},"speaker":"A","text":"この間の試合でのけがが深刻で、選手として復帰することができないらしいです。"},{"id":{"d":650,"n":4,"t":4},"speaker":"B","text":"それほどひどいけがだったのですね。"},{"id":{"d":650,"n":5,"t":4},"speaker":"A","text":"ニュースによると、そのけがのために視力が回復しないそうです。"},{"id":{"d":650,"n":6,"t":4},"speaker":"B","text":"それはボクシング選手にとって致命的ですね。"},{"id":{"d":650,"n":7,"t":4},"speaker":"A","text":"世界チャンピオンになることも期待されていたのに、残念です。"},{"id":{"d":650,"n":8,"t":4},"speaker":"B","text":"そうですね。本人もショックでしょうね。"}];
INSERT INTO sentence [{"id":{"d":651,"n":1,"t":4},"speaker":"A","text":"少し背中を見てくれませんか？"},{"id":{"d":651,"n":2,"t":4},"speaker":"B","text":"よいですよ。赤い湿疹ができています。どうしたのですか？"},{"id":{"d":651,"n":3,"t":4},"speaker":"A","text":"あせもができてしまったようなのです。"},{"id":{"d":651,"n":4,"t":4},"speaker":"B","text":"痛いですか？"},{"id":{"d":651,"n":5,"t":4},"speaker":"A","text":"かゆいような、痛いような感じです。この薬を塗ってもらえますか？"},{"id":{"d":651,"n":6,"t":4},"speaker":"B","text":"分かりました。あせもの薬を持っているのですね。"},{"id":{"d":651,"n":7,"t":4},"speaker":"A","text":"はい。よくあせもができるので、いつもこの薬を使っています。"},{"id":{"d":651,"n":8,"t":4},"speaker":"B","text":"早く治るといいですね。それでは塗りますね。"}];
INSERT INTO sentence [{"id":{"d":652,"n":1,"t":4},"speaker":"A","text":"前髪はどのようにしますか？"},{"id":{"d":652,"n":2,"t":4},"speaker":"B","text":"悩みます。前髪を下ろすか横に流すかでは、私にはどちらが似合うでしょうか？"},{"id":{"d":652,"n":3,"t":4},"speaker":"A","text":"どちらも似合いますが、実際にやってみましょう。下ろすとこういう感じです。"},{"id":{"d":652,"n":4,"t":4},"speaker":"B","text":"いつも通りという印象ですね。"},{"id":{"d":652,"n":5,"t":4},"speaker":"A","text":"横に流すとこういう感じです。"},{"id":{"d":652,"n":6,"t":4},"speaker":"B","text":"こちらの方がしっくりくるので、横に流してください。"},{"id":{"d":652,"n":7,"t":4},"speaker":"A","text":"分かりました。長さはどうしますか？"},{"id":{"d":652,"n":8,"t":4},"speaker":"B","text":"このままの長さでよいです。"}];
INSERT INTO sentence [{"id":{"d":653,"n":1,"t":4},"speaker":"A","text":"隼人君は、おしゃべりが上手ですね。"},{"id":{"d":653,"n":2,"t":4},"speaker":"B","text":"最近はいろいろな言葉を話すようになりました。"},{"id":{"d":653,"n":3,"t":4},"speaker":"A","text":"うちの正吾は、まだ全然しゃべりません。"},{"id":{"d":653,"n":4,"t":4},"speaker":"B","text":"正吾君は隼人より半年くらい後に生まれたので、仕方ありませんよ。"},{"id":{"d":653,"n":5,"t":4},"speaker":"A","text":"でも隼人君は半年前に、もうおしゃべりしていましたよね。"},{"id":{"d":653,"n":6,"t":4},"speaker":"B","text":"その頃は「ママ」の一言くらいですよ。"},{"id":{"d":653,"n":7,"t":4},"speaker":"A","text":"そうですが、やはり一言も話さないのは心配です。"},{"id":{"d":653,"n":8,"t":4},"speaker":"B","text":"個人差があるので、大丈夫ですよ。もし心配なら、健診の時に相談してみてはどうですか？"},{"id":{"d":653,"n":9,"t":4},"speaker":"A","text":"そうですね。次の健診で相談してみます。"}];
INSERT INTO sentence [{"id":{"d":654,"n":1,"t":4},"speaker":"A","text":"足首が痛みます。"},{"id":{"d":654,"n":2,"t":4},"speaker":"B","text":"けがですか？"},{"id":{"d":654,"n":3,"t":4},"speaker":"A","text":"いいえ。足首を骨折して以来、時々痛むのです。"},{"id":{"d":654,"n":4,"t":4},"speaker":"B","text":"骨折は治っているのに痛みがあるのですか？"},{"id":{"d":654,"n":5,"t":4},"speaker":"A","text":"そうなのです。特に今日のように天気が悪い日に痛むことが多いです。"},{"id":{"d":654,"n":6,"t":4},"speaker":"B","text":"一度、病院で診察してもらった方がいいですよ。"},{"id":{"d":654,"n":7,"t":4},"speaker":"A","text":"病院で診てもらったこともあるのですが、よくあることだそうで、様子を見るしかないと言われました。"},{"id":{"d":654,"n":8,"t":4},"speaker":"B","text":"それは困りますね。冷やすか温めるかして、痛みが軽くならないでしょうか？"},{"id":{"d":654,"n":9,"t":4},"speaker":"A","text":"どちらかというと冷やす方が痛みが和らぐ気がするので、冷やしてみます。"}];
INSERT INTO sentence [{"id":{"d":655,"n":1,"t":4},"speaker":"A","text":"朝ご飯にはどのような物を食べますか？"},{"id":{"d":655,"n":2,"t":4},"speaker":"B","text":"トーストとヨーグルトを食べることが多いです。"},{"id":{"d":655,"n":3,"t":4},"speaker":"A","text":"野菜は食べないのですか？"},{"id":{"d":655,"n":4,"t":4},"speaker":"B","text":"食べません。あなたはどのような朝食ですか？"},{"id":{"d":655,"n":5,"t":4},"speaker":"A","text":"私は玄米とおみそ汁と目玉焼とサラダが定番です。"},{"id":{"d":655,"n":6,"t":4},"speaker":"B","text":"健康的ですね。"},{"id":{"d":655,"n":7,"t":4},"speaker":"A","text":"そうですね、栄養バランスには気を配っています。"},{"id":{"d":655,"n":8,"t":4},"speaker":"B","text":"私は朝が弱いので、しっかり朝食を作れる時間に起きることができません。"}];
INSERT INTO sentence [{"id":{"d":656,"n":1,"t":4},"speaker":"A","text":"サウナは好きですか？"},{"id":{"d":656,"n":2,"t":4},"speaker":"B","text":"割と好きです。"},{"id":{"d":656,"n":3,"t":4},"speaker":"A","text":"近所に新しくサウナのお店ができたのですが、一緒に行きませんか？"},{"id":{"d":656,"n":4,"t":4},"speaker":"B","text":"いいですよ。そのサウナは何か特徴があるのでしょうか？"},{"id":{"d":656,"n":5,"t":4},"speaker":"A","text":"6種類のサウナがあるそうです。"},{"id":{"d":656,"n":6,"t":4},"speaker":"B","text":"それは楽しめそうですね。"},{"id":{"d":656,"n":7,"t":4},"speaker":"A","text":"レストランもあるので、サウナの後で食事をしましょう。次の週末はどうですか？"},{"id":{"d":656,"n":8,"t":4},"speaker":"B","text":"いいですね。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":657,"n":1,"t":4},"speaker":"A","text":"インフルエンザの予防接種をします。"},{"id":{"d":657,"n":2,"t":4},"speaker":"B","text":"お願いします。"},{"id":{"d":657,"n":3,"t":4},"speaker":"A","text":"今日の体調はいかがですか？"},{"id":{"d":657,"n":4,"t":4},"speaker":"B","text":"いつも通りです。"},{"id":{"d":657,"n":5,"t":4},"speaker":"A","text":"分かりました。では、注射を打ちますね。右腕と左腕のどちらがいいでしょうか？"},{"id":{"d":657,"n":6,"t":4},"speaker":"B","text":"右腕がいいです。"},{"id":{"d":657,"n":7,"t":4},"speaker":"A","text":"袖をまくって肩まで出してください。"},{"id":{"d":657,"n":8,"t":4},"speaker":"B","text":"はい。出しました。"}];
INSERT INTO sentence [{"id":{"d":658,"n":1,"t":4},"speaker":"A","text":"あなたは背が高くていいですね。"},{"id":{"d":658,"n":2,"t":4},"speaker":"B","text":"そうですか？私は背が高いことがコンプレックスです。"},{"id":{"d":658,"n":3,"t":4},"speaker":"A","text":"それは失礼しました。でも、私は高身長に憧れます。"},{"id":{"d":658,"n":4,"t":4},"speaker":"B","text":"子どもの頃から背が高いと言われ続けてきたせいかもしれません。"},{"id":{"d":658,"n":5,"t":4},"speaker":"A","text":"そうでしたか。私もつい言ってしまいましたが、褒め言葉ですよ。"},{"id":{"d":658,"n":6,"t":4},"speaker":"B","text":"それは分かっているので大丈夫ですよ。私はあなたのように小柄に生まれたかったです。"},{"id":{"d":658,"n":7,"t":4},"speaker":"A","text":"うちは家族全員が小柄なのですよ。"},{"id":{"d":658,"n":8,"t":4},"speaker":"B","text":"私の家族は全員が背が高いです。やはり遺伝の要素は大きいのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":659,"n":1,"t":4},"speaker":"A","text":"部長、気分が悪いです。"},{"id":{"d":659,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":659,"n":3,"t":4},"speaker":"A","text":"グラウンドをランニングしていたら、だんだん気持ちが悪くなってきました。"},{"id":{"d":659,"n":4,"t":4},"speaker":"B","text":"このベンチに座って、休んでください。"},{"id":{"d":659,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。すみません、喉が渇いたので私の水筒を持ってきてほしいです。"},{"id":{"d":659,"n":6,"t":4},"speaker":"B","text":"これですね。どうぞ。"},{"id":{"d":659,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。少しここで休憩しています。"},{"id":{"d":659,"n":8,"t":4},"speaker":"B","text":"無理しないでくださいね。回復しないようなら、今日の部活は早退してよいですよ。"}];
INSERT INTO sentence [{"id":{"d":660,"n":1,"t":4},"speaker":"A","text":"ママ、転んでしまいました。"},{"id":{"d":660,"n":2,"t":4},"speaker":"B","text":"けがはありませんか？"},{"id":{"d":660,"n":3,"t":4},"speaker":"A","text":"膝を擦りむいてしまいました。"},{"id":{"d":660,"n":4,"t":4},"speaker":"B","text":"少し血が出ていますね。水で洗ってから消毒しましょう。"},{"id":{"d":660,"n":5,"t":4},"speaker":"A","text":"傷に染みそうで嫌です。"},{"id":{"d":660,"n":6,"t":4},"speaker":"B","text":"放っておくとばい菌が入ってしまいます。このくらいの傷なら、きっと染みませんよ。"},{"id":{"d":660,"n":7,"t":4},"speaker":"A","text":"そうでしょうか？そっと処置してくださいね。"},{"id":{"d":660,"n":8,"t":4},"speaker":"B","text":"分かりました。ズボンをまくってください。"}];
INSERT INTO sentence [{"id":{"d":661,"n":1,"t":4},"speaker":"A","text":"夕食の献立が決まりません。"},{"id":{"d":661,"n":2,"t":4},"speaker":"B","text":"最近はどのような夕食でしたか？"},{"id":{"d":661,"n":3,"t":4},"speaker":"A","text":"料理をする時間がなくて、お弁当や総菜を買って食べることが多かったです。"},{"id":{"d":661,"n":4,"t":4},"speaker":"B","text":"それでは今日は健康的な献立がいいですね。"},{"id":{"d":661,"n":5,"t":4},"speaker":"A","text":"何か案はありますか？"},{"id":{"d":661,"n":6,"t":4},"speaker":"B","text":"野菜をたくさん使ったサラダと八宝菜はどうですか？"},{"id":{"d":661,"n":7,"t":4},"speaker":"A","text":"八宝菜は大好物です。ちょうど家に白菜があるので、それにします。"},{"id":{"d":661,"n":8,"t":4},"speaker":"B","text":"多めに作れば、明日も食べられますね。"},{"id":{"d":661,"n":9,"t":4},"speaker":"A","text":"はい。作り置きしたいと思います。"}];
INSERT INTO sentence [{"id":{"d":662,"n":1,"t":4},"speaker":"A","text":"ミキサーを買いました。"},{"id":{"d":662,"n":2,"t":4},"speaker":"B","text":"家でジュースを作るためですか？"},{"id":{"d":662,"n":3,"t":4},"speaker":"A","text":"はい。毎朝にグリーンスムージーを作って飲むためです。"},{"id":{"d":662,"n":4,"t":4},"speaker":"B","text":"グリーンスムージーはダイエットに効果的だとよく聞きますね。"},{"id":{"d":662,"n":5,"t":4},"speaker":"A","text":"そうなのです。朝食にグリーンスムージーを飲むことを習慣にするつもりです。"},{"id":{"d":662,"n":6,"t":4},"speaker":"B","text":"私は野菜が嫌いなので、飲めそうにありません。"},{"id":{"d":662,"n":7,"t":4},"speaker":"A","text":"野菜だけではなく果物や蜂蜜も入れるので、飲みやすいですよ。"},{"id":{"d":662,"n":8,"t":4},"speaker":"B","text":"そうなのですか？よかったら今度飲ませてください。"},{"id":{"d":662,"n":9,"t":4},"speaker":"A","text":"もちろんいいですよ。"}];
INSERT INTO sentence [{"id":{"d":663,"n":1,"t":4},"speaker":"A","text":"年齢のせいでしょうか。この頃は目が悪くなってきたような気がします。"},{"id":{"d":663,"n":2,"t":4},"speaker":"B","text":"私は随分前から老眼になってきています。"},{"id":{"d":663,"n":3,"t":4},"speaker":"A","text":"目には、アントシアニンが効くといわれていますよね。"},{"id":{"d":663,"n":4,"t":4},"speaker":"B","text":"そうですね。アントシアニンは、ベリー類やイモ類に多量に含まれているようです。"},{"id":{"d":663,"n":5,"t":4},"speaker":"A","text":"アントシアニン入りの健康食品もよくテレビで見かけますね。試したことはありますか？"},{"id":{"d":663,"n":6,"t":4},"speaker":"B","text":"試供品をいくつか試したことがあります。"},{"id":{"d":663,"n":7,"t":4},"speaker":"A","text":"効果はありましたか？"},{"id":{"d":663,"n":8,"t":4},"speaker":"B","text":"効果のほどは分かりません。私は薬としてではなく、目に与える栄養としてアントシアニンを捉えています。"},{"id":{"d":663,"n":9,"t":4},"speaker":"A","text":"目の状態は自分で判断せず、眼科に相談に行った方がいいでしょうか？"},{"id":{"d":663,"n":10,"t":4},"speaker":"B","text":"そうすべきだと思います。よろしければ私の主治医を紹介しましょう。"}];
INSERT INTO sentence [{"id":{"d":664,"n":1,"t":4},"speaker":"A","text":"コンドロイチンとは何ですか？"},{"id":{"d":664,"n":2,"t":4},"speaker":"B","text":"グルコサミンとともに軟骨の構成要素で、関節痛や腰痛の薬にもなっています。どうして突然コンドロイチンのことを聞いたのですか？"},{"id":{"d":664,"n":3,"t":4},"speaker":"A","text":"母が毎日飲んでいるのですが、最近は私にも飲むように勧めるのです。"},{"id":{"d":664,"n":4,"t":4},"speaker":"B","text":"どこか痛いところでもあるのですか？"},{"id":{"d":664,"n":5,"t":4},"speaker":"A","text":"この間、ぎっくり腰になってしまい、数日間会社を休んだことがありました。母は、それを心配しているのかもしれません。"},{"id":{"d":664,"n":6,"t":4},"speaker":"B","text":"それは大変でしたね。今は治ったのですか？"},{"id":{"d":664,"n":7,"t":4},"speaker":"A","text":"もう大丈夫です。やはりコンドロイチンが足りなかったのでしょうか？"},{"id":{"d":664,"n":8,"t":4},"speaker":"B","text":"それはあまり関係ないと思います。仕事や年齢、普段の生活習慣が原因だと思います。"},{"id":{"d":664,"n":9,"t":4},"speaker":"A","text":"もう、あの痛みは味わいたくありません。どうすればよいでしょうか？"},{"id":{"d":664,"n":10,"t":4},"speaker":"B","text":"日頃から自分に合ったストレッチをするのはいかがでしょうか？"}];
INSERT INTO sentence [{"id":{"d":665,"n":1,"t":4},"speaker":"A","text":"今までにどのような予防接種を受けてきたかを覚えていますか？"},{"id":{"d":665,"n":2,"t":4},"speaker":"B","text":"全部は覚えていません。インフルエンザ、おたふくかぜ、水ぼうそうの予防接種をしてことは覚えています。"},{"id":{"d":665,"n":3,"t":4},"speaker":"A","text":"幼いうちに接種することが多いので、全部は覚えていなくて当然かもしれませんね。"},{"id":{"d":665,"n":4,"t":4},"speaker":"B","text":"他にはどのような予防接種があるのですか？"},{"id":{"d":665,"n":5,"t":4},"speaker":"A","text":"ポリオ、日本脳炎、風しん、ジフテリア、破傷風などがあります。"},{"id":{"d":665,"n":6,"t":4},"speaker":"B","text":"聞いたことのある病気ばかりですが、やはり、予防接種をした記憶はありません。"},{"id":{"d":665,"n":7,"t":4},"speaker":"A","text":"多くの予防接種は法律で義務化されているので、記憶にはなくとも、恐らく接種しているはずです。"},{"id":{"d":665,"n":8,"t":4},"speaker":"B","text":"幼少期にたくさんの予防接種をするのは大変そうですね。"},{"id":{"d":665,"n":9,"t":4},"speaker":"A","text":"何度も接種しなくても済むように、3種類や4種類の成分を含む混合ワクチンもあるようです。"}];
INSERT INTO sentence [{"id":{"d":666,"n":1,"t":4},"speaker":"A","text":"家族でキャンプに行くのですが、持って行くといい薬は何でしょうか？"},{"id":{"d":666,"n":2,"t":4},"speaker":"B","text":"キャンプ地は山ですか？それとも海ですか？"},{"id":{"d":666,"n":3,"t":4},"speaker":"A","text":"2泊3日で山に行くつもりです。"},{"id":{"d":666,"n":4,"t":4},"speaker":"B","text":"テントに泊まるのですか？"},{"id":{"d":666,"n":5,"t":4},"speaker":"A","text":"私と娘はコテージに、夫と息子はキャンピングカーに泊まります。"},{"id":{"d":666,"n":6,"t":4},"speaker":"B","text":"そうなのですね。では、かゆみ止め、酔い止め、頭痛薬、胃薬、解熱剤、それに消毒薬とばんそうこうがあれば十分なのではないでしょうか？"},{"id":{"d":666,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。参考になります。虫よけもあった方がいいですよね？"},{"id":{"d":666,"n":8,"t":4},"speaker":"B","text":"確かにそうですね。また、もし持病がある方がいればいつも飲んでいる薬も忘れないでください。"},{"id":{"d":666,"n":9,"t":4},"speaker":"A","text":"分かりました。家族で行く初めてのキャンプなので、いろいろと心配してしまいます。"},{"id":{"d":666,"n":10,"t":4},"speaker":"B","text":"キャンピングカーで行くのなら、緊急時にはすぐに帰ることもできますよ。楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":667,"n":1,"t":4},"speaker":"A","text":"足首を捻挫したそうですね。"},{"id":{"d":667,"n":2,"t":4},"speaker":"B","text":"はい、捻挫をしてしまいました。しばらくの間は、右足はサンダルを履いて出社させていただきます。"},{"id":{"d":667,"n":3,"t":4},"speaker":"A","text":"包帯とサポーターが痛々しいです。どこでけがをしたのですか？"},{"id":{"d":667,"n":4,"t":4},"speaker":"B","text":"公園です。"},{"id":{"d":667,"n":5,"t":4},"speaker":"A","text":"公園ですか？"},{"id":{"d":667,"n":6,"t":4},"speaker":"B","text":"はい。おとといのことですが、散歩の途中に中学校の時の同級生3人に偶然会ったのです。彼らからバスケットボールを一緒にしないかと誘われて、公園のコートで試合をしたのです。"},{"id":{"d":667,"n":7,"t":4},"speaker":"A","text":"そういえば、高校までバスケットボール部だったと以前に言っていましたね。"},{"id":{"d":667,"n":8,"t":4},"speaker":"B","text":"そうです。その試合で、フェイントをかけて強引に相手を抜こうとした時に、足を捻ってしまったのです。"},{"id":{"d":667,"n":9,"t":4},"speaker":"A","text":"それはお気の毒です。"},{"id":{"d":667,"n":10,"t":4},"speaker":"B","text":"運動前にせめてストレッチくらいはしておくべきでした。"}];
INSERT INTO sentence [{"id":{"d":668,"n":1,"t":4},"speaker":"A","text":"気分が悪いのですか？"},{"id":{"d":668,"n":2,"t":4},"speaker":"B","text":"どうか心配しないでください。徹夜が続いているだけです。"},{"id":{"d":668,"n":3,"t":4},"speaker":"A","text":"だいぶお疲れの様子ですよ。"},{"id":{"d":668,"n":4,"t":4},"speaker":"B","text":"先ほど顔を洗おうとして鏡を見たのですが、我ながら疲れた顔をしていました。"},{"id":{"d":668,"n":5,"t":4},"speaker":"A","text":"なぜ徹夜が続いているのですか？"},{"id":{"d":668,"n":6,"t":4},"speaker":"B","text":"卒業論文の参考文献と資料の数が多く、まとめるのに時間がかかっているのです。"},{"id":{"d":668,"n":7,"t":4},"speaker":"A","text":"卒業論文は大学生活の集大成ですものね。"},{"id":{"d":668,"n":8,"t":4},"speaker":"B","text":"私は大学院に進学するので、卒業論文もしっかり書きたいと思っています。"},{"id":{"d":668,"n":9,"t":4},"speaker":"A","text":"意気込みは分かりますが、倒れてしまっては何にもなりません。どうぞご自愛ください。"},{"id":{"d":668,"n":10,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":669,"n":1,"t":4},"speaker":"A","text":"テレビでよく見るスポーツ番組はありますか？"},{"id":{"d":669,"n":2,"t":4},"speaker":"B","text":"マラソン中継が好きでよく見ています。"},{"id":{"d":669,"n":3,"t":4},"speaker":"A","text":"私には、2時間半にわたりただ走るだけのスポーツであるように感じてしまうのですが、ずっと見ていて飽きませんか？"},{"id":{"d":669,"n":4,"t":4},"speaker":"B","text":"ただ走っているだけではありません。序盤の選手同士の駆け引きが面白いですよ。"},{"id":{"d":669,"n":5,"t":4},"speaker":"A","text":"そうなのですか？選手が接近して走っていると、ぶつかるのではないかと心配です。"},{"id":{"d":669,"n":6,"t":4},"speaker":"B","text":"実際に接触してしまう選手もいますし、給水所で転倒する選手もいますが、まさに、それが駆け引きです。終盤のスパートをかけるタイミングなども見応えのある駆け引きの一つです。"},{"id":{"d":669,"n":7,"t":4},"speaker":"A","text":"速く走ることだけでなく、そのような駆け引きまで必要なのですね。選手たちは、きっと大変な練習をしてきているのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":670,"n":1,"t":4},"speaker":"A","text":"YouTubeをよく見ているようですが、何の動画を見ているのですか？"},{"id":{"d":670,"n":2,"t":4},"speaker":"B","text":"元プロ野球選手の出演している動画を見ています。"},{"id":{"d":670,"n":3,"t":4},"speaker":"A","text":"野球がそれほどお好きでしたか？"},{"id":{"d":670,"n":4,"t":4},"speaker":"B","text":"昔は野球が好きでした。動画では私が好きだった往年の名選手が、現役時代の面白い裏話をしています。"},{"id":{"d":670,"n":5,"t":4},"speaker":"A","text":"例えばどのような話が面白かったですか？"},{"id":{"d":670,"n":6,"t":4},"speaker":"B","text":"チーム内の人間関係や他人とは違った変化球の投げ方などは、当時は分からなかったことなので面白いです。"},{"id":{"d":670,"n":7,"t":4},"speaker":"A","text":"私もいくつか見ましたが、元選手同士で話すことで面白さが増しているように思います。"},{"id":{"d":670,"n":8,"t":4},"speaker":"B","text":"勝負の世界から離れると同じ野球仲間のように感じるからでしょう。"},{"id":{"d":670,"n":9,"t":4},"speaker":"A","text":"私は元監督による日本シリーズの裏話を聞いてみたいです。"}];
INSERT INTO sentence [{"id":{"d":671,"n":1,"t":4},"speaker":"A","text":"定期的に健康診断をしていますか？"},{"id":{"d":671,"n":2,"t":4},"speaker":"B","text":"地方自治体の健康診断には行っています。"},{"id":{"d":671,"n":3,"t":4},"speaker":"A","text":"健診項目は何ですか？"},{"id":{"d":671,"n":4,"t":4},"speaker":"B","text":"診察、身体計測、血圧測定、血液検査、尿検査です。"},{"id":{"d":671,"n":5,"t":4},"speaker":"A","text":"がん検診はありますか？"},{"id":{"d":671,"n":6,"t":4},"speaker":"B","text":"有料でのがん検診が何種類かあるはずです。"},{"id":{"d":671,"n":7,"t":4},"speaker":"A","text":"がん検診の費用はどのくらいかかるのでしょうか？"},{"id":{"d":671,"n":8,"t":4},"speaker":"B","text":"詳しくは分かりませんが、種類別でそれぞれ数百円から数千円程度だと思います。"},{"id":{"d":671,"n":9,"t":4},"speaker":"A","text":"女性向けのものを全て検診すると1万円くらいになるのでしょうか？"},{"id":{"d":671,"n":10,"t":4},"speaker":"B","text":"女性は受診可能な検診の種類が男性よりも多いので、そのくらいになるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":672,"n":1,"t":4},"speaker":"A","text":"近頃は風呂上がりに肌が乾燥しているように感じます。"},{"id":{"d":672,"n":2,"t":4},"speaker":"B","text":"私も肌が乾燥していると感じる時があって、ついこすってしまうことがあります。"},{"id":{"d":672,"n":3,"t":4},"speaker":"A","text":"肌は、こすらない方がいいようですよ。"},{"id":{"d":672,"n":4,"t":4},"speaker":"B","text":"私も友人から肌はこすらない方がいいと聞いたのですが、乾燥が気になってついこすってしまいます。それで、ますます乾燥がひどくなってしまって。"},{"id":{"d":672,"n":5,"t":4},"speaker":"A","text":"最近、私は入浴後に乳液を塗るようにしましたよ。"},{"id":{"d":672,"n":6,"t":4},"speaker":"B","text":"乾燥肌は改善されましたか？"},{"id":{"d":672,"n":7,"t":4},"speaker":"A","text":"まだ数日ですが、だいぶ改善されたように感じます。"},{"id":{"d":672,"n":8,"t":4},"speaker":"B","text":"何かお薦めの乳液はありますか？"},{"id":{"d":672,"n":9,"t":4},"speaker":"A","text":"私は安くて量の多いものを選んでいます。自分の肌にどのような乳液が合うかを知りたいときは、薬局で相談するといいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":673,"n":1,"t":4},"speaker":"A","text":"この季節になると発疹が出やすくなるのですが、何が原因なのでしょうか？"},{"id":{"d":673,"n":2,"t":4},"speaker":"B","text":"季節的なものでしたら、乾燥発疹かもしれません。"},{"id":{"d":673,"n":3,"t":4},"speaker":"A","text":"乾燥発疹とはどのようなものですか？"},{"id":{"d":673,"n":4,"t":4},"speaker":"B","text":"乾燥発疹は、空気の乾燥する時期になると出やすくなるそうです。"},{"id":{"d":673,"n":5,"t":4},"speaker":"A","text":"かゆくてうっかりかいてしまうので、どんどん悪化してしまいます。"},{"id":{"d":673,"n":6,"t":4},"speaker":"B","text":"なるべくかかないようにして軟こうなどを塗った方がいいと思いますよ。"},{"id":{"d":673,"n":7,"t":4},"speaker":"A","text":"どのような軟こうを選べばよいでしょうか？"},{"id":{"d":673,"n":8,"t":4},"speaker":"B","text":"発疹用の軟こうで十分だと思いますが、皮膚科で診てもらうのも一つの方法ですよ。"},{"id":{"d":673,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。まずは家の薬箱に軟こうがあるかどうかを探してみます。"}];
INSERT INTO sentence [{"id":{"d":674,"n":1,"t":4},"speaker":"A","text":"足を引きずっていますが、どうかしたのですか？"},{"id":{"d":674,"n":2,"t":4},"speaker":"B","text":"先ほどドアの角に足の小指をぶつけてしまいました。"},{"id":{"d":674,"n":3,"t":4},"speaker":"A","text":"私も経験があります。なぜ小指をぶつけただけなのにあれほど痛いのでしょうね。"},{"id":{"d":674,"n":4,"t":4},"speaker":"B","text":"骨が折れたかと思えるような痛さです。"},{"id":{"d":674,"n":5,"t":4},"speaker":"A","text":"しばらく身動きができない時さえありますよね。"},{"id":{"d":674,"n":6,"t":4},"speaker":"B","text":"先ほど小指をドアにぶつけた直後がそうでした。今はもうだいぶ楽になってきました。"},{"id":{"d":674,"n":7,"t":4},"speaker":"A","text":"ある時期を過ぎると痛みがうそのように消えるのも不思議なところです。"},{"id":{"d":674,"n":8,"t":4},"speaker":"B","text":"そうですね。結局のところは薬を塗ったり、ばんそうこうを貼ったりしたことはありません。"},{"id":{"d":674,"n":9,"t":4},"speaker":"A","text":"足の小指は感覚神経が鋭い部位なのかもしれません。"}];
INSERT INTO sentence [{"id":{"d":675,"n":1,"t":4},"speaker":"A","text":"昔に比べると、虫歯のある子どもは随分減っているそうです。"},{"id":{"d":675,"n":2,"t":4},"speaker":"B","text":"歯磨きや食べ物に気を使う親が増えたからかもしれませんね。"},{"id":{"d":675,"n":3,"t":4},"speaker":"A","text":"歯の矯正もそうですが、なるべく子どものうちに治しておこうとする傾向があるようです。"},{"id":{"d":675,"n":4,"t":4},"speaker":"B","text":"歯磨きは幼い頃から習慣化しておくといいと言います。"},{"id":{"d":675,"n":5,"t":4},"speaker":"A","text":"でも、虫歯になる子どもが少なくなったら、歯医者の仕事が減ってしまうということはありませんか？"},{"id":{"d":675,"n":6,"t":4},"speaker":"B","text":"仮に虫歯の子どもが少なくなったとしても、年配の方の歯の治療もあるでしょう。"},{"id":{"d":675,"n":7,"t":4},"speaker":"A","text":"そうですね。歯の寿命は現代の人間の平均寿命より短いと聞いたことがあります。"},{"id":{"d":675,"n":8,"t":4},"speaker":"B","text":"年齢を重ねるほど歯の治療が必要となって、歯医者の利用率は増えていくようです。"},{"id":{"d":675,"n":9,"t":4},"speaker":"A","text":"歯医者はどの年代の人にとっても必要な存在ですね。"}];
INSERT INTO sentence [{"id":{"d":676,"n":1,"t":4},"speaker":"A","text":"健康のために何かしていますか？"},{"id":{"d":676,"n":2,"t":4},"speaker":"B","text":"散歩を日課にしています。"},{"id":{"d":676,"n":3,"t":4},"speaker":"A","text":"天気が悪い日はどうしていますか？"},{"id":{"d":676,"n":4,"t":4},"speaker":"B","text":"基本的には雨くらいでは散歩を休みません。"},{"id":{"d":676,"n":5,"t":4},"speaker":"A","text":"そうなのですね。私は雨の日に散歩をしようとは思いません。"},{"id":{"d":676,"n":6,"t":4},"speaker":"B","text":"私の場合は私自身の散歩ではなく、私の犬の散歩だと思っています。"},{"id":{"d":676,"n":7,"t":4},"speaker":"A","text":"いつも犬を連れて散歩しているのですか？"},{"id":{"d":676,"n":8,"t":4},"speaker":"B","text":"はい、そうです。雨の日も犬が催促するので、散歩を休むことはできません。"},{"id":{"d":676,"n":9,"t":4},"speaker":"A","text":"犬は雨でも外に出たがりますから、結果的に毎日散歩することになるわけですね。"},{"id":{"d":676,"n":10,"t":4},"speaker":"B","text":"犬との仲も良くなり健康的な生活を送ることもできて、一挙両得です。"}];
INSERT INTO sentence [{"id":{"d":677,"n":1,"t":4},"speaker":"A","text":"オリンピックで気になる競技はありますか？"},{"id":{"d":677,"n":2,"t":4},"speaker":"B","text":"オリンピックに限りませんが、私はマイナースポーツに興味があります。"},{"id":{"d":677,"n":3,"t":4},"speaker":"A","text":"マイナースポーツとはスケートボードやスノーボードのようなものでしょうか？"},{"id":{"d":677,"n":4,"t":4},"speaker":"B","text":"それらはオリンピック種目にもなっていますね。私が気になっているスポーツはもっとマイナーなものです。"},{"id":{"d":677,"n":5,"t":4},"speaker":"A","text":"カバディやセパタクローなどでしょうか？"},{"id":{"d":677,"n":6,"t":4},"speaker":"B","text":"はい、そうです。その二つはマイナースポーツの中でもかなり有名で、毎年世界大会も行われています。"},{"id":{"d":677,"n":7,"t":4},"speaker":"A","text":"カバディやセパタクローの強い国はどこですか？"},{"id":{"d":677,"n":8,"t":4},"speaker":"B","text":"カバディはインド、セパタクローはタイやマレーシアが強豪国です。"},{"id":{"d":677,"n":9,"t":4},"speaker":"A","text":"セパタクローはボールを蹴るので、ブラジルなどのサッカーの強豪国が強いのかと思っていました。"},{"id":{"d":677,"n":10,"t":4},"speaker":"B","text":"セパタクローは東南アジアではサッカーの次に人気のあるスポーツですよ。"}];
INSERT INTO sentence [{"id":{"d":678,"n":1,"t":4},"speaker":"A","text":"おたふく風邪にかかったことはありますか？"},{"id":{"d":678,"n":2,"t":4},"speaker":"B","text":"小学校3年生か4年生の時にかかった記憶があります。"},{"id":{"d":678,"n":3,"t":4},"speaker":"A","text":"予防接種は受けていなかったのですか？"},{"id":{"d":678,"n":4,"t":4},"speaker":"B","text":"私が子どもの頃はおたふく風邪のワクチンなどなかったように思うのですが、はっきり覚えていません。"},{"id":{"d":678,"n":5,"t":4},"speaker":"A","text":"今も任意接種ですから、接種していない人もいるかもしれませんね。"},{"id":{"d":678,"n":6,"t":4},"speaker":"B","text":"おたふく風邪になった時は顎の下が膨らんで格好が悪かったのですが、インフルエンザよりは楽でした。"},{"id":{"d":678,"n":7,"t":4},"speaker":"A","text":"大人になってからおたふく風邪にかかると重症化すると聞いたことがあります。"},{"id":{"d":678,"n":8,"t":4},"speaker":"B","text":"私はおたふく風邪の免疫ができているはずですが、それでも絶対にかからないというわけではないと思っています。"},{"id":{"d":678,"n":9,"t":4},"speaker":"A","text":"そうですね。私は子どもが2人いるので、今のうちにおたふく風邪の予防接種を受けておこうと思います。"}];
INSERT INTO sentence [{"id":{"d":679,"n":1,"t":4},"speaker":"A","text":"高校の時はサッカー部だったそうですね。"},{"id":{"d":679,"n":2,"t":4},"speaker":"B","text":"はい。高校3年生の時に県大会の準決勝まで進んだのですが、そこで負けてしまいました。"},{"id":{"d":679,"n":3,"t":4},"speaker":"A","text":"ポジションはどこでしたか？"},{"id":{"d":679,"n":4,"t":4},"speaker":"B","text":"フォワードでした。"},{"id":{"d":679,"n":5,"t":4},"speaker":"A","text":"シュートが得意だったのですね。"},{"id":{"d":679,"n":6,"t":4},"speaker":"B","text":"シュートが決められることももちろん大事なのですが、ボールを受けてゴール前に走り込む動きも大切です。"},{"id":{"d":679,"n":7,"t":4},"speaker":"A","text":"シュートを決めるこつはありますか？"},{"id":{"d":679,"n":8,"t":4},"speaker":"B","text":"私は、シュートはゴールにパスをするように打つのがいいと言われていました。"},{"id":{"d":679,"n":9,"t":4},"speaker":"A","text":"それは私も聞いたことがあります。力んで蹴ってはいけないということなのでしょうね。"},{"id":{"d":679,"n":10,"t":4},"speaker":"B","text":"それでも時々は、ゴールキーパーに本当にパスしてしまうこともありました。"}];
INSERT INTO sentence [{"id":{"d":680,"n":1,"t":4},"speaker":"A","text":"相撲を見に行ったことはありますか？"},{"id":{"d":680,"n":2,"t":4},"speaker":"B","text":"あります。升席を譲ってくれた人がいたので、友人2人と朝から両国国技館に行きました。"},{"id":{"d":680,"n":3,"t":4},"speaker":"A","text":"午前中からだと、序ノ口の取組から見たのですか？"},{"id":{"d":680,"n":4,"t":4},"speaker":"B","text":"序二段からだったように思います。館内の見学もしたかったので、早めに行きました。"},{"id":{"d":680,"n":5,"t":4},"speaker":"A","text":"両国国技館名物の焼き鳥は食べましたか？"},{"id":{"d":680,"n":6,"t":4},"speaker":"B","text":"もちろんです。幕の内弁当も食べました。お酒も飲んだので中入りの頃には酔っていい気分になってしまいました。"},{"id":{"d":680,"n":7,"t":4},"speaker":"A","text":"その頃にはお客さんも増えてきましたか？"},{"id":{"d":680,"n":8,"t":4},"speaker":"B","text":"はい。お客さんはだんだん増えてきたのですが、幸いなことに4人席の升席に3人で座っていたので余裕がありました。"},{"id":{"d":680,"n":9,"t":4},"speaker":"A","text":"中入り後の取組はどうでしたか？"},{"id":{"d":680,"n":10,"t":4},"speaker":"B","text":"人気力士の迫力のある取組を間近で見ることができて大満足でした。"}];
INSERT INTO sentence [{"id":{"d":681,"n":1,"t":4},"speaker":"A","text":"あなたは野球に詳しいですか？"},{"id":{"d":681,"n":2,"t":4},"speaker":"B","text":"はい、中学校では野球部に所属していましたし、今も高校野球やプロ野球が好きです。"},{"id":{"d":681,"n":3,"t":4},"speaker":"A","text":"私も野球が好きでよく見ているのですが、少し分からないことがあるのです。"},{"id":{"d":681,"n":4,"t":4},"speaker":"B","text":"簡単なことであれば、疑問にお答えできると思いますよ。"},{"id":{"d":681,"n":5,"t":4},"speaker":"A","text":"変化球の違いについては詳しいですか？フォークボールとスプリットボールの違いを教えていただきたいのです。"},{"id":{"d":681,"n":6,"t":4},"speaker":"B","text":"一般には、落ち方のより小さい方がスプリットボールといわれています。"},{"id":{"d":681,"n":7,"t":4},"speaker":"A","text":"フォークボールとスプリットボールは投げ方も大体同じだと聞きましたが、本当ですか？"},{"id":{"d":681,"n":8,"t":4},"speaker":"B","text":"確かに似ていますが、ボールの握りの浅い方がスプリットボールです。"},{"id":{"d":681,"n":9,"t":4},"speaker":"A","text":"野球解説者の方は瞬時に球種を見分けているようですが、どのようにしているのでしょうね。"},{"id":{"d":681,"n":10,"t":4},"speaker":"B","text":"事前に投手の持ち球をチェックしておくらしいです。確かに一般の人には球種を見分けるのは難しいです。"}];
INSERT INTO sentence [{"id":{"d":682,"n":1,"t":4},"speaker":"A","text":"普段はどのようなマスクをしていますか？"},{"id":{"d":682,"n":2,"t":4},"speaker":"B","text":"不織布の立体型のマスクをしています。"},{"id":{"d":682,"n":3,"t":4},"speaker":"A","text":"使い捨てのプリーツ型は使ったことがありますか？"},{"id":{"d":682,"n":4,"t":4},"speaker":"B","text":"最初の頃はよくプリーツ型を使っていましたが、今はあまり使っていません。"},{"id":{"d":682,"n":5,"t":4},"speaker":"A","text":"なぜプリーツ型を使わなくなったのですか？"},{"id":{"d":682,"n":6,"t":4},"speaker":"B","text":"長い期間使用するなら、使い捨てマスクは不経済だと思ったからです。"},{"id":{"d":682,"n":7,"t":4},"speaker":"A","text":"それなら布製のマスクは使わないのですか？"},{"id":{"d":682,"n":8,"t":4},"speaker":"B","text":"予備のマスクとして布製マスクを持っていますよ。"},{"id":{"d":682,"n":9,"t":4},"speaker":"A","text":"予備のマスクはどのように持ち歩いていますか？"},{"id":{"d":682,"n":10,"t":4},"speaker":"B","text":"マスクケースに入れて、かばんのポケットにしまっています。"}];
INSERT INTO sentence [{"id":{"d":683,"n":1,"t":4},"speaker":"A","text":"退院おめでとうございます。もう出社しても差し支えないのですか？"},{"id":{"d":683,"n":2,"t":4},"speaker":"B","text":"はい、主治医から許可をいただいています。"},{"id":{"d":683,"n":3,"t":4},"speaker":"A","text":"それでは病み上がりですから、少しずつ体を仕事に慣らしていきましょう。"},{"id":{"d":683,"n":4,"t":4},"speaker":"B","text":"デスクワークですから仕事にあまり支障はないと思います。結石の除去手術だったので後遺症もありません。"},{"id":{"d":683,"n":5,"t":4},"speaker":"A","text":"結石の除去手術は痛そうなイメージがありますが、大丈夫でしたか？"},{"id":{"d":683,"n":6,"t":4},"speaker":"B","text":"おっしゃっているのは尿道結石のことだと思いますが、私は腎臓結石でした。"},{"id":{"d":683,"n":7,"t":4},"speaker":"A","text":"そうでしたか。ちなみに腎臓結石はどのように手術するのですか？"},{"id":{"d":683,"n":8,"t":4},"speaker":"B","text":"背中から腎臓に穴を開けて、内視鏡を見ながらレーザーなどで石を砕きました。"},{"id":{"d":683,"n":9,"t":4},"speaker":"A","text":"背中に穴を開ける手術も十分痛そうです。"},{"id":{"d":683,"n":10,"t":4},"speaker":"B","text":"全身麻酔でしたので、目が覚めたら全て終わっていましたよ。"}];
INSERT INTO sentence [{"id":{"d":684,"n":1,"t":4},"speaker":"A","text":"海外ドラマを見ていると、病院を扱ったドラマが一つのジャンルとして確立しているような気がします。"},{"id":{"d":684,"n":2,"t":4},"speaker":"B","text":"確かに海外ドラマには、病院を扱ったものが多いです。特に緊急救命室を舞台とするものは多いですね。"},{"id":{"d":684,"n":3,"t":4},"speaker":"A","text":"緊急性を持たせることで、アクションドラマのようなスリル感を取り入れているのでしょう。"},{"id":{"d":684,"n":4,"t":4},"speaker":"B","text":"日本でも病院を舞台とするドラマはありますが、人間ドラマとしての部分が大きいように思います。"},{"id":{"d":684,"n":5,"t":4},"speaker":"A","text":"外国の病院では人種や宗教や政治信条などの違いを扱うことも多いので、社会問題や医療問題に自然と関心を持つことができるのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":685,"n":1,"t":4},"speaker":"A","text":"深爪をしてしまったようです。"},{"id":{"d":685,"n":2,"t":4},"speaker":"B","text":"どの指ですか？"},{"id":{"d":685,"n":3,"t":4},"speaker":"A","text":"右足の親指です。痛みを感じるので変な歩き方になってしまいます。"},{"id":{"d":685,"n":4,"t":4},"speaker":"B","text":"なぜ深爪をしてしまったのですか？"},{"id":{"d":685,"n":5,"t":4},"speaker":"A","text":"テレビを見ながら爪を切ったのがいけませんでした。"},{"id":{"d":685,"n":6,"t":4},"speaker":"B","text":"足の指だと細菌が入りやすいかもしれません。薬を塗ってガーゼで保護しておくといいでしょう。"},{"id":{"d":685,"n":7,"t":4},"speaker":"A","text":"ばんそうこうを貼っただけですが、そこまで処置をした方がいいのでしょうか？"},{"id":{"d":685,"n":8,"t":4},"speaker":"B","text":"そうですね。ともすると炎症を起こすかもしれないので、心配であれば病院で診てもらってはいかがでしょうか？"},{"id":{"d":685,"n":9,"t":4},"speaker":"A","text":"ガーゼを巻いてからしばらく様子を見て、ひどくなりそうだったら病院に行くことにします。"}];
INSERT INTO sentence [{"id":{"d":686,"n":1,"t":4},"speaker":"A","text":"クレンジングには何を使っていますか？"},{"id":{"d":686,"n":2,"t":4},"speaker":"B","text":"オイル、ジェル、ミルク、バームと一通り使ってみましたが、今はオイルを使っています。"},{"id":{"d":686,"n":3,"t":4},"speaker":"A","text":"オイルを選んだ理由は何ですか？"},{"id":{"d":686,"n":4,"t":4},"speaker":"B","text":"オイルが一番素早くきちんとメークを落とすことができて、気に入った商品が見つかったからです。"},{"id":{"d":686,"n":5,"t":4},"speaker":"A","text":"オイルは使った後にカサカサになるような気がするので、私はクレンジングミルクを使っています。"},{"id":{"d":686,"n":6,"t":4},"speaker":"B","text":"私はしっかりとメークをするのが好きなので、それをきちんと落とすためにはオイルクレンジングが合っているようです。"},{"id":{"d":686,"n":7,"t":4},"speaker":"A","text":"なるほど、メークの仕方に関係するかもしれませんね。"},{"id":{"d":686,"n":8,"t":4},"speaker":"B","text":"私も肌が乾燥する季節には、オイル以外のものを使うかもしれません。"},{"id":{"d":686,"n":9,"t":4},"speaker":"A","text":"クレンジングのサンプルなどで、使用感を確認しておくとよさそうですね。"},{"id":{"d":686,"n":10,"t":4},"speaker":"B","text":"それは良い考えです。"}];
INSERT INTO sentence [{"id":{"d":687,"n":1,"t":4},"speaker":"A","text":"ギターを弾くそうですが、指のお手入れはしていますか？"},{"id":{"d":687,"n":2,"t":4},"speaker":"B","text":"はい、指にはとても気を使っています。普段は手袋をしています。"},{"id":{"d":687,"n":3,"t":4},"speaker":"A","text":"爪切りは普通のものを使っているのですか？"},{"id":{"d":687,"n":4,"t":4},"speaker":"B","text":"爪切りは使わず、やすりで少しずつ削っています。"},{"id":{"d":687,"n":5,"t":4},"speaker":"A","text":"左右どちらの手の爪もやすりを使って削っているのですか？"},{"id":{"d":687,"n":6,"t":4},"speaker":"B","text":"ピックを持つ右手もアルペジオでは爪を使うので、やすりで調整しています。"},{"id":{"d":687,"n":7,"t":4},"speaker":"A","text":"他に指のお手入れで気を付けていることはありますか？"},{"id":{"d":687,"n":8,"t":4},"speaker":"B","text":"透明のマニキュアで爪を補強しています。"},{"id":{"d":687,"n":9,"t":4},"speaker":"A","text":"マニキュアを使ってお手入れをしているのですね。"},{"id":{"d":687,"n":10,"t":4},"speaker":"B","text":"はい。指のわずかな違和感も演奏に差し支えるので、爪の手入れには細心の注意が必要です。"}];
INSERT INTO sentence [{"id":{"d":688,"n":1,"t":4},"speaker":"A","text":"時々学校を休むことがあるそうですね。"},{"id":{"d":688,"n":2,"t":4},"speaker":"B","text":"他の人よりも学校を休んでいると思います。"},{"id":{"d":688,"n":3,"t":4},"speaker":"A","text":"どのような理由で休むのですか？"},{"id":{"d":688,"n":4,"t":4},"speaker":"B","text":"休む理由はその時々で違います。"},{"id":{"d":688,"n":5,"t":4},"speaker":"A","text":"学校の先生も心配されているでしょうね。"},{"id":{"d":688,"n":6,"t":4},"speaker":"B","text":"子どもの頃から体が弱かったので、休みがちということは分かってもらっていると思います。"}];
INSERT INTO sentence [{"id":{"d":689,"n":1,"t":4},"speaker":"A","text":"映画の『大怪獣ガメラ』を見たことがありますか？"},{"id":{"d":689,"n":2,"t":4},"speaker":"B","text":"はい。昔の作品はいくつか見たことがあります。"},{"id":{"d":689,"n":3,"t":4},"speaker":"A","text":"ガメラの血の色は青色だったような記憶があるのですが、覚えていますか？"},{"id":{"d":689,"n":4,"t":4},"speaker":"B","text":"ガメラはけがをするといつも海底でじっとしていましたが、その時の血は確かに青色だったと思います。"},{"id":{"d":689,"n":5,"t":4},"speaker":"A","text":"そうですよね。そもそもカメの血は青いのでしょうか？"},{"id":{"d":689,"n":6,"t":4},"speaker":"B","text":"いいえ、カメは脊椎動物ですから血の色は赤です。酸素を運ぶヘモグロビンに含まれている鉄が酸化した色です。"}];
INSERT INTO sentence [{"id":{"d":690,"n":1,"t":4},"speaker":"A","text":"どのような歯磨き粉を使っていますか？"},{"id":{"d":690,"n":2,"t":4},"speaker":"B","text":"ジェルタイプのものを使っています。"},{"id":{"d":690,"n":3,"t":4},"speaker":"A","text":"今はジェルタイプもあるのですね。なぜそれを使っているのですか？"},{"id":{"d":690,"n":4,"t":4},"speaker":"B","text":"ジェルタイプには研磨剤が含まれていないからです。"},{"id":{"d":690,"n":5,"t":4},"speaker":"A","text":"そういえば、研磨剤は歯を傷めると聞いたことがあります。"},{"id":{"d":690,"n":6,"t":4},"speaker":"B","text":"私は歯医者から歯を傷めないジェルタイプを薦められて、それ以来ジェルタイプを使っています。"},{"id":{"d":690,"n":7,"t":4},"speaker":"A","text":"フッ素配合の歯磨き粉もありますが、ジェルタイプにはフッ素は入っているのですか？"},{"id":{"d":690,"n":8,"t":4},"speaker":"B","text":"フッ素は虫歯予防に効果があるといわれていますよね。私が使用しているジェルタイプにはフッ素が含まれていますが、含まれていないものもあるようです。買う前に確かめた方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":691,"n":1,"t":4},"speaker":"A","text":"片頭痛とは、どういう頭痛のことですか？"},{"id":{"d":691,"n":2,"t":4},"speaker":"B","text":"脳の血管が拡張して神経を刺激して起こるのが、片頭痛といわれています。"},{"id":{"d":691,"n":3,"t":4},"speaker":"A","text":"緊張型頭痛という言葉も耳にしたことがあります。"},{"id":{"d":691,"n":4,"t":4},"speaker":"B","text":"それは一般的に頭痛といわれているものだと思います。首や肩の筋肉が緊張することで起こりやすくなります。"},{"id":{"d":691,"n":5,"t":4},"speaker":"A","text":"筋肉の緊張とは肩凝りなどのことですか？"},{"id":{"d":691,"n":6,"t":4},"speaker":"B","text":"そうですね。肩凝りは筋肉の緊張の一種です。無理な姿勢や目の疲れが筋肉の緊張を引き起こします。デスクワークをしている時も、1時間に1回はストレッチなどを行うと予防になるそうです。"}];
INSERT INTO sentence [{"id":{"d":692,"n":1,"t":4},"speaker":"A","text":"近頃は髪の毛が薄くなってきました。"},{"id":{"d":692,"n":2,"t":4},"speaker":"B","text":"私も若い頃からその悩みを抱えていました。"},{"id":{"d":692,"n":3,"t":4},"speaker":"A","text":"遺伝やストレス、睡眠不足が原因と聞きますが、必ずしもそうではないように思います。せめて髪の洗い方に気を付けたり、ストレスをためないように生活したりしたいと思います。"},{"id":{"d":692,"n":4,"t":4},"speaker":"B","text":"育毛剤や発毛剤を使おうとしたことはありますか？"},{"id":{"d":692,"n":5,"t":4},"speaker":"A","text":"はい、興味があって調べたことがあります。でも、育毛剤や発毛剤は高額ですし、効果が分かるまでに時間がかかるところが気になっています。"},{"id":{"d":692,"n":6,"t":4},"speaker":"B","text":"仮に複数の育毛剤や発毛剤を使うと、どれが効いているのかが分からなくなってしまいますしね。"},{"id":{"d":692,"n":7,"t":4},"speaker":"A","text":"これでは逆にストレスがたまってしまいそうです。"}];
INSERT INTO sentence [{"id":{"d":693,"n":1,"t":4},"speaker":"A","text":"どの季節が一番好きですか？"},{"id":{"d":693,"n":2,"t":4},"speaker":"B","text":"冬が一番好きです。春や秋も冬に近ければ好きです。"},{"id":{"d":693,"n":3,"t":4},"speaker":"A","text":"夏が嫌いということでしょうか？"},{"id":{"d":693,"n":4,"t":4},"speaker":"B","text":"私はすぐにあせもができてしまいますし、夏バテもしやすいのであまり好きではありません。"},{"id":{"d":693,"n":5,"t":4},"speaker":"A","text":"私は海で泳ぐことが好きなので、夏が一番好きです。"},{"id":{"d":693,"n":6,"t":4},"speaker":"B","text":"私も海で泳ぐことは嫌いではありませんが、日焼けしやすいので海に行くのは避けています。"},{"id":{"d":693,"n":7,"t":4},"speaker":"A","text":"日焼け止めを塗っても効果がありませんか？"},{"id":{"d":693,"n":8,"t":4},"speaker":"B","text":"泳いでしまうと日焼け止めの効果は薄くなりますよね。帰る頃には体中が真っ赤になってしまいます。"},{"id":{"d":693,"n":9,"t":4},"speaker":"A","text":"体中が真っ赤になってしまうほどなのですね。翌日は痛くて何をするにも大変そうです。"},{"id":{"d":693,"n":10,"t":4},"speaker":"B","text":"はい。寝られないくらい痛くなった時もあります。"}];
INSERT INTO sentence [{"id":{"d":694,"n":1,"t":4},"speaker":"A","text":"「くまのい」とは何ですか？"},{"id":{"d":694,"n":2,"t":4},"speaker":"B","text":"「くまのい」は胃腸薬の名前です。「ゆうたん」とも呼ばれている熊の胆のうです。"},{"id":{"d":694,"n":3,"t":4},"speaker":"A","text":"「くまのい」は市販されているのですか？"},{"id":{"d":694,"n":4,"t":4},"speaker":"B","text":"漢方薬局で売られていると思います。「くまのい」の配合薬品であれば普通の薬局でも買えるところがあるようです。"},{"id":{"d":694,"n":5,"t":4},"speaker":"A","text":"熊の胆のうともなるとかなり特別なものだと思いますが、値段はどれくらいするのでしょうか？"},{"id":{"d":694,"n":6,"t":4},"speaker":"B","text":"「くまのい」本体は高価だそうですが、配合薬は一般的な胃腸薬より少し高い程度のようです。"},{"id":{"d":694,"n":7,"t":4},"speaker":"A","text":"高価な薬のようですが、効き目はあるのでしょうか？"},{"id":{"d":694,"n":8,"t":4},"speaker":"B","text":"これは漢方医の受け売りなのですが、非常によく効くらしいです。即効性もあるようですが、私も使ったことがないのではっきりとしたことは言えません。"}];
INSERT INTO sentence [{"id":{"d":695,"n":1,"t":4},"speaker":"A","text":"あなたは寝具にはこだわっていますか？"},{"id":{"d":695,"n":2,"t":4},"speaker":"B","text":"はい、良い眠りのために枕にはこだわりがあります。"},{"id":{"d":695,"n":3,"t":4},"speaker":"A","text":"どのような種類の枕を使っていますか？"},{"id":{"d":695,"n":4,"t":4},"speaker":"B","text":"低反発のウレタン枕を気に入って使っています。"},{"id":{"d":695,"n":5,"t":4},"speaker":"A","text":"高反発のウレタンもあると思いますが、低反発枕の使い心地はどうですか？"},{"id":{"d":695,"n":6,"t":4},"speaker":"B","text":"頭が包み込まれるように沈む感覚があって、寝るのが楽しみになりました。"},{"id":{"d":695,"n":7,"t":4},"speaker":"A","text":"枕を変えてから、睡眠の質が変わった感じはありますか？"},{"id":{"d":695,"n":8,"t":4},"speaker":"B","text":"はい。以前は寝相が悪くよく首を寝違えていましたが、今は寝た時とほとんど同じ姿勢で起きますし、寝違えることも少なくなりました。"},{"id":{"d":695,"n":9,"t":4},"speaker":"A","text":"それでは旅行の時でもお気に入りの枕は手放せませんね。"},{"id":{"d":695,"n":10,"t":4},"speaker":"B","text":"その通りです。外泊の時は必ず自分の枕を持っていくようになりました。"}];
INSERT INTO sentence [{"id":{"d":696,"n":1,"t":4},"speaker":"A","text":"就寝時に心掛けていることは何かありますか？"},{"id":{"d":696,"n":2,"t":4},"speaker":"B","text":"眠りに就きやすいように、何か音楽をかけるようにしています。"},{"id":{"d":696,"n":3,"t":4},"speaker":"A","text":"私も音楽を聴きながら眠りに就きます。どのような音楽を選んでいるのですか？"},{"id":{"d":696,"n":4,"t":4},"speaker":"B","text":"やはり静かな音楽です。クラシックかヒーリング・ミュージックが多いです。"},{"id":{"d":696,"n":5,"t":4},"speaker":"A","text":"どのような音楽がお薦めですか？"},{"id":{"d":696,"n":6,"t":4},"speaker":"B","text":"私はブライアン・イーノやマイケル・ナイマンを聴きます。あなたはどのような音楽を聴いているのですか？"},{"id":{"d":696,"n":7,"t":4},"speaker":"A","text":"私はロックやファンクを聴いています。"},{"id":{"d":696,"n":8,"t":4},"speaker":"B","text":"にぎやかな音楽を聴きながら眠るのですね。私は朝起きる時にはロックを流します。"}];
INSERT INTO sentence [{"id":{"d":697,"n":1,"t":4},"speaker":"A","text":"私の周りで帯状疱疹になる人が増えています。"},{"id":{"d":697,"n":2,"t":4},"speaker":"B","text":"私も去年発症しました。50歳を超えると発症率が高くなるそうです。"},{"id":{"d":697,"n":3,"t":4},"speaker":"A","text":"寝られないほどの痛みがあると聞いたのですが、どうでしたか？"},{"id":{"d":697,"n":4,"t":4},"speaker":"B","text":"非常に痛く、つらかったです。"},{"id":{"d":697,"n":5,"t":4},"speaker":"A","text":"帯状疱疹は免疫力の低下が原因だそうですね。"},{"id":{"d":697,"n":6,"t":4},"speaker":"B","text":"疲労やストレスも原因になるようなので、誰にでも帯状疱疹を発症する可能性があります。"},{"id":{"d":697,"n":7,"t":4},"speaker":"A","text":"帯状疱疹のワクチンがあるそうですね。予防効果が短いものと長いものがあると聞いたことがあります。"},{"id":{"d":697,"n":8,"t":4},"speaker":"B","text":"私は効果が長期間あるといわれているワクチンを接種しました。値は張りましたが、それだけの価値はあると思います。"}];
INSERT INTO sentence [{"id":{"d":698,"n":1,"t":4},"speaker":"A","text":"お子さんが理学療法士だそうですね。どのようにして理学療法士の資格を取得したのですか？"},{"id":{"d":698,"n":2,"t":4},"speaker":"B","text":"私の息子は4年制の大学で受験資格を取り、その後、国家資格を取りました。"},{"id":{"d":698,"n":3,"t":4},"speaker":"A","text":"私の娘が理学療法士になりたいと言っているのです。そのためには、受験資格を得られる大学を選ぶ必要がありますね。"},{"id":{"d":698,"n":4,"t":4},"speaker":"B","text":"そうですね。理学療法士の国家資格試験を受験できる大学を選んだ方がいいでしょう。"},{"id":{"d":698,"n":5,"t":4},"speaker":"A","text":"国家資格を得たら、すぐに就職できるのでしょうか？"},{"id":{"d":698,"n":6,"t":4},"speaker":"B","text":"就職先は自分で探すことになると思いますが、恐らく需要は多いと思います。"},{"id":{"d":698,"n":7,"t":4},"speaker":"A","text":"理学療法士の就職先といえば、主に病院なのでしょうか？"},{"id":{"d":698,"n":8,"t":4},"speaker":"B","text":"そうですね。病院以外にも、障害者施設や高齢者施設、スポーツ関係の現場で働いている理学療法士もいると聞いています。"}];
INSERT INTO sentence [{"id":{"d":699,"n":1,"t":4},"speaker":"A","text":"鼻水が止まらない時があるのですが、病院に行った方がいいのでしょうか？花粉症ではないような気がします。"},{"id":{"d":699,"n":2,"t":4},"speaker":"B","text":"アレルギー性鼻炎の可能性もありますね。症状がひどいのなら、病院へ行くべきでしょう。"},{"id":{"d":699,"n":3,"t":4},"speaker":"A","text":"ある程度年を取ってからでもアレルギー性鼻炎になるものなのでしょうか？"},{"id":{"d":699,"n":4,"t":4},"speaker":"B","text":"そうですね。いつ発症しても不思議ではないと思います。"},{"id":{"d":699,"n":5,"t":4},"speaker":"A","text":"アレルギー性鼻炎の原因は何でしょうか？"},{"id":{"d":699,"n":6,"t":4},"speaker":"B","text":"ハウスダストが原因の一つだと思いますが、一度専門医にきちんと診断してもらってはどうでしょうか？"},{"id":{"d":699,"n":7,"t":4},"speaker":"A","text":"症状は鼻水だけなので、何となく恥ずかしくて医者に行くのを躊躇していました。"},{"id":{"d":699,"n":8,"t":4},"speaker":"B","text":"鼻水もれっきとした症状です。恥ずかしがらずに、一度病院へ行ってみたらいいと思います。"}];
INSERT INTO sentence [{"id":{"d":700,"n":1,"t":4},"speaker":"A","text":"坂本さんは先日の社内防災訓練の時に心肺蘇生をしていましたが、どこかで習ったことがあるのですか？"},{"id":{"d":700,"n":2,"t":4},"speaker":"B","text":"会社から救命講習を受講するように言われて、消防署で受講しました。"},{"id":{"d":700,"n":3,"t":4},"speaker":"A","text":"私でも心肺蘇生の講習を受講できるのでしょうか？"},{"id":{"d":700,"n":4,"t":4},"speaker":"B","text":"恐らく誰でも受講できると思いますが、興味があるのですか？"},{"id":{"d":700,"n":5,"t":4},"speaker":"A","text":"近所の子どもたちをキャンプに連れて行く機会があるので、救急法を知っておいた方がいいと思ったのです。"},{"id":{"d":700,"n":6,"t":4},"speaker":"B","text":"なるほど、それは良い考えですね。"},{"id":{"d":700,"n":7,"t":4},"speaker":"A","text":"坂本さんが受けた救命講習はどのようなものでしたか？"},{"id":{"d":700,"n":8,"t":4},"speaker":"B","text":"90分の講習時間で、胸骨圧迫やAEDの使い方を習いました。終わると認定証として受講証をもらいました。"},{"id":{"d":700,"n":9,"t":4},"speaker":"A","text":"さらに上級の救命講習コースもあるのですか？"},{"id":{"d":700,"n":10,"t":4},"speaker":"B","text":"受講内容に止血法などが加わるコースがありますが、受講時間は3時間だったと思います。受講後に救命技能認定証が交付されます。"},{"id":{"d":700,"n":11,"t":4},"speaker":"A","text":"ありがとうございます。消防署に問い合わせてみます。"}];
INSERT INTO sentence [{"id":{"d":701,"n":1,"t":4},"speaker":"A","text":"眼科の前を通ると毎回気になることがあります。"},{"id":{"d":701,"n":2,"t":4},"speaker":"B","text":"それはいったい何でしょうか？"},{"id":{"d":701,"n":3,"t":4},"speaker":"A","text":"斜視が目の病気の種類として記されています。斜視は目の病気なのでしょうか？"},{"id":{"d":701,"n":4,"t":4},"speaker":"B","text":"眼科で手術をするのですから、目の病気といえると思います。"},{"id":{"d":701,"n":5,"t":4},"speaker":"A","text":"斜視を手術で治すのですか？"},{"id":{"d":701,"n":6,"t":4},"speaker":"B","text":"はい。目の筋肉をずらすことで改善すると聞いたことがあります。"},{"id":{"d":701,"n":7,"t":4},"speaker":"A","text":"斜視は脳の病気だと思っていました。"},{"id":{"d":701,"n":8,"t":4},"speaker":"B","text":"脳出血や脳梗塞が原因で斜視が起きることもあるようですが、その場合は眼科で処置はできないでしょうね。"}];
INSERT INTO sentence [{"id":{"d":702,"n":1,"t":4},"speaker":"A","text":"山下さんは、加圧トレーニングを知っていますか？"},{"id":{"d":702,"n":2,"t":4},"speaker":"B","text":"聞いたことはありますが、詳細までは知りません。"},{"id":{"d":702,"n":3,"t":4},"speaker":"A","text":"腕と足の付け根にベルトを巻くことで血流を制限し、短時間で効果を出す筋力トレーニングです。"},{"id":{"d":702,"n":4,"t":4},"speaker":"B","text":"それだけを聞くと、体に良さそうなトレーニングにはとても思えません。"},{"id":{"d":702,"n":5,"t":4},"speaker":"A","text":"筋肉が低酸素化すると多くの筋繊維が刺激されるらしいですよ。"},{"id":{"d":702,"n":6,"t":4},"speaker":"B","text":"佐々木さんは、その加圧トレーニングをしようとしているのですか？"},{"id":{"d":702,"n":7,"t":4},"speaker":"A","text":"実は今、トレーナーになるための勉強をしているのです。一般の人の加圧トレーニングに対する印象を知りたくて聞いてみたのです。"},{"id":{"d":702,"n":8,"t":4},"speaker":"B","text":"そうだったのですね。よければ加圧トレーニングについてもっと教えてください。"}];
INSERT INTO sentence [{"id":{"d":703,"n":1,"t":4},"speaker":"A","text":"最近は、学生スポーツでも高地トレーニングを取り入れているらしいですね。"},{"id":{"d":703,"n":2,"t":4},"speaker":"B","text":"そうなのですか。高地トレーニングというとマラソン選手を思い出します。"},{"id":{"d":703,"n":3,"t":4},"speaker":"A","text":"マラソンだけではなく、陸上競技、競泳、自転車、トライアスロン、サッカーにバスケットボールなど幅広くトレーニング効果があるようですよ。"},{"id":{"d":703,"n":4,"t":4},"speaker":"B","text":"高地トレーニングとは標高何メートルくらいの場所で行うのですか？"},{"id":{"d":703,"n":5,"t":4},"speaker":"A","text":"理想的な標高は1,500メートルから3,000メートルといわれています。"},{"id":{"d":703,"n":6,"t":4},"speaker":"B","text":"高地トレーニングにお詳しいですが、経験者なのですか？"},{"id":{"d":703,"n":7,"t":4},"speaker":"A","text":"実は高校生の息子が競泳のトレーニングで高地トレーニング合宿に出かけているのです。能力向上には良いことかもしれませんが、同時に学生スポーツでそこまですることには多少の抵抗感もあるのです。"},{"id":{"d":703,"n":8,"t":4},"speaker":"B","text":"息子さん本人の意思を尊重してあげるといいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":704,"n":1,"t":4},"speaker":"A","text":"佐藤さんは、先週は温泉に行っていたそうですね。"},{"id":{"d":704,"n":2,"t":4},"speaker":"B","text":"はい、温泉療法に行っていました。"},{"id":{"d":704,"n":3,"t":4},"speaker":"A","text":"日本に温泉療法施設があるのですか？"},{"id":{"d":704,"n":4,"t":4},"speaker":"B","text":"私は多目的温泉保養館というところに行きましたが、同様の施設は全国に40カ所くらいあるようです。"},{"id":{"d":704,"n":5,"t":4},"speaker":"A","text":"多目的温泉とはどのような意味ですか？"},{"id":{"d":704,"n":6,"t":4},"speaker":"B","text":"温泉と健康増進のためのジムが利用できました。私の行ったところは人間ドックまでありましたよ。"},{"id":{"d":704,"n":7,"t":4},"speaker":"A","text":"その施設には専門医がいるのですか？"},{"id":{"d":704,"n":8,"t":4},"speaker":"B","text":"温泉療法医や保健師、看護士などが常駐していました。健康運動指導士の指導で温泉プールを使った運動をしました。"}];
INSERT INTO sentence [{"id":{"d":705,"n":1,"t":4},"speaker":"A","text":"クライオセラピーという言葉を聞いたことはありますか？"},{"id":{"d":705,"n":2,"t":4},"speaker":"B","text":"はい。冷却療法のことですね。"},{"id":{"d":705,"n":3,"t":4},"speaker":"A","text":"そうなのですか。それは、痛めた患部を氷で冷やすようなものですか？"},{"id":{"d":705,"n":4,"t":4},"speaker":"B","text":"それもクライオセラピーですが、病院では冷却治療器を使ったりします。"},{"id":{"d":705,"n":5,"t":4},"speaker":"A","text":"冷やすといえば、筋肉痛や脱臼の時の処方しか浮かびません。"},{"id":{"d":705,"n":6,"t":4},"speaker":"B","text":"今では美容や皮膚の科学的治療にもクライオセラピーは使われているそうですよ。クライオサウナはご存じですか？"},{"id":{"d":705,"n":7,"t":4},"speaker":"A","text":"はい。冷凍サウナのことですね。聞いただけで寒そうです。"},{"id":{"d":705,"n":8,"t":4},"speaker":"B","text":"全身を数分間マイナス110度からマイナス190度で冷やします。血管を冷却して収縮させてから膨張させて新陳代謝を促す施術法です。"},{"id":{"d":705,"n":9,"t":4},"speaker":"A","text":"クライオサウナは人体に悪影響を及ぼしませんか？"},{"id":{"d":705,"n":10,"t":4},"speaker":"B","text":"ごく短時間の冷却なので問題はないようです。スポーツ医学の現場では一般的に活用されているそうです。"}];
INSERT INTO sentence [{"id":{"d":706,"n":1,"t":4},"speaker":"A","text":"しばらく入院されていたそうですね。もう全快されたのですか？"},{"id":{"d":706,"n":2,"t":4},"speaker":"B","text":"ストーブの消し忘れで一酸化炭素中毒になってしまったのです。今はもう元気になりました。"},{"id":{"d":706,"n":3,"t":4},"speaker":"A","text":"入院中はどのように治療したのですか？"},{"id":{"d":706,"n":4,"t":4},"speaker":"B","text":"私の場合は酸素吸入と高圧酸素療法という方法で治療しました。"},{"id":{"d":706,"n":5,"t":4},"speaker":"A","text":"酸素吸入は分かりますが、高圧酸素療法とは何でしょうか？"},{"id":{"d":706,"n":6,"t":4},"speaker":"B","text":"高圧タンクの中に入り、高い気圧環境で高濃度の酸素を吸入する治療法です。私が入院していた病院には数人を収容できる高圧タンクがありました。"},{"id":{"d":706,"n":7,"t":4},"speaker":"A","text":"すぐに治ったのですか？"},{"id":{"d":706,"n":8,"t":4},"speaker":"B","text":"症状自体はすぐに改善されましたが、後遺症の懸念があったので数週間入院していました。"},{"id":{"d":706,"n":9,"t":4},"speaker":"A","text":"全快されてよかったです。"}];
INSERT INTO sentence [{"id":{"d":707,"n":1,"t":4},"speaker":"A","text":"クローン病をご存じですか？"},{"id":{"d":707,"n":2,"t":4},"speaker":"B","text":"確か口や肛門に炎症ができる病気だったと思います。"},{"id":{"d":707,"n":3,"t":4},"speaker":"A","text":"はい。では、クローン病の原因を知っていますか？"},{"id":{"d":707,"n":4,"t":4},"speaker":"B","text":"ニュースで見たことがあるくらいなので原因までは分かりません。クローン食品と関係があるのですか？"},{"id":{"d":707,"n":5,"t":4},"speaker":"A","text":"私も初めはそう思ったのですが、そのクローンではないのだそうです。"},{"id":{"d":707,"n":6,"t":4},"speaker":"B","text":"クローン病の「クローン」は遺伝子複製のことではないのですか？"},{"id":{"d":707,"n":7,"t":4},"speaker":"A","text":"アメリカ人のクローンさんが発見したからクローン病と言われるようになったのです。"},{"id":{"d":707,"n":8,"t":4},"speaker":"B","text":"そうなのですね。ではクローン病の原因は何なのですか？"},{"id":{"d":707,"n":9,"t":4},"speaker":"A","text":"原因はまだ明らかになっていませんが、免疫系の反応異常といわれています。"},{"id":{"d":707,"n":10,"t":4},"speaker":"B","text":"クローン病が難病であることは確かなのですね。"}];
INSERT INTO sentence [{"id":{"d":708,"n":1,"t":4},"speaker":"A","text":"病院が舞台の海外ドラマの中には、時々とても怖い場面がありますよね。"},{"id":{"d":708,"n":2,"t":4},"speaker":"B","text":"手術や治療シーンのことですか？海外ドラマはリアルなシーンが多いので好き嫌いは分かれるかもしれません。"},{"id":{"d":708,"n":3,"t":4},"speaker":"A","text":"特殊メークが優れているので、病気やけがもリアルに見えてしまいます。"},{"id":{"d":708,"n":4,"t":4},"speaker":"B","text":"そうですね。エボラ出血熱や人食いバクテリアによる感染症などの恐ろしい病気が出てくる作品もありますね。"},{"id":{"d":708,"n":5,"t":4},"speaker":"A","text":"人食いバクテリアという名前は聞いたことはありますが、どういった症状を引き起こすのでしょうか？"},{"id":{"d":708,"n":6,"t":4},"speaker":"B","text":"瞬く間に筋肉が腐っていくという症状です。致死率は3割ともいわれているようです。"},{"id":{"d":708,"n":7,"t":4},"speaker":"A","text":"人食いバクテリアは名前だけではなく症状も怖いですね。"},{"id":{"d":708,"n":8,"t":4},"speaker":"B","text":"猛烈な痛みがありますが、筋肉の中の痛みなので発見が遅れて死亡することもあるそうです。"},{"id":{"d":708,"n":9,"t":4},"speaker":"A","text":"内側から侵食されていくので、見て分かる状態の時には既に全体が壊死しているのですね。"},{"id":{"d":708,"n":10,"t":4},"speaker":"B","text":"はい。エボラ出血熱と違い、人食いバクテリアによる感染症は実際に日本での症例もあるようですよ。"}];
INSERT INTO sentence [{"id":{"d":709,"n":1,"t":4},"speaker":"A","text":"最近は成人病という呼び方を聞かなくなった気がします。"},{"id":{"d":709,"n":2,"t":4},"speaker":"B","text":"成人病を生活習慣病と呼ぶようになって20年以上は経ちますね。"},{"id":{"d":709,"n":3,"t":4},"speaker":"A","text":"生活習慣病とは成人病のことだったのですね。"},{"id":{"d":709,"n":4,"t":4},"speaker":"B","text":"そうです。以前は、三大成人病といった言葉が生命保険のCMではよく使われていました。"},{"id":{"d":709,"n":5,"t":4},"speaker":"A","text":"三大成人病とは、がんと脳血管障害と心臓疾患のことですね。なぜ成人病ではなく生活習慣病と呼ぶことになったのでしょうか？"},{"id":{"d":709,"n":6,"t":4},"speaker":"B","text":"早期発見と早期治療を促すために、生活習慣の改善を強調したかったのではないかと思います。"},{"id":{"d":709,"n":7,"t":4},"speaker":"A","text":"なるほど。不治の病ではなく、生活改善によって予防や早期治療が可能ならば、名称の変更は意味がありそうです。"},{"id":{"d":709,"n":8,"t":4},"speaker":"B","text":"以前の三大成人病以外にも、高血圧や肝硬変、糖尿病なども生活習慣病に入っていますね。"},{"id":{"d":709,"n":9,"t":4},"speaker":"A","text":"どれもよく耳にする病気ばかりです。これらの病気にならないよう、普段から気を付けなければいけませんね。"}];
INSERT INTO sentence [{"id":{"d":710,"n":1,"t":4},"speaker":"A","text":"再生医療とはどのような医療ですか？"},{"id":{"d":710,"n":2,"t":4},"speaker":"B","text":"クローンを作ったり、細胞を利用したりして治療する方法だと認識しています。"},{"id":{"d":710,"n":3,"t":4},"speaker":"A","text":"患者の体外で細胞や組織を作るのですか？"},{"id":{"d":710,"n":4,"t":4},"speaker":"B","text":"はい。時々ニュースで報じられているES細胞やiPS細胞も再生医療の分野で使われているものです。"},{"id":{"d":710,"n":5,"t":4},"speaker":"A","text":"ES細胞やiPS細胞でノーベル賞を受賞した日本人もいましたね。"},{"id":{"d":710,"n":6,"t":4},"speaker":"B","text":"そうですね。再生医療は世界最先端の医療で、劇的な効果が期待されています。"},{"id":{"d":710,"n":7,"t":4},"speaker":"A","text":"クローンや細胞の作製に関しては、倫理的問題もあるのでしょうか？"},{"id":{"d":710,"n":8,"t":4},"speaker":"B","text":"はい、倫理的側面についてはまさに議論の最中です。技術的にも倫理的にも未確立な医療であることは確かですが、だからこそ広がりのある医療ともいえます。"},{"id":{"d":710,"n":9,"t":4},"speaker":"A","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":711,"n":1,"t":4},"speaker":"A","text":"人間ドックに行かれたそうですね。"},{"id":{"d":711,"n":2,"t":4},"speaker":"B","text":"はい、定期的に行くようにしています。"},{"id":{"d":711,"n":3,"t":4},"speaker":"A","text":"人間ドックを受診するときに心掛けていることはありますか？"},{"id":{"d":711,"n":4,"t":4},"speaker":"B","text":"オプションの検査を毎回変えて受けるようにしています。"},{"id":{"d":711,"n":5,"t":4},"speaker":"A","text":"前回の人間ドックでは何のオプション検査をプラスしたのですか？"},{"id":{"d":711,"n":6,"t":4},"speaker":"B","text":"腹部エコー検査をプラスしました。"},{"id":{"d":711,"n":7,"t":4},"speaker":"A","text":"CT検査とエコー検査は違うのですか？"},{"id":{"d":711,"n":8,"t":4},"speaker":"B","text":"CT検査はX線を使いますが、エコー検査は超音波を使います。エコー検査の方が短時間で済み、安価で安全だといわれています。"},{"id":{"d":711,"n":9,"t":4},"speaker":"A","text":"放射線を使う危険性を考えたのでしょうが、CT検査の利点は何ですか？"},{"id":{"d":711,"n":10,"t":4},"speaker":"B","text":"CT検査はエコー検査よりも得られる情報量が多いようです。病気の疑いがあれば私はCT検査を利用すると思います。"}];
INSERT INTO sentence [{"id":{"d":712,"n":1,"t":4},"speaker":"A","text":"リストバンドは何のためにするのでしょうか？"},{"id":{"d":712,"n":2,"t":4},"speaker":"B","text":"スポーツによって用途は違うと思いますが、手首の保護と汗拭きが主な用途でしょう。"},{"id":{"d":712,"n":3,"t":4},"speaker":"A","text":"リストバンドは、テニスや野球、バスケットボールでよく使われていると思います。"},{"id":{"d":712,"n":4,"t":4},"speaker":"B","text":"テニスでは利き腕の手首にリストバンドを着け、腕に流れる汗を防いでいます。"},{"id":{"d":712,"n":5,"t":4},"speaker":"A","text":"野球選手も同じ理由でリストバンドを着けているのですか？"},{"id":{"d":712,"n":6,"t":4},"speaker":"B","text":"打者が手首にデッドボールを受ける際に保護する役目として着けていると思います。"},{"id":{"d":712,"n":7,"t":4},"speaker":"A","text":"バスケットボールでは、腕から汗が手に落ちてくるのを防ぐためと、汗拭きのためにリストバンドを使っているようです。"},{"id":{"d":712,"n":8,"t":4},"speaker":"B","text":"バスケットボール選手はリストバンドを両手に着けている選手が多いですからね。"},{"id":{"d":712,"n":9,"t":4},"speaker":"A","text":"おしゃれを理由に着けている選手はいないのですか？"},{"id":{"d":712,"n":10,"t":4},"speaker":"B","text":"おしゃれだけを理由に着けている選手はあまりいないかもしれません。ただ、リストバンドを着けるならばどのような選手でも格好良いものを選ぶと思います。"}];
INSERT INTO sentence [{"id":{"d":713,"n":1,"t":4},"speaker":"A","text":"ヘルメットはスポーツをする上で重要な用具だと思いますが、モータースポーツ以外のメーカーについてはあまり話題に上がらないように思います。"},{"id":{"d":713,"n":2,"t":4},"speaker":"B","text":"プロスポーツの場合はその連盟で規格が決まっており、メーカーが限られているので話題に上がらないのでしょう。"},{"id":{"d":713,"n":3,"t":4},"speaker":"A","text":"モータースポーツ以外のプロスポーツでは、用具やユニフォームのメーカーがヘルメットを作っていることが多いですよね。"},{"id":{"d":713,"n":4,"t":4},"speaker":"B","text":"ええ。オリンピック競技のようなアマチュアスポーツでは、アスリートにそれぞれヘルメットメーカーがいるようです。"},{"id":{"d":713,"n":5,"t":4},"speaker":"A","text":"なるほど。自転車競技などがそうですね。"},{"id":{"d":713,"n":6,"t":4},"speaker":"B","text":"自転車競技はアマチュアもプロもあり、世界的にも競技人口が非常に多く、市場としても成立しているので、メーカーは世界中にたくさんあります。"},{"id":{"d":713,"n":7,"t":4},"speaker":"A","text":"自転車のヘルメットは流体力学を踏まえたものが多いですが、カラフルで個性的でもあります。"},{"id":{"d":713,"n":8,"t":4},"speaker":"B","text":"安全基準はモータースポーツほど厳しくはないので、自転車のヘルメットにはメーカーの自由な発想を見ることができます。"}];
INSERT INTO sentence [{"id":{"d":714,"n":1,"t":4},"speaker":"A","text":"虫歯もそうですが、ものもらいを発症する人の数も減少傾向にあるのではないでしょうか？"},{"id":{"d":714,"n":2,"t":4},"speaker":"B","text":"私が子どもの頃は、ものもらいで眼帯をしている子どもがクラスに1人はいたような気がします。"},{"id":{"d":714,"n":3,"t":4},"speaker":"A","text":"ものもらいの原因は、ばい菌によるものでしょうか？"},{"id":{"d":714,"n":4,"t":4},"speaker":"B","text":"ばい菌でまぶたにある油穴が詰まってうみがたまった状態がものもらいです。"},{"id":{"d":714,"n":5,"t":4},"speaker":"A","text":"ウイルスや細菌ではないので、人にうつることはないのですね。"},{"id":{"d":714,"n":6,"t":4},"speaker":"B","text":"ものもらいは人にはうつりません。うつるのは流行性の結膜炎です。"},{"id":{"d":714,"n":7,"t":4},"speaker":"A","text":"ものもらいが減少してきた理由は何でしょうか？"},{"id":{"d":714,"n":8,"t":4},"speaker":"B","text":"子どもの衛生環境の向上が原因の一つとして挙げられるでしょうね。"},{"id":{"d":714,"n":9,"t":4},"speaker":"A","text":"ばい菌がまぶたの裏側に到達しなければ発症しないわけですね。"},{"id":{"d":714,"n":10,"t":4},"speaker":"B","text":"汚れた手でまぶたをこする人が減ったことも、ものもらいが減少した理由の一つでしょう。"}];
INSERT INTO sentence [{"id":{"d":715,"n":1,"t":4},"speaker":"A","text":"たばこと葉巻ではどちらがより健康への害があるのでしょうか？"},{"id":{"d":715,"n":2,"t":4},"speaker":"B","text":"どちらも同じくらい害があると思います。葉巻を吸う人の中にはたばこの巻紙を問題視している人がいますが、それはあまり関係ありません。"},{"id":{"d":715,"n":3,"t":4},"speaker":"A","text":"葉巻愛好者の中には、香りをたしなむだけだから健康にはそれほど害はないと言う人もいますが、本当でしょうか？"},{"id":{"d":715,"n":4,"t":4},"speaker":"B","text":"ニコチン量は葉巻の方が多いので、肺に煙を入れなくても副流煙の影響はあると思います。"},{"id":{"d":715,"n":5,"t":4},"speaker":"A","text":"たばこでは副流煙が問題視されているのですから、その点では葉巻も同じわけですね。"},{"id":{"d":715,"n":6,"t":4},"speaker":"B","text":"ニコチン量が多いので、口の中でニコチンが吸収されることも考えなければなりません。"},{"id":{"d":715,"n":7,"t":4},"speaker":"A","text":"葉巻の方が健康に悪いのでしたら、シガーバーなどが増えることは問題ではないでしょうか？"},{"id":{"d":715,"n":8,"t":4},"speaker":"B","text":"週に一度くらいシガーバーでだけたしなむのでしたら、日常的に吸う人よりは健康的です。"},{"id":{"d":715,"n":9,"t":4},"speaker":"A","text":"日常的な喫煙でニコチン中毒となってしまっては、たしなみとは言えませんからね。"}];
INSERT INTO sentence [{"id":{"d":716,"n":1,"t":4},"speaker":"A","text":"この年になって初めて口内炎ができました。なぜ口内炎ができるのでしょうか？"},{"id":{"d":716,"n":2,"t":4},"speaker":"B","text":"私の場合は奥歯で口の中をかんでしまったときに口内炎になります。"},{"id":{"d":716,"n":3,"t":4},"speaker":"A","text":"私は口の中をかんでいないので、別の原因だと思います。"},{"id":{"d":716,"n":4,"t":4},"speaker":"B","text":"他の原因としては、ストレスや栄養不足、ウイルス感染によるといわれています。"},{"id":{"d":716,"n":5,"t":4},"speaker":"A","text":"偏食はしていないので、ストレスかウイルス感染の影響かもしれません。"},{"id":{"d":716,"n":6,"t":4},"speaker":"B","text":"ウイルス感染は症状がひどいらしいですから、それとは違う可能性がありますね。気になるようでしたら、口内炎の薬を使ってはどうですか？"},{"id":{"d":716,"n":7,"t":4},"speaker":"A","text":"そこまでは気になりません。私の場合、痛みがあるというよりは、傷口に歯が触れることへの恐怖感で会話や飲食に気を使う程度です。"},{"id":{"d":716,"n":8,"t":4},"speaker":"B","text":"それでしたら普通の口内炎かもしれませんね。もしひどくなったり1、2週間経っても治らなかったりしたら、病院に行くことをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":717,"n":1,"t":4},"speaker":"A","text":"医者といえば、真っ先に聴診器を思い浮かべます。"},{"id":{"d":717,"n":2,"t":4},"speaker":"B","text":"私も、白衣を着ていることよりも聴診器を持っていることの方が医者らしく見えるような気がします。"},{"id":{"d":717,"n":3,"t":4},"speaker":"A","text":"白衣は他の職業でも着ている人がいますからね。"},{"id":{"d":717,"n":4,"t":4},"speaker":"B","text":"なるほど、そうかもしれません。そういえば、聴診器には種類があるのでしょうか？"},{"id":{"d":717,"n":5,"t":4},"speaker":"A","text":"オープンベル型とダイヤフラム型が聴診器の主流のようですよ。"},{"id":{"d":717,"n":6,"t":4},"speaker":"B","text":"その2種類の聴診器はどう違うのですか？"},{"id":{"d":717,"n":7,"t":4},"speaker":"A","text":"一般的なタイプがオープンベル型で、振動膜を張った分だけ少し大きいのがダイヤフラム型です。"},{"id":{"d":717,"n":8,"t":4},"speaker":"B","text":"聴診器はその2種類だけなのですか？"},{"id":{"d":717,"n":9,"t":4},"speaker":"A","text":"その2種類の両方が付いたものや、耳への装着方法で種類が違うものはあります。種類ではありませんが、色のバリエーションはいろいろあります。"},{"id":{"d":717,"n":10,"t":4},"speaker":"B","text":"医者や看護士は制服を着ているので、ファッショナブルな聴診器は素敵だと思います。"}];
INSERT INTO sentence [{"id":{"d":718,"n":1,"t":4},"speaker":"A","text":"この間、耳あかは取らない方がいいという話を聞きました。"},{"id":{"d":718,"n":2,"t":4},"speaker":"B","text":"私は毎日耳あかを取っています。何が問題なのでしょうか？"},{"id":{"d":718,"n":3,"t":4},"speaker":"A","text":"綿棒で耳掃除をすると耳あかを奥に押し込んでしまうらしいのです。"},{"id":{"d":718,"n":4,"t":4},"speaker":"B","text":"お風呂上がりに綿棒を使うと、耳あかが取れているように感じますが、駄目なのでしょうか？"},{"id":{"d":718,"n":5,"t":4},"speaker":"A","text":"綿棒は太いので、風呂上がりのふやけた耳では耳あかを奥に押し込んでしまうらしいです。"},{"id":{"d":718,"n":6,"t":4},"speaker":"B","text":"そうなのですね。では、どのように耳掃除をしたらよいのですか？"},{"id":{"d":718,"n":7,"t":4},"speaker":"A","text":"耳あかは自然に排出されるので、掃除する必要はないそうですよ。"},{"id":{"d":718,"n":8,"t":4},"speaker":"B","text":"でも耳がかゆいときは掃除をしたくなります。"},{"id":{"d":718,"n":9,"t":4},"speaker":"A","text":"かゆくなっているのは耳の中が炎症を起こしているからなので、耳鼻科に行った方がいいそうです。"},{"id":{"d":718,"n":10,"t":4},"speaker":"B","text":"耳寄りな情報で驚きました。"}];
INSERT INTO sentence [{"id":{"d":719,"n":1,"t":4},"speaker":"A","text":"コンサートに行った後に、しばらくの間、耳鳴りがしたことはありませんか？"},{"id":{"d":719,"n":2,"t":4},"speaker":"B","text":"はい、私はコンサートの後には毎回のように耳鳴りがします。あれはどのような状態なのですか？"},{"id":{"d":719,"n":3,"t":4},"speaker":"A","text":"内耳の細胞が外傷を起こしている難聴の一種だそうです。"},{"id":{"d":719,"n":4,"t":4},"speaker":"B","text":"数時間か長くても一晩すれば治るのですが、難聴というものは勝手に治るのですか？"},{"id":{"d":719,"n":5,"t":4},"speaker":"A","text":"一過性の難聴ならば、時が経てば細胞は修復されて治ります。"},{"id":{"d":719,"n":6,"t":4},"speaker":"B","text":"そういえばミュージシャンには難聴の人が多いと聞きます。"},{"id":{"d":719,"n":7,"t":4},"speaker":"A","text":"長時間大音響を聞いていると細胞が修復できなくなり、ダメージが蓄積されて慢性的な難聴になってしまうのでしょうね。"},{"id":{"d":719,"n":8,"t":4},"speaker":"B","text":"家で音楽を聴いていても難聴になってしまうのでしょうか？"},{"id":{"d":719,"n":9,"t":4},"speaker":"A","text":"ヘッドホン難聴という言葉があるくらいですから、ヘッドホンで音楽を聴くときは注意が必要です。"},{"id":{"d":719,"n":10,"t":4},"speaker":"B","text":"ヘッドホンだからといって大音量で聞いてはいけないわけですね。"},{"id":{"d":719,"n":11,"t":4},"speaker":"A","text":"長時間ヘッドホンを使用することも難聴の原因になりますから注意してください。"}];
INSERT INTO sentence [{"id":{"d":720,"n":1,"t":4},"speaker":"A","text":"昔はたんつぼがあちこちにあったように思います。"},{"id":{"d":720,"n":2,"t":4},"speaker":"B","text":"駅のあちらこちらにたんつぼがあったことを覚えています。今はほとんど見かけませんね。"},{"id":{"d":720,"n":3,"t":4},"speaker":"A","text":"はい。今はたんを出す人も減りましたしね。"},{"id":{"d":720,"n":4,"t":4},"speaker":"B","text":"ええ。衛生面からも良いことだと思います。"},{"id":{"d":720,"n":5,"t":4},"speaker":"A","text":"かつてはたんを出している人が街中にいました。今思うと、たばこの影響もあったのかもしれません。"},{"id":{"d":720,"n":6,"t":4},"speaker":"B","text":"たばこを吸うことで喉に異常を感じて、それでたんを出すのでしょうが、たんを出してもたばこの害が軽減されるわけではありませんよね。"},{"id":{"d":720,"n":7,"t":4},"speaker":"A","text":"そうですね。風邪などでもたんは出やすいですが、どのようなときにたんが出てしまうのでしょうか？。"},{"id":{"d":720,"n":8,"t":4},"speaker":"B","text":"喉の粘膜が炎症を起こした時に出てくるのがたんです。炎症がなくなればたんは出なくなると思います。"},{"id":{"d":720,"n":9,"t":4},"speaker":"A","text":"映画やドラマでは、血が混じったたんを吐くシーンを見ることがあります。"},{"id":{"d":720,"n":10,"t":4},"speaker":"B","text":"あれは血を吐いているので、たんではありません。たんに血が混じっているのは、せきで喉の奥が切れた時です。"}];
INSERT INTO sentence [{"id":{"d":721,"n":1,"t":4},"speaker":"A","text":"最近の小学校では擦り傷の処置方法が変わってきているようです。"},{"id":{"d":721,"n":2,"t":4},"speaker":"B","text":"オキシドールを塗ってばんそうこうを貼る方法ではないのですか？"},{"id":{"d":721,"n":3,"t":4},"speaker":"A","text":"ばんそうこうを貼らない処置もあるそうですよ。ともすると傷口に何も処置をしていないように見えます。"},{"id":{"d":721,"n":4,"t":4},"speaker":"B","text":"そうなのですね。どのような処置をしているのでしょうか？"},{"id":{"d":721,"n":5,"t":4},"speaker":"A","text":"流水で30秒以上傷口を洗って砂や汚れは落とすようです。"},{"id":{"d":721,"n":6,"t":4},"speaker":"B","text":"そこまでは昔と変わりませんね。"},{"id":{"d":721,"n":7,"t":4},"speaker":"A","text":"はい、変わったのはその後です。その後で少しワセリンを塗って、場合によってはパッドを貼るそうです。"},{"id":{"d":721,"n":8,"t":4},"speaker":"B","text":"そのような治療法があるのですね。"},{"id":{"d":721,"n":9,"t":4},"speaker":"A","text":"湿潤療法と呼ぶそうです。パッドはばんそうこうのように傷にくっつかないので、剝がしても痛くありません。"}];
INSERT INTO sentence [{"id":{"d":722,"n":1,"t":4},"speaker":"A","text":"夏にスポーツをする上で気を付けていることはありますか？"},{"id":{"d":722,"n":2,"t":4},"speaker":"B","text":"水とタオルは多めに持っていきます。"},{"id":{"d":722,"n":3,"t":4},"speaker":"A","text":"水やタオルは冷やしておくのですか？"},{"id":{"d":722,"n":4,"t":4},"speaker":"B","text":"以前は水もタオルも冷やしていましたが、今は冷やしていません。"},{"id":{"d":722,"n":5,"t":4},"speaker":"A","text":"なぜ水やタオルを冷やさなくなったのですか？"},{"id":{"d":722,"n":6,"t":4},"speaker":"B","text":"ぬれたタオルより乾燥したタオルの方が汗を吸収してくれるからです。冷えた水はコンビニエンスストアや自動販売機で買って飲みます。"},{"id":{"d":722,"n":7,"t":4},"speaker":"A","text":"他に何か気を付けていることはありますか？"},{"id":{"d":722,"n":8,"t":4},"speaker":"B","text":"着替えも必ず用意します。長時間外にいる場合には余分に持っていきます。"},{"id":{"d":722,"n":9,"t":4},"speaker":"A","text":"汗でぬれたまま帰りたくないということでしょうか？"},{"id":{"d":722,"n":10,"t":4},"speaker":"B","text":"そうですね。汗にぬれた服で帰ると、電車や車のエアコンで急激に体が冷えて体調が悪くなることもあります。"},{"id":{"d":722,"n":11,"t":4},"speaker":"A","text":"それは気を付けた方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":723,"n":1,"t":4},"speaker":"A","text":"夏の走り込みは大変ですが、何か注意していることはありますか？"},{"id":{"d":723,"n":2,"t":4},"speaker":"B","text":"グラウンドは日差しがきついので、ランニングコースのある公園や早朝の川沿いを走るようにしています。"},{"id":{"d":723,"n":3,"t":4},"speaker":"A","text":"走るときの服装も何か工夫をしていますか？"},{"id":{"d":723,"n":4,"t":4},"speaker":"B","text":"帽子をかぶります。日に焼けると体温が上昇するので、日焼け防止のアームカバーも付けます。"},{"id":{"d":723,"n":5,"t":4},"speaker":"A","text":"なるほど。水は持って走るのですか？"},{"id":{"d":723,"n":6,"t":4},"speaker":"B","text":"はい。小まめに取るようにしていますので、水はポーチに入れています。"},{"id":{"d":723,"n":7,"t":4},"speaker":"A","text":"こまめな給水は大事ですね。暑過ぎた場合はランニングを中止したりするのですか？"},{"id":{"d":723,"n":8,"t":4},"speaker":"B","text":"35度を過ぎたらやめるように言われていますが、温度計を持っていないので暑いと思ったらすぐにランニングをやめるようにしています。"},{"id":{"d":723,"n":9,"t":4},"speaker":"A","text":"その時の体調も重要だと思います。"},{"id":{"d":723,"n":10,"t":4},"speaker":"B","text":"走るペースや距離は調整するようにしています。走り込みが足りないと感じたら体育館で少し走ります。"}];
INSERT INTO sentence [{"id":{"d":724,"n":1,"t":4},"speaker":"A","text":"ドナーカードで臓器提供の意思表示をしていますか？"},{"id":{"d":724,"n":2,"t":4},"speaker":"B","text":"意思表示はしていますが、ドナーカードは用いていません。今は臓器提供の意思表示はドナーカード以外でもできるのですよ。"},{"id":{"d":724,"n":3,"t":4},"speaker":"A","text":"ドナーカード以外でも表示できるのですか？"},{"id":{"d":724,"n":4,"t":4},"speaker":"B","text":"はい。健康保険証や運転免許証、マイナンバーカードでも臓器提供の意思表示ができます。"},{"id":{"d":724,"n":5,"t":4},"speaker":"A","text":"臓器提供の意思表示は何歳からでもできるのですか？"},{"id":{"d":724,"n":6,"t":4},"speaker":"B","text":"臓器提供の意思表示は15歳以上なら可能です。"},{"id":{"d":724,"n":7,"t":4},"speaker":"A","text":"15歳未満でも臓器移植はされていると思うのですが、その場合はどうなっているのですか？"},{"id":{"d":724,"n":8,"t":4},"speaker":"B","text":"本人が拒否していなければ、15歳未満でも家族の承認があれば臓器の提供が可能です。"},{"id":{"d":724,"n":9,"t":4},"speaker":"A","text":"あなたはどの方法で臓器提供の意思表示をしているのですか？"},{"id":{"d":724,"n":10,"t":4},"speaker":"B","text":"私は健康保険証と運転免許証、マイナンバーカードで意思表示をしています。"}];
INSERT INTO sentence [{"id":{"d":725,"n":1,"t":4},"speaker":"A","text":"冬のマラソンで気を付けていることはありますか？"},{"id":{"d":725,"n":2,"t":4},"speaker":"B","text":"手袋と帽子、ネックウォーマーを身に着けるようにしています。"},{"id":{"d":725,"n":3,"t":4},"speaker":"A","text":"寒そうですが、保温性に優れた機能性インナーは着ますか？"},{"id":{"d":725,"n":4,"t":4},"speaker":"B","text":"私は普通のランニングウエアとウインドブレーカーを着ています。"},{"id":{"d":725,"n":5,"t":4},"speaker":"A","text":"なぜ機能性インナーを使わないのですか？"},{"id":{"d":725,"n":6,"t":4},"speaker":"B","text":"暑くなり過ぎて熱がこもるような気がするので、普通のランニングウエアにしています。"},{"id":{"d":725,"n":7,"t":4},"speaker":"A","text":"そうなのですね。他に冬のマラソンで注意していることはありますか？"},{"id":{"d":725,"n":8,"t":4},"speaker":"B","text":"寒いと心臓に負担がかかるので、ウォーミングアップは念入りに行っています。"},{"id":{"d":725,"n":9,"t":4},"speaker":"A","text":"寒いと必要がなさそうに思えるのですが、水は持っていくのですか？"},{"id":{"d":725,"n":10,"t":4},"speaker":"B","text":"乾燥している方が脱水症状になりやすいので、水は必ず持っていきます。"}];
INSERT INTO sentence [{"id":{"d":726,"n":1,"t":4},"speaker":"A","text":"スポーツチャンバラなるスポーツがあるそうですね。"},{"id":{"d":726,"n":2,"t":4},"speaker":"B","text":"私もテレビでスポーツチャンバラを見ました。全国大会もあるらしいです。"},{"id":{"d":726,"n":3,"t":4},"speaker":"A","text":"全国大会どころか世界大会もあると聞きました。"},{"id":{"d":726,"n":4,"t":4},"speaker":"B","text":"大会を行うほど種目も分かれているのですか？"},{"id":{"d":726,"n":5,"t":4},"speaker":"A","text":"小太刀、長剣、棒や杖、やりも何種類かあるようです。"},{"id":{"d":726,"n":6,"t":4},"speaker":"B","text":"スポーツチャンバラにはたくさんの種類があるのですね。それでは試合の人数も違うのでしょうね。"},{"id":{"d":726,"n":7,"t":4},"speaker":"A","text":"合戦という試合では、具体的な人数は忘れましたが、数十人の対戦になります。1人対数人といった試合形式もありました。"},{"id":{"d":726,"n":8,"t":4},"speaker":"B","text":"違った人数で試合をするのは、恐らくチャンバラの戦い方から来ているのでしょう。"},{"id":{"d":726,"n":9,"t":4},"speaker":"A","text":"そうかもしれません。今度、近くの体育館でスポーツチャンバラの講習があるので、見学に行こうと思っています。"},{"id":{"d":726,"n":10,"t":4},"speaker":"B","text":"よろしければ私も連れて行ってください。"},{"id":{"d":726,"n":11,"t":4},"speaker":"A","text":"いいですよ。一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":727,"n":1,"t":4},"speaker":"A","text":"ボクシングの選手はグローブの下にバンテージを巻いていますが、何か理由があるのですか？"},{"id":{"d":727,"n":2,"t":4},"speaker":"B","text":"けがをしないように巻いています。試合のルールでもバンテージを巻くように決められています。"},{"id":{"d":727,"n":3,"t":4},"speaker":"A","text":"バンテージを巻くとなぜけがをしにくくなるのでしょうか？"},{"id":{"d":727,"n":4,"t":4},"speaker":"B","text":"拳にかかる衝撃を減らすとともに、手首を固定することで手首を痛めるリスクも減らしてくれるからです。"},{"id":{"d":727,"n":5,"t":4},"speaker":"A","text":"バンテージの生地は何ですか？薬局などでもバンテージは売っているのですか？"},{"id":{"d":727,"n":6,"t":4},"speaker":"B","text":"素材はコットン、レーヨン、ナイロン、ポリエステルなどです。薬局にあるかは分かりませんが、スポーツジムやフィットネスクラブでは売っています。"},{"id":{"d":727,"n":7,"t":4},"speaker":"A","text":"バンテージにはいろいろな種類があるのですね。値段はいくらくらいするのですか？"},{"id":{"d":727,"n":8,"t":4},"speaker":"B","text":"1,000円から2,000円くらいです。バンテージを購入しようと考えているのですか？"},{"id":{"d":727,"n":9,"t":4},"speaker":"A","text":"実はサンドバッグを買って家に置いているのです。"},{"id":{"d":727,"n":10,"t":4},"speaker":"B","text":"それでしたらボクシングジムかフィットネスクラブに通われてはいかがですか？そこでバンテージのことについて聞いてみてもいいと思います。"},{"id":{"d":727,"n":11,"t":4},"speaker":"A","text":"そうですね。ジムに入って、きちんとボクシングを学んだ方がいいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":728,"n":1,"t":4},"speaker":"A","text":"食後には何を飲みますか？"},{"id":{"d":728,"n":2,"t":4},"speaker":"B","text":"なるべくお茶を飲むようにしています。"},{"id":{"d":728,"n":3,"t":4},"speaker":"A","text":"お茶が好きなのですか？"},{"id":{"d":728,"n":4,"t":4},"speaker":"B","text":"はい。それに、お茶は健康にも良いと考えています。"},{"id":{"d":728,"n":5,"t":4},"speaker":"A","text":"お茶にはカテキンやカフェインが入っていますが、他にどのようなものが入っていますか？"},{"id":{"d":728,"n":6,"t":4},"speaker":"B","text":"血圧降下や抗ストレス作用があるテアニンや抗酸化作用のあるビタミン類も入っています。"},{"id":{"d":728,"n":7,"t":4},"speaker":"A","text":"カフェインも入っているので、飲み過ぎると体に悪いのではありませんか？"},{"id":{"d":728,"n":8,"t":4},"speaker":"B","text":"食後の数杯ならば、お茶のカフェインは体に悪いものではないと思います。"},{"id":{"d":728,"n":9,"t":4},"speaker":"A","text":"カテキンにはコレステロール低下や肥満予防、虫歯予防などの効果があると聞いたことがあります。食後の飲み物としては最適かもしれません。"},{"id":{"d":728,"n":10,"t":4},"speaker":"B","text":"リラックス効果もあるので、少し休みたいときにもお茶は欠かせませんね。"}];
INSERT INTO sentence [{"id":{"d":729,"n":1,"t":4},"speaker":"A","text":"脳もオーバーヒートのような状態になると聞いたことがあります。"},{"id":{"d":729,"n":2,"t":4},"speaker":"B","text":"気温や体温の上昇が原因でオーバーヒートが起きるのですか？"},{"id":{"d":729,"n":3,"t":4},"speaker":"A","text":"気温の上昇以外に、身体的ストレスでも脳は発熱しやすくなるそうです。"},{"id":{"d":729,"n":4,"t":4},"speaker":"B","text":"脳の最適温度は何度くらいなのでしょうか？"},{"id":{"d":729,"n":5,"t":4},"speaker":"A","text":"室温で22度から24度が最適だそうです。一説によると、25度以上になると、1度上がるごとに脳の作業効率が2パーセント下がるらしいですよ。"},{"id":{"d":729,"n":6,"t":4},"speaker":"B","text":"仕事場の室温は若干低めにしておく方が作業がはかどる、ということなのでしょうか？"},{"id":{"d":729,"n":7,"t":4},"speaker":"A","text":"室温の管理も大切ですが、血流を良くすることが大切だそうです。血流を良くするために、1時間に1回は体を動かして、水分を取るようにしたいです。"}];
INSERT INTO sentence [{"id":{"d":730,"n":1,"t":4},"speaker":"A","text":"朝起きると喉が渇いて少し痛みを感じるときがあります。"},{"id":{"d":730,"n":2,"t":4},"speaker":"B","text":"それは、寝ている時に口で呼吸しているからではありませんか？"},{"id":{"d":730,"n":3,"t":4},"speaker":"A","text":"やはり口呼吸のせいなのでしょうか？"},{"id":{"d":730,"n":4,"t":4},"speaker":"B","text":"その可能性はあると思います。以前からそのような状態なのですか？"},{"id":{"d":730,"n":5,"t":4},"speaker":"A","text":"喉が渇くような状態はここ最近のことです。"},{"id":{"d":730,"n":6,"t":4},"speaker":"B","text":"風邪や花粉症の時は鼻呼吸が難しくなることがありますが、あなたの場合はどうですか？"},{"id":{"d":730,"n":7,"t":4},"speaker":"A","text":"そういえば、今年は生まれて初めて花粉症のような症状になりました。そのことと関係があるのかもしれません。"},{"id":{"d":730,"n":8,"t":4},"speaker":"B","text":"素人判断はいけませんから、耳鼻咽喉科に相談した方がいいように思います。"},{"id":{"d":730,"n":9,"t":4},"speaker":"A","text":"そうですね。もしかしたら、自分で花粉症と思い込んでいるだけなのかもしれません。"},{"id":{"d":730,"n":10,"t":4},"speaker":"B","text":"病院で原因が分かれば対処方法も教えてもらえるでしょう。"}];
INSERT INTO sentence [{"id":{"d":731,"n":1,"t":4},"speaker":"A","text":"スポーツにはけががつきものとは言いますが、あなたは骨折と脱臼と捻挫の違いが分かりますか？"},{"id":{"d":731,"n":2,"t":4},"speaker":"B","text":"骨折は骨が折れた状態で、脱臼は関節から骨が外れた状態のことだと思います。"},{"id":{"d":731,"n":3,"t":4},"speaker":"A","text":"そうですね。骨にひびが入った状態も骨折と言います。脱臼はあなたのおっしゃる通りです。"},{"id":{"d":731,"n":4,"t":4},"speaker":"B","text":"捻挫とはどのような状態のことを言うのかが、私にはよく分かりません。"},{"id":{"d":731,"n":5,"t":4},"speaker":"A","text":"捻挫は人体や関節を包む膜である関節包というものが傷ついている状態を言います。"},{"id":{"d":731,"n":6,"t":4},"speaker":"B","text":"脱臼は、すぐに治るようなイメージがあります。テレビで脱臼した人の骨を関節に戻すシーンを見たことがあるからです。"},{"id":{"d":731,"n":7,"t":4},"speaker":"A","text":"早く戻すほど修復は楽だそうですが、専門家が行わないと骨折や周囲組織の損傷を起こす可能性があります。"},{"id":{"d":731,"n":8,"t":4},"speaker":"B","text":"そうなのですね。骨折と脱臼と捻挫の中で一番痛いのは骨折ですか？"},{"id":{"d":731,"n":9,"t":4},"speaker":"A","text":"そうとも言い切れません。脱臼はショック状態になるほど痛い場合があります。"}];
INSERT INTO sentence [{"id":{"d":732,"n":1,"t":4},"speaker":"A","text":"外傷が生じたときに行う「RICE処置」について、聞いたことはありますか？"},{"id":{"d":732,"n":2,"t":4},"speaker":"B","text":"いいえ、聞いたことがありません。どのような処置ですか？"},{"id":{"d":732,"n":3,"t":4},"speaker":"A","text":"「RICE処置」は、「安静」のRest、「冷却」のIcing、「圧迫」のCompression、「挙上」のElevationの頭文字に由来しています。外傷が生じたときに、この四つの処置をできる限り早く行うことが望ましいとされています。"},{"id":{"d":732,"n":4,"t":4},"speaker":"B","text":"それは、病院に行く前の処置なのでしょうか？"},{"id":{"d":732,"n":5,"t":4},"speaker":"A","text":"はい、そうです。"},{"id":{"d":732,"n":6,"t":4},"speaker":"B","text":"安静は体を動かさず静かにするということだとイメージできるのですが、冷却とは具体的にどの程度冷やすのですか？"},{"id":{"d":732,"n":7,"t":4},"speaker":"A","text":"感覚がなくなるくらいまで、20分程度冷やします。それを何度か繰り返します。"},{"id":{"d":732,"n":8,"t":4},"speaker":"B","text":"なるほど。圧迫と拳上のこつはありますか？"},{"id":{"d":732,"n":9,"t":4},"speaker":"A","text":"外傷のある部分を包帯やテーピングで軽めに圧迫します。拳上は患部を心臓より高い位置に上げておくことです。"}];
INSERT INTO sentence [{"id":{"d":733,"n":1,"t":4},"speaker":"A","text":"突き指とはけがの種類の正式名なのでしょうか？？"},{"id":{"d":733,"n":2,"t":4},"speaker":"B","text":"突き指は、それが起きた原因や経緯のことを指します。正式名では捻挫になることが多いですが、骨折や脱臼になることもありますよ。"},{"id":{"d":733,"n":3,"t":4},"speaker":"A","text":"突き指で骨折になることもあるのですか？"},{"id":{"d":733,"n":4,"t":4},"speaker":"B","text":"はい、ありますよ。"},{"id":{"d":733,"n":5,"t":4},"speaker":"A","text":"突き指と言われると軽症で済んだと思ってしまいますが、骨折だと重症に思えます。"},{"id":{"d":733,"n":6,"t":4},"speaker":"B","text":"骨折といっても指先なので、固定することによって修復ができる場合がほとんどです。骨折よりもむしろ腱断裂の方が重症で、手術の可能性が高いです。"},{"id":{"d":733,"n":7,"t":4},"speaker":"A","text":"突き指では病院に行かない人が多いと思いますが、それは危険なのでしょうか？"},{"id":{"d":733,"n":8,"t":4},"speaker":"B","text":"そうですね。場合によっては指が変形したまま固まることがあります。そうなってしまうと日常生活にも影響が及びます。"},{"id":{"d":733,"n":9,"t":4},"speaker":"A","text":"たかが突き指と軽く考えず、痛みがあれば病院で診てもらう方が安心ですね。"},{"id":{"d":733,"n":10,"t":4},"speaker":"B","text":"そのとおりです。"}];
INSERT INTO sentence [{"id":{"d":734,"n":1,"t":4},"speaker":"A","text":"口を開ける時に顎の辺りで音がするのですが、どうしたらよいでしょうか？"},{"id":{"d":734,"n":2,"t":4},"speaker":"B","text":"それは、恐らく顎関節症の症状だと思います。私も時々同じようになります。"},{"id":{"d":734,"n":3,"t":4},"speaker":"A","text":"なぜ顎関節症になるのでしょうか？"},{"id":{"d":734,"n":4,"t":4},"speaker":"B","text":"急激なストレス、歯の不調、悪い姿勢など、いくつか要因はあるようです。"},{"id":{"d":734,"n":5,"t":4},"speaker":"A","text":"なるほど。どのような対処方法が良いかご存じですか？"},{"id":{"d":734,"n":6,"t":4},"speaker":"B","text":"原因が分かればそれを取り除くようにすればよいのですが、他に理学療法や内服療法、私がしているスプリント治療がありますよ。"},{"id":{"d":734,"n":7,"t":4},"speaker":"A","text":"スプリント治療とはどのような治療法ですか？"},{"id":{"d":734,"n":8,"t":4},"speaker":"B","text":"マウスピースを装着する方法です。"},{"id":{"d":734,"n":9,"t":4},"speaker":"A","text":"今のところ痛みは感じられませんが、私も病院に行った方がいいでしょうか？"},{"id":{"d":734,"n":10,"t":4},"speaker":"B","text":"顎関節症は放置していても治るともいわれていますが、原因を知るためにも病院に行くことをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":735,"n":1,"t":4},"speaker":"A","text":"地方自治体で受診できるがん検診は、全部で何種類あるのですか？"},{"id":{"d":735,"n":2,"t":4},"speaker":"B","text":"正確には分かりませんが、胃がん検診、子宮頸がん検診、肺がん検診、乳がん検診、大腸がん検診あたりが一般的でしょうか。口腔がん検診や前立腺がん検診を提供している自治体もあるようです。"},{"id":{"d":735,"n":3,"t":4},"speaker":"A","text":"前立腺がん検診は男性だけで、子宮頸がん検診、乳がん検診は女性だけが対象ですね。"},{"id":{"d":735,"n":4,"t":4},"speaker":"B","text":"そうです。"},{"id":{"d":735,"n":5,"t":4},"speaker":"A","text":"女性の方が検診の項目数が多く、費用も高くなっているのですね。"},{"id":{"d":735,"n":6,"t":4},"speaker":"B","text":"まさに、そのことを問題視している自治体もあるようです。"}];
INSERT INTO sentence [{"id":{"d":736,"n":1,"t":4},"speaker":"A","text":"お客様、何かお探しですか？"},{"id":{"d":736,"n":2,"t":4},"speaker":"B","text":"父の電気シェーバーが壊れてしまったので新しいものを買いたいのです。"},{"id":{"d":736,"n":3,"t":4},"speaker":"A","text":"電気シェーバーでしたら、こちらにございます。"},{"id":{"d":736,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。でも、たくさんの種類があって、どれがいいか分かりません。"},{"id":{"d":736,"n":5,"t":4},"speaker":"A","text":"一番人気があるのはこちらの製品ですが、いかがでしょう？"},{"id":{"d":736,"n":6,"t":4},"speaker":"B","text":"他のものと比べてどこが良いのですか？"},{"id":{"d":736,"n":7,"t":4},"speaker":"A","text":"しっかりそれて水洗いができることです。"},{"id":{"d":736,"n":8,"t":4},"speaker":"B","text":"父が今まで使っていたものも水洗いができるようでした。これにします。"}];
INSERT INTO sentence [{"id":{"d":737,"n":1,"t":4},"speaker":"A","text":"近頃は太ってきて困っています。"},{"id":{"d":737,"n":2,"t":4},"speaker":"B","text":"何か思い当たる原因はありますか？"},{"id":{"d":737,"n":3,"t":4},"speaker":"A","text":"空腹で眠れないので、夜中にカップラーメンを食べています。"},{"id":{"d":737,"n":4,"t":4},"speaker":"B","text":"それが原因に間違いありませんね。"},{"id":{"d":737,"n":5,"t":4},"speaker":"A","text":"駄目だとは思うのですが、眠れないので仕方なく食べています。"},{"id":{"d":737,"n":6,"t":4},"speaker":"B","text":"食べるものを変えたらいかがですか？例えば、こんにゃくゼリーやダイエットクッキーはいかがでしょう。"},{"id":{"d":737,"n":7,"t":4},"speaker":"A","text":"こんにゃくゼリーは良いかもしれませんね。"},{"id":{"d":737,"n":8,"t":4},"speaker":"B","text":"これから暑くなるのでゼリーはお薦めですよ。"}];
INSERT INTO sentence [{"id":{"d":738,"n":1,"t":4},"speaker":"A","text":"スマートフォンの見過ぎで目が痛いです。"},{"id":{"d":738,"n":2,"t":4},"speaker":"B","text":"画面を見るのを少し控えたらよいのではありませんか？"},{"id":{"d":738,"n":3,"t":4},"speaker":"A","text":"仕事のメールがひっきりなしに入ってきて、返信をしなくてはならないので、そうも言っていられません。"},{"id":{"d":738,"n":4,"t":4},"speaker":"B","text":"お忙しいのですね。"},{"id":{"d":738,"n":5,"t":4},"speaker":"A","text":"はい、日々忙しいです。"},{"id":{"d":738,"n":6,"t":4},"speaker":"B","text":"目薬もありますけれど、目をぎゅっと閉じてぱっと開いたりする疲れ目体操もよく効きますよ。"},{"id":{"d":738,"n":7,"t":4},"speaker":"A","text":"それを教えてくれますか？"},{"id":{"d":738,"n":8,"t":4},"speaker":"B","text":"たくさんの種類があるので一つずつ教えますね。"}];
INSERT INTO sentence [{"id":{"d":739,"n":1,"t":4},"speaker":"A","text":"口紅をしていると、カップに口を付けた時に跡が付きませんか？"},{"id":{"d":739,"n":2,"t":4},"speaker":"B","text":"私が使っているのはリップクリームなので、そのようなことはほとんどありません。"},{"id":{"d":739,"n":3,"t":4},"speaker":"A","text":"本当ですね、カップに跡が付いていません。"},{"id":{"d":739,"n":4,"t":4},"speaker":"B","text":"跡は付かないけれど唇は赤いでしょう？"},{"id":{"d":739,"n":5,"t":4},"speaker":"A","text":"はい、口紅を付けていらっしゃるのだと思っていました。"},{"id":{"d":739,"n":6,"t":4},"speaker":"B","text":"リップクリームでも色が付いているものがあるのですよ。"},{"id":{"d":739,"n":7,"t":4},"speaker":"A","text":"そうなのですね。リップクリームということは保湿もできるのですか？"},{"id":{"d":739,"n":8,"t":4},"speaker":"B","text":"その通りです。とても便利なのでお薦めですよ。"}];
INSERT INTO sentence [{"id":{"d":740,"n":1,"t":4},"speaker":"A","text":"昨日の相撲を見ましたか？"},{"id":{"d":740,"n":2,"t":4},"speaker":"B","text":"見ていません。何かあったのですか？"},{"id":{"d":740,"n":3,"t":4},"speaker":"A","text":"一本背負いが決まったのですよ。"},{"id":{"d":740,"n":4,"t":4},"speaker":"B","text":"一本背負いというのは柔道の技ではありませんか？"},{"id":{"d":740,"n":5,"t":4},"speaker":"A","text":"相撲にもありますよ。ただ、めったに見られない大技なのです。"},{"id":{"d":740,"n":6,"t":4},"speaker":"B","text":"それで興奮しているのですね。"},{"id":{"d":740,"n":7,"t":4},"speaker":"A","text":"初めて見ましたからね。動画でもう一度見ようと思っています。"},{"id":{"d":740,"n":8,"t":4},"speaker":"B","text":"それほどすごいのなら私も動画で見てみます。"}];
INSERT INTO sentence [{"id":{"d":741,"n":1,"t":4},"speaker":"A","text":"明日は会社の健康診断です。ため息が出ます。"},{"id":{"d":741,"n":2,"t":4},"speaker":"B","text":"何か不安なことでもあるのですか？"},{"id":{"d":741,"n":3,"t":4},"speaker":"A","text":"毎年、バリウムを飲まされるのが憂鬱で仕方がありません。"},{"id":{"d":741,"n":4,"t":4},"speaker":"B","text":"バリウムはお世辞にもおいしいとは言えませんからね。"},{"id":{"d":741,"n":5,"t":4},"speaker":"A","text":"先輩方は「それでも昔よりは飲みやすくなりました」とおっしゃいますよ。"},{"id":{"d":741,"n":6,"t":4},"speaker":"B","text":"それはその通りです。それでも、昔の味を知らない君たちからしたら、まずいものはまずいですよね。"},{"id":{"d":741,"n":7,"t":4},"speaker":"A","text":"はい。苦手な味です。"},{"id":{"d":741,"n":8,"t":4},"speaker":"B","text":"でも、飲まないと検診が受けられませんからね。頑張って飲んでください。"}];
INSERT INTO sentence [{"id":{"d":742,"n":1,"t":4},"speaker":"A","text":"明日から、朝の時間を利用した活動を始めます。"},{"id":{"d":742,"n":2,"t":4},"speaker":"B","text":"お姉さん、それは何回目の宣言ですか？"},{"id":{"d":742,"n":3,"t":4},"speaker":"A","text":"3回目かもしれません。"},{"id":{"d":742,"n":4,"t":4},"speaker":"B","text":"私の記憶では5回目です。"},{"id":{"d":742,"n":5,"t":4},"speaker":"A","text":"そうでしたか？"},{"id":{"d":742,"n":6,"t":4},"speaker":"B","text":"今まで、読書会、ジョギング、英会話、ヨガと、宣言して2日でやめていますよ。"},{"id":{"d":742,"n":7,"t":4},"speaker":"A","text":"今度は大好きな水泳なので続くと思いますよ。"},{"id":{"d":742,"n":8,"t":4},"speaker":"B","text":"いいですね。水泳は私も好きだから一緒に行こうと思います。"}];
INSERT INTO sentence [{"id":{"d":743,"n":1,"t":4},"speaker":"A","text":"凛ちゃんが一緒に来てくれたおかげで、朝の水泳が1週間も続きました。"},{"id":{"d":743,"n":2,"t":4},"speaker":"B","text":"それはよかったです。お姉さんのおかげで、朝から泳げる場所を知ることができて、私もよかったです。"},{"id":{"d":743,"n":3,"t":4},"speaker":"A","text":"お互いにとって良かったですね。"},{"id":{"d":743,"n":4,"t":4},"speaker":"B","text":"はい。そういえば、土日はお休みなのでしょう？"},{"id":{"d":743,"n":5,"t":4},"speaker":"A","text":"そうです。ジムの平日だけのコースですからね。"},{"id":{"d":743,"n":6,"t":4},"speaker":"B","text":"では、次は月曜日ですね。月曜日に突然「やめる」なんて言わないでくださいね。"},{"id":{"d":743,"n":7,"t":4},"speaker":"A","text":"言わないとも言い切れないので、朝になったら、起こしてもらえませんか？"},{"id":{"d":743,"n":8,"t":4},"speaker":"B","text":"わかりました。世話が焼ける姉を持つと苦労します。"}];
INSERT INTO sentence [{"id":{"d":744,"n":1,"t":4},"speaker":"A","text":"お昼ご飯を食べに行きませんか？"},{"id":{"d":744,"n":2,"t":4},"speaker":"B","text":"今日は遠慮しておきます。"},{"id":{"d":744,"n":3,"t":4},"speaker":"A","text":"どうかしたのですか？"},{"id":{"d":744,"n":4,"t":4},"speaker":"B","text":"口内炎がひどくて、お茶を飲むのもつらいのです。"},{"id":{"d":744,"n":5,"t":4},"speaker":"A","text":"あまりにひどいときは病院に行かれた方がいいですよ。"},{"id":{"d":744,"n":6,"t":4},"speaker":"B","text":"退社後にすぐ行けるように病院を調べておきます。"},{"id":{"d":744,"n":7,"t":4},"speaker":"A","text":"それがいいと思います。"},{"id":{"d":744,"n":8,"t":4},"speaker":"B","text":"お昼に一人にしてしまってごめんなさい。"},{"id":{"d":744,"n":9,"t":4},"speaker":"A","text":"お気になさらず。お大事になさってくださいね。"}];
INSERT INTO sentence [{"id":{"d":745,"n":1,"t":4},"speaker":"A","text":"夜に寝ていると足がつるのです。"},{"id":{"d":745,"n":2,"t":4},"speaker":"B","text":"何か対策はしていますか？"},{"id":{"d":745,"n":3,"t":4},"speaker":"A","text":"起き上がってマッサージをするのですが、眠れなくなるのが悩みの種です。"},{"id":{"d":745,"n":4,"t":4},"speaker":"B","text":"病院には行かれましたか？"},{"id":{"d":745,"n":5,"t":4},"speaker":"A","text":"行っていません。"},{"id":{"d":745,"n":6,"t":4},"speaker":"B","text":"漢方薬を処方してもらえるらしいですよ。"},{"id":{"d":745,"n":7,"t":4},"speaker":"A","text":"お薬で治るのですか？"},{"id":{"d":745,"n":8,"t":4},"speaker":"B","text":"お隣の家のおばあさんがそう言っていました。"}];
INSERT INTO sentence [{"id":{"d":746,"n":1,"t":4},"speaker":"A","text":"子どもが熱を測るときに嫌がって困っています。"},{"id":{"d":746,"n":2,"t":4},"speaker":"B","text":"どのような体温計を使っているのですか？"},{"id":{"d":746,"n":3,"t":4},"speaker":"A","text":"脇に挟んで3分待つデジタル体温計です。"},{"id":{"d":746,"n":4,"t":4},"speaker":"B","text":"小さい子どもには3分はつらいかもしれませんね。"},{"id":{"d":746,"n":5,"t":4},"speaker":"A","text":"はい。いつも途中で腕を動かしてしまいます。"},{"id":{"d":746,"n":6,"t":4},"speaker":"B","text":"最近は数秒で測れるものや非接触で測れるものもありますよ。"},{"id":{"d":746,"n":7,"t":4},"speaker":"A","text":"本当ですか？知りませんでした。どこで買えますか？"},{"id":{"d":746,"n":8,"t":4},"speaker":"B","text":"医療用というわけではないので、薬局に行けば買えますよ。"}];
INSERT INTO sentence [{"id":{"d":747,"n":1,"t":4},"speaker":"A","text":"今日は眼鏡をかけているのですね。"},{"id":{"d":747,"n":2,"t":4},"speaker":"B","text":"はい。ものもらいができてしまって、コンタクトレンズがつけられないのです。"},{"id":{"d":747,"n":3,"t":4},"speaker":"A","text":"本当ですね。めばちこができています。"},{"id":{"d":747,"n":4,"t":4},"speaker":"B","text":"ものもらいを「めばちこ」と言うのですか？"},{"id":{"d":747,"n":5,"t":4},"speaker":"A","text":"私の家ではそう言います。"},{"id":{"d":747,"n":6,"t":4},"speaker":"B","text":"ものもらいと同じものなのでしょうか？"},{"id":{"d":747,"n":7,"t":4},"speaker":"A","text":"同じものだと前の職場の方に教えていただきました。"},{"id":{"d":747,"n":8,"t":4},"speaker":"B","text":"いろいろな言い方があるのですね。"}];
INSERT INTO sentence [{"id":{"d":748,"n":1,"t":4},"speaker":"A","text":"ものもらいが治らないので眼科に行ってきました。"},{"id":{"d":748,"n":2,"t":4},"speaker":"B","text":"和子さんの目の調子はいかがですか？"},{"id":{"d":748,"n":3,"t":4},"speaker":"A","text":"腫れが少し引いてきたと思います。"},{"id":{"d":748,"n":4,"t":4},"speaker":"B","text":"本当ですね。美人が復活してきました。"},{"id":{"d":748,"n":5,"t":4},"speaker":"A","text":"お世辞がうまいですね。"},{"id":{"d":748,"n":6,"t":4},"speaker":"B","text":"これでコンタクトレンズをつければ完璧ですよ。"},{"id":{"d":748,"n":7,"t":4},"speaker":"A","text":"この眼鏡は私に似合いませんか？"},{"id":{"d":748,"n":8,"t":4},"speaker":"B","text":"それはずっとかけていなかった眼鏡でしょう？少し流行遅れかもしれません。"}];
INSERT INTO sentence [{"id":{"d":749,"n":1,"t":4},"speaker":"A","text":"銭湯でやっている健康体操の教室に行ってきました。"},{"id":{"d":749,"n":2,"t":4},"speaker":"B","text":"お年寄りが行く体操教室ではないのですか？"},{"id":{"d":749,"n":3,"t":4},"speaker":"A","text":"母を連れて行ったのです。"},{"id":{"d":749,"n":4,"t":4},"speaker":"B","text":"そういうことだったのですね。"},{"id":{"d":749,"n":5,"t":4},"speaker":"A","text":"一緒に体操をしたのですが、意外と大変でした。"},{"id":{"d":749,"n":6,"t":4},"speaker":"B","text":"お年寄り向けなのに大変なのですか？"},{"id":{"d":749,"n":7,"t":4},"speaker":"A","text":"日頃の運動不足は母の比ではないことが判明しました。"},{"id":{"d":749,"n":8,"t":4},"speaker":"B","text":"毎日その体操をするといいですよ。"}];
INSERT INTO sentence [{"id":{"d":750,"n":1,"t":4},"speaker":"A","text":"眠そうですね。"},{"id":{"d":750,"n":2,"t":4},"speaker":"B","text":"昨日はナイターを見に行ったのですが、延長戦になってしまって帰りが遅かったのです。"},{"id":{"d":750,"n":3,"t":4},"speaker":"A","text":"昨日の試合ですか？"},{"id":{"d":750,"n":4,"t":4},"speaker":"B","text":"そうです。"},{"id":{"d":750,"n":5,"t":4},"speaker":"A","text":"確かに、深夜のスポーツニュースでも結果が報道されませんでしたからね。その時間は、まだ試合をしていたということなのでしょう。"},{"id":{"d":750,"n":6,"t":4},"speaker":"B","text":"そうなのです。試合が終わった頃には所沢から川崎に帰る手段がなくなってしまって、蒲田からタクシーに乗りました。"},{"id":{"d":750,"n":7,"t":4},"speaker":"A","text":"それは大変でしたね。"},{"id":{"d":750,"n":8,"t":4},"speaker":"B","text":"おまけに応援していたチームも負けてしまいました。踏んだり蹴ったりです。"}];
INSERT INTO sentence [{"id":{"d":751,"n":1,"t":4},"speaker":"A","text":"今の球は絶対にボールですよね？"},{"id":{"d":751,"n":2,"t":4},"speaker":"B","text":"そうでしたか？"},{"id":{"d":751,"n":3,"t":4},"speaker":"A","text":"絶対にそうです。ひいきのチームの応援に熱くなり過ぎですよ。"},{"id":{"d":751,"n":4,"t":4},"speaker":"B","text":"そちらはクール過ぎます。"},{"id":{"d":751,"n":5,"t":4},"speaker":"A","text":"私はどちらのチームのファンでもありませんから。"},{"id":{"d":751,"n":6,"t":4},"speaker":"B","text":"では、どうして球場にまで来たのですか？"},{"id":{"d":751,"n":7,"t":4},"speaker":"A","text":"夜空の下で飲むビールが最高だからです。"}];
INSERT INTO sentence [{"id":{"d":752,"n":1,"t":4},"speaker":"A","text":"応援していたチームが勝った試合の後のビールは最高です。"},{"id":{"d":752,"n":2,"t":4},"speaker":"B","text":"途中はひやひやする展開もありましたが、最後は勝利できて良かったです。"},{"id":{"d":752,"n":3,"t":4},"speaker":"A","text":"そうですね。しかし、ナイターは良いですよね。"},{"id":{"d":752,"n":4,"t":4},"speaker":"B","text":"同意します。特に夏は最高ですね。"},{"id":{"d":752,"n":5,"t":4},"speaker":"A","text":"明日も行きませんか？"},{"id":{"d":752,"n":6,"t":4},"speaker":"B","text":"明日は月曜日なので、試合はありませんよ。"},{"id":{"d":752,"n":7,"t":4},"speaker":"A","text":"あれ、そうでしたか？"},{"id":{"d":752,"n":8,"t":4},"speaker":"B","text":"はい。次の試合は火曜日です。"}];
INSERT INTO sentence [{"id":{"d":753,"n":1,"t":4},"speaker":"A","text":"昨日は、カーリングの体験教室に参加してきました。"},{"id":{"d":753,"n":2,"t":4},"speaker":"B","text":"面白そうですね。どうでしたか？"},{"id":{"d":753,"n":3,"t":4},"speaker":"A","text":"楽しかったのですが、氷の上をまともに歩けなくて大変でした。"},{"id":{"d":753,"n":4,"t":4},"speaker":"B","text":"滑り止めの靴があるのではありませんか？"},{"id":{"d":753,"n":5,"t":4},"speaker":"A","text":"カーリングでは、ストーンを投げながら滑らなくてはならないので、片方の靴は滑るようにできています。そのため、たくさん転びました。"},{"id":{"d":753,"n":6,"t":4},"speaker":"B","text":"そうなのですね。腰が痛そうにしていたのは、そのせいだったのですね。"}];
INSERT INTO sentence [{"id":{"d":754,"n":1,"t":4},"speaker":"A","text":"和子さん、腰の痛みは引きましたか？"},{"id":{"d":754,"n":2,"t":4},"speaker":"B","text":"はい。昼休みにネットカフェへ行ってマッサージ機を使ってきたら、だいぶ楽になりました。"},{"id":{"d":754,"n":3,"t":4},"speaker":"A","text":"良いことを考えましたね。"},{"id":{"d":754,"n":4,"t":4},"speaker":"B","text":"医務室で湿布ももらったのですが、マッサージ機の方が効いたような気がします。"},{"id":{"d":754,"n":5,"t":4},"speaker":"A","text":"和子さん、帰りに一緒にスーパー銭湯に行って体をほぐしませんか？"},{"id":{"d":754,"n":6,"t":4},"speaker":"B","text":"それも良いアイデアですね。ぜひ行きたいです。"},{"id":{"d":754,"n":7,"t":4},"speaker":"A","text":"私がよく行くところに一緒に行きましょう。"},{"id":{"d":754,"n":8,"t":4},"speaker":"B","text":"ありがたいです。今日は絶対に定時で上がります。"}];
INSERT INTO sentence [{"id":{"d":755,"n":1,"t":4},"speaker":"A","text":"スーパー銭湯でお湯に浸かったら、体がとても楽になりました。"},{"id":{"d":755,"n":2,"t":4},"speaker":"B","text":"それは良かったです。和子さんの体があざだらけで驚きましたよ。"},{"id":{"d":755,"n":3,"t":4},"speaker":"A","text":"驚かせてしまってすみません。自分でも気が付かないところで打っていたようです。"},{"id":{"d":755,"n":4,"t":4},"speaker":"B","text":"湿布を貼るなどして、よく手当してくださいね。"},{"id":{"d":755,"n":5,"t":4},"speaker":"A","text":"はい、そうします。"},{"id":{"d":755,"n":6,"t":4},"speaker":"B","text":"よければこの湿布をどうぞ。和子さんにお夕飯をおごっていただきましたし、この湿布はそのお礼を兼ねたお見舞いです。"},{"id":{"d":755,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。寝る前に使わせていただきます。"},{"id":{"d":755,"n":8,"t":4},"speaker":"B","text":"早く治るといいですね。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":756,"n":1,"t":4},"speaker":"A","text":"コーヒーの飲み過ぎのせいか、最近、胃の調子が悪いです。"},{"id":{"d":756,"n":2,"t":4},"speaker":"B","text":"あなたのそのマイボトルに入っているのは、コーヒーなのですか？"},{"id":{"d":756,"n":3,"t":4},"speaker":"A","text":"そうです。毎朝、行きつけのコーヒーショップで入れてもらっています。"},{"id":{"d":756,"n":4,"t":4},"speaker":"B","text":"原因が分かっているのなら、しばらくコーヒーをやめてみてはいかがですか？"},{"id":{"d":756,"n":5,"t":4},"speaker":"A","text":"それはその通りなのですが、ショップの店員さんと会話をするのが楽しいので、やめたくないのです。"},{"id":{"d":756,"n":6,"t":4},"speaker":"B","text":"そのショップにはコーヒー以外も売っているでしょう？"},{"id":{"d":756,"n":7,"t":4},"speaker":"A","text":"はい、その他の飲み物も売っています。"},{"id":{"d":756,"n":8,"t":4},"speaker":"B","text":"それなら、そのショップでコーヒーではないものを買えばよいのです。"}];
INSERT INTO sentence [{"id":{"d":757,"n":1,"t":4},"speaker":"A","text":"うっかりマスクをしたまま日焼けしてしまいました。"},{"id":{"d":757,"n":2,"t":4},"speaker":"B","text":"そのマスクを外すと口元だけが肌の色が白いのですか？"},{"id":{"d":757,"n":3,"t":4},"speaker":"A","text":"そうです。恥ずかしくてマスクを取ることができません。"},{"id":{"d":757,"n":4,"t":4},"speaker":"B","text":"でも、あなたは、明日の会議で新商品のプレゼンテーションをする必要がありますよね。"},{"id":{"d":757,"n":5,"t":4},"speaker":"A","text":"そうなのです。プレゼンテーションの時はマスクを外すように言われているので、困りました。"},{"id":{"d":757,"n":6,"t":4},"speaker":"B","text":"口元に少し濃い目のファンデーションを塗るのはどうでしょう？"},{"id":{"d":757,"n":7,"t":4},"speaker":"A","text":"口元だけに塗るのですか？"},{"id":{"d":757,"n":8,"t":4},"speaker":"B","text":"はい。そうすることで、日焼けした部分の肌と似た色になるのでないかと思います。"},{"id":{"d":757,"n":9,"t":4},"speaker":"A","text":"なるほど、それは良い案かもしれません。試してみます。"}];
INSERT INTO sentence [{"id":{"d":758,"n":1,"t":4},"speaker":"A","text":"皆さん、お散歩から帰ったら、手を洗うのですよ。"},{"id":{"d":758,"n":2,"t":4},"speaker":"B","text":"はい、先生！"},{"id":{"d":758,"n":3,"t":4},"speaker":"A","text":"せっけんをつけて、指の間もよく洗ってくださいね。"},{"id":{"d":758,"n":4,"t":4},"speaker":"B","text":"お砂場で遊んだので、爪の間に土が入ってしまっています。"},{"id":{"d":758,"n":5,"t":4},"speaker":"A","text":"本当ですね。"},{"id":{"d":758,"n":6,"t":4},"speaker":"B","text":"普通に洗うだけでは爪の間の土は落ちないようです。"},{"id":{"d":758,"n":7,"t":4},"speaker":"A","text":"もう片方の手で、爪先をよくこすってみてはどうですか？"}];
INSERT INTO sentence [{"id":{"d":759,"n":1,"t":4},"speaker":"A","text":"まあ、その顔のガーゼはどうしたのですか？"},{"id":{"d":759,"n":2,"t":4},"speaker":"B","text":"日曜日に転んで目の上を切ってしまいました。"},{"id":{"d":759,"n":3,"t":4},"speaker":"A","text":"それでガーゼを貼っているのですね。ひどいけがのようですが、病院へは行きましたか？"},{"id":{"d":759,"n":4,"t":4},"speaker":"B","text":"けがをしたのが日曜日でしたので、開いている病院を探すのが大変でしたが、救急病院でどうにか診ていただけました。"},{"id":{"d":759,"n":5,"t":4},"speaker":"A","text":"良かったですね。目は大丈夫だったのですか？"},{"id":{"d":759,"n":6,"t":4},"speaker":"B","text":"幸いなことにまぶたのけがだけで済みました。それでも、2針も縫いましたよ。"},{"id":{"d":759,"n":7,"t":4},"speaker":"A","text":"大変でしたね。どうかお大事になさってください。"},{"id":{"d":759,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":760,"n":1,"t":4},"speaker":"A","text":"あなたは肌がきれいでうらやましいです。"},{"id":{"d":760,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":760,"n":3,"t":4},"speaker":"A","text":"どのようなお手入れをされているのですか？"},{"id":{"d":760,"n":4,"t":4},"speaker":"B","text":"特に変わったことは何もしていませんよ。"},{"id":{"d":760,"n":5,"t":4},"speaker":"A","text":"肌がきれいな方は皆さんそうおっしゃいますね。"},{"id":{"d":760,"n":6,"t":4},"speaker":"B","text":"そういわれても、普通に洗顔をした後に化粧水と乳液を付けているだけです。"},{"id":{"d":760,"n":7,"t":4},"speaker":"A","text":"使っている化粧品は高級品ですか？"},{"id":{"d":760,"n":8,"t":4},"speaker":"B","text":"そこそこのお値段はしますが、そこまで高級品というわけではありませんよ。"}];
INSERT INTO sentence [{"id":{"d":761,"n":1,"t":4},"speaker":"A","text":"草野球のメンバーが足りないので、日曜日の大会に参加してもらえませんか？"},{"id":{"d":761,"n":2,"t":4},"speaker":"B","text":"よいですよ。何人足りないのですか？"},{"id":{"d":761,"n":3,"t":4},"speaker":"A","text":"3人ほど足りません。できればピッチャーが欲しいです。"},{"id":{"d":761,"n":4,"t":4},"speaker":"B","text":"では、高校時代の野球部仲間に声を掛けてみてもよいですか？"},{"id":{"d":761,"n":5,"t":4},"speaker":"A","text":"ありがたいです。ピッチャーはいますか？"},{"id":{"d":761,"n":6,"t":4},"speaker":"B","text":"控えの経験がある人ならいますが、それほどうまくはありませんよ。"},{"id":{"d":761,"n":7,"t":4},"speaker":"A","text":"それでもよいです。お願いします。"},{"id":{"d":761,"n":8,"t":4},"speaker":"B","text":"分かりました。声を掛けてみます。"}];
INSERT INTO sentence [{"id":{"d":762,"n":1,"t":4},"speaker":"A","text":"公園のジョギングコースを走ってきました。"},{"id":{"d":762,"n":2,"t":4},"speaker":"B","text":"急に走るには長い距離だと思いますが、大丈夫でしたか？"},{"id":{"d":762,"n":3,"t":4},"speaker":"A","text":"私が走ったのはジョギングコースの一部だけなので、大丈夫でしたよ。"},{"id":{"d":762,"n":4,"t":4},"speaker":"B","text":"それならよいと思いますが、無理はしないでくださいね。"},{"id":{"d":762,"n":5,"t":4},"speaker":"A","text":"はい。毎日少しずつ距離を延ばしていこうと思っています。"},{"id":{"d":762,"n":6,"t":4},"speaker":"B","text":"やる気満々ですね。"},{"id":{"d":762,"n":7,"t":4},"speaker":"A","text":"はい。今年の町民運動会では、50代の部で入賞することを目指しています。"},{"id":{"d":762,"n":8,"t":4},"speaker":"B","text":"素晴らしい目標ですね。けがをしない程度に頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":763,"n":1,"t":4},"speaker":"A","text":"足がむくんでしまって、靴がきついです。"},{"id":{"d":763,"n":2,"t":4},"speaker":"B","text":"今日は一日中、立ち仕事でしたからね。"},{"id":{"d":763,"n":3,"t":4},"speaker":"A","text":"はい。立ち仕事でも足が痛くならないパンプスはないのでしょうか？"},{"id":{"d":763,"n":4,"t":4},"speaker":"B","text":"靴の専門店に行くと、足に合わせていろいろと調整してくれますよ。"},{"id":{"d":763,"n":5,"t":4},"speaker":"A","text":"それは良いですね。少々価格は高くても、快適な靴を入手したいです。"},{"id":{"d":763,"n":6,"t":4},"speaker":"B","text":"以前、私が靴を作ってもらった専門店でよければ紹介しますよ。"},{"id":{"d":763,"n":7,"t":4},"speaker":"A","text":"ぜひお願いします。今日のむくんだ状態で見てもらいたいです。"},{"id":{"d":763,"n":8,"t":4},"speaker":"B","text":"分かりました。仕事が終わったら行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":764,"n":1,"t":4},"speaker":"A","text":"今日は隣の課の木戸さんがいませんね。"},{"id":{"d":764,"n":2,"t":4},"speaker":"B","text":"木戸さんは今日から産休ですよ。"},{"id":{"d":764,"n":3,"t":4},"speaker":"A","text":"今日からだったのですね。"},{"id":{"d":764,"n":4,"t":4},"speaker":"B","text":"はい。あまり妊婦さんらしいファッションではなかったので、驚く方が多いですね。"},{"id":{"d":764,"n":5,"t":4},"speaker":"A","text":"私も最初はかわいいワンピースを着ているだけだと思っていました。戻ってこられるはいつですか？"},{"id":{"d":764,"n":6,"t":4},"speaker":"B","text":"育児休暇もありますから、1年はお休みですね。"},{"id":{"d":764,"n":7,"t":4},"speaker":"A","text":"無事に生まれたら出産祝いを贈りたいのですが、どなたかが取りまとめているのですか？"},{"id":{"d":764,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。私と隣の課の太田さんが取りまとめています。"}];
INSERT INTO sentence [{"id":{"d":765,"n":1,"t":4},"speaker":"A","text":"最近、あなたはよく眼鏡を拭いていますね。"},{"id":{"d":765,"n":2,"t":4},"speaker":"B","text":"マスカラを新しいものに変えたら、レンズが汚れるようになってしまったのです。"},{"id":{"d":765,"n":3,"t":4},"speaker":"A","text":"まつ毛が長くなり過ぎてしまうということですか？"},{"id":{"d":765,"n":4,"t":4},"speaker":"B","text":"そうだと思います。"},{"id":{"d":765,"n":5,"t":4},"speaker":"A","text":"そのマスカラをやめたらいいのではないでしょうか？"},{"id":{"d":765,"n":6,"t":4},"speaker":"B","text":"そうなのですが、そこそこ高価なものでしたので、捨てるのが悔しくて使っているのです。"},{"id":{"d":765,"n":7,"t":4},"speaker":"A","text":"眼鏡が傷ついてしまったら、もっとお金がかかりますよ。"},{"id":{"d":765,"n":8,"t":4},"speaker":"B","text":"そうですよね。悩みどころです。"},{"id":{"d":765,"n":9,"t":4},"speaker":"A","text":"眼鏡屋さんに相談に行ってみてはどうでしょうか？"},{"id":{"d":765,"n":10,"t":4},"speaker":"B","text":"そうですね。今度の日曜日にでも行ってみます。"}];
INSERT INTO sentence [{"id":{"d":766,"n":1,"t":4},"speaker":"A","text":"なんだか頭がくらくらします。"},{"id":{"d":766,"n":2,"t":4},"speaker":"B","text":"水分補給はしていますか？"},{"id":{"d":766,"n":3,"t":4},"speaker":"A","text":"朝にコーヒーを飲みました。"},{"id":{"d":766,"n":4,"t":4},"speaker":"B","text":"それだけですか？"},{"id":{"d":766,"n":5,"t":4},"speaker":"A","text":"そうです。いつもそのようなものですよ。"},{"id":{"d":766,"n":6,"t":4},"speaker":"B","text":"今日は暑いですから、それでは水分不足ではありませんか？熱中症の初期症状かもしれませんよ。"},{"id":{"d":766,"n":7,"t":4},"speaker":"A","text":"言われてみればそうかもしれません。急いでお水を飲んできます。"},{"id":{"d":766,"n":8,"t":4},"speaker":"B","text":"スポーツドリンクの方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":767,"n":1,"t":4},"speaker":"A","text":"先ほどまで頭がくらくらしていたのですが、スポーツドリンクを飲んだら良くなりました。"},{"id":{"d":767,"n":2,"t":4},"speaker":"B","text":"それは良かったです。"},{"id":{"d":767,"n":3,"t":4},"speaker":"A","text":"水分は大切なのですね。"},{"id":{"d":767,"n":4,"t":4},"speaker":"B","text":"夏は特に気を付けた方がいいですよ。"},{"id":{"d":767,"n":5,"t":4},"speaker":"A","text":"田中さんはいつもマグボトルをお持ちですが、それは熱中症対策なのですか？"},{"id":{"d":767,"n":6,"t":4},"speaker":"B","text":"夏はその意味が大きいですが、好きなお茶があるので、普段はそれを飲むためですよ。"},{"id":{"d":767,"n":7,"t":4},"speaker":"A","text":"中身は何を入れているのですか？"},{"id":{"d":767,"n":8,"t":4},"speaker":"B","text":"ルイボスティーです。"}];
INSERT INTO sentence [{"id":{"d":768,"n":1,"t":4},"speaker":"A","text":"おなかが痛くてたまりません。"},{"id":{"d":768,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？トイレには行ってきましたか？"},{"id":{"d":768,"n":3,"t":4},"speaker":"A","text":"先ほど行ってきました。"},{"id":{"d":768,"n":4,"t":4},"speaker":"B","text":"今朝は何を食べましたか？"},{"id":{"d":768,"n":5,"t":4},"speaker":"A","text":"昨日のカレーの残りを食べました。"},{"id":{"d":768,"n":6,"t":4},"speaker":"B","text":"カレーは冷蔵庫で保存したものですか？"},{"id":{"d":768,"n":7,"t":4},"speaker":"A","text":"いいえ、コンロの上です。朝になってもそれほど冷えていなかったので、温めないでそのまま食べました。"},{"id":{"d":768,"n":8,"t":4},"speaker":"B","text":"きちんと冷蔵保存をしていないカレーを食べたことが腹痛の原因かもしれません。食中毒だといけないので、病院に行った方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":769,"n":1,"t":4},"speaker":"A","text":"夕食を食べ過ぎてしまいました。"},{"id":{"d":769,"n":2,"t":4},"speaker":"B","text":"たくさん食べ過ぎるとおなかが痛くなりますよ。今は大丈夫ですか？"},{"id":{"d":769,"n":3,"t":4},"speaker":"A","text":"はい。おなかが苦しいだけで、痛くはありません。"},{"id":{"d":769,"n":4,"t":4},"speaker":"B","text":"あまりに苦しかったら消化剤も飲みましょうね。"},{"id":{"d":769,"n":5,"t":4},"speaker":"A","text":"寝ていれば大丈夫です。明日になればいつもどおり学校へ行けます。"},{"id":{"d":769,"n":6,"t":4},"speaker":"B","text":"学校といえば、宿題は終わっているのですか？"},{"id":{"d":769,"n":7,"t":4},"speaker":"A","text":"はい。夕食に出かける前に済ませました。"},{"id":{"d":769,"n":8,"t":4},"speaker":"B","text":"それならよかったです。明日の用意をして、歯を磨いてから寝ましょうね。"}];
INSERT INTO sentence [{"id":{"d":770,"n":1,"t":4},"speaker":"A","text":"手袋をなくしてしまい、そのまま過ごしていたら、手にしもやけができてしまいました。"},{"id":{"d":770,"n":2,"t":4},"speaker":"B","text":"薬は塗りましたか？"},{"id":{"d":770,"n":3,"t":4},"speaker":"A","text":"はい。姉が病院でもらった薬を塗りました。"},{"id":{"d":770,"n":4,"t":4},"speaker":"B","text":"同じ症状なら効果があるのかもしれませんが、自分の薬はきちんと病院で診察してもらったものの方がいいですよ。"},{"id":{"d":770,"n":5,"t":4},"speaker":"A","text":"そうですね、明日にでも行ってみます。"},{"id":{"d":770,"n":6,"t":4},"speaker":"B","text":"今日は行かないのですか？"},{"id":{"d":770,"n":7,"t":4},"speaker":"A","text":"手袋を買う方が先です。"},{"id":{"d":770,"n":8,"t":4},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":771,"n":1,"t":4},"speaker":"A","text":"熱があるようです。"},{"id":{"d":771,"n":2,"t":4},"speaker":"B","text":"今日はもう帰った方がいいですね。"},{"id":{"d":771,"n":3,"t":4},"speaker":"A","text":"忙しい時にすみません。"},{"id":{"d":771,"n":4,"t":4},"speaker":"B","text":"アルバイトの方がいますから大丈夫ですよ。"},{"id":{"d":771,"n":5,"t":4},"speaker":"A","text":"それではお先に失礼します。"},{"id":{"d":771,"n":6,"t":4},"speaker":"B","text":"可能でしたら病院に寄っていくといいですよ。"},{"id":{"d":771,"n":7,"t":4},"speaker":"A","text":"かなりつらいので、薬局で解熱剤を買って帰ります。一刻も早く横になりたいです。"},{"id":{"d":771,"n":8,"t":4},"speaker":"B","text":"そうですか。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":772,"n":1,"t":4},"speaker":"A","text":"昨日は早退させてくださってありがとうございました。"},{"id":{"d":772,"n":2,"t":4},"speaker":"B","text":"体調はもう大丈夫なのですか？"},{"id":{"d":772,"n":3,"t":4},"speaker":"A","text":"おかげさまでだいぶ楽になりました。薬を飲んで一晩寝たら、熱はすっかり下がりました。"},{"id":{"d":772,"n":4,"t":4},"speaker":"B","text":"それはよかったです。でも、まだ本調子ではない顔をしていますね。"},{"id":{"d":772,"n":5,"t":4},"speaker":"A","text":"食欲がありませんでしたので、昨日から何も食べていません。"},{"id":{"d":772,"n":6,"t":4},"speaker":"B","text":"そのような状態で働いていただくわけにはいきません。"},{"id":{"d":772,"n":7,"t":4},"speaker":"A","text":"大丈夫ですよ。"},{"id":{"d":772,"n":8,"t":4},"speaker":"B","text":"大丈夫とは思えません。どうしても働くのであれば、少しでもよいので何か食べてください。"}];
INSERT INTO sentence [{"id":{"d":773,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。何かお探しですか？"},{"id":{"d":773,"n":2,"t":4},"speaker":"B","text":"クレンジングクリームを探しています。"},{"id":{"d":773,"n":3,"t":4},"speaker":"A","text":"どのようなタイプのものがお好みですか？"},{"id":{"d":773,"n":4,"t":4},"speaker":"B","text":"敏感肌でも使えるものがいいです。先月に買ったものが合わなかったらしく、肌が荒れ気味なのです。"},{"id":{"d":773,"n":5,"t":4},"speaker":"A","text":"こちらとこちらとこちらの商品は敏感肌の方にもご好評をいただいております。"},{"id":{"d":773,"n":6,"t":4},"speaker":"B","text":"この3つの商品の違いは何ですか？"},{"id":{"d":773,"n":7,"t":4},"speaker":"A","text":"入っている成分と使用時の伸びやすさが少しずつ違います。どれもお肌には優しいものですので、お好みの使用感のものをお選びいただくのが良いかと思います。"},{"id":{"d":773,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。テスターを使って使用感を確認してみます。"}];
INSERT INTO sentence [{"id":{"d":774,"n":1,"t":4},"speaker":"A","text":"随分鼻が詰まっていますね。"},{"id":{"d":774,"n":2,"t":4},"speaker":"B","text":"そうなのですよ。今すぐ誰かの物まねができそうです。"},{"id":{"d":774,"n":3,"t":4},"speaker":"A","text":"ふざけていないで、鼻炎薬を飲んでください。"},{"id":{"d":774,"n":4,"t":4},"speaker":"B","text":"鼻炎薬を飲むと頭がぼんやりとしてしまうので、飲みたくありません。"},{"id":{"d":774,"n":5,"t":4},"speaker":"A","text":"眠気が起こらないものもあるそうですよ。"},{"id":{"d":774,"n":6,"t":4},"speaker":"B","text":"そうなのですか？"},{"id":{"d":774,"n":7,"t":4},"speaker":"A","text":"はい。確か、佐藤先輩は眠くならない鼻炎薬を飲んでいると聞いたことがあります。"},{"id":{"d":774,"n":8,"t":4},"speaker":"B","text":"佐藤先輩にその薬について聞いてきます。"}];
INSERT INTO sentence [{"id":{"d":775,"n":1,"t":4},"speaker":"A","text":"先生、明日の大会にはどうしても出場したいので、痛み止めをください。"},{"id":{"d":775,"n":2,"t":4},"speaker":"B","text":"これだけのけがなのですから、やめた方がいいですよ。"},{"id":{"d":775,"n":3,"t":4},"speaker":"A","text":"大丈夫です。痛みさえなければいつもどおりに動くことができます。"},{"id":{"d":775,"n":4,"t":4},"speaker":"B","text":"痛み止めで痛みを止めても、無理をすれば後に響きますよ。"},{"id":{"d":775,"n":5,"t":4},"speaker":"A","text":"それでも明日の大会には出たいのです。"},{"id":{"d":775,"n":6,"t":4},"speaker":"B","text":"選手生命が終わってしまいますから、承諾できません。"},{"id":{"d":775,"n":7,"t":4},"speaker":"A","text":"どうしても駄目ですか？"},{"id":{"d":775,"n":8,"t":4},"speaker":"B","text":"はい。悔しい気持ちは分かりますが、明日の大会は諦めてください。"}];
INSERT INTO sentence [{"id":{"d":776,"n":1,"t":4},"speaker":"A","text":"加奈ちゃん、少しこちらに来て手伝ってくれますか？"},{"id":{"d":776,"n":2,"t":4},"speaker":"B","text":"夜中に何をしているのですか？"},{"id":{"d":776,"n":3,"t":4},"speaker":"A","text":"寝る前に柔軟体操をしているのです。"},{"id":{"d":776,"n":4,"t":4},"speaker":"B","text":"それはいいことですね。頑張ってください。"},{"id":{"d":776,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":776,"n":6,"t":4},"speaker":"B","text":"それで、私は何をすればよいのですか？"},{"id":{"d":776,"n":7,"t":4},"speaker":"A","text":"柔軟体操を手伝ってほしいのです。背中を押してくれませんか？"},{"id":{"d":776,"n":8,"t":4},"speaker":"B","text":"自分でできるところまで曲げればよいのではありませんか？"},{"id":{"d":776,"n":9,"t":4},"speaker":"A","text":"自分で曲げると苦しいのですよ。"},{"id":{"d":776,"n":10,"t":4},"speaker":"B","text":"そういうものなのですか。では、押しますよ。"},{"id":{"d":776,"n":11,"t":4},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":777,"n":1,"t":4},"speaker":"A","text":"お父さん、車を出していただけませんか？"},{"id":{"d":777,"n":2,"t":4},"speaker":"B","text":"何かあったのですか？"},{"id":{"d":777,"n":3,"t":4},"speaker":"A","text":"奈緒が熱を出しているのです。"},{"id":{"d":777,"n":4,"t":4},"speaker":"B","text":"病院に連れて行くのですね。"},{"id":{"d":777,"n":5,"t":4},"speaker":"A","text":"はい。田島医院の小児科でしたら、この時間でもまだ診てくれると思いますのでお願いします。"},{"id":{"d":777,"n":6,"t":4},"speaker":"B","text":"分かりました。支度をして連れてきてください。車のエンジンをかけておきます。"},{"id":{"d":777,"n":7,"t":4},"speaker":"A","text":"急なお願いで申し訳ありません。"},{"id":{"d":777,"n":8,"t":4},"speaker":"B","text":"いいえ、大丈夫です。お気になさらず。"}];
INSERT INTO sentence [{"id":{"d":778,"n":1,"t":4},"speaker":"A","text":"今日はありがとうございました。"},{"id":{"d":778,"n":2,"t":4},"speaker":"B","text":"お大事になさってください。"},{"id":{"d":778,"n":3,"t":4},"speaker":"A","text":"薬局はお隣ですか？"},{"id":{"d":778,"n":4,"t":4},"speaker":"B","text":"そうです。こちらの処方箋をお持ちください。"},{"id":{"d":778,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":778,"n":6,"t":4},"speaker":"B","text":"駐車券はお持ちですか？"},{"id":{"d":778,"n":7,"t":4},"speaker":"A","text":"はい、持っています。"},{"id":{"d":778,"n":8,"t":4},"speaker":"B","text":"ご提示ありがとうございます。こちらのサービス券で2時間までの駐車が無料になりますので、ぜひご利用ください。"},{"id":{"d":778,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":779,"n":1,"t":4},"speaker":"A","text":"手荒れがひどいですね。"},{"id":{"d":779,"n":2,"t":4},"speaker":"B","text":"毎年冬になると、このような感じになってしまいます。"},{"id":{"d":779,"n":3,"t":4},"speaker":"A","text":"お薬は付けていますか？"},{"id":{"d":779,"n":4,"t":4},"speaker":"B","text":"もちろんです。クリームを塗って手袋をして寝ています。"},{"id":{"d":779,"n":5,"t":4},"speaker":"A","text":"それでも良くならないのですか？"},{"id":{"d":779,"n":6,"t":4},"speaker":"B","text":"はい。お湯で皿洗いをするとひどくなるのは分かっているのですが、冬はどうしても水では洗いたくありません。"},{"id":{"d":779,"n":7,"t":4},"speaker":"A","text":"どうしてもお湯を使うのであれば、水仕事をするときは手袋をした方がいいですよ。"},{"id":{"d":779,"n":8,"t":4},"speaker":"B","text":"ゴム手袋はあまり好きではありませんが、考えてみます。"}];
INSERT INTO sentence [{"id":{"d":780,"n":1,"t":4},"speaker":"A","text":"公園でラジオ体操をしているのですね。誰でも参加できるのですか？"},{"id":{"d":780,"n":2,"t":4},"speaker":"B","text":"はい。誰でも参加できます。"},{"id":{"d":780,"n":3,"t":4},"speaker":"A","text":"毎日6時半から始まるのですか？"},{"id":{"d":780,"n":4,"t":4},"speaker":"B","text":"そうです。ラジオ体操の放送がその時間から始まります。"},{"id":{"d":780,"n":5,"t":4},"speaker":"A","text":"土日はお休みですか？"},{"id":{"d":780,"n":6,"t":4},"speaker":"B","text":"いいえ。土日もしていますよ。"},{"id":{"d":780,"n":7,"t":4},"speaker":"A","text":"そうですか。明日以降、早起きできたときには参加してみようと思います。"},{"id":{"d":780,"n":8,"t":4},"speaker":"B","text":"お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":781,"n":1,"t":4},"speaker":"A","text":"健康に良い飲み物を知りませんか？"},{"id":{"d":781,"n":2,"t":4},"speaker":"B","text":"突然のどうしたのですか？どこか悪いのですか？"},{"id":{"d":781,"n":3,"t":4},"speaker":"A","text":"このところはコーヒーの飲み過ぎなのか、胃が痛いのです。"},{"id":{"d":781,"n":4,"t":4},"speaker":"B","text":"コーヒーのせいでなくても、胃が痛いときはコーヒーをやめた方がいいですね。"},{"id":{"d":781,"n":5,"t":4},"speaker":"A","text":"はい。その代わりに胃に優しい飲み物を飲みたいのですが、何が良いのでしょうか？"},{"id":{"d":781,"n":6,"t":4},"speaker":"B","text":"私は麦茶を飲んでいますよ。"},{"id":{"d":781,"n":7,"t":4},"speaker":"A","text":"麦茶は胃を痛めないのですか？"},{"id":{"d":781,"n":8,"t":4},"speaker":"B","text":"カフェインレスですから胃に優しい飲み物だと思います。"}];
INSERT INTO sentence [{"id":{"d":782,"n":1,"t":4},"speaker":"A","text":"おじいさん、今日こそリハビリに行きましょうね。"},{"id":{"d":782,"n":2,"t":4},"speaker":"B","text":"リハビリの先生に体を動かされると、節々が痛くなるので行きたくありません。"},{"id":{"d":782,"n":3,"t":4},"speaker":"A","text":"少し痛いくらいに動かさないと、ずっと曲がらないままになってしまいますよ。"},{"id":{"d":782,"n":4,"t":4},"speaker":"B","text":"曲がらなくとも困っていません。"},{"id":{"d":782,"n":5,"t":4},"speaker":"A","text":"困っていないのは皆が助けてくれているからです。"},{"id":{"d":782,"n":6,"t":4},"speaker":"B","text":"ありがたいことです。"},{"id":{"d":782,"n":7,"t":4},"speaker":"A","text":"それだけで済まさないで、おじいさんも努力してください。そのままの格好でよいですから、出かけましょう。"}];
INSERT INTO sentence [{"id":{"d":783,"n":1,"t":4},"speaker":"A","text":"今日はお子さんの予防接種ですね。ご自宅で熱を測ってきましたか？"},{"id":{"d":783,"n":2,"t":4},"speaker":"B","text":"はい。平熱でした。"},{"id":{"d":783,"n":3,"t":4},"speaker":"A","text":"それでは予防接種の前にこちらの問診票に記入してください。"},{"id":{"d":783,"n":4,"t":4},"speaker":"B","text":"はい。記入しました。"},{"id":{"d":783,"n":5,"t":4},"speaker":"A","text":"体調に問題はないようですね。では、接種しましょう。"},{"id":{"d":783,"n":6,"t":4},"speaker":"B","text":"息子は注射が大嫌いなのですが大丈夫でしょうか？"},{"id":{"d":783,"n":7,"t":4},"speaker":"A","text":"それほど痛くありませんから大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":784,"n":1,"t":4},"speaker":"A","text":"こんにちは。"},{"id":{"d":784,"n":2,"t":4},"speaker":"B","text":"まあ、誰かと思いましたよ。どうしたのですか？"},{"id":{"d":784,"n":3,"t":4},"speaker":"A","text":"明日から専門学校に行くので、髪をピンクに染めてみたのです。"},{"id":{"d":784,"n":4,"t":4},"speaker":"B","text":"これはまたすごい色ですね。"},{"id":{"d":784,"n":5,"t":4},"speaker":"A","text":"デザイン系の専門学校ではこれくらいしないと目立たないのですよ。"},{"id":{"d":784,"n":6,"t":4},"speaker":"B","text":"そうなのですか？それにしてもとても鮮やかな色ですね。"},{"id":{"d":784,"n":7,"t":4},"speaker":"A","text":"よかったら榎本さんもいかがですか？"},{"id":{"d":784,"n":8,"t":4},"speaker":"B","text":"私は遠慮しておきます。"}];
INSERT INTO sentence [{"id":{"d":785,"n":1,"t":4},"speaker":"A","text":"おばあちゃん、お母さんはどこにお出かけしたのですか？"},{"id":{"d":785,"n":2,"t":4},"speaker":"B","text":"パーマ屋さんですよ。"},{"id":{"d":785,"n":3,"t":4},"speaker":"A","text":"パーマ屋さんというのは何ですか？"},{"id":{"d":785,"n":4,"t":4},"speaker":"B","text":"最近の若い子はパーマ屋さんを知らないのですか？"},{"id":{"d":785,"n":5,"t":4},"speaker":"A","text":"私は初めて聞きました。"},{"id":{"d":785,"n":6,"t":4},"speaker":"B","text":"今は美容院と言うのでしょうか？"},{"id":{"d":785,"n":7,"t":4},"speaker":"A","text":"美容室なら知っています。昔は美容院のことをパーマ屋と言っていたのですか？"},{"id":{"d":785,"n":8,"t":4},"speaker":"B","text":"そうですよ。"}];
INSERT INTO sentence [{"id":{"d":786,"n":1,"t":4},"speaker":"A","text":"ゴールに到着です。お疲れさまでした。"},{"id":{"d":786,"n":2,"t":4},"speaker":"B","text":"ようやく着きましたか。本当に疲れました。"},{"id":{"d":786,"n":3,"t":4},"speaker":"A","text":"他のメンバーはどうしましたか？"},{"id":{"d":786,"n":4,"t":4},"speaker":"B","text":"ゴール前の坂道で、2人ほどリタイアしていたようです。"},{"id":{"d":786,"n":5,"t":4},"speaker":"A","text":"そうでしたか。心配ですね。迎えに行った方がいいでしょうか？"},{"id":{"d":786,"n":6,"t":4},"speaker":"B","text":"そうですね。私はもう少し休んでからでないと動けそうにありませんので、先に彼らの所へ行ってください。"},{"id":{"d":786,"n":7,"t":4},"speaker":"A","text":"分かりました。それでは行ってきます。"}];
INSERT INTO sentence [{"id":{"d":787,"n":1,"t":4},"speaker":"A","text":"ゴールまであと少しです。頑張ってください。"},{"id":{"d":787,"n":2,"t":4},"speaker":"B","text":"もう無理です。これ以上は走れません。"},{"id":{"d":787,"n":3,"t":4},"speaker":"A","text":"どこか具合が悪いのですか？足のけいれんなどはありますか？"},{"id":{"d":787,"n":4,"t":4},"speaker":"B","text":"そういうことではありませんが、もうぐったりです。"},{"id":{"d":787,"n":5,"t":4},"speaker":"A","text":"走り続けて疲れもピークですよね。でも、あと少しですよ。"},{"id":{"d":787,"n":6,"t":4},"speaker":"B","text":"ここから先は歩いてもよいですか？"},{"id":{"d":787,"n":7,"t":4},"speaker":"A","text":"どうしても無理なら歩いてもよいですよ。ゴールを目指して最後まで頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":788,"n":1,"t":4},"speaker":"A","text":"陸上部の応援に行ってきたのですが、そこで見たものにとても驚きました。"},{"id":{"d":788,"n":2,"t":4},"speaker":"B","text":"何を見て驚いたのですか？"},{"id":{"d":788,"n":3,"t":4},"speaker":"A","text":"ハードル走で、とても高いハードルの器具を使っていましたのです。"},{"id":{"d":788,"n":4,"t":4},"speaker":"B","text":"体育の授業で使っているものとは違うのですか？"},{"id":{"d":788,"n":5,"t":4},"speaker":"A","text":"はい、全く違いました。"},{"id":{"d":788,"n":6,"t":4},"speaker":"B","text":"授業と競技では使う器具が違うのですね。"},{"id":{"d":788,"n":7,"t":4},"speaker":"A","text":"そのようでした。あれは私たちなら走り高跳びの高さです。"},{"id":{"d":788,"n":8,"t":4},"speaker":"B","text":"それほど高いのですか？私も一度見てみたいです。"}];
INSERT INTO sentence [{"id":{"d":789,"n":1,"t":4},"speaker":"A","text":"花粉症らしき症状が出たので、病院で調べてもらいました。"},{"id":{"d":789,"n":2,"t":4},"speaker":"B","text":"結果はどうでしたか？"},{"id":{"d":789,"n":3,"t":4},"speaker":"A","text":"スギとヒノキのアレルギーでした。"},{"id":{"d":789,"n":4,"t":4},"speaker":"B","text":"そうでしたか。症状はまだ軽いのですか？"},{"id":{"d":789,"n":5,"t":4},"speaker":"A","text":"はい。今のところ、皆さんが言われるほどつらくはありません。"},{"id":{"d":789,"n":6,"t":4},"speaker":"B","text":"油断していると、花粉症はひどくなるので気を付けてくださいね。"},{"id":{"d":789,"n":7,"t":4},"speaker":"A","text":"そうですね。気を付けます。"},{"id":{"d":789,"n":8,"t":4},"speaker":"B","text":"よかったらこのマスクを使ってください。"},{"id":{"d":789,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":790,"n":1,"t":4},"speaker":"A","text":"ボルダリングの大会を見学してきました。"},{"id":{"d":790,"n":2,"t":4},"speaker":"B","text":"いいですね。生で見るとすごい迫力でしょう。"},{"id":{"d":790,"n":3,"t":4},"speaker":"A","text":"はい。あれほど反り返った壁を登るものだとは思いませんでした。"},{"id":{"d":790,"n":4,"t":4},"speaker":"B","text":"実際に見ると驚きますよね。"},{"id":{"d":790,"n":5,"t":4},"speaker":"A","text":"ええ。ショッピングセンターなどにある体験用の壁とは全く違いました。"},{"id":{"d":790,"n":6,"t":4},"speaker":"B","text":"私も以前に見てとても驚きました。"},{"id":{"d":790,"n":7,"t":4},"speaker":"A","text":"少し興味があったのですが、自分には無理だと思いましたよ。"},{"id":{"d":790,"n":8,"t":4},"speaker":"B","text":"競技用の壁は難しいかもしれませんが、ジムでする程度なら大丈夫だと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":791,"n":1,"t":4},"speaker":"A","text":"今年の夏は猛暑の予報が出ています。"},{"id":{"d":791,"n":2,"t":4},"speaker":"B","text":"私は去年も夏バテで苦しんだので、夏が来るのが怖いです。"},{"id":{"d":791,"n":3,"t":4},"speaker":"A","text":"そうでした、夏が終わるころにはすっかり痩せていらっしゃいましたね。"},{"id":{"d":791,"n":4,"t":4},"speaker":"B","text":"私は暑いと食欲がなくなってしまうのです。"},{"id":{"d":791,"n":5,"t":4},"speaker":"A","text":"冷たいものでも食べられませんか？"},{"id":{"d":791,"n":6,"t":4},"speaker":"B","text":"最初のうちは冷たいものを食べているのですが、そのうち何も受け付けなくなるのがいつものパターンです。"},{"id":{"d":791,"n":7,"t":4},"speaker":"A","text":"それは大変ですね。冷たいものばかりでは胃に負担がくると聞いたことがあります。"},{"id":{"d":791,"n":8,"t":4},"speaker":"B","text":"暑いからといって冷たいものばかりでもいけないのですね。今年の夏は、温かいものも少しは食べてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":792,"n":1,"t":4},"speaker":"A","text":"今日は肌がきれいですね。"},{"id":{"d":792,"n":2,"t":4},"speaker":"B","text":"本当ですか？そう言ってもらえてうれしいです。"},{"id":{"d":792,"n":3,"t":4},"speaker":"A","text":"お手入れの方法を変えたのですか？"},{"id":{"d":792,"n":4,"t":4},"speaker":"B","text":"実は、お化粧の方法を変えたのです。"},{"id":{"d":792,"n":5,"t":4},"speaker":"A","text":"どのように変えたのですか？"},{"id":{"d":792,"n":6,"t":4},"speaker":"B","text":"昨日、デパートに行ったら、美容部員の方がコンシーラーの正しい使い方を教えてくれたのですよ。今日は、彼女に教えてもらった通りに使ってみました。"},{"id":{"d":792,"n":7,"t":4},"speaker":"A","text":"コンシーラーを上手に使うとここまで変わるのですね。"},{"id":{"d":792,"n":8,"t":4},"speaker":"B","text":"そのようです。シミもしわも目立たないので、気に入っています。"}];
INSERT INTO sentence [{"id":{"d":793,"n":1,"t":4},"speaker":"A","text":"もう体力の限界です。交代してくれませんか？"},{"id":{"d":793,"n":2,"t":4},"speaker":"B","text":"テニスに交代の制度はないので、それは難しいです。"},{"id":{"d":793,"n":3,"t":4},"speaker":"A","text":"いきなり上級者と試合をするのは無理です。もう動けません。"},{"id":{"d":793,"n":4,"t":4},"speaker":"B","text":"でも、そのまま座っていると負けてしまいますよ。"},{"id":{"d":793,"n":5,"t":4},"speaker":"A","text":"もうギブアップで構いません。"},{"id":{"d":793,"n":6,"t":4},"speaker":"B","text":"そう言うのなら仕方ありませんね。ではこの試合は棄権ということにしましょう。"},{"id":{"d":793,"n":7,"t":4},"speaker":"A","text":"はい、それでお願いします。私は初心者なので、次は同じ初心者を相手に試合ができたらうれしいです。"},{"id":{"d":793,"n":8,"t":4},"speaker":"B","text":"分かりました。同じくらいのレベルの人を探しておきます。"}];
INSERT INTO sentence [{"id":{"d":794,"n":1,"t":4},"speaker":"A","text":"昨日は足がつって大変でした。"},{"id":{"d":794,"n":2,"t":4},"speaker":"B","text":"水分不足だったのでしょうか？"},{"id":{"d":794,"n":3,"t":4},"speaker":"A","text":"そうではありません。運動不足だと感じていたので、「ゆるスポーツ」のイベントに行ってきたのです。"},{"id":{"d":794,"n":4,"t":4},"speaker":"B","text":"ゆるスポーツとは何ですか？"},{"id":{"d":794,"n":5,"t":4},"speaker":"A","text":"誰でも簡単にできる緩いスポーツのことです。私は100センチメートル走をやってきました。"},{"id":{"d":794,"n":6,"t":4},"speaker":"B","text":"100センチメートルとは1メートルのことですよね？"},{"id":{"d":794,"n":7,"t":4},"speaker":"A","text":"そうです。いかに遅く走るかを競うのです。片足を上げたままゆらゆらさせていたら、足がつってしまいました。"},{"id":{"d":794,"n":8,"t":4},"speaker":"B","text":"それはそれで、なかなかきつそうですね。"}];
INSERT INTO sentence [{"id":{"d":795,"n":1,"t":4},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":795,"n":2,"t":4},"speaker":"B","text":"おかえりなさい。今日のデートはどうでしたか？"},{"id":{"d":795,"n":3,"t":4},"speaker":"A","text":"楽しかったです。彼がたまたま公園にいた人たちとバスケットボールで遊び始めて、私はそばでずっと見ていました。"},{"id":{"d":795,"n":4,"t":4},"speaker":"B","text":"確か、彼はバスケットボール部でしたね。"},{"id":{"d":795,"n":5,"t":4},"speaker":"A","text":"はい、そうです。実は彼がバスケットボールをしているところを初めて見ました。とても上手でしたよ。"},{"id":{"d":795,"n":6,"t":4},"speaker":"B","text":"普段は見られない姿を見られて良かったですね。"},{"id":{"d":795,"n":7,"t":4},"speaker":"A","text":"はい、とてもいいデートでした。"}];
INSERT INTO sentence [{"id":{"d":796,"n":1,"t":4},"speaker":"A","text":"どこかでキーンという音がしませんでしたか？"},{"id":{"d":796,"n":2,"t":4},"speaker":"B","text":"私は気が付きませんでした。"},{"id":{"d":796,"n":3,"t":4},"speaker":"A","text":"ではこれは耳鳴りかもしれません。最近は耳に違和感があるような気がします。"},{"id":{"d":796,"n":4,"t":4},"speaker":"B","text":"最近よく耳鳴りがするのですか？"},{"id":{"d":796,"n":5,"t":4},"speaker":"A","text":"はい。何か音がすると思って周りの人に確認しても、誰も賛同してくれないことが多いのです。"},{"id":{"d":796,"n":6,"t":4},"speaker":"B","text":"あまりに頻繁に起きるようなら病院に行った方がいいですよ。"},{"id":{"d":796,"n":7,"t":4},"speaker":"A","text":"そうですよね。病院を探してみます。"},{"id":{"d":796,"n":8,"t":4},"speaker":"B","text":"私の姉が行っている耳鼻科を紹介しましょうか？"},{"id":{"d":796,"n":9,"t":4},"speaker":"A","text":"ぜひお願いします。"}];
INSERT INTO sentence [{"id":{"d":797,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":797,"n":2,"t":4},"speaker":"B","text":"先日たまたま血圧を測ってみたら、180という数字が出て驚いてしまったのです。それで、一度先生に診ていただこうと思って病院に来ました。"},{"id":{"d":797,"n":3,"t":4},"speaker":"A","text":"180とは、少し気がかりですね。ここでも測ってみましょう。"},{"id":{"d":797,"n":4,"t":4},"speaker":"B","text":"はい、お願いします。いかがですか？"},{"id":{"d":797,"n":5,"t":4},"speaker":"A","text":"180には達していませんが、比較的高いですね。"},{"id":{"d":797,"n":6,"t":4},"speaker":"B","text":"私は高血圧なのでしょうか？"},{"id":{"d":797,"n":7,"t":4},"speaker":"A","text":"その傾向があるかもしれません。塩分を控えた食事にしてください。お薬も出しますから、これでひと月くらい様子を見ましょう。"}];
INSERT INTO sentence [{"id":{"d":798,"n":1,"t":4},"speaker":"A","text":"最近は少し猫背気味ですね。"},{"id":{"d":798,"n":2,"t":4},"speaker":"B","text":"あなたもそう思いますか？"},{"id":{"d":798,"n":3,"t":4},"speaker":"A","text":"はい。他の人にも言われたのですか？"},{"id":{"d":798,"n":4,"t":4},"speaker":"B","text":"家族や友人によく言われるようになりました。"},{"id":{"d":798,"n":5,"t":4},"speaker":"A","text":"原因は思い当たりますか？"},{"id":{"d":798,"n":6,"t":4},"speaker":"B","text":"パソコンとスマートフォンの見過ぎだと思います。"},{"id":{"d":798,"n":7,"t":4},"speaker":"A","text":"少し利用を控えるといいですよ。"},{"id":{"d":798,"n":8,"t":4},"speaker":"B","text":"分かってはいるのですが、どちらも使わないと仕事ができないので、それは難しいです。"}];
INSERT INTO sentence [{"id":{"d":799,"n":1,"t":4},"speaker":"A","text":"土井さん、外に何か素敵なものでも見えるのですか？"},{"id":{"d":799,"n":2,"t":4},"speaker":"B","text":"いえ、何となく窓の外を見ているだけです。"},{"id":{"d":799,"n":3,"t":4},"speaker":"A","text":"そうでしたか。あまりにもじっと見ておられたので、何かあるのかと気になりました。"},{"id":{"d":799,"n":4,"t":4},"speaker":"B","text":"何もありませんが、目が疲れたので緑を見ていたのです。"},{"id":{"d":799,"n":5,"t":4},"speaker":"A","text":"なるほど、外の山を見ていらしたのですね。"},{"id":{"d":799,"n":6,"t":4},"speaker":"B","text":"はい。5分くらい見ていると、疲れ目には効くような気がします。"},{"id":{"d":799,"n":7,"t":4},"speaker":"A","text":"そうなのですね。私もやってみます。"}];
INSERT INTO sentence [{"id":{"d":800,"n":1,"t":4},"speaker":"A","text":"今日は随分元気ですね。"},{"id":{"d":800,"n":2,"t":4},"speaker":"B","text":"そうでしょうか？昨日、リラクゼーションサロンに行ってきたおかげかもしれません。"},{"id":{"d":800,"n":3,"t":4},"speaker":"A","text":"マッサージを受けてきたのですか？"},{"id":{"d":800,"n":4,"t":4},"speaker":"B","text":"マッサージとは少し違いますが、エステティックとヘッドスパをやってもらいました。"},{"id":{"d":800,"n":5,"t":4},"speaker":"A","text":"良い休日を過ごされたのですね。今日は体の調子が良さそうに見えます。"},{"id":{"d":800,"n":6,"t":4},"speaker":"B","text":"はい。とても気持ちよくて、施術の途中で眠ってしまいました。"},{"id":{"d":800,"n":7,"t":4},"speaker":"A","text":"寝てしまうほど気持ち良いとは、施術スタッフの腕がかなり良いのでしょうね。"},{"id":{"d":800,"n":8,"t":4},"speaker":"B","text":"そうだと思います。とても気に入ったので、他のコースも予約してきました。"}];
INSERT INTO sentence [{"id":{"d":801,"n":1,"t":4},"speaker":"A","text":"結婚式に招待していただいたので、ネイルサロンに行ってきました。"},{"id":{"d":801,"n":2,"t":4},"speaker":"B","text":"きれいなネイルですね。"},{"id":{"d":801,"n":3,"t":4},"speaker":"A","text":"うれしいです。凝ったデザインをお願いしたので時間がかかりましたが、きれいに仕上がって満足です。"},{"id":{"d":801,"n":4,"t":4},"speaker":"B","text":"結婚式は明日ですか？"},{"id":{"d":801,"n":5,"t":4},"speaker":"A","text":"そうです。明日まではこの爪を汚したり傷つけたりするわけにはいきません。"},{"id":{"d":801,"n":6,"t":4},"speaker":"B","text":"では、食器を洗わなくていいように、今日は外でご飯を食べて帰りませんか？"}];
INSERT INTO sentence [{"id":{"d":802,"n":1,"t":4},"speaker":"A","text":"なぜ机の下にテニスボールがあるのですか？"},{"id":{"d":802,"n":2,"t":4},"speaker":"B","text":"これで足をマッサージをするのです。"},{"id":{"d":802,"n":3,"t":4},"speaker":"A","text":"テニスボールでマッサージをするのですか？"},{"id":{"d":802,"n":4,"t":4},"speaker":"B","text":"はい。靴を脱いで、土踏まずをこのテニスボールで押すと気持ちが良いですよ。"},{"id":{"d":802,"n":5,"t":4},"speaker":"A","text":"なるほど。ハイヒールで疲れた足にはよく効くかもしれませんね。"},{"id":{"d":802,"n":6,"t":4},"speaker":"B","text":"はい。足が疲れた時には、机の下でこっそりとマッサージをしています。マッサージ中に急なお客様がいらしたときは焦りますけどね。"},{"id":{"d":802,"n":7,"t":4},"speaker":"A","text":"急いで靴を履かなければなりませんね。"}];
INSERT INTO sentence [{"id":{"d":803,"n":1,"t":4},"speaker":"A","text":"息子が夏バテで食欲がなくなってしまい困っているのです。どうしたらよいでしょうか？"},{"id":{"d":803,"n":2,"t":4},"speaker":"B","text":"夏バテにはビタミンCを取るといいと聞きましたよ。"},{"id":{"d":803,"n":3,"t":4},"speaker":"A","text":"ビタミンCですか？"},{"id":{"d":803,"n":4,"t":4},"speaker":"B","text":"はい。私の息子は、夏バテにはレモン蕎麦がいいと言っていました。"},{"id":{"d":803,"n":5,"t":4},"speaker":"A","text":"お蕎麦にレモンを入れるのですか？"},{"id":{"d":803,"n":6,"t":4},"speaker":"B","text":"そうです。レモンを絞った冷たいつゆをお蕎麦にかけて、さらにスライスレモンをたくさん乗せて作ります。"},{"id":{"d":803,"n":7,"t":4},"speaker":"A","text":"さっぱりしていて食べやすそうですし、おいしそうですね。"},{"id":{"d":803,"n":8,"t":4},"speaker":"B","text":"ビタミンCをおいしくたくさん取れるので、夏バテにも効くと思いますよ。ぜひお試しください。"},{"id":{"d":803,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。今度、作ってみます。"}];
INSERT INTO sentence [{"id":{"d":804,"n":1,"t":4},"speaker":"A","text":"今日は散歩をする道を変えてみました。"},{"id":{"d":804,"n":2,"t":4},"speaker":"B","text":"気分が変わっていいですね。いつもの道は飽きてしまったのですか？"},{"id":{"d":804,"n":3,"t":4},"speaker":"A","text":"いつもの道は、春は桜がきれいで良かったのですが、秋は寂しげな道になってしまったのです。"},{"id":{"d":804,"n":4,"t":4},"speaker":"B","text":"新しい散歩道は秋を感じられる道ですか？"},{"id":{"d":804,"n":5,"t":4},"speaker":"A","text":"はい。イチョウの木があるのですが、ちょうど葉が黄色く色付いてきていて、きれいですよ。"},{"id":{"d":804,"n":6,"t":4},"speaker":"B","text":"それはすてきですね。イチョウがあるということは、ギンナンもあるのですか？"},{"id":{"d":804,"n":7,"t":4},"speaker":"A","text":"きちんと確認したわけではありませんが、言われてみれば少しギンナンの匂いがしたかもしれません。"}];
INSERT INTO sentence [{"id":{"d":805,"n":1,"t":4},"speaker":"A","text":"地元のハーフマラソンに出場したのですが、途中で棄権してしまいました。"},{"id":{"d":805,"n":2,"t":4},"speaker":"B","text":"出場しただけでも素晴らしいですよ。お疲れさまでした。"},{"id":{"d":805,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。ジョギングの延長だと思って参加しましたが、考えが甘かったようです。"},{"id":{"d":805,"n":4,"t":4},"speaker":"B","text":"どのあたりで棄権してしまったのですか？"},{"id":{"d":805,"n":5,"t":4},"speaker":"A","text":"最初の給水所で水を飲んだら、それ以上は走るのがつらくなってしまいました。"},{"id":{"d":805,"n":6,"t":4},"speaker":"B","text":"それは残念でしたね。"},{"id":{"d":805,"n":7,"t":4},"speaker":"A","text":"でも、この経験のおかげで、次こそはハーフマラソンを完走するという目標ができました。"},{"id":{"d":805,"n":8,"t":4},"speaker":"B","text":"それは良かったですね。次の目標に向かって頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":806,"n":1,"t":4},"speaker":"A","text":"おはようございます。南さん、少し頰が腫れていませんか？"},{"id":{"d":806,"n":2,"t":4},"speaker":"B","text":"おはようございます。やはり腫れているのが分かりますか？"},{"id":{"d":806,"n":3,"t":4},"speaker":"A","text":"はい。右の頰がいつもより膨らんでいるような気がします。"},{"id":{"d":806,"n":4,"t":4},"speaker":"B","text":"昨日、歯医者に行って親知らずを抜いたのです。"},{"id":{"d":806,"n":5,"t":4},"speaker":"A","text":"なるほど。それで頰が腫れているのですね。"},{"id":{"d":806,"n":6,"t":4},"speaker":"B","text":"痛み止めをもらって飲んだのですが、あまり効かなくて痛くてたまりません。"},{"id":{"d":806,"n":7,"t":4},"speaker":"A","text":"では、今日の食事会はどうしますか？"},{"id":{"d":806,"n":8,"t":4},"speaker":"B","text":"その予定のことを忘れていました。残念ですが欠席にしてください。"}];
INSERT INTO sentence [{"id":{"d":807,"n":1,"t":4},"speaker":"A","text":"ここの電球を替えてもらえませんか？"},{"id":{"d":807,"n":2,"t":4},"speaker":"B","text":"いいですよ。でもお兄さんでも届く高さですよね。どうかしましたか？"},{"id":{"d":807,"n":3,"t":4},"speaker":"A","text":"手が上がらなくて、届かないのですよ。"},{"id":{"d":807,"n":4,"t":4},"speaker":"B","text":"万歳はできますか？"},{"id":{"d":807,"n":5,"t":4},"speaker":"A","text":"どうでしょうか。手を上げてみますが、これくらいがやっとです。"},{"id":{"d":807,"n":6,"t":4},"speaker":"B","text":"全然上がりませんね。"},{"id":{"d":807,"n":7,"t":4},"speaker":"A","text":"医者が言うには、四十肩らしいです。"},{"id":{"d":807,"n":8,"t":4},"speaker":"B","text":"それは無理しない方がいいですね。私が取り替えましょう。"}];
INSERT INTO sentence [{"id":{"d":808,"n":1,"t":4},"speaker":"A","text":"市営プールで水中ウォーキング教室があるのですが、一緒に行きませんか？"},{"id":{"d":808,"n":2,"t":4},"speaker":"B","text":"私は水着を持っていないので、遠慮しておきます。"},{"id":{"d":808,"n":3,"t":4},"speaker":"A","text":"健康にとても良いのですよ。水着を買いに行くのに付き合いますから、一緒に行きませんか？"},{"id":{"d":808,"n":4,"t":4},"speaker":"B","text":"私のような年齢の人でも着られる水着などあるのでしょうか？"},{"id":{"d":808,"n":5,"t":4},"speaker":"A","text":"もちろんたくさんありますよ。プールに行っているのは、私たちと同じような年齢の人ばかりですよ。"},{"id":{"d":808,"n":6,"t":4},"speaker":"B","text":"そうなのですね。プールの水は冷たくはありませんか？"},{"id":{"d":808,"n":7,"t":4},"speaker":"A","text":"ちょうどいい温度に調整してありますから、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":809,"n":1,"t":4},"speaker":"A","text":"新しい公園の端にできたのは何の競技場ですか？"},{"id":{"d":809,"n":2,"t":4},"speaker":"B","text":"あれはゲートボール場です。"},{"id":{"d":809,"n":3,"t":4},"speaker":"A","text":"ゲートボール場ですか。サッカーコートかと思っていました。"},{"id":{"d":809,"n":4,"t":4},"speaker":"B","text":"そこまで大きくはなさそうですよ。"},{"id":{"d":809,"n":5,"t":4},"speaker":"A","text":"確かに、言われてみればそうですね。"},{"id":{"d":809,"n":6,"t":4},"speaker":"B","text":"新しくゲートボール場ができたということは、恐らくこの辺りはゲートボールの競技人口が多いのでしょうね。"},{"id":{"d":809,"n":7,"t":4},"speaker":"A","text":"私の周りにやっている人はいないと思うのですが、少し興味があります。"},{"id":{"d":809,"n":8,"t":4},"speaker":"B","text":"私も気になっていました。今度、一緒に行ってみませんか？"}];
INSERT INTO sentence [{"id":{"d":810,"n":1,"t":4},"speaker":"A","text":"朝早くから皆さんでお出かけですか？"},{"id":{"d":810,"n":2,"t":4},"speaker":"B","text":"はい、これから子どもたちのドッジボールの大会なのです。"},{"id":{"d":810,"n":3,"t":4},"speaker":"A","text":"ドッジボールは学校の授業以外でもするのですね。"},{"id":{"d":810,"n":4,"t":4},"speaker":"B","text":"うちの子どもたちはクラブチームに入っていて、大会があるのですよ。"},{"id":{"d":810,"n":5,"t":4},"speaker":"A","text":"今日はどのような大会ですか？"},{"id":{"d":810,"n":6,"t":4},"speaker":"B","text":"県大会の予選です。"},{"id":{"d":810,"n":7,"t":4},"speaker":"A","text":"頑張ってください。予選を突破できるといいですね。"},{"id":{"d":810,"n":8,"t":4},"speaker":"B","text":"はい。去年は優勝しましたので、今年も期待しています。"}];
INSERT INTO sentence [{"id":{"d":811,"n":1,"t":4},"speaker":"A","text":"海岸にたくさんの人が集まっていますね。"},{"id":{"d":811,"n":2,"t":4},"speaker":"B","text":"ビーチバレーボールの試合をするようですよ。"},{"id":{"d":811,"n":3,"t":4},"speaker":"A","text":"試合というのは、何かの大会の一環なのですか？"},{"id":{"d":811,"n":4,"t":4},"speaker":"B","text":"いいえ。そうではなく、競技普及のためのデモンストレーションのようです。"},{"id":{"d":811,"n":5,"t":4},"speaker":"A","text":"既にかなりの人数が集まっています。人気があるようですね。"},{"id":{"d":811,"n":6,"t":4},"speaker":"B","text":"オリンピックに出たこともある有名な選手が来ているらしいです。"},{"id":{"d":811,"n":7,"t":4},"speaker":"A","text":"そうなのですか？私も見てみたいです。入場料は要るのでしょうか？"},{"id":{"d":811,"n":8,"t":4},"speaker":"B","text":"どうでしょうか。係の人に尋ねてみましょう。"}];
INSERT INTO sentence [{"id":{"d":812,"n":1,"t":4},"speaker":"A","text":"申し訳ありませんが、今日のお出かけはキャンセルさせてください。"},{"id":{"d":812,"n":2,"t":4},"speaker":"B","text":"楽しみにしていたのに、どうされたのですか？"},{"id":{"d":812,"n":3,"t":4},"speaker":"A","text":"少し具合が悪いのです。"},{"id":{"d":812,"n":4,"t":4},"speaker":"B","text":"それは仕方ありませんね。風邪ですか？"},{"id":{"d":812,"n":5,"t":4},"speaker":"A","text":"そうではありませんが、頭痛がひどいのです。"},{"id":{"d":812,"n":6,"t":4},"speaker":"B","text":"お薬は飲みましたか？"},{"id":{"d":812,"n":7,"t":4},"speaker":"A","text":"飲んでいません。多分寝ていれば大丈夫だと思います。"},{"id":{"d":812,"n":8,"t":4},"speaker":"B","text":"お大事にしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":813,"n":1,"t":4},"speaker":"A","text":"今日は友人と出かける予定だったのですが、キャンセルになりました。"},{"id":{"d":813,"n":2,"t":4},"speaker":"B","text":"もしかして相手は太田さんですか？"},{"id":{"d":813,"n":3,"t":4},"speaker":"A","text":"そうです。太田さんは昨日から具合が悪かったのでしょうか？"},{"id":{"d":813,"n":4,"t":4},"speaker":"B","text":"いいえ、昨日は元気いっぱいでしたよ。"},{"id":{"d":813,"n":5,"t":4},"speaker":"A","text":"そうですか。急にどうされたのでしょうね。"},{"id":{"d":813,"n":6,"t":4},"speaker":"B","text":"元気過ぎて、昨日の会食ではお酒をたくさん飲まれていました。"},{"id":{"d":813,"n":7,"t":4},"speaker":"A","text":"では、不調の原因は二日酔いかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":814,"n":1,"t":4},"speaker":"A","text":"最近、良い洗顔料を見つけました。"},{"id":{"d":814,"n":2,"t":4},"speaker":"B","text":"少し前までは、洗顔後に肌がつっぱると言って困っていましたよね。"},{"id":{"d":814,"n":3,"t":4},"speaker":"A","text":"はい。新しく買ってきた洗顔料は大丈夫でした。"},{"id":{"d":814,"n":4,"t":4},"speaker":"B","text":"それは良かったですね。参考までに、どのような商品かを伺ってもよいですか？"},{"id":{"d":814,"n":5,"t":4},"speaker":"A","text":"これです。日本酒を原料とした洗顔料です。"},{"id":{"d":814,"n":6,"t":4},"speaker":"B","text":"お酒好きな佐々木さんが選びそうな商品ですね。"},{"id":{"d":814,"n":7,"t":4},"speaker":"A","text":"偶然にも前の晩に飲んでいたお酒と同じ銘柄だったので、縁を感じて購入しました。"},{"id":{"d":814,"n":8,"t":4},"speaker":"B","text":"その洗顔料はお酒臭くないのでしょうか？"},{"id":{"d":814,"n":9,"t":4},"speaker":"A","text":"お酒の匂いはしませんよ。"}];
INSERT INTO sentence [{"id":{"d":815,"n":1,"t":4},"speaker":"A","text":"こんにちは。これからどこかに行かれるのですか？"},{"id":{"d":815,"n":2,"t":4},"speaker":"B","text":"こんにちは。今は家に帰るところです。"},{"id":{"d":815,"n":3,"t":4},"speaker":"A","text":"もうすぐお昼時ですから、一緒にご飯を食べに行きませんか？"},{"id":{"d":815,"n":4,"t":4},"speaker":"B","text":"ごめんなさい。歯科医院で治療をしてきた帰りなので、とても食べられそうにありません。"},{"id":{"d":815,"n":5,"t":4},"speaker":"A","text":"定期健診ではなくて治療されたのですか？"},{"id":{"d":815,"n":6,"t":4},"speaker":"B","text":"はい。麻酔を打って抜歯をしてきました。"},{"id":{"d":815,"n":7,"t":4},"speaker":"A","text":"そう言われてみれば、頰が腫れているようですね。では、また今度お誘いしますね。"},{"id":{"d":815,"n":8,"t":4},"speaker":"B","text":"ご飯に付き合えなくてすみません。"}];
INSERT INTO sentence [{"id":{"d":816,"n":1,"t":4},"speaker":"A","text":"お疲れさまです。"},{"id":{"d":816,"n":2,"t":4},"speaker":"B","text":"あれ、石井さんは先ほど定時で上がられたはずですよね。どうしたのですか？"},{"id":{"d":816,"n":3,"t":4},"speaker":"A","text":"友人と食事をしたのですが、忘れ物に気付いて戻ってきました。"},{"id":{"d":816,"n":4,"t":4},"speaker":"B","text":"気が付いてよかったですね。"},{"id":{"d":816,"n":5,"t":4},"speaker":"A","text":"はい。食事会の後、ポーチを会社に置き忘れたことに気付いたのです。会社の近くまで来たら、部屋の電気がまだ付いていたので戻ってみました。これは篠崎さんに差し入れです。"},{"id":{"d":816,"n":6,"t":4},"speaker":"B","text":"栄養ドリンクと軽食ですね。ありがとうございます。疲れていたのでうれしいです。"},{"id":{"d":816,"n":7,"t":4},"speaker":"A","text":"そろそろ残業もおしまいになさったらいかがですか？"},{"id":{"d":816,"n":8,"t":4},"speaker":"B","text":"そうですね。これを頂いて、もう少し作業したら帰ろうと思います。"},{"id":{"d":816,"n":9,"t":4},"speaker":"A","text":"頑張ってくださいね。では、お先に失礼します。"},{"id":{"d":816,"n":10,"t":4},"speaker":"B","text":"ありがとうございます。お疲れさまでした。"}];
INSERT INTO sentence [{"id":{"d":817,"n":1,"t":4},"speaker":"A","text":"先生、肩の痛みがなかなか治まらないのですが、どうしたらよいでしょうか？"},{"id":{"d":817,"n":2,"t":4},"speaker":"B","text":"骨折はしていませんから、あまり心配しなくても大丈夫ですよ。湿布を貼って安静にしていてください。"},{"id":{"d":817,"n":3,"t":4},"speaker":"A","text":"痛み止めの飲み薬は頂けませんか？"},{"id":{"d":817,"n":4,"t":4},"speaker":"B","text":"眠れないほど痛いのですか？"},{"id":{"d":817,"n":5,"t":4},"speaker":"A","text":"はい、昨夜もあまり眠れていません。"},{"id":{"d":817,"n":6,"t":4},"speaker":"B","text":"分かりました。では、痛み止めを処方しましょう。"},{"id":{"d":817,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":817,"n":8,"t":4},"speaker":"B","text":"薬は出しますが、くれぐれも安静にしてくださいね。"},{"id":{"d":817,"n":9,"t":4},"speaker":"A","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":818,"n":1,"t":4},"speaker":"A","text":"最近は新しい腕時計を着けていますね。"},{"id":{"d":818,"n":2,"t":4},"speaker":"B","text":"これは腕時計型の活動量計です。"},{"id":{"d":818,"n":3,"t":4},"speaker":"A","text":"万歩計のようなものですか？"},{"id":{"d":818,"n":4,"t":4},"speaker":"B","text":"万歩計の機能もありますが、もっといろいろな数値を知ることができます。"},{"id":{"d":818,"n":5,"t":4},"speaker":"A","text":"消費カロリーなども分かるのですね。"},{"id":{"d":818,"n":6,"t":4},"speaker":"B","text":"そうです。でも、活動量計を着けるのをそろそろやめようかと思っています。"},{"id":{"d":818,"n":7,"t":4},"speaker":"A","text":"どうしてですか？健康管理に便利そうですのに。"},{"id":{"d":818,"n":8,"t":4},"speaker":"B","text":"デスクワークが続くと「運動量が足りません」と活動量計に注意されるのです。そう言われてしまうと少し悲しくなるので、時々は外そうかなと思ってしまいます。"}];
INSERT INTO sentence [{"id":{"d":819,"n":1,"t":4},"speaker":"A","text":"今日は恥ずかしいことがありました。"},{"id":{"d":819,"n":2,"t":4},"speaker":"B","text":"何があったのですか？"},{"id":{"d":819,"n":3,"t":4},"speaker":"A","text":"ママさんバレーの試合で、レシーブをしようとしたら顔で受けてしまいました。"},{"id":{"d":819,"n":4,"t":4},"speaker":"B","text":"それは痛そうです。ぶつけたところは大丈夫だったのですか？"},{"id":{"d":819,"n":5,"t":4},"speaker":"A","text":"幸いけがはありませんでした。歯も折れませんでしたし、顔が腫れもしませんでした。"},{"id":{"d":819,"n":6,"t":4},"speaker":"B","text":"けがをしていないのであれば良かったです。次の試合も頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":820,"n":1,"t":4},"speaker":"A","text":"新体操の試合を見てきました。初めて見たのですが、格好良いものですね。"},{"id":{"d":820,"n":2,"t":4},"speaker":"B","text":"私は、新体操は格好良いというより美しいと思います。"},{"id":{"d":820,"n":3,"t":4},"speaker":"A","text":"確かに、手足の先までそろって伸びた姿は、とても美しかったです。"},{"id":{"d":820,"n":4,"t":4},"speaker":"B","text":"私は、新体操の中でもリボンの動きが特に美しいと思います。"},{"id":{"d":820,"n":5,"t":4},"speaker":"A","text":"リボンですか？"},{"id":{"d":820,"n":6,"t":4},"speaker":"B","text":"はい。新体操ではリボンをくるくると回して演技をしますよね。"},{"id":{"d":820,"n":7,"t":4},"speaker":"A","text":"私が見たときは、誰も何も持っていませんでしたよ。"},{"id":{"d":820,"n":8,"t":4},"speaker":"B","text":"もしかして、あなたが見たのは男子の新体操ですか？"},{"id":{"d":820,"n":9,"t":4},"speaker":"A","text":"はい、男子の新体操を見てきました。"}];
INSERT INTO sentence [{"id":{"d":821,"n":1,"t":4},"speaker":"A","text":"お父さん、一輪車の乗り方を教えてください。"},{"id":{"d":821,"n":2,"t":4},"speaker":"B","text":"一輪車はお父さんも乗れませんよ。"},{"id":{"d":821,"n":3,"t":4},"speaker":"A","text":"子どもの時も乗らなかったのですか？"},{"id":{"d":821,"n":4,"t":4},"speaker":"B","text":"お父さんが子どもの時は、一輪車で遊んだことはほとんどありませんでした。"},{"id":{"d":821,"n":5,"t":4},"speaker":"A","text":"そうですか。ともちゃんが一輪車を貸してくれると言ってくれたのですけれど、お父さんに教えてもらえないのなら、借りても仕方ありませんね。"},{"id":{"d":821,"n":6,"t":4},"speaker":"B","text":"教えることはできなくても、転ばないように支えてあげることはできますよ。"},{"id":{"d":821,"n":7,"t":4},"speaker":"A","text":"本当ですか？お願いします。では、一輪車を借りてきますね。"}];
INSERT INTO sentence [{"id":{"d":822,"n":1,"t":4},"speaker":"A","text":"近頃は字が見えにくくて困っています。"},{"id":{"d":822,"n":2,"t":4},"speaker":"B","text":"とうとう老眼が来ましたか？"},{"id":{"d":822,"n":3,"t":4},"speaker":"A","text":"まだ40代になったばかりなのに、老眼になりますか？"},{"id":{"d":822,"n":4,"t":4},"speaker":"B","text":"早い人は30代でも老眼になるようですよ。"},{"id":{"d":822,"n":5,"t":4},"speaker":"A","text":"老眼鏡を買わなくてはならないでしょうか？"},{"id":{"d":822,"n":6,"t":4},"speaker":"B","text":"日常に支障があるようでしたら、買った方がいいと思います。"},{"id":{"d":822,"n":7,"t":4},"speaker":"A","text":"どうせなら格好良い眼鏡を買いたいですね。"},{"id":{"d":822,"n":8,"t":4},"speaker":"B","text":"ゆっくり探してください。"}];
INSERT INTO sentence [{"id":{"d":823,"n":1,"t":4},"speaker":"A","text":"この前、足つぼマッサージに行ってきました。あれはとても痛いのですね。"},{"id":{"d":823,"n":2,"t":4},"speaker":"B","text":"私はまだ足つぼマッサージをしたことがないので、よく分かりません。"},{"id":{"d":823,"n":3,"t":4},"speaker":"A","text":"テレビでタレントさんが罰ゲームとして足つぼマッサージを受けているところを見たことがありますか？"},{"id":{"d":823,"n":4,"t":4},"speaker":"B","text":"はい。皆、大きな悲鳴を上げていますね。"},{"id":{"d":823,"n":5,"t":4},"speaker":"A","text":"私はてっきりあれは演技だと思っていたのです。"},{"id":{"d":823,"n":6,"t":4},"speaker":"B","text":"実際に受けてみたら本当に痛かったのですか？"},{"id":{"d":823,"n":7,"t":4},"speaker":"A","text":"はい。私もタレントになれそうだと思うほどの悲鳴を上げました。"},{"id":{"d":823,"n":8,"t":4},"speaker":"B","text":"足つぼマッサージには興味がありましたが、それほど痛いのならやめておこうかと思います。"}];
INSERT INTO sentence [{"id":{"d":824,"n":1,"t":4},"speaker":"A","text":"のど飴はどちらにありますか？"},{"id":{"d":824,"n":2,"t":4},"speaker":"B","text":"医薬品ののど飴をご希望ですか？"},{"id":{"d":824,"n":3,"t":4},"speaker":"A","text":"はい。昨日までなめていたのは「のどすっきり飴」という飴ですが、同じものはありますか？"},{"id":{"d":824,"n":4,"t":4},"speaker":"B","text":"それは医薬品ではなくお菓子ですので、お菓子売り場でご購入ください。"},{"id":{"d":824,"n":5,"t":4},"speaker":"A","text":"薬売り場には置いていませんか？"},{"id":{"d":824,"n":6,"t":4},"speaker":"B","text":"はい、医薬品ではありませんのでこちらにはございません。"},{"id":{"d":824,"n":7,"t":4},"speaker":"A","text":"そうなのですね。とてもよく効くような気がしていたので、てっきり医薬品だと思っていました。"},{"id":{"d":824,"n":8,"t":4},"speaker":"B","text":"お菓子ののど飴にも喉に良い成分は入っていますよ。"}];
INSERT INTO sentence [{"id":{"d":825,"n":1,"t":4},"speaker":"A","text":"お昼に飲む薬を飲み忘れてしまいました。"},{"id":{"d":825,"n":2,"t":4},"speaker":"B","text":"飲まなくても体に支障はありませんか？"},{"id":{"d":825,"n":3,"t":4},"speaker":"A","text":"血糖値を下げる薬なので、食べ物に気を付ければ大丈夫だと思います。"},{"id":{"d":825,"n":4,"t":4},"speaker":"B","text":"そうですか。薬を飲み忘れることはよくあるのですか？"},{"id":{"d":825,"n":5,"t":4},"speaker":"A","text":"実は、これまでもたまに忘れてしまっています。"},{"id":{"d":825,"n":6,"t":4},"speaker":"B","text":"薬を飲み忘れたために病院に担ぎ込まれたことはありますか？"},{"id":{"d":825,"n":7,"t":4},"speaker":"A","text":"幸いなことに、今のところはありません。"},{"id":{"d":825,"n":8,"t":4},"speaker":"B","text":"あなたに倒れられたら、どうしたらよいか分からず慌ててしまいそうです。どうぞ気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":826,"n":1,"t":4},"speaker":"A","text":"お母さん、転んで膝から血が出ました。"},{"id":{"d":826,"n":2,"t":4},"speaker":"B","text":"見せてごらんなさい。それほどひどくはありませんね。"},{"id":{"d":826,"n":3,"t":4},"speaker":"A","text":"服が汚れてしまいました。"},{"id":{"d":826,"n":4,"t":4},"speaker":"B","text":"少し汚れてしまいましたね。それは気にしなくていいですから、すぐにお風呂場に行って足を洗ってきてください。"},{"id":{"d":826,"n":5,"t":4},"speaker":"A","text":"はい、そうします。"},{"id":{"d":826,"n":6,"t":4},"speaker":"B","text":"もう血は止まっていますね。消毒して一応、ばんそうこうを貼っておきましょう。着替えたらおやつにしますよ。"}];
INSERT INTO sentence [{"id":{"d":827,"n":1,"t":4},"speaker":"A","text":"歯磨き教室は勉強になりましたね。"},{"id":{"d":827,"n":2,"t":4},"speaker":"B","text":"まだ虫歯になったことはありませんが、虫歯ができたらやはり痛いのですか？"},{"id":{"d":827,"n":3,"t":4},"speaker":"A","text":"とても痛いですよ。ですから、虫歯にならないために歯磨きをきちんとすることが大切です。"},{"id":{"d":827,"n":4,"t":4},"speaker":"B","text":"はい、頑張ります。"},{"id":{"d":827,"n":5,"t":4},"speaker":"A","text":"土曜日には歯医者さんへ行きましょうね。"},{"id":{"d":827,"n":6,"t":4},"speaker":"B","text":"虫歯はないのになぜ行くのですか？"},{"id":{"d":827,"n":7,"t":4},"speaker":"A","text":"歯磨き教室で、定期健診をした方がいいと言われたでしょう？健診のために行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":828,"n":1,"t":4},"speaker":"A","text":"会社の健康診断までに体脂肪を減らしたいと思っています。"},{"id":{"d":828,"n":2,"t":4},"speaker":"B","text":"健康診断の後は薄着の季節に入りますから、自分の体型が気になりますね。"},{"id":{"d":828,"n":3,"t":4},"speaker":"A","text":"はい。ついゆったりとした服ばかりを着てしまいそうです。"},{"id":{"d":828,"n":4,"t":4},"speaker":"B","text":"健康診断までの2週間で、どこまで体脂肪を減らせるものでしょうか？"},{"id":{"d":828,"n":5,"t":4},"speaker":"A","text":"先週からサプリメントを試しているのですが、今のところは全く変化がありません。"},{"id":{"d":828,"n":6,"t":4},"speaker":"B","text":"やはり運動や食事制限によって体脂肪を減らした方がいいのではないでしょうか？"},{"id":{"d":828,"n":7,"t":4},"speaker":"A","text":"食生活は大事ですね。残業後だと夕飯を食べるのが遅くなってしまいます。それではいけないとは思っているのですが。"},{"id":{"d":828,"n":8,"t":4},"speaker":"B","text":"とりあえず2週間だけでも、その食生活を改善してみてはいかがでしょうか？"}];
INSERT INTO sentence [{"id":{"d":829,"n":1,"t":4},"speaker":"A","text":"昨日は少し恥ずかしい思いをしました。"},{"id":{"d":829,"n":2,"t":4},"speaker":"B","text":"何があったのですか？"},{"id":{"d":829,"n":3,"t":4},"speaker":"A","text":"夜にシートマスクでパックをしているところに宅配便が来たのです。"},{"id":{"d":829,"n":4,"t":4},"speaker":"B","text":"もしかして、そのままの顔で応対したのですか？"},{"id":{"d":829,"n":5,"t":4},"speaker":"A","text":"シートマスクを顔から剝がすべきかどうか、一瞬、葛藤はしたのですが。"},{"id":{"d":829,"n":6,"t":4},"speaker":"B","text":"でも、マスクを着けたまま応対したのですね。"},{"id":{"d":829,"n":7,"t":4},"speaker":"A","text":"ええ。シートマスクを顔に着けたばかりだったので、すぐに剝がしてしまうのはもったいなくて嫌だったのです。配達員のお兄さんは、私の顔を見て驚いていました。"},{"id":{"d":829,"n":8,"t":4},"speaker":"B","text":"それはそうでしょう。さぞ驚いたでしょうね。"}];
INSERT INTO sentence [{"id":{"d":830,"n":1,"t":4},"speaker":"A","text":"今日は肩凝りに効くストレッチを教わってきましたよ。"},{"id":{"d":830,"n":2,"t":4},"speaker":"B","text":"体操教室にでも行かれたのですか？"},{"id":{"d":830,"n":3,"t":4},"speaker":"A","text":"はい。町内で年配の方向けの健康体操があって、母の付き添いで行ってきました。"},{"id":{"d":830,"n":4,"t":4},"speaker":"B","text":"健康体操の中に、肩凝り解消のストレッチもあるのですね。"},{"id":{"d":830,"n":5,"t":4},"speaker":"A","text":"いいえ。肩凝りに悩んでいたので、講習が終わった後に、先生に聞いてみたのです。"},{"id":{"d":830,"n":6,"t":4},"speaker":"B","text":"終わった後に教えてくれるなんて、親切な先生ですね。"},{"id":{"d":830,"n":7,"t":4},"speaker":"A","text":"はい、感謝しています。とても気持ちが良いので一緒にやりましょう。"},{"id":{"d":830,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。ぜひその体操を教えてください。"}];
INSERT INTO sentence [{"id":{"d":831,"n":1,"t":4},"speaker":"A","text":"喜美ちゃん、はさみを取ってください。"},{"id":{"d":831,"n":2,"t":4},"speaker":"B","text":"はい。何を始めるのですか？"},{"id":{"d":831,"n":3,"t":4},"speaker":"A","text":"おじいさんのお薬を、薬カレンダーのポケットに仕分けるのです。"},{"id":{"d":831,"n":4,"t":4},"speaker":"B","text":"そういえば、おじいさんは今日病院に行ってきたのでしたね。"},{"id":{"d":831,"n":5,"t":4},"speaker":"A","text":"はい、病院に行って薬もたくさんもらってきました。喜美ちゃんも手伝ってくれませんか？"},{"id":{"d":831,"n":6,"t":4},"speaker":"B","text":"もちろん手伝います。一粒ずつ切り離せばよいのですか？"},{"id":{"d":831,"n":7,"t":4},"speaker":"A","text":"そうです。種類が交ざらないように分けておいてくださいね。"},{"id":{"d":831,"n":8,"t":4},"speaker":"B","text":"分かりました。薬の整理も大変ですね。"}];
INSERT INTO sentence [{"id":{"d":832,"n":1,"t":4},"speaker":"A","text":"少し飲み過ぎてしまったようです。"},{"id":{"d":832,"n":2,"t":4},"speaker":"B","text":"帰りの電車に乗れますか？"},{"id":{"d":832,"n":3,"t":4},"speaker":"A","text":"今すぐ乗るのは厳しいかもしれません。そこのベンチで少しだけ休んでから帰ります。"},{"id":{"d":832,"n":4,"t":4},"speaker":"B","text":"お水を買ってきましょうか？"},{"id":{"d":832,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。でも、先に帰ってもよいですよ。"},{"id":{"d":832,"n":6,"t":4},"speaker":"B","text":"心配ですから、付き合いますよ。"},{"id":{"d":832,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":833,"n":1,"t":4},"speaker":"A","text":"すみません。この辺りに小児科の病院はありませんか？"},{"id":{"d":833,"n":2,"t":4},"speaker":"B","text":"ありますよ。お子さんの具合が悪いのですか？"},{"id":{"d":833,"n":3,"t":4},"speaker":"A","text":"はい。息子が熱を出しているのです。"},{"id":{"d":833,"n":4,"t":4},"speaker":"B","text":"引っ越してきてそれほど経っていませんから、どこに病院があるのか分かりませんよね。"},{"id":{"d":833,"n":5,"t":4},"speaker":"A","text":"そうなのです。学校やスーパーは事前に調べていたのですが、病院は失念していました。"},{"id":{"d":833,"n":6,"t":4},"speaker":"B","text":"ここから近い小児科は、小学校の正門の斜め向かいのこども病院です。"},{"id":{"d":833,"n":7,"t":4},"speaker":"A","text":"正門の斜め向かいですね。今から行ってみます。"},{"id":{"d":833,"n":8,"t":4},"speaker":"B","text":"すぐ診てもらえるといいですね。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":834,"n":1,"t":4},"speaker":"A","text":"腕にあざがありますね。どこかにぶつけたのですか？"},{"id":{"d":834,"n":2,"t":4},"speaker":"B","text":"昨日の健康診断で採血があったので、そのせいかもしれません。"},{"id":{"d":834,"n":3,"t":4},"speaker":"A","text":"採血だけでそのようなあざになってしまったのですか？"},{"id":{"d":834,"n":4,"t":4},"speaker":"B","text":"採血に失敗したようです。"},{"id":{"d":834,"n":5,"t":4},"speaker":"A","text":"痛そうですね。大丈夫ですか？"},{"id":{"d":834,"n":6,"t":4},"speaker":"B","text":"はい、大丈夫です。触らなければ痛くはありません。"},{"id":{"d":834,"n":7,"t":4},"speaker":"A","text":"それならいいのですが、半袖の季節ですから少し目立つかもしれません。"},{"id":{"d":834,"n":8,"t":4},"speaker":"B","text":"実は、既に何人もの人から「大丈夫ですか？」と聞かれました。ばんそうこうでもすべきでしょうか？"},{"id":{"d":834,"n":9,"t":4},"speaker":"A","text":"あなたが気にならなければ、そのままでよいと思いますよ。"},{"id":{"d":834,"n":10,"t":4},"speaker":"B","text":"そうですね。ばんそうこうでかぶれてもいけませんからね。"}];
INSERT INTO sentence [{"id":{"d":835,"n":1,"t":4},"speaker":"A","text":"手にできたできものは治りましたか？"},{"id":{"d":835,"n":2,"t":4},"speaker":"B","text":"治りました。ほら、もうないでしょう？"},{"id":{"d":835,"n":3,"t":4},"speaker":"A","text":"本当ですね。あれは何だったのですか？"},{"id":{"d":835,"n":4,"t":4},"speaker":"B","text":"よくある皮膚の炎症とのことでした。"},{"id":{"d":835,"n":5,"t":4},"speaker":"A","text":"薬で治したのですか？"},{"id":{"d":835,"n":6,"t":4},"speaker":"B","text":"はい。きれいに治ってよかったです。"}];
INSERT INTO sentence [{"id":{"d":836,"n":1,"t":4},"speaker":"A","text":"蚊が寄ってきますね。"},{"id":{"d":836,"n":2,"t":4},"speaker":"B","text":"そうですね。蚊に刺されないように、これを使ってみますか？"},{"id":{"d":836,"n":3,"t":4},"speaker":"A","text":"これは何ですか？"},{"id":{"d":836,"n":4,"t":4},"speaker":"B","text":"虫よけスプレーです。"},{"id":{"d":836,"n":5,"t":4},"speaker":"A","text":"今は何も持っていないのでありがたいです。お借りしますね。"},{"id":{"d":836,"n":6,"t":4},"speaker":"B","text":"この辺りは蚊が多いので持ち歩いているのです。"},{"id":{"d":836,"n":7,"t":4},"speaker":"A","text":"このくらい小さい容器のものでしたら、持ち歩けますね。それにとても良い香りです。"},{"id":{"d":836,"n":8,"t":4},"speaker":"B","text":"アロマオイルで自作したのですよ。"}];
INSERT INTO sentence [{"id":{"d":837,"n":1,"t":4},"speaker":"A","text":"天気が悪いと関節が痛くてたまりません。"},{"id":{"d":837,"n":2,"t":4},"speaker":"B","text":"私も膝の痛みがひどくなります。"},{"id":{"d":837,"n":3,"t":4},"speaker":"A","text":"膝は以前に手術されたのでしたよね？"},{"id":{"d":837,"n":4,"t":4},"speaker":"B","text":"そうです。半月板を痛めたことがありました。"},{"id":{"d":837,"n":5,"t":4},"speaker":"A","text":"お天気で古傷が痛むというのはどういうことなのでしょうね？"},{"id":{"d":837,"n":6,"t":4},"speaker":"B","text":"気象病というらしいですよ。"},{"id":{"d":837,"n":7,"t":4},"speaker":"A","text":"気圧の変化が関係しているのでしょうか？"},{"id":{"d":837,"n":8,"t":4},"speaker":"B","text":"詳しいことは分かりませんが、そうとも言われているようですよ。"}];
INSERT INTO sentence [{"id":{"d":838,"n":1,"t":4},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":838,"n":2,"t":4},"speaker":"B","text":"おかえりなさい。ご飯ができるまでもう少し待ってくださいね。"},{"id":{"d":838,"n":3,"t":4},"speaker":"A","text":"今日はお母さんも帰りが遅かったのですか？"},{"id":{"d":838,"n":4,"t":4},"speaker":"B","text":"いつもと同じ時間に帰って食事の支度をしていたのですが、うっかり熱くなったお鍋に触ってしまったのです。"},{"id":{"d":838,"n":5,"t":4},"speaker":"A","text":"やけどをしたのですか？"},{"id":{"d":838,"n":6,"t":4},"speaker":"B","text":"はい。でも、大したことはありません。しばらく水に浸けて薬を塗ったら痛くなくなりました。"},{"id":{"d":838,"n":7,"t":4},"speaker":"A","text":"それでも安静にしていてください。お料理の続きは私がやりますよ。"},{"id":{"d":838,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。もう大丈夫ですが、手伝ってくれると助かります。"}];
INSERT INTO sentence [{"id":{"d":839,"n":1,"t":4},"speaker":"A","text":"手の指にとげが刺さったようなのですが、よく見えません。"},{"id":{"d":839,"n":2,"t":4},"speaker":"B","text":"私も老眼で見えませんね。困りました。"},{"id":{"d":839,"n":3,"t":4},"speaker":"A","text":"虫眼鏡はありませんか？"},{"id":{"d":839,"n":4,"t":4},"speaker":"B","text":"虫眼鏡と言えるか分かりませんが、私が新聞を読むときに使っている、字が拡大できて、ライトがつく道具があります。"},{"id":{"d":839,"n":5,"t":4},"speaker":"A","text":"それなら見えるかもしれません。お借りしても良いですか？"},{"id":{"d":839,"n":6,"t":4},"speaker":"B","text":"はい。これです、どうぞ。"},{"id":{"d":839,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。おかげで見えるようになりましたが、うまくつかめなくてとげ抜きでも取れません。"},{"id":{"d":839,"n":8,"t":4},"speaker":"B","text":"指をふやかしてから取るといいと聞いたことがありますよ。やってみましょう。"}];
INSERT INTO sentence [{"id":{"d":840,"n":1,"t":4},"speaker":"A","text":"おばあさん、これほど暑い部屋では、クーラーを使わないと熱中症になりますよ。"},{"id":{"d":840,"n":2,"t":4},"speaker":"B","text":"私は暑くないので大丈夫ですよ。"},{"id":{"d":840,"n":3,"t":4},"speaker":"A","text":"室温が30度を超えているのに暑さを感じないのですか？今日のような日はクーラーを使ってください。"},{"id":{"d":840,"n":4,"t":4},"speaker":"B","text":"クーラーは苦手なのですよ。"},{"id":{"d":840,"n":5,"t":4},"speaker":"A","text":"では、せめて扇風機をつけてください。それから、水分も取ってください。"},{"id":{"d":840,"n":6,"t":4},"speaker":"B","text":"分かりました。では、扇風機をつけたら、お茶を入れてきましょう。"}];
INSERT INTO sentence [{"id":{"d":841,"n":1,"t":4},"speaker":"A","text":"最近は半身浴にはまっているのです。"},{"id":{"d":841,"n":2,"t":4},"speaker":"B","text":"半身浴をすると何か良いことがあるのですか？"},{"id":{"d":841,"n":3,"t":4},"speaker":"A","text":"私はよく眠れるようになりました。"},{"id":{"d":841,"n":4,"t":4},"speaker":"B","text":"以前は眠れないと言っていましたから、良かったですね。"},{"id":{"d":841,"n":5,"t":4},"speaker":"A","text":"半身浴にはリラックス効果があるようです。"},{"id":{"d":841,"n":6,"t":4},"speaker":"B","text":"湯船に入っている間は何かしているのですか？"},{"id":{"d":841,"n":7,"t":4},"speaker":"A","text":"スマートフォンを持ち込んで、動画を見ています。"},{"id":{"d":841,"n":8,"t":4},"speaker":"B","text":"お湯に落とさないように気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":842,"n":1,"t":4},"speaker":"A","text":"うちの子が歯磨きを嫌がって困っています。"},{"id":{"d":842,"n":2,"t":4},"speaker":"B","text":"一人で磨かせているのですか？"},{"id":{"d":842,"n":3,"t":4},"speaker":"A","text":"はい。なんでも一人でできるようになった方がいいかと思いまして。"},{"id":{"d":842,"n":4,"t":4},"speaker":"B","text":"うちは家族みんなで同じ時間にやっていますよ。"},{"id":{"d":842,"n":5,"t":4},"speaker":"A","text":"みんなで、ですか？旦那さんも一緒なのですか？"},{"id":{"d":842,"n":6,"t":4},"speaker":"B","text":"はい。一緒にできるときはやります。"},{"id":{"d":842,"n":7,"t":4},"speaker":"A","text":"そうすると嫌がらずに歯磨きをしてくれますか？"},{"id":{"d":842,"n":8,"t":4},"speaker":"B","text":"はい。みんなで同じ行動をするのは楽しいようで、毎日喜んで歯磨きをしていますよ。"},{"id":{"d":842,"n":9,"t":4},"speaker":"A","text":"そうなのですね。せめて私だけでも一緒にしてみようと思います。"},{"id":{"d":842,"n":10,"t":4},"speaker":"B","text":"私のお薦めは歯磨き体操の動画です。合わせて磨くと、楽しく磨けますよ。"}];
INSERT INTO sentence [{"id":{"d":843,"n":1,"t":4},"speaker":"A","text":"今日はまとめ髪なのですね。"},{"id":{"d":843,"n":2,"t":4},"speaker":"B","text":"はい、今朝は寝ぐせがひどくて大変だったのです。"},{"id":{"d":843,"n":3,"t":4},"speaker":"A","text":"それでまとめ髪にしたのですか？"},{"id":{"d":843,"n":4,"t":4},"speaker":"B","text":"その通りです。"},{"id":{"d":843,"n":5,"t":4},"speaker":"A","text":"寝ぐせを隠すためにまとめたとはいえ、とてもかわいらしく似合っていますよ。"},{"id":{"d":843,"n":6,"t":4},"speaker":"B","text":"これならイメージチェンジしたと思ってもらえますか？"},{"id":{"d":843,"n":7,"t":4},"speaker":"A","text":"はい。暑くなってきましたし、夏用の髪形にしたということにすればよいのではありませんか？"},{"id":{"d":843,"n":8,"t":4},"speaker":"B","text":"そうですね。夏用ということで、明日からもしばらくこの髪型にしようと思います。"}];
INSERT INTO sentence [{"id":{"d":844,"n":1,"t":4},"speaker":"A","text":"帽子をかぶっているなんて珍しいですね。"},{"id":{"d":844,"n":2,"t":4},"speaker":"B","text":"髪を染めようとしたら染料が合わなかったらしくて、皮膚がかぶれてしまったのです。"},{"id":{"d":844,"n":3,"t":4},"speaker":"A","text":"かぶれたのは頭皮全体ですか？"},{"id":{"d":844,"n":4,"t":4},"speaker":"B","text":"そうです。頭皮だけならあまり目立たなかったのですけれど、おでこがひどいのです。"},{"id":{"d":844,"n":5,"t":4},"speaker":"A","text":"美容院で染めたのですか？"},{"id":{"d":844,"n":6,"t":4},"speaker":"B","text":"美容師さんならそのようなミスはしないと思います。自分でやったのが間違いでした。"},{"id":{"d":844,"n":7,"t":4},"speaker":"A","text":"なるほど。それで、髪は染められたのですか？"},{"id":{"d":844,"n":8,"t":4},"speaker":"B","text":"いいえ。途中でかゆくなったことに焦って中断してしまったので、中途半端なまだら模様になっています。"},{"id":{"d":844,"n":9,"t":4},"speaker":"A","text":"それは災難でしたね。"}];
INSERT INTO sentence [{"id":{"d":845,"n":1,"t":4},"speaker":"A","text":"体中がかゆくてたまりません。"},{"id":{"d":845,"n":2,"t":4},"speaker":"B","text":"あなたは乾燥肌なのですか？"},{"id":{"d":845,"n":3,"t":4},"speaker":"A","text":"どちらかと言えばそうかもしれません。"},{"id":{"d":845,"n":4,"t":4},"speaker":"B","text":"病院には行きましたか？"},{"id":{"d":845,"n":5,"t":4},"speaker":"A","text":"はい。保湿クリームなどをいろいろともらっているのですが、なかなか治りません。"},{"id":{"d":845,"n":6,"t":4},"speaker":"B","text":"今もそう言いながらも肌をかいてしまっていますよ。悪化してしまいますよ。"},{"id":{"d":845,"n":7,"t":4},"speaker":"A","text":"分かってはいるのですが、つい無意識に触ってしまうのです。"},{"id":{"d":845,"n":8,"t":4},"speaker":"B","text":"それなら手袋を着けておくといいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":846,"n":1,"t":4},"speaker":"A","text":"休み明けは職場にお土産のお菓子があふれますね。"},{"id":{"d":846,"n":2,"t":4},"speaker":"B","text":"休暇期間はダイエットしていたのに、お菓子でリバウンドしそうです。"},{"id":{"d":846,"n":3,"t":4},"speaker":"A","text":"給湯室に置いてあるだけなら選んで食べることもできますが、直接頂くものもありますしね。"},{"id":{"d":846,"n":4,"t":4},"speaker":"B","text":"わざわざ私のためにお土産を買ってきてくれたことを思うと、お断りするのも申し訳ありません。"},{"id":{"d":846,"n":5,"t":4},"speaker":"A","text":"持って帰ってご家族に食べてもらうのはどうですか？"},{"id":{"d":846,"n":6,"t":4},"speaker":"B","text":"私は1人暮らしですから、あげる人がいません。"},{"id":{"d":846,"n":7,"t":4},"speaker":"A","text":"それは悩ましい問題ですね。"},{"id":{"d":846,"n":8,"t":4},"speaker":"B","text":"マンションの大家さんなら喜んでくれるかもしれません。今度持っていこうと思います。"}];
INSERT INTO sentence [{"id":{"d":847,"n":1,"t":4},"speaker":"A","text":"瞳が青くなっていませんか？"},{"id":{"d":847,"n":2,"t":4},"speaker":"B","text":"今日は、カラーコンタクトレンズを入れているのです。"},{"id":{"d":847,"n":3,"t":4},"speaker":"A","text":"そういうおしゃれを始めたのですか？"},{"id":{"d":847,"n":4,"t":4},"speaker":"B","text":"今度コスチューム・プレイで撮影会をするので、その練習で入れているのです。"},{"id":{"d":847,"n":5,"t":4},"speaker":"A","text":"コンタクトレンズは初めてなのですか？"},{"id":{"d":847,"n":6,"t":4},"speaker":"B","text":"はい、初めてです。視力は良いので普段はコンタクトレンズをつけていません。"},{"id":{"d":847,"n":7,"t":4},"speaker":"A","text":"コスチューム・プレイのためなら、慣れないコンタクトレンズもするのですね。"},{"id":{"d":847,"n":8,"t":4},"speaker":"B","text":"はい。本番までには慣れておきたいので、しばらくはコンタクトレンズを入れて生活するつもりです。"}];
INSERT INTO sentence [{"id":{"d":848,"n":1,"t":4},"speaker":"A","text":"口唇ヘルペスがなかなか治りません。"},{"id":{"d":848,"n":2,"t":4},"speaker":"B","text":"このところはずっと薬を飲んでいますものね。"},{"id":{"d":848,"n":3,"t":4},"speaker":"A","text":"薬を飲んでも、薬を付けても、なかなか良くならないのです。"},{"id":{"d":848,"n":4,"t":4},"speaker":"B","text":"口唇ヘルぺスは体が弱っているときやストレスがあるときに出やすいと聞いたことがありますよ。"},{"id":{"d":848,"n":5,"t":4},"speaker":"A","text":"確かにストレスはずっとありますし、疲れもなかなか取れません。"},{"id":{"d":848,"n":6,"t":4},"speaker":"B","text":"休暇を取るのが一番の治療法のような気がします。"},{"id":{"d":848,"n":7,"t":4},"speaker":"A","text":"そうかもしれませんが、熱があるわけではないので、仕事はなかなか休めません。"},{"id":{"d":848,"n":8,"t":4},"speaker":"B","text":"事情は分かりますが、無理を続けていたら他の病気になりますよ。"}];
INSERT INTO sentence [{"id":{"d":849,"n":1,"t":4},"speaker":"A","text":"翔君はすっかり元気になりましたね。"},{"id":{"d":849,"n":2,"t":4},"speaker":"B","text":"はい。アトピー性皮膚炎の症状はだいぶ落ち着いてきました。"},{"id":{"d":849,"n":3,"t":4},"speaker":"A","text":"それは良かったですね。夏休みは楽しめましたか？"},{"id":{"d":849,"n":4,"t":4},"speaker":"B","text":"はい。夏休みは白浜のおばあちゃんの家で過ごしてきました。"},{"id":{"d":849,"n":5,"t":4},"speaker":"A","text":"白浜は海がきれいなところですね。"},{"id":{"d":849,"n":6,"t":4},"speaker":"B","text":"海でたくさん遊んできました。皮膚炎が落ち着いたのは、もしかしたら、海水浴のおかげかもしれません。"}];
INSERT INTO sentence [{"id":{"d":850,"n":1,"t":4},"speaker":"A","text":"おじいちゃん、これは何のトロフィーですか？"},{"id":{"d":850,"n":2,"t":4},"speaker":"B","text":"それは健康ボウリング大会で優勝した時のものですよ。"},{"id":{"d":850,"n":3,"t":4},"speaker":"A","text":"おじいちゃんが優勝したのですか？"},{"id":{"d":850,"n":4,"t":4},"speaker":"B","text":"そうです。"},{"id":{"d":850,"n":5,"t":4},"speaker":"A","text":"ボウリングが得意だなんて知りませんでした。"},{"id":{"d":850,"n":6,"t":4},"speaker":"B","text":"昔、ボウリングが流行した時に、ボウリング場に足しげく通って練習したのですよ。おかげで、かなり上手になりました。"},{"id":{"d":850,"n":7,"t":4},"speaker":"A","text":"昔取った杵柄というものですね。"},{"id":{"d":850,"n":8,"t":4},"speaker":"B","text":"実は、今でもたまにボウリングをしています。継続は力なりですよ。"}];
INSERT INTO sentence [{"id":{"d":851,"n":1,"t":4},"speaker":"A","text":"今日はいつもより少し長い距離を走りました。"},{"id":{"d":851,"n":2,"t":4},"speaker":"B","text":"まだジョギングが続いていたのですね。"},{"id":{"d":851,"n":3,"t":4},"speaker":"A","text":"足が痛くなったため少しジョギングを休んでいたことは、話していたでしょうか？"},{"id":{"d":851,"n":4,"t":4},"speaker":"B","text":"はい、聞いていました。お休みして、そのままやめてしまったのかと思っていました。"},{"id":{"d":851,"n":5,"t":4},"speaker":"A","text":"10日くらい休んでしまいましたが、再開しました。"},{"id":{"d":851,"n":6,"t":4},"speaker":"B","text":"もう足は痛くないのですか？"},{"id":{"d":851,"n":7,"t":4},"speaker":"A","text":"はい、痛くありません。走る前にラジオ体操をするようにしたら、調子が良くなりました。"},{"id":{"d":851,"n":8,"t":4},"speaker":"B","text":"準備運動は大切なのですね。"}];
INSERT INTO sentence [{"id":{"d":852,"n":1,"t":4},"speaker":"A","text":"小学生に卓球で負けてしまいました。"},{"id":{"d":852,"n":2,"t":4},"speaker":"B","text":"調子が悪かったのですか？"},{"id":{"d":852,"n":3,"t":4},"speaker":"A","text":"相手がとても強い子だったのです。地方大会では小学2年生以下の部で準優勝の経験があるそうです。"},{"id":{"d":852,"n":4,"t":4},"speaker":"B","text":"それは相当に強い子ですね。"},{"id":{"d":852,"n":5,"t":4},"speaker":"A","text":"次の大会での優勝を目指して、大人を相手に特訓するつもりだったようです。"},{"id":{"d":852,"n":6,"t":4},"speaker":"B","text":"それは仕方ありませんね。真剣勝負の世界には年齢は関係ないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":853,"n":1,"t":4},"speaker":"A","text":"こんにちは。久しぶりではありませんか？"},{"id":{"d":853,"n":2,"t":4},"speaker":"B","text":"お久しぶりです。1カ月ほど入院していましたので、お会いできませんでした。"},{"id":{"d":853,"n":3,"t":4},"speaker":"A","text":"1カ月もですか！存じ上げなくて、お見舞いにも行かず申し訳ありません。"},{"id":{"d":853,"n":4,"t":4},"speaker":"B","text":"誰にも言っていませんでしたから、当然ですよ。"},{"id":{"d":853,"n":5,"t":4},"speaker":"A","text":"どこが悪かったのですか？"},{"id":{"d":853,"n":6,"t":4},"speaker":"B","text":"転んで、足を骨折してしまいました。"},{"id":{"d":853,"n":7,"t":4},"speaker":"A","text":"それは大変でしたね。もう大丈夫なのですか？"},{"id":{"d":853,"n":8,"t":4},"speaker":"B","text":"はい。リハビリをして退院しましたから、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":854,"n":1,"t":4},"speaker":"A","text":"リモートで仕事をしていると、全く日に当たらないまま一日が過ぎてしまいます。"},{"id":{"d":854,"n":2,"t":4},"speaker":"B","text":"それは私も同じです。"},{"id":{"d":854,"n":3,"t":4},"speaker":"A","text":"今日は休みですので、日光浴を兼ねて公園に散歩に行くつもりです。"},{"id":{"d":854,"n":4,"t":4},"speaker":"B","text":"いいですね。ところで、たくさん荷物を持っていらっしゃいますね。"},{"id":{"d":854,"n":5,"t":4},"speaker":"A","text":"はい。敷物、お弁当、水筒、お菓子、それに本とクッションも持っています。"},{"id":{"d":854,"n":6,"t":4},"speaker":"B","text":"それらは全て公園で使うのですか？"},{"id":{"d":854,"n":7,"t":4},"speaker":"A","text":"そのつもりで持ってきました。"},{"id":{"d":854,"n":8,"t":4},"speaker":"B","text":"公園がリビングになりそうですね。"},{"id":{"d":854,"n":9,"t":4},"speaker":"A","text":"はい。心地よく一日を過ごしてくるつもりです。"},{"id":{"d":854,"n":10,"t":4},"speaker":"B","text":"楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":855,"n":1,"t":4},"speaker":"A","text":"昨日は入所者のおじいさんが食べ物をのどに詰まらせて大変でした。"},{"id":{"d":855,"n":2,"t":4},"speaker":"B","text":"詰まったものはすぐに取れたのですか？"},{"id":{"d":855,"n":3,"t":4},"speaker":"A","text":"はい。ちょうど先日、職員全員で応急処置を学んだところでしたので、無事に取ることができました。"},{"id":{"d":855,"n":4,"t":4},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":855,"n":5,"t":4},"speaker":"A","text":"ただ、私の力では腹部突き上げ法はできなかったので、他の職員の手を借りました。"},{"id":{"d":855,"n":6,"t":4},"speaker":"B","text":"その方法は、力が必要なのですか？"},{"id":{"d":855,"n":7,"t":4},"speaker":"A","text":"はい、体を持ち上げなければなりませんので力が要ります。"},{"id":{"d":855,"n":8,"t":4},"speaker":"B","text":"力のある方がいて良かったですね。"}];
INSERT INTO sentence [{"id":{"d":856,"n":1,"t":4},"speaker":"A","text":"今日、知り合いのお母さんから良いことを教わりました。"},{"id":{"d":856,"n":2,"t":4},"speaker":"B","text":"何を教えてもらったのですか？"},{"id":{"d":856,"n":3,"t":4},"speaker":"A","text":"今日、理久が公園で鼻血を出してしまったのです。すぐに上を向かせて鼻にティッシュペーパーを詰めようとしたら、その処置ではない方がいいと言われて、より良い方法を教えてもらいました。"},{"id":{"d":856,"n":4,"t":4},"speaker":"B","text":"そうなのですか？私も子どもの頃は、母が私の顔を上に向けてティッシュペーパーを詰めてくれましたよ。"},{"id":{"d":856,"n":5,"t":4},"speaker":"A","text":"ところが、今の正しい処置方法は、椅子に座って小鼻のところをぎゅっとつまんで下を向かせるのだそうです。その通りにしたらすぐに鼻血が止まりました。"},{"id":{"d":856,"n":6,"t":4},"speaker":"B","text":"そうなのですね。私も正しい処置方法を知ることができて良かったです。"}];
INSERT INTO sentence [{"id":{"d":857,"n":1,"t":4},"speaker":"A","text":"家永です。よろしくお願いします。"},{"id":{"d":857,"n":2,"t":4},"speaker":"B","text":"家永さんですね。お待ちしておりました。レントゲン写真を撮る前に金属が付いているものを外して、検査着に着替えてください。"},{"id":{"d":857,"n":3,"t":4},"speaker":"A","text":"検査着はサイズを選べますか？"},{"id":{"d":857,"n":4,"t":4},"speaker":"B","text":"はい。家永さんでしたらMサイズで大丈夫ですよ。Mサイズは、あちらの真ん中の棚にございます。"},{"id":{"d":857,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。着替え終わりました。"},{"id":{"d":857,"n":6,"t":4},"speaker":"B","text":"それではこちらへどうぞ。息を吸って数秒間止めてください。"},{"id":{"d":857,"n":7,"t":4},"speaker":"A","text":"はい。"},{"id":{"d":857,"n":8,"t":4},"speaker":"B","text":"これで終わりです。着替えて次の検査室に向かってください。"},{"id":{"d":857,"n":9,"t":4},"speaker":"A","text":"はい。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":858,"n":1,"t":4},"speaker":"A","text":"もしもし、先生、祖母が熱を出しているのですが診ていただけないでしょうか？"},{"id":{"d":858,"n":2,"t":4},"speaker":"B","text":"体温は測りましたか？"},{"id":{"d":858,"n":3,"t":4},"speaker":"A","text":"はい。38度3分です。"},{"id":{"d":858,"n":4,"t":4},"speaker":"B","text":"高いですね。以前に処方した解熱剤は飲ませましたか？"},{"id":{"d":858,"n":5,"t":4},"speaker":"A","text":"おかゆを少しだけ食べてくれましたので、その後に飲ませました。"},{"id":{"d":858,"n":6,"t":4},"speaker":"B","text":"分かりました。18時ごろになってしまうのですが、往診するようにします。"},{"id":{"d":858,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。お待ちしています。"},{"id":{"d":858,"n":8,"t":4},"speaker":"B","text":"それまで、頻繁におばあさんの様子を診るようにしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":859,"n":1,"t":4},"speaker":"A","text":"おばあちゃん、今日はこれで遊んで良いですか？"},{"id":{"d":859,"n":2,"t":4},"speaker":"B","text":"これは危ないから触ってはいけませんよ。"},{"id":{"d":859,"n":3,"t":4},"speaker":"A","text":"これはおもちゃの注射セットではないのですか？"},{"id":{"d":859,"n":4,"t":4},"speaker":"B","text":"それは私のインスリン注射の道具です。おもちゃではありませんよ。"},{"id":{"d":859,"n":5,"t":4},"speaker":"A","text":"おばあちゃんは自分で注射をするのですか？"},{"id":{"d":859,"n":6,"t":4},"speaker":"B","text":"おばあちゃんは病気なので、この注射を自分ですることが必要なのです。"},{"id":{"d":859,"n":7,"t":4},"speaker":"A","text":"怖い病気なのですか？"},{"id":{"d":859,"n":8,"t":4},"speaker":"B","text":"この注射をすれば心配ありませんよ。"}];
INSERT INTO sentence [{"id":{"d":860,"n":1,"t":4},"speaker":"A","text":"腕をつっていますね。どうされたのですか？"},{"id":{"d":860,"n":2,"t":4},"speaker":"B","text":"お恥ずかしいですが、転んで腕を折ってしまいました。"},{"id":{"d":860,"n":3,"t":4},"speaker":"A","text":"それは大変ですね。"},{"id":{"d":860,"n":4,"t":4},"speaker":"B","text":"利き腕ではない左腕なのがせめてもの救いなのですが、お風呂などは不便で仕方がありません。"},{"id":{"d":860,"n":5,"t":4},"speaker":"A","text":"そうでしょうね。何かお手伝いできることがあればお手伝いしますよ。"},{"id":{"d":860,"n":6,"t":4},"speaker":"B","text":"今のところは何もありません、と言いたいのですが、新聞をひもでくくるのを手伝っていただけますか？"},{"id":{"d":860,"n":7,"t":4},"speaker":"A","text":"いいですよ。確かに、その腕では難しいですね。"},{"id":{"d":860,"n":8,"t":4},"speaker":"B","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":861,"n":1,"t":4},"speaker":"A","text":"先生、私は土曜日には退院できますか？"},{"id":{"d":861,"n":2,"t":4},"speaker":"B","text":"熱が出なければ大丈夫だと思いますよ。"},{"id":{"d":861,"n":3,"t":4},"speaker":"A","text":"家族に連絡をしてよいですか？"},{"id":{"d":861,"n":4,"t":4},"speaker":"B","text":"よいですよ。一応、土曜日に退院の予定であることをご家族の方にもお伝えください。"},{"id":{"d":861,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。家族に連絡しておきます。"},{"id":{"d":861,"n":6,"t":4},"speaker":"B","text":"何事もなければ、10時ごろに事務局の方から退院のための書類が運ばれてくると思います。"},{"id":{"d":861,"n":7,"t":4},"speaker":"A","text":"早く退院したかったのでうれしいです。"},{"id":{"d":861,"n":8,"t":4},"speaker":"B","text":"回復して良かったですね。でも喜び過ぎて熱を出したら、退院は延期ですよ。"}];
INSERT INTO sentence [{"id":{"d":862,"n":1,"t":4},"speaker":"A","text":"お相撲さんの背中に丸い痕があるのを見たことがありますが、あれは何ですか？"},{"id":{"d":862,"n":2,"t":4},"speaker":"B","text":"あれはカッピングという治療を受けた痕ですよ。"},{"id":{"d":862,"n":3,"t":4},"speaker":"A","text":"どういう治療なのですか？"},{"id":{"d":862,"n":4,"t":4},"speaker":"B","text":"ガラスのカップの中を熱して、肌を吸うのです。"},{"id":{"d":862,"n":5,"t":4},"speaker":"A","text":"それをすると何が良くなるのですか？"},{"id":{"d":862,"n":6,"t":4},"speaker":"B","text":"血行が良くなったり、痛みが軽くなったりするらしいですよ。"},{"id":{"d":862,"n":7,"t":4},"speaker":"A","text":"痕が付いているお相撲さんはそこが痛かったのですね。"},{"id":{"d":862,"n":8,"t":4},"speaker":"B","text":"私のひいきの力士はその治療のおかげか、今日は良く動けて勝ったようですよ。"}];
INSERT INTO sentence [{"id":{"d":863,"n":1,"t":4},"speaker":"A","text":"今日は病院に寄ってから帰ります。"},{"id":{"d":863,"n":2,"t":4},"speaker":"B","text":"具合が悪いのですか？"},{"id":{"d":863,"n":3,"t":4},"speaker":"A","text":"はい。会社の健康診断で血圧が高いと言われてしまいました。"},{"id":{"d":863,"n":4,"t":4},"speaker":"B","text":"そうなのですか。大変ですね。"},{"id":{"d":863,"n":5,"t":4},"speaker":"A","text":"普段は血圧など測りませんから気が付きませんでした。君も血圧を測ってみるといいですよ。"},{"id":{"d":863,"n":6,"t":4},"speaker":"B","text":"いつも行くスーパーの健康コーナーに血圧計があったと思うので、今度行った時にでも測ってみます。"},{"id":{"d":863,"n":7,"t":4},"speaker":"A","text":"ぜひそうしてください。では、そろそろ出かけますね。"},{"id":{"d":863,"n":8,"t":4},"speaker":"B","text":"はい、いってらっしゃい。血圧のために、塩分が控えめの夕食を用意しておきますね。"},{"id":{"d":863,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。行ってきます。"}];
INSERT INTO sentence [{"id":{"d":864,"n":1,"t":4},"speaker":"A","text":"先生、金田さんがしゃがんでしまいました。"},{"id":{"d":864,"n":2,"t":4},"speaker":"B","text":"顔が真っ青ですね。大丈夫でしょうか？"},{"id":{"d":864,"n":3,"t":4},"speaker":"A","text":"金田さんは、立てないと言っています。"},{"id":{"d":864,"n":4,"t":4},"speaker":"B","text":"貧血でしょうか。先生が保健室に連れていきます。"},{"id":{"d":864,"n":5,"t":4},"speaker":"A","text":"先生、私も一緒に連れて行ってください。"},{"id":{"d":864,"n":6,"t":4},"speaker":"B","text":"付き添いはしなくても大丈夫ですよ。藤田さんはこのまま校長先生のお話を聞いていてください。"},{"id":{"d":864,"n":7,"t":4},"speaker":"A","text":"私も体がふらふらするのです。"},{"id":{"d":864,"n":8,"t":4},"speaker":"B","text":"そうでしたか。では、歩けるようでしたら、一緒に保健室に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":865,"n":1,"t":4},"speaker":"A","text":"時々、体が痛くてたまらないのですが、病院に行った方がいいでしょうか？"},{"id":{"d":865,"n":2,"t":4},"speaker":"B","text":"一度は見てもらった方がいいと思いますよ。"},{"id":{"d":865,"n":3,"t":4},"speaker":"A","text":"最近は暴飲暴食が続いたので、そのせいではないかとも思います。"},{"id":{"d":865,"n":4,"t":4},"speaker":"B","text":"暴飲暴食による体の痛みですか？それは痛風のサインではありませんか？"},{"id":{"d":865,"n":5,"t":4},"speaker":"A","text":"そうかもしれないと何となく思っています。"},{"id":{"d":865,"n":6,"t":4},"speaker":"B","text":"早く病院に行った方がいいですよ。"},{"id":{"d":865,"n":7,"t":4},"speaker":"A","text":"明日の有給は取れるでしょうか？"},{"id":{"d":865,"n":8,"t":4},"speaker":"B","text":"有給の申請をするなら、課長のところに一緒に行ってあげますよ。"},{"id":{"d":865,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。心強いです。"}];
INSERT INTO sentence [{"id":{"d":866,"n":1,"t":4},"speaker":"A","text":"何か変な音が聞こえませんか？"},{"id":{"d":866,"n":2,"t":4},"speaker":"B","text":"何も聞こえませんよ。"},{"id":{"d":866,"n":3,"t":4},"speaker":"A","text":"耳鳴りでしょうか？キーンという音が聞こえます。"},{"id":{"d":866,"n":4,"t":4},"speaker":"B","text":"私には何も聞こえませんけれど、どうしたのでしょうね。"},{"id":{"d":866,"n":5,"t":4},"speaker":"A","text":"先輩に聞こえないとしたら、モスキート音というものなのかもしれません。"},{"id":{"d":866,"n":6,"t":4},"speaker":"B","text":"年を取ると聞こえなくなるという周波数のことですか？"},{"id":{"d":866,"n":7,"t":4},"speaker":"A","text":"そうです。あ、年寄りだと言っているわけではありませんよ。お気を悪くされたらすみません。"},{"id":{"d":866,"n":8,"t":4},"speaker":"B","text":"実際、私はあなたより一回り以上も年上ですからね。謝らなくていいですよ。"}];
INSERT INTO sentence [{"id":{"d":867,"n":1,"t":4},"speaker":"A","text":"亜紀ちゃん、このシャンプーをインターネットで買ってくれませんか？"},{"id":{"d":867,"n":2,"t":4},"speaker":"B","text":"いいですよ。これはインターネットだけで売っている商品なのですか？"},{"id":{"d":867,"n":3,"t":4},"speaker":"A","text":"そのようです。"},{"id":{"d":867,"n":4,"t":4},"speaker":"B","text":"この情報はどこで知ったのですか？お母さんはインターネットは使えませんよね？"},{"id":{"d":867,"n":5,"t":4},"speaker":"A","text":"この間、急に幸子叔母さんの家に泊まったでしょう？その時にお風呂にあったのを借りたのですが、それが良かったので商品名と購入先を書いてもらったのです。"},{"id":{"d":867,"n":6,"t":4},"speaker":"B","text":"そういうことでしたか。"},{"id":{"d":867,"n":7,"t":4},"speaker":"A","text":"商品名と購入先を書いてもらった紙がこれです。"},{"id":{"d":867,"n":8,"t":4},"speaker":"B","text":"ここにアクセスすればよいのですね。今から注文します。"}];
INSERT INTO sentence [{"id":{"d":868,"n":1,"t":4},"speaker":"A","text":"お待たせしてすみません。暑かったでしょう？"},{"id":{"d":868,"n":2,"t":4},"speaker":"B","text":"ここはそれほど暑くありませんでしたから大丈夫ですよ。"},{"id":{"d":868,"n":3,"t":4},"speaker":"A","text":"そういえば、この場所はミストシャワーが設置されていますね。"},{"id":{"d":868,"n":4,"t":4},"speaker":"B","text":"あそこから霧が出ていて、なんだか涼しかったですよ。"},{"id":{"d":868,"n":5,"t":4},"speaker":"A","text":"でも、お待たせしたのは申し訳ありませんでした。"},{"id":{"d":868,"n":6,"t":4},"speaker":"B","text":"お気になさらず。まだ、開場には時間がありますよ。"},{"id":{"d":868,"n":7,"t":4},"speaker":"A","text":"軽くお茶をしていく時間はありそうですね。冷たいものでもごちそうします。"},{"id":{"d":868,"n":8,"t":4},"speaker":"B","text":"それでは、あちらのタピオカミルクティーをお願いします。"}];
INSERT INTO sentence [{"id":{"d":869,"n":1,"t":4},"speaker":"A","text":"今日はいつもより髪がぼさぼさですね。"},{"id":{"d":869,"n":2,"t":4},"speaker":"B","text":"水泳部の朝練習の後に、整える時間がありませんでした。"},{"id":{"d":869,"n":3,"t":4},"speaker":"A","text":"タオルで拭いてそのまま来てしまった感じに見えます。"},{"id":{"d":869,"n":4,"t":4},"speaker":"B","text":"その通りです。プールに入るとただでさえも塩素で髪が傷むのに、手入れの時間がないとこの通りです。"},{"id":{"d":869,"n":5,"t":4},"speaker":"A","text":"いつもはお手入れをしているのですね。"},{"id":{"d":869,"n":6,"t":4},"speaker":"B","text":"手入れといってもシャンプーとトリートメントだけなのですけれどね。"},{"id":{"d":869,"n":7,"t":4},"speaker":"A","text":"塩素を洗い流すだけできれいになるのですね。"},{"id":{"d":869,"n":8,"t":4},"speaker":"B","text":"はい、かなり変わります。大会前はずっとシャンプーをする時間がなさそうなので、明日からはヘアピンをたくさん持ってきて留めようと思います。"}];
INSERT INTO sentence [{"id":{"d":870,"n":1,"t":4},"speaker":"A","text":"遅くなって申し訳ありません。"},{"id":{"d":870,"n":2,"t":4},"speaker":"B","text":"待っていましたよ。まずは、ビールで乾杯しましょう。先ほど頼んでおきました。"},{"id":{"d":870,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":870,"n":4,"t":4},"speaker":"B","text":"ビールを待つ間に、つまみも追加注文してくださいね。"},{"id":{"d":870,"n":5,"t":4},"speaker":"A","text":"では、焼きおにぎりを1つお願いします。"},{"id":{"d":870,"n":6,"t":4},"speaker":"B","text":"いきなり、おにぎりなのですか？"},{"id":{"d":870,"n":7,"t":4},"speaker":"A","text":"今、とても空腹なのです。すきっ腹に酒はいけませんよね？"},{"id":{"d":870,"n":8,"t":4},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":871,"n":1,"t":4},"speaker":"A","text":"今、くしゃみが3回出ましたか？"},{"id":{"d":871,"n":2,"t":4},"speaker":"B","text":"はい、うるさかったですか？すみません。"},{"id":{"d":871,"n":3,"t":4},"speaker":"A","text":"いいえ。少しうらやましかっただけです。"},{"id":{"d":871,"n":4,"t":4},"speaker":"B","text":"くしゃみがうらやましいのですか？"},{"id":{"d":871,"n":5,"t":4},"speaker":"A","text":"「くしゃみ3回はほれられる」と言うではありませんか。"},{"id":{"d":871,"n":6,"t":4},"speaker":"B","text":"初めて聞きました。誰かが私のことを好きだと、3回くしゃみが出るということですか？"},{"id":{"d":871,"n":7,"t":4},"speaker":"A","text":"そうですよ。うらやましいです。どこの誰でしょうね。"},{"id":{"d":871,"n":8,"t":4},"speaker":"B","text":"そういう人に心当たりはありませんね。"}];
INSERT INTO sentence [{"id":{"d":872,"n":1,"t":4},"speaker":"A","text":"このホテルの近くにはパターゴルフ場があるのですね。"},{"id":{"d":872,"n":2,"t":4},"speaker":"B","text":"はい。宿泊されるお客様でしたら、どなたでもお使いになれます。"},{"id":{"d":872,"n":3,"t":4},"speaker":"A","text":"子ども連れでも大丈夫ですか？"},{"id":{"d":872,"n":4,"t":4},"speaker":"B","text":"はい。4歳以上のお子様でしたら、ご利用いただけます。"},{"id":{"d":872,"n":5,"t":4},"speaker":"A","text":"明日の予約はできますか？大人2人と子ども2人です。"},{"id":{"d":872,"n":6,"t":4},"speaker":"B","text":"明日は10時からと11時からでしたら空いております。"},{"id":{"d":872,"n":7,"t":4},"speaker":"A","text":"では10時に予約をお願いします。"},{"id":{"d":872,"n":8,"t":4},"speaker":"B","text":"かしこまりました。お名前とお部屋番号をこちらにお願いします。"}];
INSERT INTO sentence [{"id":{"d":873,"n":1,"t":4},"speaker":"A","text":"バレーボールをお借りしてもよいですか？"},{"id":{"d":873,"n":2,"t":4},"speaker":"B","text":"構いませんが、美術部がなぜバレーボールを使うのですか？"},{"id":{"d":873,"n":3,"t":4},"speaker":"A","text":"ボールを絵に描こうと思っています。"},{"id":{"d":873,"n":4,"t":4},"speaker":"B","text":"そういうことでしたか。でも、なぜバレーボールを描くのですか？できれば部員が試合をしているところも描いてくれたらうれしいです。"},{"id":{"d":873,"n":5,"t":4},"speaker":"A","text":"造形を研究したいのです。最近のバレーボールは変わった形の革が縫い合わせてありますよね。"},{"id":{"d":873,"n":6,"t":4},"speaker":"B","text":"そう言われればそうですね。"},{"id":{"d":873,"n":7,"t":4},"speaker":"A","text":"部員の絵はまたの機会に描きますね。"},{"id":{"d":873,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":874,"n":1,"t":4},"speaker":"A","text":"砲丸投げの砲丸は、重いのですね。"},{"id":{"d":874,"n":2,"t":4},"speaker":"B","text":"ええ、重いですよ。選手が筋骨隆々なのも納得でしょう？"},{"id":{"d":874,"n":3,"t":4},"speaker":"A","text":"はい。私は持ち上げられませんでした。"},{"id":{"d":874,"n":4,"t":4},"speaker":"B","text":"女子用の砲丸も持てませんでしたか？"},{"id":{"d":874,"n":5,"t":4},"speaker":"A","text":"恐らく、持たせてもらったのは、男子用の砲丸だと思います。女子用のものは触ったことがありません。"},{"id":{"d":874,"n":6,"t":4},"speaker":"B","text":"男子用のものだったのなら、持ち上げられないのも納得ですね。"},{"id":{"d":874,"n":7,"t":4},"speaker":"A","text":"ボウリングの玉よりも重いように感じられました。"},{"id":{"d":874,"n":8,"t":4},"speaker":"B","text":"恐らく重さは同じくらいだと思いますが、玉が小さいから重さを感じやすいのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":875,"n":1,"t":4},"speaker":"A","text":"また救急車が来ましたね。"},{"id":{"d":875,"n":2,"t":4},"speaker":"B","text":"気になりますか？"},{"id":{"d":875,"n":3,"t":4},"speaker":"A","text":"はい。誰かが救急車で運ばれていると思うとドキッとします。"},{"id":{"d":875,"n":4,"t":4},"speaker":"B","text":"ここは総合病院の隣なので、一日中救急車が来ますよ。"},{"id":{"d":875,"n":5,"t":4},"speaker":"A","text":"住んでいて気になりませんか？"},{"id":{"d":875,"n":6,"t":4},"speaker":"B","text":"最初から病院があると分かっていてこの部屋を買ったのですし、気になりません。"},{"id":{"d":875,"n":7,"t":4},"speaker":"A","text":"そういうものなのですね。"},{"id":{"d":875,"n":8,"t":4},"speaker":"B","text":"せっかくランチに来てくださったのに、うるさくてすみません。"}];
INSERT INTO sentence [{"id":{"d":876,"n":1,"t":4},"speaker":"A","text":"大家さん、明日から1泊で検査入院をするのですが、郵便物を取っておいていただけますか？"},{"id":{"d":876,"n":2,"t":4},"speaker":"B","text":"検査入院ですか？どこか悪いのですか？"},{"id":{"d":876,"n":3,"t":4},"speaker":"A","text":"健康診断で少し心臓のデータが良くなかったのです。"},{"id":{"d":876,"n":4,"t":4},"speaker":"B","text":"心臓ですか。心配ですね。"},{"id":{"d":876,"n":5,"t":4},"speaker":"A","text":"カテーテル検査をして、悪いところがはっきりしたら治療しますから、大丈夫ですよ。"},{"id":{"d":876,"n":6,"t":4},"speaker":"B","text":"軽く済むことを祈っていますよ。"},{"id":{"d":876,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。では、郵便物をお願いします。"},{"id":{"d":876,"n":8,"t":4},"speaker":"B","text":"はい、お任せください。お大事にしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":877,"n":1,"t":4},"speaker":"A","text":"今年のボーナスは全部エステティックに注ぎ込みました。"},{"id":{"d":877,"n":2,"t":4},"speaker":"B","text":"道理で最近は、生き生きしていらっしゃると思いました。"},{"id":{"d":877,"n":3,"t":4},"speaker":"A","text":"リップサービスがうまいですね。"},{"id":{"d":877,"n":4,"t":4},"speaker":"B","text":"いえいえ、本当にそう思いますよ。"},{"id":{"d":877,"n":5,"t":4},"speaker":"A","text":"全身脱毛が主な目的なのですけれどね。"},{"id":{"d":877,"n":6,"t":4},"speaker":"B","text":"なぜ脱毛しようと思ったのですか？"},{"id":{"d":877,"n":7,"t":4},"speaker":"A","text":"水着を着るときに備えようと思ったのです。"},{"id":{"d":877,"n":8,"t":4},"speaker":"B","text":"もうすぐ夏休みですものね。"}];
INSERT INTO sentence [{"id":{"d":878,"n":1,"t":4},"speaker":"A","text":"あら、お出かけですか？しばらくお仕事は休みだと言っていませんでしたか？"},{"id":{"d":878,"n":2,"t":4},"speaker":"B","text":"はい。美容室が改装中で1カ月も休みになるのですが、何もしないと腕が鈍るでしょう？"},{"id":{"d":878,"n":3,"t":4},"speaker":"A","text":"どこかでアルバイトをするのですか？"},{"id":{"d":878,"n":4,"t":4},"speaker":"B","text":"アルバイトではなくて、ボランティアをしようと思っています。"},{"id":{"d":878,"n":5,"t":4},"speaker":"A","text":"どこでボランティアをするのですか？"},{"id":{"d":878,"n":6,"t":4},"speaker":"B","text":"老人ホームです。ご老人の髪をきれいにしてあげると喜ばれるのです。"},{"id":{"d":878,"n":7,"t":4},"speaker":"A","text":"それはとてもすてきな活動ですね。"},{"id":{"d":878,"n":8,"t":4},"speaker":"B","text":"はい。やりがいがあって楽しいです。美容室が再開しても、お休みの日にボランティアを続けようかと思っています。"}];
INSERT INTO sentence [{"id":{"d":879,"n":1,"t":4},"speaker":"A","text":"すみません、何か落とされましたよ。"},{"id":{"d":879,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。これは、恥ずかしいものを落としてしまいました。"},{"id":{"d":879,"n":3,"t":4},"speaker":"A","text":"これは湿布薬ですか？"},{"id":{"d":879,"n":4,"t":4},"speaker":"B","text":"はい。膝が痛くて貼っていたのですが、歩いていたら剝がれてしまったようです。"},{"id":{"d":879,"n":5,"t":4},"speaker":"A","text":"サポーターをしていなかったのですか？"},{"id":{"d":879,"n":6,"t":4},"speaker":"B","text":"暑かったのでサポーターはしていませんでした。"},{"id":{"d":879,"n":7,"t":4},"speaker":"A","text":"夏場は汗で湿布薬も落ちやすいですから、ネット包帯などをするといいですよ。"},{"id":{"d":879,"n":8,"t":4},"speaker":"B","text":"ネット包帯ですか。その手がありましたね。"}];
INSERT INTO sentence [{"id":{"d":880,"n":1,"t":4},"speaker":"A","text":"大滝さん、この冬はスノーボードをしに新潟へ行きませんか？"},{"id":{"d":880,"n":2,"t":4},"speaker":"B","text":"スノーボードですか？どうして私を誘うのですか？"},{"id":{"d":880,"n":3,"t":4},"speaker":"A","text":"大滝さんはスケートボードをやっているでしょう？"},{"id":{"d":880,"n":4,"t":4},"speaker":"B","text":"隠れた趣味にしていたのに、ご存じだったのですね。"},{"id":{"d":880,"n":5,"t":4},"speaker":"A","text":"偶然ですが、スケートボードパークで大滝さんの格好良い姿をお見かけしたのです。"},{"id":{"d":880,"n":6,"t":4},"speaker":"B","text":"そうでしたか。でも、スノーボードはやったことがありませんよ。"},{"id":{"d":880,"n":7,"t":4},"speaker":"A","text":"スケートボードであれだけできるのですから、あなたにとってはきっと簡単ですよ。"},{"id":{"d":880,"n":8,"t":4},"speaker":"B","text":"そうでしょうか。少し考えてみます。"}];
INSERT INTO sentence [{"id":{"d":881,"n":1,"t":4},"speaker":"A","text":"すみません、お待たせしましたか？"},{"id":{"d":881,"n":2,"t":4},"speaker":"B","text":"今、来たところです。"},{"id":{"d":881,"n":3,"t":4},"speaker":"A","text":"それなら良かったです。ところで、今日は日焼け対策を万全にしていますね。"},{"id":{"d":881,"n":4,"t":4},"speaker":"B","text":"今日のようによく晴れた夏の日は、日傘とアームカバーとサングラス、そしてロングスカートが必須アイテムです。"},{"id":{"d":881,"n":5,"t":4},"speaker":"A","text":"本気ですね。私は、アームカバーは暑くて着けていられません。"},{"id":{"d":881,"n":6,"t":4},"speaker":"B","text":"UVカット素材のものだと割に涼しいですよ。"},{"id":{"d":881,"n":7,"t":4},"speaker":"A","text":"そうなのですか？私はいつも日焼け止めだけです。"},{"id":{"d":881,"n":8,"t":4},"speaker":"B","text":"日焼け止めは私も全身に使っていますよ。"}];
INSERT INTO sentence [{"id":{"d":882,"n":1,"t":4},"speaker":"A","text":"マラソン大会で棄権してしまいました。"},{"id":{"d":882,"n":2,"t":4},"speaker":"B","text":"あれほどトレーニングしていたのに残念でしたね。"},{"id":{"d":882,"n":3,"t":4},"speaker":"A","text":"走っている最中に脇腹が痛くなって、前に進めなくなってしまったのです。"},{"id":{"d":882,"n":4,"t":4},"speaker":"B","text":"マラソンランナーが時々なる症状ですね。"},{"id":{"d":882,"n":5,"t":4},"speaker":"A","text":"体の鍛え方が足りなかったのでしょうか？"},{"id":{"d":882,"n":6,"t":4},"speaker":"B","text":"そのようなことはありませんよ。いつもなってしまうようでしたら病気を疑った方がいいと思いますが、この間だけでしょう？"},{"id":{"d":882,"n":7,"t":4},"speaker":"A","text":"そうです。"},{"id":{"d":882,"n":8,"t":4},"speaker":"B","text":"では、次に向けてトレーニングを続ければ大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":883,"n":1,"t":4},"speaker":"A","text":"どうしましたか？"},{"id":{"d":883,"n":2,"t":4},"speaker":"B","text":"徒競走で転んで、膝を擦りむいてしまいました。"},{"id":{"d":883,"n":3,"t":4},"speaker":"A","text":"膝から血が出ていますね。消毒しますね。"},{"id":{"d":883,"n":4,"t":4},"speaker":"B","text":"先生、手も痛いです。"},{"id":{"d":883,"n":5,"t":4},"speaker":"A","text":"指は動かせますか？開いたり閉じたりしてみてください。"},{"id":{"d":883,"n":6,"t":4},"speaker":"B","text":"痛くて動かせません。"},{"id":{"d":883,"n":7,"t":4},"speaker":"A","text":"どこかを骨折しているかもしれませんね。すぐに病院に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":884,"n":1,"t":4},"speaker":"A","text":"市の武道館で面白そうな講座があるのですが、一緒に参加してみませんか？"},{"id":{"d":884,"n":2,"t":4},"speaker":"B","text":"何の講座ですか？"},{"id":{"d":884,"n":3,"t":4},"speaker":"A","text":"スポーツチャンバラを体験する講座です。スポーツチャンバラを知っていますか？"},{"id":{"d":884,"n":4,"t":4},"speaker":"B","text":"ちゃんばらは知っていますよ。でも、スポーツチャンバラは知りません。"},{"id":{"d":884,"n":5,"t":4},"speaker":"A","text":"決められたコートの中で、まさにちゃんばらをする競技です。"},{"id":{"d":884,"n":6,"t":4},"speaker":"B","text":"それは面白そうですね。その講座には誰でも参加できるのですか？"},{"id":{"d":884,"n":7,"t":4},"speaker":"A","text":"はい。子どもから大人まで、誰でも参加してよいそうです。"}];
INSERT INTO sentence [{"id":{"d":885,"n":1,"t":4},"speaker":"A","text":"先輩、ラグビーのルールは分かりますか？"},{"id":{"d":885,"n":2,"t":4},"speaker":"B","text":"あまり詳しくは分かりませんが、あの線の向こうにボールを置いたら得点が入ります。"},{"id":{"d":885,"n":3,"t":4},"speaker":"A","text":"他にはどのようなときに点が入るのですか？"},{"id":{"d":885,"n":4,"t":4},"speaker":"B","text":"あそこの棒の上をボールが通っても得点が入ります。"},{"id":{"d":885,"n":5,"t":4},"speaker":"A","text":"なるほど。何となく理解しました。"},{"id":{"d":885,"n":6,"t":4},"speaker":"B","text":"ラグビーのルールは一度きちんと勉強したいと思っていたのですが、結局、それをする前にワールドカップが始まってしまいました。"},{"id":{"d":885,"n":7,"t":4},"speaker":"A","text":"ルールを知らなくても十分盛り上がれますが、ルールを知ればより観戦を楽しめそうですよね。私も近いうちにきちんと勉強してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":886,"n":1,"t":4},"speaker":"A","text":"ボクシングを観戦していて、パンチが当たるたびに「痛そう」と言うのはやめてくれませんか？"},{"id":{"d":886,"n":2,"t":4},"speaker":"B","text":"本当に痛そうで、つい、口から出てしまうのです。"},{"id":{"d":886,"n":3,"t":4},"speaker":"A","text":"ボクシングは殴り合うスポーツなのですから、パンチが当たるのは当然です。隣でそのようなことを言われたら、集中して見ていられません。"},{"id":{"d":886,"n":4,"t":4},"speaker":"B","text":"分かりました。では、黙って応援します。"},{"id":{"d":886,"n":5,"t":4},"speaker":"A","text":"でも、静かすぎるのも気味が悪いです。"},{"id":{"d":886,"n":6,"t":4},"speaker":"B","text":"勝手ですね。どのように応援したらよいのか分かりませんよ。"}];
INSERT INTO sentence [{"id":{"d":887,"n":1,"t":4},"speaker":"A","text":"健康診断の結果票が届きました。基準値から外れた赤字の数値がなくなりましたよ。"},{"id":{"d":887,"n":2,"t":4},"speaker":"B","text":"本当ですね。中性脂肪と血圧も、去年の数値からかなり改善されているようです。"},{"id":{"d":887,"n":3,"t":4},"speaker":"A","text":"ダイエットのおかげでしょうか？確かに、去年より体が軽いです。"},{"id":{"d":887,"n":4,"t":4},"speaker":"B","text":"それは良いことです。私の食事管理もお役に立てたでしょうか？"},{"id":{"d":887,"n":5,"t":4},"speaker":"A","text":"もちろん、君の功績は素晴らしいですよ。"},{"id":{"d":887,"n":6,"t":4},"speaker":"B","text":"まだまだ続けますよ。健康で長生きしてください。"}];
INSERT INTO sentence [{"id":{"d":888,"n":1,"t":4},"speaker":"A","text":"誕生日に、娘から犬をプレゼントされました。"},{"id":{"d":888,"n":2,"t":4},"speaker":"B","text":"かわいいワンちゃんですね。"},{"id":{"d":888,"n":3,"t":4},"speaker":"A","text":"ええ。まだしつけもできていませんが、かわいくて仕方がありません。"},{"id":{"d":888,"n":4,"t":4},"speaker":"B","text":"もうワンちゃん一緒にお散歩もしているのですか？"},{"id":{"d":888,"n":5,"t":4},"speaker":"A","text":"はい、最近の私の日課です。"},{"id":{"d":888,"n":6,"t":4},"speaker":"B","text":"毎日のお散歩は健康にも良さそうですね。"},{"id":{"d":888,"n":7,"t":4},"speaker":"A","text":"おかげで、毎日楽しく散歩ができています。犬が来る前は一人でウォーキングをしていたのですが、一人ではなかなか続かなくて休みの日が増えてきたところに犬が来ました。"},{"id":{"d":888,"n":8,"t":4},"speaker":"B","text":"ワンちゃんが、途切れそうになっていたウォーキングを復活させてくれたのですね。"}];
INSERT INTO sentence [{"id":{"d":889,"n":1,"t":4},"speaker":"A","text":"目が充血していますよ。"},{"id":{"d":889,"n":2,"t":4},"speaker":"B","text":"寝不足なのです。"},{"id":{"d":889,"n":3,"t":4},"speaker":"A","text":"何か眠れない原因があるのですか？"},{"id":{"d":889,"n":4,"t":4},"speaker":"B","text":"屋根裏から物音が聞こえるのです。"},{"id":{"d":889,"n":5,"t":4},"speaker":"A","text":"何か動物がすみ着いてしまったのでしょうか？"},{"id":{"d":889,"n":6,"t":4},"speaker":"B","text":"そうかもしれません。このところは、ずっと気になって仕方ありません。"},{"id":{"d":889,"n":7,"t":4},"speaker":"A","text":"寝不足解消のためにも業者に見てもらった方がいいですよ。"},{"id":{"d":889,"n":8,"t":4},"speaker":"B","text":"確かに、その手がありましたね。もし、良い業者を知っていたら教えてください。"}];
INSERT INTO sentence [{"id":{"d":890,"n":1,"t":4},"speaker":"A","text":"視力が落ちていますね。"},{"id":{"d":890,"n":2,"t":4},"speaker":"B","text":"やはりそうですか。スマートフォンとゲームのやり過ぎだということは自覚しています。"},{"id":{"d":890,"n":3,"t":4},"speaker":"A","text":"適度に休みながらできませんか？"},{"id":{"d":890,"n":4,"t":4},"speaker":"B","text":"つい、夢中になってしまって休めないのです。"},{"id":{"d":890,"n":5,"t":4},"speaker":"A","text":"スマートフォンの依存症も考えられますから、その時は専門医にかかった方がいいですよ。"},{"id":{"d":890,"n":6,"t":4},"speaker":"B","text":"はい、覚えておきます。"},{"id":{"d":890,"n":7,"t":4},"speaker":"A","text":"かなり目が悪くなっているので、眼鏡やコンタクトレンズも度数を変えた方がいいでしょう。"},{"id":{"d":890,"n":8,"t":4},"speaker":"B","text":"分かりました。近いうちに、新しい眼鏡を作りに眼鏡屋さんに行ってきます。"}];
INSERT INTO sentence [{"id":{"d":891,"n":1,"t":4},"speaker":"A","text":"久しぶりに腹筋運動をしようとしたら1回もできませんでした。"},{"id":{"d":891,"n":2,"t":4},"speaker":"B","text":"最近は何も運動をしていなかったのですか？"},{"id":{"d":891,"n":3,"t":4},"speaker":"A","text":"はい。仕事が忙しくて全く何もしていませんでした。"},{"id":{"d":891,"n":4,"t":4},"speaker":"B","text":"腹筋運動くらいはする暇がありませんでしたか？"},{"id":{"d":891,"n":5,"t":4},"speaker":"A","text":"はい、それすらできませんでした。気付いたら筋肉がすっかり落ちてしまい、どうにもならなくて困っています。"},{"id":{"d":891,"n":6,"t":4},"speaker":"B","text":"ジムに行ってみたらいかがですか？"},{"id":{"d":891,"n":7,"t":4},"speaker":"A","text":"ジムに行く時間もなかなか取れません。今は娘に引っ張ってもらって腹筋運動を頑張っています。"},{"id":{"d":891,"n":8,"t":4},"speaker":"B","text":"落ちた筋肉が、早く元に戻ると良いですね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":892,"n":1,"t":4},"speaker":"A","text":"娘のクラスがインフルエンザで学級閉鎖になりました。"},{"id":{"d":892,"n":2,"t":4},"speaker":"B","text":"娘さんは大丈夫なのですか？"},{"id":{"d":892,"n":3,"t":4},"speaker":"A","text":"娘は元気いっぱいです。でも、一人にはしておけませんので、今日はお休みします。"},{"id":{"d":892,"n":4,"t":4},"speaker":"B","text":"分かりました。今日はそれほど忙しくありませんから、大丈夫ですよ。"},{"id":{"d":892,"n":5,"t":4},"speaker":"A","text":"明日以降は母に留守番をお願いしますから、仕事に行けると思います。"},{"id":{"d":892,"n":6,"t":4},"speaker":"B","text":"無理はしないでくださいね。"},{"id":{"d":892,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。でも元気過ぎる娘と一日いたら、こちらが疲れてしまうかもしれません。"},{"id":{"d":892,"n":8,"t":4},"speaker":"B","text":"疲れない程度に相手をしてあげたら娘さんも喜びますよ。"}];
INSERT INTO sentence [{"id":{"d":893,"n":1,"t":4},"speaker":"A","text":"少しドラッグストアに寄りますね。"},{"id":{"d":893,"n":2,"t":4},"speaker":"B","text":"私も買うものがありますので、一緒に行きます。"},{"id":{"d":893,"n":3,"t":4},"speaker":"A","text":"もしかして、いつも食べているあのチョコレートですか？"},{"id":{"d":893,"n":4,"t":4},"speaker":"B","text":"そうです。砂糖ゼロのチョコレートはここのドラッグストアが安いのです。"},{"id":{"d":893,"n":5,"t":4},"speaker":"A","text":"私はベビーパウダーを買うので、あちらの通路ですね。"},{"id":{"d":893,"n":6,"t":4},"speaker":"B","text":"ベビーパウダーですか？何に使っているのですか？"},{"id":{"d":893,"n":7,"t":4},"speaker":"A","text":"あせもの予防のために、お風呂上がりにポンポンと付けるのです。"},{"id":{"d":893,"n":8,"t":4},"speaker":"B","text":"丁寧にお手入れをされているのですね。だからきっとお肌がきれいなのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":894,"n":1,"t":4},"speaker":"A","text":"すみません、このヘアスプレーはありますか？"},{"id":{"d":894,"n":2,"t":4},"speaker":"B","text":"申し訳ありませんが、在庫切れです。代わりにこちらの商品はいかがですか？"},{"id":{"d":894,"n":3,"t":4},"speaker":"A","text":"他の物でも良いですが、これが気に入っているのです。在庫切れとは残念です。"},{"id":{"d":894,"n":4,"t":4},"speaker":"B","text":"申し訳ありません。最近はなぜか、急にこればかりが売れるのです。"},{"id":{"d":894,"n":5,"t":4},"speaker":"A","text":"テレビや雑誌で特集でもしたのでしょうか？"},{"id":{"d":894,"n":6,"t":4},"speaker":"B","text":"分かりません。お客様は何かを見ていらしたのではないのですか？"},{"id":{"d":894,"n":7,"t":4},"speaker":"A","text":"私は数年前からこれを愛用しています。うちの息子が、私のものを勝手に使って、なくなってしまったので買いに来たのです。"}];
INSERT INTO sentence [{"id":{"d":895,"n":1,"t":4},"speaker":"A","text":"美容院でパーマを当ててきたのですか？"},{"id":{"d":895,"n":2,"t":4},"speaker":"B","text":"いいえ。おばあちゃん、私のこれは天然パーマというものです。"},{"id":{"d":895,"n":3,"t":4},"speaker":"A","text":"天然パーマというのは、癖毛のことですか？"},{"id":{"d":895,"n":4,"t":4},"speaker":"B","text":"そうです。"},{"id":{"d":895,"n":5,"t":4},"speaker":"A","text":"今まで、貴子の髪は、真っすぐだったのではありませんか？"},{"id":{"d":895,"n":6,"t":4},"speaker":"B","text":"高校生の時はストレートパーマをかけていたのです。"},{"id":{"d":895,"n":7,"t":4},"speaker":"A","text":"パーマは校則で禁止のはずではありませんか？"},{"id":{"d":895,"n":8,"t":4},"speaker":"B","text":"直毛にするパーマはなぜか叱られなかったのですよ。不思議な校則ですよね。"}];
INSERT INTO sentence [{"id":{"d":896,"n":1,"t":4},"speaker":"A","text":"これは大きなボールですね。"},{"id":{"d":896,"n":2,"t":4},"speaker":"B","text":"バランスボールというのです。"},{"id":{"d":896,"n":3,"t":4},"speaker":"A","text":"どのようにして使うのですか？"},{"id":{"d":896,"n":4,"t":4},"speaker":"B","text":"上に座って体幹を鍛えたり、寝そべって筋力をアップさせたりします。"},{"id":{"d":896,"n":5,"t":4},"speaker":"A","text":"いろいろと使えるのですね。"},{"id":{"d":896,"n":6,"t":4},"speaker":"B","text":"軽い運動にはとても良い道具ですよ。"},{"id":{"d":896,"n":7,"t":4},"speaker":"A","text":"場所を取らなければよいのですが、うちは狭いので置けませんね。"},{"id":{"d":896,"n":8,"t":4},"speaker":"B","text":"大丈夫ですよ。椅子の代わりに使っているオフィスもありますよ。"}];
INSERT INTO sentence [{"id":{"d":897,"n":1,"t":4},"speaker":"A","text":"今日はみんなでフリフリ運動をしましょう。"},{"id":{"d":897,"n":2,"t":4},"speaker":"B","text":"フリフリ運動とは何ですか？"},{"id":{"d":897,"n":3,"t":4},"speaker":"A","text":"このペットボトルを両手に持ってください。"},{"id":{"d":897,"n":4,"t":4},"speaker":"B","text":"はい。この白い液体は何ですか？"},{"id":{"d":897,"n":5,"t":4},"speaker":"A","text":"牛乳ですよ。では両手でフリフリしましょう。"},{"id":{"d":897,"n":6,"t":4},"speaker":"B","text":"もう疲れました。牛乳が塊と透明な汁になっています。"},{"id":{"d":897,"n":7,"t":4},"speaker":"A","text":"良いですね。これでおいしそうなバターが完成です。"},{"id":{"d":897,"n":8,"t":4},"speaker":"B","text":"驚きました、知らないうちにバターを作らされていたのですね。"},{"id":{"d":897,"n":9,"t":4},"speaker":"A","text":"お疲れさまでした。早速パンに付けて食べましょう。"}];
INSERT INTO sentence [{"id":{"d":898,"n":1,"t":4},"speaker":"A","text":"保育園では、手足口病がはやっているそうですよ。"},{"id":{"d":898,"n":2,"t":4},"speaker":"B","text":"そうなのですね。真奈は大丈夫なのですか？"},{"id":{"d":898,"n":3,"t":4},"speaker":"A","text":"今のところは、症状は出ていません。潜伏期間もあるらしいですが。"},{"id":{"d":898,"n":4,"t":4},"speaker":"B","text":"それなら一安心ですね。予防法はないのですか？"},{"id":{"d":898,"n":5,"t":4},"speaker":"A","text":"インフルエンザなどと一緒で、手洗い、うがい、マスクでしょうね。"},{"id":{"d":898,"n":6,"t":4},"speaker":"B","text":"しばらくはマスクを着けて登園した方がいいですね。"},{"id":{"d":898,"n":7,"t":4},"speaker":"A","text":"かかってしまったら口が痛くて食べにくくなるそうですから、ヨーグルトなどの食べやすそうなものを用意しておきます。"},{"id":{"d":898,"n":8,"t":4},"speaker":"B","text":"なるほど。かかった時のために、病院の情報も集めておきましょう。"}];
INSERT INTO sentence [{"id":{"d":899,"n":1,"t":4},"speaker":"A","text":"髪をショートカットにしたのですね。"},{"id":{"d":899,"n":2,"t":4},"speaker":"B","text":"はい。髪が長いとシャンプーも大変なので、バッサリと切ってみました。"},{"id":{"d":899,"n":3,"t":4},"speaker":"A","text":"とてもお似合いですよ。"},{"id":{"d":899,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。でも、一つ悩みができてしまいました。"},{"id":{"d":899,"n":5,"t":4},"speaker":"A","text":"悩みとは何ですか？"},{"id":{"d":899,"n":6,"t":4},"speaker":"B","text":"髪型が変わったので、今までの服が似合わないのです。"},{"id":{"d":899,"n":7,"t":4},"speaker":"A","text":"確かに、以前より、きりっとした雰囲気に見えます。"},{"id":{"d":899,"n":8,"t":4},"speaker":"B","text":"ボーイッシュでスタイリッシュな感じの服に変えたいのですが、急に全部は変えられないので、少し困っています。"}];
INSERT INTO sentence [{"id":{"d":900,"n":1,"t":4},"speaker":"A","text":"おはようございます。"},{"id":{"d":900,"n":2,"t":4},"speaker":"B","text":"おはようございます。筋肉痛もなく元気そうですね。富士登山はいかがでしたか？"},{"id":{"d":900,"n":3,"t":4},"speaker":"A","text":"なぜ知っているのですか？先の週末に富士登山に行く話を私は以前していたでしょうか？"},{"id":{"d":900,"n":4,"t":4},"speaker":"B","text":"私は鈴本さんに聞きました。その鈴本さんは筋肉痛でお休みですか？"},{"id":{"d":900,"n":5,"t":4},"speaker":"A","text":"鈴本さんは下山途中に滑って骨折してしまい、救急搬送されてしまったのです。"},{"id":{"d":900,"n":6,"t":4},"speaker":"B","text":"鈴本さんが骨折されたのですか？"},{"id":{"d":900,"n":7,"t":4},"speaker":"A","text":"はい。すぐに入院となり、今もまだ、山梨の病院にいますよ。"},{"id":{"d":900,"n":8,"t":4},"speaker":"B","text":"それは大変な富士登山でしたね。"}];
INSERT INTO sentence [{"id":{"d":901,"n":1,"t":4},"speaker":"A","text":"みんな、スキーがうまいですね。"},{"id":{"d":901,"n":2,"t":4},"speaker":"B","text":"雪国育ちですからね。"},{"id":{"d":901,"n":3,"t":4},"speaker":"A","text":"転校生の私には分が悪すぎます。"},{"id":{"d":901,"n":4,"t":4},"speaker":"B","text":"松永君も元は転校生ですけれど、上手になりましたよ。"},{"id":{"d":901,"n":5,"t":4},"speaker":"A","text":"本当ですね。みんなと同じくらいうまく滑っています。"},{"id":{"d":901,"n":6,"t":4},"speaker":"B","text":"松永君も最初は初心者でしたよ。"},{"id":{"d":901,"n":7,"t":4},"speaker":"A","text":"そうなのですか？"},{"id":{"d":901,"n":8,"t":4},"speaker":"B","text":"はい。きっと大畑さんもすぐにうまくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":902,"n":1,"t":4},"speaker":"A","text":"お母さん、今日も市民プールに行くのですか？"},{"id":{"d":902,"n":2,"t":4},"speaker":"B","text":"はい。泳ぎがうまくなりたいので行ってきます。"},{"id":{"d":902,"n":3,"t":4},"speaker":"A","text":"世界水泳の中継を見たから、影響されたのですね。"},{"id":{"d":902,"n":4,"t":4},"speaker":"B","text":"そうです。あの映像が頭に残っていると、練習中のバタフライも上手に泳げそうな気がしてきます。"},{"id":{"d":902,"n":5,"t":4},"speaker":"A","text":"応援しています。ところで、市民プールでバタフライをするときは、周りにもよく気を付けた方がいいですよ。"},{"id":{"d":902,"n":6,"t":4},"speaker":"B","text":"どうしてですか？"},{"id":{"d":902,"n":7,"t":4},"speaker":"A","text":"混雑しているときはしぶきが周りにかかりそうだからです。"}];
INSERT INTO sentence [{"id":{"d":903,"n":1,"t":4},"speaker":"A","text":"このハンドクリームは、伸びが良くて気に入りました。"},{"id":{"d":903,"n":2,"t":4},"speaker":"B","text":"塗った後もべたつかないでしょう？"},{"id":{"d":903,"n":3,"t":4},"speaker":"A","text":"そうですね。"},{"id":{"d":903,"n":4,"t":4},"speaker":"B","text":"それでいて保湿も万全のお薦めのクリームです。"},{"id":{"d":903,"n":5,"t":4},"speaker":"A","text":"これでおいくらなのですか？"},{"id":{"d":903,"n":6,"t":4},"speaker":"B","text":"12,000円です。"},{"id":{"d":903,"n":7,"t":4},"speaker":"A","text":"少し考えてしまいますね。もう少し小さい容量のものはありませんか？"},{"id":{"d":903,"n":8,"t":4},"speaker":"B","text":"こちらの容量で7,000円です。"}];
INSERT INTO sentence [{"id":{"d":904,"n":1,"t":4},"speaker":"A","text":"大きな箱が届きましたよ。"},{"id":{"d":904,"n":2,"t":4},"speaker":"B","text":"届いたのですね。実は、兄弟3人で、お母さんに誕生日プレゼントを買ったのです。"},{"id":{"d":904,"n":3,"t":4},"speaker":"A","text":"これは私へのプレゼントなのですね、うれしいです。何が入っているのですか？"},{"id":{"d":904,"n":4,"t":4},"speaker":"B","text":"ぜひ開けてみてください。"},{"id":{"d":904,"n":5,"t":4},"speaker":"A","text":"これは、マッサージチェアですか？"},{"id":{"d":904,"n":6,"t":4},"speaker":"B","text":"そうです。量販店で使い心地を試していたでしょう？"},{"id":{"d":904,"n":7,"t":4},"speaker":"A","text":"兄弟3人に買ってもらえるなんて思ってもいませんでした。ありがとうございます。"},{"id":{"d":904,"n":8,"t":4},"speaker":"B","text":"喜んでもらえてよかったです。お誕生日おめでとうございます。"}];
INSERT INTO sentence [{"id":{"d":905,"n":1,"t":4},"speaker":"A","text":"少し風邪気味で食欲がありません。"},{"id":{"d":905,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？ランチに豚丼を食べに行こうと思っていたのですが、今日はやめましょうか？"},{"id":{"d":905,"n":3,"t":4},"speaker":"A","text":"すみません。豚丼は今日の私の体調にはきつすぎますので、そうしていただけると助かります。"},{"id":{"d":905,"n":4,"t":4},"speaker":"B","text":"そのお店には豚汁もあるのですが、豚汁ならどうですか？"},{"id":{"d":905,"n":5,"t":4},"speaker":"A","text":"そのくらいなら食べられると思います。"},{"id":{"d":905,"n":6,"t":4},"speaker":"B","text":"では、一緒に行きましょう。"},{"id":{"d":905,"n":7,"t":4},"speaker":"A","text":"食事の後に飲めるように、葛根湯を買っていきます。お店の近くにドラッグストアはありますか？"},{"id":{"d":905,"n":8,"t":4},"speaker":"B","text":"確か、お店の向かいがドラッグストアですよ。"}];
INSERT INTO sentence [{"id":{"d":906,"n":1,"t":4},"speaker":"A","text":"髪を伸ばしているのですか？"},{"id":{"d":906,"n":2,"t":4},"speaker":"B","text":"意図的に伸ばしているわけではありません。近頃は美容院に行けていないので、長くなってしまっているのです。"},{"id":{"d":906,"n":3,"t":4},"speaker":"A","text":"美容院に行けないほど、お忙しいのですか？"},{"id":{"d":906,"n":4,"t":4},"speaker":"B","text":"いいえ。通っていた美容院が閉店になってしまって、次のところをまだ決めていないのですよ。"},{"id":{"d":906,"n":5,"t":4},"speaker":"A","text":"新しく通う美容院を決めるのは難しいですからね。"},{"id":{"d":906,"n":6,"t":4},"speaker":"B","text":"前の美容師の方の紹介でもあればよかったのですが、何もありませんでした。"},{"id":{"d":906,"n":7,"t":4},"speaker":"A","text":"次を決めるまで、困りますね。"},{"id":{"d":906,"n":8,"t":4},"speaker":"B","text":"はい。でも、目を付けているところはあるので、今度、試しに行ってみます。"}];
INSERT INTO sentence [{"id":{"d":907,"n":1,"t":4},"speaker":"A","text":"食後のお薬ですか？"},{"id":{"d":907,"n":2,"t":4},"speaker":"B","text":"はい。でも、病気のためのお薬ではありません。"},{"id":{"d":907,"n":3,"t":4},"speaker":"A","text":"ビタミン剤ですか？"},{"id":{"d":907,"n":4,"t":4},"speaker":"B","text":"そうです。このところは、炭水化物と肉ばかりを食べているので、飲んでいます。"},{"id":{"d":907,"n":5,"t":4},"speaker":"A","text":"私と一緒に牛丼やローストビーフ丼や肉巻きおにぎりを食べていましたからね。"},{"id":{"d":907,"n":6,"t":4},"speaker":"B","text":"あなたも野菜を取っていないので、サプリメントでも飲んだ方がいいですよ。"},{"id":{"d":907,"n":7,"t":4},"speaker":"A","text":"私は薬を上手に飲めないので、なるべく飲みたくありません。"},{"id":{"d":907,"n":8,"t":4},"speaker":"B","text":"では、こちらの野菜ジュースはいかがですか？これはおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":908,"n":1,"t":4},"speaker":"A","text":"祖父の補聴器のことで相談があります。"},{"id":{"d":908,"n":2,"t":4},"speaker":"B","text":"どういったことでしょうか？"},{"id":{"d":908,"n":3,"t":4},"speaker":"A","text":"祖父は補聴器をよくなくしてしまうのです。そうならないために、何か良い方法はありますか？"},{"id":{"d":908,"n":4,"t":4},"speaker":"B","text":"小さいものですから、そういう方もいらっしゃいますね。こちらのストラップを付けてはいかがでしょうか？"},{"id":{"d":908,"n":5,"t":4},"speaker":"A","text":"なるほど、両耳の補聴器に付けて首に掛けられるのですね。これなら、補聴器を着けているときに間違って落としてしまうことは防ぐことができそうです。"},{"id":{"d":908,"n":6,"t":4},"speaker":"B","text":"これで問題の解決になりますか？"},{"id":{"d":908,"n":7,"t":4},"speaker":"A","text":"補聴器を着けているときに落とすことは減りそうですが、祖父は着けているのがうっとうしいと言って、補聴器をどこかに置いて忘れてしまうことも多いのです。"},{"id":{"d":908,"n":8,"t":4},"speaker":"B","text":"それについても、補聴器にストラップが付いていれば、補聴器がより目立って探しやすいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":909,"n":1,"t":4},"speaker":"A","text":"田中さん、久しぶりですね。"},{"id":{"d":909,"n":2,"t":4},"speaker":"B","text":"インフルエンザでお休みさせていただいたので、1週間ぶりです。"},{"id":{"d":909,"n":3,"t":4},"speaker":"A","text":"もう体調は万全なのですか？"},{"id":{"d":909,"n":4,"t":4},"speaker":"B","text":"熱が下がってから、何日も安静にしていましたから大丈夫です。"},{"id":{"d":909,"n":5,"t":4},"speaker":"A","text":"今日は無理なさらないでくださいね。"},{"id":{"d":909,"n":6,"t":4},"speaker":"B","text":"仕事がたまっていると思って出勤しました。でも、それほどたまっていないようですね。"},{"id":{"d":909,"n":7,"t":4},"speaker":"A","text":"代わりに私がやっておきました。"},{"id":{"d":909,"n":8,"t":4},"speaker":"B","text":"全部をやっておいてくれた福原さんが神様に見えます。"}];
INSERT INTO sentence [{"id":{"d":910,"n":1,"t":4},"speaker":"A","text":"最近、あなたは蜂蜜とレモンのジュースばかりを飲んでいますね。"},{"id":{"d":910,"n":2,"t":4},"speaker":"B","text":"ええ。喉が少し痛いのです。"},{"id":{"d":910,"n":3,"t":4},"speaker":"A","text":"蜂蜜とレモンのジュースが喉に効くのですか？"},{"id":{"d":910,"n":4,"t":4},"speaker":"B","text":"子どもの頃、喉が痛いときには母が蜂蜜とレモンの飲み物を作ってくれました。私は効くと思って飲んでいます。"},{"id":{"d":910,"n":5,"t":4},"speaker":"A","text":"確かに、蜂蜜とレモンの飲み物は喉に優しい感じがして効きそうですね。"},{"id":{"d":910,"n":6,"t":4},"speaker":"B","text":"ジュースでも調子が良くなる気がします。"},{"id":{"d":910,"n":7,"t":4},"speaker":"A","text":"私も今度、喉が痛くなったら試してみます。"},{"id":{"d":910,"n":8,"t":4},"speaker":"B","text":"ぜひそうしてください。このメーカーのジュースがおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":911,"n":1,"t":4},"speaker":"A","text":"今日は日焼け対策が万全ですね。"},{"id":{"d":911,"n":2,"t":4},"speaker":"B","text":"はい。先日、腕に発疹ができて病院に行ったら、紫外線アレルギーだと言われたのです。"},{"id":{"d":911,"n":3,"t":4},"speaker":"A","text":"それは紫外線に当たるとそうなってしまうのですか？"},{"id":{"d":911,"n":4,"t":4},"speaker":"B","text":"そうです。少しの距離だと思って、日傘も差さずに歩いたので発症したようです。"},{"id":{"d":911,"n":5,"t":4},"speaker":"A","text":"少しの紫外線でも発症することがあるのですか？"},{"id":{"d":911,"n":6,"t":4},"speaker":"B","text":"そのようです。少し怖くなって、あれから暑くてもアームカバーと日傘は手放せません。"},{"id":{"d":911,"n":7,"t":4},"speaker":"A","text":"誰でもなる症状なのでしょうか？"},{"id":{"d":911,"n":8,"t":4},"speaker":"B","text":"肌が強い人は大丈夫だと思いますが、体調にもよるらしいですよ。"}];
INSERT INTO sentence [{"id":{"d":912,"n":1,"t":4},"speaker":"A","text":"鼻が真っ赤ですね。"},{"id":{"d":912,"n":2,"t":4},"speaker":"B","text":"花粉症がひどくて、ずっと鼻をかんでいたら、このような状態になりました。"},{"id":{"d":912,"n":3,"t":4},"speaker":"A","text":"朝は気が付きませんでした。"},{"id":{"d":912,"n":4,"t":4},"speaker":"B","text":"マスクをしていましたから見えなかったのですよ。"},{"id":{"d":912,"n":5,"t":4},"speaker":"A","text":"そういえばそうですね。ランチを食べるために今はマスクを外しているので、気付けたのですね。"},{"id":{"d":912,"n":6,"t":4},"speaker":"B","text":"もう早く花粉の季節が終わってほしいです。"},{"id":{"d":912,"n":7,"t":4},"speaker":"A","text":"最近は少しお高いですけれど、柔らかいティッシュペーパーも売っていますよ。"},{"id":{"d":912,"n":8,"t":4},"speaker":"B","text":"言われるまで柔らかいティッシュペーパーの存在を忘れていました。後で買ってきます。"}];
INSERT INTO sentence [{"id":{"d":913,"n":1,"t":4},"speaker":"A","text":"今日も寝ている時にこむら返りが起こりました。"},{"id":{"d":913,"n":2,"t":4},"speaker":"B","text":"今日も、ということはよく起こるのですか？"},{"id":{"d":913,"n":3,"t":4},"speaker":"A","text":"今日で3日連続です。"},{"id":{"d":913,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。"},{"id":{"d":913,"n":5,"t":4},"speaker":"A","text":"ストレッチでその場は治すのですが、きっと根本的な原因がある気がしています。"},{"id":{"d":913,"n":6,"t":4},"speaker":"B","text":"ビタミン不足や水分不足でなると聞いたことがありますよ。"},{"id":{"d":913,"n":7,"t":4},"speaker":"A","text":"水分は寝る前に取っているから大丈夫だと思います。"},{"id":{"d":913,"n":8,"t":4},"speaker":"B","text":"では、ビタミン不足かもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":914,"n":1,"t":4},"speaker":"A","text":"ショッピングモールにスケートリンクがオープンしましたよ。"},{"id":{"d":914,"n":2,"t":4},"speaker":"B","text":"子どもたちが楽しみにしていましたから、日曜日に一緒に行きましょう。"},{"id":{"d":914,"n":3,"t":4},"speaker":"A","text":"行きましょう。手袋をしていくだけでよいのでしたよね？"},{"id":{"d":914,"n":4,"t":4},"speaker":"B","text":"はい。去年は、会場で靴などをレンタルですることがきました。"},{"id":{"d":914,"n":5,"t":4},"speaker":"A","text":"一年中やっているリンクもあるのですけれど、私たちには季節限定の方がいいですね。"},{"id":{"d":914,"n":6,"t":4},"speaker":"B","text":"そうですね。子どもたちに一年中付き合わされたら、きっと大変な筋肉痛になってしまいます。"},{"id":{"d":914,"n":7,"t":4},"speaker":"A","text":"そうなのですね。では、日曜日に、大人2人、子ども2人で予約をしておきますね。"},{"id":{"d":914,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。夫も行けると思いますので、大人3人でお願いします。"}];
INSERT INTO sentence [{"id":{"d":915,"n":1,"t":4},"speaker":"A","text":"お父さん「りーりーりー」とは何ですか？"},{"id":{"d":915,"n":2,"t":4},"speaker":"B","text":"どこで見聞きしてきたのですか？"},{"id":{"d":915,"n":3,"t":4},"speaker":"A","text":"健太君の野球を見に行ったら、みんなが言っていたのです。健太君が二塁に行こうとしていた時にそう言っていました。"},{"id":{"d":915,"n":4,"t":4},"speaker":"B","text":"野球の時に言っていたのですね。それは、恐らく「リード、リード、リード」がそう聞こえたのだと思いますよ。"},{"id":{"d":915,"n":5,"t":4},"speaker":"A","text":"「リード」というのは英語ですか？"},{"id":{"d":915,"n":6,"t":4},"speaker":"B","text":"そうです。「進んでよい」という意味です。"},{"id":{"d":915,"n":7,"t":4},"speaker":"A","text":"なるほど。それで健太は二塁に進んだのですね。"}];
INSERT INTO sentence [{"id":{"d":916,"n":1,"t":4},"speaker":"A","text":"おはようございます。今朝は早いのですね。"},{"id":{"d":916,"n":2,"t":4},"speaker":"B","text":"朝活動と称して、ジム通いをすることにしたのです。"},{"id":{"d":916,"n":3,"t":4},"speaker":"A","text":"では、今日はもう運動をしてきたのですか？"},{"id":{"d":916,"n":4,"t":4},"speaker":"B","text":"はい。ランニングマシンで少々走ってきました。"},{"id":{"d":916,"n":5,"t":4},"speaker":"A","text":"すごいです。私は30分前に起きたばかりです。"},{"id":{"d":916,"n":6,"t":4},"speaker":"B","text":"早起きは気持ちが良いですよ。一緒にどうですか？"},{"id":{"d":916,"n":7,"t":4},"speaker":"A","text":"私は出かける直前にしか起きられません。ましてや走るなど私には無理です。"},{"id":{"d":916,"n":8,"t":4},"speaker":"B","text":"走らなくてもよいですから、早起きしましょうよ。"}];
INSERT INTO sentence [{"id":{"d":917,"n":1,"t":4},"speaker":"A","text":"今のプレーは危なかったですね。"},{"id":{"d":917,"n":2,"t":4},"speaker":"B","text":"ええ。聡は塁に出られましたけれど、相手の選手が心配です。"},{"id":{"d":917,"n":3,"t":4},"speaker":"A","text":"外野同士がぶつかってしまうのは、よくあることなのですか？"},{"id":{"d":917,"n":4,"t":4},"speaker":"B","text":"プロではほとんどありませんけれど、中学生や高校生ではありますね。"},{"id":{"d":917,"n":5,"t":4},"speaker":"A","text":"そうですか。けがをしていないといいですね。"},{"id":{"d":917,"n":6,"t":4},"speaker":"B","text":"大丈夫だったようですよ。2人ともマウンドに戻ってきました。"},{"id":{"d":917,"n":7,"t":4},"speaker":"A","text":"良かったです。"},{"id":{"d":917,"n":8,"t":4},"speaker":"B","text":"これで聡も安心して走れますね。"}];
INSERT INTO sentence [{"id":{"d":918,"n":1,"t":4},"speaker":"A","text":"靴擦れが痛くなってきました。"},{"id":{"d":918,"n":2,"t":4},"speaker":"B","text":"ばんそうこうを持っていますよ。これを使ってください。"},{"id":{"d":918,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。ストッキングにも穴が開いてしまいました。"},{"id":{"d":918,"n":4,"t":4},"speaker":"B","text":"このショッピングセンターのお手洗いには着替え用の台もありますから、新しいストッキングを購入したら、そこではき替えてくるといいですよ。"},{"id":{"d":918,"n":5,"t":4},"speaker":"A","text":"そうしてきます。少し待っていてください。"},{"id":{"d":918,"n":6,"t":4},"speaker":"B","text":"分かりました。100円ショップに靴擦れ防止用のアイテムがありますから、後で見に行きましょう。"},{"id":{"d":918,"n":7,"t":4},"speaker":"A","text":"それは靴の方に付けるものですか？"},{"id":{"d":918,"n":8,"t":4},"speaker":"B","text":"そうです。痛みが和らぐと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":919,"n":1,"t":4},"speaker":"A","text":"この間、あなたに教えていただいたゼリーはとても良かったです。"},{"id":{"d":919,"n":2,"t":4},"speaker":"B","text":"「らくらく服用ゼリー」のことですね。"},{"id":{"d":919,"n":3,"t":4},"speaker":"A","text":"はい。熱があるのに息子がどうしても薬を飲んでくれなくて困っていたのです。"},{"id":{"d":919,"n":4,"t":4},"speaker":"B","text":"それが「らくらく服用ゼリー」で飲めるようになったのですね。それはよかったです。"},{"id":{"d":919,"n":5,"t":4},"speaker":"A","text":"いずれは水でお薬が飲めるようになるといいのですけれど、今のところは飲んでくれただけでも良かったです。"},{"id":{"d":919,"n":6,"t":4},"speaker":"B","text":"息子さんのお熱は下がりましたか？"},{"id":{"d":919,"n":7,"t":4},"speaker":"A","text":"おかげさまで元気になりました。"},{"id":{"d":919,"n":8,"t":4},"speaker":"B","text":"それは良かったです。"}];
INSERT INTO sentence [{"id":{"d":920,"n":1,"t":4},"speaker":"A","text":"もしもし、榎本さん。体調が悪いと聞きましたが、今日は仕事をお休みしますか？"},{"id":{"d":920,"n":2,"t":4},"speaker":"B","text":"はい、そうさせてください。今日はおなかが痛いので、家で寝ています。"},{"id":{"d":920,"n":3,"t":4},"speaker":"A","text":"病院には行ったのですか？"},{"id":{"d":920,"n":4,"t":4},"speaker":"B","text":"いいえ。歩くとトイレに行きたくなってしまうので、病院に行けません。"},{"id":{"d":920,"n":5,"t":4},"speaker":"A","text":"下痢なのですか？"},{"id":{"d":920,"n":6,"t":4},"speaker":"B","text":"そうです。トイレの前に布団を敷きたいくらいです。"},{"id":{"d":920,"n":7,"t":4},"speaker":"A","text":"それは大変ですね。後で、食べられそうなものと下痢止めのお薬をアパートのドアに掛けておきますね。"},{"id":{"d":920,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。持つべきものは友です。"}];
INSERT INTO sentence [{"id":{"d":921,"n":1,"t":4},"speaker":"A","text":"最近は、お休みの日は何をしているのですか？"},{"id":{"d":921,"n":2,"t":4},"speaker":"B","text":"海外の医療ドラマを見ています。"},{"id":{"d":921,"n":3,"t":4},"speaker":"A","text":"見ているドラマは海外のものだけなのですか？"},{"id":{"d":921,"n":4,"t":4},"speaker":"B","text":"はい。日本の医療ドラマを見ていると、「そのようなことが実際にあるわけがない」と気になってしまうのです。"},{"id":{"d":921,"n":5,"t":4},"speaker":"A","text":"看護師さんから見た日本の医療ドラマはそのような感じなのですか？"},{"id":{"d":921,"n":6,"t":4},"speaker":"B","text":"実際、現実とはかけ離れていますからね。その点で、海外のものは「このような国もあるのね」で済みます。"},{"id":{"d":921,"n":7,"t":4},"speaker":"A","text":"現実との乖離が気になるのであれば、医療ドラマ以外を見ればよいのではありませんか？"},{"id":{"d":921,"n":8,"t":4},"speaker":"B","text":"医療ドラマが好きなのです。医療ドラマを見ていると、医療の道を志した頃の気持ちを思い出すのです。"}];
INSERT INTO sentence [{"id":{"d":922,"n":1,"t":4},"speaker":"A","text":"昼間に、大きくて重い箱が届きましたよ。何か注文しましたか？"},{"id":{"d":922,"n":2,"t":4},"speaker":"B","text":"この間インターネットの通信販売で注文した栄養ドリンクだと思います。"},{"id":{"d":922,"n":3,"t":4},"speaker":"A","text":"本当に大きな箱でしたよ。"},{"id":{"d":922,"n":4,"t":4},"speaker":"B","text":"どれが効くのかが分からないので、人気リストの上から10種類を買ってみたのです。"},{"id":{"d":922,"n":5,"t":4},"speaker":"A","text":"最近は残業が多いですけれど、ドリンクで元気になって働き続けるのは賛成できません。"},{"id":{"d":922,"n":6,"t":4},"speaker":"B","text":"それほど無理はしませんよ。ドリンクの味に興味があっただけですから。"},{"id":{"d":922,"n":7,"t":4},"speaker":"A","text":"本当ですか？無理はしないでくださいね。"},{"id":{"d":922,"n":8,"t":4},"speaker":"B","text":"無理はしないと約束します。"}];
INSERT INTO sentence [{"id":{"d":923,"n":1,"t":4},"speaker":"A","text":"お母さん、バスケットボールを習いに行きたいです。"},{"id":{"d":923,"n":2,"t":4},"speaker":"B","text":"1年生でも習えるのかを調べてみますね。"},{"id":{"d":923,"n":3,"t":4},"speaker":"A","text":"習えると思いますよ。誠君の弟の実君も行っていますから。"},{"id":{"d":923,"n":4,"t":4},"speaker":"B","text":"実君はまだ幼稚園でしたよね。"},{"id":{"d":923,"n":5,"t":4},"speaker":"A","text":"そうです。誠君たちと一緒に習いたいです。"},{"id":{"d":923,"n":6,"t":4},"speaker":"B","text":"分かりました。バスケットボール教室について、誠君のお母さんに聞いてみますね。"},{"id":{"d":923,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。すぐに誠君のお母さんに電話しましょう。"},{"id":{"d":923,"n":8,"t":4},"speaker":"B","text":"せっかちですね。今から電話してみます。"}];
INSERT INTO sentence [{"id":{"d":924,"n":1,"t":4},"speaker":"A","text":"最近は、体の不調が少ないです。"},{"id":{"d":924,"n":2,"t":4},"speaker":"B","text":"それは素晴らしいことですね。何か体に良いことをやっているのですか？"},{"id":{"d":924,"n":3,"t":4},"speaker":"A","text":"座禅をしています。京都に行った時に座禅体験をしたら、気分がすっきりしたような気がしたのです。"},{"id":{"d":924,"n":4,"t":4},"speaker":"B","text":"その1回で体の不調が治ってしまったのですか？"},{"id":{"d":924,"n":5,"t":4},"speaker":"A","text":"いいえ。家の近くのお寺で座禅ができるところを探して、帰ってきてからも続けています。"},{"id":{"d":924,"n":6,"t":4},"speaker":"B","text":"座禅を続けておられるのですね。"},{"id":{"d":924,"n":7,"t":4},"speaker":"A","text":"はい。姿勢が良くなったのと呼吸法が分かったのが、健康に良い影響を与えているような気がします。"}];
INSERT INTO sentence [{"id":{"d":925,"n":1,"t":4},"speaker":"A","text":"夫が糖尿病だと言われてしまいました。"},{"id":{"d":925,"n":2,"t":4},"speaker":"B","text":"それは心配ですね。治療法は薬ですか？注射ですか？"},{"id":{"d":925,"n":3,"t":4},"speaker":"A","text":"お医者さんには「薬を飲みながら、食事療法で対処してください」と言われましたが、どのような食事を作ったらよいか分かりません。"},{"id":{"d":925,"n":4,"t":4},"speaker":"B","text":"あまり考えすぎずに、まずは、量を減らしたらどうでしょう？"},{"id":{"d":925,"n":5,"t":4},"speaker":"A","text":"なかなか難しいです。夜は、飲んで帰ってくることが多いので、夫の一日の摂取カロリーを計算できません。"},{"id":{"d":925,"n":6,"t":4},"speaker":"B","text":"飲み会をやめてはお仕事にならないのですか？"},{"id":{"d":925,"n":7,"t":4},"speaker":"A","text":"本人は「酒はやめられない」と言い張っていますが、本当に病気を治す気があるならそのようなことはないと思います。やる気がないだけですよ。"},{"id":{"d":925,"n":8,"t":4},"speaker":"B","text":"困りましたね。もう少し話し合いが必要ですね。"}];
INSERT INTO sentence [{"id":{"d":926,"n":1,"t":4},"speaker":"A","text":"ムカデに刺されてしまいました。"},{"id":{"d":926,"n":2,"t":4},"speaker":"B","text":"ひどいみみず腫れですね。"},{"id":{"d":926,"n":3,"t":4},"speaker":"A","text":"痛くてたまりません。"},{"id":{"d":926,"n":4,"t":4},"speaker":"B","text":"何か処置はしましたか？"},{"id":{"d":926,"n":5,"t":4},"speaker":"A","text":"いいえ。家に薬がありませんでしたので、何もしていません。"},{"id":{"d":926,"n":6,"t":4},"speaker":"B","text":"かゆみ止めは私の家にもありますけれど、ムカデに刺されたときは何を付ければよいのでしょうか？"},{"id":{"d":926,"n":7,"t":4},"speaker":"A","text":"私も分からないので、水で洗った後はそのままなのです。"},{"id":{"d":926,"n":8,"t":4},"speaker":"B","text":"これは皮膚科に行った方がいいですね。"},{"id":{"d":926,"n":9,"t":4},"speaker":"A","text":"そうします。午後から半日休暇を取って行ってみます。"},{"id":{"d":926,"n":10,"t":4},"speaker":"B","text":"それがいいです。腫れがひどくならないうちに行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":927,"n":1,"t":4},"speaker":"A","text":"祖母が白内障の手術を受けるのです。"},{"id":{"d":927,"n":2,"t":4},"speaker":"B","text":"日帰り手術ですか？"},{"id":{"d":927,"n":3,"t":4},"speaker":"A","text":"その予定です。"},{"id":{"d":927,"n":4,"t":4},"speaker":"B","text":"その日は仕事を休むのですか？"},{"id":{"d":927,"n":5,"t":4},"speaker":"A","text":"はい。私が祖母の付き添いで病院へ行くことになりましたから、お休みします。"},{"id":{"d":927,"n":6,"t":4},"speaker":"B","text":"忘れずに、休暇届を出しておいてくださいね。"},{"id":{"d":927,"n":7,"t":4},"speaker":"A","text":"分かりました。万が一、祖母が入院することになりましたら、お休みを延長させてください。"},{"id":{"d":927,"n":8,"t":4},"speaker":"B","text":"了解しました。状況が分かりましたら連絡をくださいね。"}];
INSERT INTO sentence [{"id":{"d":928,"n":1,"t":4},"speaker":"A","text":"山本さんのその歩行器はとても良いですね。"},{"id":{"d":928,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。孫が買ってくれたのです。"},{"id":{"d":928,"n":3,"t":4},"speaker":"A","text":"籠も椅子も付いているのですね。"},{"id":{"d":928,"n":4,"t":4},"speaker":"B","text":"そうです。おかげでどこでも休めますし、そこの商店街までなら買い物にも行けますよ。"},{"id":{"d":928,"n":5,"t":4},"speaker":"A","text":"今から買い物ですか？"},{"id":{"d":928,"n":6,"t":4},"speaker":"B","text":"はい。朝ご飯のパンを買いに行こうと思っています。"},{"id":{"d":928,"n":7,"t":4},"speaker":"A","text":"私もご一緒してもよいですか？"},{"id":{"d":928,"n":8,"t":4},"speaker":"B","text":"はい。一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":929,"n":1,"t":4},"speaker":"A","text":"あなたは包帯の巻き方が上手ですね。"},{"id":{"d":929,"n":2,"t":4},"speaker":"B","text":"そう言われるとうれしいです。"},{"id":{"d":929,"n":3,"t":4},"speaker":"A","text":"どうしてそれほどまでに包帯の巻き方が上手なのですか？"},{"id":{"d":929,"n":4,"t":4},"speaker":"B","text":"卒業後は看護学校に行こうと思って、少し勉強しているのです。"},{"id":{"d":929,"n":5,"t":4},"speaker":"A","text":"将来は看護師になるのですか？"},{"id":{"d":929,"n":6,"t":4},"speaker":"B","text":"はい、そのつもりです。"},{"id":{"d":929,"n":7,"t":4},"speaker":"A","text":"では、私があなたの患者第1号ということですね。"},{"id":{"d":929,"n":8,"t":4},"speaker":"B","text":"残念でした。私はこれまでも妹や弟にしょっちゅう手当てをしていますから、あなたは第1号ではありません。"}];
INSERT INTO sentence [{"id":{"d":930,"n":1,"t":4},"speaker":"A","text":"胃カメラ検査を受けてきました。"},{"id":{"d":930,"n":2,"t":4},"speaker":"B","text":"大変でしたでしょう？私は去年、泣きそうになりました。"},{"id":{"d":930,"n":3,"t":4},"speaker":"A","text":"それほど大変には感じませんでしたよ。"},{"id":{"d":930,"n":4,"t":4},"speaker":"B","text":"本当ですか？口からカメラを入れられたら、吐きそうになりませんか？"},{"id":{"d":930,"n":5,"t":4},"speaker":"A","text":"私は鼻から入れましたよ。"},{"id":{"d":930,"n":6,"t":4},"speaker":"B","text":"鼻から入れる検査方法もあるのですね。"},{"id":{"d":930,"n":7,"t":4},"speaker":"A","text":"私は今回が初めての胃カメラ検査でしたので、鼻から入れるのが普通だと思っていました。確かに、口から入れるのは大変かもしれませんね。"},{"id":{"d":930,"n":8,"t":4},"speaker":"B","text":"私も今年は鼻からにしてもらいます。口は大変でしたから。"}];
INSERT INTO sentence [{"id":{"d":931,"n":1,"t":4},"speaker":"A","text":"近くに救急車が来ましたね。"},{"id":{"d":931,"n":2,"t":4},"speaker":"B","text":"向かいの家のおばあちゃんが倒れられたそうですよ。"},{"id":{"d":931,"n":3,"t":4},"speaker":"A","text":"昨日おばあちゃんにお会いしましたけれど、お元気そうでしたよ。"},{"id":{"d":931,"n":4,"t":4},"speaker":"B","text":"今日は暑かったので、熱中症かもしれませんね。"},{"id":{"d":931,"n":5,"t":4},"speaker":"A","text":"大丈夫でしょうか？"},{"id":{"d":931,"n":6,"t":4},"speaker":"B","text":"なかなか出発しませんね。受け入れ先の決定に時間がかかっているのでしょうか？"},{"id":{"d":931,"n":7,"t":4},"speaker":"A","text":"心配ですね。早く病院に運んであげてほしいです。"}];
INSERT INTO sentence [{"id":{"d":932,"n":1,"t":4},"speaker":"A","text":"少し待ってください。足がしびれてしまって歩けません。"},{"id":{"d":932,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":932,"n":3,"t":4},"speaker":"A","text":"しばらく動けそうにありません。皆さんはよく普通に歩けますね。"},{"id":{"d":932,"n":4,"t":4},"speaker":"B","text":"法事の時には、この正座椅子を持ってきていますからね。"},{"id":{"d":932,"n":5,"t":4},"speaker":"A","text":"小さい枕みたいな椅子ですね。これはどのように使うのですか？"},{"id":{"d":932,"n":6,"t":4},"speaker":"B","text":"正座をする時にはこれをお尻の下に置いておくのです。そうすると、足がしびれません。"},{"id":{"d":932,"n":7,"t":4},"speaker":"A","text":"そのような良いものがあったのなら教えてくださいよ。"},{"id":{"d":932,"n":8,"t":4},"speaker":"B","text":"知っていると思っていましたよ。今日の法事で知らなかったのはあなただけかもしれません。"}];
INSERT INTO sentence [{"id":{"d":933,"n":1,"t":4},"speaker":"A","text":"先生、先月に出していただいたお薬を、他のものに変えていただけませんか？"},{"id":{"d":933,"n":2,"t":4},"speaker":"B","text":"効かなかったということですか？"},{"id":{"d":933,"n":3,"t":4},"speaker":"A","text":"いいえ。粉薬が苦手で、飲むのに苦労したのです。"},{"id":{"d":933,"n":4,"t":4},"speaker":"B","text":"なるほど、錠剤がいいということですね。"},{"id":{"d":933,"n":5,"t":4},"speaker":"A","text":"はい、そうしていただけると助かります。"},{"id":{"d":933,"n":6,"t":4},"speaker":"B","text":"残念ですが、同じ薬効の錠剤はまだ出ていません。"},{"id":{"d":933,"n":7,"t":4},"speaker":"A","text":"そうですか。カプセルもありませんか？"},{"id":{"d":933,"n":8,"t":4},"speaker":"B","text":"はい。粉薬しかありませんので、服薬ゼリーなどを使うなどして飲んでみてください。"}];
INSERT INTO sentence [{"id":{"d":934,"n":1,"t":4},"speaker":"A","text":"サッカーの天皇杯決勝のチケットが取れました。"},{"id":{"d":934,"n":2,"t":4},"speaker":"B","text":"よく取れましたね。"},{"id":{"d":934,"n":3,"t":4},"speaker":"A","text":"一緒に行く人を探しているのですが、どうですか？"},{"id":{"d":934,"n":4,"t":4},"speaker":"B","text":"私が行ってもよいのですか？"},{"id":{"d":934,"n":5,"t":4},"speaker":"A","text":"SS席なのでチケット代が高いと言われ、一緒に行く人がなかなか見つからないのです。"},{"id":{"d":934,"n":6,"t":4},"speaker":"B","text":"チケット代はいくらですか？"},{"id":{"d":934,"n":7,"t":4},"speaker":"A","text":"9,000円です。"},{"id":{"d":934,"n":8,"t":4},"speaker":"B","text":"それくらいでしたら、アルバイト代で何とかなります。ぜひ一緒に行かせてください。"}];
INSERT INTO sentence [{"id":{"d":935,"n":1,"t":4},"speaker":"A","text":"花粉症なのでマスクをしていますが、息苦しくて困っています。"},{"id":{"d":935,"n":2,"t":4},"speaker":"B","text":"通気性が良いものもありますが、それだと、花粉も通してしまいますものね。"},{"id":{"d":935,"n":3,"t":4},"speaker":"A","text":"はい。鼻も詰まっているので、余計に息苦しく感じます。"},{"id":{"d":935,"n":4,"t":4},"speaker":"B","text":"ところで、あなたが着けているそのマスクは少し小さ過ぎませんか？"},{"id":{"d":935,"n":5,"t":4},"speaker":"A","text":"ぴったりとしていた方が、花粉を防げると思ったのですが、違うのでしょうか？"},{"id":{"d":935,"n":6,"t":4},"speaker":"B","text":"鼻と口の周りにもう少し空間があった方が、息がしやすいように思いますよ。"},{"id":{"d":935,"n":7,"t":4},"speaker":"A","text":"もう少し大きい方がいいということでしょうか？"},{"id":{"d":935,"n":8,"t":4},"speaker":"B","text":"そうかもしれません。一度、薬局で相談してみるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":936,"n":1,"t":4},"speaker":"A","text":"今日はネイルをしていませんね。"},{"id":{"d":936,"n":2,"t":4},"speaker":"B","text":"最近は爪が弱くなって、割れるようになってしまったのです。"},{"id":{"d":936,"n":3,"t":4},"speaker":"A","text":"ネイルのし過ぎですか？"},{"id":{"d":936,"n":4,"t":4},"speaker":"B","text":"はい。病院で見てもらったら、そう指摘されました。"},{"id":{"d":936,"n":5,"t":4},"speaker":"A","text":"それで、ネイルはしばらくお休みなのですね。"},{"id":{"d":936,"n":6,"t":4},"speaker":"B","text":"はい。これからは、披露宴にお呼ばれしたときなど特別な機会にだけネイルをしようと思っています。"},{"id":{"d":936,"n":7,"t":4},"speaker":"A","text":"毎日きれいにされていたのに、残念ですね。"},{"id":{"d":936,"n":8,"t":4},"speaker":"B","text":"爪が割れるよりは良いと思うことにしています。"}];
INSERT INTO sentence [{"id":{"d":937,"n":1,"t":4},"speaker":"A","text":"昨日、自宅に電話をしたのですが、いらっしゃいませんでしたね。"},{"id":{"d":937,"n":2,"t":4},"speaker":"B","text":"何時ごろですか？"},{"id":{"d":937,"n":3,"t":4},"speaker":"A","text":"18時くらいです。"},{"id":{"d":937,"n":4,"t":4},"speaker":"B","text":"18時でしたら、お風呂に入っていたのだと思います。"},{"id":{"d":937,"n":5,"t":4},"speaker":"A","text":"そうなのですね。お風呂の時間はいつもそのくらいなのですか？"},{"id":{"d":937,"n":6,"t":4},"speaker":"B","text":"はい。帰宅してからお風呂に入って、それから夕飯をいただいています。"},{"id":{"d":937,"n":7,"t":4},"speaker":"A","text":"早い時間にお風呂に入るのですね。私はいつも寝る前にお風呂に入ります。"},{"id":{"d":937,"n":8,"t":4},"speaker":"B","text":"食事の後に晩酌をしてしまうと、そのまま眠りたくなってしまうので、私はご飯の前に入っています。"}];
INSERT INTO sentence [{"id":{"d":938,"n":1,"t":4},"speaker":"A","text":"おじいさん、久しぶりにお会いしましたね。"},{"id":{"d":938,"n":2,"t":4},"speaker":"B","text":"お久しぶりです。しばらく病院にいたのですが、昨日ようやく自宅に戻ってくることができました。"},{"id":{"d":938,"n":3,"t":4},"speaker":"A","text":"入院されていたのですか？"},{"id":{"d":938,"n":4,"t":4},"speaker":"B","text":"はい。転んで骨折したので、手術して入院していたのです。"},{"id":{"d":938,"n":5,"t":4},"speaker":"A","text":"それは大変でしたね。"},{"id":{"d":938,"n":6,"t":4},"speaker":"B","text":"リハビリは大変でしたが、病院食のおかげで他の病気も治ったようでよかったです。"},{"id":{"d":938,"n":7,"t":4},"speaker":"A","text":"他の病気もあったのですか？"},{"id":{"d":938,"n":8,"t":4},"speaker":"B","text":"糖尿病気味だったのですが、数値が改善されました。"}];
INSERT INTO sentence [{"id":{"d":939,"n":1,"t":4},"speaker":"A","text":"夏場所の番付が発表されましたね。"},{"id":{"d":939,"n":2,"t":4},"speaker":"B","text":"今場所は新入幕の力士が多くて期待しています。今年も観戦されるのですか？"},{"id":{"d":939,"n":3,"t":4},"speaker":"A","text":"はい。今場所は「和装day」に行く予定です。"},{"id":{"d":939,"n":4,"t":4},"speaker":"B","text":"良いですね。着物は家から着ていくのですか？"},{"id":{"d":939,"n":5,"t":4},"speaker":"A","text":"いいえ。家から着ていくと国技館に到着するまでに着崩れてしまいますから、現地でレンタルします。"},{"id":{"d":939,"n":6,"t":4},"speaker":"B","text":"両国のレンタル着物屋さんを利用するのですか？"},{"id":{"d":939,"n":7,"t":4},"speaker":"A","text":"国技館の中のレンタル着物屋です。早めに予約して利用すると、記念品ももらえるのですよ。"},{"id":{"d":939,"n":8,"t":4},"speaker":"B","text":"それは良いですね。いつかは私も和装dayにチャレンジしたいです。"}];
INSERT INTO sentence [{"id":{"d":940,"n":1,"t":4},"speaker":"A","text":"うちの子は、注射をする時でも平気な顔で笑っているのですよ。"},{"id":{"d":940,"n":2,"t":4},"speaker":"B","text":"それは素晴らしいですね。うちは泣き叫んで大変でしたよ。"},{"id":{"d":940,"n":3,"t":4},"speaker":"A","text":"手間がかからなくていいとは思いますが、感じ方が鈍いのではないかと心配になります。"},{"id":{"d":940,"n":4,"t":4},"speaker":"B","text":"発達に問題はないのでしょう？"},{"id":{"d":940,"n":5,"t":4},"speaker":"A","text":"はい。今のところは、何も指摘されていません。"},{"id":{"d":940,"n":6,"t":4},"speaker":"B","text":"いつもご機嫌で笑っていられるのは良いことだと思いますよ。"},{"id":{"d":940,"n":7,"t":4},"speaker":"A","text":"そう言っていただけると安心します。"},{"id":{"d":940,"n":8,"t":4},"speaker":"B","text":"予防接種となると泣いてばかりいるうちの子に見習ってほしいです。"}];
INSERT INTO sentence [{"id":{"d":941,"n":1,"t":4},"speaker":"A","text":"少し痩せましたか？"},{"id":{"d":941,"n":2,"t":4},"speaker":"B","text":"分かりますか？最近、筋力トレーニングらしきものを始めたのです。"},{"id":{"d":941,"n":3,"t":4},"speaker":"A","text":"筋力トレーニングらしきものとは何ですか？ジムに行っているのではないのですか？"},{"id":{"d":941,"n":4,"t":4},"speaker":"B","text":"いいえ、トレーニングの動画を見て、それをまねているだけです。"},{"id":{"d":941,"n":5,"t":4},"speaker":"A","text":"それで筋力トレーニングらしきものというのですね。"},{"id":{"d":941,"n":6,"t":4},"speaker":"B","text":"正しいかどうかが分からない情報ですから。でも少しは痩せて、筋肉が付きましたよ。"},{"id":{"d":941,"n":7,"t":4},"speaker":"A","text":"確かに、体形が変わっていますね。"},{"id":{"d":941,"n":8,"t":4},"speaker":"B","text":"とても手軽にできるので、一緒にやってみませんか？"}];
INSERT INTO sentence [{"id":{"d":942,"n":1,"t":4},"speaker":"A","text":"口紅の色選びを失敗してしまいました。"},{"id":{"d":942,"n":2,"t":4},"speaker":"B","text":"試供品がなかったのですか？"},{"id":{"d":942,"n":3,"t":4},"speaker":"A","text":"ありましたが、口紅の試供品は唇に付けてみるわけにはいきませんよ。"},{"id":{"d":942,"n":4,"t":4},"speaker":"B","text":"そうですね。手の甲に付けたりしますね。"},{"id":{"d":942,"n":5,"t":4},"speaker":"A","text":"唇と手の甲では色が違うので、手の甲で試して買ったものを唇に塗ったら、想像していたイメージと違ったのです。"},{"id":{"d":942,"n":6,"t":4},"speaker":"B","text":"そういうことなのですね。"},{"id":{"d":942,"n":7,"t":4},"speaker":"A","text":"高かったのに返品もできないし、悲しいです。"},{"id":{"d":942,"n":8,"t":4},"speaker":"B","text":"きっと、そういう色を付けたくなる日もありますよ。"}];
INSERT INTO sentence [{"id":{"d":943,"n":1,"t":4},"speaker":"A","text":"エナジードリンクを買ってきてください。"},{"id":{"d":943,"n":2,"t":4},"speaker":"B","text":"最近は飲み過ぎではありませんか？"},{"id":{"d":943,"n":3,"t":4},"speaker":"A","text":"あれを飲まないと、夜中まで起きていられません。"},{"id":{"d":943,"n":4,"t":4},"speaker":"B","text":"エナジードリンクは糖分も多いと聞きますよ。"},{"id":{"d":943,"n":5,"t":4},"speaker":"A","text":"確かに、少し太ってきたような気はしています。"},{"id":{"d":943,"n":6,"t":4},"speaker":"B","text":"カフェインの取り過ぎも良くないのではありませんか？"},{"id":{"d":943,"n":7,"t":4},"speaker":"A","text":"1日1本にしますから、お願いします。"},{"id":{"d":943,"n":8,"t":4},"speaker":"B","text":"せめて2日に1本にしてください。"}];
INSERT INTO sentence [{"id":{"d":944,"n":1,"t":4},"speaker":"A","text":"ナイターの延長戦はテレビ中継されないのですね。"},{"id":{"d":944,"n":2,"t":4},"speaker":"B","text":"最近は延長戦まで放送されることが少なくなりました。"},{"id":{"d":944,"n":3,"t":4},"speaker":"A","text":"このようなことなら球場に行っておくのでした。"},{"id":{"d":944,"n":4,"t":4},"speaker":"B","text":"今からでは遅いですよ。"},{"id":{"d":944,"n":5,"t":4},"speaker":"A","text":"分かっています。結果を知るには、インターネットの速報を見るしか方法がないのですね。"},{"id":{"d":944,"n":6,"t":4},"speaker":"B","text":"わが家ではそうですね。"},{"id":{"d":944,"n":7,"t":4},"speaker":"A","text":"インターネット以外の方法もあるのですか？"},{"id":{"d":944,"n":8,"t":4},"speaker":"B","text":"はい。有料チャンネルを契約すればテレビで見られるそうですよ。"}];
INSERT INTO sentence [{"id":{"d":945,"n":1,"t":4},"speaker":"A","text":"カッターで手を切ってしまいました。"},{"id":{"d":945,"n":2,"t":4},"speaker":"B","text":"ばんそうこうを貼れるくらいの傷ですか？"},{"id":{"d":945,"n":3,"t":4},"speaker":"A","text":"はい。ばんそうこうがあれば大丈夫だと思います。"},{"id":{"d":945,"n":4,"t":4},"speaker":"B","text":"ばんそうこうは薬箱に入っています。手を洗って貼っておきなさい。"},{"id":{"d":945,"n":5,"t":4},"speaker":"A","text":"お母さん、薬箱にばんそうこうがありません。ばんそうこうの空き箱しか入っていません。"},{"id":{"d":945,"n":6,"t":4},"speaker":"B","text":"使いきってしまったのですね。"},{"id":{"d":945,"n":7,"t":4},"speaker":"A","text":"どうしたらよいですか？"},{"id":{"d":945,"n":8,"t":4},"speaker":"B","text":"そのままティッシュペーパーで押さえておいてください。今から、急いでばんそうこうを買ってきます。"}];
INSERT INTO sentence [{"id":{"d":946,"n":1,"t":4},"speaker":"A","text":"手洗いには液体せっけんを使っているのですか？"},{"id":{"d":946,"n":2,"t":4},"speaker":"B","text":"はい。わが家はいつもそれで手を洗っています。"},{"id":{"d":946,"n":3,"t":4},"speaker":"A","text":"うちは固形せっけんしか使ったことがありません。"},{"id":{"d":946,"n":4,"t":4},"speaker":"B","text":"固形せっけんはせっけん置きの掃除も大変なので、液体せっけんに替えたのです。"},{"id":{"d":946,"n":5,"t":4},"speaker":"A","text":"確かにそうですね。わが家も液体せっけんに替えたくなりました。"},{"id":{"d":946,"n":6,"t":4},"speaker":"B","text":"詰め替え用も安く売っていますし、お薦めですよ。"}];
INSERT INTO sentence [{"id":{"d":947,"n":1,"t":4},"speaker":"A","text":"そこのグラウンドでは、いつも男の人がたくさん走っていますね。"},{"id":{"d":947,"n":2,"t":4},"speaker":"B","text":"あれは大学の駅伝部の人たちですよ。"},{"id":{"d":947,"n":3,"t":4},"speaker":"A","text":"大学の駅伝というと、箱根駅伝に出たりするチームですか？"},{"id":{"d":947,"n":4,"t":4},"speaker":"B","text":"そうです。あの中には、今年のお正月に箱根駅伝を走った選手もいますよ。"},{"id":{"d":947,"n":5,"t":4},"speaker":"A","text":"そのようなスターが地元にいるなんて知りませんでした。"},{"id":{"d":947,"n":6,"t":4},"speaker":"B","text":"駅伝はお好きなのですか？"},{"id":{"d":947,"n":7,"t":4},"speaker":"A","text":"はい。箱根駅伝は欠かさず見ています。"}];
INSERT INTO sentence [{"id":{"d":948,"n":1,"t":4},"speaker":"A","text":"このお菓子には保存料が入っていますか？"},{"id":{"d":948,"n":2,"t":4},"speaker":"B","text":"いいえ、入っていません。そのため、あまり日持ちがしません。"},{"id":{"d":948,"n":3,"t":4},"speaker":"A","text":"今日中に食べれば大丈夫ですか？"},{"id":{"d":948,"n":4,"t":4},"speaker":"B","text":"冷蔵庫で保存していただければ、明日まで大丈夫です。"},{"id":{"d":948,"n":5,"t":4},"speaker":"A","text":"そうですか。では、そちらをいただきます。"},{"id":{"d":948,"n":6,"t":4},"speaker":"B","text":"おいくつ必要ですか？"},{"id":{"d":948,"n":7,"t":4},"speaker":"A","text":"5個ください。"},{"id":{"d":948,"n":8,"t":4},"speaker":"B","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":949,"n":1,"t":4},"speaker":"A","text":"昨日雨にぬれたせいか、風邪をひいてしまったようですね。"},{"id":{"d":949,"n":2,"t":4},"speaker":"B","text":"はい。今日は学校を休んだ方がいいでしょうか？"},{"id":{"d":949,"n":3,"t":4},"speaker":"A","text":"熱を測ってみましたか？"},{"id":{"d":949,"n":4,"t":4},"speaker":"B","text":"36度9分です。"},{"id":{"d":949,"n":5,"t":4},"speaker":"A","text":"微熱がありますね。学校はお休みしましょう。"},{"id":{"d":949,"n":6,"t":4},"speaker":"B","text":"分かりました。今日は美玖ちゃんと一緒に学校に行く約束をしていたのに、残念です。"},{"id":{"d":949,"n":7,"t":4},"speaker":"A","text":"美玖ちゃんには私が電話をして、あなたが今日学校を休むことを伝えておきますよ。"},{"id":{"d":949,"n":8,"t":4},"speaker":"B","text":"お願いします。美玖ちゃんに「ごめんなさい」と伝えてください。"}];
INSERT INTO sentence [{"id":{"d":950,"n":1,"t":4},"speaker":"A","text":"プロ野球中継はまだ放送されているのですか？"},{"id":{"d":950,"n":2,"t":4},"speaker":"B","text":"はい。日本シリーズの第2戦の真最中ですよ。"},{"id":{"d":950,"n":3,"t":4},"speaker":"A","text":"では、他のチャンネルは見られませんね。今日はアニメーションが見られると思っていたのに残念です。"},{"id":{"d":950,"n":4,"t":4},"speaker":"B","text":"野球も面白いですよ。一緒に見ませんか？"},{"id":{"d":950,"n":5,"t":4},"speaker":"A","text":"お父さんはそう思うかもしれませんが、僕はそうは思いません。部屋で宿題をしてきます。"},{"id":{"d":950,"n":6,"t":4},"speaker":"B","text":"そうですか。宿題をするのは偉いですね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":951,"n":1,"t":4},"speaker":"A","text":"手がかゆいのです。それに、少し腫れてもいるようです。"},{"id":{"d":951,"n":2,"t":4},"speaker":"B","text":"その手は、しもやけではありませんか？"},{"id":{"d":951,"n":3,"t":4},"speaker":"A","text":"そういえば、昨日は寒かったのに手袋を忘れて出かけてしまいました。"},{"id":{"d":951,"n":4,"t":4},"speaker":"B","text":"やはりしもやけの可能性が高そうです。"},{"id":{"d":951,"n":5,"t":4},"speaker":"A","text":"しもやけは、どのようにしたら治るのですか？"},{"id":{"d":951,"n":6,"t":4},"speaker":"B","text":"軟こうを付けて、暖かくしておけばよいと思いますよ。"},{"id":{"d":951,"n":7,"t":4},"speaker":"A","text":"どのような軟こうでもよいのですか？"},{"id":{"d":951,"n":8,"t":4},"speaker":"B","text":"ドラッグストアにはしもやけ用の軟こうも売っていると思います。それが一番効くと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":952,"n":1,"t":4},"speaker":"A","text":"この部屋はクーラーが効き過ぎていて、寒くてたまりません。"},{"id":{"d":952,"n":2,"t":4},"speaker":"B","text":"私はそこまで寒くはありませんよ。"},{"id":{"d":952,"n":3,"t":4},"speaker":"A","text":"そうですか？私にとっては、夏なのに、カーディガンとひざ掛けが必要なほど寒いです。"},{"id":{"d":952,"n":4,"t":4},"speaker":"B","text":"志田さんは冷え性でしょうか？"},{"id":{"d":952,"n":5,"t":4},"speaker":"A","text":"そうかもしれません。手も足もいつも冷たいです。"},{"id":{"d":952,"n":6,"t":4},"speaker":"B","text":"クーラーはビル全体で管理されているので設定温度を変えるのは難しいかもしれません。とりあえず温かいお茶を飲むといいですよ。"},{"id":{"d":952,"n":7,"t":4},"speaker":"A","text":"普段からなるべく飲むようにはしていますが、それでも寒いです。"},{"id":{"d":952,"n":8,"t":4},"speaker":"B","text":"お昼はしょうが焼きでも食べに行きましょう。しょうがは体を温めてくれますよ。"}];
INSERT INTO sentence [{"id":{"d":953,"n":1,"t":4},"speaker":"A","text":"健康診断でコレステロール値が高いと言われました。"},{"id":{"d":953,"n":2,"t":4},"speaker":"B","text":"私はなるべくヘルシーな料理を出しているつもりですが、もっと気を付けた方がいいのかもしれませんね。"},{"id":{"d":953,"n":3,"t":4},"speaker":"A","text":"コレステロール値が高いのは、食べ物のせいなのですか？"},{"id":{"d":953,"n":4,"t":4},"speaker":"B","text":"もちろん関係していますよ。普段、お昼は何を食べているのですか？"},{"id":{"d":953,"n":5,"t":4},"speaker":"A","text":"会社の近くの店のランチセットです。"},{"id":{"d":953,"n":6,"t":4},"speaker":"B","text":"どのようなメニューですか？"},{"id":{"d":953,"n":7,"t":4},"speaker":"A","text":"今日はフォアグラを食べて、昨日はエビと白子の天丼を食べました。"},{"id":{"d":953,"n":8,"t":4},"speaker":"B","text":"そのような食事が続いているのなら、コレステロールも高くなって当然でしょう。当分は、フォアグラも白子もエビも控えてください。"}];
INSERT INTO sentence [{"id":{"d":954,"n":1,"t":4},"speaker":"A","text":"今日の仕事帰りに、サウナへ行きませんか？"},{"id":{"d":954,"n":2,"t":4},"speaker":"B","text":"いいですね。でも、今日は化粧品を持ってきていません。"},{"id":{"d":954,"n":3,"t":4},"speaker":"A","text":"珍しいですね。忘れてきたのですか？"},{"id":{"d":954,"n":4,"t":4},"speaker":"B","text":"ファンデーションやアイメーク用の化粧品は持っていますが、化粧水と乳液がありません。"},{"id":{"d":954,"n":5,"t":4},"speaker":"A","text":"私のものでよければ貸しますよ。"},{"id":{"d":954,"n":6,"t":4},"speaker":"B","text":"よいのですか？ありがとうございます。"},{"id":{"d":954,"n":7,"t":4},"speaker":"A","text":"普段は、基礎化粧品は持ち歩かないのですか？"},{"id":{"d":954,"n":8,"t":4},"speaker":"B","text":"はい。少し直すだけですから、基礎化粧品は持ち歩いていません。"},{"id":{"d":954,"n":9,"t":4},"speaker":"A","text":"そうなのですね。では、次にサウナに誘うときは、前日までに声を掛けるようにします。"},{"id":{"d":954,"n":10,"t":4},"speaker":"B","text":"ありがとうございます。そうしていただけると、基礎化粧品を準備していけるので安心です。"}];
INSERT INTO sentence [{"id":{"d":955,"n":1,"t":4},"speaker":"A","text":"この公園にはテニスコートがあるのですね。"},{"id":{"d":955,"n":2,"t":4},"speaker":"B","text":"そうです。事前に予約をして、料金を払えば使えるようです。"},{"id":{"d":955,"n":3,"t":4},"speaker":"A","text":"こちらの方に来たことがなかったので知りませんでした。"},{"id":{"d":955,"n":4,"t":4},"speaker":"B","text":"テニススクールもあるようですよ。"},{"id":{"d":955,"n":5,"t":4},"speaker":"A","text":"実は、私は中学ではソフトテニス部だったのですよ。懐かしいです。"},{"id":{"d":955,"n":6,"t":4},"speaker":"B","text":"私は球技はからきしなのでご一緒できませんが、テニス部時代のお友達を誘ってみたらいかがですか？"},{"id":{"d":955,"n":7,"t":4},"speaker":"A","text":"それは良い案です。同窓会のようなものになりますね。"},{"id":{"d":955,"n":8,"t":4},"speaker":"B","text":"予約の問い合わせ先はこの看板に書いてありますよ。"}];
INSERT INTO sentence [{"id":{"d":956,"n":1,"t":4},"speaker":"A","text":"太郎君、随分元気になりましたね。"},{"id":{"d":956,"n":2,"t":4},"speaker":"B","text":"おかげさまで、週末には退院できるそうです。"},{"id":{"d":956,"n":3,"t":4},"speaker":"A","text":"元気になってよかったです。入院した時は大変でしたものね。"},{"id":{"d":956,"n":4,"t":4},"speaker":"B","text":"はい。熱は下がらないし、息は苦しそうだしで、本当に心配しました。"},{"id":{"d":956,"n":5,"t":4},"speaker":"A","text":"週末にはお父さんもお迎えにいらっしゃるのですか？"},{"id":{"d":956,"n":6,"t":4},"speaker":"B","text":"はい。仕事が休みですので、車で迎えに来ます。"},{"id":{"d":956,"n":7,"t":4},"speaker":"A","text":"そうですか。では、退院当日は10時に書類を届けに来ますね。"},{"id":{"d":956,"n":8,"t":4},"speaker":"B","text":"はい。それまでに支度をしておきます。"}];
INSERT INTO sentence [{"id":{"d":957,"n":1,"t":4},"speaker":"A","text":"会議の間、眠そうでしたね。"},{"id":{"d":957,"n":2,"t":4},"speaker":"B","text":"すみません。まさかこのようなことになるとは思いませんでした。"},{"id":{"d":957,"n":3,"t":4},"speaker":"A","text":"どうしたのですか？"},{"id":{"d":957,"n":4,"t":4},"speaker":"B","text":"朝に、鼻炎薬を飲んだのですが、それは眠くなってしまう薬だったようです。"},{"id":{"d":957,"n":5,"t":4},"speaker":"A","text":"今日は車で来たのではありませんか？"},{"id":{"d":957,"n":6,"t":4},"speaker":"B","text":"はい、車で来ました。"},{"id":{"d":957,"n":7,"t":4},"speaker":"A","text":"帰りまであくびが出るほど眠いようなら、車は置いて電車で帰った方がいいですよ。"},{"id":{"d":957,"n":8,"t":4},"speaker":"B","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":958,"n":1,"t":4},"speaker":"A","text":"度付きのサングラスが欲しいのですが、ありますか？"},{"id":{"d":958,"n":2,"t":4},"speaker":"B","text":"いろいろございますが、どのような時に使われますか？"},{"id":{"d":958,"n":3,"t":4},"speaker":"A","text":"マラソン大会に出ようと思っているのです。前に普通の眼鏡で出た時は、まぶしくてリタイアしてしまいました。"},{"id":{"d":958,"n":4,"t":4},"speaker":"B","text":"ランニング用のサングラスですね。この辺りの商品でしたらお客様に適していると思いますよ。"},{"id":{"d":958,"n":5,"t":4},"speaker":"A","text":"このサイングラスは、とても軽いですね。"},{"id":{"d":958,"n":6,"t":4},"speaker":"B","text":"はい。軽くて走ってもずれません。雨や汗にも強いですよ。"},{"id":{"d":958,"n":7,"t":4},"speaker":"A","text":"こちらはUVカットレンズを使っていますか？"},{"id":{"d":958,"n":8,"t":4},"speaker":"B","text":"もちろんレンズはUVカット付きです。"}];
INSERT INTO sentence [{"id":{"d":959,"n":1,"t":4},"speaker":"A","text":"めまいがひどいので病院に行きました。"},{"id":{"d":959,"n":2,"t":4},"speaker":"B","text":"それは心配ですね。何かの病気だったのですか？"},{"id":{"d":959,"n":3,"t":4},"speaker":"A","text":"メニエール病ではないかと言われました。"},{"id":{"d":959,"n":4,"t":4},"speaker":"B","text":"その病気は、私の叔母がかかったことがあります。"},{"id":{"d":959,"n":5,"t":4},"speaker":"A","text":"叔母さんの病気は治ったのですか？"},{"id":{"d":959,"n":6,"t":4},"speaker":"B","text":"お薬をもらっていたようですが、その後の様子は聞いていません。"},{"id":{"d":959,"n":7,"t":4},"speaker":"A","text":"私も薬を処方されました。"},{"id":{"d":959,"n":8,"t":4},"speaker":"B","text":"しばらくは安静を心掛けて様子を見た方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":960,"n":1,"t":4},"speaker":"A","text":"お母さん、トリートメントがありません。"},{"id":{"d":960,"n":2,"t":4},"speaker":"B","text":"すみません、言い忘れていましたが、今日からはコンディショナーにしました。"},{"id":{"d":960,"n":3,"t":4},"speaker":"A","text":"トリートメントの代わりにコンディショナーを使えばよいのですか？"},{"id":{"d":960,"n":4,"t":4},"speaker":"B","text":"そうです。前のトリートメントより少し値段が高いものにしたので、大切に使ってくださいね。"},{"id":{"d":960,"n":5,"t":4},"speaker":"A","text":"そのようなことを言われたら、ほんの少ししか使えなくなってしまいそうです。"},{"id":{"d":960,"n":6,"t":4},"speaker":"B","text":"普通にポンプをひと押しすればよいですよ。"},{"id":{"d":960,"n":7,"t":4},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":961,"n":1,"t":4},"speaker":"A","text":"階段を上がるときに息切れを起こすようになりました。"},{"id":{"d":961,"n":2,"t":4},"speaker":"B","text":"私も同じです。年を取ったということでしょうか？"},{"id":{"d":961,"n":3,"t":4},"speaker":"A","text":"私は長年の運動不足がたたったのだと思います。"},{"id":{"d":961,"n":4,"t":4},"speaker":"B","text":"私も運動不足気味ですね。"},{"id":{"d":961,"n":5,"t":4},"speaker":"A","text":"少し歩かないと足腰が駄目になってしまいますね。"},{"id":{"d":961,"n":6,"t":4},"speaker":"B","text":"そうですね。私は犬の散歩を息子にさせてしまっていたのですが、今度からは自分で行くようにします。"},{"id":{"d":961,"n":7,"t":4},"speaker":"A","text":"犬を飼っていると、歩くきっかけができて良いかもしれませんね。"},{"id":{"d":961,"n":8,"t":4},"speaker":"B","text":"あなたも犬を飼ってはいかがですか？"},{"id":{"d":961,"n":9,"t":4},"speaker":"A","text":"考えてみます。"}];
INSERT INTO sentence [{"id":{"d":962,"n":1,"t":4},"speaker":"A","text":"本屋さんに来ると、いつも、店員の方が本をきれいに並べ直していますよね。"},{"id":{"d":962,"n":2,"t":4},"speaker":"B","text":"長年、本屋に勤めている人の職業病ですよ。"},{"id":{"d":962,"n":3,"t":4},"speaker":"A","text":"確かに、それは職業病と言えそうです。そういえば、あなたも本屋で働いていましたね。"},{"id":{"d":962,"n":4,"t":4},"speaker":"B","text":"はい。しかし、本屋の本当の職業病はもっと深刻です。"},{"id":{"d":962,"n":5,"t":4},"speaker":"A","text":"それは何ですか？"},{"id":{"d":962,"n":6,"t":4},"speaker":"B","text":"腰痛です。本屋は肉体労働なのです。"},{"id":{"d":962,"n":7,"t":4},"speaker":"A","text":"本は重いですからね。持ち運ぶのが大変そうです。"},{"id":{"d":962,"n":8,"t":4},"speaker":"B","text":"はい。品出しの時は本当に大変です。"}];
INSERT INTO sentence [{"id":{"d":963,"n":1,"t":4},"speaker":"A","text":"捻挫をしてしまったので、明日のお出かけには行けなくなりました。すみません。"},{"id":{"d":963,"n":2,"t":4},"speaker":"B","text":"明日は久しぶりにウインドーショッピングを楽しもうと思っていたのに残念です。"},{"id":{"d":963,"n":3,"t":4},"speaker":"A","text":"本当にごめんなさい。とても痛くて、歩くのもやっとの状態なのです。"},{"id":{"d":963,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。どこで捻挫をしてしまったのですか？"},{"id":{"d":963,"n":5,"t":4},"speaker":"A","text":"昨日、コンビニエンスストアの駐車場で、車から降りて歩き出そうとしたところに車止めがあったのです。その車止めでつまずいてしまいました。"},{"id":{"d":963,"n":6,"t":4},"speaker":"B","text":"車止めには、私もつまずきそうになったことがあります。大きな事故にならなくてよかったですね。"},{"id":{"d":963,"n":7,"t":4},"speaker":"A","text":"はい。ぼんやりと歩いていてはいけませんね。"},{"id":{"d":963,"n":8,"t":4},"speaker":"B","text":"気を付けてくださいね。明日は、そちらにお見舞いを持って行きますよ。"}];
INSERT INTO sentence [{"id":{"d":964,"n":1,"t":4},"speaker":"A","text":"あなたは何かスポーツをしていますか？"},{"id":{"d":964,"n":2,"t":4},"speaker":"B","text":"私はサッカーをしています。"},{"id":{"d":964,"n":3,"t":4},"speaker":"A","text":"学校の部活動でしているのですか？それともどこかのクラブに入っているのですか？"},{"id":{"d":964,"n":4,"t":4},"speaker":"B","text":"地元の少年サッカークラブに入っています。"},{"id":{"d":964,"n":5,"t":4},"speaker":"A","text":"クラブにはいつから入っているのですか？"},{"id":{"d":964,"n":6,"t":4},"speaker":"B","text":"小学校に入ってからですから、もう6年になります。"},{"id":{"d":964,"n":7,"t":4},"speaker":"A","text":"ポジションはどこですか？"},{"id":{"d":964,"n":8,"t":4},"speaker":"B","text":"ボランチです。"},{"id":{"d":964,"n":9,"t":4},"speaker":"A","text":"ボランチは重要なポジションだそうですね。"},{"id":{"d":964,"n":10,"t":4},"speaker":"B","text":"はい、そうです。難しさもありますが、ゲームを組み立てることができるので、やりがいがあります。"}];
INSERT INTO sentence [{"id":{"d":965,"n":1,"t":4},"speaker":"A","text":"最近、スカッシュを始めました。"},{"id":{"d":965,"n":2,"t":4},"speaker":"B","text":"室内で壁に向かってボールを打ち合うスポーツですね。"},{"id":{"d":965,"n":3,"t":4},"speaker":"A","text":"そうです。あなたもやってみませんか？"},{"id":{"d":965,"n":4,"t":4},"speaker":"B","text":"スカッシュには興味はあるのですが、とても動きの激しいスポーツと聞いていて、自信がありません。"},{"id":{"d":965,"n":5,"t":4},"speaker":"A","text":"確かにハードなスポーツですが、とても面白いですよ。"},{"id":{"d":965,"n":6,"t":4},"speaker":"B","text":"私はあまり体力がないので、皆についていけるかどうか分かりません。"},{"id":{"d":965,"n":7,"t":4},"speaker":"A","text":"大丈夫ですよ。初めはゆっくりでいいのです。"},{"id":{"d":965,"n":8,"t":4},"speaker":"B","text":"そうですか？健康にも良さそうですし、私もやってみたいです。"},{"id":{"d":965,"n":9,"t":4},"speaker":"A","text":"では、私の入っているクラブを紹介しましょう。"},{"id":{"d":965,"n":10,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":966,"n":1,"t":4},"speaker":"A","text":"最近、健康のためにウォーキングを始めました。"},{"id":{"d":966,"n":2,"t":4},"speaker":"B","text":"それはいいですね。一日に歩く歩数のノルマは決めているのですか？"},{"id":{"d":966,"n":3,"t":4},"speaker":"A","text":"はい、一日に8,000歩以上は歩きたいと思っています。"},{"id":{"d":966,"n":4,"t":4},"speaker":"B","text":"8,000歩とは大変ですね。"},{"id":{"d":966,"n":5,"t":4},"speaker":"A","text":"普段の生活で歩いている分も含めた歩数ですから、ウォーキングそのものは5,000歩ぐらいです。"},{"id":{"d":966,"n":6,"t":4},"speaker":"B","text":"それでも結構な歩数ではありませんか。何か効果がありましたか？"},{"id":{"d":966,"n":7,"t":4},"speaker":"A","text":"まだ始めたばかりですから、目立った効果はありません。"},{"id":{"d":966,"n":8,"t":4},"speaker":"B","text":"続けていれば、そのうち効果が現れてくるでしょう。"},{"id":{"d":966,"n":9,"t":4},"speaker":"A","text":"私もそう信じて頑張るつもりです。"}];
INSERT INTO sentence [{"id":{"d":967,"n":1,"t":4},"speaker":"A","text":"しゃっくりが止まらなくて困っています。"},{"id":{"d":967,"n":2,"t":4},"speaker":"B","text":"それはお困りでしょう。止める方法はいろいろとありますよね。"},{"id":{"d":967,"n":3,"t":4},"speaker":"A","text":"どのような方法でもよいから止まってほしいです。"},{"id":{"d":967,"n":4,"t":4},"speaker":"B","text":"不意に驚かす、という方法もありますよ。"},{"id":{"d":967,"n":5,"t":4},"speaker":"A","text":"でも、今ここであなたに「ワッ」と言われても、全然驚きませんよ。"},{"id":{"d":967,"n":6,"t":4},"speaker":"B","text":"では、鼻をこよりでくすぐってくしゃみをしたらどうでしょうか？"},{"id":{"d":967,"n":7,"t":4},"speaker":"A","text":"それは先ほど自分でやってみました。でも、効きませんでした。"},{"id":{"d":967,"n":8,"t":4},"speaker":"B","text":"昔からあるおまじないで、箸を頭の上に立てるというものもありますよ。"},{"id":{"d":967,"n":9,"t":4},"speaker":"A","text":"姿を想像するだけで笑いがこみ上げてきて、なおさらしゃっくりがひどくなりそうです。"}];
INSERT INTO sentence [{"id":{"d":968,"n":1,"t":4},"speaker":"A","text":"腰痛で悩んでいるのですが、どこかいい病院を知りませんか？"},{"id":{"d":968,"n":2,"t":4},"speaker":"B","text":"整形外科にかかった方がいいかもしれませんね。"},{"id":{"d":968,"n":3,"t":4},"speaker":"A","text":"そうですか。"},{"id":{"d":968,"n":4,"t":4},"speaker":"B","text":"まず整形外科に行って、レントゲン検査などの精密検査を受けるのがいいと思います。"},{"id":{"d":968,"n":5,"t":4},"speaker":"A","text":"確かに原因をはっきりさせる必要はありますね。"},{"id":{"d":968,"n":6,"t":4},"speaker":"B","text":"整形外科の他にも、整骨院のような東洋医学系の施療院に行く方法もあります。"},{"id":{"d":968,"n":7,"t":4},"speaker":"A","text":"分かりました。まず、整形外科の診察を受けてみます。"},{"id":{"d":968,"n":8,"t":4},"speaker":"B","text":"もしご希望なら私の知っている整形外科の病院を紹介しますよ。"},{"id":{"d":968,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。ぜひお願いします。"}];
INSERT INTO sentence [{"id":{"d":969,"n":1,"t":4},"speaker":"A","text":"お年はいくつですか？"},{"id":{"d":969,"n":2,"t":4},"speaker":"B","text":"今年で52歳になります。"},{"id":{"d":969,"n":3,"t":4},"speaker":"A","text":"人間ドックを受けたことがありますか？"},{"id":{"d":969,"n":4,"t":4},"speaker":"B","text":"いいえ、ありません。会社の健康診断を年に一回受けているだけです。"},{"id":{"d":969,"n":5,"t":4},"speaker":"A","text":"50歳を過ぎているなら、人間ドックも受けることをお勧めしますよ。"},{"id":{"d":969,"n":6,"t":4},"speaker":"B","text":"健康診断と人間ドックはどう違うのですか？"},{"id":{"d":969,"n":7,"t":4},"speaker":"A","text":"一番大きな違いは検査項目が多いことです。人間ドックでは40から100の項目について検査をします。"},{"id":{"d":969,"n":8,"t":4},"speaker":"B","text":"かなり多いですね。でも、もちろんそれだけ費用がかかるのでしょうね。"},{"id":{"d":969,"n":9,"t":4},"speaker":"A","text":"検査項目の数にもよりますが、大体、3万円から10万円ほどかかります。健康保険組合から補助が出る場合もあるので調べてみてはどうですか？"},{"id":{"d":969,"n":10,"t":4},"speaker":"B","text":"そうなのですね。早速調べてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":970,"n":1,"t":4},"speaker":"A","text":"母の体が不自由になったので、介護をお願いしたいと思っています。"},{"id":{"d":970,"n":2,"t":4},"speaker":"B","text":"介護を受けられる状態には、要支援状態と要介護状態とがあります。"},{"id":{"d":970,"n":3,"t":4},"speaker":"A","text":"どう違うのですか？"},{"id":{"d":970,"n":4,"t":4},"speaker":"B","text":"要支援状態というのは、基本的には一人で生活できる人が部分的に介護を必要とする状態です。"},{"id":{"d":970,"n":5,"t":4},"speaker":"A","text":"要介護状態というのはどういう状態ですか？"},{"id":{"d":970,"n":6,"t":4},"speaker":"B","text":"日常生活全般において自分一人で行うことが難しく、誰かの介護が必要な状態です。身体的に介護が必要な場合もありますし、思考力や理解力が低下して一人では生活できない場合も当てはまります。"},{"id":{"d":970,"n":7,"t":4},"speaker":"A","text":"母は認知機能も少し衰えているようですし、一人ではとても生活できません。"},{"id":{"d":970,"n":8,"t":4},"speaker":"B","text":"それなら介護サービスを受けることができます。まず、専門家による要介護度の認定が必要です。"},{"id":{"d":970,"n":9,"t":4},"speaker":"A","text":"その認定はどこで受けることができるのですか？"},{"id":{"d":970,"n":10,"t":4},"speaker":"B","text":"市町村の介護認定審査会で判定してもらいますが、まずは近くの地域包括支援センターや役所の高齢者福祉窓口に相談してみるといいですよ。"},{"id":{"d":970,"n":11,"t":4},"speaker":"A","text":"ありがとうございます。早速相談しようと思います。"}];
INSERT INTO sentence [{"id":{"d":971,"n":1,"t":4},"speaker":"A","text":"最近、奥歯がぐらぐらしてきて、かむと痛みがあります。"},{"id":{"d":971,"n":2,"t":4},"speaker":"B","text":"それは歯周病かもしれませんね。放っておくと大変なことになりますよ。"},{"id":{"d":971,"n":3,"t":4},"speaker":"A","text":"心配になってきました。"},{"id":{"d":971,"n":4,"t":4},"speaker":"B","text":"歯周病が原因で、重い病気になる場合もあるということも最近分かってきたのです。"},{"id":{"d":971,"n":5,"t":4},"speaker":"A","text":"どのような病気になるのですか？"},{"id":{"d":971,"n":6,"t":4},"speaker":"B","text":"例えば、歯周病が原因で糖尿病や脳梗塞などになることもあるそうです。"},{"id":{"d":971,"n":7,"t":4},"speaker":"A","text":"歯はきちんと磨いているのですが、それでも歯周病になるのですか？"},{"id":{"d":971,"n":8,"t":4},"speaker":"B","text":"歯茎の間の歯垢はなかなか歯磨きだけでは取れないので、デンタルフロスや歯間ブラシを使って歯垢を取らなくてはいけません。一度歯科医に診てもらったらいかがですか？"},{"id":{"d":971,"n":9,"t":4},"speaker":"A","text":"確かに歯垢は取っていませんでした。早めに歯科医院に行こうと思います。"}];
INSERT INTO sentence [{"id":{"d":972,"n":1,"t":4},"speaker":"A","text":"顔色が良くないようです。どうしましたか？"},{"id":{"d":972,"n":2,"t":4},"speaker":"B","text":"片頭痛がひどいのです。"},{"id":{"d":972,"n":3,"t":4},"speaker":"A","text":"どのような具合ですか？"},{"id":{"d":972,"n":4,"t":4},"speaker":"B","text":"頭の右側が絶えずズキンズキンと痛むのです。"},{"id":{"d":972,"n":5,"t":4},"speaker":"A","text":"吐き気はありますか？"},{"id":{"d":972,"n":6,"t":4},"speaker":"B","text":"はい、時々吐き気がします。"},{"id":{"d":972,"n":7,"t":4},"speaker":"A","text":"光や音の刺激があると、痛みに変化はありますか？"},{"id":{"d":972,"n":8,"t":4},"speaker":"B","text":"はい、特に光に当たると痛みが激しくなるようです。"},{"id":{"d":972,"n":9,"t":4},"speaker":"A","text":"分かりました。それでは、精密検査をしてみましょう。"},{"id":{"d":972,"n":10,"t":4},"speaker":"B","text":"治るでしょうか？"},{"id":{"d":972,"n":11,"t":4},"speaker":"A","text":"心配しなくても大丈夫ですよ。症状を軽くする薬もあります。"}];
INSERT INTO sentence [{"id":{"d":973,"n":1,"t":4},"speaker":"A","text":"ぜんそくで悩んでいます。夜中の3時ごろになると発作が起きるのです。"},{"id":{"d":973,"n":2,"t":4},"speaker":"B","text":"それはおつらいですね。どれくらい続いているのですか？"},{"id":{"d":973,"n":3,"t":4},"speaker":"A","text":"3年ほど前から続いています。"},{"id":{"d":973,"n":4,"t":4},"speaker":"B","text":"実は私も同じような症状に悩んでいたことがあります。ですから、つらさはよく分かります。"},{"id":{"d":973,"n":5,"t":4},"speaker":"A","text":"そうでしたか。あなたはもう治ったのですか？"},{"id":{"d":973,"n":6,"t":4},"speaker":"B","text":"いいえ、今でも時々せき込むことがありますが、対処方法を見つけてから軽くなりました。"},{"id":{"d":973,"n":7,"t":4},"speaker":"A","text":"どうするのですか？ぜひ教えてください。"},{"id":{"d":973,"n":8,"t":4},"speaker":"B","text":"インターネットでたまたま見つけた方法なのですが、湯たんぽをおなかの上に載せておくのです。"},{"id":{"d":973,"n":9,"t":4},"speaker":"A","text":"そのようなことで発作を抑えられるのですか？"},{"id":{"d":973,"n":10,"t":4},"speaker":"B","text":"はい、腸の免疫の働きを良くするのだそうです。効果は人によって違うと思いますが、私にはてきめんに効きました。"},{"id":{"d":973,"n":11,"t":4},"speaker":"A","text":"それでは、試しに私もやってみます。"}];
INSERT INTO sentence [{"id":{"d":974,"n":1,"t":4},"speaker":"A","text":"息子がそばアレルギーを持っています。"},{"id":{"d":974,"n":2,"t":4},"speaker":"B","text":"そうなのですね。どのような症状が出るのですか？"},{"id":{"d":974,"n":3,"t":4},"speaker":"A","text":"ぜんそくの発作やくしゃみが出ます。じんましんや嘔吐、下痢などの症状が出ることもあります。"},{"id":{"d":974,"n":4,"t":4},"speaker":"B","text":"息子さんはつらいでしょうね。日常生活にもよほど注意が必要なのでしょうね。"},{"id":{"d":974,"n":5,"t":4},"speaker":"A","text":"はい、麺のそばだけでなくいろいろな食べ物にそばが含まれていますから、それらにも注意しなくてはいけません。"},{"id":{"d":974,"n":6,"t":4},"speaker":"B","text":"どのような食べ物に含まれているのですか？"},{"id":{"d":974,"n":7,"t":4},"speaker":"A","text":"例えば、五穀米などの雑穀や、和菓子にも含まれています。"},{"id":{"d":974,"n":8,"t":4},"speaker":"B","text":"成分にまで気を配らなければいけないのですね。"},{"id":{"d":974,"n":9,"t":4},"speaker":"A","text":"それだけではありません。ひどいときには、そば屋の前を通るだけで発作を起こすこともあります。"},{"id":{"d":974,"n":10,"t":4},"speaker":"B","text":"そばアレルギーがそれほど大変なものだとは知りませんでした。どうかお大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":975,"n":1,"t":4},"speaker":"A","text":"あなたは睡眠時無呼吸症候群の疑いがありますね。"},{"id":{"d":975,"n":2,"t":4},"speaker":"B","text":"やはりそうですか。妻に言われてそうかもしれないと思っていたのです。"},{"id":{"d":975,"n":3,"t":4},"speaker":"A","text":"奥さまはどう言っておられますか？"},{"id":{"d":975,"n":4,"t":4},"speaker":"B","text":"大きないびきをかいていたのが急に止まって、無呼吸の状態が長い間続くようだと言っています。"},{"id":{"d":975,"n":5,"t":4},"speaker":"A","text":"それは、典型的な睡眠時無呼吸症候群の症状ですね。"},{"id":{"d":975,"n":6,"t":4},"speaker":"B","text":"放っておくと良くありませんよね？"},{"id":{"d":975,"n":7,"t":4},"speaker":"A","text":"そうです。高血圧や脳卒中、心筋梗塞が起こりやすくなります。"},{"id":{"d":975,"n":8,"t":4},"speaker":"B","text":"それは怖いですね。どうしたらよいのですか？"},{"id":{"d":975,"n":9,"t":4},"speaker":"A","text":"まず、病状を把握するために精密検査をしてみましょう。"},{"id":{"d":975,"n":10,"t":4},"speaker":"B","text":"どのような検査をするのですか？"},{"id":{"d":975,"n":11,"t":4},"speaker":"A","text":"睡眠時の脳波や呼吸、心電図などを調べる睡眠ポリグラフ検査をしてみましょうか？病状が分かりましたら、それに応じて治療方針を決めます。"},{"id":{"d":975,"n":12,"t":4},"speaker":"B","text":"分かりました。どうぞよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":976,"n":1,"t":4},"speaker":"A","text":"血圧が低くて、朝起きるのがつらいです。"},{"id":{"d":976,"n":2,"t":4},"speaker":"B","text":"血圧は測っていますか？"},{"id":{"d":976,"n":3,"t":4},"speaker":"A","text":"はい。たいてい最高血圧が100以下で、最低血圧が60以下です。"},{"id":{"d":976,"n":4,"t":4},"speaker":"B","text":"めまいや立ちくらみはありますか？"},{"id":{"d":976,"n":5,"t":4},"speaker":"A","text":"それはあまりありません。"},{"id":{"d":976,"n":6,"t":4},"speaker":"B","text":"それならまだ軽い方ですね。頭痛や倦怠感、不眠といった症状の出る方もいます。"},{"id":{"d":976,"n":7,"t":4},"speaker":"A","text":"時々眠れなくなることがあります。どのようなことに気を付ければよいのですか？"},{"id":{"d":976,"n":8,"t":4},"speaker":"B","text":"栄養バランスの取れた食事を3食取ることと、水分をしっかりと取ることが大切です。手や足を鍛える運動をするのも良いでしょう。"},{"id":{"d":976,"n":9,"t":4},"speaker":"A","text":"分かりました。生活を少し改善することにします。"}];
INSERT INTO sentence [{"id":{"d":977,"n":1,"t":4},"speaker":"A","text":"来週から、岐阜の御嶽山で高地トレーニングをするつもりです。"},{"id":{"d":977,"n":2,"t":4},"speaker":"B","text":"高地トレーニングといえば、コロラド州のボルダーが有名ですが、日本にもそのような場所があるのですか？"},{"id":{"d":977,"n":3,"t":4},"speaker":"A","text":"はい、日本でもいくつか高地トレーニングができるところがあります。最近は飛騨御嶽高原高地トレーニングエリアがトレンドです。"},{"id":{"d":977,"n":4,"t":4},"speaker":"B","text":"御嶽山といえば、数年前に大噴火があって犠牲者がたくさん出ましたね。トレーニングをしても大丈夫なのですか？"},{"id":{"d":977,"n":5,"t":4},"speaker":"A","text":"トレーニングエリアは制限区域の外にあるので問題ありません。"},{"id":{"d":977,"n":6,"t":4},"speaker":"B","text":"高地トレーニングはやはり効果がありますか？"},{"id":{"d":977,"n":7,"t":4},"speaker":"A","text":"はい、肺活量や持久力が向上します。マラソン選手にとっては大きな利点があります。"},{"id":{"d":977,"n":8,"t":4},"speaker":"B","text":"どのくらいの期間、トレーニングをするのですか？"},{"id":{"d":977,"n":9,"t":4},"speaker":"A","text":"今回は1カ月の予定です。次の大会に合わせて日程を調整しています。"},{"id":{"d":977,"n":10,"t":4},"speaker":"B","text":"大会で良い結果が出るといいですね。私も応援しています。"},{"id":{"d":977,"n":11,"t":4},"speaker":"A","text":"どうもありがとうございます。良い結果が出るように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":978,"n":1,"t":4},"speaker":"A","text":"靴擦れで足が痛いです。"},{"id":{"d":978,"n":2,"t":4},"speaker":"B","text":"少し見せてください。なるほど、かかとが赤くなっていますね。"},{"id":{"d":978,"n":3,"t":4},"speaker":"A","text":"新しい靴を履いてきたのですが、失敗でした。"},{"id":{"d":978,"n":4,"t":4},"speaker":"B","text":"足になじんでいなかったのですね。"},{"id":{"d":978,"n":5,"t":4},"speaker":"A","text":"皆さんにご迷惑をかけてしまってすみません。"},{"id":{"d":978,"n":6,"t":4},"speaker":"B","text":"ばんそうこうは持っていますか？"},{"id":{"d":978,"n":7,"t":4},"speaker":"A","text":"いいえ、持っていません。"},{"id":{"d":978,"n":8,"t":4},"speaker":"B","text":"それでは、応急処置としてティッシュペーパーを靴とかかとの間に挟んでおきましょう。"},{"id":{"d":978,"n":9,"t":4},"speaker":"A","text":"随分と楽になりました。"},{"id":{"d":978,"n":10,"t":4},"speaker":"B","text":"それでしばらくは我慢できると思います。"},{"id":{"d":978,"n":11,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":979,"n":1,"t":4},"speaker":"A","text":"黒くて艶がある髪ですね。白髪染めを使っているのですか？"},{"id":{"d":979,"n":2,"t":4},"speaker":"B","text":"分かりますか？行きつけの床屋に勧められました。"},{"id":{"d":979,"n":3,"t":4},"speaker":"A","text":"私も髪を染めたいと思っているのですが、髪を傷めるのではないかと思ってためらっています。"},{"id":{"d":979,"n":4,"t":4},"speaker":"B","text":"確かに市販のヘアカラーには髪にあまり良くない成分を含んでいるものもあるようです。"},{"id":{"d":979,"n":5,"t":4},"speaker":"A","text":"その床屋さんは、何か特別なヘアカラーを使っているのですか？"},{"id":{"d":979,"n":6,"t":4},"speaker":"B","text":"はい。無添加の天然成分が入ったヘアカラーを使っていて、髪を傷めることが少ないそうです。"},{"id":{"d":979,"n":7,"t":4},"speaker":"A","text":"それはいいですね。私もやってみたいです。"},{"id":{"d":979,"n":8,"t":4},"speaker":"B","text":"ぜひやってみるといいですよ。床屋さんを紹介しましょう。"},{"id":{"d":979,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。ぜひお願いします。"}];
INSERT INTO sentence [{"id":{"d":980,"n":1,"t":4},"speaker":"A","text":"自宅で筋力トレーニングをしようと思うのですが、何かいい器具をご存じですか？"},{"id":{"d":980,"n":2,"t":4},"speaker":"B","text":"どこの筋肉を鍛えたいのですか？"},{"id":{"d":980,"n":3,"t":4},"speaker":"A","text":"腕や胸周り、腹筋などを、一通りトレーニングしたいと思っています。"},{"id":{"d":980,"n":4,"t":4},"speaker":"B","text":"腕ならやはりダンベルでしょうね。重さを調節できますから、適当な負荷をかけることができます。"},{"id":{"d":980,"n":5,"t":4},"speaker":"A","text":"胸周りはどうでしょうか？"},{"id":{"d":980,"n":6,"t":4},"speaker":"B","text":"胸周りを鍛えるには、プッシュアップバーがお薦めです。"},{"id":{"d":980,"n":7,"t":4},"speaker":"A","text":"初めて聞きますが、それはどのような器具ですか？"},{"id":{"d":980,"n":8,"t":4},"speaker":"B","text":"腕立て伏せの補助器具で、通常よりも深く身体を沈ませることができます。"},{"id":{"d":980,"n":9,"t":4},"speaker":"A","text":"私は腹筋運動が苦手で、いつも長続きしません。"},{"id":{"d":980,"n":10,"t":4},"speaker":"B","text":"それなら腹筋ローラーがあります。"},{"id":{"d":980,"n":11,"t":4},"speaker":"A","text":"どのようにして使うのですか？"},{"id":{"d":980,"n":12,"t":4},"speaker":"B","text":"両膝を床につけ、グリップを握ってローラーをゆっくり転がして身体を前に伸ばします。"},{"id":{"d":980,"n":13,"t":4},"speaker":"A","text":"教えてくれてありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":981,"n":1,"t":4},"speaker":"A","text":"山歩きの途中で何かにかぶれてしまったようです。"},{"id":{"d":981,"n":2,"t":4},"speaker":"B","text":"どうやら漆にかぶれたようですね。山歩きをしたのはいつですか？"},{"id":{"d":981,"n":3,"t":4},"speaker":"A","text":"1週間ほど前です。4日ほど前に発疹が出てきました。"},{"id":{"d":981,"n":4,"t":4},"speaker":"B","text":"もっと前に来てもらえれば、これほどひどくならずに済んだかもしれません。"},{"id":{"d":981,"n":5,"t":4},"speaker":"A","text":"大したことはないだろうと思っていたのですが、どんどん悪化してしまいました。反省しています。"},{"id":{"d":981,"n":6,"t":4},"speaker":"B","text":"薬を出しておきましょう。市販の薬より効果が高いステロイド剤を処方しますね。"},{"id":{"d":981,"n":7,"t":4},"speaker":"A","text":"副作用はありませんか？"},{"id":{"d":981,"n":8,"t":4},"speaker":"B","text":"正しい回数と分量を使えば問題のない薬です。でも使って何か異常を感じるようだったら、使用を中止して再度診察を受けに来てください。"},{"id":{"d":981,"n":9,"t":4},"speaker":"A","text":"分かりました。"},{"id":{"d":981,"n":10,"t":4},"speaker":"B","text":"様子を見たいので、2、3日後にまた来てください。"},{"id":{"d":981,"n":11,"t":4},"speaker":"A","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":982,"n":1,"t":4},"speaker":"A","text":"先生、ギプスはいつごろ取れますか？"},{"id":{"d":982,"n":2,"t":4},"speaker":"B","text":"3週間後ぐらいです。"},{"id":{"d":982,"n":3,"t":4},"speaker":"A","text":"リハビリはいつから始めるのですか？"},{"id":{"d":982,"n":4,"t":4},"speaker":"B","text":"リハビリはギプスが取れる少し前から行います。"},{"id":{"d":982,"n":5,"t":4},"speaker":"A","text":"私は、ギプスが取れてから始めるのかと思っていました。"},{"id":{"d":982,"n":6,"t":4},"speaker":"B","text":"以前はそうでしたが、最近はギプスが取れる前に始めることが多くなっています。"},{"id":{"d":982,"n":7,"t":4},"speaker":"A","text":"どのようなリハビリをするのですか？"},{"id":{"d":982,"n":8,"t":4},"speaker":"B","text":"大山さんは手首にギプスをしていますから、指や肘や肩を少しずつ動かすリハビリをすることになります。"},{"id":{"d":982,"n":9,"t":4},"speaker":"A","text":"分かりました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":983,"n":1,"t":4},"speaker":"A","text":"整髪料は何を使っていますか？"},{"id":{"d":983,"n":2,"t":4},"speaker":"B","text":"私はスタイリングフォームを使っていますよ。"},{"id":{"d":983,"n":3,"t":4},"speaker":"A","text":"私はスタイリングフォームだと手がべたべたするので、スプレーを使っています。"},{"id":{"d":983,"n":4,"t":4},"speaker":"B","text":"私はスプレーだと何だか物足りない感じがします。"},{"id":{"d":983,"n":5,"t":4},"speaker":"A","text":"使っているスタイリングフォームはハードタイプですか？それともソフトタイプですか？"},{"id":{"d":983,"n":6,"t":4},"speaker":"B","text":"髪をしっかり固めたいので、スーパーハードタイプを使っています。"},{"id":{"d":983,"n":7,"t":4},"speaker":"A","text":"髪がごわごわして気持ちが悪くありませんか？"},{"id":{"d":983,"n":8,"t":4},"speaker":"B","text":"そのようなことはありません。しっとりして艶も出ます。"}];
INSERT INTO sentence [{"id":{"d":984,"n":1,"t":4},"speaker":"A","text":"何か健康に良いことをしていますか？"},{"id":{"d":984,"n":2,"t":4},"speaker":"B","text":"健康に良いかどうか分かりませんが、毎朝6時ごろに起きてラジオ体操をしています。"},{"id":{"d":984,"n":3,"t":4},"speaker":"A","text":"随分早起きですね。私は9時ごろにやっと起きます。"},{"id":{"d":984,"n":4,"t":4},"speaker":"B","text":"夜10時には寝ますので、6時ごろには目が覚めてしまいます。"},{"id":{"d":984,"n":5,"t":4},"speaker":"A","text":"それで6時半からラジオ体操をするのですね。"},{"id":{"d":984,"n":6,"t":4},"speaker":"B","text":"そうです。心身ともに爽やかになります。"},{"id":{"d":984,"n":7,"t":4},"speaker":"A","text":"素晴らしいですね。"},{"id":{"d":984,"n":8,"t":4},"speaker":"B","text":"あなたもラジオ体操をなさるといいですよ。"},{"id":{"d":984,"n":9,"t":4},"speaker":"A","text":"私はとても6時に起きることはできません。"}];
INSERT INTO sentence [{"id":{"d":985,"n":1,"t":4},"speaker":"A","text":"まぶたが腫れて痛みがあります。"},{"id":{"d":985,"n":2,"t":4},"speaker":"B","text":"これは麦粒腫ですね。俗に言うものもらいです。まぶたにある脂腺や汗腺が細菌に感染して起こるものです。"},{"id":{"d":985,"n":3,"t":4},"speaker":"A","text":"薬で治りますか？それとも手術をしなければいけないのでしょうか？"},{"id":{"d":985,"n":4,"t":4},"speaker":"B","text":"症状が軽いから目薬で治りますよ。今のところまだ手術する必要はないでしょう。"},{"id":{"d":985,"n":5,"t":4},"speaker":"A","text":"良かったです。"},{"id":{"d":985,"n":6,"t":4},"speaker":"B","text":"抗菌目薬を出しますから、1週間ほど使ってください。それで症状が治ったら、通院する必要はありません。"},{"id":{"d":985,"n":7,"t":4},"speaker":"A","text":"分かりました。何か生活の中で気を付けることはありますか？"},{"id":{"d":985,"n":8,"t":4},"speaker":"B","text":"目の周りを清潔にすることが大事です。汚れた手で目をこすったりしないようにしてください。"},{"id":{"d":985,"n":9,"t":4},"speaker":"A","text":"分かりました。どうもありがとうございました。"},{"id":{"d":985,"n":10,"t":4},"speaker":"B","text":"お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":986,"n":1,"t":4},"speaker":"A","text":"奥の歯がぐらぐらして、かむと痛みます。"},{"id":{"d":986,"n":2,"t":4},"speaker":"B","text":"いつからそのような症状が出ていますか？"},{"id":{"d":986,"n":3,"t":4},"speaker":"A","text":"1カ月ほど前からです。そのうち治るだろうと放っておいたのですが、この2、3日で、急に痛みが激しくなりました。"},{"id":{"d":986,"n":4,"t":4},"speaker":"B","text":"歯周病になっているかもしれません。歯を支える骨が溶けてぐらぐらしてくる症状です。"},{"id":{"d":986,"n":5,"t":4},"speaker":"A","text":"歯を抜かなくてはいけませんか？"},{"id":{"d":986,"n":6,"t":4},"speaker":"B","text":"歯自体には悪いところはないので、抜かずに治療しましょう。"},{"id":{"d":986,"n":7,"t":4},"speaker":"A","text":"なるべく歯を抜きたくないので、それはありがたいです。"},{"id":{"d":986,"n":8,"t":4},"speaker":"B","text":"炎症止めの薬と痛み止めの薬を出しますので、それを飲んで様子を見てください。しっかり歯磨きをすることも大切です。"},{"id":{"d":986,"n":9,"t":4},"speaker":"A","text":"分かりました。ありがとうございました。"},{"id":{"d":986,"n":10,"t":4},"speaker":"B","text":"1週間ほど経ったら、もう一度来院してください。"}];
INSERT INTO sentence [{"id":{"d":987,"n":1,"t":4},"speaker":"A","text":"夏休みの合宿のスケジュールを立てなくてはいけませんね。"},{"id":{"d":987,"n":2,"t":4},"speaker":"B","text":"場所は例年どおりに山中湖にしますか？"},{"id":{"d":987,"n":3,"t":4},"speaker":"A","text":"そうですね。今年も平野屋にお願いしましょう。"},{"id":{"d":987,"n":4,"t":4},"speaker":"B","text":"平野屋は、今年、テニスコートの人工芝を張り替えたそうですね。"},{"id":{"d":987,"n":5,"t":4},"speaker":"A","text":"平野屋も代替わりして、息子さんが張り切っているのでしょう。"},{"id":{"d":987,"n":6,"t":4},"speaker":"B","text":"すてきですね。選手たちも新しい気持ちでトレーニングに励むことができそうです。"},{"id":{"d":987,"n":7,"t":4},"speaker":"A","text":"はい。それでは予約してもらえますか？"},{"id":{"d":987,"n":8,"t":4},"speaker":"B","text":"分かりました。連絡してみます。"}];
INSERT INTO sentence [{"id":{"d":988,"n":1,"t":4},"speaker":"A","text":"耳の中がこもっているようで、あまりよく聞こえません。"},{"id":{"d":988,"n":2,"t":4},"speaker":"B","text":"どちらの耳ですか？"},{"id":{"d":988,"n":3,"t":4},"speaker":"A","text":"右の耳です。左の耳は普通に聞こえます。"},{"id":{"d":988,"n":4,"t":4},"speaker":"B","text":"診てみましょう。左の方を向いてください。"},{"id":{"d":988,"n":5,"t":4},"speaker":"A","text":"何か悪いところでもあるのでしょうか？"},{"id":{"d":988,"n":6,"t":4},"speaker":"B","text":"耳あかがたまって、耳道をふさいでいるようです。"},{"id":{"d":988,"n":7,"t":4},"speaker":"A","text":"耳掃除をしておけばよかったのでしょうか？"},{"id":{"d":988,"n":8,"t":4},"speaker":"B","text":"そうですが、自分ですると鼓膜を傷めることがありますので、病院に来ていただいたのは正しいです。"},{"id":{"d":988,"n":9,"t":4},"speaker":"A","text":"それなら、よかったです。"},{"id":{"d":988,"n":10,"t":4},"speaker":"B","text":"今から耳あかを除去しますが、少し痛むかもしれません。痛かったら「痛い」と言ってください。"},{"id":{"d":988,"n":11,"t":4},"speaker":"A","text":"分かりました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":989,"n":1,"t":4},"speaker":"A","text":"最近、少し便秘気味です。"},{"id":{"d":989,"n":2,"t":4},"speaker":"B","text":"腸内環境が悪くなっている可能性がありますね。"},{"id":{"d":989,"n":3,"t":4},"speaker":"A","text":"腸内環境が悪いとはどのような状態なのでしょうか？"},{"id":{"d":989,"n":4,"t":4},"speaker":"B","text":"腸の中の善玉菌と悪玉菌、それに日和見菌のバランスが悪くなるのです。"},{"id":{"d":989,"n":5,"t":4},"speaker":"A","text":"善玉菌と悪玉菌は何となく分かりますが、日和見菌というのは何ですか？"},{"id":{"d":989,"n":6,"t":4},"speaker":"B","text":"日和見菌というのは、善玉と悪玉の優勢な方に加勢する菌です。"},{"id":{"d":989,"n":7,"t":4},"speaker":"A","text":"バランスを良くするには、どのようなことをすればよいのですか？"},{"id":{"d":989,"n":8,"t":4},"speaker":"B","text":"まず、日々の生活のリズムを整えることです。それから、食物繊維と水分をよく取ることが大切です。"},{"id":{"d":989,"n":9,"t":4},"speaker":"A","text":"分かりました。あなたのアドバイスに従って生活を見直してみます。"}];
INSERT INTO sentence [{"id":{"d":990,"n":1,"t":4},"speaker":"A","text":"随分と髪が伸びましたね。そろそろ散髪に行った方がいいのではありませんか？"},{"id":{"d":990,"n":2,"t":4},"speaker":"B","text":"そうなのですが、散髪に行くのが面倒なのです。"},{"id":{"d":990,"n":3,"t":4},"speaker":"A","text":"あなたには短い髪の方が似合うと思いますよ。"},{"id":{"d":990,"n":4,"t":4},"speaker":"B","text":"それなら今日の午後にでも床屋に行ってきます。"},{"id":{"d":990,"n":5,"t":4},"speaker":"A","text":"ついでに白髪染めもしてもらったらいかがですか？"},{"id":{"d":990,"n":6,"t":4},"speaker":"B","text":"あまり気が進まないので、白髪染めはしないつもりです。"},{"id":{"d":990,"n":7,"t":4},"speaker":"A","text":"それなら、今回は髪を切ってもらうだけですね。"},{"id":{"d":990,"n":8,"t":4},"speaker":"B","text":"はい。このくらい白髪が交じっている方が、年相応でいいです。"}];
INSERT INTO sentence [{"id":{"d":991,"n":1,"t":4},"speaker":"A","text":"寒気がしてきました。"},{"id":{"d":991,"n":2,"t":4},"speaker":"B","text":"お風呂から上がった後、しばらく下着のままで過ごしていたからですよ。"},{"id":{"d":991,"n":3,"t":4},"speaker":"A","text":"つい油断していました。何か薬はありませんか？"},{"id":{"d":991,"n":4,"t":4},"speaker":"B","text":"葛根湯があります。このようなときのために買っておきました。"},{"id":{"d":991,"n":5,"t":4},"speaker":"A","text":"それは助かります。本格的に風邪をひかないうちに飲もうと思います。"},{"id":{"d":991,"n":6,"t":4},"speaker":"B","text":"今、水と一緒に持ってきますから待っていてください。"},{"id":{"d":991,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":991,"n":8,"t":4},"speaker":"B","text":"今度から、湯上がりには身体を冷やさないように気を付けてくださいね。"},{"id":{"d":991,"n":9,"t":4},"speaker":"A","text":"はい、気を付けます。"}];
INSERT INTO sentence [{"id":{"d":992,"n":1,"t":4},"speaker":"A","text":"筋力トレーニングを始めようと思うのですが、ポイントを教えてください。"},{"id":{"d":992,"n":2,"t":4},"speaker":"B","text":"まず、何のためにどこを鍛えるかを明確にすることが大切です。"},{"id":{"d":992,"n":3,"t":4},"speaker":"A","text":"なるほど、何となく行うのでは効果がないのですね。"},{"id":{"d":992,"n":4,"t":4},"speaker":"B","text":"トレーニングの前後にストレッチをすることも大事です。"},{"id":{"d":992,"n":5,"t":4},"speaker":"A","text":"ストレッチはどのような要領ですればよいのですか？"},{"id":{"d":992,"n":6,"t":4},"speaker":"B","text":"筋力トレーニングの前にはダイナミックに動いて筋肉を伸ばすようにします。血行が良くなります。"},{"id":{"d":992,"n":7,"t":4},"speaker":"A","text":"筋力トレーニングの後はどうですか？"},{"id":{"d":992,"n":8,"t":4},"speaker":"B","text":"終わってからは、ゆっくりと時間をかけて体を動かすと、筋肉の疲労が回復しやすくなりますよ。"},{"id":{"d":992,"n":9,"t":4},"speaker":"A","text":"他に注意することはありますか？"},{"id":{"d":992,"n":10,"t":4},"speaker":"B","text":"栄養補給に気を付けてください。タンパク質やビタミンを十分に摂取すると良いでしょう。"},{"id":{"d":992,"n":11,"t":4},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":993,"n":1,"t":4},"speaker":"A","text":"最近、足がしびれるようになったのですが、どこか悪いのでしょうか？"},{"id":{"d":993,"n":2,"t":4},"speaker":"B","text":"どのようなときにしびれますか？"},{"id":{"d":993,"n":3,"t":4},"speaker":"A","text":"大体いつもしびれているような気がします。"},{"id":{"d":993,"n":4,"t":4},"speaker":"B","text":"いろいろな原因が考えられますが、検査をしてみないとはっきりしたことは言えません。"},{"id":{"d":993,"n":5,"t":4},"speaker":"A","text":"例えばどのような原因が考えられるのですか？"},{"id":{"d":993,"n":6,"t":4},"speaker":"B","text":"一つは神経や骨格の病気です。腰椎椎間板ヘルニアや、脊柱管狭窄症の可能性があります。"},{"id":{"d":993,"n":7,"t":4},"speaker":"A","text":"他にどのような原因が考えられますか？"},{"id":{"d":993,"n":8,"t":4},"speaker":"B","text":"もう一つは内臓の病気です。脳血管障害や糖尿病も考えられます。"},{"id":{"d":993,"n":9,"t":4},"speaker":"A","text":"早く原因を知りたいので検査をしていただけますか？"},{"id":{"d":993,"n":10,"t":4},"speaker":"B","text":"はい、では検査の予約を入れましょう。"}];
INSERT INTO sentence [{"id":{"d":994,"n":1,"t":4},"speaker":"A","text":"おめでとうございます。妊娠なさっていますよ。6週目頃ですね。"},{"id":{"d":994,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。とてもうれしいです。吐き気が続いたのはつわりだったのでしょうか？"},{"id":{"d":994,"n":3,"t":4},"speaker":"A","text":"恐らくそうでしょう。妊娠初期は母子ともにデリケートな時ですから、無理をしないことが大事です。"},{"id":{"d":994,"n":4,"t":4},"speaker":"B","text":"どのようなことに気を付けたらよいのでしょうか？"},{"id":{"d":994,"n":5,"t":4},"speaker":"A","text":"普段の生活を送っていただいて構いませんが、妊娠すると免疫力が低下しますからまずは感染症にかからないように注意しましょう。また、激しい運動は控えましょうね。"},{"id":{"d":994,"n":6,"t":4},"speaker":"B","text":"はい。初めての妊娠なので、うれしい半面、分からないことも多くて心配です。"},{"id":{"d":994,"n":7,"t":4},"speaker":"A","text":"それは自然なことですよ。待合室に妊娠が分かった方向けのパンフレットがありますので、ぜひご覧ください。心配なことがあれば、私も看護師もいつでも相談に乗りますよ。"},{"id":{"d":994,"n":8,"t":4},"speaker":"B","text":"分かりました。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":995,"n":1,"t":4},"speaker":"A","text":"友人ががんにかかって、長い闘病生活をしています。"},{"id":{"d":995,"n":2,"t":4},"speaker":"B","text":"それはお気の毒ですね。どこのがんですか？"},{"id":{"d":995,"n":3,"t":4},"speaker":"A","text":"初めは肺にできたのだそうです。"},{"id":{"d":995,"n":4,"t":4},"speaker":"B","text":"そうですか。"},{"id":{"d":995,"n":5,"t":4},"speaker":"A","text":"友人の場合、いったんは治癒したようなのですが、その後に咽喉や口腔などに転移しました。"},{"id":{"d":995,"n":6,"t":4},"speaker":"B","text":"その間、ずっと闘病生活を続けるというのはつらいですね。"},{"id":{"d":995,"n":7,"t":4},"speaker":"A","text":"でも、本人は家庭菜園をしたりボランティア活動をしたりして、元気に暮らしています。"},{"id":{"d":995,"n":8,"t":4},"speaker":"B","text":"それは素晴らしいですね。私にはとてもできないことです。"}];
INSERT INTO sentence [{"id":{"d":996,"n":1,"t":4},"speaker":"A","text":"健康のために水泳を始めようと思います。"},{"id":{"d":996,"n":2,"t":4},"speaker":"B","text":"それは良いことですね。私も水泳を始めてから、体調が良くなりました。"},{"id":{"d":996,"n":3,"t":4},"speaker":"A","text":"それで、よかったらあなたの通っているスイミングクラブを紹介していただきたいのです。"},{"id":{"d":996,"n":4,"t":4},"speaker":"B","text":"もちろん、喜んで紹介しましょう。"},{"id":{"d":996,"n":5,"t":4},"speaker":"A","text":"実は、私は泳げないのですが、それでも構いませんか？"},{"id":{"d":996,"n":6,"t":4},"speaker":"B","text":"大丈夫ですよ。初心者用のコースもあって、丁寧に教えてくれます。"},{"id":{"d":996,"n":7,"t":4},"speaker":"A","text":"それならよかったです。安心しました。"},{"id":{"d":996,"n":8,"t":4},"speaker":"B","text":"そうと決まったら、早速明日にでも紹介しましょう。"},{"id":{"d":996,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":997,"n":1,"t":4},"speaker":"A","text":"どうも熱があるようです。体が熱っぽいです。"},{"id":{"d":997,"n":2,"t":4},"speaker":"B","text":"体温計を持ってきますから、測ってみてください。"},{"id":{"d":997,"n":3,"t":4},"speaker":"A","text":"やはり38度あります。"},{"id":{"d":997,"n":4,"t":4},"speaker":"B","text":"他にどこか具合の悪いところはありますか？"},{"id":{"d":997,"n":5,"t":4},"speaker":"A","text":"喉が痛いです。"},{"id":{"d":997,"n":6,"t":4},"speaker":"B","text":"もしかしたら、新型コロナウイルスかもしれません。宮下医院に電話してみましょう。"},{"id":{"d":997,"n":7,"t":4},"speaker":"A","text":"もし新型コロナウイルスだったら、お母さんも濃厚接触者になりますね。"},{"id":{"d":997,"n":8,"t":4},"speaker":"B","text":"もちろんです。私もPCR検査を受けなくてはいけません。"},{"id":{"d":997,"n":9,"t":4},"speaker":"A","text":"どうしましょう。困りましたね。"},{"id":{"d":997,"n":10,"t":4},"speaker":"B","text":"ともかく、宮下先生に相談してみましょう。"}];
INSERT INTO sentence [{"id":{"d":998,"n":1,"t":4},"speaker":"A","text":"日焼け止めクリームを塗りましたか？"},{"id":{"d":998,"n":2,"t":4},"speaker":"B","text":"今日は曇っているから塗らなくてもいいでしょう。"},{"id":{"d":998,"n":3,"t":4},"speaker":"A","text":"とんでもない。曇り空だって、紫外線は降り注いでいるのですよ。"},{"id":{"d":998,"n":4,"t":4},"speaker":"B","text":"それはそうでしょうけど、大した量ではありませんよね？"},{"id":{"d":998,"n":5,"t":4},"speaker":"A","text":"いいえ。今日のような薄曇りの日の紫外線は、晴れの日よりも多いくらいなのです。"},{"id":{"d":998,"n":6,"t":4},"speaker":"B","text":"なぜですか？雲に遮られて紫外線の量は減るのではないのですか？"},{"id":{"d":998,"n":7,"t":4},"speaker":"A","text":"いいえ。雲の間から日が差しているときは、雲に反射する光が加わって、かえって紫外線の量が増えるのです。"},{"id":{"d":998,"n":8,"t":4},"speaker":"B","text":"そのようなことがあるのですか。それでは気を付けなければいけませんね。"},{"id":{"d":998,"n":9,"t":4},"speaker":"A","text":"はい。ですから、早く日焼け止めクリームを塗ってください。"},{"id":{"d":998,"n":10,"t":4},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":999,"n":1,"t":4},"speaker":"A","text":"痩せるためにジョギングを始めました。"},{"id":{"d":999,"n":2,"t":4},"speaker":"B","text":"それほど太っているようには見えませんよ。"},{"id":{"d":999,"n":3,"t":4},"speaker":"A","text":"見た目というよりは、動きやすさのために体を軽くしたいのです。"},{"id":{"d":999,"n":4,"t":4},"speaker":"B","text":"なるほど。何キロメートルぐらい走るのですか？"},{"id":{"d":999,"n":5,"t":4},"speaker":"A","text":"まだ始めたばかりですから、せいぜい5キロメートルほどです。"},{"id":{"d":999,"n":6,"t":4},"speaker":"B","text":"始めたばかりで5キロメートルはきついでしょう。"},{"id":{"d":999,"n":7,"t":4},"speaker":"A","text":"もちろん休みながら走っています。木立の中を走るのは気持ちの良いものですよ。"},{"id":{"d":999,"n":8,"t":4},"speaker":"B","text":"この辺りだと、どこを走るのですか？"},{"id":{"d":999,"n":9,"t":4},"speaker":"A","text":"代々木公園です。私の家から近いのです。"},{"id":{"d":999,"n":10,"t":4},"speaker":"B","text":"それは気持ちが良いでしょうね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":1000,"n":1,"t":4},"speaker":"A","text":"頭痛薬はありますか？"},{"id":{"d":1000,"n":2,"t":4},"speaker":"B","text":"頭痛ですか？どのような痛みがあるのですか？"},{"id":{"d":1000,"n":3,"t":4},"speaker":"A","text":"後頭部から肩にかけて締め付けられるように痛いのです。"},{"id":{"d":1000,"n":4,"t":4},"speaker":"B","text":"吐き気はありますか？"},{"id":{"d":1000,"n":5,"t":4},"speaker":"A","text":"いいえ、吐き気はありません。"},{"id":{"d":1000,"n":6,"t":4},"speaker":"B","text":"もしかして、パソコンを使うお仕事をしてはいませんか？"},{"id":{"d":1000,"n":7,"t":4},"speaker":"A","text":"はい、一日中パソコンに向かっています。"},{"id":{"d":1000,"n":8,"t":4},"speaker":"B","text":"それなら、マッサージをしたり、蒸しタオルで首筋を温めたりして、血行を良くするといいです。"},{"id":{"d":1000,"n":9,"t":4},"speaker":"A","text":"薬は必要ないでしょうか？"},{"id":{"d":1000,"n":10,"t":4},"speaker":"B","text":"それでも頭痛が治まらなかったら、頭痛薬を出します。"}];
INSERT INTO sentence [{"id":{"d":1001,"n":1,"t":4},"speaker":"A","text":"どうもシェーバーの具合が良くないようです。"},{"id":{"d":1001,"n":2,"t":4},"speaker":"B","text":"どのような具合ですか？"},{"id":{"d":1001,"n":3,"t":4},"speaker":"A","text":"力が弱くて、すぐに止まってしまうのです。"},{"id":{"d":1001,"n":4,"t":4},"speaker":"B","text":"電池切れではありませんか？充電はしてあるのですか？"},{"id":{"d":1001,"n":5,"t":4},"speaker":"A","text":"昨日充電しておきました。"},{"id":{"d":1001,"n":6,"t":4},"speaker":"B","text":"電池の寿命かもしれません。電池を取り替えたらどうですか？"},{"id":{"d":1001,"n":7,"t":4},"speaker":"A","text":"このシェーバーは電池の取り替えができません。"},{"id":{"d":1001,"n":8,"t":4},"speaker":"B","text":"それなら、新しいシェーバーを買うしか方法がありませんね。"},{"id":{"d":1001,"n":9,"t":4},"speaker":"A","text":"長年使い続けてきたので、確かに、そろそろ買い換えてもいい頃ですね。"}];
INSERT INTO sentence [{"id":{"d":1002,"n":1,"t":4},"speaker":"A","text":"ヨガを始めたいのですが、この近くにあるヨガ教室を知りませんか？"},{"id":{"d":1002,"n":2,"t":4},"speaker":"B","text":"それなら、オンラインでレッスンをしてくれるところがありますよ。"},{"id":{"d":1002,"n":3,"t":4},"speaker":"A","text":"それは良いですね。今はコロナ禍で人数制限もありそうですが、オンラインならその心配はありませんね。"},{"id":{"d":1002,"n":4,"t":4},"speaker":"B","text":"自宅で自分の都合の良い時間にレッスンを受けることができます。"},{"id":{"d":1002,"n":5,"t":4},"speaker":"A","text":"でも、あまり自由だと続けられるかどうか心配です。"},{"id":{"d":1002,"n":6,"t":4},"speaker":"B","text":"お試し期間がありますよ。月額500円で3カ月間は受けることができます。"},{"id":{"d":1002,"n":7,"t":4},"speaker":"A","text":"それなら、自分に合うかどうかを試してみることができますね。"},{"id":{"d":1002,"n":8,"t":4},"speaker":"B","text":"はい。合わないようならお試し期間だけでやめてもよいですしね。"},{"id":{"d":1002,"n":9,"t":4},"speaker":"A","text":"それでは、早速試してみます。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":1003,"n":1,"t":4},"speaker":"A","text":"裕太君は、注射が嫌いですか？"},{"id":{"d":1003,"n":2,"t":4},"speaker":"B","text":"はい、注射は痛いので嫌いです。"},{"id":{"d":1003,"n":3,"t":4},"speaker":"A","text":"初めは少し痛いけれど、すぐに終わりますよ。"},{"id":{"d":1003,"n":4,"t":4},"speaker":"B","text":"でも、前に注射をされたときはとても痛かったです。"},{"id":{"d":1003,"n":5,"t":4},"speaker":"A","text":"ここの先生はとても注射が上手だから、全然痛くありませんよ。"},{"id":{"d":1003,"n":6,"t":4},"speaker":"B","text":"看護師さんはいつもそう言います。"},{"id":{"d":1003,"n":7,"t":4},"speaker":"A","text":"注射をすると頭痛の症状が楽になりますよ。"},{"id":{"d":1003,"n":8,"t":4},"speaker":"B","text":"確かに、ずっと頭が痛いのも困ります。"},{"id":{"d":1003,"n":9,"t":4},"speaker":"A","text":"では、ほんの少しだけ我慢をして注射をしましょうね。終わったらシールをあげますよ。"},{"id":{"d":1003,"n":10,"t":4},"speaker":"B","text":"本当ですか？では少しだけ我慢をします。"}];
INSERT INTO sentence [{"id":{"d":1004,"n":1,"t":4},"speaker":"A","text":"二日酔いで頭が痛いです。"},{"id":{"d":1004,"n":2,"t":4},"speaker":"B","text":"やはり二日酔いになってしまったのですね。そのようなことになるのではないかと思っていました。"},{"id":{"d":1004,"n":3,"t":4},"speaker":"A","text":"昨日は緒方君が久しぶりに家に遊びに来てくれたので、つい飲み過ぎてしまいました。"},{"id":{"d":1004,"n":4,"t":4},"speaker":"B","text":"緒方さんはまだ眠っていらっしゃるのですか？"},{"id":{"d":1004,"n":5,"t":4},"speaker":"A","text":"はい、いびきをかいて眠っていました。"},{"id":{"d":1004,"n":6,"t":4},"speaker":"B","text":"朝食ができていますが、どうしますか？"},{"id":{"d":1004,"n":7,"t":4},"speaker":"A","text":"緒方さんは、もう少し眠らせてあげましょう。"},{"id":{"d":1004,"n":8,"t":4},"speaker":"B","text":"分かりました。あなたは食べられますか？"},{"id":{"d":1004,"n":9,"t":4},"speaker":"A","text":"はい、頭が少し痛いのですが、朝食は食べられます。"},{"id":{"d":1004,"n":10,"t":4},"speaker":"B","text":"それでは今用意します。"},{"id":{"d":1004,"n":11,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1005,"n":1,"t":4},"speaker":"A","text":"きちんと手洗いとうがいをしましたか？"},{"id":{"d":1005,"n":2,"t":4},"speaker":"B","text":"まだしていません。うっかり忘れていました。"},{"id":{"d":1005,"n":3,"t":4},"speaker":"A","text":"昨日もお母さんに言われてから手を洗っていましたね。"},{"id":{"d":1005,"n":4,"t":4},"speaker":"B","text":"いつもは忘れずに手洗いとうがいをしていますよ。"},{"id":{"d":1005,"n":5,"t":4},"speaker":"A","text":"昨日と今日だけ忘れてしまったのですね。"},{"id":{"d":1005,"n":6,"t":4},"speaker":"B","text":"そうです。洗面所に行ってきます。"}];
INSERT INTO sentence [{"id":{"d":1006,"n":1,"t":4},"speaker":"A","text":"護身術を身に付けたいと思っているのですが、何かお勧めはありますか？"},{"id":{"d":1006,"n":2,"t":4},"speaker":"B","text":"合気道をお勧めします。実は、私もやっているのです。"},{"id":{"d":1006,"n":3,"t":4},"speaker":"A","text":"合気道のことはよく知りません。どのようなものですか？"},{"id":{"d":1006,"n":4,"t":4},"speaker":"B","text":"天地の気に合わせることを基本とする武道です。"},{"id":{"d":1006,"n":5,"t":4},"speaker":"A","text":"抽象的で何だかよく分かりません。"},{"id":{"d":1006,"n":6,"t":4},"speaker":"B","text":"体を自然の気に合わせて動かすので、体格や体力によらずに相手をコントロールすることができます。"},{"id":{"d":1006,"n":7,"t":4},"speaker":"A","text":"私のような非力な者でも大丈夫ということですか？"},{"id":{"d":1006,"n":8,"t":4},"speaker":"B","text":"はい、力の弱い女性や高齢者にこそふさわしい武道なのです。"},{"id":{"d":1006,"n":9,"t":4},"speaker":"A","text":"護身術としては理想的ですね。やってみます。"}];
INSERT INTO sentence [{"id":{"d":1007,"n":1,"t":4},"speaker":"A","text":"ドラッグストアに行きますが、何か買ってくるものはありますか？"},{"id":{"d":1007,"n":2,"t":4},"speaker":"B","text":"ちょうどよかったです。洗濯洗剤がなくなったので買ってきてください。"},{"id":{"d":1007,"n":3,"t":4},"speaker":"A","text":"分かりました。希望の商品はありますか？"},{"id":{"d":1007,"n":4,"t":4},"speaker":"B","text":"今使っているこの洗剤と同じものがあれば買ってきてください。"},{"id":{"d":1007,"n":5,"t":4},"speaker":"A","text":"テレビでよく宣伝している洗剤ですね。"},{"id":{"d":1007,"n":6,"t":4},"speaker":"B","text":"はい、そうです。"},{"id":{"d":1007,"n":7,"t":4},"speaker":"A","text":"もし置いていなかったらどうしますか？"},{"id":{"d":1007,"n":8,"t":4},"speaker":"B","text":"多分あると思いますが、なかったら別の洗剤でもよいです。"},{"id":{"d":1007,"n":9,"t":4},"speaker":"A","text":"分かりました。他に何か買ってきてほしいものがありますか？"},{"id":{"d":1007,"n":10,"t":4},"speaker":"B","text":"いいえ、今のところはそれだけです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":1008,"n":1,"t":4},"speaker":"A","text":"両チームともなかなか点が入りませんね。"},{"id":{"d":1008,"n":2,"t":4},"speaker":"B","text":"良い形で攻撃するのですが、最後の決定力が足りません。"},{"id":{"d":1008,"n":3,"t":4},"speaker":"A","text":"ディフェンスも頑張っていますね。"},{"id":{"d":1008,"n":4,"t":4},"speaker":"B","text":"ええ、どちらのチームも疲れているはずですが動きは衰えませんね。"},{"id":{"d":1008,"n":5,"t":4},"speaker":"A","text":"アディショナルタイムは2分だそうです。"},{"id":{"d":1008,"n":6,"t":4},"speaker":"B","text":"どちらも気を緩めることはできませんね。でも、得てしてこのようなときに点が入るものです。"},{"id":{"d":1008,"n":7,"t":4},"speaker":"A","text":"そうだと良いのですが。このままだと延長戦に入りそうですね。"},{"id":{"d":1008,"n":8,"t":4},"speaker":"B","text":"延長では選手の入れ替えもあるでしょうね。監督の腕の見せ所ですね。"},{"id":{"d":1008,"n":9,"t":4},"speaker":"A","text":"はい。どちらもPK戦になる前に決着をつけたいでしょうしね。"}];
INSERT INTO sentence [{"id":{"d":1009,"n":1,"t":4},"speaker":"A","text":"随分とたくさんサプリメントを飲んでいるのですね。"},{"id":{"d":1009,"n":2,"t":4},"speaker":"B","text":"はい、忙しくて食事を十分に取れないので、その代わりです。"},{"id":{"d":1009,"n":3,"t":4},"speaker":"A","text":"何を飲んでいるのですか？"},{"id":{"d":1009,"n":4,"t":4},"speaker":"B","text":"主にビタミンとミネラルです。"},{"id":{"d":1009,"n":5,"t":4},"speaker":"A","text":"ビタミンとミネラルといってもたくさん種類がありそうですが、それを全部飲んでいるのですか？"},{"id":{"d":1009,"n":6,"t":4},"speaker":"B","text":"いいえ、いくつか必須のビタミンとミネラルを選んで飲んでいます。"},{"id":{"d":1009,"n":7,"t":4},"speaker":"A","text":"飲んでみて調子はいかがですか？"},{"id":{"d":1009,"n":8,"t":4},"speaker":"B","text":"調子は良いですよ。よく眠れますし、疲れも取れる気がします。"},{"id":{"d":1009,"n":9,"t":4},"speaker":"A","text":"それほど良いのでしたら、私も飲んでみましょう。"}];
INSERT INTO sentence [{"id":{"d":1010,"n":1,"t":4},"speaker":"A","text":"ここに置いてあったローションを知りませんか？"},{"id":{"d":1010,"n":2,"t":4},"speaker":"B","text":"どのようなローションですか？"},{"id":{"d":1010,"n":3,"t":4},"speaker":"A","text":"オレンジ色の瓶に入っているものです。"},{"id":{"d":1010,"n":4,"t":4},"speaker":"B","text":"あれなら空っぽでしたからごみ袋に入れてしまいました。"},{"id":{"d":1010,"n":5,"t":4},"speaker":"A","text":"それは困りました。私はあの瓶に詰め替え用のローションを入れるつもりだったのです。"},{"id":{"d":1010,"n":6,"t":4},"speaker":"B","text":"そうだったのですか。それはごめんなさい。"},{"id":{"d":1010,"n":7,"t":4},"speaker":"A","text":"ごみ袋はどこにありますか？"},{"id":{"d":1010,"n":8,"t":4},"speaker":"B","text":"外の物置の中に置いてあります。"},{"id":{"d":1010,"n":9,"t":4},"speaker":"A","text":"分かりました。瓶を探してきます。"}];
INSERT INTO sentence [{"id":{"d":1011,"n":1,"t":4},"speaker":"A","text":"ヒットを打つこつがあったら教えてください。"},{"id":{"d":1011,"n":2,"t":4},"speaker":"B","text":"これといったこつはありませんよ。ボールが来たらそれを打つだけです。"},{"id":{"d":1011,"n":3,"t":4},"speaker":"A","text":"それだけですか？でも、山中さんのような天才ならそれでも良いのかもれませんね。"},{"id":{"d":1011,"n":4,"t":4},"speaker":"B","text":"そのようなことはありません。私は人の何倍も努力をしました。"},{"id":{"d":1011,"n":5,"t":4},"speaker":"A","text":"もちろん今のご活躍は普段の努力の賜物だと思っていますが、山中さんは楽々と打っておられるように見えるので、何か気を付けていることがあればぜひお聞きしたいのです。"},{"id":{"d":1011,"n":6,"t":4},"speaker":"B","text":"そう見えるとしたら、私ができるだけ全身の力を抜くようにしているからだと思います。"},{"id":{"d":1011,"n":7,"t":4},"speaker":"A","text":"なるほど、それがヒットを打つこつなのかもしれませんね。"},{"id":{"d":1011,"n":8,"t":4},"speaker":"B","text":"そうかもしれません。リラックスしてバットを振ることは大切だと思います。"}];
INSERT INTO sentence [{"id":{"d":1012,"n":1,"t":4},"speaker":"A","text":"最近は、息苦しさを感じたり動悸がしたりします。"},{"id":{"d":1012,"n":2,"t":4},"speaker":"B","text":"それは気がかりですね。自律神経が乱れていると息が苦しくなることがあるそうですよ。"},{"id":{"d":1012,"n":3,"t":4},"speaker":"A","text":"そうなのですか？"},{"id":{"d":1012,"n":4,"t":4},"speaker":"B","text":"交感神経と副交感神経のバランスが大切だと聞いたことがあります。"},{"id":{"d":1012,"n":5,"t":4},"speaker":"A","text":"この症状を和らげたいと思っているのですが、病院に行った方がいいでしょうか？"},{"id":{"d":1012,"n":6,"t":4},"speaker":"B","text":"そう思います。ちなみに、私は息苦しさを感じるときは深い呼吸を心掛けていますよ。"},{"id":{"d":1012,"n":7,"t":4},"speaker":"A","text":"それだけで息苦しさが和らぐのですか？"},{"id":{"d":1012,"n":8,"t":4},"speaker":"B","text":"はい。リラックスできるので一緒にしてみませんか？"},{"id":{"d":1012,"n":9,"t":4},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":1013,"n":1,"t":4},"speaker":"A","text":"フケがたくさん出て困っているのですが、何か良い対策はありませんか？"},{"id":{"d":1013,"n":2,"t":4},"speaker":"B","text":"フケを抑えるシャンプーがありますよ。"},{"id":{"d":1013,"n":3,"t":4},"speaker":"A","text":"そのようなシャンプーもいくつか試してみましたが、効きませんでした。"},{"id":{"d":1013,"n":4,"t":4},"speaker":"B","text":"洗髪するときに、頭皮をごしごしとこすっていませんか？"},{"id":{"d":1013,"n":5,"t":4},"speaker":"A","text":"シャンプーブラシを使ってこすっています。"},{"id":{"d":1013,"n":6,"t":4},"speaker":"B","text":"それはかえって頭皮に良くない場合もあるそうですよ。"},{"id":{"d":1013,"n":7,"t":4},"speaker":"A","text":"私は、頭皮に適度な刺激を与えるのが毛髪にいいと聞いたので、ブラシを使っていたのですが、それが間違いだったのでしょうか？"},{"id":{"d":1013,"n":8,"t":4},"speaker":"B","text":"刺激を与えすぎると頭皮の新陳代謝が早くなって、フケが出やすくなるそうです。"},{"id":{"d":1013,"n":9,"t":4},"speaker":"A","text":"頭皮は強くこすらない方がいいのですか？"},{"id":{"d":1013,"n":10,"t":4},"speaker":"B","text":"はい。指でこするのも避けた方がいいそうです。"},{"id":{"d":1013,"n":11,"t":4},"speaker":"A","text":"私は真逆のことをしていたのですね。今日から頭皮をこすらずに洗髪してみます。"}];
INSERT INTO sentence [{"id":{"d":1014,"n":1,"t":4},"speaker":"A","text":"今日はパーマをかけたいと思っています。"},{"id":{"d":1014,"n":2,"t":4},"speaker":"B","text":"かしこまりました。どのような髪型がお望みですか？"},{"id":{"d":1014,"n":3,"t":4},"speaker":"A","text":"顎が角ばっているので、それをカバーできるような髪型がいいです。"},{"id":{"d":1014,"n":4,"t":4},"speaker":"B","text":"分かりました。では、この写真のような髪型ではいかがですか？"},{"id":{"d":1014,"n":5,"t":4},"speaker":"A","text":"すてきですね。でも、私の髪はごわごわした直毛なのでこのモデルのように仕上がるかどうかが不安です。"},{"id":{"d":1014,"n":6,"t":4},"speaker":"B","text":"お客様のような真っすぐな髪でも、このような柔らかい自然なウェーブに仕上がりますよ。"},{"id":{"d":1014,"n":7,"t":4},"speaker":"A","text":"では、この髪型にしていただけますか？"},{"id":{"d":1014,"n":8,"t":4},"speaker":"B","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":1015,"n":1,"t":4},"speaker":"A","text":"今日は歯科医院に行ってきました。"},{"id":{"d":1015,"n":2,"t":4},"speaker":"B","text":"お母さんはしばらく歯医者さんに行っていませんでしたよね。歯が痛くなったのですか？"},{"id":{"d":1015,"n":3,"t":4},"speaker":"A","text":"いいえ、そうではありません。虫歯がないかを調べるために行ってきました。初めて行くクリニックだったのですが、とても丁寧に診ていただけました。"},{"id":{"d":1015,"n":4,"t":4},"speaker":"B","text":"なぜこれまで行っていた歯医者さんには行かなかったのですか？"},{"id":{"d":1015,"n":5,"t":4},"speaker":"A","text":"以前行っていた所は担当の先生がいなくなってしまったので、これを機に、交通の便の良い別のクリニックに行ってきたのです。院内は清潔感もあるし、最新の設備もあって良い治療をしてもらえそうな気がします。"},{"id":{"d":1015,"n":6,"t":4},"speaker":"B","text":"それは良かったですね。私もそのクリニックに診てもらいたくなりました。"},{"id":{"d":1015,"n":7,"t":4},"speaker":"A","text":"人気のクリニックのようで、予約が入れにくいのが難点です。本当に行くのなら、早めに予約を入れた方がいいと思います。初診の場合は、電話で診察を申し込まなくてはなりません。"},{"id":{"d":1015,"n":8,"t":4},"speaker":"B","text":"分かりました。早速電話をしようと思います。"}];
INSERT INTO sentence [{"id":{"d":1016,"n":1,"t":4},"speaker":"A","text":"今朝は、手首がかゆくて目が覚めました。眠っている間に蚊に刺されたようです。"},{"id":{"d":1016,"n":2,"t":4},"speaker":"B","text":"これはかゆそうですね。かゆみ止めは塗りましたか？"},{"id":{"d":1016,"n":3,"t":4},"speaker":"A","text":"塗り薬を付けましたが、いっこうに治まりません。無意識のうちにかいてしまったようで、一層腫れてきてしまいました。"},{"id":{"d":1016,"n":4,"t":4},"speaker":"B","text":"これ以上腫れがひどくなるようであれば、皮膚科に行った方がいいかもしれません。症状が重い虫刺されもあると聞きますよ。"},{"id":{"d":1016,"n":5,"t":4},"speaker":"A","text":"甘く見ない方がいいのですね。どのような虫に刺されると重症化してしまうのでしょうか？"},{"id":{"d":1016,"n":6,"t":4},"speaker":"B","text":"私が聞いたことがあるのはマダニです。珍しい虫ではなく、庭や都会の植え込みにもいるそうです。刺されると発熱や食欲低下、下痢や嘔吐などの消化器症状を引き起こすそうです。"},{"id":{"d":1016,"n":7,"t":4},"speaker":"A","text":"心配になってきました。皮膚科には行ったことがないのですが、この近所に良い皮膚科はあるでしょうか？"},{"id":{"d":1016,"n":8,"t":4},"speaker":"B","text":"駅前の皮膚科はいいですよ。先生も看護師さんも優しいですし、的確な処置をしてくれます。"}];
INSERT INTO sentence [{"id":{"d":1017,"n":1,"t":4},"speaker":"A","text":"大塚さん、目が充血していますね。大丈夫ですか？"},{"id":{"d":1017,"n":2,"t":4},"speaker":"B","text":"昨日眼科を受診したらドライアイと診断されました。1週間ほど前から目に違和感があって、次第に白目が充血してきてしまいました。"},{"id":{"d":1017,"n":3,"t":4},"speaker":"A","text":"ドライアイには、どのような処置がされるのですか？"},{"id":{"d":1017,"n":4,"t":4},"speaker":"B","text":"視力や眼圧の測定などを一通り受け、重篤な疾患の兆候がないかを調べました。私の場合は特に問題がなかったので、ドライアイ用の目薬が処方されて終わりました。"},{"id":{"d":1017,"n":5,"t":4},"speaker":"A","text":"目薬が効いて早くよくなるといいですね。"},{"id":{"d":1017,"n":6,"t":4},"speaker":"B","text":"はい、充血が早く治まってほしいと思います。でも、日がな一日パソコンの画面を見続ける仕事ですから、なかなか完治しないかもしれません。"},{"id":{"d":1017,"n":7,"t":4},"speaker":"A","text":"パソコンの画面を凝視するとまばたきの頻度が減ってしまって目が乾くのですね。大塚さんの場合、ドライアイは職業病ということですね。"},{"id":{"d":1017,"n":8,"t":4},"speaker":"B","text":"そうとも言えると思います。これからは1時間に1回、目を休める時間をつくることを心掛けようと思います。"}];
INSERT INTO sentence [{"id":{"d":1018,"n":1,"t":4},"speaker":"A","text":"梨香子さんの活躍を新聞で拝見しましたよ。全日本陸上選手権に出場するとは、素晴らしいですね。"},{"id":{"d":1018,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。大会で経験を積むに従って記録が伸び、娘も満足しているようです。"},{"id":{"d":1018,"n":3,"t":4},"speaker":"A","text":"親御さんのサポートもあってこそのことと思います。遠征試合には一緒に行かれるのですか？"},{"id":{"d":1018,"n":4,"t":4},"speaker":"B","text":"高校生になるまでは、私か夫のどちらかがついて行っていましたので、私たち夫婦まで各地の陸上競技場に詳しくなりました。子どもがやる気になっていたので親としてサポートしたいと思っていました。"},{"id":{"d":1018,"n":5,"t":4},"speaker":"A","text":"そうでしたか。あの輝かしい記録は、梨香子ちゃんだけでなく、菊地さんご夫婦へのご褒美でもあるのですね。"},{"id":{"d":1018,"n":6,"t":4},"speaker":"B","text":"はい、梨香子から大きなプレゼントをもらったような気持ちです。"},{"id":{"d":1018,"n":7,"t":4},"speaker":"A","text":"梨香子ちゃんは、今はどちらにいるのですか？"},{"id":{"d":1018,"n":8,"t":4},"speaker":"B","text":"次の大会に向けて、合宿に行っています。次の試合は、2カ月後の都道府県対抗女子駅伝です。"}];
INSERT INTO sentence [{"id":{"d":1019,"n":1,"t":4},"speaker":"A","text":"梅雨時から夏の間は、憂鬱な気分になります。早く秋になってほしいものです。"},{"id":{"d":1019,"n":2,"t":4},"speaker":"B","text":"長島さんは暑いのが苦手なのですか？"},{"id":{"d":1019,"n":3,"t":4},"speaker":"A","text":"暑さというより湿気が嫌なのです。私の髪の毛は湿度が高いと好き勝手な方向に跳ねてしまうのです。"},{"id":{"d":1019,"n":4,"t":4},"speaker":"B","text":"髪の毛の水分をコントロールするシャンプーやコンディショナーがあるようですよ。試してみてはいかがですか？"},{"id":{"d":1019,"n":5,"t":4},"speaker":"A","text":"いろいろと試してはいるのですが、効果を感じられる製品を見つけられずにいます。ドラッグストアではなく、美容院で売っているようなシャンプーとコンディショナーを買った方がいいのかもしれません。"},{"id":{"d":1019,"n":6,"t":4},"speaker":"B","text":"ストレートパーマをかけてみるのはどうでしょう？"},{"id":{"d":1019,"n":7,"t":4},"speaker":"A","text":"ストレートパーマはまだ試したことがありません。髪の毛を傷めるような気がするからです。桜井さんはパーマをかけたことはありますか？"},{"id":{"d":1019,"n":8,"t":4},"speaker":"B","text":"はい、あります。でも、気になるほど髪は傷みませんでした。むしろ、髪の毛を真っすぐに伸ばすためにヘアアイロンやドライヤーを毎日長時間使うことの方が髪の毛への負担が大きいかもしれませんよ。"}];
INSERT INTO sentence [{"id":{"d":1020,"n":1,"t":4},"speaker":"A","text":"青木さんは、昨日の午後は体調が優れず、半日休暇を取って帰られたと聞きました。お加減はいかがですか？"},{"id":{"d":1020,"n":2,"t":4},"speaker":"B","text":"ご迷惑をおかけして申し訳ありませんでした。貧血でめまいがひどかったので午後に半日休暇を頂きました。クリニックで鉄剤を処方してもらい服用したので、今は幾分か楽になりました。"},{"id":{"d":1020,"n":3,"t":4},"speaker":"A","text":"青木さんは普段から貧血気味なのですか？"},{"id":{"d":1020,"n":4,"t":4},"speaker":"B","text":"時々貧血の症状が出ます。爪が白っぽくなっているときは貧血になりかけているので、無理をしないように気を付けています。"},{"id":{"d":1020,"n":5,"t":4},"speaker":"A","text":"爪がバロメーターなのですね。青木さんはスポーツマンでいつもお元気なご様子なので、体調が優れないと聞いて少し驚きました。"},{"id":{"d":1020,"n":6,"t":4},"speaker":"B","text":"強度の高い運動をしている人は貧血を起こすことが多いのです。学生時代の陸上部の仲間も、半数ほどが貧血持ちでした。"},{"id":{"d":1020,"n":7,"t":4},"speaker":"A","text":"そうなのですね。貧血予防に効果があるといわれている食材は取られていますか？レバー、ひじき、ほうれん草などは貧血予防によく効くそうですよ。"},{"id":{"d":1020,"n":8,"t":4},"speaker":"B","text":"それらの食材は積極的に食べるようにしています。でも、一番大事なのは栄養バランスの良い食事を取ることだそうなので、これからも食生活には気を付けたいと思います。"}];
INSERT INTO sentence [{"id":{"d":1021,"n":1,"t":4},"speaker":"A","text":"井上さん、受付までお越しください。"},{"id":{"d":1021,"n":2,"t":4},"speaker":"B","text":"はい、井上です。"},{"id":{"d":1021,"n":3,"t":4},"speaker":"A","text":"井上さん、カルテと照合しますので、フルネームと生年月日を教えてください。"},{"id":{"d":1021,"n":4,"t":4},"speaker":"B","text":"はい。井上玲子、昭和50年12月28日生まれです。"},{"id":{"d":1021,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。確認できました。井上さん、今日はどうなさいましたか？"},{"id":{"d":1021,"n":6,"t":4},"speaker":"B","text":"はい、ここ数日喉の痛みがあり、昨晩から熱も出てきました。今朝からは頭痛もします。"},{"id":{"d":1021,"n":7,"t":4},"speaker":"A","text":"熱があるのですね。この体温計で改めて測って、この用紙に記入してください。それから、壁際にある血圧計で血圧を測定して、結果が印刷された紙をこのファイルに挟んでこちらへお持ちください。"},{"id":{"d":1021,"n":8,"t":4},"speaker":"B","text":"はい、分かりました。待ち時間は長くなりそうですか？"},{"id":{"d":1021,"n":9,"t":4},"speaker":"A","text":"30分ほどお待ちいただくことになると思います。待っている間に具合が悪くなったら遠慮なく受付に声を掛けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1022,"n":1,"t":4},"speaker":"A","text":"あなたのネイルの色はとてもすてきですね。"},{"id":{"d":1022,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。でも、少し派手ではないかと気にしているのですが、大丈夫でしょうか？"},{"id":{"d":1022,"n":3,"t":4},"speaker":"A","text":"全然派手ではありませんよ。よく似合っています。"},{"id":{"d":1022,"n":4,"t":4},"speaker":"B","text":"それならよかったです。"},{"id":{"d":1022,"n":5,"t":4},"speaker":"A","text":"そういえば、平井さんがネイルをしているところは初めて見た気がします。"},{"id":{"d":1022,"n":6,"t":4},"speaker":"B","text":"はい。最近塗り始めたばかりです。乾燥のせいで爪の縦じわが目立つようになって気になっていたところ、娘からマニキュアには爪を保護する効果もあると教えてもらい、マニキュアを塗るようになりました。"},{"id":{"d":1022,"n":7,"t":4},"speaker":"A","text":"マニキュアを使うようになって、爪の調子はいかがですか？"},{"id":{"d":1022,"n":8,"t":4},"speaker":"B","text":"気になっていた縦じわが薄くなりましたし、爪が反り気味になっていたのも治りました。"},{"id":{"d":1022,"n":9,"t":4},"speaker":"A","text":"いいですね。私も爪のケアを兼ねてマニキュアをしてみようかしら。ドラッグストアで売っているマニキュアでも効果があるでしょうか？"},{"id":{"d":1022,"n":10,"t":4},"speaker":"B","text":"充分あると思います。マニキュアを塗る前にベースコート、塗った後にトップコートを付けると、マニキュアが剝がれにくくなり、爪をしっかりと守ることができますよ。"}];
INSERT INTO sentence [{"id":{"d":1023,"n":1,"t":4},"speaker":"A","text":"もしもし、お母さんですか？早苗です。毎日暑いですね。お父さんもお母さんも変わりありませんか？"},{"id":{"d":1023,"n":2,"t":4},"speaker":"B","text":"早苗さん、お電話をありがとうございます。お父さんもお母さんも元気ですよ。"},{"id":{"d":1023,"n":3,"t":4},"speaker":"A","text":"熱中症にならないように、きちんとエアコンを使って部屋の温度を下げていますか？"},{"id":{"d":1023,"n":4,"t":4},"speaker":"B","text":"窓を開けていれば涼しい風が入ってくるので、エアコンは使っていません。それでも、さほど汗もかきませんよ。"},{"id":{"d":1023,"n":5,"t":4},"speaker":"A","text":"それはいけません。年齢を重ねると、体温調節の機能が落ちてくるといわれています。汗もかきにくくなるそうです。感覚ではなく、室温計を確認して、数値で判断してください。"},{"id":{"d":1023,"n":6,"t":4},"speaker":"B","text":"はい、分かりました。これからそのようにします。水分は意識して摂取するようにしていますよ。"},{"id":{"d":1023,"n":7,"t":4},"speaker":"A","text":"それは良いことです。お水やお茶だけでなく、スポーツドリンクも飲んでください。スポーツドリンクは水分が体に吸収されやすいように、塩分や糖分が調整されていますから。"},{"id":{"d":1023,"n":8,"t":4},"speaker":"B","text":"甘い飲み物はお父さんが好きではないのですが、早苗さんのアドバイスだと言えば飲んでくれると思います。"}];
INSERT INTO sentence [{"id":{"d":1024,"n":1,"t":4},"speaker":"A","text":"先月から仕事に追われて寝不足が続いているためか、肌のコンディションが優れません。ニキビのようなできものができてなかなか消えないのです。"},{"id":{"d":1024,"n":2,"t":4},"speaker":"B","text":"肌荒れの原因は寝不足だけではないかもしれません。忙しくて栄養のある食事が取れていないのではありませんか？"},{"id":{"d":1024,"n":3,"t":4},"speaker":"A","text":"その通りです。食事は、コンビニエンスストアでお弁当やおにぎりを買ってきて、さっと済ませることが多いです。肌荒れ対策としてビタミン剤を飲んでいるのですが、効果を感じられません。"},{"id":{"d":1024,"n":4,"t":4},"speaker":"B","text":"やはりサプリメントより食事から栄養を取った方がいいと思いますよ。寝不足で疲れも出ているのではありませんか？"},{"id":{"d":1024,"n":5,"t":4},"speaker":"A","text":"日中に眠くなってしまっては困るので、栄養ドリンクを常備して飲むようにしています。ドリンク剤は即効性があり、エネルギーが身体にみなぎる感じがします。"},{"id":{"d":1024,"n":6,"t":4},"speaker":"B","text":"栄養ドリンクに頼り過ぎるのもあまり良くありませんね。柴田さんの身体が心配です。私は毎日お弁当を作ってきているので、今度柴田さんの分も作ってきましょうか？職場の休憩スペースで一緒に食べましょう。"},{"id":{"d":1024,"n":7,"t":4},"speaker":"A","text":"とてもありがたいことですが、それでは船木さんの負担が増えますよね。心苦しく思います。"},{"id":{"d":1024,"n":8,"t":4},"speaker":"B","text":"ついでですから、気にしないでください。ただし、朝食にバナナとヨーグルトくらいのものは食べてきてください。すきっ腹のままでいると、身体が栄養を吸収し過ぎて太ってしまったり、血糖値スパイクを引き起こしたりしますから。"},{"id":{"d":1024,"n":9,"t":4},"speaker":"A","text":"分かりました。明日から、食事に気を付けます。"}];
INSERT INTO sentence [{"id":{"d":1025,"n":1,"t":4},"speaker":"A","text":"今日は身体のあちらこちらが筋肉痛になってしまったので、階段の上り下りがつらいです。"},{"id":{"d":1025,"n":2,"t":4},"speaker":"B","text":"昨日は筋力トレーニングをしたのですか？"},{"id":{"d":1025,"n":3,"t":4},"speaker":"A","text":"筋力トレーニングではなく、YouTubeの有酸素運動の動画を見ながら20分程度、身体を動かしただけです。しかもそれは昨日ではなく、おとといのことなのです。運動してから筋肉痛になるまでにタイムラグがあるということは、もう若くないということですね。"},{"id":{"d":1025,"n":4,"t":4},"speaker":"B","text":"筋肉痛がすぐに出るから若いというわけではないようです。ハードな筋力トレーニングをすると早く筋肉痛が出るようです。"},{"id":{"d":1025,"n":5,"t":4},"speaker":"A","text":"私くらいの年齢になると、運動といっても身体への負荷の小さい運動になりがちですから、筋肉痛が出るのが遅いのかもしれません。今度、ジムに行って器具を使ったトレーニングを試してみようかと思います。"},{"id":{"d":1025,"n":6,"t":4},"speaker":"B","text":"器具を使うときはトレーナーに教わるといいですよ。でも、ただの筋肉痛では済まないかもしれません。場合によっては肉離れや靭帯損傷の可能性もありますから、くれぐれも慎重になさってください。"},{"id":{"d":1025,"n":7,"t":4},"speaker":"A","text":"そうですね。そのようなことになったら、トレーニングどころではなくなってしまいますよね。ほどほどにしておきます。"},{"id":{"d":1025,"n":8,"t":4},"speaker":"B","text":"はい、それがいいと思います。無理をしてけがをすることのないようにしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1026,"n":1,"t":4},"speaker":"A","text":"こう暑いと、どれほど水分を取っても全部が汗で出て行ってしまうようです。"},{"id":{"d":1026,"n":2,"t":4},"speaker":"B","text":"本当に暑いですね。汗をかくと、時間をかけてセットした髪も台無しです。美恵子さんのように髪をまとめられると涼しげでいいですね。"},{"id":{"d":1026,"n":3,"t":4},"speaker":"A","text":"麻美さんの髪もあと少し伸びればまとめられそうです。汗でお化粧が崩れてしまうのにも閉口します。何か良い方法はないものでしょうか？"},{"id":{"d":1026,"n":4,"t":4},"speaker":"B","text":"私は、最近は汗に強いといわれているファンデーションに替えました。夏用の下地クリームと併用したら、お化粧の持ちが良くなったように思います。"},{"id":{"d":1026,"n":5,"t":4},"speaker":"A","text":"麻美さんはきれいなお肌をしていますよね。どちらのメーカーの化粧品ですか？教えてください。"},{"id":{"d":1026,"n":6,"t":4},"speaker":"B","text":"下地は美天化粧堂、ファンデーションはカバーマークです。"},{"id":{"d":1026,"n":7,"t":4},"speaker":"A","text":"カバーマークの化粧品は使ったことがありません。自分に合うものがあるかどうかを調べてみようと思います。基礎化粧品はどちらのものを使っていますか？"},{"id":{"d":1026,"n":8,"t":4},"speaker":"B","text":"化粧水も乳液も洗顔フォームもメーク落としも全て美天化粧堂でそろえています。美容液も美天化粧堂です。私に合っているようで、今まで肌のトラブルで困ったことはありません。"}];
INSERT INTO sentence [{"id":{"d":1027,"n":1,"t":4},"speaker":"A","text":"前田さんのご主人を見かけましたが、とてもお痩せになっていて驚きました。ご主人は週に一度は草野球をしていると奥さんから聞いたことがありますが、その効果でしょうか？"},{"id":{"d":1027,"n":2,"t":4},"speaker":"B","text":"前田さんは随分と細くなられましたよね。草野球の効果だけではなくて、毎日歩いているのが良いのだと思います。"},{"id":{"d":1027,"n":3,"t":4},"speaker":"A","text":"毎日歩いているのですね。そういえば、以前に奥さんが話していたのですが、ご主人が会社の定期健康診断でメタボリックシンドロームと診断されたそうですよ。確か3年ほど前のことだったと思います。"},{"id":{"d":1027,"n":4,"t":4},"speaker":"B","text":"僕が歩いている前田さんを見かけるようになったのも確か3年ほど前からだと思います。ほとんど毎朝、僕の出勤の時間帯に歩いていらっしゃいます。"},{"id":{"d":1027,"n":5,"t":4},"speaker":"A","text":"毎朝ですか。偉いですね。"},{"id":{"d":1027,"n":6,"t":4},"speaker":"B","text":"ウォーキングは痩せると聞きますけれど、前田さんを見ると、それは本当だと思います。"},{"id":{"d":1027,"n":7,"t":4},"speaker":"A","text":"私も最近おなか周りが気になってきました。ウォーキングを始めようかと思っています。"},{"id":{"d":1027,"n":8,"t":4},"speaker":"B","text":"僕もお付き合いしましょう。帰宅後にウォーキングに出ることを習慣にしませんか？"}];
INSERT INTO sentence [{"id":{"d":1028,"n":1,"t":4},"speaker":"A","text":"インフルエンザの予防接種は打ちましたか？"},{"id":{"d":1028,"n":2,"t":4},"speaker":"B","text":"まだ打ちに行っていません。実は、ここ3年ほど、インフルエンザの予防接種をしていません。"},{"id":{"d":1028,"n":3,"t":4},"speaker":"A","text":"どうしてですか？"},{"id":{"d":1028,"n":4,"t":4},"speaker":"B","text":"ワクチンを打つつもりだったのですが、その前にインフルエンザにかかってしまったのです。"},{"id":{"d":1028,"n":5,"t":4},"speaker":"A","text":"予防接種が間に合わなかったのですね。インフルエンザにかかった時は高熱が出ましたか？"},{"id":{"d":1028,"n":6,"t":4},"speaker":"B","text":"去年は、熱はそれほど出ず症状も軽かったのですが、おととしとその前はつらい思いをしました。38度台後半の熱が出て、体の節々も痛かったです。家族にうつさないよう部屋に閉じこもり一人で熱と闘っていました。"},{"id":{"d":1028,"n":7,"t":4},"speaker":"A","text":"それは大変でしたね。今年こそは早く接種した方がよさそうですね。"},{"id":{"d":1028,"n":8,"t":4},"speaker":"B","text":"はい、そのつもりです。かかりつけのクリニックの予約を入れました。あさってが接種日です。"}];
INSERT INTO sentence [{"id":{"d":1029,"n":1,"t":4},"speaker":"A","text":"真田さんはマスクをしていますが、風邪をひいてしまったのですか？"},{"id":{"d":1029,"n":2,"t":4},"speaker":"B","text":"朝起きたら少し喉が痛かったので、念のためにマスクをしてきました。"},{"id":{"d":1029,"n":3,"t":4},"speaker":"A","text":"喉の痛み以外に発熱やせき、くしゃみなどはありませんか？"},{"id":{"d":1029,"n":4,"t":4},"speaker":"B","text":"その他の症状はありません。でも、私の風邪は喉の痛みから始まることが多いので、気を付けなければいけないと思っています。"},{"id":{"d":1029,"n":5,"t":4},"speaker":"A","text":"喉が痛いときはマヌカハニーをスプーンひとさじなめると、痛みが治まると聞いたことがありますよ。"},{"id":{"d":1029,"n":6,"t":4},"speaker":"B","text":"ニュージーランドの伝統食のマヌカハニーですね。帰りにスーパーで探してみようと思います。"},{"id":{"d":1029,"n":7,"t":4},"speaker":"A","text":"マヌカハニーをなめて栄養のある食事を食べて、たっぷりの睡眠を取ったら元気になると思いますよ。"},{"id":{"d":1029,"n":8,"t":4},"speaker":"B","text":"はい。今日は夜更かしせず、よく寝ようと思います。"}];
INSERT INTO sentence [{"id":{"d":1030,"n":1,"t":4},"speaker":"A","text":"今日は筋力トレーニングが終わったら走りに行きませんか？"},{"id":{"d":1030,"n":2,"t":4},"speaker":"B","text":"いいですね。昨日はランニングがなかったので、今日は少し長めの距離を走りましょう。外周コースと市街地コースのどちらにしましょうか？"},{"id":{"d":1030,"n":3,"t":4},"speaker":"A","text":"アップダウンのない外周コースにしましょう。宮崎さんが先頭を走ってくれますか？"},{"id":{"d":1030,"n":4,"t":4},"speaker":"B","text":"了解です。まだ部活に慣れていない1年生たちがいますから、ペースは抑えめで行きますね。"},{"id":{"d":1030,"n":5,"t":4},"speaker":"A","text":"そうしてください。私は一番後ろを行きます。先頭と最後尾は大きく離れてしまうと思うので、先にグラウンドに戻ったら、クールダウンをしっかりとするように皆に指示を出してください。"},{"id":{"d":1030,"n":6,"t":4},"speaker":"B","text":"分かりました。"},{"id":{"d":1030,"n":7,"t":4},"speaker":"A","text":"よろしくお願いします。それから水分補給も忘れないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":1031,"n":1,"t":4},"speaker":"A","text":"今日は歩き過ぎました。その上、履き慣れない靴を履いていたものですから、靴擦れができてしまったようです。"},{"id":{"d":1031,"n":2,"t":4},"speaker":"B","text":"かかとの横が真っ赤になっていますね。反対側は水膨れになっています。ばんそうこうは持っていますか？"},{"id":{"d":1031,"n":3,"t":4},"speaker":"A","text":"いいえ、今日に限って持っていません。"},{"id":{"d":1031,"n":4,"t":4},"speaker":"B","text":"確か私のハンドバックにばんそうこうが入っていたはずです。探しますから少し待っていてくださいね。見つかりました。これを使ってください。"},{"id":{"d":1031,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。とても助かります。これはお風呂に入ったときに染みそうです。"},{"id":{"d":1031,"n":6,"t":4},"speaker":"B","text":"そうですね。数日は痛むかもしれませんね。私は履き慣れた靴を履いてきましたが、それでもところどころ靴が当たる場所が赤くなっています。"},{"id":{"d":1031,"n":7,"t":4},"speaker":"A","text":"今日はこれ以上は歩かないようにしましょう。どこかに休憩できるところはないでしょうか？"},{"id":{"d":1031,"n":8,"t":4},"speaker":"B","text":"すぐそこにファミリーレストランがあります。あそこに入って少し休みましょう。"}];
INSERT INTO sentence [{"id":{"d":1032,"n":1,"t":4},"speaker":"A","text":"最近は何か運動をしていますか？"},{"id":{"d":1032,"n":2,"t":4},"speaker":"B","text":"はい。スポーツジムに通っています。"},{"id":{"d":1032,"n":3,"t":4},"speaker":"A","text":"偉いですね。週にどれくらいのペースで通っていますか？"},{"id":{"d":1032,"n":4,"t":4},"speaker":"B","text":"週3回のペースで行っていますよ。"},{"id":{"d":1032,"n":5,"t":4},"speaker":"A","text":"理想的なペースですね。続けるモチベーションは何ですか？"},{"id":{"d":1032,"n":6,"t":4},"speaker":"B","text":"腹筋が割れてくる過程を見ることです。美しい腹筋を見ると自己肯定感が上がります。"},{"id":{"d":1032,"n":7,"t":4},"speaker":"A","text":"そうなのですね。実は、私も少し前からジムに興味があるのです。"},{"id":{"d":1032,"n":8,"t":4},"speaker":"B","text":"私が通っているジムには、2週間の体験コースがありますよ。"},{"id":{"d":1032,"n":9,"t":4},"speaker":"A","text":"ぜひ試してみたいです。今度、一緒に行っても構いませんか？"},{"id":{"d":1032,"n":10,"t":4},"speaker":"B","text":"もちろんです。一緒に頑張りましょうね。"}];
INSERT INTO sentence [{"id":{"d":1033,"n":1,"t":4},"speaker":"A","text":"最近は何か運動をしていますか？"},{"id":{"d":1033,"n":2,"t":4},"speaker":"B","text":"はい。スイミングスクールに通っていますよ。"},{"id":{"d":1033,"n":3,"t":4},"speaker":"A","text":"水泳ですか？私は泳げないので、縁遠い話です。"},{"id":{"d":1033,"n":4,"t":4},"speaker":"B","text":"いいえ、そのようなことはありませんよ。私も泳げない状態からスタートしました。"},{"id":{"d":1033,"n":5,"t":4},"speaker":"A","text":"通っているうちに泳げるようになったのですか？"},{"id":{"d":1033,"n":6,"t":4},"speaker":"B","text":"ええ。クロールは完璧にできるまでになりました。"},{"id":{"d":1033,"n":7,"t":4},"speaker":"A","text":"どれくらいの期間で泳げるようになりましたか？"},{"id":{"d":1033,"n":8,"t":4},"speaker":"B","text":"私の場合は2カ月程度で泳げるようになりました。"},{"id":{"d":1033,"n":9,"t":4},"speaker":"A","text":"それは素晴らしいですね。スイミングスクールに通えば、私も泳げるようになるでしょうか？"},{"id":{"d":1033,"n":10,"t":4},"speaker":"B","text":"きっとなりますよ。もしよければ、来週から一緒に行きませんか？"}];
INSERT INTO sentence [{"id":{"d":1034,"n":1,"t":4},"speaker":"A","text":"夏バテをしてしまって、食欲がありません。"},{"id":{"d":1034,"n":2,"t":4},"speaker":"B","text":"そうだと思っていましたよ。ここ数日でかなりやつれてしまったように見えて心配です。"},{"id":{"d":1034,"n":3,"t":4},"speaker":"A","text":"そうめん以外のものが一切食べられなくて、困っています。"},{"id":{"d":1034,"n":4,"t":4},"speaker":"B","text":"私の食欲を分けてあげたいくらいです。私は、暇さえあればずっと何かを口に運んでしまいます。"},{"id":{"d":1034,"n":5,"t":4},"speaker":"A","text":"うらやましいです。胃腸の疲れは感じませんか？"},{"id":{"d":1034,"n":6,"t":4},"speaker":"B","text":"まったく感じません。毎日アイスを3個は食べていますが、胃腸は通常運転です。"},{"id":{"d":1034,"n":7,"t":4},"speaker":"A","text":"素晴らしいですね。聞いただけで胃がキリキリしてきました。"},{"id":{"d":1034,"n":8,"t":4},"speaker":"B","text":"すみません。夏バテがあまりにひどいようだったら病院に行ってくださいね。"},{"id":{"d":1034,"n":9,"t":4},"speaker":"A","text":"はい。今日の仕事が終わったら診療の予約を入れようと思います。"}];
INSERT INTO sentence [{"id":{"d":1035,"n":1,"t":4},"speaker":"A","text":"こんにちは。本日はどうされましたか？"},{"id":{"d":1035,"n":2,"t":4},"speaker":"B","text":"ニキビが治らないのです。最近はさらにひどくなってきました。"},{"id":{"d":1035,"n":3,"t":4},"speaker":"A","text":"現在、薬やサプリメントを服用したり、塗り薬を使ったりしていますか？"},{"id":{"d":1035,"n":4,"t":4},"speaker":"B","text":"はい。ステロイド配合の塗り薬を使用しております。"},{"id":{"d":1035,"n":5,"t":4},"speaker":"A","text":"いつから使用していますか？"},{"id":{"d":1035,"n":6,"t":4},"speaker":"B","text":"1カ月ほど前から塗布を開始し、それ以来、毎日欠かさず塗っています。"},{"id":{"d":1035,"n":7,"t":4},"speaker":"A","text":"それが原因の可能性がありますね。ステロイドは長期間使用すると皮膚が薄くなることもあるのです。"},{"id":{"d":1035,"n":8,"t":4},"speaker":"B","text":"まさか、良かれと思ってしていたことが裏目に出ていたとは驚きました。"},{"id":{"d":1035,"n":9,"t":4},"speaker":"A","text":"今日からお薬を変えてみましょう。処方箋をお出ししますね。"},{"id":{"d":1035,"n":10,"t":4},"speaker":"B","text":"ありがとうございます。よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":1036,"n":1,"t":4},"speaker":"A","text":"先生、ひどい頭痛がします。"},{"id":{"d":1036,"n":2,"t":4},"speaker":"B","text":"頭の痛みの他には、いつもと変わったことはありませんか？"},{"id":{"d":1036,"n":3,"t":4},"speaker":"A","text":"手足に力が入りにくいようです。"},{"id":{"d":1036,"n":4,"t":4},"speaker":"B","text":"それは熱中症の初期症状だと思われます。まずは水分を取ってください。"},{"id":{"d":1036,"n":5,"t":4},"speaker":"A","text":"そうします。あと、先ほどからあそこの電柱が3本にぶれて見えます。"},{"id":{"d":1036,"n":6,"t":4},"speaker":"B","text":"保健の先生を呼んでくるので、阿部さんに付き添ってもらって日陰で待っていてください。阿部さんにはあなたに水分補給をさせることと、首と脇と太ももを冷やすように伝えています。"},{"id":{"d":1036,"n":7,"t":4},"speaker":"A","text":"分かりました。不安なので早めに保健室の先生を連れて戻ってきてください。"},{"id":{"d":1036,"n":8,"t":4},"speaker":"B","text":"もちろんです。涼しいところで休んでいてください。"}];
INSERT INTO sentence [{"id":{"d":1037,"n":1,"t":4},"speaker":"A","text":"由紀さんはどうして日焼けをしないのですか？"},{"id":{"d":1037,"n":2,"t":4},"speaker":"B","text":"日焼け止めクリームを塗っているからだと思います。"},{"id":{"d":1037,"n":3,"t":4},"speaker":"A","text":"私も日焼け止めクリームを使用しているのですが、この通り、小麦色の肌です。"},{"id":{"d":1037,"n":4,"t":4},"speaker":"B","text":"私はサンブロックの日焼け止めクリームを使っていますよ。値段は高いですが効果はあると思います。"},{"id":{"d":1037,"n":5,"t":4},"speaker":"A","text":"高価なので買ったことがありませんでしたが、由紀さんの肌の白さを見て購入したくなってきました。"},{"id":{"d":1037,"n":6,"t":4},"speaker":"B","text":"もしよければ、今日一日だけでも試してみませんか？私のものを貸しますよ。"},{"id":{"d":1037,"n":7,"t":4},"speaker":"A","text":"よいのですか？高い日焼け止めだけに、使わせていただくのは気が引けます。"},{"id":{"d":1037,"n":8,"t":4},"speaker":"B","text":"1回分が減っても全然気になりませんよ。どうぞ使ってください。"},{"id":{"d":1037,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。由紀さんは優しいですね。"}];
INSERT INTO sentence [{"id":{"d":1038,"n":1,"t":4},"speaker":"A","text":"玉置君、足をけがしたと聞きましたが、来週の試合への影響はなさそうですか？"},{"id":{"d":1038,"n":2,"t":4},"speaker":"B","text":"いいえ、出場は難しそうです。ご覧の通り、松葉杖をつかなければならない状況です。"},{"id":{"d":1038,"n":3,"t":4},"speaker":"A","text":"それは残念です。あなたがいないと県大会に出場できるかが怪しいところですね。骨折していたのですか？"},{"id":{"d":1038,"n":4,"t":4},"speaker":"B","text":"骨折はしていませんでしたが、靭帯が切れていました。"},{"id":{"d":1038,"n":5,"t":4},"speaker":"A","text":"靭帯を痛めてしまったのですね。靭帯は骨よりも治りが遅いようですから、心配です。"},{"id":{"d":1038,"n":6,"t":4},"speaker":"B","text":"せめて引退試合には間に合うといいのですが。"},{"id":{"d":1038,"n":7,"t":4},"speaker":"A","text":"去年に中島先輩が靭帯を断裂した時は、3カ月間は本調子ではなかったようです。引退試合は3カ月後なので微妙なところですね。"},{"id":{"d":1038,"n":8,"t":4},"speaker":"B","text":"そうですね。でも、僕は引退試合への出場を諦めていません。これから、懸命にリハビリに励もうと思います。"}];
INSERT INTO sentence [{"id":{"d":1039,"n":1,"t":4},"speaker":"A","text":"沖田さん、今度の日曜日にご飯を食べに行きませんか？"},{"id":{"d":1039,"n":2,"t":4},"speaker":"B","text":"いいですね。どこに行きましょうか？"},{"id":{"d":1039,"n":3,"t":4},"speaker":"A","text":"駅前においしい鳥刺しで有名なお店があるのですが、そこはいかがですか？"},{"id":{"d":1039,"n":4,"t":4},"speaker":"B","text":"鳥刺しですか。鳥刺しは食中毒が怖いので、今まで避けてきていました。"},{"id":{"d":1039,"n":5,"t":4},"speaker":"A","text":"鶏肉はきちんと火を通さなければならないと言いますからね。"},{"id":{"d":1039,"n":6,"t":4},"speaker":"B","text":"はい。お店で出す場合は、焼かなくてもよいのでしょうか？"},{"id":{"d":1039,"n":7,"t":4},"speaker":"A","text":"新鮮な肉であれば、刺身で食べても問題ないのかもしれませんね。とにかく、とてもおいしいので、ぜひあなたと一緒に行きたいのです。"},{"id":{"d":1039,"n":8,"t":4},"speaker":"B","text":"分かりました。あなたがそれほどまでに勧めるお店なら、ぜひご一緒させてください。"}];
INSERT INTO sentence [{"id":{"d":1040,"n":1,"t":4},"speaker":"A","text":"腕を蚊に刺されていますね。かなり腫れていて、とてもかゆそうです。"},{"id":{"d":1040,"n":2,"t":4},"speaker":"B","text":"昨晩、寝ている時に刺されたようです。蚊を見つけ出して、家から追い出さなければなりません。"},{"id":{"d":1040,"n":3,"t":4},"speaker":"A","text":"ちょうど先日、室内で使用できる虫よけを買ってきましたよ。蚊が見つかるまではこれを使いましょう。"},{"id":{"d":1040,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。ところで、蚊に刺されたところがまたかゆくなってきました。"},{"id":{"d":1040,"n":5,"t":4},"speaker":"A","text":"去年買ったものですが、かゆみ止めがありますよ。使いますか？"},{"id":{"d":1040,"n":6,"t":4},"speaker":"B","text":"気持ちはありがたいのですが、かゆみ止めの使用期限は半年だそうです。"},{"id":{"d":1040,"n":7,"t":4},"speaker":"A","text":"そうですか、ではこれは使わない方がいいですね。代わりに、氷で冷やして、かゆみを紛らわしましょう。"},{"id":{"d":1040,"n":8,"t":4},"speaker":"B","text":"助かります。"}];
INSERT INTO sentence [{"id":{"d":1041,"n":1,"t":4},"speaker":"A","text":"飯田さん、顔色があまり良くありませんね。体調が悪いのですか？"},{"id":{"d":1041,"n":2,"t":4},"speaker":"B","text":"はい。ひどい二日酔いなのです。"},{"id":{"d":1041,"n":3,"t":4},"speaker":"A","text":"昨晩に飲み過ぎたのですね。お酒は百害あって一利なしですよ。"},{"id":{"d":1041,"n":4,"t":4},"speaker":"B","text":"もちろん、分かっています。普段はそれほど飲みませんが、昨日は飲まなければどうしようもない気分だったのです。"},{"id":{"d":1041,"n":5,"t":4},"speaker":"A","text":"何かあったのですか？私でよければお話を聞きますよ。"},{"id":{"d":1041,"n":6,"t":4},"speaker":"B","text":"実は、恋人と別れたのです。急に別れを切り出されたので、気持ちが追いつかない状態です。"},{"id":{"d":1041,"n":7,"t":4},"speaker":"A","text":"それはつらかったですね。心の傷を癒やすには、おいしいものを食べるに限りますよ。今晩、一緒にいかがですか？"},{"id":{"d":1041,"n":8,"t":4},"speaker":"B","text":"お誘いはありがたいのですが、明日行きましょう。今日はとにかく胃腸の調子を整えたいです。"},{"id":{"d":1041,"n":9,"t":4},"speaker":"A","text":"そうでしたね。お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":1042,"n":1,"t":4},"speaker":"A","text":"今日のメークはいつもと違いますね。新しい化粧品を買ったのですか？"},{"id":{"d":1042,"n":2,"t":4},"speaker":"B","text":"朝美さんはさすが鋭いですね。アイシャドーを新調しました。"},{"id":{"d":1042,"n":3,"t":4},"speaker":"A","text":"やはりそうでしたか。とても似合っていますよ。肌の色がよりきれいに見えます。"},{"id":{"d":1042,"n":4,"t":4},"speaker":"B","text":"自分に合うかが心配だったので、褒めていただいてうれしいです。"},{"id":{"d":1042,"n":5,"t":4},"speaker":"A","text":"とてもすてきです。どちらの商品か教えてもらえますか？"},{"id":{"d":1042,"n":6,"t":4},"speaker":"B","text":"LALAオーガニックで買いました。いろいろな色があって、選ぶのが楽しかったです。"},{"id":{"d":1042,"n":7,"t":4},"speaker":"A","text":"LALAオーガニックはこれほどまでに発色がいいのですね。海外のブランドのものは日本のものに比べて派手な色が多い印象があります。"},{"id":{"d":1042,"n":8,"t":4},"speaker":"B","text":"そうですね。確かに、日本のブランドは、海外のブランドに比べると肌なじみのいい色が多いかもしれません。"}];
INSERT INTO sentence [{"id":{"d":1043,"n":1,"t":4},"speaker":"A","text":"1時間前からしゃっくりが止まらなくて困っています。対処法をご存じありませんか？"},{"id":{"d":1043,"n":2,"t":4},"speaker":"B","text":"それはつらいですね。30秒間息を止めるとしゃっくりも止まると聞いたことがありますよ。"},{"id":{"d":1043,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。早速試してみます。"},{"id":{"d":1043,"n":4,"t":4},"speaker":"B","text":"どうですか？止まりましたか？"},{"id":{"d":1043,"n":5,"t":4},"speaker":"A","text":"あ、止まったようです！効果てきめんでした。"},{"id":{"d":1043,"n":6,"t":4},"speaker":"B","text":"止まって良かったです。ところで、最近は十分な睡眠時間を確保できていますか？"},{"id":{"d":1043,"n":7,"t":4},"speaker":"A","text":"いいえ、寝不足の日々が続いていますが、しゃっくりと何か関係があるのですか？"},{"id":{"d":1043,"n":8,"t":4},"speaker":"B","text":"しゃっくりは横隔膜のけいれんのことを言うようで、そのけいれんの症状は寝不足の時に多く見られるそうです。"},{"id":{"d":1043,"n":9,"t":4},"speaker":"A","text":"そうなのですか。知りませんでした。"},{"id":{"d":1043,"n":10,"t":4},"speaker":"B","text":"今晩からよく寝るようにしてみてください。"}];
INSERT INTO sentence [{"id":{"d":1044,"n":1,"t":4},"speaker":"A","text":"岡本さん、今日はどうされましたか？"},{"id":{"d":1044,"n":2,"t":4},"speaker":"B","text":"足のむくみがひどいのです。以前に先生に診ていただいた時には、何ともありませんでしたのに。"},{"id":{"d":1044,"n":3,"t":4},"speaker":"A","text":"そうですか。以前にお渡しした薬が体に合っていなかったのかもしれませんね。"},{"id":{"d":1044,"n":4,"t":4},"speaker":"B","text":"そのようなことがあるのですか？"},{"id":{"d":1044,"n":5,"t":4},"speaker":"A","text":"ええ。100人に1人程度の割合で、副反応としてむくみが見られることがあります。"},{"id":{"d":1044,"n":6,"t":4},"speaker":"B","text":"では、前に出していただいた薬を飲み切るまでは、むくみを我慢しなければならないということでしょうか？"},{"id":{"d":1044,"n":7,"t":4},"speaker":"A","text":"いいえ、別の薬に切り替えましょう。成分は違いますが効果は同じです。"},{"id":{"d":1044,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。よろしくお願いいたします。"},{"id":{"d":1044,"n":9,"t":4},"speaker":"A","text":"これで改善されなかったら、腎臓の検査をしてみましょう。"}];
INSERT INTO sentence [{"id":{"d":1045,"n":1,"t":4},"speaker":"A","text":"最近は、各メーカーから多様なシャンプーが販売されていますね。"},{"id":{"d":1045,"n":2,"t":4},"speaker":"B","text":"そうですね。スーパーに行っても、シャンプー売り場の広さに驚かされます。"},{"id":{"d":1045,"n":3,"t":4},"speaker":"A","text":"そこで問題なのが、たくさんありすぎて、どのシャンプーを選ぶべきか分からないということです。"},{"id":{"d":1045,"n":4,"t":4},"speaker":"B","text":"そうですね。私は、売り場の前で吟味した挙げ句、いつも一番安いものを選んでしまいます。"},{"id":{"d":1045,"n":5,"t":4},"speaker":"A","text":"私は交流サイトで話題のものに手を伸ばしがちですが、今のところは髪質が良くなった実感はありません。"},{"id":{"d":1045,"n":6,"t":4},"speaker":"B","text":"家庭用のものでは限界があるのかもしれませんね。ちなみに、今は何を使っていますか？"},{"id":{"d":1045,"n":7,"t":4},"speaker":"A","text":"YUFUというシャンプーです。森さんは何を使っていますか？"},{"id":{"d":1045,"n":8,"t":4},"speaker":"B","text":"輝姫のものを使用しています。輝姫はいつでも安いので重宝しています。"}];
INSERT INTO sentence [{"id":{"d":1046,"n":1,"t":4},"speaker":"A","text":"そろそろ夏ですね。肌を見せる機会が多いので、肌荒れが気になります。"},{"id":{"d":1046,"n":2,"t":4},"speaker":"B","text":"そうですね。冬なら服で隠せる腕や脚も、夏は隠せませんからね。"},{"id":{"d":1046,"n":3,"t":4},"speaker":"A","text":"実は、カミソリ負けをしてしまって、余計に肌を見せたくないのです。"},{"id":{"d":1046,"n":4,"t":4},"speaker":"B","text":"なるほど。それなら、脱毛してみてはどうですか？"},{"id":{"d":1046,"n":5,"t":4},"speaker":"A","text":"脱毛は高額で通えません。他に方法を知りませんか？"},{"id":{"d":1046,"n":6,"t":4},"speaker":"B","text":"カミソリで処理をするのなら、そるときの肌の負担を軽くすることが重要です。カミソリを使うときにクリームを塗っていますか？"},{"id":{"d":1046,"n":7,"t":4},"speaker":"A","text":"いいえ、ボディーソープを潤滑剤として使っています。"},{"id":{"d":1046,"n":8,"t":4},"speaker":"B","text":"それも肌に負担をかける一因かと思われます。まずは、少なくとも肌が元通りになるまでは、カミソリを使うことをやめてみてはどうでしょうか？"}];
INSERT INTO sentence [{"id":{"d":1047,"n":1,"t":4},"speaker":"A","text":"最近は、冷たいものが歯に染みるようになってきました。"},{"id":{"d":1047,"n":2,"t":4},"speaker":"B","text":"もしかして、それは虫歯ではありませんか？"},{"id":{"d":1047,"n":3,"t":4},"speaker":"A","text":"今まで虫歯は一度もできたことがないので、そのようなはずはないと思います。"},{"id":{"d":1047,"n":4,"t":4},"speaker":"B","text":"虫歯というものは、気付いたらできているものなのですよ。"},{"id":{"d":1047,"n":5,"t":4},"speaker":"A","text":"そうなのですか？嫌ですね。"},{"id":{"d":1047,"n":6,"t":4},"speaker":"B","text":"しかも、治療が遅れると神経まで取らなければいけないこともあります。"},{"id":{"d":1047,"n":7,"t":4},"speaker":"A","text":"神経までですか。それは大ごとですね。"},{"id":{"d":1047,"n":8,"t":4},"speaker":"B","text":"はい。ですから、歯に違和感がある時は、できるだけ早く歯科医を受診することをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":1048,"n":1,"t":4},"speaker":"A","text":"ここ数日、耳の奥に違和感があります。"},{"id":{"d":1048,"n":2,"t":4},"speaker":"B","text":"どのような違和感ですか？"},{"id":{"d":1048,"n":3,"t":4},"speaker":"A","text":"耳に水が入っているときのような感覚です。常にゴーッという音が鳴り続けています。"},{"id":{"d":1048,"n":4,"t":4},"speaker":"B","text":"私も2年ほど前に同じような症状を経験しました。耳鼻科で診察してもらったら、中耳炎と診断されました。"},{"id":{"d":1048,"n":5,"t":4},"speaker":"A","text":"そうでしたか。では私も、病院へ行った方がよさそうですね。"},{"id":{"d":1048,"n":6,"t":4},"speaker":"B","text":"はい。悪化する前にできるだけ早く受診してください。"},{"id":{"d":1048,"n":7,"t":4},"speaker":"A","text":"そうします。ちなみに、どのくらいの期間で治すことができましたか？"},{"id":{"d":1048,"n":8,"t":4},"speaker":"B","text":"抗生物質を処方してもらったら、1週間ほどで良くなりましたよ。"}];
INSERT INTO sentence [{"id":{"d":1049,"n":1,"t":4},"speaker":"A","text":"お母さん、喉の奥に痛みがありますがどうしたらよいでしょうか？"},{"id":{"d":1049,"n":2,"t":4},"speaker":"B","text":"いつから痛みを感じますか？"},{"id":{"d":1049,"n":3,"t":4},"speaker":"A","text":"夕食の後からです。ちくちくするような痛みがあります。"},{"id":{"d":1049,"n":4,"t":4},"speaker":"B","text":"もしかしたら、今晩に食べた焼き魚の骨が喉に刺さってしまったのかもしれません。"},{"id":{"d":1049,"n":5,"t":4},"speaker":"A","text":"怖いです。喉を貫通することはないでしょうか？"},{"id":{"d":1049,"n":6,"t":4},"speaker":"B","text":"それはないと思いますが、念のためにすぐにお医者さんに診てもらいましょう。"},{"id":{"d":1049,"n":7,"t":4},"speaker":"A","text":"今の時間に開いている病院があるのですか？"},{"id":{"d":1049,"n":8,"t":4},"speaker":"B","text":"緊急外来は開いているはずです。"},{"id":{"d":1049,"n":9,"t":4},"speaker":"A","text":"それは少し大げさに感じるので、明日の朝に病院へ連れて行ってください。"},{"id":{"d":1049,"n":10,"t":4},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":1050,"n":1,"t":4},"speaker":"A","text":"鈴木君、足を痛めたと聞きましたが、大丈夫でしたか？"},{"id":{"d":1050,"n":2,"t":4},"speaker":"B","text":"病院に行ったら骨折と診断されてしまいました。見ての通り、松葉杖がないと移動できません。"},{"id":{"d":1050,"n":3,"t":4},"speaker":"A","text":"それは大変ですね。日常生活を送る上で最も支障が出るのはどのようなことですか？"},{"id":{"d":1050,"n":4,"t":4},"speaker":"B","text":"階段の上り下りです。学校にはエレベーターがあるので助かりました。"},{"id":{"d":1050,"n":5,"t":4},"speaker":"A","text":"そうですか。確か、鈴木君の家は3階建てでしたよね。"},{"id":{"d":1050,"n":6,"t":4},"speaker":"B","text":"そうです。僕の部屋は3階にあるので、家の中での移動が一番大変でした。"},{"id":{"d":1050,"n":7,"t":4},"speaker":"A","text":"それは大変そうです。早く治るといいですね。"},{"id":{"d":1050,"n":8,"t":4},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1051,"n":1,"t":4},"speaker":"A","text":"玲香さん、首に湿布を貼っていますがどうしたのですか？"},{"id":{"d":1051,"n":2,"t":4},"speaker":"B","text":"首をひどく寝違えてしまいました。"},{"id":{"d":1051,"n":3,"t":4},"speaker":"A","text":"そうだったのですか。言われてみれば、ずっと首が傾いていますね。"},{"id":{"d":1051,"n":4,"t":4},"speaker":"B","text":"はい。首をまっすぐにすると、激痛が走るのです。"},{"id":{"d":1051,"n":5,"t":4},"speaker":"A","text":"それはお気の毒です。枕が合っていなかったのでしょうか？"},{"id":{"d":1051,"n":6,"t":4},"speaker":"B","text":"いいえ。実は、昨日、本を読みながらいつの間にか眠ってしまったのです。そのまま5時間ほど変な体勢で寝ていたせいで、寝違えてしまったのだと思います。"},{"id":{"d":1051,"n":7,"t":4},"speaker":"A","text":"私もよくやってしまいます。そのようなときには頭痛にもなりやすいですよね。"},{"id":{"d":1051,"n":8,"t":4},"speaker":"B","text":"はい。先ほどまで頭痛もあって、おでこに冷却ジェルシートを貼っていましたよ。"},{"id":{"d":1051,"n":9,"t":4},"speaker":"A","text":"早く治るといいですね。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":1052,"n":1,"t":4},"speaker":"A","text":"動きがぎこちなく見えますが、どうしましたか？"},{"id":{"d":1052,"n":2,"t":4},"speaker":"B","text":"昨日は久しぶりにジムに行ったので、朝から筋肉痛がひどいのです。"},{"id":{"d":1052,"n":3,"t":4},"speaker":"A","text":"けがではなくて安心しました。どこの部位をトレーニングしたのですか？"},{"id":{"d":1052,"n":4,"t":4},"speaker":"B","text":"背中と腕と足です。"},{"id":{"d":1052,"n":5,"t":4},"speaker":"A","text":"つまり、全身が筋肉痛ということですか？"},{"id":{"d":1052,"n":6,"t":4},"speaker":"B","text":"はい。全身の至るところがとても痛いです。"},{"id":{"d":1052,"n":7,"t":4},"speaker":"A","text":"筋肉痛にはマッサージがいいと聞いたことがあります。試してみますか？"},{"id":{"d":1052,"n":8,"t":4},"speaker":"B","text":"いいえ、やめておきます。今は少しでも刺激を与えると痛みで大騒ぎしてしまいそうです。"}];
INSERT INTO sentence [{"id":{"d":1053,"n":1,"t":4},"speaker":"A","text":"誰か、ばんそうこうを持っている人はいませんか？"},{"id":{"d":1053,"n":2,"t":4},"speaker":"B","text":"持っていますよ。けがをしたのですか？"},{"id":{"d":1053,"n":3,"t":4},"speaker":"A","text":"けがというほどではありませんが、靴擦れを起こしてしまいました。"},{"id":{"d":1053,"n":4,"t":4},"speaker":"B","text":"本当ですね。かなり大きな水膨れになってしまっています。"},{"id":{"d":1053,"n":5,"t":4},"speaker":"A","text":"靴が合っていなかったのかもしれませんね。"},{"id":{"d":1053,"n":6,"t":4},"speaker":"B","text":"今日の靴はおろしたばかりですか？"},{"id":{"d":1053,"n":7,"t":4},"speaker":"A","text":"そうです。奮発して買ったものなので、早速靴擦れになってしまってとても悲しいです。"},{"id":{"d":1053,"n":8,"t":4},"speaker":"B","text":"革靴は履けば履くほど足になじんできます。初めのうちは靴擦れ対策をしっかりしながら履き続ければ、そのうち問題なく履けるようになるはずですよ。"},{"id":{"d":1053,"n":9,"t":4},"speaker":"A","text":"そうなのですね。そうなるのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":1054,"n":1,"t":4},"speaker":"A","text":"美里さん、一つお願いがあります。"},{"id":{"d":1054,"n":2,"t":4},"speaker":"B","text":"何ですか？"},{"id":{"d":1054,"n":3,"t":4},"speaker":"A","text":"次の公式戦でホームランを打ったら、僕とお付き合いしてもらえませんか？"},{"id":{"d":1054,"n":4,"t":4},"speaker":"B","text":"次の相手は前回大会で優勝の強豪校ですよ。そのような約束をして平気なのですか？"},{"id":{"d":1054,"n":5,"t":4},"speaker":"A","text":"はい。どのような状況でも打てるのが本物の強い選手だと思います。僕が強い選手になれたら、そのときに僕とのお付き合いを考えてほしいのです。"},{"id":{"d":1054,"n":6,"t":4},"speaker":"B","text":"分かりました。絶対に打ってきてくださいね。ヒットでは許しませんよ。"},{"id":{"d":1054,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。頑張ります。では、今から打ち込みに行ってきます。"},{"id":{"d":1054,"n":8,"t":4},"speaker":"B","text":"体を壊さないように頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":1055,"n":1,"t":4},"speaker":"A","text":"今朝も、ひげそり中に切り傷を作ってしまいました。"},{"id":{"d":1055,"n":2,"t":4},"speaker":"B","text":"てっきり飼い猫に引っかかれた傷だと思っていました。痛そうですね。"},{"id":{"d":1055,"n":3,"t":4},"speaker":"A","text":"とても痛いです。何か良い方法はないでしょうか？"},{"id":{"d":1055,"n":4,"t":4},"speaker":"B","text":"シェービングクリームは使用していますか？肌を保護してくれると父が言っていました。"},{"id":{"d":1055,"n":5,"t":4},"speaker":"A","text":"使っていますが、それでもどうしても肌が傷ついてしまいます。"},{"id":{"d":1055,"n":6,"t":4},"speaker":"B","text":"それでは、ひげ脱毛はどうでしょうか？最近はお手頃な価格で体験ができるところもあるようです。"},{"id":{"d":1055,"n":7,"t":4},"speaker":"A","text":"脱毛は考えたことがありませんでした。"},{"id":{"d":1055,"n":8,"t":4},"speaker":"B","text":"お手入れが格段に楽になるそうですよ。"},{"id":{"d":1055,"n":9,"t":4},"speaker":"A","text":"それはとても魅力的です。脱毛を検討してみます。"}];
INSERT INTO sentence [{"id":{"d":1056,"n":1,"t":4},"speaker":"A","text":"サッカー部に女子部員はいますか？"},{"id":{"d":1056,"n":2,"t":4},"speaker":"B","text":"いいえ。去年まではいましたが、彼女が卒業してからは男子部員のみです。"},{"id":{"d":1056,"n":3,"t":4},"speaker":"A","text":"そうですか。私は、マネージャーではなく部員としての入部を希望しているのですが可能でしょうか？"},{"id":{"d":1056,"n":4,"t":4},"speaker":"B","text":"はい、もちろん歓迎します。サッカー経験はありますか？"},{"id":{"d":1056,"n":5,"t":4},"speaker":"A","text":"はい。小中学校でサッカー部に所属していました。レギュラーになった経験もあります。"},{"id":{"d":1056,"n":6,"t":4},"speaker":"B","text":"それでしたら何も問題ないと思います。練習で走り込みをすることが多いですが、大丈夫ですか？"},{"id":{"d":1056,"n":7,"t":4},"speaker":"A","text":"はい、そのために毎日5キロメートルのランニングをしています。"},{"id":{"d":1056,"n":8,"t":4},"speaker":"B","text":"それは有望ですね。では運動着に着替えてきてください。女子更衣室は校旗の横のプレハブの建物です。"},{"id":{"d":1056,"n":9,"t":4},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1057,"n":1,"t":4},"speaker":"A","text":"最近は、お昼ご飯に自作のお弁当を持ってきておられますね。"},{"id":{"d":1057,"n":2,"t":4},"speaker":"B","text":"はい。ダイエットのために、低カロリーの食べ物を持参しています。"},{"id":{"d":1057,"n":3,"t":4},"speaker":"A","text":"外食となると、どうしてもカロリーの高い食べ物になりがちですものね。今日のお弁当は何ですか？"},{"id":{"d":1057,"n":4,"t":4},"speaker":"B","text":"今日は、鶏ハム、ブロッコリー、ゆで卵です。"},{"id":{"d":1057,"n":5,"t":4},"speaker":"A","text":"まるで、ボディービルディングの選手のような食事ですね。ダイエットの効果は出ていますか？"},{"id":{"d":1057,"n":6,"t":4},"speaker":"B","text":"はい。1カ月続けたところ、2キログラム減量しました。"},{"id":{"d":1057,"n":7,"t":4},"speaker":"A","text":"すばらしい成果ですね。私もあなたを見習って、お弁当を自作してみようかと思い始めました。"},{"id":{"d":1057,"n":8,"t":4},"speaker":"B","text":"慣れてしまえば案外簡単なものですよ。ぜひ一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":1058,"n":1,"t":4},"speaker":"A","text":"今日もお昼ご飯を抜くのですか？ダイエット中ですか？"},{"id":{"d":1058,"n":2,"t":4},"speaker":"B","text":"はい。摂取カロリーを抑えるために、お昼ご飯を食べないようにしています。"},{"id":{"d":1058,"n":3,"t":4},"speaker":"A","text":"食事量の制限をするならば、ランチではなくディナーで行った方が効果的ですよ。"},{"id":{"d":1058,"n":4,"t":4},"speaker":"B","text":"そうなのですか？それはどうしてでしょうか？"},{"id":{"d":1058,"n":5,"t":4},"speaker":"A","text":"お昼時は体のカロリー消費が大きいため、食べたものを消化しやすいのです。一方で、夜はカロリー消費量が少ないため、食べたものが体に蓄えられやすいのです。"},{"id":{"d":1058,"n":6,"t":4},"speaker":"B","text":"なるほど。納得しました。明日から夜に食事制限をしたいと思います。"},{"id":{"d":1058,"n":7,"t":4},"speaker":"A","text":"より効率的にダイエットができますよ。"},{"id":{"d":1058,"n":8,"t":4},"speaker":"B","text":"教えてくださり、ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":1059,"n":1,"t":4},"speaker":"A","text":"お母さん、毛虫に刺されてしまったかもしれません。"},{"id":{"d":1059,"n":2,"t":4},"speaker":"B","text":"刺されたところを急いで洗い流しましょう。どこを刺されましたか？"},{"id":{"d":1059,"n":3,"t":4},"speaker":"A","text":"分かりました。右手の親指の付け根あたりが怪しいです。"},{"id":{"d":1059,"n":4,"t":4},"speaker":"B","text":"毛虫はどこにいたのですか？"},{"id":{"d":1059,"n":5,"t":4},"speaker":"A","text":"庭のブルーベリーの木にいました。果実の収穫をしていたところ、手に毛虫が落ちてきたのです。"},{"id":{"d":1059,"n":6,"t":4},"speaker":"B","text":"そうでしたか。無農薬で育てているのでいろいろな虫がいることは知っていましたが、害虫がいることは把握していませんでした。"},{"id":{"d":1059,"n":7,"t":4},"speaker":"A","text":"私も知りませんでした。お母さんも気を付けてくださいね。"},{"id":{"d":1059,"n":8,"t":4},"speaker":"B","text":"ありがとうございます、気を付けます。"},{"id":{"d":1059,"n":9,"t":4},"speaker":"A","text":"刺された所は、水で洗ったら、あとはこのままにしておいてよいのですか？"},{"id":{"d":1059,"n":10,"t":4},"speaker":"B","text":"はい。一応、付け根の様子は注意深く観察しておいてください。腫れやかゆみが少しでも感じられたら、すぐに病院へ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1060,"n":1,"t":4},"speaker":"A","text":"先生、パスの練習中に突き指をしてしまいました。"},{"id":{"d":1060,"n":2,"t":4},"speaker":"B","text":"見せてください。少し腫れがありますね。今日の午後の練習は休んでください。"},{"id":{"d":1060,"n":3,"t":4},"speaker":"A","text":"嫌です。他の部員に差をつけられたくないため、練習には参加します。"},{"id":{"d":1060,"n":4,"t":4},"speaker":"B","text":"いけません、必ず休んでください。突き指は靭帯が傷ついたときに起こるので、その部位を休めないと痛みが長引きます。そうすると、その指をかばうフォームが定着してしまいますよ。"},{"id":{"d":1060,"n":5,"t":4},"speaker":"A","text":"分かりました。でも、走り込みやフットワークなど、手を使わないメニューには参加してもよいですよね？"},{"id":{"d":1060,"n":6,"t":4},"speaker":"B","text":"運動をして血流が良くなると腫れがひどくなりますから、それらの練習も避けた方が無難です。突き指を早く治すことも、練習の一つだと思ってください。"},{"id":{"d":1060,"n":7,"t":4},"speaker":"A","text":"はい。今日の午後の練習はお休みします。"},{"id":{"d":1060,"n":8,"t":4},"speaker":"B","text":"今日だけではなく、指の状態が元に戻るまで運動は控えてください。お大事になさってくださいね。"}];
