INSERT INTO sentence [{"id":{"d":94,"n":1,"t":4},"speaker":"A","text":"こんにちは、お待たせしました。今日はどのような髪型になさいますか？"},{"id":{"d":94,"n":2,"t":4},"speaker":"B","text":"今日は伸びた分を切って、色を染め直したいと思っています。"},{"id":{"d":94,"n":3,"t":4},"speaker":"A","text":"そうしましたら、髪は少しだけ切って、色は明るくされますか？"},{"id":{"d":94,"n":4,"t":4},"speaker":"B","text":"そうですね、髪は傷んでいる部分を、切っていただきたいと思っています。色は、今の色が明るすぎるので、もう少し暗くしたいです。"},{"id":{"d":94,"n":5,"t":4},"speaker":"A","text":"承知しました。もうすぐ秋ですし、傷んだ部分を切って、秋らしい落ち着いた色に染め直しましょうか？"},{"id":{"d":94,"n":6,"t":4},"speaker":"B","text":"はい、よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":95,"n":1,"t":4},"speaker":"A","text":"最近、あちこちにリラクゼーション業が増えましたね。"},{"id":{"d":95,"n":2,"t":4},"speaker":"B","text":"会社への通勤途中にもあります。"},{"id":{"d":95,"n":3,"t":4},"speaker":"A","text":"新聞によるとコンビニエンスストアより店舗数が増えているらしいです。こうなると生存競争が厳しいですね。"},{"id":{"d":95,"n":4,"t":4},"speaker":"B","text":"いくらくらいで、ケアをしてもらえるのですか？"},{"id":{"d":95,"n":5,"t":4},"speaker":"A","text":"インターネットの予約サイトだと、60分3,000円前後が相場らしいです。"},{"id":{"d":95,"n":6,"t":4},"speaker":"B","text":"詳しいですね。そのくらいの金額であれば、今度、近所のお店に行ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":96,"n":1,"t":4},"speaker":"A","text":"先月、インフルエンザの予防接種に行ってきました。"},{"id":{"d":96,"n":2,"t":4},"speaker":"B","text":"風邪ばかりに目が行くけれど、インフルエンザにも気を付けないといけませんね。"},{"id":{"d":96,"n":3,"t":4},"speaker":"A","text":"予防接種費用の相場は、4,000円から4,500円くらいですよね？"},{"id":{"d":96,"n":4,"t":4},"speaker":"B","text":"結構高いですね。"},{"id":{"d":96,"n":5,"t":4},"speaker":"A","text":"予防接種をするかしないかは本人の自由ですが、お守り代わりに接種するのも良いですよね。"},{"id":{"d":96,"n":6,"t":4},"speaker":"B","text":"確かにそうですね。前向きに検討します。"}];
INSERT INTO sentence [{"id":{"d":97,"n":1,"t":4},"speaker":"A","text":"最近血圧が高くて、頭がくらくらする時があります。"},{"id":{"d":97,"n":2,"t":4},"speaker":"B","text":"お医者さんには、かかっているのですか？"},{"id":{"d":97,"n":3,"t":4},"speaker":"A","text":"循環器科に、半年前から通院しています。"},{"id":{"d":97,"n":4,"t":4},"speaker":"B","text":"そこでお医者さんからは、何かアドバイスがありますか？"},{"id":{"d":97,"n":5,"t":4},"speaker":"A","text":"塩分の取り過ぎに注意するように言われました。食事から見直すように指導を受けています。"},{"id":{"d":97,"n":6,"t":4},"speaker":"B","text":"塩分の取り過ぎには気を付けなければいけませんね。薬はもらっているのですか？"},{"id":{"d":97,"n":7,"t":4},"speaker":"A","text":"強い薬をもらっています。これから一生付き合っていかなくてはならない病気の可能性があると、お医者さんが話していました。"},{"id":{"d":97,"n":8,"t":4},"speaker":"B","text":"くれぐれも、無理はしないようにしてください。"}];
INSERT INTO sentence [{"id":{"d":98,"n":1,"t":4},"speaker":"A","text":"最近、私はウオーキングを始めました。"},{"id":{"d":98,"n":2,"t":4},"speaker":"B","text":"あなたはどのくらい、歩いているのですか？"},{"id":{"d":98,"n":3,"t":4},"speaker":"A","text":"休日に3,000歩程度です。以前は5,000歩以上歩いていましたが、疲れが残るので調整しています。"},{"id":{"d":98,"n":4,"t":4},"speaker":"B","text":"運動をしないよりは、良いと思います。僕は、家で寝転がってスマートフォンで動画を見ることが一番の癒しで、なかなかやめられません。"},{"id":{"d":98,"n":5,"t":4},"speaker":"A","text":"動画を見るのも良いですが、休日くらいは30分ほどでもよいので体を動かすと良いですよ。"},{"id":{"d":98,"n":6,"t":4},"speaker":"B","text":"それは、そうですね。来月から始めてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":99,"n":1,"t":4},"speaker":"A","text":"花粉症にかかったことは、ありますか？"},{"id":{"d":99,"n":2,"t":4},"speaker":"B","text":"はい。今でも毎年2月末から4月末くらいまで、花粉症の症状に苦しんでいます。"},{"id":{"d":99,"n":3,"t":4},"speaker":"A","text":"耳鼻咽喉科には行っているのですか？"},{"id":{"d":99,"n":4,"t":4},"speaker":"B","text":"市販の鼻炎スプレーで対処しています。"},{"id":{"d":99,"n":5,"t":4},"speaker":"A","text":"効果はありますか？"},{"id":{"d":99,"n":6,"t":4},"speaker":"B","text":"何ともいえません。症状が強い時はあまり効果を感じない時もあります。"},{"id":{"d":99,"n":7,"t":4},"speaker":"A","text":"一度、医者に行ってみたらいかがですか？"},{"id":{"d":99,"n":8,"t":4},"speaker":"B","text":"考えておきます。"}];
INSERT INTO sentence [{"id":{"d":100,"n":1,"t":4},"speaker":"A","text":"今日はどうしますか？"},{"id":{"d":100,"n":2,"t":4},"speaker":"B","text":"ヘアカラーをお願いします。"},{"id":{"d":100,"n":3,"t":4},"speaker":"A","text":"だいぶ髪が伸びて根元の地毛が目立つようになりましたね。"},{"id":{"d":100,"n":4,"t":4},"speaker":"B","text":"そうなのです。なかなか時間が取れず、今日やっと美容室に来ることができました。"},{"id":{"d":100,"n":5,"t":4},"speaker":"A","text":"そうでしたか。色はどうしますか？"},{"id":{"d":100,"n":6,"t":4},"speaker":"B","text":"思い切ってピンク色にしようと思います。"},{"id":{"d":100,"n":7,"t":4},"speaker":"A","text":"かしこまりました。きれいに染まりますよ。"},{"id":{"d":100,"n":8,"t":4},"speaker":"B","text":"そうでしょうか？楽しみです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":101,"n":1,"t":4},"speaker":"A","text":"その足の傷はどうしたのですか？"},{"id":{"d":101,"n":2,"t":4},"speaker":"B","text":"今日、つまずいて転んでしまいました。"},{"id":{"d":101,"n":3,"t":4},"speaker":"A","text":"消毒はしましたか？"},{"id":{"d":101,"n":4,"t":4},"speaker":"B","text":"大したことはなかったので、消毒はしていません。"},{"id":{"d":101,"n":5,"t":4},"speaker":"A","text":"一応消毒をした方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":102,"n":1,"t":4},"speaker":"A","text":"最近、お化粧を控えているのですか？"},{"id":{"d":102,"n":2,"t":4},"speaker":"B","text":"はい。どうやら化粧品が肌に合わないのです。"},{"id":{"d":102,"n":3,"t":4},"speaker":"A","text":"そうでしたか。季節の変わり目で、今までトラブルなく使えていた化粧品が、急に肌に合わなくなることもありますよね。"},{"id":{"d":102,"n":4,"t":4},"speaker":"B","text":"肌年齢やストレスも関係しているかもしれないと思っています。"},{"id":{"d":102,"n":5,"t":4},"speaker":"A","text":"そうですね、化粧品売場の美容部員に肌チェックをしてもらったらいかがですか？"},{"id":{"d":102,"n":6,"t":4},"speaker":"B","text":"それは良い考えです。今度、美容部員に相談をしてみます。"}];
INSERT INTO sentence [{"id":{"d":103,"n":1,"t":4},"speaker":"A","text":"顔色が悪いようですが、体調が悪いのですか？"},{"id":{"d":103,"n":2,"t":4},"speaker":"B","text":"はい、倦怠感があり、寒気がします。"},{"id":{"d":103,"n":3,"t":4},"speaker":"A","text":"高熱がありそうですね。体温を測ってみましょう。"},{"id":{"d":103,"n":4,"t":4},"speaker":"B","text":"37.8度ありました。"},{"id":{"d":103,"n":5,"t":4},"speaker":"A","text":"高熱がありますね。解熱剤を飲んで、様子を見ましょうか？"},{"id":{"d":103,"n":6,"t":4},"speaker":"B","text":"そうですね。とりあえず解熱剤を飲んで休みます。"}];
INSERT INTO sentence [{"id":{"d":104,"n":1,"t":4},"speaker":"A","text":"お久しぶりです。少しすっきりされたようですね。"},{"id":{"d":104,"n":2,"t":4},"speaker":"B","text":"はい、実はここ半月で5キログラム痩せました。"},{"id":{"d":104,"n":3,"t":4},"speaker":"A","text":"そうだったのですね。まるで別人のようで、一瞬誰なのか分かりませんでした。"},{"id":{"d":104,"n":4,"t":4},"speaker":"B","text":"実は、トレーニングジムに通い始めて、食事にも気を付けるようになったら、自然と痩せました。"},{"id":{"d":104,"n":5,"t":4},"speaker":"A","text":"それは、良かったですね。どのように痩せたのかぜひ詳しく教えてください。私もダイエットを考えているのです。"},{"id":{"d":104,"n":6,"t":4},"speaker":"B","text":"はい、いつでもお教えしますよ。"}];
INSERT INTO sentence [{"id":{"d":105,"n":1,"t":4},"speaker":"A","text":"最近抜け毛の量が増えているような気がします。"},{"id":{"d":105,"n":2,"t":4},"speaker":"B","text":"私も同感です。"},{"id":{"d":105,"n":3,"t":4},"speaker":"A","text":"季節の変わり目と抜け毛には、関係があると思いますか？"},{"id":{"d":105,"n":4,"t":4},"speaker":"B","text":"どうでしょう？季節の変わり目だけが理由なら良いのですが。"},{"id":{"d":105,"n":5,"t":4},"speaker":"A","text":"抜け毛予防のヘアトニックを使ってみようと考えています。"}];
INSERT INTO sentence [{"id":{"d":106,"n":1,"t":4},"speaker":"A","text":"顔色が悪いですが、大丈夫ですか？"},{"id":{"d":106,"n":2,"t":4},"speaker":"B","text":"そうですか？最近、目まいが多いので、貧血気味かもしれません。"},{"id":{"d":106,"n":3,"t":4},"speaker":"A","text":"病院には行っているのですか？"},{"id":{"d":106,"n":4,"t":4},"speaker":"B","text":"今までは行っていなかったのですが、最近あまりに症状がひどいので、今回は病院に行こうと思っています。"},{"id":{"d":106,"n":5,"t":4},"speaker":"A","text":"行った方がいいですよ。女性の貧血は特に気を付けた方がいいですね。"},{"id":{"d":106,"n":6,"t":4},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":107,"n":1,"t":4},"speaker":"A","text":"最近、あなたはゲームばかりしていますが、運動はしていますか？"},{"id":{"d":107,"n":2,"t":4},"speaker":"B","text":"ほとんど運動をしていません。"},{"id":{"d":107,"n":3,"t":4},"speaker":"A","text":"ゲームばかりはいけませんね。健康のために、外に出て体を動かしましょう。"},{"id":{"d":107,"n":4,"t":4},"speaker":"B","text":"外は寒いのでおっくうです。私は、体を動かすこともあまり好きではありません。"},{"id":{"d":107,"n":5,"t":4},"speaker":"A","text":"寒いのは分かりますが、ゲームをする前に少しだけ、ウオーキングをするというのはいかがですか？"},{"id":{"d":107,"n":6,"t":4},"speaker":"B","text":"ウオーキングならできそうです。明日からしてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":108,"n":1,"t":4},"speaker":"A","text":"顔の乾燥が気になっていますが、何か良い化粧品を知っていますか？"},{"id":{"d":108,"n":2,"t":4},"speaker":"B","text":"それなら、お薦めのパックがありますよ。"},{"id":{"d":108,"n":3,"t":4},"speaker":"A","text":"ぜひ教えてください。"},{"id":{"d":108,"n":4,"t":4},"speaker":"B","text":"韓国製のパックですが、保湿効果があり、化粧のりも良くなる気がします。"},{"id":{"d":108,"n":5,"t":4},"speaker":"A","text":"それはいいですね。私も使いたいです。"},{"id":{"d":108,"n":6,"t":4},"speaker":"B","text":"今、たくさん持っているので何枚か差し上げます。"},{"id":{"d":108,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。使うのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":109,"n":1,"t":4},"speaker":"A","text":"なんだか、良い香りがしますね。香水ですか？柔軟剤ですか？"},{"id":{"d":109,"n":2,"t":4},"speaker":"B","text":"香水は付けていないので、柔軟剤の香りだと思います。"},{"id":{"d":109,"n":3,"t":4},"speaker":"A","text":"とても好きな香りです。何を使っているのですか？"},{"id":{"d":109,"n":4,"t":4},"speaker":"B","text":"レノアオードリュクスという柔軟剤です。"},{"id":{"d":109,"n":5,"t":4},"speaker":"A","text":"良い香りですが、レノアオードリュクスは少し値段が高いですよね？"},{"id":{"d":109,"n":6,"t":4},"speaker":"B","text":"はい。高いので、お小遣いをためて買いました。"}];
INSERT INTO sentence [{"id":{"d":110,"n":1,"t":4},"speaker":"A","text":"最近、1人暮らしを始めたばかりで、食生活が偏りがちです。"},{"id":{"d":110,"n":2,"t":4},"speaker":"B","text":"野菜は食べていますか？"},{"id":{"d":110,"n":3,"t":4},"speaker":"A","text":"野菜はあまり食べていません。出来合いのお総菜を購入する事がほとんどです。"},{"id":{"d":110,"n":4,"t":4},"speaker":"B","text":"そうなのですね。まずは食生活を変えることが大切です。自炊に挑戦してみるのはいかがですか？"},{"id":{"d":110,"n":5,"t":4},"speaker":"A","text":"そうですね、少し頑張ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":111,"n":1,"t":4},"speaker":"A","text":"この間、雑誌に掲載されていた健康特集の記事はご覧になりましたか？"},{"id":{"d":111,"n":2,"t":4},"speaker":"B","text":"はい、見ました。長生きをする人が、どのような心得で生きているのか、示唆にあふれた話が多く、とても勉強になりました。"},{"id":{"d":111,"n":3,"t":4},"speaker":"A","text":"私もそう思いました。一番印象に残った記事は、「長生きをする人は、前向きな気持ちで日々を過ごしている」というものでした。"},{"id":{"d":111,"n":4,"t":4},"speaker":"B","text":"やはり、心の持ち方と健康は相関関係があるのでしょうか？"},{"id":{"d":111,"n":5,"t":4},"speaker":"A","text":"はい、私はそう思いました。私も改めて、より前向きな心の持ち方を意識していきたいと思いました。"}];
INSERT INTO sentence [{"id":{"d":112,"n":1,"t":4},"speaker":"A","text":"今日、仕事の帰りに食事に行きませんか？"},{"id":{"d":112,"n":2,"t":4},"speaker":"B","text":"良いですね。金曜日ですし、最近外食もしていなかったので行きたいです。"},{"id":{"d":112,"n":3,"t":4},"speaker":"A","text":"では、行きましょう。今日は何時に仕事が終わりますか？"},{"id":{"d":112,"n":4,"t":4},"speaker":"B","text":"16時から会議があるのですが、17時には退社できます。"},{"id":{"d":112,"n":5,"t":4},"speaker":"A","text":"分かりました。最近私は、健康のためにオーガニック食品をよく食べています。会社の近くにオーガニック食品を使った料理で有名なレストランがあるので、そこを予約しておきます。"},{"id":{"d":112,"n":6,"t":4},"speaker":"B","text":"健康的で良いですね。予約をお願いします。"}];
INSERT INTO sentence [{"id":{"d":113,"n":1,"t":4},"speaker":"A","text":"今から、美容院に行ってきます。"},{"id":{"d":113,"n":2,"t":4},"speaker":"B","text":"いいですね。いつもどちらの美容院に行ってらっしゃるのですか？"},{"id":{"d":113,"n":3,"t":4},"speaker":"A","text":"表参道のあおば美容院です。"},{"id":{"d":113,"n":4,"t":4},"speaker":"B","text":"知っています。有名な美容院ですね。"},{"id":{"d":113,"n":5,"t":4},"speaker":"A","text":"はい、お気に入りの美容師さんがいますので、いつもここに通っています。"},{"id":{"d":113,"n":6,"t":4},"speaker":"B","text":"良かったら、今度紹介していただけませんか？"},{"id":{"d":113,"n":7,"t":4},"speaker":"A","text":"もちろんです。美容院に行きたい時にでも、お声がけいただければご紹介します。"},{"id":{"d":113,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":114,"n":1,"t":4},"speaker":"A","text":"今度、お休みの日にマッサージを予約してもよいですか？"},{"id":{"d":114,"n":2,"t":4},"speaker":"B","text":"よいですよ。出産してから、ゆっくりする時間がなかったですものね。"},{"id":{"d":114,"n":3,"t":4},"speaker":"A","text":"そうなのです。子どもたちを置いて、少しリフレッシュしてきますね。"},{"id":{"d":114,"n":4,"t":4},"speaker":"B","text":"子どもたちのことは、こちらは任せてください。あなたのリフレッシュの時間も必要です。ゆっくり買い物もしてきてください。"},{"id":{"d":114,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。散歩もしたいと思います。"},{"id":{"d":114,"n":6,"t":4},"speaker":"B","text":"良いですね。天気も良いし楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":115,"n":1,"t":4},"speaker":"A","text":"今日は、どうされましたか？"},{"id":{"d":115,"n":2,"t":4},"speaker":"B","text":"少し風邪気味で、寒気と頭痛がします。"},{"id":{"d":115,"n":3,"t":4},"speaker":"A","text":"呼吸器系は問題なさそうですが、喉が少し赤いですね。風邪から来るものだと思います。お薬を出しておきます。"},{"id":{"d":115,"n":4,"t":4},"speaker":"B","text":"そうですか。お薬は眠くならない成分が入ったものを出していただけますか？"},{"id":{"d":115,"n":5,"t":4},"speaker":"A","text":"よいですよ。それなら少し漢方薬も処方しますね。"},{"id":{"d":115,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":115,"n":7,"t":4},"speaker":"A","text":"お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":116,"n":1,"t":4},"speaker":"A","text":"最近、腰が痛いです。"},{"id":{"d":116,"n":2,"t":4},"speaker":"B","text":"何か痛めるようなことをしたのですか？"},{"id":{"d":116,"n":3,"t":4},"speaker":"A","text":"特に思い当たることはしていないのですが、昔から「猫背」と人に言われます。"},{"id":{"d":116,"n":4,"t":4},"speaker":"B","text":"なるほど。だから腰が曲がって痛くなるのかもしれませんね。"},{"id":{"d":116,"n":5,"t":4},"speaker":"A","text":"評判の良いボディケア店を知っていますか？"},{"id":{"d":116,"n":6,"t":4},"speaker":"B","text":"リラクが良いらしいですよ。予約もすぐにできるそうです。"},{"id":{"d":116,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。早速、予約します。"},{"id":{"d":116,"n":8,"t":4},"speaker":"B","text":"痛みが楽になると良いですね。"}];
INSERT INTO sentence [{"id":{"d":117,"n":1,"t":4},"speaker":"A","text":"今日は、あなたの声が少し鼻声ですが、風邪をひいたのですか？"},{"id":{"d":117,"n":2,"t":4},"speaker":"B","text":"はい。微熱もあるようです。"},{"id":{"d":117,"n":3,"t":4},"speaker":"A","text":"そうなのですね。早く帰って休んだ方がいいですよ。"},{"id":{"d":117,"n":4,"t":4},"speaker":"B","text":"そうします。明日は大切な会議があるから、会社を休めないのです。"},{"id":{"d":117,"n":5,"t":4},"speaker":"A","text":"薬を飲んで、体を温かくして休んでください。無理はしないでくださいね。"},{"id":{"d":117,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。お先に失礼します。"}];
INSERT INTO sentence [{"id":{"d":118,"n":1,"t":4},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":118,"n":2,"t":4},"speaker":"B","text":"おかえりなさい。学校は楽しかったですか？"},{"id":{"d":118,"n":3,"t":4},"speaker":"A","text":"楽しかったです。おなかがすいたのですが、おやつはありますか？"},{"id":{"d":118,"n":4,"t":4},"speaker":"B","text":"おやつは、パンケーキですよ。その前にうがい、手洗い、消毒はしましたか？"},{"id":{"d":118,"n":5,"t":4},"speaker":"A","text":"忘れていました。今からします。"},{"id":{"d":118,"n":6,"t":4},"speaker":"B","text":"それがきちんと終わってから、おやつにしましょう。"}];
INSERT INTO sentence [{"id":{"d":119,"n":1,"t":4},"speaker":"A","text":"明日のキャンプが楽しみですね。"},{"id":{"d":119,"n":2,"t":4},"speaker":"B","text":"私は、行きたくありません。"},{"id":{"d":119,"n":3,"t":4},"speaker":"A","text":"どうして行きたくないのですか？"},{"id":{"d":119,"n":4,"t":4},"speaker":"B","text":"外には、虫がたくさんいるからです。"},{"id":{"d":119,"n":5,"t":4},"speaker":"A","text":"それなら、虫除けスプレーなどを利用すれば平気ですよ。"},{"id":{"d":119,"n":6,"t":4},"speaker":"B","text":"でも、虫に刺されたらどうするのですか？"},{"id":{"d":119,"n":7,"t":4},"speaker":"A","text":"かゆみ止めも持っていくので、安心してキャンプを楽しみましょう。"},{"id":{"d":119,"n":8,"t":4},"speaker":"B","text":"キャンプに行きたくないという気持ちが少し薄れてきました。"}];
INSERT INTO sentence [{"id":{"d":120,"n":1,"t":4},"speaker":"A","text":"今日は日差しが強かったので、日焼けで肌が真っ赤になってしまいました。"},{"id":{"d":120,"n":2,"t":4},"speaker":"B","text":"すごく痛々しいですね。やけどのようになっていますよ。"},{"id":{"d":120,"n":3,"t":4},"speaker":"A","text":"実は、すごくヒリヒリして痛いです。"},{"id":{"d":120,"n":4,"t":4},"speaker":"B","text":"それなら皮膚科に行って、診てもらった方がいいですね。"},{"id":{"d":120,"n":5,"t":4},"speaker":"A","text":"日焼けで皮膚科に行くのは、大げさなのではありませんか？"},{"id":{"d":120,"n":6,"t":4},"speaker":"B","text":"大げさではありません。痛いのであれば、なおさら病院で診てもらった方がいいです。早めに治さないと染みになることもありますよ。"},{"id":{"d":120,"n":7,"t":4},"speaker":"A","text":"そうですね。染みになる前に皮膚科で診てもらうことにします。"}];
INSERT INTO sentence [{"id":{"d":121,"n":1,"t":4},"speaker":"A","text":"赤ちゃんのお尻にある、青いあざは何ですか？"},{"id":{"d":121,"n":2,"t":4},"speaker":"B","text":"赤ちゃんのお尻のあざはもう古斑といいます。"},{"id":{"d":121,"n":3,"t":4},"speaker":"A","text":"もう古斑は、消えないのですか？"},{"id":{"d":121,"n":4,"t":4},"speaker":"B","text":"5、6歳まで成長すれば、自然と消えてなくなります。"},{"id":{"d":121,"n":5,"t":4},"speaker":"A","text":"だから、僕にはもう古斑がないのですね。"},{"id":{"d":121,"n":6,"t":4},"speaker":"B","text":"もう大きくなったから、なくなったのですよ。"}];
INSERT INTO sentence [{"id":{"d":122,"n":1,"t":4},"speaker":"A","text":"お母さん、今日は学校を休んでもよいですか？"},{"id":{"d":122,"n":2,"t":4},"speaker":"B","text":"どうしましたか？具合でも悪いのですか？"},{"id":{"d":122,"n":3,"t":4},"speaker":"A","text":"テストがあるので、学校へ行きたくないのです。"},{"id":{"d":122,"n":4,"t":4},"speaker":"B","text":"テストを受けたくないから学校を休むのですか？"},{"id":{"d":122,"n":5,"t":4},"speaker":"A","text":"はい。先生には、「発熱がある」と適当に言ってください。"},{"id":{"d":122,"n":6,"t":4},"speaker":"B","text":"それは仮病ですよ。うそは良くありません。具合が悪くないのであれば、学校へ行きましょう。"},{"id":{"d":122,"n":7,"t":4},"speaker":"A","text":"そうですね。やはり学校に行きます。"},{"id":{"d":122,"n":8,"t":4},"speaker":"B","text":"うそをついて休むより、思い切って学校へ行った方がきっと楽しいですよ。"}];
INSERT INTO sentence [{"id":{"d":123,"n":1,"t":4},"speaker":"A","text":"新しい化粧水を使い始めたら、肌がふっくらとして張りがあるようになりました。"},{"id":{"d":123,"n":2,"t":4},"speaker":"B","text":"何という化粧水を使っているのですか？"},{"id":{"d":123,"n":3,"t":4},"speaker":"A","text":"エヌオーガニックといいます。インターネットで見つけて買いました。"},{"id":{"d":123,"n":4,"t":4},"speaker":"B","text":"その化粧水は、私も気になっていました。"},{"id":{"d":123,"n":5,"t":4},"speaker":"A","text":"さほど高くないので、試しに使ってみたらどうですか？"},{"id":{"d":123,"n":6,"t":4},"speaker":"B","text":"さっそく、買ってみます。"}];
INSERT INTO sentence [{"id":{"d":124,"n":1,"t":4},"speaker":"A","text":"目が充血していますが、大丈夫ですか？"},{"id":{"d":124,"n":2,"t":4},"speaker":"B","text":"昨夜は、遅くまで起きていたからだと思います。"},{"id":{"d":124,"n":3,"t":4},"speaker":"A","text":"夜遅くまで、何をしていたのですか？"},{"id":{"d":124,"n":4,"t":4},"speaker":"B","text":"オンラインでゲームをしていたら、いつの間にか寝るのが遅くなってしまいました。"},{"id":{"d":124,"n":5,"t":4},"speaker":"A","text":"寝不足ですね。体に悪いし、目も悪くなりますよ。"},{"id":{"d":124,"n":6,"t":4},"speaker":"B","text":"そうですよね。今夜はゲームをしないで、早く休むことにします。"}];
INSERT INTO sentence [{"id":{"d":125,"n":1,"t":4},"speaker":"A","text":"今日の髪型は、良いですね。どのような整髪剤を使っているのですか？"},{"id":{"d":125,"n":2,"t":4},"speaker":"B","text":"SALAのまとめ髪メイクワックスEXという、ヘアスタイリング剤を使っています。"},{"id":{"d":125,"n":3,"t":4},"speaker":"A","text":"そうなのですね。どのようなスタイリング剤なのですか？"},{"id":{"d":125,"n":4,"t":4},"speaker":"B","text":"手ぐしで簡単に浮き毛を抑えてくれる、ヘアスタイリング剤です。"},{"id":{"d":125,"n":5,"t":4},"speaker":"A","text":"良いですね。私も試しに購入してみようと思います。"},{"id":{"d":125,"n":6,"t":4},"speaker":"B","text":"ぜひ、試してみてください。"}];
INSERT INTO sentence [{"id":{"d":126,"n":1,"t":4},"speaker":"A","text":"普段、私は洗顔フォームを使わないのですが、あなたはどのような洗顔フォームを使っているのですか？"},{"id":{"d":126,"n":2,"t":4},"speaker":"B","text":"私は、Bioreのメイクも落とせる洗顔料つるすべ美肌を使っています。"},{"id":{"d":126,"n":3,"t":4},"speaker":"A","text":"どこが魅力的で、その商品を選んだのですか？"},{"id":{"d":126,"n":4,"t":4},"speaker":"B","text":"クリーミーな泡立ちで、ベトつかずに汚れをスッキリ落とせるところが魅力的で選びました。"},{"id":{"d":126,"n":5,"t":4},"speaker":"A","text":"良さそうですね。私も購入してみます。"}];
INSERT INTO sentence [{"id":{"d":127,"n":1,"t":4},"speaker":"A","text":"最近、私はくしゃみが止まらないことがあります。"},{"id":{"d":127,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？花粉症ですか？"},{"id":{"d":127,"n":3,"t":4},"speaker":"A","text":"花粉症だと思います。鼻詰まりもあります。"},{"id":{"d":127,"n":4,"t":4},"speaker":"B","text":"いつから花粉症なのですか？"},{"id":{"d":127,"n":5,"t":4},"speaker":"A","text":"もう10年以上前から花粉症です。最近はスギ花粉症よりヒノキ花粉症の方が重症です。"},{"id":{"d":127,"n":6,"t":4},"speaker":"B","text":"この時期はおつらいですね。お大事にしてください。"},{"id":{"d":127,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":128,"n":1,"t":4},"speaker":"A","text":"昨年は、インフルエンザが猛威を振るった1年でしたね。"},{"id":{"d":128,"n":2,"t":4},"speaker":"B","text":"インフルエンザにかかると、ひどい風邪のような症状が出るとのことで、感染しないか毎日不安でした。"},{"id":{"d":128,"n":3,"t":4},"speaker":"A","text":"感染症予防に手洗い、うがい、消毒を徹底しました。"},{"id":{"d":128,"n":4,"t":4},"speaker":"B","text":"アルコール消毒液とマスクは必需品でしたね。"},{"id":{"d":128,"n":5,"t":4},"speaker":"A","text":"本当ですね。私は手が肌荒れしてしまって、ハンドクリームをよく使っていました。"},{"id":{"d":128,"n":6,"t":4},"speaker":"B","text":"分かります。肌が荒れやすい人は大変ですよね。"}];
INSERT INTO sentence [{"id":{"d":129,"n":1,"t":4},"speaker":"A","text":"最近、私はアイメークを練習中で、アイシャドウをつけるのに凝っています。"},{"id":{"d":129,"n":2,"t":4},"speaker":"B","text":"そうなのですね。何色を使っているのですか？"},{"id":{"d":129,"n":3,"t":4},"speaker":"A","text":"ブラウンやグリーンなど、濃い色を使うことが多いです。"},{"id":{"d":129,"n":4,"t":4},"speaker":"B","text":"濃い色はつけすぎると目の下にクマができて疲れているように見えるので、バランスが難しいですよね。"},{"id":{"d":129,"n":5,"t":4},"speaker":"A","text":"そうなのです。そこに気を付けながら、メークの練習を頑張っています。"},{"id":{"d":129,"n":6,"t":4},"speaker":"B","text":"濃い色の代わりに、淡い色のピンク系やオレンジ系にチャレンジしてみてはどうですか？似合うと思いますよ。"},{"id":{"d":129,"n":7,"t":4},"speaker":"A","text":"そうでしょうか？では、今度試したいと思います。"}];
INSERT INTO sentence [{"id":{"d":130,"n":1,"t":4},"speaker":"A","text":"すみません。アイライナーを探しているのですが、どのようなものが良いでしょうか？"},{"id":{"d":130,"n":2,"t":4},"speaker":"B","text":"アイライナーですね。これまで、アイライナーをお使いになったことはありますか？"},{"id":{"d":130,"n":3,"t":4},"speaker":"A","text":"いいえ。初めてです。"},{"id":{"d":130,"n":4,"t":4},"speaker":"B","text":"それでは、リキッドタイプよりはペンシルタイプの方がお薦めです。"},{"id":{"d":130,"n":5,"t":4},"speaker":"A","text":"そうなのですね。では、ペンシルタイプを購入したいと思います。"},{"id":{"d":130,"n":6,"t":4},"speaker":"B","text":"お色の希望はございますか？"},{"id":{"d":130,"n":7,"t":4},"speaker":"A","text":"ブラウンをお願いします。"},{"id":{"d":130,"n":8,"t":4},"speaker":"B","text":"かしこまりました。ブラウンのアイライナーをご用意しますね。"}];
INSERT INTO sentence [{"id":{"d":131,"n":1,"t":4},"speaker":"A","text":"どのようなハンドクリームを使っていますか？"},{"id":{"d":131,"n":2,"t":4},"speaker":"B","text":"私は、メンソレータム薬用ハンドベールのうるおいさらっとジェルを使っています。"},{"id":{"d":131,"n":3,"t":4},"speaker":"A","text":"使用感はどうですか？"},{"id":{"d":131,"n":4,"t":4},"speaker":"B","text":"肌に伸ばすと、すぐになじんで浸透し保湿してくれます。表面はすぐさらさらになるので、仕事や家事もしやすいです。"},{"id":{"d":131,"n":5,"t":4},"speaker":"A","text":"どのような香りなのですか？"},{"id":{"d":131,"n":6,"t":4},"speaker":"B","text":"無香料なので、特に香りはしません。"},{"id":{"d":131,"n":7,"t":4},"speaker":"A","text":"私は、無香料のハンドクリームが好きです。良い情報をありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":132,"n":1,"t":4},"speaker":"A","text":"私は、化粧初心者なのですが、どの化粧品を使ったら良いですか？"},{"id":{"d":132,"n":2,"t":4},"speaker":"B","text":"当店では、初心者の方でもお求めやすい、ちふれ化粧品をお薦めしております。"},{"id":{"d":132,"n":3,"t":4},"speaker":"A","text":"どの辺に置いてありますか？"},{"id":{"d":132,"n":4,"t":4},"speaker":"B","text":"ちふれの商品はこちらにございます。"},{"id":{"d":132,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":132,"n":6,"t":4},"speaker":"B","text":"ごゆっくりご覧ください。"}];
INSERT INTO sentence [{"id":{"d":133,"n":1,"t":4},"speaker":"A","text":"こんにちは。髪の毛が、だいぶ伸びてきましたね。今日はどうしますか？"},{"id":{"d":133,"n":2,"t":4},"speaker":"B","text":"髪の毛のボリュームを抑えたいのですが、どのような方法がありますか？"},{"id":{"d":133,"n":3,"t":4},"speaker":"A","text":"ストレートパーマをお薦めする場合もありますが、小島さんの場合はトリートメントで抑えるだけでも十分かも知れません。"},{"id":{"d":133,"n":4,"t":4},"speaker":"B","text":"そうなのですね。では、今日はトリートメントとカットをお願いします。"},{"id":{"d":133,"n":5,"t":4},"speaker":"A","text":"分かりました。カットの長さはどれくらいがよろしいですか？"},{"id":{"d":133,"n":6,"t":4},"speaker":"B","text":"軽くそろえる程度で結構です。"},{"id":{"d":133,"n":7,"t":4},"speaker":"A","text":"分かりました。"},{"id":{"d":133,"n":8,"t":4},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":134,"n":1,"t":4},"speaker":"A","text":"自分で、ヘアカラーをするのは難しいですか？"},{"id":{"d":134,"n":2,"t":4},"speaker":"B","text":"最近は泡で出るタイプのカラーリング剤があるので、以前に比べたら自分でも染めやすくなりました。"},{"id":{"d":134,"n":3,"t":4},"speaker":"A","text":"佐藤さんは、いつも自分でヘアカラーをしているのですか？"},{"id":{"d":134,"n":4,"t":4},"speaker":"B","text":"はい。美容院に行くと、結構料金がかかるので、自分でできることは自分でしています。"},{"id":{"d":134,"n":5,"t":4},"speaker":"A","text":"そうなのですね。今度、私も挑戦してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":135,"n":1,"t":4},"speaker":"A","text":"今度、沖縄に旅行に行くのですが、一緒に行きませんか？"},{"id":{"d":135,"n":2,"t":4},"speaker":"B","text":"沖縄には行きたいのですが、実は、私は飛行機が苦手なのです。"},{"id":{"d":135,"n":3,"t":4},"speaker":"A","text":"そうなのですね。"},{"id":{"d":135,"n":4,"t":4},"speaker":"B","text":"子どもの頃に、1回飛行機に乗った事があるのですが、離着陸時に揺れるのが怖くて、その感覚がいまだに忘れられません。トラウマになっているのかもしれません。"},{"id":{"d":135,"n":5,"t":4},"speaker":"A","text":"そうでしたか。それでは無理しなくても大丈夫ですよ。"},{"id":{"d":135,"n":6,"t":4},"speaker":"B","text":"でも、せっかく誘っていただいたので考えてみます。離着陸時の揺れなどは、ほんの一瞬ですものね。"},{"id":{"d":135,"n":7,"t":4},"speaker":"A","text":"無理はしないでほしいのですが、もし一緒に行くことができたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":136,"n":1,"t":4},"speaker":"A","text":"失礼します。"},{"id":{"d":136,"n":2,"t":4},"speaker":"B","text":"こんにちは。今日はどうなさいましたか？"},{"id":{"d":136,"n":3,"t":4},"speaker":"A","text":"体温が微熱と平熱を行ったり来たりしていて、おなかの調子も下痢と便秘を繰り返すのでつらいです。"},{"id":{"d":136,"n":4,"t":4},"speaker":"B","text":"それはつらいですね。自律神経の働きが乱れているのかもしれません。錠剤の整腸剤と、体温を調整する漢方薬を処方しておきますね。"},{"id":{"d":136,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":137,"n":1,"t":4},"speaker":"A","text":"私は運動不足なので、運動を始めようと思います。何から始めたら良いと思いますか？"},{"id":{"d":137,"n":2,"t":4},"speaker":"B","text":"私は、ジムに通い始めようと思っています。一緒にどうですか？ヨガやストレッチ体操、水泳もできるところのようです。"},{"id":{"d":137,"n":3,"t":4},"speaker":"A","text":"それなら、運動不足の私でも楽しく通えそうです。"},{"id":{"d":137,"n":4,"t":4},"speaker":"B","text":"今度、ジムの見学だけでも一緒に行きましょう。"},{"id":{"d":137,"n":5,"t":4},"speaker":"A","text":"お誘いいただきありがとうございます。ぜひ、一緒に行きたいです。"}];
INSERT INTO sentence [{"id":{"d":138,"n":1,"t":4},"speaker":"A","text":"鈴木さん、おはようございます。体調はいかがですか？"},{"id":{"d":138,"n":2,"t":4},"speaker":"B","text":"だいぶ良くなりました。"},{"id":{"d":138,"n":3,"t":4},"speaker":"A","text":"それは良かったです。今日は手術して5日ほど経過したので、髪を洗おうと思いますが、いかがですか？"},{"id":{"d":138,"n":4,"t":4},"speaker":"B","text":"ぜひ、洗っていただきたいです。さっぱりしたいです。"},{"id":{"d":138,"n":5,"t":4},"speaker":"A","text":"では、10時ごろ、またお迎えに来ますね。"},{"id":{"d":138,"n":6,"t":4},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":139,"n":1,"t":4},"speaker":"A","text":"佐藤さんの好きなスポーツは、何ですか？"},{"id":{"d":139,"n":2,"t":4},"speaker":"B","text":"バレーボールです。"},{"id":{"d":139,"n":3,"t":4},"speaker":"A","text":"バレーボールの、どのようなところが好きですか？"},{"id":{"d":139,"n":4,"t":4},"speaker":"B","text":"バレーボールは、唯一ボールを床に落としてはいけないスポーツです。1つのボールが人と人とをつなぐようで、一体感が高まるところが好きです。"},{"id":{"d":139,"n":5,"t":4},"speaker":"A","text":"すてきな魅力ですね。"}];
INSERT INTO sentence [{"id":{"d":140,"n":1,"t":4},"speaker":"A","text":"スポーツをする上で、大切なことは何だと思いますか？"},{"id":{"d":140,"n":2,"t":4},"speaker":"B","text":"ストレッチ体操や、柔軟体操を怠らないことです。体をほぐし、温めてから筋力トレーニングをするのが大切だと思います。"},{"id":{"d":140,"n":3,"t":4},"speaker":"A","text":"スポーツをするための、準備が大切ということですね。"},{"id":{"d":140,"n":4,"t":4},"speaker":"B","text":"けがをしないためにも、ストレッチ体操や柔軟体操は大切です。"},{"id":{"d":140,"n":5,"t":4},"speaker":"A","text":"参考になります。"}];
INSERT INTO sentence [{"id":{"d":141,"n":1,"t":4},"speaker":"A","text":"今日は、体調が悪そうですが、どうかなさいましたか？"},{"id":{"d":141,"n":2,"t":4},"speaker":"B","text":"微熱があって偏頭痛がします。"},{"id":{"d":141,"n":3,"t":4},"speaker":"A","text":"季節の変わり目も関係しているのでしょうか？"},{"id":{"d":141,"n":4,"t":4},"speaker":"B","text":"そうだと思います。この時期になるといつも調子が悪いです。"},{"id":{"d":141,"n":5,"t":4},"speaker":"A","text":"あまり無理をせず、しっかり睡眠を取ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":142,"n":1,"t":4},"speaker":"A","text":"僕は、高い所が苦手です。"},{"id":{"d":142,"n":2,"t":4},"speaker":"B","text":"観覧車も苦手ですか？"},{"id":{"d":142,"n":3,"t":4},"speaker":"A","text":"苦手です。昔、遊園地で観覧車に乗ったときに、強風でゴンドラが激しく揺れた事があり、その時から苦手になりました。"},{"id":{"d":142,"n":4,"t":4},"speaker":"B","text":"それは、怖い経験をしましたね。小さい頃の思い出は、いつまでも残っていますからね。"}];
INSERT INTO sentence [{"id":{"d":143,"n":1,"t":4},"speaker":"A","text":"先生、私はもう走れません。"},{"id":{"d":143,"n":2,"t":4},"speaker":"B","text":"ラストスパートですよ。一緒に頑張りましょう。"},{"id":{"d":143,"n":3,"t":4},"speaker":"A","text":"ゴールまで、残りの距離はどのくらいありますか？"},{"id":{"d":143,"n":4,"t":4},"speaker":"B","text":"あの丘を越えたらゴールです。ゴールまでもう少し、頑張りましょう。"},{"id":{"d":143,"n":5,"t":4},"speaker":"A","text":"分かりました。もう少し頑張ります。"}];
INSERT INTO sentence [{"id":{"d":144,"n":1,"t":4},"speaker":"A","text":"志望校選びのことですが、僕は野球を続けられる高校に進学したいと考えています。"},{"id":{"d":144,"n":2,"t":4},"speaker":"B","text":"野球部の強い高校へ進学を希望するということですか？"},{"id":{"d":144,"n":3,"t":4},"speaker":"A","text":"私立の國學院高校に推薦で行けたら良いと思っていますが、お母さんは推薦入試のことをどう思いますか？"},{"id":{"d":144,"n":4,"t":4},"speaker":"B","text":"健が、野球を続けたいというのであれば、私は応援しますよ。"},{"id":{"d":144,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。僕は、阪神甲子園球場で開かれる、全国高等学校野球選手権大会に出て、プロ野球選手になって、お母さんに球場に応援に来てもらうことが夢なのです。その夢がかなうように高校に行っても野球を頑張ります。"},{"id":{"d":144,"n":6,"t":4},"speaker":"B","text":"私も、楽しみにしています。頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":145,"n":1,"t":4},"speaker":"A","text":"休みの日は、何をしていますか？"},{"id":{"d":145,"n":2,"t":4},"speaker":"B","text":"ヨガ教室に行ったり、ジョギングをしたりしています。"},{"id":{"d":145,"n":3,"t":4},"speaker":"A","text":"体を動かすのが好きなのですね。ジョギングは、朝晩どちらに行うのですか？"},{"id":{"d":145,"n":4,"t":4},"speaker":"B","text":"朝です。ジョギングから帰ってきてからは、ゆっくり過ごしています。"},{"id":{"d":145,"n":5,"t":4},"speaker":"A","text":"今度、私と一緒に走りませんか？"},{"id":{"d":145,"n":6,"t":4},"speaker":"B","text":"はい、もちろんです。一緒に汗を流しましょう。"},{"id":{"d":145,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":146,"n":1,"t":4},"speaker":"A","text":"目が赤く充血していますが、どうしましたか？"},{"id":{"d":146,"n":2,"t":4},"speaker":"B","text":"花粉症なので、この時期になるとウサギの目のように赤くなってしまいます。"},{"id":{"d":146,"n":3,"t":4},"speaker":"A","text":"花粉症はつらいですね。お薬は飲んでいますか？"},{"id":{"d":146,"n":4,"t":4},"speaker":"B","text":"薬は飲んでいます。ひどい時は注射もしています。でも、完全には良くなりません。"},{"id":{"d":146,"n":5,"t":4},"speaker":"A","text":"この時期は、外も暖かくなって気持ちが良いですが、花粉症の人にはつらい季節ですね。"}];
INSERT INTO sentence [{"id":{"d":147,"n":1,"t":4},"speaker":"A","text":"今日はどうされましたか？"},{"id":{"d":147,"n":2,"t":4},"speaker":"B","text":"今朝起きたら首が痛くて、動かすと痛みがさらにひどいのです。"},{"id":{"d":147,"n":3,"t":4},"speaker":"A","text":"何か、首に負荷がかかるようなことはしましたか？"},{"id":{"d":147,"n":4,"t":4},"speaker":"B","text":"特に、何もしていません。"},{"id":{"d":147,"n":5,"t":4},"speaker":"A","text":"そうですか。寝違えたのかもしれませんね。湿布を出しておくので、毎日貼って様子見てください。痛みが続くようなら、また来てくださいね。"},{"id":{"d":147,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":148,"n":1,"t":4},"speaker":"A","text":"肌に、吹き出物ができてしまいました。"},{"id":{"d":148,"n":2,"t":4},"speaker":"B","text":"吹き出物ではなくて、それは思春期のニキビといって小学校の高学年から高校生のころにできるものです。"},{"id":{"d":148,"n":3,"t":4},"speaker":"A","text":"そうでしたか。大人になったら治りますか？"},{"id":{"d":148,"n":4,"t":4},"speaker":"B","text":"ホルモンバランスが安定してくると、自然に治ることも多いと聞きますよ。"},{"id":{"d":148,"n":5,"t":4},"speaker":"A","text":"安心しました。早く大人になりたいです。"}];
INSERT INTO sentence [{"id":{"d":149,"n":1,"t":4},"speaker":"A","text":"明日の遠足が楽しみです。お弁当には、絶対に唐揚げと卵焼きを入れてください。"},{"id":{"d":149,"n":2,"t":4},"speaker":"B","text":"分かりました。早起きして、おいしいお弁当を作りますね。"},{"id":{"d":149,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":149,"n":4,"t":4},"speaker":"B","text":"公園に行くのなら、朝出かける前に、虫除けスプレーを体に吹き付けてから行くと良いですよ。"},{"id":{"d":149,"n":5,"t":4},"speaker":"A","text":"そうですね。この前は、虫除けスプレーを体に吹き付けることを忘れてしまって、公園で10カ所くらい虫に刺されてしまいました。明日は、忘れないようにします。"},{"id":{"d":149,"n":6,"t":4},"speaker":"B","text":"それほど、虫に刺されたのですか？大変でしたね。明日は、絶対に虫除けスプレーを忘れないようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":150,"n":1,"t":4},"speaker":"A","text":"最近、肌荒れがひどいです。"},{"id":{"d":150,"n":2,"t":4},"speaker":"B","text":"寒くなってきたし、乾燥していますからね。頬の辺りですか？"},{"id":{"d":150,"n":3,"t":4},"speaker":"A","text":"マスクで覆われている部分が、全体的に荒れています。かゆみもあります。"},{"id":{"d":150,"n":4,"t":4},"speaker":"B","text":"そうなのですね。マスクの中は温度や湿度が高いので菌が繁殖して、肌が荒れやすくなるそうです。今は家にいる時以外は、ずっとマスクをしていますものね。"},{"id":{"d":150,"n":5,"t":4},"speaker":"A","text":"そうですね。早くマスクが必要ない生活を送りたいです。"}];
INSERT INTO sentence [{"id":{"d":151,"n":1,"t":4},"speaker":"A","text":"最近、スリムな体になりましたね。"},{"id":{"d":151,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。毎日、家で筋力トレーニングをしています。"},{"id":{"d":151,"n":3,"t":4},"speaker":"A","text":"筋力トレーニングは、具体的に何をしていますか？腕立て伏せですか？"},{"id":{"d":151,"n":4,"t":4},"speaker":"B","text":"懸垂と腕立て伏せ、それに腹筋運動と背筋運動も行っています。"},{"id":{"d":151,"n":5,"t":4},"speaker":"A","text":"すごいですね。僕は仕事から帰ると疲れてしまっているので、筋力トレーニングができないのですよ。"},{"id":{"d":151,"n":6,"t":4},"speaker":"B","text":"筋力トレーニングは、一度やり出すと止まらなくなります。僕の最も好きな時間です。"},{"id":{"d":151,"n":7,"t":4},"speaker":"A","text":"尊敬します。僕も、少し運動します。"}];
INSERT INTO sentence [{"id":{"d":152,"n":1,"t":4},"speaker":"A","text":"佐藤さんの健康法は、何ですか？"},{"id":{"d":152,"n":2,"t":4},"speaker":"B","text":"私の健康法は、半身浴です。"},{"id":{"d":152,"n":3,"t":4},"speaker":"A","text":"毎日何分くらい半身浴をしているのですか？"},{"id":{"d":152,"n":4,"t":4},"speaker":"B","text":"平日は50分ですが、休日は90分くらい半身浴をしています。"},{"id":{"d":152,"n":5,"t":4},"speaker":"A","text":"90分も、半身浴をするのですか？"},{"id":{"d":152,"n":6,"t":4},"speaker":"B","text":"はい。半身浴をしながら、本を読んだりしているとあっという間に時間がたちますよ。"},{"id":{"d":152,"n":7,"t":4},"speaker":"A","text":"そうなのですね。今度私も試してみます。"}];
INSERT INTO sentence [{"id":{"d":153,"n":1,"t":4},"speaker":"A","text":"佐藤さん、パラリンピックでも行われていたゴールボールという競技を知っていますか？"},{"id":{"d":153,"n":2,"t":4},"speaker":"B","text":"確か、視覚障害のある人を対象に考えられたスポーツですよね？"},{"id":{"d":153,"n":3,"t":4},"speaker":"A","text":"はい。3人でチームになり、鈴の入ったバスケットボールくらいの大きさのボールを互いに投げ合い、点を競い合うスポーツです。"},{"id":{"d":153,"n":4,"t":4},"speaker":"B","text":"以前、テレビ番組で見たことがあります。視覚障害の程度を合わせるため、全盲状態でプレーするために、アイシェードという目隠しをしてプレーするのですよね。"},{"id":{"d":153,"n":5,"t":4},"speaker":"A","text":"そうです。鈴の音を頼りにプレーするとは、すごいですよね。"},{"id":{"d":153,"n":6,"t":4},"speaker":"B","text":"私たちも、今度6人を集めて、3対3で対決してみませんか？"},{"id":{"d":153,"n":7,"t":4},"speaker":"A","text":"はい、ぜひ、挑戦したいです。私は鈴とボールを手配しておきます。"},{"id":{"d":153,"n":8,"t":4},"speaker":"B","text":"お願いします。では、私は目隠しを準備しますね。"}];
INSERT INTO sentence [{"id":{"d":154,"n":1,"t":4},"speaker":"A","text":"足に、いぼができました。"},{"id":{"d":154,"n":2,"t":4},"speaker":"B","text":"それは大変です。皮膚科に行って治療をしてもらいましょう。"},{"id":{"d":154,"n":3,"t":4},"speaker":"A","text":"どのような治療をするのですか？"},{"id":{"d":154,"n":4,"t":4},"speaker":"B","text":"ドライアイスでいぼを凍らせて、取り除くと思います。"},{"id":{"d":154,"n":5,"t":4},"speaker":"A","text":"それは痛そうですね。"},{"id":{"d":154,"n":6,"t":4},"speaker":"B","text":"少し痛みはあると思いますが、一瞬の痛みなので、我慢できると思います。"},{"id":{"d":154,"n":7,"t":4},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":155,"n":1,"t":4},"speaker":"A","text":"今日は、疲れました。"},{"id":{"d":155,"n":2,"t":4},"speaker":"B","text":"お疲れさまです。よく頑張った証拠ですね。"},{"id":{"d":155,"n":3,"t":4},"speaker":"A","text":"眠くて、あくびが止まりません。"},{"id":{"d":155,"n":4,"t":4},"speaker":"B","text":"ゆっくり休養してください。"},{"id":{"d":155,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":156,"n":1,"t":4},"speaker":"A","text":"いつも、良い香りがしますね。香水か何かをつけていますか？"},{"id":{"d":156,"n":2,"t":4},"speaker":"B","text":"何もつけていません。洗濯洗剤の香りでしょうか？"},{"id":{"d":156,"n":3,"t":4},"speaker":"A","text":"洗濯洗剤は、何を使っているのですか？"},{"id":{"d":156,"n":4,"t":4},"speaker":"B","text":"アタックを使っています。私の家では、昔からアタックを使っているのですよ。"},{"id":{"d":156,"n":5,"t":4},"speaker":"A","text":"洗濯洗剤はさまざまな種類がありますが、同じものを、長く使っていらっしゃるのですね。"},{"id":{"d":156,"n":6,"t":4},"speaker":"B","text":"私は、洗濯洗剤の中でアタックの香りが一番好きです。"},{"id":{"d":156,"n":7,"t":4},"speaker":"A","text":"そうなのですね。洗剤を選ぶときには、香りも重要ですね。"}];
INSERT INTO sentence [{"id":{"d":157,"n":1,"t":4},"speaker":"A","text":"最近、私はとても疲れが溜まっています。"},{"id":{"d":157,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？あなたが栄養剤を飲んでいるのをよく見かけます。栄養剤には糖分が多く含まれるので注意してくださいね。"},{"id":{"d":157,"n":3,"t":4},"speaker":"A","text":"そうなのですね。知りませんでした。"},{"id":{"d":157,"n":4,"t":4},"speaker":"B","text":"それに栄養剤には、カフェインも含まれているので、栄養剤の過剰摂取にならないよう、飲む回数には注意してください。疲れているときは、チョコレートがお薦めですよ。"},{"id":{"d":157,"n":5,"t":4},"speaker":"A","text":"教えてくれてありがとうございます。チョコレートもいいですね。"},{"id":{"d":157,"n":6,"t":4},"speaker":"B","text":"ただ、チョコレートも糖分の取りすぎにならないように、ほどほどにしてくださいね。"},{"id":{"d":157,"n":7,"t":4},"speaker":"A","text":"はい。そうします。"}];
INSERT INTO sentence [{"id":{"d":158,"n":1,"t":4},"speaker":"A","text":"佐藤さんは、最近元気がないようですがどうかしましたか？"},{"id":{"d":158,"n":2,"t":4},"speaker":"B","text":"疲れが、なかなか抜けないのです。"},{"id":{"d":158,"n":3,"t":4},"speaker":"A","text":"そうでしたか。ビタミン不足でしたらお薦めのビタミン剤を紹介しましょうか？"},{"id":{"d":158,"n":4,"t":4},"speaker":"B","text":"私はビタミン剤を飲んだことはありませんが、田中さんはよく飲んでいるのですか？お薦めのビタミン剤があれば教えてください。"},{"id":{"d":158,"n":5,"t":4},"speaker":"A","text":"私が飲んでいるのは、キューピーコーワαゼロドリンクです。ビタミンB類が配合されていることに加えて、糖類が入っていないところも気に入っています。"},{"id":{"d":158,"n":6,"t":4},"speaker":"B","text":"それは、良いことを聞きました。私も試してみます。"},{"id":{"d":158,"n":7,"t":4},"speaker":"A","text":"休息も、しっかり取ってくださいね。"},{"id":{"d":158,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":159,"n":1,"t":4},"speaker":"A","text":"佐藤さん、今日は採血をしますね。アレルギーは何かお持ちですか？"},{"id":{"d":159,"n":2,"t":4},"speaker":"B","text":"アルコールアレルギーと、ラテックスアレルギーがあります。"},{"id":{"d":159,"n":3,"t":4},"speaker":"A","text":"分かりました。では、アルコールの脱脂綿とゴム手袋を取り替えますね。"},{"id":{"d":159,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":159,"n":5,"t":4},"speaker":"A","text":"では、針を刺しますので、手をぎゅっと握ってください。手のしびれはありませんか？"},{"id":{"d":159,"n":6,"t":4},"speaker":"B","text":"大丈夫です。"},{"id":{"d":159,"n":7,"t":4},"speaker":"A","text":"手を楽にしてください。これで採血は終わりです。お疲れさまでした。"},{"id":{"d":159,"n":8,"t":4},"speaker":"B","text":"ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":160,"n":1,"t":4},"speaker":"A","text":"明日は、今日よりも寒くなるそうです。明日の服装に迷いますね。"},{"id":{"d":160,"n":2,"t":4},"speaker":"B","text":"そうですね。スカートを履こうと思っていましたが、寒いならズボンの方が良さそうですね。"},{"id":{"d":160,"n":3,"t":4},"speaker":"A","text":"明日は、外での活動が多いので、風邪をひかないようにズボンの方がいいと思います。"},{"id":{"d":160,"n":4,"t":4},"speaker":"B","text":"では、ズボンにします。防寒具も準備して、できるだけ厚着をします。"},{"id":{"d":160,"n":5,"t":4},"speaker":"A","text":"はい、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":161,"n":1,"t":4},"speaker":"A","text":"ボールを使って遊びませんか？"},{"id":{"d":161,"n":2,"t":4},"speaker":"B","text":"良いですよ。僕は、サッカーがしたいです。"},{"id":{"d":161,"n":3,"t":4},"speaker":"A","text":"2人では、ゲームはできないので、どちらが先にゴールを決めるか勝負をしませんか？"},{"id":{"d":161,"n":4,"t":4},"speaker":"B","text":"良いですよ。では、ここからゴールに向かってボールを蹴って、先に5点を入れた人が勝ち、ということにしましょう。"},{"id":{"d":161,"n":5,"t":4},"speaker":"A","text":"分かりました。負けた人は、腕立て伏せを10回する、というルールも決めませんか？"},{"id":{"d":161,"n":6,"t":4},"speaker":"B","text":"それは、負けたら大変ですね。でも、がぜん燃えてきました。頑張って勝ちたいです。"}];
INSERT INTO sentence [{"id":{"d":162,"n":1,"t":4},"speaker":"A","text":"高校野球の観戦は好きですか？"},{"id":{"d":162,"n":2,"t":4},"speaker":"B","text":"はい。夏に阪神甲子園球場で行われる試合は、毎年テレビ番組の放送を見て応援しています。日焼けした選手が、一生懸命ボールを追いかける姿に、毎回感動しています。"},{"id":{"d":162,"n":3,"t":4},"speaker":"A","text":"私も夏の甲子園は、毎年見ています。彼らの頑張る姿に、勇気と感動をもらっています。"},{"id":{"d":162,"n":4,"t":4},"speaker":"B","text":"目標に向かって、3年間も厳しい練習に耐えてきた彼らの思いを考えると、どの学校にも頑張ってほしいと思いますよね。"},{"id":{"d":162,"n":5,"t":4},"speaker":"A","text":"はい。本当にそう思います。今年も、そろそろ夏の甲子園が始まるので、楽しみにしています。"},{"id":{"d":162,"n":6,"t":4},"speaker":"B","text":"今年も、いろいろなドラマが待っているのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":163,"n":1,"t":4},"speaker":"A","text":"監督、そろそろ変化球を投げても良いでしょうか？"},{"id":{"d":163,"n":2,"t":4},"speaker":"B","text":"以前、「調子が悪い」と言っていた、肘の具合は大丈夫なのですか？"},{"id":{"d":163,"n":3,"t":4},"speaker":"A","text":"はい。おかげさまで、医者からも「そろそろ変化球を投げても良い」と言われました。"},{"id":{"d":163,"n":4,"t":4},"speaker":"B","text":"そうでしたか。しかし、今ここで無理をしてまた肘を痛めてしまったら、最後の試合に出られなくなってしまいます。"},{"id":{"d":163,"n":5,"t":4},"speaker":"A","text":"大丈夫です。僕は、早く変化球を投げたいのです。"},{"id":{"d":163,"n":6,"t":4},"speaker":"B","text":"そうですか。分かりました。ただ無理はさせられないので、変化球を投げる際は、球数を決めて、練習が終わったらアイシングとマッサージをしてください。"},{"id":{"d":163,"n":7,"t":4},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":164,"n":1,"t":4},"speaker":"A","text":"兼子さんは、術後も順調なので、明日からはリハビリテーションを始めますね。"},{"id":{"d":164,"n":2,"t":4},"speaker":"B","text":"リハビリテーションですか？まだ痛みがあるので、リハビリテーションをするのは無理です。"},{"id":{"d":164,"n":3,"t":4},"speaker":"A","text":"レントゲンを撮りましたが、手術は成功していますので安心してください。痛みが少し残っていても、リハビリテーションを始めますよ。"},{"id":{"d":164,"n":4,"t":4},"speaker":"B","text":"まだ、私は立つこともできないのにリハビリテーションはできません。"},{"id":{"d":164,"n":5,"t":4},"speaker":"A","text":"リハビリテーションは、立って体重をかけるところから少しずつ始めますから、心配しなくても大丈夫ですよ。明日は、14時にまた来ますね。"},{"id":{"d":164,"n":6,"t":4},"speaker":"B","text":"分かりました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":165,"n":1,"t":4},"speaker":"A","text":"もしもし、私は、下北沢幼稚園のさくら組担任の田中です。こちらは、三谷さんのお母さまのお電話で間違いないでしょうか？"},{"id":{"d":165,"n":2,"t":4},"speaker":"B","text":"はい、三谷です。いつもお世話になっております。"},{"id":{"d":165,"n":3,"t":4},"speaker":"A","text":"娘さんの真子さんに微熱があるようなので、お迎えに来てもらえますか？"},{"id":{"d":165,"n":4,"t":4},"speaker":"B","text":"承知しました。出先にいるので30分後くらいに着きます。"},{"id":{"d":165,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":166,"n":1,"t":4},"speaker":"A","text":"兼子さん、これからMRIを撮ります。"},{"id":{"d":166,"n":2,"t":4},"speaker":"B","text":"先生、私はMRIのような閉ざされた空間が苦手なのです。パニック障害の発作が起きてしまうかもしれません。"},{"id":{"d":166,"n":3,"t":4},"speaker":"A","text":"そうなのですね。今までMRIでパニックを起こしたことはありましたか？"},{"id":{"d":166,"n":4,"t":4},"speaker":"B","text":"はい。以前、足を骨折した際にMRIを撮る時に、パニックを起こしたことがありました。"},{"id":{"d":166,"n":5,"t":4},"speaker":"A","text":"分かりました。では、個室に入らずにできるMRIが良いと思うのですが、あいにく当院には設備がありません。別の病院を紹介します。後ほど紹介状を書いてお渡しするので、そちらの病院でMRIを撮って画像を持ってきてもらえますか？"},{"id":{"d":166,"n":6,"t":4},"speaker":"B","text":"分かりました。そのようにさせていただきます。"}];
INSERT INTO sentence [{"id":{"d":167,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。今日は、どのような髪形にしますか？"},{"id":{"d":167,"n":2,"t":4},"speaker":"B","text":"髪の毛の長さはあまり変えずに、明るめのカラーでお願いします。"},{"id":{"d":167,"n":3,"t":4},"speaker":"A","text":"前回と、同じ色でよろしいですか？"},{"id":{"d":167,"n":4,"t":4},"speaker":"B","text":"前回より、もう少し明るめのカラーでお願いします。"},{"id":{"d":167,"n":5,"t":4},"speaker":"A","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":168,"n":1,"t":4},"speaker":"A","text":"北條さん、会社の健康診断の予約はしてくれましたか？"},{"id":{"d":168,"n":2,"t":4},"speaker":"B","text":"申し訳ありません。失念しておりました。"},{"id":{"d":168,"n":3,"t":4},"speaker":"A","text":"早めに予約して、全社員に日程を伝えたいので、今日中に手配をお願いします。"},{"id":{"d":168,"n":4,"t":4},"speaker":"B","text":"分かりました。昨年と同様の集団健診で良いでしょうか？"},{"id":{"d":168,"n":5,"t":4},"speaker":"A","text":"そうですね。個別で行くのは大変なので、会社に健診車に来てもらって、会社で実施しましょう。"},{"id":{"d":168,"n":6,"t":4},"speaker":"B","text":"はい、そのように手配いたします。"}];
INSERT INTO sentence [{"id":{"d":169,"n":1,"t":4},"speaker":"A","text":"最近、デスクワークが続いて肩凝りがつらいです。"},{"id":{"d":169,"n":2,"t":4},"speaker":"B","text":"デスクワークですと、同じ姿勢が続きますからね。肩以外は大丈夫ですか？"},{"id":{"d":169,"n":3,"t":4},"speaker":"A","text":"はい、他は問題ありません。休憩中にストレッチ体操をしているのですが、なかなか改善されないようです。何か解消法をご存知ですか？"},{"id":{"d":169,"n":4,"t":4},"speaker":"B","text":"そうですね。私は血行を良くするために肩を温めています。使い捨てカイロを肩に貼ると、だいぶ楽になると思います。"},{"id":{"d":169,"n":5,"t":4},"speaker":"A","text":"そうなのですね。早速、試してみます。"}];
INSERT INTO sentence [{"id":{"d":170,"n":1,"t":4},"speaker":"A","text":"学校では、マスクを着用しなければならないのでしょうか？"},{"id":{"d":170,"n":2,"t":4},"speaker":"B","text":"はい、着用しなければならないそうです。"},{"id":{"d":170,"n":3,"t":4},"speaker":"A","text":"以前は、マスクの種類の指定はありませんでした。しかし、最近になって、できるだけ不織布のものにするよう連絡が回ってきました。"},{"id":{"d":170,"n":4,"t":4},"speaker":"B","text":"そうなのですね。やはり、不織布のマスクの方が安全なのでしょうか？"},{"id":{"d":170,"n":5,"t":4},"speaker":"A","text":"はい。しかし、私はマスク不足の時に、布マスクを大量に作ってしまいました。"},{"id":{"d":170,"n":6,"t":4},"speaker":"B","text":"私もたくさん作ったので、家にたくさん余ってしまっています。学校以外で使おうと思います。"}];
INSERT INTO sentence [{"id":{"d":171,"n":1,"t":4},"speaker":"A","text":"今年も、花粉症の症状が出て、つらいですね。"},{"id":{"d":171,"n":2,"t":4},"speaker":"B","text":"私は出産してから、一時的に少し症状が楽になりました。"},{"id":{"d":171,"n":3,"t":4},"speaker":"A","text":"うらやましいです。花粉症の症状が治ったのですか？"},{"id":{"d":171,"n":4,"t":4},"speaker":"B","text":"そうだったら良かったのですが、去年から、また症状が悪化してきました。今も目のかゆみと鼻水が止まりません。"},{"id":{"d":171,"n":5,"t":4},"speaker":"A","text":"また、以前のように戻ってしまったのですね。"},{"id":{"d":171,"n":6,"t":4},"speaker":"B","text":"はい、また薬を飲まないといけません。"}];
INSERT INTO sentence [{"id":{"d":172,"n":1,"t":4},"speaker":"A","text":"こんにちは。いつもの塗り薬が処方されています。"},{"id":{"d":172,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。すみません、お聞きしたいことがあるのですが、子どもの肌にも日焼け止めクリームを塗った方がいいですか？"},{"id":{"d":172,"n":3,"t":4},"speaker":"A","text":"そうですね。昔よりも、紫外線が強くなっているので、塗った方がいいと思います。子ども用の日焼け止めクリームも販売されていますよ。"},{"id":{"d":172,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。ドラッグストアで探してみます。"},{"id":{"d":172,"n":5,"t":4},"speaker":"A","text":"どういたしまして。お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":173,"n":1,"t":4},"speaker":"A","text":"佐藤さんは、マスクをずっとつけていると、耳の後ろがかゆくなりませんか？"},{"id":{"d":173,"n":2,"t":4},"speaker":"B","text":"私は、今のところは大丈夫です。"},{"id":{"d":173,"n":3,"t":4},"speaker":"A","text":"私は、かいてしまったためか、かさぶたになってしまいました。"},{"id":{"d":173,"n":4,"t":4},"speaker":"B","text":"保湿クリームを塗らないと、皮膚の症状がひどくなりそうですね。"},{"id":{"d":173,"n":5,"t":4},"speaker":"A","text":"今、保湿クリームがないので、薬局に行ってみようと思います。"},{"id":{"d":173,"n":6,"t":4},"speaker":"B","text":"それが良いと思います。"}];
INSERT INTO sentence [{"id":{"d":174,"n":1,"t":4},"speaker":"A","text":"最近、あなたは、運動をしていますか？"},{"id":{"d":174,"n":2,"t":4},"speaker":"B","text":"全然、運動をしていません。少しは体を動かさないと、健康のためには良くありませんよね。"},{"id":{"d":174,"n":3,"t":4},"speaker":"A","text":"はい。ただ、そうは思っていても、なかなか実行に移せません。"},{"id":{"d":174,"n":4,"t":4},"speaker":"B","text":"そうですね。毎日、散歩を続けている人は偉いと思います。"},{"id":{"d":174,"n":5,"t":4},"speaker":"A","text":"私もそう思います。"}];
INSERT INTO sentence [{"id":{"d":175,"n":1,"t":4},"speaker":"A","text":"最近、息子がサッカー教室に通い始めました。"},{"id":{"d":175,"n":2,"t":4},"speaker":"B","text":"それは奇遇ですね。実は私の息子も、最近サッカー教室に通い始めたのです。"},{"id":{"d":175,"n":3,"t":4},"speaker":"A","text":"あら、それは意外です。息子さんは、確か、「スポーツが嫌い」とおっしゃっていたと記憶していたので。"},{"id":{"d":175,"n":4,"t":4},"speaker":"B","text":"そうだったのですが、どういう心境の変化か、急に「通いたい」と言い始めたのです。"},{"id":{"d":175,"n":5,"t":4},"speaker":"A","text":"成長期にスポーツをすることは、健康増進などメリットも多いですから、良かったですね。"},{"id":{"d":175,"n":6,"t":4},"speaker":"B","text":"はい、私も夫もとても喜んでいます。さらにこれが続けば良いと思っています。"},{"id":{"d":175,"n":7,"t":4},"speaker":"A","text":"応援しています！"}];
INSERT INTO sentence [{"id":{"d":176,"n":1,"t":4},"speaker":"A","text":"最近、あなたのお肌の調子が、いつも以上に良さそうですね。"},{"id":{"d":176,"n":2,"t":4},"speaker":"B","text":"メーク落としと洗顔料を変えました。"},{"id":{"d":176,"n":3,"t":4},"speaker":"A","text":"やはり、少しの変化をし続けることが、いつまでも美しく居続けられる秘訣なのですね。"},{"id":{"d":176,"n":4,"t":4},"speaker":"B","text":"はい、調子が良いと心地よく過ごせますので、日々努力を続けています。"},{"id":{"d":176,"n":5,"t":4},"speaker":"A","text":"素晴らしいですね、見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":177,"n":1,"t":4},"speaker":"A","text":"鹿沼市に、蜂蜜を使った料理が食べられるレストランがあります。"},{"id":{"d":177,"n":2,"t":4},"speaker":"B","text":"その料理は、おいしそうですね。行ってみましょう。"},{"id":{"d":177,"n":3,"t":4},"speaker":"A","text":"本でも紹介されているから、有名なお店のようですよ。"},{"id":{"d":177,"n":4,"t":4},"speaker":"B","text":"そうなのですね。知らない住所ですが、カーナビゲーションシステムを使えば行けそうですね。"},{"id":{"d":177,"n":5,"t":4},"speaker":"A","text":"今はカーナビゲーションシステムがあるのでとても便利です。どのようなメニューがあるか、気になります。蜂蜜が売っていたら、お土産に買って帰りましょう。"},{"id":{"d":177,"n":6,"t":4},"speaker":"B","text":"それは、良いお土産になりそうですね。"}];
INSERT INTO sentence [{"id":{"d":178,"n":1,"t":4},"speaker":"A","text":"今から、運動と気分転換を兼ねて、散歩に行ってきます。"},{"id":{"d":178,"n":2,"t":4},"speaker":"B","text":"どちらまで行かれるのですか？"},{"id":{"d":178,"n":3,"t":4},"speaker":"A","text":"千鳥ヶ淵の方まで行ってみようと思っています。"},{"id":{"d":178,"n":4,"t":4},"speaker":"B","text":"今、千鳥ヶ淵は桜のきれいな季節なので楽しめそうですね。"},{"id":{"d":178,"n":5,"t":4},"speaker":"A","text":"よろしければ、ご一緒にいかがですか？"},{"id":{"d":178,"n":6,"t":4},"speaker":"B","text":"お誘いいただきありがとうございます。ぜひ、ご一緒させていただきたいのですが、あいにく先約があるのです。今回はお断りしたいと思います。"},{"id":{"d":178,"n":7,"t":4},"speaker":"A","text":"了解しました。またの機会を、楽しみにしております。"}];
INSERT INTO sentence [{"id":{"d":179,"n":1,"t":4},"speaker":"A","text":"貧血のせいか、立ちくらみがします。"},{"id":{"d":179,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？貧血には鉄分が良いそうですよ。鉄分を意識的に取ったらいかがですか？"},{"id":{"d":179,"n":3,"t":4},"speaker":"A","text":"どうして、鉄分を取ると、貧血に効果があるのでしょう？"},{"id":{"d":179,"n":4,"t":4},"speaker":"B","text":"血液に多く含まれる、ヘモグロビンは鉄分で、できているので、貧血の人は鉄分を多く取ると良いと聞きました。"},{"id":{"d":179,"n":5,"t":4},"speaker":"A","text":"詳しいですね。"},{"id":{"d":179,"n":6,"t":4},"speaker":"B","text":"ちょうどこの前、高校の授業で習いました。"},{"id":{"d":179,"n":7,"t":4},"speaker":"A","text":"授業で習ったことをきちんと覚えているとは、偉いですね。"}];
INSERT INTO sentence [{"id":{"d":180,"n":1,"t":4},"speaker":"A","text":"最近、あくびが止まりません。"},{"id":{"d":180,"n":2,"t":4},"speaker":"B","text":"十分な睡眠はとれていますか？"},{"id":{"d":180,"n":3,"t":4},"speaker":"A","text":"毎日の睡眠時間は、7時間くらいです。日中、眠いという自覚はありませんが、いつもあくびが出ます。"},{"id":{"d":180,"n":4,"t":4},"speaker":"B","text":"脳内の酸素が、足りないのかもしれませんね。"},{"id":{"d":180,"n":5,"t":4},"speaker":"A","text":"酸素が足りないとはどういうことでしょうか？"},{"id":{"d":180,"n":6,"t":4},"speaker":"B","text":"人間は脳内に酸素を取り込むために、大きな口を開けてあくびをするらしいです。"},{"id":{"d":180,"n":7,"t":4},"speaker":"A","text":"それは、知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":181,"n":1,"t":4},"speaker":"A","text":"私は、顔にニキビがたくさんできてしまって、困っています。"},{"id":{"d":181,"n":2,"t":4},"speaker":"B","text":"毎日洗顔は、していますか？"},{"id":{"d":181,"n":3,"t":4},"speaker":"A","text":"毎日洗顔をしていますし、洗顔後の保湿もしています。"},{"id":{"d":181,"n":4,"t":4},"speaker":"B","text":"皮膚科へかかった方がいいのではないでしょうか？"},{"id":{"d":181,"n":5,"t":4},"speaker":"A","text":"皮膚科に行くほどではないと思うのですが、それでも顔のニキビは気になります。"},{"id":{"d":181,"n":6,"t":4},"speaker":"B","text":"顔のニキビは気になりますよね。ひどくなる前に一度皮膚科で診てもらいましょう。"}];
INSERT INTO sentence [{"id":{"d":182,"n":1,"t":4},"speaker":"A","text":"外から帰ったら、手洗いとうがいを忘れないようにしましょう。"},{"id":{"d":182,"n":2,"t":4},"speaker":"B","text":"もちろんです。必ずしています。"},{"id":{"d":182,"n":3,"t":4},"speaker":"A","text":"家の中でも、小まめに手の消毒をすることを、忘れないようにしましょう。"},{"id":{"d":182,"n":4,"t":4},"speaker":"B","text":"家の中でも、消毒をする意味はありますか？"},{"id":{"d":182,"n":5,"t":4},"speaker":"A","text":"家の中にも、目には見えない菌があるのですよ。"},{"id":{"d":182,"n":6,"t":4},"speaker":"B","text":"そうなのですね。これからは、家にいても消毒するようにします。"}];
INSERT INTO sentence [{"id":{"d":183,"n":1,"t":4},"speaker":"A","text":"最近、あなたは家の掃除をよくしていますね。"},{"id":{"d":183,"n":2,"t":4},"speaker":"B","text":"家族で過ごす家を、きれいにすると気持ちが良いからです。"},{"id":{"d":183,"n":3,"t":4},"speaker":"A","text":"すがすがしい気持ちになりますね。いつもありがとうございます。"},{"id":{"d":183,"n":4,"t":4},"speaker":"B","text":"はい。掃除をすると、自分の心もきれいになる気がして、とても気持ちが良いですよ。"}];
INSERT INTO sentence [{"id":{"d":184,"n":1,"t":4},"speaker":"A","text":"昨夜は、お酒を飲みすぎて、今日は二日酔いです。"},{"id":{"d":184,"n":2,"t":4},"speaker":"B","text":"昨日は、どれくらい飲んだのですか？"},{"id":{"d":184,"n":3,"t":4},"speaker":"A","text":"結構な量を飲みました。ビールとワインをたくさん飲みました。"},{"id":{"d":184,"n":4,"t":4},"speaker":"B","text":"お酒も適量にしないと、体を壊してしまいますよ。"},{"id":{"d":184,"n":5,"t":4},"speaker":"A","text":"その通りですね。年末は、お酒を飲む機会が増えるので気を付けます。"}];
INSERT INTO sentence [{"id":{"d":185,"n":1,"t":4},"speaker":"A","text":"今日の体育の授業の内容は、徒競走でした。"},{"id":{"d":185,"n":2,"t":4},"speaker":"B","text":"結果はどうでしたか？"},{"id":{"d":185,"n":3,"t":4},"speaker":"A","text":"私の順位は、2位でした。途中まで1位でしたが、最後は、佐藤君に抜かれてしまいました。"},{"id":{"d":185,"n":4,"t":4},"speaker":"B","text":"それは残念でしたね。次は1位を取れると良いですね。"},{"id":{"d":185,"n":5,"t":4},"speaker":"A","text":"はい。次こそは1位を取れるように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":186,"n":1,"t":4},"speaker":"A","text":"加藤さんは、サウナは好きですか？"},{"id":{"d":186,"n":2,"t":4},"speaker":"B","text":"サウナは大好きです。毎週末に、サウナへに出かけています。"},{"id":{"d":186,"n":3,"t":4},"speaker":"A","text":"それはすごいですね。私はサウナには1回しか行ったことがありません。"},{"id":{"d":186,"n":4,"t":4},"speaker":"B","text":"ぜひもっと、あなたもサウナに行ってみてください。ちなみに、私はサウナ同好会に入っています。"},{"id":{"d":186,"n":5,"t":4},"speaker":"A","text":"サウナ同好会ですか？どのような活動をしているのですか？"},{"id":{"d":186,"n":6,"t":4},"speaker":"B","text":"お薦めのサウナの情報を交換したり、仲間同士でサウナに出かけたりしています。"},{"id":{"d":186,"n":7,"t":4},"speaker":"A","text":"それは楽しそうですね。サウナ以外にも楽しみがありますね。良かったら、私にも紹介してくれませんか？"},{"id":{"d":186,"n":8,"t":4},"speaker":"B","text":"ええ、ぜひ。同じ趣味の仲間とのやりとりはとても楽しいですよ。"}];
INSERT INTO sentence [{"id":{"d":187,"n":1,"t":4},"speaker":"A","text":"最近、下腹部に痛みがあります。"},{"id":{"d":187,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":187,"n":3,"t":4},"speaker":"A","text":"はい。便秘気味のせいかと思います。"},{"id":{"d":187,"n":4,"t":4},"speaker":"B","text":"便秘も放っておくと病気の元になるので、注意が必要です。便秘解消には食物繊維を取ると良いですよ。"},{"id":{"d":187,"n":5,"t":4},"speaker":"A","text":"心掛けてはいるのですが、食物繊維を取るのは難しいです。"},{"id":{"d":187,"n":6,"t":4},"speaker":"B","text":"今は食物繊維入りのサプリメントもあるので、手軽に摂取することができます。他にも運動や水分の摂取も効果がありますよ。"},{"id":{"d":187,"n":7,"t":4},"speaker":"A","text":"運動や水分摂取ならできそうです。試してみます。"}];
INSERT INTO sentence [{"id":{"d":188,"n":1,"t":4},"speaker":"A","text":"先生、失礼します。"},{"id":{"d":188,"n":2,"t":4},"speaker":"B","text":"どうしましたか？"},{"id":{"d":188,"n":3,"t":4},"speaker":"A","text":"体育の授業中に転んでしまい、膝を擦りむいてしまいました。"},{"id":{"d":188,"n":4,"t":4},"speaker":"B","text":"それは痛かったですね。傷を診ますので、こちらに来てください。"},{"id":{"d":188,"n":5,"t":4},"speaker":"A","text":"はい、先生。"},{"id":{"d":188,"n":6,"t":4},"speaker":"B","text":"見た目よりも傷が少し深いですね。消毒して治療しておきますので、もし痛みが続くようなら一度病院へ行ってくださいね。"},{"id":{"d":188,"n":7,"t":4},"speaker":"A","text":"分かりました。先生、ありがとうございました。"},{"id":{"d":188,"n":8,"t":4},"speaker":"B","text":"お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":189,"n":1,"t":4},"speaker":"A","text":"今度の休みは、海へ泳ぎに行きませんか？"},{"id":{"d":189,"n":2,"t":4},"speaker":"B","text":"ごめんなさい、海は苦手です。"},{"id":{"d":189,"n":3,"t":4},"speaker":"A","text":"どうしてですか？"},{"id":{"d":189,"n":4,"t":4},"speaker":"B","text":"子どもの頃、家族で海に行った時、溺れそうになったことがあるからです。"},{"id":{"d":189,"n":5,"t":4},"speaker":"A","text":"そうでしたか。それは怖かったですね。"},{"id":{"d":189,"n":6,"t":4},"speaker":"B","text":"それ以来、海は苦手になりました。"},{"id":{"d":189,"n":7,"t":4},"speaker":"A","text":"分かりました。また別のことでお誘いします。"},{"id":{"d":189,"n":8,"t":4},"speaker":"B","text":"すみません。声を掛けてくださってありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":190,"n":1,"t":4},"speaker":"A","text":"くしゃみをしているけれど、大丈夫ですか？"},{"id":{"d":190,"n":2,"t":4},"speaker":"B","text":"なかなかくしゃみが止まらなくて困っています。"},{"id":{"d":190,"n":3,"t":4},"speaker":"A","text":"風邪ですか？"},{"id":{"d":190,"n":4,"t":4},"speaker":"B","text":"いえ、多分お昼に食べたマーボー豆腐の香辛料のせいだと思います。"},{"id":{"d":190,"n":5,"t":4},"speaker":"A","text":"それで、くしゃみが止まらないのですね。風邪でないのなら安心しました。"},{"id":{"d":190,"n":6,"t":4},"speaker":"B","text":"先ほどより、だいぶくしゃみは出なくなったので、大丈夫だと思います。"},{"id":{"d":190,"n":7,"t":4},"speaker":"A","text":"早く体調が落ち着くと良いですね。"}];
INSERT INTO sentence [{"id":{"d":191,"n":1,"t":4},"speaker":"A","text":"私は、禁煙して2週間になりました。"},{"id":{"d":191,"n":2,"t":4},"speaker":"B","text":"それは、素晴らしい心掛けですね。"},{"id":{"d":191,"n":3,"t":4},"speaker":"A","text":"タバコは体に悪いですし、周りの人にも迷惑をかけてしまいますので。"},{"id":{"d":191,"n":4,"t":4},"speaker":"B","text":"体に何か変化はありますか？"},{"id":{"d":191,"n":5,"t":4},"speaker":"A","text":"禁煙を始めて、まだ2週間ですが、階段の上り下りの際の息苦しさは軽減した感じがします。"},{"id":{"d":191,"n":6,"t":4},"speaker":"B","text":"早速、効果が出ているのですね。"},{"id":{"d":191,"n":7,"t":4},"speaker":"A","text":"はい。これからはジョギングにも挑戦したいと思います。"},{"id":{"d":191,"n":8,"t":4},"speaker":"B","text":"それは、楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":192,"n":1,"t":4},"speaker":"A","text":"冬は、空気が乾燥するので苦手です。"},{"id":{"d":192,"n":2,"t":4},"speaker":"B","text":"乾燥で、手や唇が荒れますよね。"},{"id":{"d":192,"n":3,"t":4},"speaker":"A","text":"はい、私は、ハンドクリームとリップクリームをいつも持ち歩いています。"},{"id":{"d":192,"n":4,"t":4},"speaker":"B","text":"私もそうです。家では加湿器をつけて乾燥予防をしています。"},{"id":{"d":192,"n":5,"t":4},"speaker":"A","text":"洗濯物を部屋干しするだけでも、部屋の湿度が保たれるようですね。"},{"id":{"d":192,"n":6,"t":4},"speaker":"B","text":"加湿器の電気代を節約しながら、部屋の湿度も保つことができて、とても良いですね。"}];
INSERT INTO sentence [{"id":{"d":193,"n":1,"t":4},"speaker":"A","text":"最近、近くの物が見えないことがあります。"},{"id":{"d":193,"n":2,"t":4},"speaker":"B","text":"それは、いつ頃からですか？"},{"id":{"d":193,"n":3,"t":4},"speaker":"A","text":"近くの物が見えにくくなってから、3カ月くらいたちます。"},{"id":{"d":193,"n":4,"t":4},"speaker":"B","text":"そうなのですね。それでは、目の検査を行いましょう。"},{"id":{"d":193,"n":5,"t":4},"speaker":"A","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":194,"n":1,"t":4},"speaker":"A","text":"今日は、採血を行いますね。採血が終わったら再度お呼びします。"},{"id":{"d":194,"n":2,"t":4},"speaker":"B","text":"何を検査するための採血ですか？"},{"id":{"d":194,"n":3,"t":4},"speaker":"A","text":"今回は、貧血の検査です。採血をして、具合が悪くなったことはありますか？"},{"id":{"d":194,"n":4,"t":4},"speaker":"B","text":"ありません。"},{"id":{"d":194,"n":5,"t":4},"speaker":"A","text":"分かりました。採血は隣の診察室で行いますので、いったん待合室でお待ちください。"},{"id":{"d":194,"n":6,"t":4},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":195,"n":1,"t":4},"speaker":"A","text":"寝る前にストレッチ体操をすると、良い睡眠が取れるそうです。"},{"id":{"d":195,"n":2,"t":4},"speaker":"B","text":"そうなのですね。毎日ストレッチ体操をしてから寝ているのですか？"},{"id":{"d":195,"n":3,"t":4},"speaker":"A","text":"はい。必ず5分ほどストレッチ体操をするようにしています。"},{"id":{"d":195,"n":4,"t":4},"speaker":"B","text":"効果はありますか？"},{"id":{"d":195,"n":5,"t":4},"speaker":"A","text":"そうですね。ストレッチ体操を行うようになってから、睡眠の質が良くなりました。"},{"id":{"d":195,"n":6,"t":4},"speaker":"B","text":"それは良いですね。私もストレッチ体操をしてみます。"}];
INSERT INTO sentence [{"id":{"d":196,"n":1,"t":4},"speaker":"A","text":"ラジオ体操は健康に良いそうです。"},{"id":{"d":196,"n":2,"t":4},"speaker":"B","text":"ラジオ体操ですか。子どもの時以来、全くしていません。"},{"id":{"d":196,"n":3,"t":4},"speaker":"A","text":"ラジオ体操をしっかりと行うと、良い全身運動になりますよ。"},{"id":{"d":196,"n":4,"t":4},"speaker":"B","text":"そうなのですね。子どもと一緒に朝からラジオ体操をしてみようと思います。"},{"id":{"d":196,"n":5,"t":4},"speaker":"A","text":"少しでも、運動不足解消になると良いですね。"}];
INSERT INTO sentence [{"id":{"d":197,"n":1,"t":4},"speaker":"A","text":"しゃっくりが止まらない時は、どうしたら良いですか？"},{"id":{"d":197,"n":2,"t":4},"speaker":"B","text":"息を止めると良いと、聞いたことがあります。"},{"id":{"d":197,"n":3,"t":4},"speaker":"A","text":"それはもう試してみましたが、まだ止まりません。その他に良い方法はありますか？"},{"id":{"d":197,"n":4,"t":4},"speaker":"B","text":"水をたくさん飲むと良いともいわれていますね。それ以外の方法は、分かりません。"},{"id":{"d":197,"n":5,"t":4},"speaker":"A","text":"水も既に飲んでいました。止まるまでおとなしく待つことにします。"},{"id":{"d":197,"n":6,"t":4},"speaker":"B","text":"お役に立てず、すみません。"}];
INSERT INTO sentence [{"id":{"d":198,"n":1,"t":4},"speaker":"A","text":"昨日、焼き肉を食べ過ぎてしまったせいか、胃が気持ち悪いです。"},{"id":{"d":198,"n":2,"t":4},"speaker":"B","text":"せっかくおいしいものを食べたのに、胃がもたれてしまったのですね。"},{"id":{"d":198,"n":3,"t":4},"speaker":"A","text":"そうなのです。最近年のせいか油っぽいものを食べると、いつも胃もたれを起こしてしまいます。"},{"id":{"d":198,"n":4,"t":4},"speaker":"B","text":"胃薬は飲みましたか？"},{"id":{"d":198,"n":5,"t":4},"speaker":"A","text":"まだ飲んでいません。"},{"id":{"d":198,"n":6,"t":4},"speaker":"B","text":"胃薬を飲むと、症状が和らぐと思います。ドラッグストアに売っていますよ。"},{"id":{"d":198,"n":7,"t":4},"speaker":"A","text":"分かりました。夕方、仕事が終わったら、買いに行くことにします。"},{"id":{"d":198,"n":8,"t":4},"speaker":"B","text":"はい、ぜひそうしてください。それでも良くならない時は、病院へ行った方がいいですね。"},{"id":{"d":198,"n":9,"t":4},"speaker":"A","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":199,"n":1,"t":4},"speaker":"A","text":"診察の予約をお願いします。"},{"id":{"d":199,"n":2,"t":4},"speaker":"B","text":"当院のご利用は初めてですか？"},{"id":{"d":199,"n":3,"t":4},"speaker":"A","text":"はい。"},{"id":{"d":199,"n":4,"t":4},"speaker":"B","text":"初診の方は、来週以降であれば予約することができるのですが、それでもよろしいですか？"},{"id":{"d":199,"n":5,"t":4},"speaker":"A","text":"大丈夫です。"},{"id":{"d":199,"n":6,"t":4},"speaker":"B","text":"来週の月曜日の午後はいかがでしょうか？"},{"id":{"d":199,"n":7,"t":4},"speaker":"A","text":"16時はすいていますか？"},{"id":{"d":199,"n":8,"t":4},"speaker":"B","text":"はい、すいています。"},{"id":{"d":199,"n":9,"t":4},"speaker":"A","text":"では、その時間で予約をお願いします。"}];
INSERT INTO sentence [{"id":{"d":200,"n":1,"t":4},"speaker":"A","text":"最近、肩凝りがひどいです。"},{"id":{"d":200,"n":2,"t":4},"speaker":"B","text":"肩凝りはつらいですよね。"},{"id":{"d":200,"n":3,"t":4},"speaker":"A","text":"あなたも肩が凝っていますか？"},{"id":{"d":200,"n":4,"t":4},"speaker":"B","text":"はい。時々、子どもに肩をもんでもらっています。"},{"id":{"d":200,"n":5,"t":4},"speaker":"A","text":"いいですね。私は1人暮らしなのでうらやましいです。"},{"id":{"d":200,"n":6,"t":4},"speaker":"B","text":"マッサージに行ってみてはどうですか？"},{"id":{"d":200,"n":7,"t":4},"speaker":"A","text":"インターネットでお店を調べたら、予約がいっぱいでした。"},{"id":{"d":200,"n":8,"t":4},"speaker":"B","text":"私たちと同じように、肩凝りの人が多いのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":201,"n":1,"t":4},"speaker":"A","text":"処方箋をお預かりします。"},{"id":{"d":201,"n":2,"t":4},"speaker":"B","text":"お願いします。"},{"id":{"d":201,"n":3,"t":4},"speaker":"A","text":"健康保険証はお持ちですか？"},{"id":{"d":201,"n":4,"t":4},"speaker":"B","text":"はい、これです。"},{"id":{"d":201,"n":5,"t":4},"speaker":"A","text":"今まで、薬を服用して、アレルギーの症状が出たことはありますか？"},{"id":{"d":201,"n":6,"t":4},"speaker":"B","text":"ありません。"},{"id":{"d":201,"n":7,"t":4},"speaker":"A","text":"他に服用している薬はありますか？"},{"id":{"d":201,"n":8,"t":4},"speaker":"B","text":"血圧を下げる薬を飲んでします。"},{"id":{"d":201,"n":9,"t":4},"speaker":"A","text":"分かりました。薬の用意ができるまで、待合室で座ってお待ちください。"}];
INSERT INTO sentence [{"id":{"d":202,"n":1,"t":4},"speaker":"A","text":"採血をするので、腕を出してください。"},{"id":{"d":202,"n":2,"t":4},"speaker":"B","text":"右腕ですか？左腕ですか？"},{"id":{"d":202,"n":3,"t":4},"speaker":"A","text":"どちらでもよいですよ。"},{"id":{"d":202,"n":4,"t":4},"speaker":"B","text":"右は利き腕なので、左腕でお願いします。"},{"id":{"d":202,"n":5,"t":4},"speaker":"A","text":"では左腕を、こちらの台にのせてください。"},{"id":{"d":202,"n":6,"t":4},"speaker":"B","text":"緊張してしまいます。"},{"id":{"d":202,"n":7,"t":4},"speaker":"A","text":"すぐに済みますよ。"},{"id":{"d":202,"n":8,"t":4},"speaker":"B","text":"以前、採血で気分が悪くなったことがあるのです。"},{"id":{"d":202,"n":9,"t":4},"speaker":"A","text":"そうでしたか。もし今回も、気分が悪くなるようでしたら、遠慮なく言ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":203,"n":1,"t":4},"speaker":"A","text":"花粉症で、くしゃみばかりしてしまいます。"},{"id":{"d":203,"n":2,"t":4},"speaker":"B","text":"大変ですね。病院に行ってみてはどうですか？"},{"id":{"d":203,"n":3,"t":4},"speaker":"A","text":"先週行きました。"},{"id":{"d":203,"n":4,"t":4},"speaker":"B","text":"それでは、薬を処方してもらえたのではありませんか？"},{"id":{"d":203,"n":5,"t":4},"speaker":"A","text":"そうなのですが、薬を飲むと眠くなってしまうのです。"},{"id":{"d":203,"n":6,"t":4},"speaker":"B","text":"副作用でしょうか？"},{"id":{"d":203,"n":7,"t":4},"speaker":"A","text":"恐らく、そうだと思います。とても眠くて、飲むのをためらってしまいます。"},{"id":{"d":203,"n":8,"t":4},"speaker":"B","text":"他の薬を処方してもらっては、どうですか？"},{"id":{"d":203,"n":9,"t":4},"speaker":"A","text":"そうですね。近いうちに、病院で相談してみます。"}];
INSERT INTO sentence [{"id":{"d":204,"n":1,"t":4},"speaker":"A","text":"蚊が多いので、虫除けスプレーを吹き付けましましょうか？"},{"id":{"d":204,"n":2,"t":4},"speaker":"B","text":"いつも虫除けを持ち歩いているのですか？"},{"id":{"d":204,"n":3,"t":4},"speaker":"A","text":"はい、いつもバッグに入れています。"},{"id":{"d":204,"n":4,"t":4},"speaker":"B","text":"準備がいいですね。"},{"id":{"d":204,"n":5,"t":4},"speaker":"A","text":"蚊に刺されると、皮膚がひどく腫れてしまうので、なるべく刺されないようにしたいのです。"},{"id":{"d":204,"n":6,"t":4},"speaker":"B","text":"そうですか。私は虫除けスプレーを家に置いてきてしまいました。"},{"id":{"d":204,"n":7,"t":4},"speaker":"A","text":"良かったら、私の物を使ってください。"},{"id":{"d":204,"n":8,"t":4},"speaker":"B","text":"よいのですか？ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":205,"n":1,"t":4},"speaker":"A","text":"あなたは、家で加湿器を使っていますか？"},{"id":{"d":205,"n":2,"t":4},"speaker":"B","text":"はい。冬は、乾燥するので毎日使っています。"},{"id":{"d":205,"n":3,"t":4},"speaker":"A","text":"私も乾燥が気になるので加湿器を買いたいのですが、どれがいいか迷っています。"},{"id":{"d":205,"n":4,"t":4},"speaker":"B","text":"私は、加湿器を買うときにたくさん調べました。"},{"id":{"d":205,"n":5,"t":4},"speaker":"A","text":"どれにするか、決めるポイントは何でしたか？"},{"id":{"d":205,"n":6,"t":4},"speaker":"B","text":"空気清浄機能が、付いていることを条件に選びました。"},{"id":{"d":205,"n":7,"t":4},"speaker":"A","text":"それは大事ですね。"},{"id":{"d":205,"n":8,"t":4},"speaker":"B","text":"さらに、手入れが楽かどうかも重要だと思います。"},{"id":{"d":205,"n":9,"t":4},"speaker":"A","text":"そうですね。参考にします。"}];
INSERT INTO sentence [{"id":{"d":206,"n":1,"t":4},"speaker":"A","text":"学校で、黒板の文字が見えにくくなりました。"},{"id":{"d":206,"n":2,"t":4},"speaker":"B","text":"席を前の方にしてもらってはどうですか？"},{"id":{"d":206,"n":3,"t":4},"speaker":"A","text":"既に、教室では一番前の席なのですが、それでも見えにくいのです。"},{"id":{"d":206,"n":4,"t":4},"speaker":"B","text":"視力が落ちているのかもしれませんね。"},{"id":{"d":206,"n":5,"t":4},"speaker":"A","text":"ゲームのし過ぎでしょうか？"},{"id":{"d":206,"n":6,"t":4},"speaker":"B","text":"そうかもしれませんが、私も近視なので遺伝なのかもしれませんよ。"},{"id":{"d":206,"n":7,"t":4},"speaker":"A","text":"眼科に行って、視力を測った方がいいでしょうか？"},{"id":{"d":206,"n":8,"t":4},"speaker":"B","text":"そうですね。眼科に予約をしておきます。"}];
INSERT INTO sentence [{"id":{"d":207,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":207,"n":2,"t":4},"speaker":"B","text":"せきがひどく、発熱も少しあります。"},{"id":{"d":207,"n":3,"t":4},"speaker":"A","text":"せきはいつから出ていますか？"},{"id":{"d":207,"n":4,"t":4},"speaker":"B","text":"3日くらい前からです。"},{"id":{"d":207,"n":5,"t":4},"speaker":"A","text":"熱も3日前から出ているのですか？"},{"id":{"d":207,"n":6,"t":4},"speaker":"B","text":"いえ、発熱は今日からです。"},{"id":{"d":207,"n":7,"t":4},"speaker":"A","text":"他に、症状はありませんか？"},{"id":{"d":207,"n":8,"t":4},"speaker":"B","text":"ありません。"},{"id":{"d":207,"n":9,"t":4},"speaker":"A","text":"それでは喉を診ますので、口を大きく開けてください。"}];
INSERT INTO sentence [{"id":{"d":208,"n":1,"t":4},"speaker":"A","text":"今度、ぜひうちに遊びに来てください。"},{"id":{"d":208,"n":2,"t":4},"speaker":"B","text":"そうしたいのですが、確か、あなたは猫を飼っていますよね？"},{"id":{"d":208,"n":3,"t":4},"speaker":"A","text":"飼っていますが、どうしてですか？"},{"id":{"d":208,"n":4,"t":4},"speaker":"B","text":"私は、猫アレルギーなのです。"},{"id":{"d":208,"n":5,"t":4},"speaker":"A","text":"どのような症状が出るのですか？"},{"id":{"d":208,"n":6,"t":4},"speaker":"B","text":"くしゃみが止まらなくなってしまいます。"},{"id":{"d":208,"n":7,"t":4},"speaker":"A","text":"そうですか。お招きできなくて残念です。"},{"id":{"d":208,"n":8,"t":4},"speaker":"B","text":"代わりに、うちにいらしてください。"}];
INSERT INTO sentence [{"id":{"d":209,"n":1,"t":4},"speaker":"A","text":"歯科医院で診てもらった結果は、どうでしたか？"},{"id":{"d":209,"n":2,"t":4},"speaker":"B","text":"痛みがある歯は、虫歯でした。"},{"id":{"d":209,"n":3,"t":4},"speaker":"A","text":"ひどい虫歯ですか？"},{"id":{"d":209,"n":4,"t":4},"speaker":"B","text":"はい。歯を抜かなければならないそうです。"},{"id":{"d":209,"n":5,"t":4},"speaker":"A","text":"もっと早めに、歯科医院で診察してもらえば良かったですね。"},{"id":{"d":209,"n":6,"t":4},"speaker":"B","text":"後悔しています。"},{"id":{"d":209,"n":7,"t":4},"speaker":"A","text":"長く通院することになりそうですか？"},{"id":{"d":209,"n":8,"t":4},"speaker":"B","text":"他にも虫歯があるようなので、長期間、歯科医院へ通うことを覚悟しています。"}];
INSERT INTO sentence [{"id":{"d":210,"n":1,"t":4},"speaker":"A","text":"土曜日は、予定がありますか？"},{"id":{"d":210,"n":2,"t":4},"speaker":"B","text":"午後は、部活動の練習があります。"},{"id":{"d":210,"n":3,"t":4},"speaker":"A","text":"日差しが強いので、日焼け止めを肌に塗ってくださいね。"},{"id":{"d":210,"n":4,"t":4},"speaker":"B","text":"そうですね。出かける前に塗ります。"},{"id":{"d":210,"n":5,"t":4},"speaker":"A","text":"部活動の練習時間が長いようなら、途中で日焼け止めを塗り直した方がいいですよ。"},{"id":{"d":210,"n":6,"t":4},"speaker":"B","text":"では、日焼け止めを持って部活動へ行ってよいですか？"},{"id":{"d":210,"n":7,"t":4},"speaker":"A","text":"もちろん、よいですよ。帽子も忘れず持参してくださいね。"},{"id":{"d":210,"n":8,"t":4},"speaker":"B","text":"はい。忘れないように、玄関に置いておきます。"}];
INSERT INTO sentence [{"id":{"d":211,"n":1,"t":4},"speaker":"A","text":"私は、髪の毛の傷みが気になります。"},{"id":{"d":211,"n":2,"t":4},"speaker":"B","text":"トリートメントを買ってみましょうか？"},{"id":{"d":211,"n":3,"t":4},"speaker":"A","text":"昨日、ドラッグストアでトリートメントの商品棚を見たのですが、種類が多くてどれが良いか分かりませんでした。"},{"id":{"d":211,"n":4,"t":4},"speaker":"B","text":"洗い流さないタイプのトリートメントが、良いと聞いたことがあります。"},{"id":{"d":211,"n":5,"t":4},"speaker":"A","text":"それでも、いくつか種類がありますよね。"},{"id":{"d":211,"n":6,"t":4},"speaker":"B","text":"私が前に使ったことのある、トリートメントをあなたも試してみますか？"},{"id":{"d":211,"n":7,"t":4},"speaker":"A","text":"そのトリートメントの効き目はどうですか？"},{"id":{"d":211,"n":8,"t":4},"speaker":"B","text":"髪の毛が、しっとりまとまるようになりました。"},{"id":{"d":211,"n":9,"t":4},"speaker":"A","text":"試したいので、そのトリートメントの名称を教えてください。"}];
INSERT INTO sentence [{"id":{"d":212,"n":1,"t":4},"speaker":"A","text":"コーヒーでも飲みましょうか？"},{"id":{"d":212,"n":2,"t":4},"speaker":"B","text":"夜なので、やめておきます。"},{"id":{"d":212,"n":3,"t":4},"speaker":"A","text":"カフェインを気にしているのですか？"},{"id":{"d":212,"n":4,"t":4},"speaker":"B","text":"はい。夜にカフェインを取ると、眠れなくなってしまうのです。"},{"id":{"d":212,"n":5,"t":4},"speaker":"A","text":"それほどに、睡眠に影響があるのですね。"},{"id":{"d":212,"n":6,"t":4},"speaker":"B","text":"あなたは、大丈夫ですか？"},{"id":{"d":212,"n":7,"t":4},"speaker":"A","text":"寝る直前にカフェインを取っても、全く影響ありません。"},{"id":{"d":212,"n":8,"t":4},"speaker":"B","text":"体質の違いなのでしょうね。うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":213,"n":1,"t":4},"speaker":"A","text":"手の甲をけがしたのですか？"},{"id":{"d":213,"n":2,"t":4},"speaker":"B","text":"やけどをしてしまったのです。"},{"id":{"d":213,"n":3,"t":4},"speaker":"A","text":"どうして、やけどをしてしまったのですか？"},{"id":{"d":213,"n":4,"t":4},"speaker":"B","text":"揚げ物をしていたら、急に油が跳ねたのです。"},{"id":{"d":213,"n":5,"t":4},"speaker":"A","text":"それは熱くて痛そうですね。"},{"id":{"d":213,"n":6,"t":4},"speaker":"B","text":"とても痛かったです。すぐに冷やしましたが、水膨れになりました。"},{"id":{"d":213,"n":7,"t":4},"speaker":"A","text":"痕が残らないといいですね。"},{"id":{"d":213,"n":8,"t":4},"speaker":"B","text":"はい。きれいに治ることを願っています。"}];
INSERT INTO sentence [{"id":{"d":214,"n":1,"t":4},"speaker":"A","text":"夫が、急性腰痛になってしまいました。"},{"id":{"d":214,"n":2,"t":4},"speaker":"B","text":"それは大変ですね。"},{"id":{"d":214,"n":3,"t":4},"speaker":"A","text":"夫は2日間、寝込んでいます。"},{"id":{"d":214,"n":4,"t":4},"speaker":"B","text":"仕事も休んでいるのですか？"},{"id":{"d":214,"n":5,"t":4},"speaker":"A","text":"はい。かろうじてトイレに行くことができるくらいで、出勤はとても無理です。"},{"id":{"d":214,"n":6,"t":4},"speaker":"B","text":"病院には、行きましたか？"},{"id":{"d":214,"n":7,"t":4},"speaker":"A","text":"昨日、かろうじて病院へ連れて行きましたが、自然に回復するのを待つより、ほかはないようです。"},{"id":{"d":214,"n":8,"t":4},"speaker":"B","text":"お大事にと、お伝えください。"},{"id":{"d":214,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":215,"n":1,"t":4},"speaker":"A","text":"妊婦健診に行ってきました。"},{"id":{"d":215,"n":2,"t":4},"speaker":"B","text":"先生は、何とおっしゃっていましたか？"},{"id":{"d":215,"n":3,"t":4},"speaker":"A","text":"赤ちゃんは順調に育っているそうです。"},{"id":{"d":215,"n":4,"t":4},"speaker":"B","text":"それは安心です。"},{"id":{"d":215,"n":5,"t":4},"speaker":"A","text":"ただ、私自身が貧血気味だと指摘されました。"},{"id":{"d":215,"n":6,"t":4},"speaker":"B","text":"鉄分を摂取した方がいいですね。"},{"id":{"d":215,"n":7,"t":4},"speaker":"A","text":"はい。鉄分の多い食材をいろいろと教えてもらいました。"},{"id":{"d":215,"n":8,"t":4},"speaker":"B","text":"料理に積極的に取り入れていきましょう。"}];
INSERT INTO sentence [{"id":{"d":216,"n":1,"t":4},"speaker":"A","text":"湿布はどこにありますか？"},{"id":{"d":216,"n":2,"t":4},"speaker":"B","text":"リビングの棚にありますよ。どこかを痛めたのですか？"},{"id":{"d":216,"n":3,"t":4},"speaker":"A","text":"足首をひねってしまいました。"},{"id":{"d":216,"n":4,"t":4},"speaker":"B","text":"痛みは、ひどいですか？"},{"id":{"d":216,"n":5,"t":4},"speaker":"A","text":"少し痛いだけなので、湿布をすれば大丈夫だと思います。"},{"id":{"d":216,"n":6,"t":4},"speaker":"B","text":"どうして、ひねってしまったのですか？"},{"id":{"d":216,"n":7,"t":4},"speaker":"A","text":"サッカーの練習中に、足首をくじいてしまいました。"},{"id":{"d":216,"n":8,"t":4},"speaker":"B","text":"明日まで様子を見て、腫れているようなら病院に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":217,"n":1,"t":4},"speaker":"A","text":"おいしいワインが飲めるお店を見つけたので、一緒に行きませんか？"},{"id":{"d":217,"n":2,"t":4},"speaker":"B","text":"すみませんが、実は禁酒をしているのです。"},{"id":{"d":217,"n":3,"t":4},"speaker":"A","text":"そうなのですね。何かあったのですか？"},{"id":{"d":217,"n":4,"t":4},"speaker":"B","text":"健康診断の結果が出たのですが、肝臓の数値が悪かったのです。"},{"id":{"d":217,"n":5,"t":4},"speaker":"A","text":"いつから禁酒しているのですか？"},{"id":{"d":217,"n":6,"t":4},"speaker":"B","text":"2カ月前からです。"},{"id":{"d":217,"n":7,"t":4},"speaker":"A","text":"私も見習いたいです。"},{"id":{"d":217,"n":8,"t":4},"speaker":"B","text":"健康なのであれば、適度にお酒を飲むのはいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":218,"n":1,"t":4},"speaker":"A","text":"健康のために、あなたは、何かを行っていますか？"},{"id":{"d":218,"n":2,"t":4},"speaker":"B","text":"朝起きたら、ストレッチ体操をしています。"},{"id":{"d":218,"n":3,"t":4},"speaker":"A","text":"いいですね。長く続けているのですか？"},{"id":{"d":218,"n":4,"t":4},"speaker":"B","text":"5年以上続けていると思います。あなたは何かしていることはありますか？"},{"id":{"d":218,"n":5,"t":4},"speaker":"A","text":"私が健康のために行っていることは、早寝早起きくらいです。"},{"id":{"d":218,"n":6,"t":4},"speaker":"B","text":"それも体にいいですね。"},{"id":{"d":218,"n":7,"t":4},"speaker":"A","text":"しかし、なかなか運動が習慣になりません。"},{"id":{"d":218,"n":8,"t":4},"speaker":"B","text":"ストレッチ体操は短い時間で済むので、お勧めですよ。"},{"id":{"d":218,"n":9,"t":4},"speaker":"A","text":"そうですね。試してみます。"}];
INSERT INTO sentence [{"id":{"d":219,"n":1,"t":4},"speaker":"A","text":"学校で、身体測定がありました。"},{"id":{"d":219,"n":2,"t":4},"speaker":"B","text":"身長は伸びていましたか？"},{"id":{"d":219,"n":3,"t":4},"speaker":"A","text":"はい。3センチメートル伸びました。"},{"id":{"d":219,"n":4,"t":4},"speaker":"B","text":"成長していますね。"},{"id":{"d":219,"n":5,"t":4},"speaker":"A","text":"さらに10センチメートルくらい伸びてほしいです。"},{"id":{"d":219,"n":6,"t":4},"speaker":"B","text":"健康的な生活をして、しっかり栄養を取れば、身長が伸びるかもしれませんよ。"},{"id":{"d":219,"n":7,"t":4},"speaker":"A","text":"今より10センチメートル伸びたら、お母さんの身長を追い越しますね。"},{"id":{"d":219,"n":8,"t":4},"speaker":"B","text":"そうなることを、期待しています。"}];
INSERT INTO sentence [{"id":{"d":220,"n":1,"t":4},"speaker":"A","text":"駅前で献血を呼びかけていました。"},{"id":{"d":220,"n":2,"t":4},"speaker":"B","text":"あなたは、献血をしたことはありますか？"},{"id":{"d":220,"n":3,"t":4},"speaker":"A","text":"一度もありませんが、献血をしたい気持ちはあります。"},{"id":{"d":220,"n":4,"t":4},"speaker":"B","text":"世の中の役に立つので、献血をしてみるといいですよ。"},{"id":{"d":220,"n":5,"t":4},"speaker":"A","text":"献血した血液は、輸血などに使われるのですよね。"},{"id":{"d":220,"n":6,"t":4},"speaker":"B","text":"そうです。お父さんが手術の際に輸血が必要だったので、献血のありがたさが分かりました。"},{"id":{"d":220,"n":7,"t":4},"speaker":"A","text":"それから、お母さんも献血するようになったのですか？"},{"id":{"d":220,"n":8,"t":4},"speaker":"B","text":"はい、1年に1回は献血しています。"},{"id":{"d":220,"n":9,"t":4},"speaker":"A","text":"私も、次の機会に献血してみます。"}];
INSERT INTO sentence [{"id":{"d":221,"n":1,"t":4},"speaker":"A","text":"昼食にしましょう。"},{"id":{"d":221,"n":2,"t":4},"speaker":"B","text":"私は、結構です。"},{"id":{"d":221,"n":3,"t":4},"speaker":"A","text":"体調が悪いのですか？"},{"id":{"d":221,"n":4,"t":4},"speaker":"B","text":"この暑さで、夏バテしたようです。"},{"id":{"d":221,"n":5,"t":4},"speaker":"A","text":"毎日、本当に暑いですよね。"},{"id":{"d":221,"n":6,"t":4},"speaker":"B","text":"おなかがすいているはずなのに、食べたい気持ちになりません。"},{"id":{"d":221,"n":7,"t":4},"speaker":"A","text":"何も食べないと、体に良くないのですよ。"},{"id":{"d":221,"n":8,"t":4},"speaker":"B","text":"麺類なら食べられるかもしれません。"},{"id":{"d":221,"n":9,"t":4},"speaker":"A","text":"それでは、そうめんを用意しますね。"}];
INSERT INTO sentence [{"id":{"d":222,"n":1,"t":4},"speaker":"A","text":"随分着込んでいますね。"},{"id":{"d":222,"n":2,"t":4},"speaker":"B","text":"私は、冷え性なのです。"},{"id":{"d":222,"n":3,"t":4},"speaker":"A","text":"冷え性は治せないものなのでしょうか？"},{"id":{"d":222,"n":4,"t":4},"speaker":"B","text":"病院で相談したら、漢方薬を薦められて、飲んでいます。"},{"id":{"d":222,"n":5,"t":4},"speaker":"A","text":"効果は、ありますか？"},{"id":{"d":222,"n":6,"t":4},"speaker":"B","text":"まだ飲み始めたばかりなので分かりません。"},{"id":{"d":222,"n":7,"t":4},"speaker":"A","text":"漢方薬は、効き目が表れるまで時間がかかるといわれていますよね。"},{"id":{"d":222,"n":8,"t":4},"speaker":"B","text":"医師にも、「長期的に飲み続けてください」と言われました。"},{"id":{"d":222,"n":9,"t":4},"speaker":"A","text":"効果が出て、症状が改善されるといいですね。"}];
INSERT INTO sentence [{"id":{"d":223,"n":1,"t":4},"speaker":"A","text":"そろそろ寝ましょう。"},{"id":{"d":223,"n":2,"t":4},"speaker":"B","text":"先に寝てください。"},{"id":{"d":223,"n":3,"t":4},"speaker":"A","text":"まだ、あなたは寝ないのですか？"},{"id":{"d":223,"n":4,"t":4},"speaker":"B","text":"宿題を終わらせてから寝ます。"},{"id":{"d":223,"n":5,"t":4},"speaker":"A","text":"夜更かしは、体に良くありませんよ。"},{"id":{"d":223,"n":6,"t":4},"speaker":"B","text":"でも、宿題をやらないわけにもいきません。"},{"id":{"d":223,"n":7,"t":4},"speaker":"A","text":"明日の朝は、早起きして宿題をしてはどうですか？"},{"id":{"d":223,"n":8,"t":4},"speaker":"B","text":"そうですね。目覚まし時計をセットして、早起きを頑張ります。"}];
INSERT INTO sentence [{"id":{"d":224,"n":1,"t":4},"speaker":"A","text":"口内炎ができてしまいました。"},{"id":{"d":224,"n":2,"t":4},"speaker":"B","text":"痛いですか？"},{"id":{"d":224,"n":3,"t":4},"speaker":"A","text":"痛みもありますし、食べ物が染みることもあります。"},{"id":{"d":224,"n":4,"t":4},"speaker":"B","text":"栄養素の不足が口内炎の原因かもしれませんね。"},{"id":{"d":224,"n":5,"t":4},"speaker":"A","text":"最近、外食ばかりしていました。"},{"id":{"d":224,"n":6,"t":4},"speaker":"B","text":"頑張って、自炊してみてはどうですか？"},{"id":{"d":224,"n":7,"t":4},"speaker":"A","text":"料理は、ほとんどしたことがありません。"},{"id":{"d":224,"n":8,"t":4},"speaker":"B","text":"手軽に作れる料理も、たくさんありますよ。"},{"id":{"d":224,"n":9,"t":4},"speaker":"A","text":"そうですね。簡単なレシピを探してみます。"}];
INSERT INTO sentence [{"id":{"d":225,"n":1,"t":4},"speaker":"A","text":"クラスメイトが松葉づえで登校してきました。"},{"id":{"d":225,"n":2,"t":4},"speaker":"B","text":"足をけがしたのでしょうか？"},{"id":{"d":225,"n":3,"t":4},"speaker":"A","text":"自転車で転んで骨折してしまったそうです。"},{"id":{"d":225,"n":4,"t":4},"speaker":"B","text":"お気の毒ですね。"},{"id":{"d":225,"n":5,"t":4},"speaker":"A","text":"1カ月間くらいは、松葉づえが必要らしいです。"},{"id":{"d":225,"n":6,"t":4},"speaker":"B","text":"それでは、いろいろと不便でしょうね。"},{"id":{"d":225,"n":7,"t":4},"speaker":"A","text":"先生から、「積極的に協力してください」と話がありました。"},{"id":{"d":225,"n":8,"t":4},"speaker":"B","text":"みんなで助け合えると良いですね。"}];
INSERT INTO sentence [{"id":{"d":226,"n":1,"t":4},"speaker":"A","text":"図書館の近くに、新しくジムができましたね。"},{"id":{"d":226,"n":2,"t":4},"speaker":"B","text":"先ほど、その前を通りましたが、大きくて立派なジムでした。"},{"id":{"d":226,"n":3,"t":4},"speaker":"A","text":"無料で体験ができるようですよ。"},{"id":{"d":226,"n":4,"t":4},"speaker":"B","text":"今まで、ジムに行ったことがないので興味があります。"},{"id":{"d":226,"n":5,"t":4},"speaker":"A","text":"一緒に体験をしてみましょうか？"},{"id":{"d":226,"n":6,"t":4},"speaker":"B","text":"ぜひ、そうしましょう。"},{"id":{"d":226,"n":7,"t":4},"speaker":"A","text":"私が体験を申し込みますから、都合の良い日を教えてください。"},{"id":{"d":226,"n":8,"t":4},"speaker":"B","text":"スケジュールを確認して、あなたに連絡します。"}];
INSERT INTO sentence [{"id":{"d":227,"n":1,"t":4},"speaker":"A","text":"あなたは、薬を飲んでいるのですか？"},{"id":{"d":227,"n":2,"t":4},"speaker":"B","text":"これは、薬ではなくサプリメントです。"},{"id":{"d":227,"n":3,"t":4},"speaker":"A","text":"何種類もサプリメントを飲むのですね。"},{"id":{"d":227,"n":4,"t":4},"speaker":"B","text":"それぞれ成分や効果が違うのです。"},{"id":{"d":227,"n":5,"t":4},"speaker":"A","text":"そういば、ドラッグストアでたくさんの種類のサプリメントが売られていますね。"},{"id":{"d":227,"n":6,"t":4},"speaker":"B","text":"はい。お店で見ていると、あれもこれも欲しくなってしまいます。"},{"id":{"d":227,"n":7,"t":4},"speaker":"A","text":"そうですね、迷ってしまいそうです。"},{"id":{"d":227,"n":8,"t":4},"speaker":"B","text":"じっくり選んで、自分に不足している成分が入っているものを買うようにしています。"},{"id":{"d":227,"n":9,"t":4},"speaker":"A","text":"手軽に栄養素を補えて、良いですね。"}];
INSERT INTO sentence [{"id":{"d":228,"n":1,"t":4},"speaker":"A","text":"見舞いに来てくれてありがとうございます。"},{"id":{"d":228,"n":2,"t":4},"speaker":"B","text":"具合は、どうですか？"},{"id":{"d":228,"n":3,"t":4},"speaker":"A","text":"おかげさまで、順調に回復しています。"},{"id":{"d":228,"n":4,"t":4},"speaker":"B","text":"顔色も良いですね。安心しました。"},{"id":{"d":228,"n":5,"t":4},"speaker":"A","text":"このままなら、予定通りの日に退院できるそうです。"},{"id":{"d":228,"n":6,"t":4},"speaker":"B","text":"退院の予定は、いつですか？"},{"id":{"d":228,"n":7,"t":4},"speaker":"A","text":"10日後です。"},{"id":{"d":228,"n":8,"t":4},"speaker":"B","text":"あなたは退院したら、自宅で静養するのですか？"},{"id":{"d":228,"n":9,"t":4},"speaker":"A","text":"はい。仕事に復帰するまでには、しばらく時間がかかりそうです。"},{"id":{"d":228,"n":10,"t":4},"speaker":"B","text":"焦らずにゆっくり過ごしてください。"}];
INSERT INTO sentence [{"id":{"d":229,"n":1,"t":4},"speaker":"A","text":"将来は、どのような職業につきたいですか？"},{"id":{"d":229,"n":2,"t":4},"speaker":"B","text":"私は、医療関係の仕事をしたいです。"},{"id":{"d":229,"n":3,"t":4},"speaker":"A","text":"医療関係というと、さまざまな職業がありますね。"},{"id":{"d":229,"n":4,"t":4},"speaker":"B","text":"私は、薬の開発に興味があります。"},{"id":{"d":229,"n":5,"t":4},"speaker":"A","text":"とても需要のある分野ですね。"},{"id":{"d":229,"n":6,"t":4},"speaker":"B","text":"大学に進学して、薬に関連した勉強をするつもりです。"},{"id":{"d":229,"n":7,"t":4},"speaker":"A","text":"大学卒業後は、製薬会社に就職するのですか？"},{"id":{"d":229,"n":8,"t":4},"speaker":"B","text":"製薬会社に就職するか、大学院で研究を続けるか、まだ決めていません。"},{"id":{"d":229,"n":9,"t":4},"speaker":"A","text":"いずれにしても、素晴らしい進路だと思います。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":230,"n":1,"t":4},"speaker":"A","text":"あなたは、何を調べているのですか？"},{"id":{"d":230,"n":2,"t":4},"speaker":"B","text":"息子のために、誕生日のケーキを手作りたいので、作り方を調べています。"},{"id":{"d":230,"n":3,"t":4},"speaker":"A","text":"手作りでケーキを作ってあげるとは、偉いですね。"},{"id":{"d":230,"n":4,"t":4},"speaker":"B","text":"息子は卵アレルギーなので、市販のケーキは食べられないのです。"},{"id":{"d":230,"n":5,"t":4},"speaker":"A","text":"それで、卵を使わないケーキの作り方を調べていたのですね。"},{"id":{"d":230,"n":6,"t":4},"speaker":"B","text":"はい。なるべく、息子においしいケーキを作ってあげたいです。"},{"id":{"d":230,"n":7,"t":4},"speaker":"A","text":"インターネットなら情報が多いので、卵を使わないケーキのレシピが見つかりそうですね。"},{"id":{"d":230,"n":8,"t":4},"speaker":"B","text":"調べてみると、多くのレシピがあって助かります。"}];
INSERT INTO sentence [{"id":{"d":231,"n":1,"t":4},"speaker":"A","text":"冬は、空気が乾いていますね。"},{"id":{"d":231,"n":2,"t":4},"speaker":"B","text":"本当ですね。肌も乾燥してしまいます。"},{"id":{"d":231,"n":3,"t":4},"speaker":"A","text":"特に顔の乾燥が気になります。"},{"id":{"d":231,"n":4,"t":4},"speaker":"B","text":"洗顔後のお手入れには、何を使っていますか？"},{"id":{"d":231,"n":5,"t":4},"speaker":"A","text":"化粧水と乳液をつけて、その後保湿クリームを塗っています。"},{"id":{"d":231,"n":6,"t":4},"speaker":"B","text":"きちんとお手入れしていますね。"},{"id":{"d":231,"n":7,"t":4},"speaker":"A","text":"それでも肌が乾燥してしまいます。"},{"id":{"d":231,"n":8,"t":4},"speaker":"B","text":"パックを、使ってみてはどうですか？"},{"id":{"d":231,"n":9,"t":4},"speaker":"A","text":"そうですね、保湿効果のあるパックを試してみます。"}];
INSERT INTO sentence [{"id":{"d":232,"n":1,"t":4},"speaker":"A","text":"今年の遠足は、森林公園に行くそうですね。"},{"id":{"d":232,"n":2,"t":4},"speaker":"B","text":"はい。でも気が重いです。"},{"id":{"d":232,"n":3,"t":4},"speaker":"A","text":"どうしたのですか？"},{"id":{"d":232,"n":4,"t":4},"speaker":"B","text":"バスで行くので、乗り物酔いしてしまうと思います。"},{"id":{"d":232,"n":5,"t":4},"speaker":"A","text":"森林公園は遠いから、長い時間バスに乗ることになりますね。"},{"id":{"d":232,"n":6,"t":4},"speaker":"B","text":"片道で、バス移動で1時間以上かかるそうです。"},{"id":{"d":232,"n":7,"t":4},"speaker":"A","text":"バスに乗る前に、薬を飲みましょう。"},{"id":{"d":232,"n":8,"t":4},"speaker":"B","text":"はい。忘れずに薬を飲むようにします。"},{"id":{"d":232,"n":9,"t":4},"speaker":"A","text":"遠足の帰りに飲む薬も用意した方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":233,"n":1,"t":4},"speaker":"A","text":"先日の検査の結果が出ました。"},{"id":{"d":233,"n":2,"t":4},"speaker":"B","text":"検査の結果は、どうでしたか？"},{"id":{"d":233,"n":3,"t":4},"speaker":"A","text":"胃の状態があまり良くないようです。"},{"id":{"d":233,"n":4,"t":4},"speaker":"B","text":"何か、病気にかかっているのでしょうか？"},{"id":{"d":233,"n":5,"t":4},"speaker":"A","text":"詳しく調べるために、精密検査が必要です。"},{"id":{"d":233,"n":6,"t":4},"speaker":"B","text":"この病院で、精密検査を受けられますか？"},{"id":{"d":233,"n":7,"t":4},"speaker":"A","text":"検査機器が整った病院で検査を受けることが望ましいので、紹介状を書きますね。"},{"id":{"d":233,"n":8,"t":4},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":234,"n":1,"t":4},"speaker":"A","text":"病院で「血圧が高めだ」と言われてしまいました。"},{"id":{"d":234,"n":2,"t":4},"speaker":"B","text":"何か、気を付けるべきことを言われましたか？"},{"id":{"d":234,"n":3,"t":4},"speaker":"A","text":"「塩分を控えるように」と医者から言われました。"},{"id":{"d":234,"n":4,"t":4},"speaker":"B","text":"そういえば、あなたは、味の濃い料理を作ることが多いですね。"},{"id":{"d":234,"n":5,"t":4},"speaker":"A","text":"これからは、薄味を心掛けます。"},{"id":{"d":234,"n":6,"t":4},"speaker":"B","text":"他に、言われたことはありますか？"},{"id":{"d":234,"n":7,"t":4},"speaker":"A","text":"「運動を習慣にするように」と医者から言われました。"},{"id":{"d":234,"n":8,"t":4},"speaker":"B","text":"あなたは車で出かけることが多いので、普段あまり歩きませんよね。"},{"id":{"d":234,"n":9,"t":4},"speaker":"A","text":"はい。ですので、徒歩で出かける機会を増やすようにします。"}];
INSERT INTO sentence [{"id":{"d":235,"n":1,"t":4},"speaker":"A","text":"ご退院おめでとうございます。"},{"id":{"d":235,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。ご心配をおかけしました。"},{"id":{"d":235,"n":3,"t":4},"speaker":"A","text":"入院中は大変でしたか？"},{"id":{"d":235,"n":4,"t":4},"speaker":"B","text":"手術の後は、普通の食事ができるようになるまで、つらかったです。"},{"id":{"d":235,"n":5,"t":4},"speaker":"A","text":"術後の食事は、しばらくおかゆだったのですか？"},{"id":{"d":235,"n":6,"t":4},"speaker":"B","text":"手術直後は、スープのようなものだけでした。"},{"id":{"d":235,"n":7,"t":4},"speaker":"A","text":"栄養は、足りるのでしょうか？"},{"id":{"d":235,"n":8,"t":4},"speaker":"B","text":"点滴で栄養を補っていたので大丈夫でしたが、普通の食事が恋しかったです。"},{"id":{"d":235,"n":9,"t":4},"speaker":"A","text":"今は、何でも食べて良いのですか？"},{"id":{"d":235,"n":10,"t":4},"speaker":"B","text":"「塩分と油分が多い物は控えるように」と医者から言われています。"}];
INSERT INTO sentence [{"id":{"d":236,"n":1,"t":4},"speaker":"A","text":"今日の試合に、和田選手が出場していませんでしたね。"},{"id":{"d":236,"n":2,"t":4},"speaker":"B","text":"昨日の試合で、けがをしてしまったそうです。"},{"id":{"d":236,"n":3,"t":4},"speaker":"A","text":"どうしてけがをしたのか、知っていますか？"},{"id":{"d":236,"n":4,"t":4},"speaker":"B","text":"スポーツニュースで、肩にデッドボールを受けたと報じられていました。"},{"id":{"d":236,"n":5,"t":4},"speaker":"A","text":"けがの状態が心配です。"},{"id":{"d":236,"n":6,"t":4},"speaker":"B","text":"骨に異常はないらしいので、ひどいけがではないと思います。"},{"id":{"d":236,"n":7,"t":4},"speaker":"A","text":"今年はとても活躍しているので、早く試合に復帰してほしいです。"},{"id":{"d":236,"n":8,"t":4},"speaker":"B","text":"私も、彼が試合に復帰することが待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":237,"n":1,"t":4},"speaker":"A","text":"私は、目の疲れを感じることが多くなりました。"},{"id":{"d":237,"n":2,"t":4},"speaker":"B","text":"私も同じです。スマートフォンの使い過ぎかもしれないと感じています。"},{"id":{"d":237,"n":3,"t":4},"speaker":"A","text":"確かに、スマートフォンを見ている時間が多いかもしれません。"},{"id":{"d":237,"n":4,"t":4},"speaker":"B","text":"手元にスマートフォンがあると、つい見てしまいますよね。"},{"id":{"d":237,"n":5,"t":4},"speaker":"A","text":"そうですね。スマートフォンを見ることが、癖になっている気がします。"},{"id":{"d":237,"n":6,"t":4},"speaker":"B","text":"ずっとスマートフォンの画面を見ていると、目が疲れるのも当然ですね。"},{"id":{"d":237,"n":7,"t":4},"speaker":"A","text":"意識的に、スマートフォンを使わない時間を作った方が良さそうです。"},{"id":{"d":237,"n":8,"t":4},"speaker":"B","text":"早速、今日から始めましょう。"}];
INSERT INTO sentence [{"id":{"d":238,"n":1,"t":4},"speaker":"A","text":"いつもと、お茶の味が違いますね。"},{"id":{"d":238,"n":2,"t":4},"speaker":"B","text":"お茶の葉を変えたのです。"},{"id":{"d":238,"n":3,"t":4},"speaker":"A","text":"どうして変えたのですか？"},{"id":{"d":238,"n":4,"t":4},"speaker":"B","text":"テレビ番組で、このお茶が紹介されていて、試したいと思ったのです。"},{"id":{"d":238,"n":5,"t":4},"speaker":"A","text":"飲むと、何か良い効果があるのですか？"},{"id":{"d":238,"n":6,"t":4},"speaker":"B","text":"糖分や脂質の体への吸収を抑えてくれるらしいです。"},{"id":{"d":238,"n":7,"t":4},"speaker":"A","text":"それは、うれしい効果ですね。"},{"id":{"d":238,"n":8,"t":4},"speaker":"B","text":"効果が感じられるかどうか、しばらく飲み続けてみましょう。"}];
INSERT INTO sentence [{"id":{"d":239,"n":1,"t":4},"speaker":"A","text":"明日は健康診断に行くので、午後から出社します。"},{"id":{"d":239,"n":2,"t":4},"speaker":"B","text":"分かりました。私も先週行きましたけど、結構大変ですよね。"},{"id":{"d":239,"n":3,"t":4},"speaker":"A","text":"当日は朝食が食べられないし、うっかり採尿を忘れそうになったりします。"},{"id":{"d":239,"n":4,"t":4},"speaker":"B","text":"私は、問診がとても憂鬱です。"},{"id":{"d":239,"n":5,"t":4},"speaker":"A","text":"どうしてですか？何か気がかりなことがあるのですか？"},{"id":{"d":239,"n":6,"t":4},"speaker":"B","text":"いつも運動しているかどうかを聞かれて「していない」と答えると、それではいけないと注意されてしまうのです。"},{"id":{"d":239,"n":7,"t":4},"speaker":"A","text":"ああ、なるほど。運動の習慣をつけるのは、なかなか難しいですよね。"},{"id":{"d":239,"n":8,"t":4},"speaker":"B","text":"年々体重が増えているので、運動不足はいけないと分かってはいるのですが。"},{"id":{"d":239,"n":9,"t":4},"speaker":"A","text":"私も運動不足でしたが、しばらく前から会社の最寄り駅ではなく、2、3駅前で降りて歩くようにしています。"},{"id":{"d":239,"n":10,"t":4},"speaker":"B","text":"偉いですね。見習いたいです。"},{"id":{"d":239,"n":11,"t":4},"speaker":"A","text":"一度行うと、案外すぐに慣れますよ。特に準備も必要なく、手軽にできるのでお勧めです。"}];
INSERT INTO sentence [{"id":{"d":240,"n":1,"t":4},"speaker":"A","text":"少し頭が痛いので、薬を飲んできます。"},{"id":{"d":240,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":240,"n":3,"t":4},"speaker":"A","text":"私は偏頭痛持ちで、よくあることなので大丈夫です。"},{"id":{"d":240,"n":4,"t":4},"speaker":"B","text":"私の姉も偏頭痛が頻繁にあるのですが、つらいようですね。"},{"id":{"d":240,"n":5,"t":4},"speaker":"A","text":"薬が効いてくれば楽になるのですが、できればあまり薬に頼りたくないと思っています。お姉さまは、通院されているのですか？"},{"id":{"d":240,"n":6,"t":4},"speaker":"B","text":"はい、姉はだいぶ前から病院に通っていますが、あまり改善はしていません。ただ、処方してもらっている薬が合っているようです。"},{"id":{"d":240,"n":7,"t":4},"speaker":"A","text":"合う薬と合わない薬がありますよね。"},{"id":{"d":240,"n":8,"t":4},"speaker":"B","text":"頭痛が起きるきっかけは、何かあるのですか？"},{"id":{"d":240,"n":9,"t":4},"speaker":"A","text":"私の場合は、気圧が関係していると感じています。低気圧の日は、頭痛になりやすいです。"},{"id":{"d":240,"n":10,"t":4},"speaker":"B","text":"気圧も関係があるのですね。姉は「疲れてくると、頭が痛くなる」とよく言っています。"},{"id":{"d":240,"n":11,"t":4},"speaker":"A","text":"そうですね、疲労も関係があるでしょうね。"},{"id":{"d":240,"n":12,"t":4},"speaker":"B","text":"複数の要因がありますね。"}];
INSERT INTO sentence [{"id":{"d":241,"n":1,"t":4},"speaker":"A","text":"あなたは、普段から運動をしていますか？"},{"id":{"d":241,"n":2,"t":4},"speaker":"B","text":"私は、スポーツクラブに通っています。"},{"id":{"d":241,"n":3,"t":4},"speaker":"A","text":"スポーツクラブでは、どのような運動をしているのですか？"},{"id":{"d":241,"n":4,"t":4},"speaker":"B","text":"水泳がメインです。"},{"id":{"d":241,"n":5,"t":4},"speaker":"A","text":"どのくらい泳いでいるのですか？"},{"id":{"d":241,"n":6,"t":4},"speaker":"B","text":"週2日で3キロメートルから5キロメートルほどです。でも、最近は仕事が忙しくて、ここ2週間は全く泳げていません。"},{"id":{"d":241,"n":7,"t":4},"speaker":"A","text":"ウオーキングやジョギングは、水泳よりも簡単にできそうですが、それはしないのですか？"},{"id":{"d":241,"n":8,"t":4},"speaker":"B","text":"足腰への負担を考えて、水泳にしていたのですが、結局続けないと駄目ですね。"},{"id":{"d":241,"n":9,"t":4},"speaker":"A","text":"なるほど。水泳は心肺機能にも良いそうですし、続けると、とても良質な運動になりそうです。"},{"id":{"d":241,"n":10,"t":4},"speaker":"B","text":"そうですね。どのようにかして暇を見つけて、運動を続けたいところです。"}];
INSERT INTO sentence [{"id":{"d":242,"n":1,"t":4},"speaker":"A","text":"私は、健康のために運動をしたいと思っているのですが、なかなか長続きがしなくて困っています。"},{"id":{"d":242,"n":2,"t":4},"speaker":"B","text":"今までどのようなことをしたのですか？"},{"id":{"d":242,"n":3,"t":4},"speaker":"A","text":"DVDを購入し、エクササイズに挑戦しましたが、あまりにもハードだったので途中で断念しました。他にも通信販売で健康マシンを購入しましたが、組み立てと装着が面倒で、すぐに物置にしまってしまいました。"},{"id":{"d":242,"n":4,"t":4},"speaker":"B","text":"いろいろ試しているのですね。もしかしたら、もっと手軽にできる簡単な運動から始めてみるのが良いかもしれません。"},{"id":{"d":242,"n":5,"t":4},"speaker":"A","text":"なるほど。それならできるかもしれません。"},{"id":{"d":242,"n":6,"t":4},"speaker":"B","text":"天気や場所に左右されない運動が良さそうです。身一つで、家でもできる運動はいかがでしょうか？"},{"id":{"d":242,"n":7,"t":4},"speaker":"A","text":"ラジオ体操や、腕立て伏せ、腹筋運動などでしょうか？"},{"id":{"d":242,"n":8,"t":4},"speaker":"B","text":"いいですね。それができるようになったら、エアロバイクやウオーキングマシンに挑戦しても良いかもしれませんね。"},{"id":{"d":242,"n":9,"t":4},"speaker":"A","text":"これなら少しの空き時間にでもできそうですし、毎日続けられそうです。"},{"id":{"d":242,"n":10,"t":4},"speaker":"B","text":"応援しています。一気にたくさんやろうとせず、まずはできる回数から始めてみることも大事ですよ。"}];
INSERT INTO sentence [{"id":{"d":243,"n":1,"t":4},"speaker":"A","text":"どうやら私は、四十肩らしいです。腕を上げると、肩に痛みが走ります。"},{"id":{"d":243,"n":2,"t":4},"speaker":"B","text":"部長、私は30歳で急性腰痛です。"},{"id":{"d":243,"n":3,"t":4},"speaker":"A","text":"君は、ラグビーをしていましたよね。激しいスポーツは、健康に良くないとも聞きます。"},{"id":{"d":243,"n":4,"t":4},"speaker":"B","text":"それは、私も聞きます。しかし、部長も社会人野球でレギュラーだったと思います。"},{"id":{"d":243,"n":5,"t":4},"speaker":"A","text":"私の場合は四十肩というくらいだから、スポーツではなくて年齢からきたものだと思います。"},{"id":{"d":243,"n":6,"t":4},"speaker":"B","text":"確かにラグビーは、体を痛めやすいスポーツかもしれません。"},{"id":{"d":243,"n":7,"t":4},"speaker":"A","text":"君は、どういうときに急性腰痛になったのですか？"},{"id":{"d":243,"n":8,"t":4},"speaker":"B","text":"トイレで便座に座ろうとしたときです。しばらく座ったまま身動きができませんでした。"},{"id":{"d":243,"n":9,"t":4},"speaker":"A","text":"今度、会社近くの整体に行こうと思っているのですが、良かったら君も行きませんか？"},{"id":{"d":243,"n":10,"t":4},"speaker":"B","text":"あそこの整体ですね。実は、私はもう常連です。"}];
INSERT INTO sentence [{"id":{"d":244,"n":1,"t":4},"speaker":"A","text":"歯科医院を探していますが、この沿線にあるいいところを知っていますか？"},{"id":{"d":244,"n":2,"t":4},"speaker":"B","text":"心当たりはありますが、あなたは、どういった理由で歯科医院を探しているのですか？"},{"id":{"d":244,"n":3,"t":4},"speaker":"A","text":"数日前から親知らずの痛みが気になっています。抜いた方がいいのか相談したいのです。"},{"id":{"d":244,"n":4,"t":4},"speaker":"B","text":"親知らずの炎症も考えられますから、口腔外科が併設されている歯科医院がいいと思います。"},{"id":{"d":244,"n":5,"t":4},"speaker":"A","text":"この沿線にありますか？"},{"id":{"d":244,"n":6,"t":4},"speaker":"B","text":"次の乗換駅近くに、口腔外科のある病院があります。まだ、すいているはずですから、まずは連絡してみましょうか？"},{"id":{"d":244,"n":7,"t":4},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":245,"n":1,"t":4},"speaker":"A","text":"わが社のバドミントン部には、専用のコートはありませんし、体を鍛えるためのジムもありません。"},{"id":{"d":245,"n":2,"t":4},"speaker":"B","text":"練習は、どこでするのですか？"},{"id":{"d":245,"n":3,"t":4},"speaker":"A","text":"自治体の所有するコートを借りて、練習しています。"},{"id":{"d":245,"n":4,"t":4},"speaker":"B","text":"勤務時間が終わってから、移動するのですか？"},{"id":{"d":245,"n":5,"t":4},"speaker":"A","text":"はい。毎回同じ場所になるわけではないので、マネージャーに確認しておかなければなりません。"},{"id":{"d":245,"n":6,"t":4},"speaker":"B","text":"私は高校時代バドミントン部に所属していましたが、強くはありませんでした。この会社もバドミントン部があると知らずに入社したほどですが、そのような私でも入部できますか？"},{"id":{"d":245,"n":7,"t":4},"speaker":"A","text":"歓迎しますよ。来年は2部リーグ復帰を目標にしています。一緒に頑張りましょう。"},{"id":{"d":245,"n":8,"t":4},"speaker":"B","text":"選手は、何人いるのですか？"},{"id":{"d":245,"n":9,"t":4},"speaker":"A","text":"部長と私と君、さらにダブルスの2人組は、君の2年先輩です。"},{"id":{"d":245,"n":10,"t":4},"speaker":"B","text":"監督とマネージャーは、今いらっしゃいますか？あいさつだけでもしたいと思います。"},{"id":{"d":245,"n":11,"t":4},"speaker":"A","text":"実は、監督は私で、マネージャーも私です。"}];
INSERT INTO sentence [{"id":{"d":246,"n":1,"t":4},"speaker":"A","text":"この季節になると、花粉症の症状が出て、私は本当につらいです。"},{"id":{"d":246,"n":2,"t":4},"speaker":"B","text":"今年は、特に花粉の飛散がひどいと聞きます。"},{"id":{"d":246,"n":3,"t":4},"speaker":"A","text":"テレビ番組の天気予報内の花粉予想を見るたびに、憂鬱になってきます。"},{"id":{"d":246,"n":4,"t":4},"speaker":"B","text":"薬を飲む以外の対策を、何かしていますか？"},{"id":{"d":246,"n":5,"t":4},"speaker":"A","text":"乾燥機付きの洗濯機を買って、洗濯物の外干しをやめることにしました。"},{"id":{"d":246,"n":6,"t":4},"speaker":"B","text":"そういえば、洗濯物を外へ干していませんね。"},{"id":{"d":246,"n":7,"t":4},"speaker":"A","text":"空気清浄機を手に入れてからは、窓もめったに開けなくなりました。"},{"id":{"d":246,"n":8,"t":4},"speaker":"B","text":"完全な、花粉対策だと思います。"},{"id":{"d":246,"n":9,"t":4},"speaker":"A","text":"ところが、子どもたちが花粉をいっぱい浴びて帰ってくるのです。"},{"id":{"d":246,"n":10,"t":4},"speaker":"B","text":"全ての苦労が水泡に帰す、ということですね。"}];
INSERT INTO sentence [{"id":{"d":247,"n":1,"t":4},"speaker":"A","text":"献血カードを拝見すると、久しぶりの献血のようですね。"},{"id":{"d":247,"n":2,"t":4},"speaker":"B","text":"はい。大学生の時以来だから、5年くらいたっています。"},{"id":{"d":247,"n":3,"t":4},"speaker":"A","text":"血圧は高めですが、いつもそうですか？"},{"id":{"d":247,"n":4,"t":4},"speaker":"B","text":"はい。毎回測り直されて、基準値内になるくらいの高さです。通院もしていませんし、薬を服用しているわけでもありません。"},{"id":{"d":247,"n":5,"t":4},"speaker":"A","text":"あなたが1日に摂取する、お酒やタバコの量はどれくらいですか？"},{"id":{"d":247,"n":6,"t":4},"speaker":"B","text":"タバコはやめて10年以上たちます。お酒は晩酌で、缶ビール2本を飲んでいます。"},{"id":{"d":247,"n":7,"t":4},"speaker":"A","text":"それでは全血献血400ミリリットルということでよろしいですか？"},{"id":{"d":247,"n":8,"t":4},"speaker":"B","text":"はい。ところで献血すると高血圧が改善しますか？"},{"id":{"d":247,"n":9,"t":4},"speaker":"A","text":"いいえ、しません。血圧が高すぎると献血をお断りすることもあります。"}];
INSERT INTO sentence [{"id":{"d":248,"n":1,"t":4},"speaker":"A","text":"家でも、ストレッチ体操やその他の体操をした方がいいでしょうか？"},{"id":{"d":248,"n":2,"t":4},"speaker":"B","text":"それは注意した方がいいです。間違ったやり方をすると筋を違えたり痛めたりします。"},{"id":{"d":248,"n":3,"t":4},"speaker":"A","text":"子どもの時のように、体操は簡単にできるものと思っていました。ラジオ体操のようにはできないのでしょうか？"},{"id":{"d":248,"n":4,"t":4},"speaker":"B","text":"子どもは関節の可動域が広くて、筋肉も柔らかくてけがをしにくい体質です。"},{"id":{"d":248,"n":5,"t":4},"speaker":"A","text":"確かにそうです。息子が子どもの時、階段を転げ落ちたのにかすり傷一つありませんでした。"},{"id":{"d":248,"n":6,"t":4},"speaker":"B","text":"小さい子だと体重が軽いので、けがをしにくいということもあります。"},{"id":{"d":248,"n":7,"t":4},"speaker":"A","text":"もう、私は運動しない方がいい年齢なのでしょうか？"},{"id":{"d":248,"n":8,"t":4},"speaker":"B","text":"安全で、簡単なストレッチ体操をお教えしますので、少しずつ始められてはいかがですか？"},{"id":{"d":248,"n":9,"t":4},"speaker":"A","text":"それは助かります。ぜひとも、お願いします。"}];
INSERT INTO sentence [{"id":{"d":249,"n":1,"t":4},"speaker":"A","text":"あなた、娘にインフルエンザの予防接種を打った方がいいでしょうか？"},{"id":{"d":249,"n":2,"t":4},"speaker":"B","text":"考えていませんでした。学校では何と言われていますか？"},{"id":{"d":249,"n":3,"t":4},"speaker":"A","text":"「ご家庭にお任せします」とのことです。"},{"id":{"d":249,"n":4,"t":4},"speaker":"B","text":"お金がかかるから、「インフルエンザの予防接種を打ってくれ」と学校側からは言えないのでしょう。"},{"id":{"d":249,"n":5,"t":4},"speaker":"A","text":"あなたと私は打っていないのに、子どもにだけワクチンを打つのも変な気がします。"},{"id":{"d":249,"n":6,"t":4},"speaker":"B","text":"ついでに、家族みんなでインフルエンザのワクチンを打つのはどうでしょうか？"},{"id":{"d":249,"n":7,"t":4},"speaker":"A","text":"悩ましいですね。あなたも私も、今までインフルエンザにかかったことはありませんし。"},{"id":{"d":249,"n":8,"t":4},"speaker":"B","text":"実は会社から、「なるべく予防接種を受けるように」と言われています。そういえば、健康保険組合から補助金も出るそうですよ。"},{"id":{"d":249,"n":9,"t":4},"speaker":"A","text":"先にそれを言ってください。うちの子にも助成金が出るか調べてみます。"}];
INSERT INTO sentence [{"id":{"d":250,"n":1,"t":4},"speaker":"A","text":"あなたは昔、歯ぎしりをしていました。覚えていますか？"},{"id":{"d":250,"n":2,"t":4},"speaker":"B","text":"姉に指摘されたことを覚えていますが、誰にも迷惑をかけていないと思って、長年そのままにしていました。"},{"id":{"d":250,"n":3,"t":4},"speaker":"A","text":"今は、歯ぎしりをしていないということでしょうか？"},{"id":{"d":250,"n":4,"t":4},"speaker":"B","text":"いえ、歯科医院で「歯が擦り切れています」と言われました。"},{"id":{"d":250,"n":5,"t":4},"speaker":"A","text":"そのようなことがあるとは驚きです。それで、どのように治療をしたのですか？"},{"id":{"d":250,"n":6,"t":4},"speaker":"B","text":"その歯科医院で、マウスピースを作ってもらいました。"},{"id":{"d":250,"n":7,"t":4},"speaker":"A","text":"最近は、マウスピースを作る人も多いようですね。テレビの放送でマウスピース用洗浄剤のCMも見るくらいです。"},{"id":{"d":250,"n":8,"t":4},"speaker":"B","text":"でも、僕はマウスピースを水で洗っています。"}];
INSERT INTO sentence [{"id":{"d":251,"n":1,"t":4},"speaker":"A","text":"あなたは、いつも携帯している薬などはありますか？"},{"id":{"d":251,"n":2,"t":4},"speaker":"B","text":"ほとんどありません。目薬くらいです。"},{"id":{"d":251,"n":3,"t":4},"speaker":"A","text":"そうですか。リップクリームや、ハンドクリームも、いつも使用しているものがありましたら持参してくださいね。"},{"id":{"d":251,"n":4,"t":4},"speaker":"B","text":"旅先に行ってから、買ってはいけないのでしょうか？"},{"id":{"d":251,"n":5,"t":4},"speaker":"A","text":"外国の方は、日本の方より体格が大きいので、表示通りの分量を摂取すると効きすぎる傾向があります。"},{"id":{"d":251,"n":6,"t":4},"speaker":"B","text":"なるほど、そうですか。外国で日本と同じ製品は売っていないのですか？"},{"id":{"d":251,"n":7,"t":4},"speaker":"A","text":"たとえ同じ製品のように見えても、外国仕様になっている恐れがありますので、注意が必要です。"},{"id":{"d":251,"n":8,"t":4},"speaker":"B","text":"頭痛薬や、腹痛の薬も、必要ですか？"},{"id":{"d":251,"n":9,"t":4},"speaker":"A","text":"私どもで持参する薬の数量は限られていますし、皆さんの常用しているものとは限りません。できれば、小分けにして所持してください。"}];
INSERT INTO sentence [{"id":{"d":252,"n":1,"t":4},"speaker":"A","text":"魚のサバやサンマは、なぜあまり刺身にならないのですか？"},{"id":{"d":252,"n":2,"t":4},"speaker":"B","text":"それらの魚にはアニサキスが多いので、生食は食中毒の原因になる可能性が高いと聞いたことがあります。"},{"id":{"d":252,"n":3,"t":4},"speaker":"A","text":"アニサキスとは何ですか？"},{"id":{"d":252,"n":4,"t":4},"speaker":"B","text":"魚介類に見られる寄生虫です。生魚では生きている場合があり、そのまま食べると人間の腸を食い破ることもあります。"},{"id":{"d":252,"n":5,"t":4},"speaker":"A","text":"恐ろしいです。調理してすぐに食べれば大丈夫ではないのですか？"},{"id":{"d":252,"n":6,"t":4},"speaker":"B","text":"逆です。新鮮だと、アニサキスが生きている可能性が高くなります。普通は熱したり凍らせたりして殺す調理法を行います。"},{"id":{"d":252,"n":7,"t":4},"speaker":"A","text":"よくご存じですね。さすが厨房担当だけのことはあります。"},{"id":{"d":252,"n":8,"t":4},"speaker":"B","text":"今度、調理師試験を受けようと思っていて、ちょうど勉強していたところでした。"}];
INSERT INTO sentence [{"id":{"d":253,"n":1,"t":4},"speaker":"A","text":"先生、ばんそうこうはありませんか？"},{"id":{"d":253,"n":2,"t":4},"speaker":"B","text":"けがですか。傷を見せてください。"},{"id":{"d":253,"n":3,"t":4},"speaker":"A","text":"彫刻刀で指を切ったのですが、もう血は止まっているのでばんそうこうだけで大丈夫です。"},{"id":{"d":253,"n":4,"t":4},"speaker":"B","text":"念のために見せてください。この傷ならば、このパッドを貼ると良いです。"},{"id":{"d":253,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。布や脱脂綿を使ったばんそうこうを使わないのですか？"},{"id":{"d":253,"n":6,"t":4},"speaker":"B","text":"このパッドは、薬を使わなくても、自然治癒力を使って早くきれいに治すことができます。"},{"id":{"d":253,"n":7,"t":4},"speaker":"A","text":"早く治癒するのですね。防水性があるところも気に入りました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":254,"n":1,"t":4},"speaker":"A","text":"わが国は、もうサッカーのFIFAワールドカップ常連国だと思いますが、毎回アジア予選で苦戦しますね。"},{"id":{"d":254,"n":2,"t":4},"speaker":"B","text":"海外チームに所属している選手が多いのに、その力の差を生かしていないように思います。"},{"id":{"d":254,"n":3,"t":4},"speaker":"A","text":"欧州リーグから日本の代表戦に出ると、移動距離や時差でかなりの負担があるからではないでしょうか？"},{"id":{"d":254,"n":4,"t":4},"speaker":"B","text":"なるほど、代表戦に出てから欧州に戻った選手がけがをするケースも多いようです。"}];
INSERT INTO sentence [{"id":{"d":255,"n":1,"t":4},"speaker":"A","text":"警備隊や、特別機動警備隊では、武道採用があるようですね。"},{"id":{"d":255,"n":2,"t":4},"speaker":"B","text":"ええ。でも、もし、あなたが武道採用枠での受験を検討しているのなら、しっかり考えた方がいいと思いますよ。"},{"id":{"d":255,"n":3,"t":4},"speaker":"A","text":"やはり、武道で実績がないといけないのでしょうか？私は6年くらい剣道をしていました。"},{"id":{"d":255,"n":4,"t":4},"speaker":"B","text":"武道だけでは難しいという意味です。特別な体力がないと、大変な部隊だと聞いたことがあります。"}];
INSERT INTO sentence [{"id":{"d":256,"n":1,"t":4},"speaker":"A","text":"悠を保育園に入れる前に、アレルギー検査を受けさせたいと考えています。"},{"id":{"d":256,"n":2,"t":4},"speaker":"B","text":"僕も君もアレルギーはないのだから、アレルギー検査の必要はないのではありませんか？"},{"id":{"d":256,"n":3,"t":4},"speaker":"A","text":"必ずしも、遺伝でアレルギーになるとは限りません。それに、私たちも、これからアレルギーになる可能性はあるのですよ。"},{"id":{"d":256,"n":4,"t":4},"speaker":"B","text":"そうなのですか。検査は簡単にできるのでしょうか？"},{"id":{"d":256,"n":5,"t":4},"speaker":"A","text":"小児科や、アレルギー専門の病院で、血液検査か皮膚検査を行うだけで、できるそうです。"},{"id":{"d":256,"n":6,"t":4},"speaker":"B","text":"どれくらいの金額なのか、分かりますか？"},{"id":{"d":256,"n":7,"t":4},"speaker":"A","text":"3万円前後ですが、未就学児の場合は、自治体でいくらか負担してくれるそうです。"},{"id":{"d":256,"n":8,"t":4},"speaker":"B","text":"そうですか。それならばついでに、僕たちも検査を受けてみましょうか？"},{"id":{"d":256,"n":9,"t":4},"speaker":"A","text":"そうですね。みんなで検査を受けに行くのならば、アレルギー専門の病院に行くことにしましょう。"}];
INSERT INTO sentence [{"id":{"d":257,"n":1,"t":4},"speaker":"A","text":"健康診断と、人間ドックはどう違うのでしょうか？"},{"id":{"d":257,"n":2,"t":4},"speaker":"B","text":"健康診断は定期的に受けなければならないものですが、人間ドックはそうではありません。"},{"id":{"d":257,"n":3,"t":4},"speaker":"A","text":"あなたは、人間ドックに行ったことはありますか？"},{"id":{"d":257,"n":4,"t":4},"speaker":"B","text":"去年、健康診断の結果を見て、「精密検査をした方がいい」と担当医に言われて、人間ドックに行きました。"}];
INSERT INTO sentence [{"id":{"d":258,"n":1,"t":4},"speaker":"A","text":"埼玉にはたくさんのプロスポーツチームがありますね。野球チームの埼玉西武ライオンズ、サッカーチームの浦和レッドダイヤモンズ、大宮アルディージャ、他には何があったでしょうか？"},{"id":{"d":258,"n":2,"t":4},"speaker":"B","text":"越谷に、プロバスケットボールチームの越谷アルファーズもありますね。"},{"id":{"d":258,"n":3,"t":4},"speaker":"A","text":"バスケットボールはいつからプロ化していますか？"},{"id":{"d":258,"n":4,"t":4},"speaker":"B","text":"2016年だったと思います。Bリーグとなってプロ化しました。"}];
INSERT INTO sentence [{"id":{"d":259,"n":1,"t":4},"speaker":"A","text":"海外ドラマでは、医療系が1つのジャンルになっています。"},{"id":{"d":259,"n":2,"t":4},"speaker":"B","text":"そうですね。海外ドラマの中でも、とても人気のあるジャンルのひとつです。"},{"id":{"d":259,"n":3,"t":4},"speaker":"A","text":"医療ドラマは、やはり緊張感のある魅力的なストーリーを作りやすいのでしょうか？"},{"id":{"d":259,"n":4,"t":4},"speaker":"B","text":"そうかもしれませんね。看護師と医者、内科と外科、経営と社会貢献などの対照的で重層的な構造は、物語の設定として魅力的です。"},{"id":{"d":259,"n":5,"t":4},"speaker":"A","text":"確かに、そのような設定も医療系ドラマに魅力を感じるところの1つです。"}];
INSERT INTO sentence [{"id":{"d":260,"n":1,"t":4},"speaker":"A","text":"今週は、残業続きで疲れました。"},{"id":{"d":260,"n":2,"t":4},"speaker":"B","text":"そうですね。今日は幸いにも早く帰れそうなので、リフレクソロジーに行きませんか？"},{"id":{"d":260,"n":3,"t":4},"speaker":"A","text":"興味はあるのですが、これまで行ったことはありませんでした。ぜひとも、連れて行ってください。"},{"id":{"d":260,"n":4,"t":4},"speaker":"B","text":"はい、一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":261,"n":1,"t":4},"speaker":"A","text":"まだ6月ですが、気温が高いので熱中症に注意が必要ですね。"},{"id":{"d":261,"n":2,"t":4},"speaker":"B","text":"はい、今日のような暑い日は、冷たいスポーツ飲料が飲みたいです。"},{"id":{"d":261,"n":3,"t":4},"speaker":"A","text":"汗拭き用のハンカチは、持っていますか？"},{"id":{"d":261,"n":4,"t":4},"speaker":"B","text":"私はタオルを持っています。ハンカチよりもタオルの方が大きくて便利です。"},{"id":{"d":261,"n":5,"t":4},"speaker":"A","text":"それは良いアイデアですね。私もこれからは、タオルを持ち歩こうと思います。"}];
INSERT INTO sentence [{"id":{"d":262,"n":1,"t":4},"speaker":"A","text":"風邪はひき始めが肝心といいますが、ひき始めはどのようにして分かりますか？"},{"id":{"d":262,"n":2,"t":4},"speaker":"B","text":"微熱、鼻水や鼻詰まり、くしゃみやせきなどが風邪の初期症状だと思います。"},{"id":{"d":262,"n":3,"t":4},"speaker":"A","text":"それらは全て花粉症の症状にも当てはまるので、花粉症の患者でもある私には、判断が難しいです。"},{"id":{"d":262,"n":4,"t":4},"speaker":"B","text":"なるほど、それは大変ですね。病院に行って、診察してもらう方が安心ですね。"}];
INSERT INTO sentence [{"id":{"d":263,"n":1,"t":4},"speaker":"A","text":"ボールコントロールを良くしたいのですが、どのような方法があるでしょうか？"},{"id":{"d":263,"n":2,"t":4},"speaker":"B","text":"プレートの踏む位置を、変える方法があります。今は、どこを踏んでいますか？"},{"id":{"d":263,"n":3,"t":4},"speaker":"A","text":"中央より向かって左寄りを踏んでいます。"},{"id":{"d":263,"n":4,"t":4},"speaker":"B","text":"左投げの場合は、投げる時にプレートの右端を踏むようにして、投げてみると良いかもしれません。"}];
INSERT INTO sentence [{"id":{"d":264,"n":1,"t":4},"speaker":"A","text":"病み上がりなので、何か軽いものが食べたいです。冷蔵庫の中に何があるか、見てもらえますか？"},{"id":{"d":264,"n":2,"t":4},"speaker":"B","text":"冷蔵庫には、水以外は何も入っていませんね。"},{"id":{"d":264,"n":3,"t":4},"speaker":"A","text":"そうですか。買い物にも行けず、寝込んでいたので食料が尽きてしまいました。"},{"id":{"d":264,"n":4,"t":4},"speaker":"B","text":"代わりに私が買い物に行きましょう。食べ物の他に、必要なものはありますか？"},{"id":{"d":264,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。栄養剤と解熱剤もなくなっているので、買ってきてほしいのですが、お願いしてもよいですか？"},{"id":{"d":264,"n":6,"t":4},"speaker":"B","text":"もちろん、良いですよ。車で来たので、すぐに買い物を済ませて帰ってきますね。"}];
INSERT INTO sentence [{"id":{"d":265,"n":1,"t":4},"speaker":"A","text":"あなたはどのようにしてひげをそっていますか？私は電気カミソリを使っています。"},{"id":{"d":265,"n":2,"t":4},"speaker":"B","text":"シェービングクリームを肌につけてから、T字カミソリでそっています。"},{"id":{"d":265,"n":3,"t":4},"speaker":"A","text":"カミソリ負けは、しませんか？"},{"id":{"d":265,"n":4,"t":4},"speaker":"B","text":"5枚刃のカミソリに変えてからは、それほどカミソリ負けをしなくなりました。"}];
INSERT INTO sentence [{"id":{"d":266,"n":1,"t":4},"speaker":"A","text":"感染症と、伝染病の違いが分かりません。もしご存知でしたら、教えていただけますか？"},{"id":{"d":266,"n":2,"t":4},"speaker":"B","text":"伝染病も、感染症の1種です。細菌や微生物が体の中に入って、増殖することを感染症といいます。"},{"id":{"d":266,"n":3,"t":4},"speaker":"A","text":"伝染病は、感染症と同じということですか？"},{"id":{"d":266,"n":4,"t":4},"speaker":"B","text":"感染症の中でも、伝染性のあるものを伝染病といいます。集団感染するようなものは疫病といわれています。"}];
INSERT INTO sentence [{"id":{"d":267,"n":1,"t":4},"speaker":"A","text":"もう少し、体を引き締めたいのですが、効果的な筋力トレーニングはありますか？"},{"id":{"d":267,"n":2,"t":4},"speaker":"B","text":"大迫さんは、週2回のジム通いで、1時間のトレーニングを行っていますが、やや回数が少ないかも知れません。"},{"id":{"d":267,"n":3,"t":4},"speaker":"A","text":"仕事の関係で、これ以上回数を増やせないのです。"},{"id":{"d":267,"n":4,"t":4},"speaker":"B","text":"では、家に帰ってからトレーニングを行う時間はありますか？"},{"id":{"d":267,"n":5,"t":4},"speaker":"A","text":"10分くらいなら、どうにかして毎日できると思います。"},{"id":{"d":267,"n":6,"t":4},"speaker":"B","text":"10分でも、効果は出ますよ。今日の帰りにメニューをお知らせしましょう。"},{"id":{"d":267,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。もう少し頑張ってみます。"}];
INSERT INTO sentence [{"id":{"d":268,"n":1,"t":4},"speaker":"A","text":"野球のホームベース上のクロスプレーは、昔と少し変わってきていますが、ご存じですか？"},{"id":{"d":268,"n":2,"t":4},"speaker":"B","text":"タックルがなくなったことですか？"},{"id":{"d":268,"n":3,"t":4},"speaker":"A","text":"はい、タックルは禁止になりました。"},{"id":{"d":268,"n":4,"t":4},"speaker":"B","text":"テレビ番組のスポーツニュースで見たときは、キャッチャーが吹っ飛んでいました。プロテクターやマスクがなかったら大変です。"},{"id":{"d":268,"n":5,"t":4},"speaker":"A","text":"タックルの禁止とともに、守備側がホームベース全体を覆うことも禁止となりました。"},{"id":{"d":268,"n":6,"t":4},"speaker":"B","text":"そうなのですね。今度野球観戦をするときは、注意して見てみます。"}];
INSERT INTO sentence [{"id":{"d":269,"n":1,"t":4},"speaker":"A","text":"家にどのような常備薬を置いていますか？"},{"id":{"d":269,"n":2,"t":4},"speaker":"B","text":"痛み止め、解熱剤、消毒液、風邪薬、湿布、頭痛薬、胃薬など一通りあります。"},{"id":{"d":269,"n":3,"t":4},"speaker":"A","text":"常備薬の使用期限は、確認していますか？"},{"id":{"d":269,"n":4,"t":4},"speaker":"B","text":"自分では確認していないのですが、置き薬を利用しているので、薬屋さんが定期的に確認してくれます。"}];
INSERT INTO sentence [{"id":{"d":270,"n":1,"t":4},"speaker":"A","text":"特別養護老人ホームのボランティアで、何をするか考えましたか？"},{"id":{"d":270,"n":2,"t":4},"speaker":"B","text":"はい、希望の方にお化粧することを考えました。"},{"id":{"d":270,"n":3,"t":4},"speaker":"A","text":"お化粧となると、ホームのスタッフの方に話しておいた方がいいと思います。"},{"id":{"d":270,"n":4,"t":4},"speaker":"B","text":"実は、さっき電話をして、確認を取りました。明日事前に化粧品を見てもらうことにしました。"},{"id":{"d":270,"n":5,"t":4},"speaker":"A","text":"それは、準備がいいですね。喜んでもらえるといいですね。"}];
INSERT INTO sentence [{"id":{"d":271,"n":1,"t":4},"speaker":"A","text":"けがから復帰されて、シーズンも半ばになりましたが、体の調子はいかがですか？"},{"id":{"d":271,"n":2,"t":4},"speaker":"B","text":"調子は良いです。以前より、疲れが残らないようになりました。"},{"id":{"d":271,"n":3,"t":4},"speaker":"A","text":"何か健康上、特別なことはされているのですか？"},{"id":{"d":271,"n":4,"t":4},"speaker":"B","text":"大けがをきっかけに、食事のことを考えるようになり、ビーガンを実践するようになりました。"},{"id":{"d":271,"n":5,"t":4},"speaker":"A","text":"卓球のような激しい動きでは、かなりのエネルギーを消耗すると思うのですが、ビーガン食でも十分な栄養とエネルギーを確保することはできるのですか？"},{"id":{"d":271,"n":6,"t":4},"speaker":"B","text":"ビーガンを実践するにあたっては、栄養士やチームドクター、そして、何よりビーガン食の調理師と綿密な打ち合わせをしています。何より、以前より調子が良いので、しばらく続けてみようと思っています。"}];
INSERT INTO sentence [{"id":{"d":272,"n":1,"t":4},"speaker":"A","text":"あなたは、どこに行くのですか？"},{"id":{"d":272,"n":2,"t":4},"speaker":"B","text":"友達と、体育館に行きます。"},{"id":{"d":272,"n":3,"t":4},"speaker":"A","text":"体育館で、何をするのですか？"},{"id":{"d":272,"n":4,"t":4},"speaker":"B","text":"バドミントンの練習です。最近、運動不足だったので、体を動かしたいと思っています。"},{"id":{"d":272,"n":5,"t":4},"speaker":"A","text":"この前の健康診断でも、お医者さんに「運動をしてください」と言われていましたね。"},{"id":{"d":272,"n":6,"t":4},"speaker":"B","text":"はい、そうです。これからは、定期的にバドミントンの練習をしたいと思っています。"},{"id":{"d":272,"n":7,"t":4},"speaker":"A","text":"いいですね。準備運動とストレッチ体操をして、けがはしないようにしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":273,"n":1,"t":4},"speaker":"A","text":"洋子さんの肌は、とてもきれいですね。"},{"id":{"d":273,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":273,"n":3,"t":4},"speaker":"A","text":"スキンケアで、気を付けていることはありますか？"},{"id":{"d":273,"n":4,"t":4},"speaker":"B","text":"特別なことはしていませんよ。お風呂で洗顔をした後、肌には化粧水、乳液、美容液、クリームの順番で塗っています。"},{"id":{"d":273,"n":5,"t":4},"speaker":"A","text":"なるほど。きちんと続けることが大事なのでしょうか？私は、スキンケアが面倒になってしまい、ついさぼってしまうことがあります。"},{"id":{"d":273,"n":6,"t":4},"speaker":"B","text":"私も、たまには手を抜いていますよ。洗顔後に、パックをするだけや、オールインワンジェルを使う日もあります。"}];
INSERT INTO sentence [{"id":{"d":274,"n":1,"t":4},"speaker":"A","text":"インフルエンザが、流行していますね。"},{"id":{"d":274,"n":2,"t":4},"speaker":"B","text":"息子のクラスは、インフルエンザの流行によって学級閉鎖になりました。"},{"id":{"d":274,"n":3,"t":4},"speaker":"A","text":"それは、大変ですね。"},{"id":{"d":274,"n":4,"t":4},"speaker":"B","text":"幸いにも、家族はまだ誰も感染していないので、引き続き予防を頑張りたいです。予防といえば、田中さんは、インフルエンザの予防接種をしましたか？"},{"id":{"d":274,"n":5,"t":4},"speaker":"A","text":"はい。先週、病院でインフルエンザワクチンを打ってもらいました。高橋さんはどうですか？"},{"id":{"d":274,"n":6,"t":4},"speaker":"B","text":"私は、今週インフルエンザワクチンを打ちに病院へ行きます。"},{"id":{"d":274,"n":7,"t":4},"speaker":"A","text":"手洗いと、うがいも、丁寧にしなければなりませんね。"},{"id":{"d":274,"n":8,"t":4},"speaker":"B","text":"そうですね。予防のために、できることをしましょう。"}];
INSERT INTO sentence [{"id":{"d":275,"n":1,"t":4},"speaker":"A","text":"新井さんは、健康のために、何かしていることはありますか？"},{"id":{"d":275,"n":2,"t":4},"speaker":"B","text":"はい。ヨガをしています。"},{"id":{"d":275,"n":3,"t":4},"speaker":"A","text":"それは習い事として、しているのですか？"},{"id":{"d":275,"n":4,"t":4},"speaker":"B","text":"そうです。週に1回、ヨガ教室に通っていますがもうすぐ1年たちます。"},{"id":{"d":275,"n":5,"t":4},"speaker":"A","text":"続けられているとは、すごいです。私は飽き性で、なかなか続きません。"},{"id":{"d":275,"n":6,"t":4},"speaker":"B","text":"続けることは難しいですよね。でも続けると、効果が出てきますよ。"},{"id":{"d":275,"n":7,"t":4},"speaker":"A","text":"ヨガを続けることによって、どのような効果がありましたか？"},{"id":{"d":275,"n":8,"t":4},"speaker":"B","text":"肩凝りが改善しました。"},{"id":{"d":275,"n":9,"t":4},"speaker":"A","text":"それは素晴らしいですね。"}];
INSERT INTO sentence [{"id":{"d":276,"n":1,"t":4},"speaker":"A","text":"お兄さん、暖房の温度を上げてください。"},{"id":{"d":276,"n":2,"t":4},"speaker":"B","text":"もう部屋は、十分暖かいですよ。寒いのですか？"},{"id":{"d":276,"n":3,"t":4},"speaker":"A","text":"はい、寒気がします。"},{"id":{"d":276,"n":4,"t":4},"speaker":"B","text":"体温は測りましたか？"},{"id":{"d":276,"n":5,"t":4},"speaker":"A","text":"さっき測ったときは、平熱でした。"},{"id":{"d":276,"n":6,"t":4},"speaker":"B","text":"これから熱が上がるかもしれません。他に何か症状は、ありますか？"},{"id":{"d":276,"n":7,"t":4},"speaker":"A","text":"頭痛がします。"},{"id":{"d":276,"n":8,"t":4},"speaker":"B","text":"風邪の症状ですね。今日は早く寝て、熱が出たら風邪薬を飲んでください。"},{"id":{"d":276,"n":9,"t":4},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":277,"n":1,"t":4},"speaker":"A","text":"斎藤さん、こんにちは。"},{"id":{"d":277,"n":2,"t":4},"speaker":"B","text":"こんにちは。今朝は、息子さんがランニングしているところを見かけました。"},{"id":{"d":277,"n":3,"t":4},"speaker":"A","text":"そうでしたか。息子は毎日朝になると、ランニングをしているのです。"},{"id":{"d":277,"n":4,"t":4},"speaker":"B","text":"毎日ですか。すごいですね。体力づくりですか？"},{"id":{"d":277,"n":5,"t":4},"speaker":"A","text":"趣味だそうです。「身も心もリフレッシュできる」と言っていました。"},{"id":{"d":277,"n":6,"t":4},"speaker":"B","text":"良い習慣ですね。斎藤さんもランニングをすることはありますか？"},{"id":{"d":277,"n":7,"t":4},"speaker":"A","text":"私は走ることが苦手なので、しません。代わりに、散歩をすることはあります。"},{"id":{"d":277,"n":8,"t":4},"speaker":"B","text":"散歩は健康に良いと聞きますね。私は家から一歩も出ない日もあります。"},{"id":{"d":277,"n":9,"t":4},"speaker":"A","text":"私も寒い日は、外に出るのがおっくうです。毎日散歩をすることが、今の目標です。"},{"id":{"d":277,"n":10,"t":4},"speaker":"B","text":"私も、まずは、2日に1回は散歩をすることにします。"}];
INSERT INTO sentence [{"id":{"d":278,"n":1,"t":4},"speaker":"A","text":"新しい髪型ですね。よく似合っています。"},{"id":{"d":278,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。1週間前に美容院に行って、ヘアカラーとパーマをしてもらいました。"},{"id":{"d":278,"n":3,"t":4},"speaker":"A","text":"パーマは初めてかけましたか？"},{"id":{"d":278,"n":4,"t":4},"speaker":"B","text":"はい。ヘアカラーは大学生になってから何度かしましたが、パーマは初めてです。"},{"id":{"d":278,"n":5,"t":4},"speaker":"A","text":"私はヘアカラーだけで、パーマはしたことありません。"},{"id":{"d":278,"n":6,"t":4},"speaker":"B","text":"朝の準備が楽になりますよ。ヘアアイロンで、カールをする手間がなくなりました。"},{"id":{"d":278,"n":7,"t":4},"speaker":"A","text":"それは良いですね。私は、いつもヘアセットに時間がかかってしまいます。"},{"id":{"d":278,"n":8,"t":4},"speaker":"B","text":"出かける前は、化粧と髪の毛のセットに時間がかかりますよね。"},{"id":{"d":278,"n":9,"t":4},"speaker":"A","text":"はい。毎日大忙しです。"}];
INSERT INTO sentence [{"id":{"d":279,"n":1,"t":4},"speaker":"A","text":"風呂場で使用しているシャンプーがなくなりそうです。未開封のものはありますか？"},{"id":{"d":279,"n":2,"t":4},"speaker":"B","text":"ありません。ドラッグストアに買いに行かなければなりませんね。"},{"id":{"d":279,"n":3,"t":4},"speaker":"A","text":"分かりました。今から買ってきます。以前、使っていたものと同じシャンプーで良いですか？"},{"id":{"d":279,"n":4,"t":4},"speaker":"B","text":"はい。香料がきつくなくて気に入っているので、同じものを買ってきてください。"},{"id":{"d":279,"n":5,"t":4},"speaker":"A","text":"分かりました。トリートメントはまだありますか？"},{"id":{"d":279,"n":6,"t":4},"speaker":"B","text":"風呂場に置いてあるものはまだ残っていますが、未開封のものはありません。"},{"id":{"d":279,"n":7,"t":4},"speaker":"A","text":"それでは、シャンプーとトリートメントのセットを買ってきますね。"},{"id":{"d":279,"n":8,"t":4},"speaker":"B","text":"その方がお得ですね。そうしてください。"}];
INSERT INTO sentence [{"id":{"d":280,"n":1,"t":4},"speaker":"A","text":"最近、ふくらはぎがむくみやすいです。"},{"id":{"d":280,"n":2,"t":4},"speaker":"B","text":"入浴時に、湯船に漬かりながらマッサージするといいですよ。"},{"id":{"d":280,"n":3,"t":4},"speaker":"A","text":"そうなのですか。私は、暑い日はシャンプーだけで済ませてしまうこともあります。"},{"id":{"d":280,"n":4,"t":4},"speaker":"B","text":"湯船に漬かってみてください。血行が良くなって、むくみが改善されます。"},{"id":{"d":280,"n":5,"t":4},"speaker":"A","text":"試してみます。他に何かしていることはありますか？"},{"id":{"d":280,"n":6,"t":4},"speaker":"B","text":"入浴後に、ストレッチ体操をしています。"},{"id":{"d":280,"n":7,"t":4},"speaker":"A","text":"体のケアを、しっかりされているのですね。"},{"id":{"d":280,"n":8,"t":4},"speaker":"B","text":"私もむくみやすいのですが、マッサージとストレッチ体操でだいぶ改善されますよ。"},{"id":{"d":280,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。私も見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":281,"n":1,"t":4},"speaker":"A","text":"えみさんは、化粧品をたくさん持っていますね。"},{"id":{"d":281,"n":2,"t":4},"speaker":"B","text":"はい。化粧品を集めることが趣味なのです。"},{"id":{"d":281,"n":3,"t":4},"speaker":"A","text":"リップクリームだけで、何種類もありますね。全部で何本あるのですか？"},{"id":{"d":281,"n":4,"t":4},"speaker":"B","text":"30本です。色味やパッケージに引かれて、お店に行くとつい買ってしまうのです。"},{"id":{"d":281,"n":5,"t":4},"speaker":"A","text":"アイシャドウもたくさんありますね。これだけそろっていると、いろいろなメークが楽しめそうです。"},{"id":{"d":281,"n":6,"t":4},"speaker":"B","text":"はい。その日のコーディネートに合わせて、毎日違うメークをしています。"},{"id":{"d":281,"n":7,"t":4},"speaker":"A","text":"なるほど。洋服とメークを合わせるのですね。私は、そこまで考えたことはありませんでした。"},{"id":{"d":281,"n":8,"t":4},"speaker":"B","text":"統一感が出て、一気におしゃれになりますよ。"},{"id":{"d":281,"n":9,"t":4},"speaker":"A","text":"えみさんが、おしゃれな理由が分かりました。"}];
INSERT INTO sentence [{"id":{"d":282,"n":1,"t":4},"speaker":"A","text":"たかし君、昨日、私のひげそりを使いましたか？"},{"id":{"d":282,"n":2,"t":4},"speaker":"B","text":"はい、お父さんのひげそりを借りました。"},{"id":{"d":282,"n":3,"t":4},"speaker":"A","text":"ひげそりをどこに置いたか覚えていますか？いつも置く場所に見当たらないのです。"},{"id":{"d":282,"n":4,"t":4},"speaker":"B","text":"洗面所の引き出しの中に入れました。目薬や消毒薬を入れているところです。"},{"id":{"d":282,"n":5,"t":4},"speaker":"A","text":"あそこですか。分かりました。ありがとうございます。"},{"id":{"d":282,"n":6,"t":4},"speaker":"B","text":"定位置と違うところに置いてしまって、すみません。普段はどこに置いているのですか？"},{"id":{"d":282,"n":7,"t":4},"speaker":"A","text":"目薬や消毒薬を入れている引き出しの、1つ下の引き出しです。"},{"id":{"d":282,"n":8,"t":4},"speaker":"B","text":"場所を間違えていたのですね。次からは、間違えないようにします。"},{"id":{"d":282,"n":9,"t":4},"speaker":"A","text":"お願いします。シェービングクリームも同じ引き出しにあるので、次にひげをそるときは使ってみてください。"},{"id":{"d":282,"n":10,"t":4},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":283,"n":1,"t":4},"speaker":"A","text":"今からサッカー部の練習に出かけるところですか？"},{"id":{"d":283,"n":2,"t":4},"speaker":"B","text":"はい。行ってきます。"},{"id":{"d":283,"n":3,"t":4},"speaker":"A","text":"少し待ってください。日焼け止めクリームを肌に塗りましたか？"},{"id":{"d":283,"n":4,"t":4},"speaker":"B","text":"忘れていました。塗る習慣がなかなかつきません。"},{"id":{"d":283,"n":5,"t":4},"speaker":"A","text":"日焼けすると皮むけしたり、将来染みになったりしますよ。"},{"id":{"d":283,"n":6,"t":4},"speaker":"B","text":"それは怖いですね。毎日肌に日焼け止めクリームを塗るようにします。"},{"id":{"d":283,"n":7,"t":4},"speaker":"A","text":"そうしてください。暑いので、熱中症にも気を付けてくださいね。"},{"id":{"d":283,"n":8,"t":4},"speaker":"B","text":"はい。水分を小まめに取るようにします。"}];
INSERT INTO sentence [{"id":{"d":284,"n":1,"t":4},"speaker":"A","text":"内山さん、足の手術後の経過はどうですか？"},{"id":{"d":284,"n":2,"t":4},"speaker":"B","text":"順調です。痛み止めがまだ効いているので、痛みもありません。"},{"id":{"d":284,"n":3,"t":4},"speaker":"A","text":"それは良かったです。薬が切れると痛みが出てくるかもしれませんが、手術は成功しているので安心してくださいね。"},{"id":{"d":284,"n":4,"t":4},"speaker":"B","text":"はい。先生には、感謝してもしきれません。"},{"id":{"d":284,"n":5,"t":4},"speaker":"A","text":"いえいえ、内山さんの頑張りのおかげですよ。術後の経過が順調なら、予定通り、退院できそうです。"},{"id":{"d":284,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。退院後は、リハビリテーションで歩行練習を頑張ります。"},{"id":{"d":284,"n":7,"t":4},"speaker":"A","text":"はい。何かあれば、いつでもご連絡ください。"},{"id":{"d":284,"n":8,"t":4},"speaker":"B","text":"本当にありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":285,"n":1,"t":4},"speaker":"A","text":"ご飯が食べづらそうですね。どうかしたのですか？"},{"id":{"d":285,"n":2,"t":4},"speaker":"B","text":"実は、また口内炎ができてしまったのです。"},{"id":{"d":285,"n":3,"t":4},"speaker":"A","text":"れいなさんは、口内炎ができやすいですね？"},{"id":{"d":285,"n":4,"t":4},"speaker":"B","text":"そうなのです。舌や唇をかんだ覚えがないのに、いつの間にかできています。"},{"id":{"d":285,"n":5,"t":4},"speaker":"A","text":"それは大変ですね。ビタミンが足りていないのかもしれません。"},{"id":{"d":285,"n":6,"t":4},"speaker":"B","text":"ビタミン不足だと口内炎になるのですか？"},{"id":{"d":285,"n":7,"t":4},"speaker":"A","text":"そう聞いたことがあります。私は肌荒れ改善のために、ビタミン剤を飲んでいますが、口内炎ができたことはありません。"},{"id":{"d":285,"n":8,"t":4},"speaker":"B","text":"そうなのですか。ビタミン剤を飲んでみます。"}];
INSERT INTO sentence [{"id":{"d":286,"n":1,"t":4},"speaker":"A","text":"飲み物が来たので、先に乾杯しませんか？"},{"id":{"d":286,"n":2,"t":4},"speaker":"B","text":"もうすぐつまみも来ると思いますよ。少し待ちましょう。"},{"id":{"d":286,"n":3,"t":4},"speaker":"A","text":"そうしたら、つまみまでそろってから飲み始めますか？"},{"id":{"d":286,"n":4,"t":4},"speaker":"B","text":"そうしましょう。すきっ腹のときに、アルコールをたくさん飲むと悪酔いしてしまいます。"},{"id":{"d":286,"n":5,"t":4},"speaker":"A","text":"悪酔いは避けたいですね。食べ物が来てから乾杯しましょう。"},{"id":{"d":286,"n":6,"t":4},"speaker":"B","text":"そうしましょう。なるべく肝臓に負担をかけないで、お酒を楽しみたいです。"},{"id":{"d":286,"n":7,"t":4},"speaker":"A","text":"はい。お冷も頂いておきましょう。"},{"id":{"d":286,"n":8,"t":4},"speaker":"B","text":"これで万全ですね。"}];
INSERT INTO sentence [{"id":{"d":287,"n":1,"t":4},"speaker":"A","text":"テレビで何の番組を見ているのですか？"},{"id":{"d":287,"n":2,"t":4},"speaker":"B","text":"高校野球です。"},{"id":{"d":287,"n":3,"t":4},"speaker":"A","text":"お父さんは野球観戦が好きですね。今、どのような展開ですか？"},{"id":{"d":287,"n":4,"t":4},"speaker":"B","text":"9回裏の2アウト満塁です。0対0の投手戦でしたが、最大の山がきました。"},{"id":{"d":287,"n":5,"t":4},"speaker":"A","text":"得点を入れられなければ、延長戦に入るのですね。"},{"id":{"d":287,"n":6,"t":4},"speaker":"B","text":"そうです。バッターもピッチャーも、プロ野球界注目の選手です。"},{"id":{"d":287,"n":7,"t":4},"speaker":"A","text":"それは、楽しみな対戦ですね。"},{"id":{"d":287,"n":8,"t":4},"speaker":"B","text":"先ほどピッチャーが投げたのはカーブで、空振りでした。フルカウントです。"},{"id":{"d":287,"n":9,"t":4},"speaker":"A","text":"緊張感のある展開ですね。目が離せません。"}];
INSERT INTO sentence [{"id":{"d":288,"n":1,"t":4},"speaker":"A","text":"春の暖かい陽気になってきましたね。"},{"id":{"d":288,"n":2,"t":4},"speaker":"B","text":"気候は気持ちが良いのですが、春はあまり好きではありません。"},{"id":{"d":288,"n":3,"t":4},"speaker":"A","text":"どうしてですか？"},{"id":{"d":288,"n":4,"t":4},"speaker":"B","text":"私は花粉症なのです。この時期になると、くしゃみと目のかゆみが止まりません。"},{"id":{"d":288,"n":5,"t":4},"speaker":"A","text":"花粉症ですか？それはつらいですね。"},{"id":{"d":288,"n":6,"t":4},"speaker":"B","text":"はい。飲み薬と目薬が、毎日欠かせません。"},{"id":{"d":288,"n":7,"t":4},"speaker":"A","text":"あなたは、子どもの頃から花粉症だったのですか？"},{"id":{"d":288,"n":8,"t":4},"speaker":"B","text":"いいえ、ここ数年です。大人になってから花粉症になる人も多いようですよ。"},{"id":{"d":288,"n":9,"t":4},"speaker":"A","text":"そうなのですか。私は、今は花粉症ではありませんが、近い将来発病するかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":289,"n":1,"t":4},"speaker":"A","text":"健康診断の結果はどうでしたか？"},{"id":{"d":289,"n":2,"t":4},"speaker":"B","text":"おおむね良かったのですが、高血圧を指摘されてしまいました。"},{"id":{"d":289,"n":3,"t":4},"speaker":"A","text":"高血圧ですか？食事を見直した方がいいかもしれません。"},{"id":{"d":289,"n":4,"t":4},"speaker":"B","text":"はい。「塩分を控えるように」と医者から言われました。"},{"id":{"d":289,"n":5,"t":4},"speaker":"A","text":"これまで、塩分の高い食事をしていましたか？"},{"id":{"d":289,"n":6,"t":4},"speaker":"B","text":"そのつもりはなかったのですが、よく考えるとそうだと思います。"},{"id":{"d":289,"n":7,"t":4},"speaker":"A","text":"例えば、どのようなことが思い当たりますか？"},{"id":{"d":289,"n":8,"t":4},"speaker":"B","text":"ラーメンが好きなのですが、毎回スープを飲み干していました。"},{"id":{"d":289,"n":9,"t":4},"speaker":"A","text":"なるほど。それは確かに塩分が高い食事のようです。"},{"id":{"d":289,"n":10,"t":4},"speaker":"B","text":"今後は、ラーメンのスープを飲み干すことは控えようと思います。"}];
INSERT INTO sentence [{"id":{"d":290,"n":1,"t":4},"speaker":"A","text":"山中さんの娘さんは、小学校2年生ですか？"},{"id":{"d":290,"n":2,"t":4},"speaker":"B","text":"はい。高木さんの息子さんと同い年です。"},{"id":{"d":290,"n":3,"t":4},"speaker":"A","text":"そうでしたね。娘さんは、歯科クリニックに行くことを嫌がりませんか？"},{"id":{"d":290,"n":4,"t":4},"speaker":"B","text":"少し嫌がりますが、定期検診には行ってくれます。"},{"id":{"d":290,"n":5,"t":4},"speaker":"A","text":"それは素晴らしいです。私の息子は、歯科クリニックを怖がって検診に行きたがらないのです。"},{"id":{"d":290,"n":6,"t":4},"speaker":"B","text":"そうなのですか。何かあったのですか？"},{"id":{"d":290,"n":7,"t":4},"speaker":"A","text":"1年前に抜歯をしたことがトラウマになったようです。それ以降、行かなくなってしまいました。"},{"id":{"d":290,"n":8,"t":4},"speaker":"B","text":"子ども向けの歯科クリニックはどうですか？キッズルームが充実していると、息子さんの気持ちも軽くなるかもしれません。"},{"id":{"d":290,"n":9,"t":4},"speaker":"A","text":"詳しく調べてみます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":291,"n":1,"t":4},"speaker":"A","text":"顔に、吹き出ものができてしまいました。最近、肌が荒れてしまうことが多いです。"},{"id":{"d":291,"n":2,"t":4},"speaker":"B","text":"一度、皮膚科に行ってみるといいかもしれません。"},{"id":{"d":291,"n":3,"t":4},"speaker":"A","text":"なつみさんは、皮膚科に通っていますか？"},{"id":{"d":291,"n":4,"t":4},"speaker":"B","text":"はい。漢方薬と塗り薬を処方してもらっています。私は、通院するようになってから、肌荒れがだいぶ落ち着いてきました。"},{"id":{"d":291,"n":5,"t":4},"speaker":"A","text":"私も行ってみようと思います。肌荒れのときは、化粧もあまりしたくありません。"},{"id":{"d":291,"n":6,"t":4},"speaker":"B","text":"そうですよね。私も、自分に合う化粧品を見つけるまで時間がかかりました。"},{"id":{"d":291,"n":7,"t":4},"speaker":"A","text":"今は、どのようなものを使っているのですか？"},{"id":{"d":291,"n":8,"t":4},"speaker":"B","text":"皮膚科で買える、敏感肌用の下地クリームやファンデーションを使っています。"},{"id":{"d":291,"n":9,"t":4},"speaker":"A","text":"皮膚科で化粧品も買えるのですね。ますます行きたくなりました。"}];
INSERT INTO sentence [{"id":{"d":292,"n":1,"t":4},"speaker":"A","text":"たくみさん、膝のけがは、どうしたのですか？"},{"id":{"d":292,"n":2,"t":4},"speaker":"B","text":"学校で運動会の練習をしているときに、転んで出血してしまいました。"},{"id":{"d":292,"n":3,"t":4},"speaker":"A","text":"そうだったのですね。すぐに保健室に行って、手当てをしてもらいましたか？"},{"id":{"d":292,"n":4,"t":4},"speaker":"B","text":"はい。先生に消毒をしてもらいました。"},{"id":{"d":292,"n":5,"t":4},"speaker":"A","text":"それは、良かったです。"},{"id":{"d":292,"n":6,"t":4},"speaker":"B","text":"家に、ばんそうこうはありますか？今、貼っているばんそうこうが剥がれてしまいそうです。"},{"id":{"d":292,"n":7,"t":4},"speaker":"A","text":"ありますよ。薬箱の中に入っています。"},{"id":{"d":292,"n":8,"t":4},"speaker":"B","text":"分かりました。シャワーを浴びた後に、新しいばんそうこうに替えますね。"},{"id":{"d":292,"n":9,"t":4},"speaker":"A","text":"そうしてください。"}];
INSERT INTO sentence [{"id":{"d":293,"n":1,"t":4},"speaker":"A","text":"最近、買って良かった化粧品はありますか？"},{"id":{"d":293,"n":2,"t":4},"speaker":"B","text":"はい。ファンデーションです。"},{"id":{"d":293,"n":3,"t":4},"speaker":"A","text":"これまでのものと、どう違うのですか？"},{"id":{"d":293,"n":4,"t":4},"speaker":"B","text":"時間がたっても、全く崩れないのです。"},{"id":{"d":293,"n":5,"t":4},"speaker":"A","text":"それはいいですね。"},{"id":{"d":293,"n":6,"t":4},"speaker":"B","text":"はい。化粧直しをしなくても、1日過ごせるようになりました。"},{"id":{"d":293,"n":7,"t":4},"speaker":"A","text":"私は、夕方必ず化粧が崩れてきてしまいます。"},{"id":{"d":293,"n":8,"t":4},"speaker":"B","text":"良かったら、私の愛用品を使ってみてください。買ったお店を紹介します。"},{"id":{"d":293,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。ぜひ教えてください。"}];
INSERT INTO sentence [{"id":{"d":294,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。何かお探しですか？"},{"id":{"d":294,"n":2,"t":4},"speaker":"B","text":"こんにちは。彼女の誕生日プレゼントを買いに来ました。"},{"id":{"d":294,"n":3,"t":4},"speaker":"A","text":"すてきですね。もう商品は決められていますか？"},{"id":{"d":294,"n":4,"t":4},"speaker":"B","text":"いえ、まだ決められていません。プレゼントにお薦めの商品はありますか？"},{"id":{"d":294,"n":5,"t":4},"speaker":"A","text":"ギフトセットがお薦めです。ラッピングがおしゃれで、プレゼントとして人気です。"},{"id":{"d":294,"n":6,"t":4},"speaker":"B","text":"良いですね。ギフトセットには、何が入っているのですか？"},{"id":{"d":294,"n":7,"t":4},"speaker":"A","text":"リップクリームとハンドクリームです。"},{"id":{"d":294,"n":8,"t":4},"speaker":"B","text":"では、それを買います。"},{"id":{"d":294,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":295,"n":1,"t":4},"speaker":"A","text":"先日、私は、あなたが食中毒になってしまったという話を聞きました。"},{"id":{"d":295,"n":2,"t":4},"speaker":"B","text":"はい。なかなか大変でした。"},{"id":{"d":295,"n":3,"t":4},"speaker":"A","text":"食中毒の原因は何だったのですか？"},{"id":{"d":295,"n":4,"t":4},"speaker":"B","text":"鶏肉です。バーベキューをしたときに、十分火が通っていないものを食べてしまったようです。"},{"id":{"d":295,"n":5,"t":4},"speaker":"A","text":"それは災難でしたね。"},{"id":{"d":295,"n":6,"t":4},"speaker":"B","text":"はい。激しい腹痛と発熱がありました。"},{"id":{"d":295,"n":7,"t":4},"speaker":"A","text":"今の体調はどうですか？"},{"id":{"d":295,"n":8,"t":4},"speaker":"B","text":"すっかり良くなりました。腹痛と発熱は、対処療法で治まっていきました。"},{"id":{"d":295,"n":9,"t":4},"speaker":"A","text":"大事に至らなくて良かったです。食べ物には気を付けなければなりませんね。"},{"id":{"d":295,"n":10,"t":4},"speaker":"B","text":"おっしゃる通りです。私も、これまで以上に気を付けようと思います。"}];
INSERT INTO sentence [{"id":{"d":296,"n":1,"t":4},"speaker":"A","text":"肩に、湿布を貼ってもらえませんか？"},{"id":{"d":296,"n":2,"t":4},"speaker":"B","text":"お安い御用です。両肩に1枚ずつで良いですか？"},{"id":{"d":296,"n":3,"t":4},"speaker":"A","text":"はい。ありがとうございます。自分ではうまく貼れないので、助かります。"},{"id":{"d":296,"n":4,"t":4},"speaker":"B","text":"あなたが肩に湿布を貼るとは、珍しいですね。痛むのですか？"},{"id":{"d":296,"n":5,"t":4},"speaker":"A","text":"はい。デスクワーク中の姿勢が悪く、肩が凝り固まってしまったようです。"},{"id":{"d":296,"n":6,"t":4},"speaker":"B","text":"あなたは長時間、デスクワークをしていたのですか？"},{"id":{"d":296,"n":7,"t":4},"speaker":"A","text":"はい。1日中していました。"},{"id":{"d":296,"n":8,"t":4},"speaker":"B","text":"それは肩凝りになりますね。適度に立ち上がって、肩のストレッチ体操をすると良いですよ。"},{"id":{"d":296,"n":9,"t":4},"speaker":"A","text":"はい。そうしてみます。"}];
INSERT INTO sentence [{"id":{"d":297,"n":1,"t":4},"speaker":"A","text":"冬は、肌が乾燥しますね。"},{"id":{"d":297,"n":2,"t":4},"speaker":"B","text":"はい。あなたが、特に乾燥を感じる部分はどこですか？"},{"id":{"d":297,"n":3,"t":4},"speaker":"A","text":"私は、手です。"},{"id":{"d":297,"n":4,"t":4},"speaker":"B","text":"私も手に乾燥を感じます。手が荒れて、ひびやあかぎれができてしまいます。"},{"id":{"d":297,"n":5,"t":4},"speaker":"A","text":"分かります。手が荒れているときは、皿洗いがおっくうになりますよね。"},{"id":{"d":297,"n":6,"t":4},"speaker":"B","text":"はい。冬の時期は、ゴム手袋なしでは皿洗いができません。"},{"id":{"d":297,"n":7,"t":4},"speaker":"A","text":"ゴム手袋を使うアイデアは、とてもいいですね。ひびやあかぎれに貼ったばんそうこうが、すぐに剥がれてしまうので困っていました。"},{"id":{"d":297,"n":8,"t":4},"speaker":"B","text":"手はよくぬれるので、ばんそうこうが剥がれやすいですよね。ゴム手袋は、お薦めですよ。"},{"id":{"d":297,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。ゴム手袋を試してみます。"}];
INSERT INTO sentence [{"id":{"d":298,"n":1,"t":4},"speaker":"A","text":"翔さんは、ブラックコーヒーがお好きなのですか？"},{"id":{"d":298,"n":2,"t":4},"speaker":"B","text":"はい、好きです。どうして分かったのですか？"},{"id":{"d":298,"n":3,"t":4},"speaker":"A","text":"いつも会社の机の上に缶コーヒーが置いてあるからです。"},{"id":{"d":298,"n":4,"t":4},"speaker":"B","text":"なるほど。確かに、毎日机の上に缶コーヒーを置いていますね。"},{"id":{"d":298,"n":5,"t":4},"speaker":"A","text":"1日に何缶も飲むのですか？"},{"id":{"d":298,"n":6,"t":4},"speaker":"B","text":"いえ、普段は1缶で、多くても2缶です。"},{"id":{"d":298,"n":7,"t":4},"speaker":"A","text":"しっかりと制限されているのですね。"},{"id":{"d":298,"n":8,"t":4},"speaker":"B","text":"はい。カフェインの取り過ぎは良くないと聞くので、飲み過ぎないようにしています。"},{"id":{"d":298,"n":9,"t":4},"speaker":"A","text":"それを聞いて安心しました。"}];
INSERT INTO sentence [{"id":{"d":299,"n":1,"t":4},"speaker":"A","text":"あなたは、歯科の定期検診を受けていますか？"},{"id":{"d":299,"n":2,"t":4},"speaker":"B","text":"はい。定期的に歯科医に歯を診てもらっています。"},{"id":{"d":299,"n":3,"t":4},"speaker":"A","text":"それは良い習慣ですね。私はしばらく歯科に行っていません。"},{"id":{"d":299,"n":4,"t":4},"speaker":"B","text":"私も以前は行けていなかったのですが、虫歯を放置して後悔してからは、定期的に歯科クリニックへ行くようになりました。"},{"id":{"d":299,"n":5,"t":4},"speaker":"A","text":"虫歯を放置したらどうなるのですか？"},{"id":{"d":299,"n":6,"t":4},"speaker":"B","text":"私の場合は、痛みがひどくなる程度でしたが、「もう少し放置していたら、神経を抜くことになっていた」と歯科クリニックの先生に言われました。"},{"id":{"d":299,"n":7,"t":4},"speaker":"A","text":"それは怖いですね。私も虫歯があるかどうか、診てもらった方がいいですね。"},{"id":{"d":299,"n":8,"t":4},"speaker":"B","text":"はい。定期的に診てもらえば、虫歯ができたときも、すぐに治療ができて安心です。"},{"id":{"d":299,"n":9,"t":4},"speaker":"A","text":"そうですね。私も定期検診を受けようと思います。"}];
INSERT INTO sentence [{"id":{"d":300,"n":1,"t":4},"speaker":"A","text":"昨日、キャンプをしに山奥へ行ったら、たくさん虫に刺されてしまいました。"},{"id":{"d":300,"n":2,"t":4},"speaker":"B","text":"それは災難でしたね。虫除けスプレーは使わなかったのですか？"},{"id":{"d":300,"n":3,"t":4},"speaker":"A","text":"使いました。ただ、何年も前に買ったもので、効果が薄れていたようです。"},{"id":{"d":300,"n":4,"t":4},"speaker":"B","text":"なるほど。そういうことでしたか？"},{"id":{"d":300,"n":5,"t":4},"speaker":"A","text":"はい。新しいものを買えば良かったと後悔しています。"},{"id":{"d":300,"n":6,"t":4},"speaker":"B","text":"虫に刺されたところは、かゆみや腫れがあるのですか？"},{"id":{"d":300,"n":7,"t":4},"speaker":"A","text":"幸い腫れはないのですが、かゆみはかなりあります。"},{"id":{"d":300,"n":8,"t":4},"speaker":"B","text":"かゆみ止めの薬は持っていますか？"},{"id":{"d":300,"n":9,"t":4},"speaker":"A","text":"今朝は、薬局で塗り薬を買いました。先ほど塗ったので、ひとまずかゆみは落ち着いています。"},{"id":{"d":300,"n":10,"t":4},"speaker":"B","text":"良かったです。治るまでは辛抱ですね。"}];
INSERT INTO sentence [{"id":{"d":301,"n":1,"t":4},"speaker":"A","text":"今日は、よくあくびをしていますね？"},{"id":{"d":301,"n":2,"t":4},"speaker":"B","text":"はい。眠気が覚めません。"},{"id":{"d":301,"n":3,"t":4},"speaker":"A","text":"昨夜は、寝る時間が遅かったのですか？"},{"id":{"d":301,"n":4,"t":4},"speaker":"B","text":"実は、寝ていないのです。徹夜で課題をしていました。"},{"id":{"d":301,"n":5,"t":4},"speaker":"A","text":"そういうことでしたか。一度、仮眠を取ってみたらどうですか？"},{"id":{"d":301,"n":6,"t":4},"speaker":"B","text":"そうですね。今は、ちょうど休み時間なので、昼寝をしてみます。"},{"id":{"d":301,"n":7,"t":4},"speaker":"A","text":"はい。次の授業が始まる頃に起こしに来ますね。"},{"id":{"d":301,"n":8,"t":4},"speaker":"B","text":"助かります。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":302,"n":1,"t":4},"speaker":"A","text":"背中を見てもらえますか？"},{"id":{"d":302,"n":2,"t":4},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":302,"n":3,"t":4},"speaker":"A","text":"何かできものができている気がするのですが、自分では見えないのです。"},{"id":{"d":302,"n":4,"t":4},"speaker":"B","text":"そういうことでしたか。それでは、見てみますね。"},{"id":{"d":302,"n":5,"t":4},"speaker":"A","text":"お願いします。"},{"id":{"d":302,"n":6,"t":4},"speaker":"B","text":"できものが1つできていますね。痛みやかゆみはありませんか？"},{"id":{"d":302,"n":7,"t":4},"speaker":"A","text":"痛みもかゆみもありません。念のため、皮膚科で診てもらった方がよさそうですか？"},{"id":{"d":302,"n":8,"t":4},"speaker":"B","text":"そうですね。かなり赤くなっているので、お医者さんに診てもらうと安心だと思います。"},{"id":{"d":302,"n":9,"t":4},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":303,"n":1,"t":4},"speaker":"A","text":"お父さん、寝癖がついていますよ。"},{"id":{"d":303,"n":2,"t":4},"speaker":"B","text":"本当ですね。洗面所へ行って直してきます。"},{"id":{"d":303,"n":3,"t":4},"speaker":"A","text":"ここ最近、寝癖がつきやすくなっていませんか？"},{"id":{"d":303,"n":4,"t":4},"speaker":"B","text":"はい。髪が伸びて、寝癖がつきやすくなってしまいました。"},{"id":{"d":303,"n":5,"t":4},"speaker":"A","text":"理髪店にはしばらく行っていないのですか？"},{"id":{"d":303,"n":6,"t":4},"speaker":"B","text":"前回行ったときから1カ月半は空きました。"},{"id":{"d":303,"n":7,"t":4},"speaker":"A","text":"そろそろ、理髪店へ行った方が良さそうですね。"},{"id":{"d":303,"n":8,"t":4},"speaker":"B","text":"はい。今日か明日は理髪店へ行って、散髪してきます。"},{"id":{"d":303,"n":9,"t":4},"speaker":"A","text":"それが良いと思います。"}];
INSERT INTO sentence [{"id":{"d":304,"n":1,"t":4},"speaker":"A","text":"山崎さんは、車酔いをしますか？"},{"id":{"d":304,"n":2,"t":4},"speaker":"B","text":"いいえ、私は全く車酔いをしません。"},{"id":{"d":304,"n":3,"t":4},"speaker":"A","text":"それは良いですね。私は、バスやタクシーに乗ると、毎回酔ってしまいます。"},{"id":{"d":304,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。普段の移動はどうされているのですか？"},{"id":{"d":304,"n":5,"t":4},"speaker":"A","text":"主に電車を使っています。"},{"id":{"d":304,"n":6,"t":4},"speaker":"B","text":"車は乗らないのですか？"},{"id":{"d":304,"n":7,"t":4},"speaker":"A","text":"なるべく乗らないようにしています。どうしても乗らなければならない時は、必ず乗り物の酔い止めの薬を飲みます。"},{"id":{"d":304,"n":8,"t":4},"speaker":"B","text":"そうだったのですか。それでは、一緒に旅行をするときは、電車移動にしましょう。"},{"id":{"d":304,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":305,"n":1,"t":4},"speaker":"A","text":"何だか最近は、体が怠くてやる気が起きません。"},{"id":{"d":305,"n":2,"t":4},"speaker":"B","text":"夏バテかもしれませんね。"},{"id":{"d":305,"n":3,"t":4},"speaker":"A","text":"そうですね。連日、猛暑です。"},{"id":{"d":305,"n":4,"t":4},"speaker":"B","text":"はい。暑いと、なかなか寝付けず、睡眠不足になりますよね。"},{"id":{"d":305,"n":5,"t":4},"speaker":"A","text":"そうなのです。食欲不振にもなりますが、しっかり食べなければいけませんよね。"},{"id":{"d":305,"n":6,"t":4},"speaker":"B","text":"はい。食事は大切です。私は1日3食を必ず食べるようにしています。"},{"id":{"d":305,"n":7,"t":4},"speaker":"A","text":"素晴らしい習慣です。私も食事は抜かないようにします。"},{"id":{"d":305,"n":8,"t":4},"speaker":"B","text":"そうしてください。十分な睡眠と小まめな運動、バランスの整った食事をして、元気になりましょう。"}];
INSERT INTO sentence [{"id":{"d":306,"n":1,"t":4},"speaker":"A","text":"お母さんの付けている、白いお面のようなものは何ですか？"},{"id":{"d":306,"n":2,"t":4},"speaker":"B","text":"これは、パックですよ。"},{"id":{"d":306,"n":3,"t":4},"speaker":"A","text":"何のためにしているのですか？"},{"id":{"d":306,"n":4,"t":4},"speaker":"B","text":"肌を保湿するためです。パックをすると、肌が潤うのですよ。"},{"id":{"d":306,"n":5,"t":4},"speaker":"A","text":"そうなのですね。お母さんが毎日お面のようなものを付けているので、不思議に思っていました。"},{"id":{"d":306,"n":6,"t":4},"speaker":"B","text":"日々のスキンケアには、欠かせないものなのですよ。"},{"id":{"d":306,"n":7,"t":4},"speaker":"A","text":"私も、使ってみてもよいですか？"},{"id":{"d":306,"n":8,"t":4},"speaker":"B","text":"よいですよ。あなたがお風呂から上がったら、パックを1枚渡しますね。"},{"id":{"d":306,"n":9,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":307,"n":1,"t":4},"speaker":"A","text":"本間さんは、スポーツの試合を見に行ったことはありますか？"},{"id":{"d":307,"n":2,"t":4},"speaker":"B","text":"はい。私は何度もサッカーを見に行っています。"},{"id":{"d":307,"n":3,"t":4},"speaker":"A","text":"そうなのですね。応援しているチームがあるのですか？"},{"id":{"d":307,"n":4,"t":4},"speaker":"B","text":"はい。1カ月に一度は、スタジアムに行って、応援しているチームの試合を、観戦しています。"},{"id":{"d":307,"n":5,"t":4},"speaker":"A","text":"そうなのですね。私は、サッカー観戦は一度もしたことがありません。"},{"id":{"d":307,"n":6,"t":4},"speaker":"B","text":"他のスポーツの試合を観戦したことがあるのですか？"},{"id":{"d":307,"n":7,"t":4},"speaker":"A","text":"私は、水泳の競技会を見に行ったことがあります。"},{"id":{"d":307,"n":8,"t":4},"speaker":"B","text":"水泳ですか。どうでしたか？"},{"id":{"d":307,"n":9,"t":4},"speaker":"A","text":"テレビで見るときよりも、選手の皆さんの泳ぎが速く感じます。特に、ラストスパートの追い上げは、迫力がありました。"},{"id":{"d":307,"n":10,"t":4},"speaker":"B","text":"白熱したレースを見ることができたのですね。"}];
INSERT INTO sentence [{"id":{"d":308,"n":1,"t":4},"speaker":"A","text":"吉川さんは、最近筋肉がついて体が大きくなった気がします。"},{"id":{"d":308,"n":2,"t":4},"speaker":"B","text":"うれしいです。ありがとうございます。"},{"id":{"d":308,"n":3,"t":4},"speaker":"A","text":"何かトレーニングをしているのですか？"},{"id":{"d":308,"n":4,"t":4},"speaker":"B","text":"実は、3カ月前から、ジムに通い始めたのです。そこで、トレーニングをしています。"},{"id":{"d":308,"n":5,"t":4},"speaker":"A","text":"なるほど。ジムですか。効果がしっかりと出てきますね。"},{"id":{"d":308,"n":6,"t":4},"speaker":"B","text":"最近やっと出てきました。"},{"id":{"d":308,"n":7,"t":4},"speaker":"A","text":"週にどのくらい通っているのですか？"},{"id":{"d":308,"n":8,"t":4},"speaker":"B","text":"少なくとも1回、多い時で3回は行っています。"},{"id":{"d":308,"n":9,"t":4},"speaker":"A","text":"定期的に通えているのですね。"}];
INSERT INTO sentence [{"id":{"d":309,"n":1,"t":4},"speaker":"A","text":"なつみさんのお家には、香水がたくさんありますね。"},{"id":{"d":309,"n":2,"t":4},"speaker":"B","text":"はい。私は、香水集めが趣味なのです。"},{"id":{"d":309,"n":3,"t":4},"speaker":"A","text":"昔から集めているのですか？"},{"id":{"d":309,"n":4,"t":4},"speaker":"B","text":"いえ、大学生になってからです。ここ2年くらいで集めています。"},{"id":{"d":309,"n":5,"t":4},"speaker":"A","text":"これだけたくさんあったら、1週間毎日違う香りを楽しめますね。"},{"id":{"d":309,"n":6,"t":4},"speaker":"B","text":"はい。その日の気分に合わせて、香水を変えています。"},{"id":{"d":309,"n":7,"t":4},"speaker":"A","text":"今日は、何の香りですか？"},{"id":{"d":309,"n":8,"t":4},"speaker":"B","text":"シトラスです。気分がすっきりとします。"},{"id":{"d":309,"n":9,"t":4},"speaker":"A","text":"いいですね。私も好きな香りです。"}];
INSERT INTO sentence [{"id":{"d":310,"n":1,"t":4},"speaker":"A","text":"お父さんは、ひげそりを使った後、しびれるような痛みはありますか？"},{"id":{"d":310,"n":2,"t":4},"speaker":"B","text":"いえ、ありません。たかしさんは、しびれるような痛みがあるのですか？"},{"id":{"d":310,"n":3,"t":4},"speaker":"A","text":"はい。毎回あります。"},{"id":{"d":310,"n":4,"t":4},"speaker":"B","text":"シェービングクリームは使っていますか？"},{"id":{"d":310,"n":5,"t":4},"speaker":"A","text":"いいえ、使っていません。"},{"id":{"d":310,"n":6,"t":4},"speaker":"B","text":"それでは、今度はひげそりを使うときに、シェービングクリームを肌に塗ってみてください。これまでよりも肌に負担をかけず、そることができるはずです。"},{"id":{"d":310,"n":7,"t":4},"speaker":"A","text":"シェービングクリームは、ひげそりを使う前に塗るのですか？"},{"id":{"d":310,"n":8,"t":4},"speaker":"B","text":"はい、そうです。ひげをそり終わったら、保湿クリームを塗ってください。"},{"id":{"d":310,"n":9,"t":4},"speaker":"A","text":"分かりました。さっそく試してみます。"}];
INSERT INTO sentence [{"id":{"d":311,"n":1,"t":4},"speaker":"A","text":"アイシャドウが輝いていますね。かわいいです。"},{"id":{"d":311,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。今日は、お気に入りのアイシャドウを使ってメークしました。"},{"id":{"d":311,"n":3,"t":4},"speaker":"A","text":"良く似合っています。どこで買ったのですか？"},{"id":{"d":311,"n":4,"t":4},"speaker":"B","text":"自分で買ったものではないのです。友達が、誕生日プレゼントとしてくれました。"},{"id":{"d":311,"n":5,"t":4},"speaker":"A","text":"そうなのですね。プレゼントには、化粧品を送ったりもらったりすることが多いのですか？"},{"id":{"d":311,"n":6,"t":4},"speaker":"B","text":"はい。大学生になってからは化粧品が多いです。"},{"id":{"d":311,"n":7,"t":4},"speaker":"A","text":"これまでに、どのようなものを送りましたか？"},{"id":{"d":311,"n":8,"t":4},"speaker":"B","text":"私は、リップクリームを送ることが多いです。"}];
INSERT INTO sentence [{"id":{"d":312,"n":1,"t":4},"speaker":"A","text":"最近、体の至る所が凝っているように感じたので、整体に行ってきました。"},{"id":{"d":312,"n":2,"t":4},"speaker":"B","text":"まだ一度も整体に行ったことがないので感想をぜひ教えてください。"},{"id":{"d":312,"n":3,"t":4},"speaker":"A","text":"施術を受けている時には、とても痛かったのですが、終わったら体が軽くなりました。"},{"id":{"d":312,"n":4,"t":4},"speaker":"B","text":"リラクゼーション的なボディケアだけを受けたのですか？それとも電気治療やはり治療も受けましたか？"},{"id":{"d":312,"n":5,"t":4},"speaker":"A","text":"電気治療もしてもらいました。自分の筋肉が勝手に動くのがとても面白く、少し痛かったですが笑ってしまいました。"},{"id":{"d":312,"n":6,"t":4},"speaker":"B","text":"電気治療やはり治療まですると、とても料金が高くなりませんでしたか？"},{"id":{"d":312,"n":7,"t":4},"speaker":"A","text":"はい。整体では、健康保険が利用できないので、それなりの料金になりました。"},{"id":{"d":312,"n":8,"t":4},"speaker":"B","text":"なるほど。とても興味が湧いたので私も今度近所の整体に行ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":313,"n":1,"t":4},"speaker":"A","text":"昨シーズンの大谷翔平選手の活躍を見ましたか？"},{"id":{"d":313,"n":2,"t":4},"speaker":"B","text":"はい、毎朝仕事をしながら、テレビのスポーツ番組で観戦をしていました。"},{"id":{"d":313,"n":3,"t":4},"speaker":"A","text":"ベーブ・ルース以来の二刀流で活躍していましたね。また、勝ち数はチームトップ、ホームラン数はリーグ3位という素晴らしい活躍でした。"},{"id":{"d":313,"n":4,"t":4},"speaker":"B","text":"投手としては、チームで唯一、ローテーションを守った選手でした。"},{"id":{"d":313,"n":5,"t":4},"speaker":"A","text":"アメリカンリーグの中でも上位の成績を残していたため、野手としての活躍が多く取り上げられていますが、投手としても素晴らしい活躍でしたね。"},{"id":{"d":313,"n":6,"t":4},"speaker":"B","text":"ストレートは、最速時速約164キロメートルを記録しており、スプリットも被打率が0.077という数字を残しています。"},{"id":{"d":313,"n":7,"t":4},"speaker":"A","text":"来シーズンは、開幕投手に選ばれる可能性があるという報道もあります。とても楽しみですね。"},{"id":{"d":313,"n":8,"t":4},"speaker":"B","text":"来シーズンも素晴らしい成績を期待したいと思います。"}];
INSERT INTO sentence [{"id":{"d":314,"n":1,"t":4},"speaker":"A","text":"ついに、第24回オリンピック冬季競技大会の開幕が迫ってきましたね。"},{"id":{"d":314,"n":2,"t":4},"speaker":"B","text":"そうですね。夏のオリンピックが特別に昨年開催されたため、冬季と夏季のオリンピックが日程的に近く、少し変な感じがしています。"},{"id":{"d":314,"n":3,"t":4},"speaker":"A","text":"確かに、そうですね。選手たちは、一生懸命練習をして大会に出ているので、たくさん応援したいですね。"},{"id":{"d":314,"n":4,"t":4},"speaker":"B","text":"はい。私は髙木美帆選手にとても期待しています。"},{"id":{"d":314,"n":5,"t":4},"speaker":"A","text":"スピードスケートの第一人者ですね。長距離のみならず中距離、短距離にも出場するのですよね。"},{"id":{"d":314,"n":6,"t":4},"speaker":"B","text":"テレビ番組で特集していましたが、とても理想的な滑り方をするので、海外のトップ選手も髙木選手の滑りを学んでいるそうです。"},{"id":{"d":314,"n":7,"t":4},"speaker":"A","text":"理想的な滑りだからこそ、距離を気にさせないスピードを出すことができるのですね。センスと努力のたまものですね。"},{"id":{"d":314,"n":8,"t":4},"speaker":"B","text":"他にも日本人選手はたくさん出場しますので、皆さん良い笑顔で大会を終えてほしいです。"}];
INSERT INTO sentence [{"id":{"d":315,"n":1,"t":4},"speaker":"A","text":"新型コロナウイルス感染症が流行していますが、体調は大丈夫ですか？"},{"id":{"d":315,"n":2,"t":4},"speaker":"B","text":"私は毎日元気に過ごしています。祖父母は3回目の新型コロナワクチンの接種も済ませ、安心しています。"},{"id":{"d":315,"n":3,"t":4},"speaker":"A","text":"最近は、感染力が強まった代わりに弱毒化され、重症者の増加率が減ってきていますね。"},{"id":{"d":315,"n":4,"t":4},"speaker":"B","text":"しかし、若い人はあまり重症化しないようですが、高齢者は重症化のリスクが高いようです。"},{"id":{"d":315,"n":5,"t":4},"speaker":"A","text":"高齢者はやはり心配ですね。加えて新型コロナワクチンを2回打っていない人も、重症化のリスクは高いと言われています。"},{"id":{"d":315,"n":6,"t":4},"speaker":"B","text":"やはり、新型コロナワクチンを打つことができない方以外は、できるだけ打った方がいいのでしょうか？"},{"id":{"d":315,"n":7,"t":4},"speaker":"A","text":"新型コロナワクチンを打つことで、後遺症を抑える可能性がある、という研究結果もあるそうです。"},{"id":{"d":315,"n":8,"t":4},"speaker":"B","text":"変異したとはいえ、やはり後遺症は怖いですね。日頃から予防に努めたいと思います。"}];
INSERT INTO sentence [{"id":{"d":316,"n":1,"t":4},"speaker":"A","text":"4年ほど前に、自転車で転倒し、鎖骨を折りました。"},{"id":{"d":316,"n":2,"t":4},"speaker":"B","text":"「鎖骨を折ることが一番痛い」と聞いたことがありますが、どうでしたか？"},{"id":{"d":316,"n":3,"t":4},"speaker":"A","text":"寝返りを打つことができないどころか、ベッドに背中をつくことも痛かったので、クッションを買うまでは座って寝ていました。"},{"id":{"d":316,"n":4,"t":4},"speaker":"B","text":"とても大変ですね。どのようにして治したのですか？"},{"id":{"d":316,"n":5,"t":4},"speaker":"A","text":"折った2カ月後には、合唱サークルの本番があったので、それに参加したかったことに加え、「折った場所が端すぎて自然治癒は難しい」と医師に言われ、手術をしました。"},{"id":{"d":316,"n":6,"t":4},"speaker":"B","text":"手術は大変ですね。全身麻酔ですか？"},{"id":{"d":316,"n":7,"t":4},"speaker":"A","text":"全身麻酔で手術をしました。人生で初めての、貴重な体験でした。"},{"id":{"d":316,"n":8,"t":4},"speaker":"B","text":"そのような状況でも、経験を前向きに捉えているのなら良かったです。今後は気を付けて生活してくださいね。"}];
INSERT INTO sentence [{"id":{"d":317,"n":1,"t":4},"speaker":"A","text":"冬になり、肌が乾燥するようになったので、手にハンドクリームを塗りました。"},{"id":{"d":317,"n":2,"t":4},"speaker":"B","text":"効果はどうですか？"},{"id":{"d":317,"n":3,"t":4},"speaker":"A","text":"夜に塗って、手袋をして寝ているのですが、肌の状態が2日で改善されたように感じています。"},{"id":{"d":317,"n":4,"t":4},"speaker":"B","text":"お風呂上がりにハンドクリームを塗ることで、普段塗る以上に効果があるということであれば、その方法は効果的ですね。"},{"id":{"d":317,"n":5,"t":4},"speaker":"A","text":"全く乾燥しているように見えなかった手の甲にも塗っていたら、滑らかで張りのある肌になりました。"},{"id":{"d":317,"n":6,"t":4},"speaker":"B","text":"それはいいですね。うらましさを感じる人が多いと思います。"},{"id":{"d":317,"n":7,"t":4},"speaker":"A","text":"ただ寝る時に手袋をしていても、朝起きたらいつの間にか外しています。夜どれくらいまでつけているのかは分かりません。"},{"id":{"d":317,"n":8,"t":4},"speaker":"B","text":"多分眠りについたら、手袋はすぐ外していると思います。思い込みでも改善されているなら効果ありですね。"}];
INSERT INTO sentence [{"id":{"d":318,"n":1,"t":4},"speaker":"A","text":"今年は、初の中東開催となるFIFAワールドカップカタール2022がありますね。"},{"id":{"d":318,"n":2,"t":4},"speaker":"B","text":"そうですね。中東開催ということに加えて、開催期間が冬ですので、珍しい大会ですね。"},{"id":{"d":318,"n":3,"t":4},"speaker":"A","text":"そのFIFAワールドカップアジア最終予選が現在行われています。日本は現在2位につけています。"},{"id":{"d":318,"n":4,"t":4},"speaker":"B","text":"最終予選が始まった時には連敗していたので、出場することができないのではないかと思っていましたが、5連勝し、チームとしてもまとまってきています。"},{"id":{"d":318,"n":5,"t":4},"speaker":"A","text":"今まで日本代表を引っ張ってきたベテランの選手が全盛期を過ぎ、若手が成長してきている中でベテランが一番全力でプレーしているところを見て胸が熱くなりました。"},{"id":{"d":318,"n":6,"t":4},"speaker":"B","text":"何事も実績を残してきた人が、全盛期後に背中でチームを引っ張っているのは格好良いですね。"},{"id":{"d":318,"n":7,"t":4},"speaker":"A","text":"今の若手の選手もそのようなベテランからポジションを奪うということで、気持ちがより強くなることでしょう。"},{"id":{"d":318,"n":8,"t":4},"speaker":"B","text":"チーム全体がベテランの熱さに引っ張られて、より気持ちのこもったプレーができたら、FIFAワールドカップカタール2022では良い成績を収めるかもしれません。"}];
INSERT INTO sentence [{"id":{"d":319,"n":1,"t":4},"speaker":"A","text":"マスク焼けという言葉を、知っていますか？"},{"id":{"d":319,"n":2,"t":4},"speaker":"B","text":"マスクの形に日焼けすることですよね？最近、マスクすることが多いのでよく聞きますね。"},{"id":{"d":319,"n":3,"t":4},"speaker":"A","text":"そうです。実は私もマスク焼けをしてしまいました。"},{"id":{"d":319,"n":4,"t":4},"speaker":"B","text":"マスク焼けしたのですね。それほどに外出をしていたのですか？"},{"id":{"d":319,"n":5,"t":4},"speaker":"A","text":"毎日1時間ほどウオーキングをしているので、それの影響だと思います。小さい頃から人一倍日焼けをしやすかったので、そのこともあると思います。"},{"id":{"d":319,"n":6,"t":4},"speaker":"B","text":"大変ですね。しっかり日焼け止めを塗ってくださいね。"},{"id":{"d":319,"n":7,"t":4},"speaker":"A","text":"人生で初めて日焼け止めを購入したので、しっかりと使って美白を目指したいと思います。"},{"id":{"d":319,"n":8,"t":4},"speaker":"B","text":"肌が荒れないように、気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":320,"n":1,"t":4},"speaker":"A","text":"最近、私は筋力トレーニングをしているのですが、ただ筋肉をつけるのは嫌なので自重トレーニングをしています。"},{"id":{"d":320,"n":2,"t":4},"speaker":"B","text":"自重トレーニングは、どのようなことをしているのですか？"},{"id":{"d":320,"n":3,"t":4},"speaker":"A","text":"よくいわれる腹筋運動や、プランクや、腕立て伏せ、懸垂をしています。"},{"id":{"d":320,"n":4,"t":4},"speaker":"B","text":"それらを、どれくらいの期間しているのですか？"},{"id":{"d":320,"n":5,"t":4},"speaker":"A","text":"今5カ月目です。毎日続けていると自分の体型の変化を感じることができ、とても楽しいです。"},{"id":{"d":320,"n":6,"t":4},"speaker":"B","text":"ジムに行くのではなく、自重トレーニングをする理由はなぜですか？"},{"id":{"d":320,"n":7,"t":4},"speaker":"A","text":"ジムはお金がかかることと、自分の体を一番動かしやすい状態にしておきたいので、自分の重さで鍛えるのが良いと考えています。"},{"id":{"d":320,"n":8,"t":4},"speaker":"B","text":"自分のパフォーマンスのために自重トレーニングをすることは良いことですね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":321,"n":1,"t":4},"speaker":"A","text":"ストレッチ体操をすると、けがをしにくくなるそうです。"},{"id":{"d":321,"n":2,"t":4},"speaker":"B","text":"筋肉が柔らかくなることから、柔軟な動きをすることができるようになるそうですね。"},{"id":{"d":321,"n":3,"t":4},"speaker":"A","text":"けがをするような運動をしているわけではないのですが、ジムのトレーナーから「筋肉が硬いと腰痛を発症する」と聞いたのでストレッチ体操を始めました。"},{"id":{"d":321,"n":4,"t":4},"speaker":"B","text":"腰痛に効くストレッチ体操は、やはり腰回りの筋肉をほぐすのですか？"},{"id":{"d":321,"n":5,"t":4},"speaker":"A","text":"それが、太ももの前の筋肉をほぐすそうです。"},{"id":{"d":321,"n":6,"t":4},"speaker":"B","text":"太ももの前の筋肉が硬くなると、腰が痛くなるということですか？"},{"id":{"d":321,"n":7,"t":4},"speaker":"A","text":"その通りです。腰を反らす動きをしてしまうそうで、トレーナーは「腰痛が悪化する」と言っていました。"},{"id":{"d":321,"n":8,"t":4},"speaker":"B","text":"そうなのですね。腰痛の予防のためにもストレッチ体操を、特に腰の前の筋肉を意識してしようと思います。"}];
INSERT INTO sentence [{"id":{"d":322,"n":1,"t":4},"speaker":"A","text":"あなたは、献血をしたことがありますか？"},{"id":{"d":322,"n":2,"t":4},"speaker":"B","text":"あります。毎回、「あなたは健康そうだから400立方センチメートル採ります」と言われます。"},{"id":{"d":322,"n":3,"t":4},"speaker":"A","text":"私も毎回言われます。初めて献血をした時、血液型を知らずに献血をして、その場で血液型を教えてもらったことがあります。"},{"id":{"d":322,"n":4,"t":4},"speaker":"B","text":"そのようなこともできるとは、驚きました。"},{"id":{"d":322,"n":5,"t":4},"speaker":"A","text":"私も驚きました。母や妹がO型で、父はA型だったのでどちらかであることは分かっていたのですが、採血してくれた方に「どっちだと思う？」と聞かれたのを覚えています。"},{"id":{"d":322,"n":6,"t":4},"speaker":"B","text":"とても楽しそうな献血ですね。結局どちらでしたか？"},{"id":{"d":322,"n":7,"t":4},"speaker":"A","text":"蚊に刺されやすいことや、性格を考えてO型だと思っていたのですがA型でした。どちらにしても知ることができて良かったです。"},{"id":{"d":322,"n":8,"t":4},"speaker":"B","text":"そうですね。通常は血液検査をしないと、血液型を調べることができないので、無料で調べることができて良かったですね。"}];
INSERT INTO sentence [{"id":{"d":323,"n":1,"t":4},"speaker":"A","text":"花粉症から、果物アレルギーになることを知っていますか？"},{"id":{"d":323,"n":2,"t":4},"speaker":"B","text":"知っています。バラ科アレルギーなどを発症するというものですね。"},{"id":{"d":323,"n":3,"t":4},"speaker":"A","text":"私はまさに花粉症から果物アレルギーになりました。元々イチゴなどバラ科の食べ物が大好きだったので、とても悲しかったです。"},{"id":{"d":323,"n":4,"t":4},"speaker":"B","text":"いまだに食べることができていないのですか？"},{"id":{"d":323,"n":5,"t":4},"speaker":"A","text":"いいえ。去年から花粉症が全く発症せず、チャレンジとして、今年果物を食べたところ、少しずつ食べていたら全くアレルギーが出ませんでした。"},{"id":{"d":323,"n":6,"t":4},"speaker":"B","text":"つまり、花粉症が発症しないと、果物アレルギーも発症しないということですか？"},{"id":{"d":323,"n":7,"t":4},"speaker":"A","text":"はい。詳しいことは分かりませんが、私は花粉症が発症しないと果物アレルギーも発症しませんでした。"},{"id":{"d":323,"n":8,"t":4},"speaker":"B","text":"何か関係性がありそうですが、アレルギー検査をして「果物アレルギーは続いている」と言われたら悲しくなりそうなので検査したくありませんね。"}];
INSERT INTO sentence [{"id":{"d":324,"n":1,"t":4},"speaker":"A","text":"あるプロサッカー選手が、「子どもが将来どのようなスポーツをするにしても、体操はやらせる」と言っていました。"},{"id":{"d":324,"n":2,"t":4},"speaker":"B","text":"どのようなスポーツにも共通する動きが、体操にはあるということでしょうか？"},{"id":{"d":324,"n":3,"t":4},"speaker":"A","text":"「体操では体幹を鍛えることができる」と言っていました。確かに体操をする時に、軸がぶれていたら何もできませんね。"},{"id":{"d":324,"n":4,"t":4},"speaker":"B","text":"体操で体幹を鍛えることができることは理解できましたが、なぜどのスポーツでも必要なのでしょうか？"},{"id":{"d":324,"n":5,"t":4},"speaker":"A","text":"体幹がしっかりしていれば、自分で体を支えることが可能になります。もちろん体幹がしっかりしていなくても支えられますが、より安定して支えることが可能です。"},{"id":{"d":324,"n":6,"t":4},"speaker":"B","text":"そうすると、野球ではバッティングが安定し、投げる動作もどの体勢にあっても、投げることが可能になり、サッカーでも、当たり負けをしないということですね。"},{"id":{"d":324,"n":7,"t":4},"speaker":"A","text":"その通りです。加えて私は柔軟性も得ることができ、けがも減ると考えます。"},{"id":{"d":324,"n":8,"t":4},"speaker":"B","text":"体操教室はあまり見たことがありませんが、そのような話を聞いたら、通う子どもが増えそうですね。"}];
INSERT INTO sentence [{"id":{"d":325,"n":1,"t":4},"speaker":"A","text":"あるテレビ番組で解説していましたが、乾燥肌の方は入浴をすると良いそうです。"},{"id":{"d":325,"n":2,"t":4},"speaker":"B","text":"汗をかくと、体内の水分が減りそうな気がしますが、汗をかくことが良いのですか？"},{"id":{"d":325,"n":3,"t":4},"speaker":"A","text":"はい、汗の中にはさまざまな成分が含まれており、「天然美容液」とテレビ番組では言っていました。"},{"id":{"d":325,"n":4,"t":4},"speaker":"B","text":"成分が美容液のようなもの、ということでしょうか。それなら汗をかくことは推奨されそうですね。"},{"id":{"d":325,"n":5,"t":4},"speaker":"A","text":"その通りです。汗をかくためには湯船に漬かる、運動をする、温かいものを食べることが良いようです。"},{"id":{"d":325,"n":6,"t":4},"speaker":"B","text":"そのような習慣を付けることで汗をかき、保湿されるということですね。"},{"id":{"d":325,"n":7,"t":4},"speaker":"A","text":"脱水症状にならない程度に汗をかいて、天然の美容液を使って乾燥肌を防止しましょう。"},{"id":{"d":325,"n":8,"t":4},"speaker":"B","text":"今日からしっかりと湯船に漬かり、簡単な運動も心掛けたいと思います。"}];
INSERT INTO sentence [{"id":{"d":326,"n":1,"t":4},"speaker":"A","text":"最近になって、平日の朝にラジオ体操を始めました。"},{"id":{"d":326,"n":2,"t":4},"speaker":"B","text":"何か、始めるきっかけになることがあったのですか？"},{"id":{"d":326,"n":3,"t":4},"speaker":"A","text":"毎日の朝食後に何もしない時間があり、とてももったいないと思い、1日の活動を開始するという意味でも、ラジオ体操を始めました。"},{"id":{"d":326,"n":4,"t":4},"speaker":"B","text":"朝の時間を有効活用していることはとても良いことですね。朝から活動をすることで、充実した1日を過ごすことができそうです。"},{"id":{"d":326,"n":5,"t":4},"speaker":"A","text":"朝から活動することで、夜更かしすることがなくなり、早寝早起きのサイクルができるようになりました。"},{"id":{"d":326,"n":6,"t":4},"speaker":"B","text":"すごく健康的な日々を過ごしていますね。ラジオ体操によって、早寝早起きのサイクルが守られているのですね。"},{"id":{"d":326,"n":7,"t":4},"speaker":"A","text":"そうですね。ルーティンであることに加えて、ラジオ体操は難しくなく、簡単な動きのため続けることができていると考えています。"},{"id":{"d":326,"n":8,"t":4},"speaker":"B","text":"簡単な動きでも、朝に体を起こすという意味では、とても良い運動ですね。"}];
INSERT INTO sentence [{"id":{"d":327,"n":1,"t":4},"speaker":"A","text":"母が、老眼鏡を購入してきました。"},{"id":{"d":327,"n":2,"t":4},"speaker":"B","text":"視力が良い人ほど、老眼になるのが早いといいますよね。"},{"id":{"d":327,"n":3,"t":4},"speaker":"A","text":"そうですね。私は視力がとても良いので、親から「老眼が早く来る」とずっと言われています。"},{"id":{"d":327,"n":4,"t":4},"speaker":"B","text":"今とても視力が良いなら、一般には40歳くらいから老眼が来るといわれますが、それよりも早く老眼が来るかもしれませんね。"},{"id":{"d":327,"n":5,"t":4},"speaker":"A","text":"小さい頃は、目が良いと老眼が早く来るなら、目が悪くなればいいと思い、暗い部屋の中でテレビを見たり、画面の近くでテレビを見たりしていました。"},{"id":{"d":327,"n":6,"t":4},"speaker":"B","text":"かわいい発想ですが、目は悪くない方がいいので、そのようなことをしない方が良かったかもしれません。結果として、目は悪くなったのですか？"},{"id":{"d":327,"n":7,"t":4},"speaker":"A","text":"全く悪くなりませんでした。視力検査では両目ともにずっと1.5でした。"},{"id":{"d":327,"n":8,"t":4},"speaker":"B","text":"目が悪い人からしたら、視力が良いのはとてもうらやましいことですね。目を大切にしてください。"}];
INSERT INTO sentence [{"id":{"d":328,"n":1,"t":4},"speaker":"A","text":"北京2022オリンピック競技大会で、スキージャンプ男子ノーマルヒルの日本選手が24年ぶりに金メダルを獲得しました。"},{"id":{"d":328,"n":2,"t":4},"speaker":"B","text":"テレビ番組で生中継を見ていましたが、圧倒的な強さで見ていて面白かったです。"},{"id":{"d":328,"n":3,"t":4},"speaker":"A","text":"ジャンプは風向きや風速も関係しているとても難しい競技のようですが、安定したジャンプをすることができるのはさすがですね。"},{"id":{"d":328,"n":4,"t":4},"speaker":"B","text":"決勝の1回目は、前半に飛んだ人の方が風は有利だったようです。その中でも2位に6ポイントも差をつけて1回目から1位だったことは、「素晴らしい」の一言に尽きます。"},{"id":{"d":328,"n":5,"t":4},"speaker":"A","text":"今季のスキージャンプ・ワールドカップの好成績から日本国内のみでなく、さまざまな人からとても期待され、プレッシャーがとても大きい中でも、自分の実力を発揮したことはさすがプロと思わせられました。"},{"id":{"d":328,"n":6,"t":4},"speaker":"B","text":"この後は団体戦も控えているようなので、団体戦でも好成績を期待したいですね。"},{"id":{"d":328,"n":7,"t":4},"speaker":"A","text":"個人戦での疲れや、プレッシャーを一度リセットして最高の状態で挑んでほしいです。"},{"id":{"d":328,"n":8,"t":4},"speaker":"B","text":"そうですね。テレビ番組を見て、しっかり応援しましょう。"}];
INSERT INTO sentence [{"id":{"d":329,"n":1,"t":4},"speaker":"A","text":"5年ほど前までは、この時期になると、インフルエンザが流行していましたね。"},{"id":{"d":329,"n":2,"t":4},"speaker":"B","text":"インフルエンザで学級閉鎖になることも多々ありましたが、最近はありませんね。"},{"id":{"d":329,"n":3,"t":4},"speaker":"A","text":"最近変化したことは、全員しっかりマスクをして、手洗い、うがいをするようになったことだと思います。"},{"id":{"d":329,"n":4,"t":4},"speaker":"B","text":"そうですね。インフルエンザはマスク、手洗い、うがいで予防できるということですね。"},{"id":{"d":329,"n":5,"t":4},"speaker":"A","text":"今後いつまで毎日マスクをつける生活になるのか分かりませんが、インフルエンザがはびこる季節は、今のようにマスクを毎日した方がいいですね。"},{"id":{"d":329,"n":6,"t":4},"speaker":"B","text":"ええ。小さな子どもは、手洗いやうがいをすることを面倒に思うことも多いです。でも、そうしなければいけない理由を一緒に伝えることで、行動することの大切さをわかってくれると思います。"},{"id":{"d":329,"n":7,"t":4},"speaker":"A","text":"その通りですね。小さい頃からインフルエンザの季節でなくても、毎日、手洗い、うがいをしっかり習慣付けることで、感染症予防になることを教えることが大切です。"},{"id":{"d":329,"n":8,"t":4},"speaker":"B","text":"毎日するべきことを習慣としてできるよう、声を掛けていきましょう。"}];
INSERT INTO sentence [{"id":{"d":330,"n":1,"t":4},"speaker":"A","text":"あなたは、プラセボ効果を知っていますか？"},{"id":{"d":330,"n":2,"t":4},"speaker":"B","text":"はい、知っています。偽薬といわれるものですね。実際に有効成分が入っていない薬を投与することです。"},{"id":{"d":330,"n":3,"t":4},"speaker":"A","text":"その通りです。ワクチン接種による副反応の検査として、プラセボをした研究があるそうです。"},{"id":{"d":330,"n":4,"t":4},"speaker":"B","text":"さすがに副反応は、体の拒絶反応なので、異なる反応をするのではありませんか？"},{"id":{"d":330,"n":5,"t":4},"speaker":"A","text":"それが同じ反応になったようです。発熱、注射箇所の痛み、腫れが実際にあったようです。"},{"id":{"d":330,"n":6,"t":4},"speaker":"B","text":"すると副反応は、気持ち次第で出ない可能性もあるということですか？"},{"id":{"d":330,"n":7,"t":4},"speaker":"A","text":"可能性はありますね。思い込んで副反応を出すことができるわけなので、副反応はないと思い込めば副反応は無くなる可能性があります。"},{"id":{"d":330,"n":8,"t":4},"speaker":"B","text":"副反応を言い訳にして、注射を打っていない人にぜひ聞かせてあげたい研究ですね。"}];
INSERT INTO sentence [{"id":{"d":331,"n":1,"t":4},"speaker":"A","text":"あなたには、いくつつむじがありますか？"},{"id":{"d":331,"n":2,"t":4},"speaker":"B","text":"私は、つむじが1つ頭頂部にあります。"},{"id":{"d":331,"n":3,"t":4},"speaker":"A","text":"私は、頭頂部につむじが2つあります。つむじが1つの人がとてもうらやましいです。"},{"id":{"d":331,"n":4,"t":4},"speaker":"B","text":"つむじが2つの人は天才や頑固者、薄毛になりやすいとさまざまなうわさがありますね。"},{"id":{"d":331,"n":5,"t":4},"speaker":"A","text":"天才とだけいわれていたら、まだ救われました。私はつむじが2つあることに加え、逆回転をしているので髪の毛のセットが大変です。"},{"id":{"d":331,"n":6,"t":4},"speaker":"B","text":"つむじが逆回転していると、どのようになるのですか？"},{"id":{"d":331,"n":7,"t":4},"speaker":"A","text":"頭頂部の真ん中に髪の毛が集まるので、どのようにしても真ん中の髪の毛が立ってしまいます。そのため必ず整髪料を用いて、真ん中だけではなく全体的に立たせる必要があります。"},{"id":{"d":331,"n":8,"t":4},"speaker":"B","text":"ヘアセットに時間がかかるのはとても大変ですね。"}];
INSERT INTO sentence [{"id":{"d":332,"n":1,"t":4},"speaker":"A","text":"フィギュアスケートでネイサン・チェン選手が世界最高得点を出しました。"},{"id":{"d":332,"n":2,"t":4},"speaker":"B","text":"北京2022オリンピック競技大会という舞台で、最高の演技をすることができるのは素晴らしいですね。"},{"id":{"d":332,"n":3,"t":4},"speaker":"A","text":"それまでも素晴らしい選手だといわれていましたが、大舞台で力を発揮したことから、より素晴らしい選手であることを証明しましたね。"},{"id":{"d":332,"n":4,"t":4},"speaker":"B","text":"一方で、元々世界最高得点を持っていた羽生結弦選手は、ジャンプのタイミングでスケート靴が穴にはまってしまい、跳ぶことができませんでした。"},{"id":{"d":332,"n":5,"t":4},"speaker":"A","text":"世界初の4回転半を披露する予定でしたが、そのタイミングで穴にはまってしまいましたね。"},{"id":{"d":332,"n":6,"t":4},"speaker":"B","text":"試合後のインタビューでも「氷に嫌われた」と言っていましたね。"},{"id":{"d":332,"n":7,"t":4},"speaker":"A","text":"順位も8位でしたが、フリーの演技で挽回してほしいです。"},{"id":{"d":332,"n":8,"t":4},"speaker":"B","text":"1位になって3連覇してほしいですね。"}];
INSERT INTO sentence [{"id":{"d":333,"n":1,"t":4},"speaker":"A","text":"男子スノーボードハーフパイプで日本選手が4人全員決勝へ進みました。"},{"id":{"d":333,"n":2,"t":4},"speaker":"B","text":"北京2022オリンピック競技大会に出場することもすごいことなのに、北京2022オリンピック競技大会の舞台で、全員が決勝進出するのはとてもすごいことですね。"},{"id":{"d":333,"n":3,"t":4},"speaker":"A","text":"予選は2回滑りましたが、1回目は全員が必ず得点を残すために、安全な滑りをしていました。"},{"id":{"d":333,"n":4,"t":4},"speaker":"B","text":"明らかに攻めていない、安全に得点を残すための滑りをしているのにもかかわらず、各選手がとても高得点を残していて日本のレベルの高さを感じました。"},{"id":{"d":333,"n":5,"t":4},"speaker":"A","text":"日本選手全員が決勝へ残ることが可能な順位を取った上で、2回目の試技に入りました。"},{"id":{"d":333,"n":6,"t":4},"speaker":"B","text":"2回目は、やはり平野歩夢選手が、圧倒的な力を発揮していましたね。"},{"id":{"d":333,"n":7,"t":4},"speaker":"A","text":"他の選手も、もちろん完成度は高かったのですが、素人が見ても1人だけ完成度が違う圧倒的な滑りでした。"},{"id":{"d":333,"n":8,"t":4},"speaker":"B","text":"あさってに行われる決勝も楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":334,"n":1,"t":4},"speaker":"A","text":"最近、私はウオーキングに夢中になっています。"},{"id":{"d":334,"n":2,"t":4},"speaker":"B","text":"とても良いことですね。1日何歩、何キロメートルほど歩いているのですか？"},{"id":{"d":334,"n":3,"t":4},"speaker":"A","text":"1日平均して約15,000歩、距離にすると10キロメートルほど歩いています。"},{"id":{"d":334,"n":4,"t":4},"speaker":"B","text":"とても歩いていますね。1回でそれほど歩いているのですか？"},{"id":{"d":334,"n":5,"t":4},"speaker":"A","text":"朝に7キロメートルほど、勉強に疲れたら午後に好きなだけ歩くようにしています。"},{"id":{"d":334,"n":6,"t":4},"speaker":"B","text":"とても良い習慣ですね。見習いたいです。"},{"id":{"d":334,"n":7,"t":4},"speaker":"A","text":"朝のウオーキングは1キロメートル8分10秒ほどで歩いているので、相当速く、毎日良い朝活動をしています。"},{"id":{"d":334,"n":8,"t":4},"speaker":"B","text":"良い朝活動をすると、良い1日が過ごせるので私も試してみます。"}];
INSERT INTO sentence [{"id":{"d":335,"n":1,"t":4},"speaker":"A","text":"成人男性の歩くペースの平均を知っていますか？"},{"id":{"d":335,"n":2,"t":4},"speaker":"B","text":"知りません。どれくらいですか？"},{"id":{"d":335,"n":3,"t":4},"speaker":"A","text":"1キロメートルで約12分半だそうです。何か話しながら歩いているペースなのかと思うくらい、遅いなと感じます。"},{"id":{"d":335,"n":4,"t":4},"speaker":"B","text":"あなたは、1キロメートルを、どれくらいで歩くのですか？"},{"id":{"d":335,"n":5,"t":4},"speaker":"A","text":"私は最近8分10秒で歩いています。調子が良いときは8分を少し切るくらいで歩きます。"},{"id":{"d":335,"n":6,"t":4},"speaker":"B","text":"とても速いですね。一般男性よりも4分半も速いです。"},{"id":{"d":335,"n":7,"t":4},"speaker":"A","text":"歩くスペシャリストである競歩選手は1キロメートルを3分半で歩くそうです。"},{"id":{"d":335,"n":8,"t":4},"speaker":"B","text":"とても速いですね。ただ競歩選手とのペースの差と、一般男性との差がほとんど同じですね。頑張れば競歩選手くらい速く歩けるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":336,"n":1,"t":4},"speaker":"A","text":"最近は、女性だけでなく、男性も脱毛をしている人が増えています。"},{"id":{"d":336,"n":2,"t":4},"speaker":"B","text":"男性の間でも、脱毛への関心が高まっているのですね。"},{"id":{"d":336,"n":3,"t":4},"speaker":"A","text":"全身脱毛まではしなくても、清潔感がある人は、好ましく思われるでしょうね。"},{"id":{"d":336,"n":4,"t":4},"speaker":"B","text":"清潔感のある男性の方が、好印象であることは、昔から変わらないことですね。"}];
INSERT INTO sentence [{"id":{"d":337,"n":1,"t":4},"speaker":"A","text":"腰がとても痛かったため、体幹トレーニングを始めたところ、腰の痛みがなくなりました。"},{"id":{"d":337,"n":2,"t":4},"speaker":"B","text":"サッカー選手の長友佑都選手も、昔ヘルニアを発症したときに体幹を鍛えたそうです。"},{"id":{"d":337,"n":3,"t":4},"speaker":"A","text":"私もその長友選手の発言を知っています。私は多分ヘルニアではないと思いますが、体幹を半年ほど鍛えたら、どのような痛みだったかを忘れてしまうくらい、腰が楽になりました。"},{"id":{"d":337,"n":4,"t":4},"speaker":"B","text":"体幹トレーニングをすることで、整体や整形外科に通うことなく、痛みが引くのはとてもいいことです。"},{"id":{"d":337,"n":5,"t":4},"speaker":"A","text":"以前、整体に行った時、「腰の痛みを治すには10万円かかる」と言われました。そうであれば、体幹トレーニングは10万円の価値があるともいえるでしょうか？"},{"id":{"d":337,"n":6,"t":4},"speaker":"B","text":"提示された金額が少し高い気がしますが、それでもお金をかけずにトレーニングで治したのは素晴らしいです。"},{"id":{"d":337,"n":7,"t":4},"speaker":"A","text":"体幹を鍛えたおかげで姿勢も良くなり、歩行姿勢もきれいになりました。基礎代謝も上がったのでいいこと尽くしでした。"},{"id":{"d":337,"n":8,"t":4},"speaker":"B","text":"体幹を鍛えると、良いことがたくさんあるのですね。"}];
INSERT INTO sentence [{"id":{"d":338,"n":1,"t":4},"speaker":"A","text":"Apple Watchを知っていますか？"},{"id":{"d":338,"n":2,"t":4},"speaker":"B","text":"存在は知っていますが、持っていません。"},{"id":{"d":338,"n":3,"t":4},"speaker":"A","text":"私は持っているのですが、とても機能が素晴らしいのでお薦めします。"},{"id":{"d":338,"n":4,"t":4},"speaker":"B","text":"どのようなところが素晴らしいのですか？"},{"id":{"d":338,"n":5,"t":4},"speaker":"A","text":"まず自分の1日の運動を記録してくれることがあります。消費カロリーや、運動した時間、立っていた時間などを、見て簡単に分かるようにしてくれます。"},{"id":{"d":338,"n":6,"t":4},"speaker":"B","text":"それは、運動をするモチベーションが上がりますね。他には、どのような機能が付いていますか？"},{"id":{"d":338,"n":7,"t":4},"speaker":"A","text":"はい。Apple Watchを買ってから、運動が楽しくなりました。"}];
INSERT INTO sentence [{"id":{"d":339,"n":1,"t":4},"speaker":"A","text":"久しぶりですね。今日はどうしましたか？"},{"id":{"d":339,"n":2,"t":4},"speaker":"B","text":"花粉症がひどいので、目薬を出していただけますか？"},{"id":{"d":339,"n":3,"t":4},"speaker":"A","text":"前回処方した目薬で大丈夫ですか？何か他の目薬に変えますか？"},{"id":{"d":339,"n":4,"t":4},"speaker":"B","text":"前回の目薬で大丈夫なのですが、少し症状が重い時があるので、そうした時に使える目薬も出していただけますか？"},{"id":{"d":339,"n":5,"t":4},"speaker":"A","text":"それでは2種類処方しますね。こちらは症状が重い時に使用してください。"},{"id":{"d":339,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。それから、来週、再来週は予定があるため、病院へ来ることができないので、少し多めに出していただくことはできますか？"},{"id":{"d":339,"n":7,"t":4},"speaker":"A","text":"分かりました。それでは5本ずつ処方しますね。次は薬がなくなったら来院してください。"},{"id":{"d":339,"n":8,"t":4},"speaker":"B","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":340,"n":1,"t":4},"speaker":"A","text":"ビタミン剤をかんだことはありますか？"},{"id":{"d":340,"n":2,"t":4},"speaker":"B","text":"全くありません。お兄さんはかんだことがあるのですか？"},{"id":{"d":340,"n":3,"t":4},"speaker":"A","text":"この間、少し興味を持ったのでかんでみました。とても苦くて、本当に後悔しました。"},{"id":{"d":340,"n":4,"t":4},"speaker":"B","text":"私は、かもうと思ったことはありません。"},{"id":{"d":340,"n":5,"t":4},"speaker":"A","text":"興味があったのでつい出来心で、してしまいました。"},{"id":{"d":340,"n":6,"t":4},"speaker":"B","text":"家にはいくつかのビタミン剤がありますが、どの種類のビタミン剤をかんだのですか？"},{"id":{"d":340,"n":7,"t":4},"speaker":"A","text":"ブルーベリー配合のものをかみました。口の中が紫色になって、とても焦りました。"},{"id":{"d":340,"n":8,"t":4},"speaker":"B","text":"その時の紫の口の中を、少し見てみたかったです。"}];
INSERT INTO sentence [{"id":{"d":341,"n":1,"t":4},"speaker":"A","text":"北京2022オリンピック競技大会スノーボード男子ハーフパイプの決勝がありました。見事に平野歩夢選手が優勝しました。"},{"id":{"d":341,"n":2,"t":4},"speaker":"B","text":"私も生で観戦していましたが、素晴らしい滑りでした。"},{"id":{"d":341,"n":3,"t":4},"speaker":"A","text":"惜しくも平野歩夢選手以外の日本人選手は、メダルこそ逃しましたが、全員素晴らしい滑りでしたね。"},{"id":{"d":341,"n":4,"t":4},"speaker":"B","text":"そうですね。その中でも優勝した平野歩夢選手は北京2022オリンピック競技大会で初めての大技を決め、滑り切りました。"},{"id":{"d":341,"n":5,"t":4},"speaker":"A","text":"3回の滑走全てを成功させ、1回目は惜しくも途中で転んでしまいましたが、2回目、3回目は滑り切りましたね。"},{"id":{"d":341,"n":6,"t":4},"speaker":"B","text":"世界初の大技を決めて滑走するという快挙でしたが、2回目の得点はあまり伸びず2位でした。これには平野歩夢選手も「信じられなかった」と言っていましたね。"},{"id":{"d":341,"n":7,"t":4},"speaker":"A","text":"「得点をしっかり出してくれなかったことへの怒りを力にして、最後の3回目を滑った」といった内容のことを言っていました。とても気持ちが強いですね。"},{"id":{"d":341,"n":8,"t":4},"speaker":"B","text":"怒りを力にして滑走した3回目は、2回目よりも高さ、余裕の全てが上回っていて、安心して見ていられました。"}];
INSERT INTO sentence [{"id":{"d":342,"n":1,"t":4},"speaker":"A","text":"いらっしゃいませ。当店は初めてですか？"},{"id":{"d":342,"n":2,"t":4},"speaker":"B","text":"初めてです。"},{"id":{"d":342,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。全身マッサージも可能ですし、特に疲れている部分がありましたら、そこを重点的にほぐしますが、どのようにいたしますか？"},{"id":{"d":342,"n":4,"t":4},"speaker":"B","text":"荷物が重たくて、肩がとても凝っているので肩と、革靴で歩き回って、足が疲れているので足をお願いします。"},{"id":{"d":342,"n":5,"t":4},"speaker":"A","text":"かしこまりました。何か他に追加でマッサージする箇所や、質問などはありますか？"},{"id":{"d":342,"n":6,"t":4},"speaker":"B","text":"追加でマッサージは結構です。質問ですが、合計で支払いは、いくらくらいになりますか？"},{"id":{"d":342,"n":7,"t":4},"speaker":"A","text":"はい、3,000円頂戴いたします。マッサージ終了後にお支払いとなりますので、まずは上着を脱いでいただいてよろしいですか？"},{"id":{"d":342,"n":8,"t":4},"speaker":"B","text":"分かりました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":343,"n":1,"t":4},"speaker":"A","text":"運動する前に、しっかり準備体操はしましたか？"},{"id":{"d":343,"n":2,"t":4},"speaker":"B","text":"あまりしていませんが、早く練習をしたいです。"},{"id":{"d":343,"n":3,"t":4},"speaker":"A","text":"準備体操をしてないならば、練習をさせることはできません。"},{"id":{"d":343,"n":4,"t":4},"speaker":"B","text":"なぜですか？練習をたくさんした方がうまくなると思います。"},{"id":{"d":343,"n":5,"t":4},"speaker":"A","text":"練習をたくさんした方がうまくなると私も思います。ただ、かつて私は準備体操をしないで練習をしたところ肉離れになってしまい、2カ月間練習できなかったことがあるので、このような助言をしています。"},{"id":{"d":343,"n":6,"t":4},"speaker":"B","text":"福原コーチが、2カ月も練習できなかったのですか？"},{"id":{"d":343,"n":7,"t":4},"speaker":"A","text":"そうです。15分の準備体操をしてけがをしないで済ませるか、15分の準備体操を怠ったためにけがをして2カ月間練習できなくなるのか、どちらがいいですか？"},{"id":{"d":343,"n":8,"t":4},"speaker":"B","text":"今準備体操をして、けがをしない方がいいです。今から、しっかり準備体操します。"}];
INSERT INTO sentence [{"id":{"d":344,"n":1,"t":4},"speaker":"A","text":"スキージャンプ男子ワールドカップ個人で小林陵侑選手が第2位になりましたね。"},{"id":{"d":344,"n":2,"t":4},"speaker":"B","text":"ライブで見ましたが、素晴らしい活躍でした。1回目は142メートルも飛んでとても格好よかったですね。"},{"id":{"d":344,"n":3,"t":4},"speaker":"A","text":"葛西紀明監督も「素晴らしい飛行でした」とおっしゃっていました。そのような飛行を見せてくれたことだけでも、私たちは感謝するべきですね。"},{"id":{"d":344,"n":4,"t":4},"speaker":"B","text":"最後のジャンプが終わった後に、日本の仲間が駆け付けてきましたが、結果が出たときに落ち込んでいたことが印象に残っています。"},{"id":{"d":344,"n":5,"t":4},"speaker":"A","text":"しかし、小林選手はとても満足そうな顔をしていました。やり切ったという顔だったのでしょうか？"},{"id":{"d":344,"n":6,"t":4},"speaker":"B","text":"1回目に完璧な飛行を、2回目も素晴らしい飛行を見せたので、自分自身でも納得できる結果だったのでしょう。"},{"id":{"d":344,"n":7,"t":4},"speaker":"A","text":"十分素晴らしい結果だったので、胸を張ってほしいですね。"},{"id":{"d":344,"n":8,"t":4},"speaker":"B","text":"そうですね。残りの団体戦も良い結果が出せるように期待したいです。"}];
INSERT INTO sentence [{"id":{"d":345,"n":1,"t":4},"speaker":"A","text":"最近おなかがすくのが、早くなっている気がします。"},{"id":{"d":345,"n":2,"t":4},"speaker":"B","text":"食べる量が減っている、ということはありませんか？"},{"id":{"d":345,"n":3,"t":4},"speaker":"A","text":"全くありません。むしろよく間食をしているので、おなかがすきにくくなるはずです。"},{"id":{"d":345,"n":4,"t":4},"speaker":"B","text":"体重に変化はありますか？"},{"id":{"d":345,"n":5,"t":4},"speaker":"A","text":"何も変化はありません。少し軽くなっているような気はしますが、誤差の範囲だと思います。"},{"id":{"d":345,"n":6,"t":4},"speaker":"B","text":"消化が早くなったのかもしれませんね。基礎代謝が上がったのだとしたら、むしろ健康的ですね。"}];
INSERT INTO sentence [{"id":{"d":346,"n":1,"t":4},"speaker":"A","text":"先週から、新しい革靴を履いているのですが、靴擦れが治りません。"},{"id":{"d":346,"n":2,"t":4},"speaker":"B","text":"新しい靴は、靴擦れをしやすいので仕方ないと思います。ばんそうこうは貼っていますか？"},{"id":{"d":346,"n":3,"t":4},"speaker":"A","text":"二重に貼っています。そのおかげで足が靴の中で前の方に出てしまい、今度は爪先が痛いです。"},{"id":{"d":346,"n":4,"t":4},"speaker":"B","text":"ばんそうこうを、二重に貼る必要はないと思います。1枚だけ貼って、もしも、ずれてしまったら新しいばんそうこうを貼れば良いと思います。"}];
INSERT INTO sentence [{"id":{"d":347,"n":1,"t":4},"speaker":"A","text":"今日の放課後は4時からいつもの公園で野球をしましょう。"},{"id":{"d":347,"n":2,"t":4},"speaker":"B","text":"分かりました。バットとグローブを忘れないように気を付けますね。"},{"id":{"d":347,"n":3,"t":4},"speaker":"A","text":"今日は何人遊びに来ますか？"},{"id":{"d":347,"n":4,"t":4},"speaker":"B","text":"今のところ、14人来る予定です。7対7で試合ができますね。"},{"id":{"d":347,"n":5,"t":4},"speaker":"A","text":"チーム分けは、どうしますか？確か、前回遊んだ時は、チームを決めるために15分くらいかかっていた気がします。"},{"id":{"d":347,"n":6,"t":4},"speaker":"B","text":"今日の掃除の時間に、くじ引きをしてチームを決めましょう。"},{"id":{"d":347,"n":7,"t":4},"speaker":"A","text":"くじ引きということは、決まったチームの内容については、絶対文句は言わないようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":348,"n":1,"t":4},"speaker":"A","text":"現在、アラブ首長国連邦でサッカーのFIFAクラブワールドカップUAE2021が開催されています。"},{"id":{"d":348,"n":2,"t":4},"speaker":"B","text":"そうなのですね。知りませんでした。注目するべきチームはどこですか？"},{"id":{"d":348,"n":3,"t":4},"speaker":"A","text":"やはりヨーロッパ王者のチェルシー・フットボールクラブでしょう。最近リーグ戦では調子を落としていますが、やはり強いので圧倒的な力を見せてくれると思います。"},{"id":{"d":348,"n":4,"t":4},"speaker":"B","text":"それではチェルシー・フットボールクラブに注目しながら、観戦したいと思います。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":349,"n":1,"t":4},"speaker":"A","text":"処方箋の電子化が検討されています。電子化が運用されたら、処方箋は紙ではなくデータでやりとりをします。"},{"id":{"d":349,"n":2,"t":4},"speaker":"B","text":"処方箋をデータでやりとりするとはどういうことですか？"},{"id":{"d":349,"n":3,"t":4},"speaker":"A","text":"医療機関で作成した処方箋を、紙ではなく電子データで薬局の薬剤師が確認できるようになります。"},{"id":{"d":349,"n":4,"t":4},"speaker":"B","text":"患者は、どのように薬を処方していただけるのですか？"},{"id":{"d":349,"n":5,"t":4},"speaker":"A","text":"薬局に本人確認可能なものを持参していただければ、結構です。"},{"id":{"d":349,"n":6,"t":4},"speaker":"B","text":"いつから、処方箋の電子化が行われるのですか？"},{"id":{"d":349,"n":7,"t":4},"speaker":"A","text":"令和5年1月より運用が開始される予定だそうです。"}];
INSERT INTO sentence [{"id":{"d":350,"n":1,"t":4},"speaker":"A","text":"本日、北京2022オリンピック競技大会ではメダルラッシュでしたね。"},{"id":{"d":350,"n":2,"t":4},"speaker":"B","text":"素晴らしい活躍でしたね。全てテレビでチェックしていたので感動しました。"},{"id":{"d":350,"n":3,"t":4},"speaker":"A","text":"まずは高木美帆選手が金メダルを獲得しましたね。"},{"id":{"d":350,"n":4,"t":4},"speaker":"B","text":"なんと北京2022オリンピック競技大会で7回目の滑走だったそうです。オリンピックに出場するだけでも素晴らしいですが、7回も滑るとは体力的にも大変だと思います。"},{"id":{"d":350,"n":5,"t":4},"speaker":"A","text":"加えてオリンピック新記録だったそうですね。"},{"id":{"d":350,"n":6,"t":4},"speaker":"B","text":"オリンピック最後の滑走で新記録を達成するとは本当に素晴らしいですね。"},{"id":{"d":350,"n":7,"t":4},"speaker":"A","text":"ノルディック複合は銅メダルでしたね。"},{"id":{"d":350,"n":8,"t":4},"speaker":"B","text":"なんと28年ぶりのメダル獲得だったようで、選手全員がとても喜んでいたのが印象的でした。"},{"id":{"d":350,"n":9,"t":4},"speaker":"A","text":"ジャンプとクロスカントリーという、互いにとても難しくハードな種目をこなす本当にすごい競技ですね。"},{"id":{"d":350,"n":10,"t":4},"speaker":"B","text":"フィギュアスケート女子フリーのショートプログラムで坂本花織選手が銅メダルを獲得しました。"},{"id":{"d":350,"n":11,"t":4},"speaker":"A","text":"さまざまな問題で揺れたフィギュアスケート女子でしたが、その中でもショート、フリーともに自己ベストを更新していましたね。全選手が素晴らしい活躍でした。"}];
INSERT INTO sentence [{"id":{"d":351,"n":1,"t":4},"speaker":"A","text":"昨晩、足の筋肉が、ずっとけいれんをしているような感覚があったので、診察していただきたいと思い、来院しました。"},{"id":{"d":351,"n":2,"t":4},"speaker":"B","text":"分かりました。それでは診察します。昨日は、筋力トレーニングや激しい運動はしましたか？"},{"id":{"d":351,"n":3,"t":4},"speaker":"A","text":"筋力トレーニングをしましたが、筋力トレーニングはほとんど毎日しています。"},{"id":{"d":351,"n":4,"t":4},"speaker":"B","text":"お風呂には入りましたか？また、お風呂に入ってマッサージなどはしましたか？"},{"id":{"d":351,"n":5,"t":4},"speaker":"A","text":"お風呂に入って、湯船の中でマッサージや、ストレッチ体操はしました。"},{"id":{"d":351,"n":6,"t":4},"speaker":"B","text":"そうですか。筋力トレーニングの疲れが残っているのかと思いましたが、そうではなさそうですね。"}];
INSERT INTO sentence [{"id":{"d":352,"n":1,"t":4},"speaker":"A","text":"お久しぶりですね。いつ以来の来院ですか？"},{"id":{"d":352,"n":2,"t":4},"speaker":"B","text":"最後に来たのは、10年以上前でしょうか。正確に、何年前か分からないくらい前のことです。"},{"id":{"d":352,"n":3,"t":4},"speaker":"A","text":"それほど前でしたか。確か中学生の時に「校内の歯がきれいな人」に選ばれた時以来ですよね？"},{"id":{"d":352,"n":4,"t":4},"speaker":"B","text":"そうですね。懐かしいです。"},{"id":{"d":352,"n":5,"t":4},"speaker":"A","text":"今日は、なぜ来ようと思ったのですか？"},{"id":{"d":352,"n":6,"t":4},"speaker":"B","text":"再来月から社会人になるので、久しぶりに歯をきれいにして、虫歯がないかを確認したいと思って来ました。"},{"id":{"d":352,"n":7,"t":4},"speaker":"A","text":"ついこの間中学生だった気がしますが、もう社会人になるのですね。"},{"id":{"d":352,"n":8,"t":4},"speaker":"B","text":"時がたつのは早いものですね。"}];
INSERT INTO sentence [{"id":{"d":353,"n":1,"t":4},"speaker":"A","text":"あなたは、寝ている時に、いびきをかきますか？"},{"id":{"d":353,"n":2,"t":4},"speaker":"B","text":"私はいびきをしないと思っていましたが、親が私の「いびきがひどい」と言っていました。"},{"id":{"d":353,"n":3,"t":4},"speaker":"A","text":"自分では気付かないものですね。年中、いびきがひどいのですか？"},{"id":{"d":353,"n":4,"t":4},"speaker":"B","text":"私は、特に花粉症の時期にひどくなるようです。鼻が詰まっていますから、仕方ないと思います。"},{"id":{"d":353,"n":5,"t":4},"speaker":"A","text":"花粉症の人は、その時期は仕方がありませんね。"}];
INSERT INTO sentence [{"id":{"d":354,"n":1,"t":4},"speaker":"A","text":"今日は24,000歩も歩きました。"},{"id":{"d":354,"n":2,"t":4},"speaker":"B","text":"たくさん歩きましたね。健康的な生活で素晴らしいです。"},{"id":{"d":354,"n":3,"t":4},"speaker":"A","text":"しかし、その分、甘いパンやお菓子を買ってきて食べてしまいました。"},{"id":{"d":354,"n":4,"t":4},"speaker":"B","text":"消費したカロリー分を摂取してしまったかもしれませんね。"},{"id":{"d":354,"n":5,"t":4},"speaker":"A","text":"カロリーの高い食べ物は、おいしいものが多いです。"},{"id":{"d":354,"n":6,"t":4},"speaker":"B","text":"おいしいものを食べるためには、その分動かないといけないということですね。"},{"id":{"d":354,"n":7,"t":4},"speaker":"A","text":"ちなみに今日の消費カロリーは1,300キロカロリーでした。"}];
INSERT INTO sentence [{"id":{"d":355,"n":1,"t":4},"speaker":"A","text":"今日北京2022オリンピック競技大会が終わりましたね。"},{"id":{"d":355,"n":2,"t":4},"speaker":"B","text":"つい先日、開幕したような気がします。とても早かったです。"},{"id":{"d":355,"n":3,"t":4},"speaker":"A","text":"日本は、過去最高のメダル獲得数になりましたね。素晴らしかったです。"},{"id":{"d":355,"n":4,"t":4},"speaker":"B","text":"冬の競技は、夏の競技ほどには注目がされませんが、これを機に注目してほしいです。"},{"id":{"d":355,"n":5,"t":4},"speaker":"A","text":"私は、スノーボードにとても興味を持ちました。"},{"id":{"d":355,"n":6,"t":4},"speaker":"B","text":"やはり、平野歩夢選手が格好よかったですね。憧れる人がたくさんいると思います。"},{"id":{"d":355,"n":7,"t":4},"speaker":"A","text":"ただ、あのようなジャンプを、一般の人ができるようになるとは到底思えません。"},{"id":{"d":355,"n":8,"t":4},"speaker":"B","text":"普通に、スキーを滑ることも難しいものですが、少しずつ練習したらうまくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":356,"n":1,"t":4},"speaker":"A","text":"シャンプーは、何を使っていますか？"},{"id":{"d":356,"n":2,"t":4},"speaker":"B","text":"私は普通の市販のものを使っています。あなたは、何を使っていますか？"},{"id":{"d":356,"n":3,"t":4},"speaker":"A","text":"私はお米のシャンプーを使っています。"},{"id":{"d":356,"n":4,"t":4},"speaker":"B","text":"美容に良いといわれるシャンプーですね。使ってみてどうですか？"},{"id":{"d":356,"n":5,"t":4},"speaker":"A","text":"髪の調子がとても良い気がします。シャンプーをしただけで髪の毛が整います。"},{"id":{"d":356,"n":6,"t":4},"speaker":"B","text":"リンスをしなくても整うのですか？"},{"id":{"d":356,"n":7,"t":4},"speaker":"A","text":"全員がそうではないと思いますが、私はそのような気がしています。"},{"id":{"d":356,"n":8,"t":4},"speaker":"B","text":"私もお米のシャンプーを試したいです。リンスが要らなくなったら家計にも優しいですよね。"}];
INSERT INTO sentence [{"id":{"d":357,"n":1,"t":4},"speaker":"A","text":"今日は、久しぶりに髪の毛をセットしていますね。"},{"id":{"d":357,"n":2,"t":4},"speaker":"B","text":"少し外に買い物に行く用事があったので、セットしてみました。"},{"id":{"d":357,"n":3,"t":4},"speaker":"A","text":"セットしたところを見ると、髪の毛が長くなったと感じますね。"},{"id":{"d":357,"n":4,"t":4},"speaker":"B","text":"私も、とてもセットしにくかったので、もう長い髪は嫌だなと思いました。"},{"id":{"d":357,"n":5,"t":4},"speaker":"A","text":"髪の毛を切りにいかないのですか？"},{"id":{"d":357,"n":6,"t":4},"speaker":"B","text":"再来週に切りに行きます。2カ月ぶりに髪を切りに行くので、楽しみです。"},{"id":{"d":357,"n":7,"t":4},"speaker":"A","text":"どのような髪型にするのですか？"},{"id":{"d":357,"n":8,"t":4},"speaker":"B","text":"ショートカットにしようと考えています。私は長髪よりも、短髪の方が似合うと思うので、楽しみです。"}];
INSERT INTO sentence [{"id":{"d":358,"n":1,"t":4},"speaker":"A","text":"お兄さん、ヘアオイルを貸してください。"},{"id":{"d":358,"n":2,"t":4},"speaker":"B","text":"この良い香りがするオイルですね。良いですよ。"},{"id":{"d":358,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。この香りがとても好きで、毎日使ってしまいます。"},{"id":{"d":358,"n":4,"t":4},"speaker":"B","text":"もし、これが欲しかったら持っていってよいですよ。もう1つ新しいものを持っているので、私の分は大丈夫です。"},{"id":{"d":358,"n":5,"t":4},"speaker":"A","text":"本当ですか？それではお言葉に甘えて頂きます。"},{"id":{"d":358,"n":6,"t":4},"speaker":"B","text":"オイルなので、持ち帰る際、バッグの中であふれたら大変です。このストッパーを持っていって良いですよ。"},{"id":{"d":358,"n":7,"t":4},"speaker":"A","text":"何から何まで、ありがとうございます。ワックスか何かを買ってお返ししますね。"},{"id":{"d":358,"n":8,"t":4},"speaker":"B","text":"それではギャッツビーのワックスを買ってください。"}];
INSERT INTO sentence [{"id":{"d":359,"n":1,"t":4},"speaker":"A","text":"サッカーのルールが変わるかもしれないそうですね。"},{"id":{"d":359,"n":2,"t":4},"speaker":"B","text":"中学校の大会の方針では、変わるとは聞いていませんので、私は今のままで続けたいと思っています。"},{"id":{"d":359,"n":3,"t":4},"speaker":"A","text":"しかし高校のサッカー大会では変更するかもしれません。高校でサッカーを続ける子どものためにも今のうちに新しいルールに慣れておくべきではないのですか？"},{"id":{"d":359,"n":4,"t":4},"speaker":"B","text":"その考えはとてもよく分かりますが、中学の大会でルールがバラバラになっている方が、生徒は混乱すると思います。"}];
INSERT INTO sentence [{"id":{"d":360,"n":1,"t":4},"speaker":"A","text":"今日も、リハビリテーションですか？"},{"id":{"d":360,"n":2,"t":4},"speaker":"B","text":"今日は診察を受けてからリハビリテーションです。ようやく「電気を流しても良い」と言われたので電気治療に入れます。"},{"id":{"d":360,"n":3,"t":4},"speaker":"A","text":"「骨折箇所に電気を流す」とおっしゃっていましたが、大丈夫なのですか？"},{"id":{"d":360,"n":4,"t":4},"speaker":"B","text":"それほどに強い電気ではなく、治すために一番良い周波数の電気を流してくれるそうです。"},{"id":{"d":360,"n":5,"t":4},"speaker":"A","text":"電気を体に流されると、その部分が小刻みに動き、変な筋肉を使ってしまいそうな気がします。"},{"id":{"d":360,"n":6,"t":4},"speaker":"B","text":"私もそのような気がしていますが、初体験なので実はとても楽しみです。"},{"id":{"d":360,"n":7,"t":4},"speaker":"A","text":"チャレンジャー精神ですね。くれぐれも無理はしないでくださいね。"},{"id":{"d":360,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。感想はお伝えしますね。"}];
INSERT INTO sentence [{"id":{"d":361,"n":1,"t":4},"speaker":"A","text":"痛み止めについて、少し疑問があります。"},{"id":{"d":361,"n":2,"t":4},"speaker":"B","text":"私も専門家ではないので、回答することは難しいですが、お聞きします。"},{"id":{"d":361,"n":3,"t":4},"speaker":"A","text":"痛い部分に注射するタイプの痛み止めは、効果があることは理解できますが、錠剤は本当に効くのですか？"},{"id":{"d":361,"n":4,"t":4},"speaker":"B","text":"実際に、痛みはひきますよ。私の場合は、錠剤でも効きました。"},{"id":{"d":361,"n":5,"t":4},"speaker":"A","text":"それを聞いて安心しました。"}];
INSERT INTO sentence [{"id":{"d":362,"n":1,"t":4},"speaker":"A","text":"サッカーの試合の前は、必ずあくびが止まらなくなります。"},{"id":{"d":362,"n":2,"t":4},"speaker":"B","text":"前日に楽しみにし過ぎて、寝られないことが多かったのですか？"},{"id":{"d":362,"n":3,"t":4},"speaker":"A","text":"違います。あくびは、緊張したら出てしまうものだと聞いたことがあります。"},{"id":{"d":362,"n":4,"t":4},"speaker":"B","text":"眠いから出るのではないのですか？"},{"id":{"d":362,"n":5,"t":4},"speaker":"A","text":"頭に酸素が足りなくなり、酸素を補給するためにあくびをするのです。"},{"id":{"d":362,"n":6,"t":4},"speaker":"B","text":"それでは、眠い時にあくびが出るのはなぜですか？"},{"id":{"d":362,"n":7,"t":4},"speaker":"A","text":"それも、頭に酸素が少なくなっているからでしょう。"},{"id":{"d":362,"n":8,"t":4},"speaker":"B","text":"なるほど、人体は不思議ですね。"}];
INSERT INTO sentence [{"id":{"d":363,"n":1,"t":4},"speaker":"A","text":"今日のサッカーの試合で、オウンゴールが2回も起こってしまいました。"},{"id":{"d":363,"n":2,"t":4},"speaker":"B","text":"それは大変な試合ですね。2点とも同じチームがオウンゴールをしてしまったのですか？"},{"id":{"d":363,"n":3,"t":4},"speaker":"A","text":"お互いにオウンゴールをし合っているので、今は1-1の同点です。"},{"id":{"d":363,"n":4,"t":4},"speaker":"B","text":"その試合を会場まで見に行っている人にとっては、オウンゴールだけでは面白くないかもしれませんね。"},{"id":{"d":363,"n":5,"t":4},"speaker":"A","text":"私はサッカーが好きで、試合を会場まで見にいくこともありますが、オウンゴールで点が決まってもあまりうれしくありません。"},{"id":{"d":363,"n":6,"t":4},"speaker":"B","text":"見に行く人としては試合の勝利もうれしいですが、試合内容も良いものを見たいですね。"},{"id":{"d":363,"n":7,"t":4},"speaker":"A","text":"その通りです。点がたくさん決まっても、内容が良くない時にはあまり面白くありませんし、点が決まらなくても、内容が面白かったらそれでいいかなと思ってしまいます。"},{"id":{"d":363,"n":8,"t":4},"speaker":"B","text":"サッカーを知らない私からすると、たくさん点が入って内容が面白い試合を期待したいです。"}];
INSERT INTO sentence [{"id":{"d":364,"n":1,"t":4},"speaker":"A","text":"虫除けのスプレーをください。"},{"id":{"d":364,"n":2,"t":4},"speaker":"B","text":"そこに置いてあるので使ってよいですよ。"},{"id":{"d":364,"n":3,"t":4},"speaker":"A","text":"このスプレーは、どれほどの効果がありますか？"},{"id":{"d":364,"n":4,"t":4},"speaker":"B","text":"感覚的には1日は持つと思います。今日のバーベキューの間は、効果が保たれるはずです。"},{"id":{"d":364,"n":5,"t":4},"speaker":"A","text":"私は蚊に刺されやすいため、できるだけしっかりと虫除けをしたいのですが、他に何か対策はありませんか？"},{"id":{"d":364,"n":6,"t":4},"speaker":"B","text":"それでは、蚊取り線香も使いましょう。"},{"id":{"d":364,"n":7,"t":4},"speaker":"A","text":"蚊取り線香は、どこにありますか？"},{"id":{"d":364,"n":8,"t":4},"speaker":"B","text":"玄関の階段下収納にあるはずです。確認してください。"}];
INSERT INTO sentence [{"id":{"d":365,"n":1,"t":4},"speaker":"A","text":"普段、あなたはバスケットボールの試合を見ますか？"},{"id":{"d":365,"n":2,"t":4},"speaker":"B","text":"あまり見ません。"},{"id":{"d":365,"n":3,"t":4},"speaker":"A","text":"そのような人は多いと思います。私は、NBLのスーパープレー動画をぜひ見てほしいと思っています。"},{"id":{"d":365,"n":4,"t":4},"speaker":"B","text":"どのようなプレーが紹介されていますか？"},{"id":{"d":365,"n":5,"t":4},"speaker":"A","text":"コートの真ん中辺りから、ロングシュートを決めるプレーなどが紹介されています。"},{"id":{"d":365,"n":6,"t":4},"speaker":"B","text":"それはすごいですね。私のバスケに関する知識は、3ポイントシュートと、ダンクシュートくらいなのでそのような動画は見たことがありません。"},{"id":{"d":365,"n":7,"t":4},"speaker":"A","text":"ダンクでも自分でドリブルして決めるものと、味方からのパスを、空中で受け取って直接ゴールに決めるアリウープがあります。"},{"id":{"d":365,"n":8,"t":4},"speaker":"B","text":"イメージをする限り、アリウープはとても格好よさそうですね。そのようなことが本当に可能なのですか？"},{"id":{"d":365,"n":9,"t":4},"speaker":"A","text":"NBLの選手は、たくさんしています。興味が湧いたら、ぜひスーパープレー集だけでも見てみてください。"}];
INSERT INTO sentence [{"id":{"d":366,"n":1,"t":4},"speaker":"A","text":"あなたは、水泳を習ったことはありますか？"},{"id":{"d":366,"n":2,"t":4},"speaker":"B","text":"習っていたことはありませんが、海で少し泳ぐことはできます。"},{"id":{"d":366,"n":3,"t":4},"speaker":"A","text":"どのような泳ぎをすることができますか？"},{"id":{"d":366,"n":4,"t":4},"speaker":"B","text":"クロールと背泳ぎはできます。あなたは、どのような泳ぎ方ができますか？"},{"id":{"d":366,"n":5,"t":4},"speaker":"A","text":"私は、かつて水泳を習っていたので、一応、4つの泳法全てできます。"},{"id":{"d":366,"n":6,"t":4},"speaker":"B","text":"バタフライもできるのですか？"},{"id":{"d":366,"n":7,"t":4},"speaker":"A","text":"13年近く前に水泳を習っていたので、感覚を取り戻すのに時間がかかるかもしれませんが、多分できると思います。"},{"id":{"d":366,"n":8,"t":4},"speaker":"B","text":"すごいですね。私は平泳ぎをマスターしたいので、ぜひ教えてほしいです。"},{"id":{"d":366,"n":9,"t":4},"speaker":"A","text":"分かりました。今度、プールへ行った時に教えますね。"}];
INSERT INTO sentence [{"id":{"d":367,"n":1,"t":4},"speaker":"A","text":"夕食後にコーヒーを飲み過ぎてしまい、眠ることができません。"},{"id":{"d":367,"n":2,"t":4},"speaker":"B","text":"カフェインの影響ですね。誰でも寝られなくなると思うので仕方ありません。"},{"id":{"d":367,"n":3,"t":4},"speaker":"A","text":"これほどまでに効果があるとは思いませんでした。私の高校の同級生は、カフェインに体が慣れ過ぎて、コーヒーでは眠気が取れなくなっている人がいました。"},{"id":{"d":367,"n":4,"t":4},"speaker":"B","text":"それは、相当飲んでいる人ですね。それか、元々、カフェインに強い人だったのかもしれません。"}];
INSERT INTO sentence [{"id":{"d":368,"n":1,"t":4},"speaker":"A","text":"最近は、カーリングの人気が高まっているそうですね。"},{"id":{"d":368,"n":2,"t":4},"speaker":"B","text":"北京2022オリンピック競技大会のカーリング女子の活躍のおかげかもしれませんね。"},{"id":{"d":368,"n":3,"t":4},"speaker":"A","text":"オリンピックという大舞台で、準優勝という素晴らしい活躍でした。"},{"id":{"d":368,"n":4,"t":4},"speaker":"B","text":"はい。日本代表の選手たちの雰囲気が明るかったことも、人気が出た理由と考えられます。"},{"id":{"d":368,"n":5,"t":4},"speaker":"A","text":"北海道弁の、ほんわかした雰囲気が良かったですね。"},{"id":{"d":368,"n":6,"t":4},"speaker":"B","text":"試合の放送時間も良かったですね。休日の昼間にテレビ番組が放送されていました。"},{"id":{"d":368,"n":7,"t":4},"speaker":"A","text":"私も家族で視聴していました。とても盛り上がりました。"}];
INSERT INTO sentence [{"id":{"d":369,"n":1,"t":4},"speaker":"A","text":"本日は、どうされましたか？"},{"id":{"d":369,"n":2,"t":4},"speaker":"B","text":"最近、目が痛いので来院しました。"},{"id":{"d":369,"n":3,"t":4},"speaker":"A","text":"少し充血していますね。最近、生活の変化はありましたか？"},{"id":{"d":369,"n":4,"t":4},"speaker":"B","text":"最近パソコンを扱うことが増えたため、パソコンの画面を見過ぎているかもしれません。"},{"id":{"d":369,"n":5,"t":4},"speaker":"A","text":"それは、ブルーライトの影響があるかもしれませんね。"},{"id":{"d":369,"n":6,"t":4},"speaker":"B","text":"ブルーライトカットの眼鏡を購入するべきでしょうか？"},{"id":{"d":369,"n":7,"t":4},"speaker":"A","text":"持っていないのであれば、購入をお薦めします。そのことで少しは改善されると思います。"},{"id":{"d":369,"n":8,"t":4},"speaker":"B","text":"家に帰る途中で、ブルーライトカットの眼鏡を購入しようと思います。"}];
INSERT INTO sentence [{"id":{"d":370,"n":1,"t":4},"speaker":"A","text":"今日は、とてもよく歩きました。"},{"id":{"d":370,"n":2,"t":4},"speaker":"B","text":"素晴らしいですね。ちなみに、どれくらい歩きましたか？"},{"id":{"d":370,"n":3,"t":4},"speaker":"A","text":"14キロメートルほど歩きました。歩数にすると約17,000歩です。"},{"id":{"d":370,"n":4,"t":4},"speaker":"B","text":"とても歩いていますね。なぜそれほど歩いたのですか？"},{"id":{"d":370,"n":5,"t":4},"speaker":"A","text":"天気が良かったので歩こうと思い、2時間半ほどで歩きました。"},{"id":{"d":370,"n":6,"t":4},"speaker":"B","text":"気分転換に歩くことは良いですね。とても健康的だと思います。"},{"id":{"d":370,"n":7,"t":4},"speaker":"A","text":"歩くことで、車移動の時には気付かない風景にも、気付けるのでとても良かったです。"},{"id":{"d":370,"n":8,"t":4},"speaker":"B","text":"私も、明日歩いてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":371,"n":1,"t":4},"speaker":"A","text":"私は、家で炭酸水をよく飲むのですが、リフレッシュのためにとても良いようです。"},{"id":{"d":371,"n":2,"t":4},"speaker":"B","text":"確かに、飲んだら喉がすっきりして、気分転換になりそうですね。"},{"id":{"d":371,"n":3,"t":4},"speaker":"A","text":"あなたは意識的に飲んでいるものはありますか？"},{"id":{"d":371,"n":4,"t":4},"speaker":"B","text":"私は、白湯を飲んでいます。体の芯から温まりますよ。"},{"id":{"d":371,"n":5,"t":4},"speaker":"A","text":"朝起きて、白湯を飲むと体が動き始めて良いと聞いたことがあります。私も、朝飲んでみようと思います。"}];
INSERT INTO sentence [{"id":{"d":372,"n":1,"t":4},"speaker":"A","text":"走り終わった後に、すぐに座らない方がいいですよ。"},{"id":{"d":372,"n":2,"t":4},"speaker":"B","text":"とても疲れているので、もう座りたいです。"},{"id":{"d":372,"n":3,"t":4},"speaker":"A","text":"すぐに座ると、乳酸が溜まってしまいますよ。少し歩きましょう。"},{"id":{"d":372,"n":4,"t":4},"speaker":"B","text":"少しとは、どれくらいですか？"},{"id":{"d":372,"n":5,"t":4},"speaker":"A","text":"まずは、息が整うまで、ゆっくり歩きましょう。"},{"id":{"d":372,"n":6,"t":4},"speaker":"B","text":"分かりました。息が整ったら、座っても良いですか？"},{"id":{"d":372,"n":7,"t":4},"speaker":"A","text":"座る前にストレッチ体操をして、体を伸ばした方がいいですよ。"},{"id":{"d":372,"n":8,"t":4},"speaker":"B","text":"分かりました。歩いてストレッチ体操をしたら、多分座りたいと思わなくなります。"},{"id":{"d":372,"n":9,"t":4},"speaker":"A","text":"そうですか。もしかしたら、その頃には汗が冷えて寒くなっているかもしれませんので、風邪をひかないように暖かくしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":373,"n":1,"t":4},"speaker":"A","text":"子どもの運動神経を育てるために、良い遊びを何か知っていますか？"},{"id":{"d":373,"n":2,"t":4},"speaker":"B","text":"分かりません。外で遊ぶことは基本的に良いと思いますが、その中でも特に良い遊びということですか？"},{"id":{"d":373,"n":3,"t":4},"speaker":"A","text":"はい。実は、鬼ごっこが良いそうです。"},{"id":{"d":373,"n":4,"t":4},"speaker":"B","text":"なぜ、鬼ごっこが良いのですか？"},{"id":{"d":373,"n":5,"t":4},"speaker":"A","text":"全身運動になるそうです。向きを切り替えたり、ダッシュしたりすることが良いそうです。"},{"id":{"d":373,"n":6,"t":4},"speaker":"B","text":"確かに、鬼をかわすために切り返しはよくしますね。その動きが良いということですね。"},{"id":{"d":373,"n":7,"t":4},"speaker":"A","text":"また、どこに逃げることが一番良いかということを考えるため、頭も使うとても良い遊びのようです。"},{"id":{"d":373,"n":8,"t":4},"speaker":"B","text":"大人になった今でも、挑戦したいです。"}];
INSERT INTO sentence [{"id":{"d":374,"n":1,"t":4},"speaker":"A","text":"バレーボールはご存じですか？"},{"id":{"d":374,"n":2,"t":4},"speaker":"B","text":"もちろん知っています。実際に試合をすることも好きです。"},{"id":{"d":374,"n":3,"t":4},"speaker":"A","text":"元々、バレーボール部でしたか？"},{"id":{"d":374,"n":4,"t":4},"speaker":"B","text":"いえ、バレーボール部には所属していませんでした。昼休みや授業で少しした程度です。"},{"id":{"d":374,"n":5,"t":4},"speaker":"A","text":"その時に、腕は青あざができましたか？"},{"id":{"d":374,"n":6,"t":4},"speaker":"B","text":"私には、あざがたくさんできましたが、バレーボール部の友達は全くできていませんでした。"},{"id":{"d":374,"n":7,"t":4},"speaker":"A","text":"バレーボール部員は、慣れているため青あざができにくいようです。"},{"id":{"d":374,"n":8,"t":4},"speaker":"B","text":"慣れというのは、すごいですね。"}];
INSERT INTO sentence [{"id":{"d":375,"n":1,"t":4},"speaker":"A","text":"私は、野球の球種で、カーブを投げることができる人がうらやましいです。"},{"id":{"d":375,"n":2,"t":4},"speaker":"B","text":"あなたは、カーブを投げることができないのですか？"},{"id":{"d":375,"n":3,"t":4},"speaker":"A","text":"私は、ボールにカーブをかけて蹴ることはできるのですが、投げることはできません。"},{"id":{"d":375,"n":4,"t":4},"speaker":"B","text":"私は野球をしていたので、逆にカーブを蹴ることができる人がうらやましいです。"},{"id":{"d":375,"n":5,"t":4},"speaker":"A","text":"お互い、できないことがうらやましく感じるということですね。"},{"id":{"d":375,"n":6,"t":4},"speaker":"B","text":"ちなみに、カーブはどのように蹴るのですか？"},{"id":{"d":375,"n":7,"t":4},"speaker":"A","text":"ボールの横を擦るように蹴ることで、ボールが弧を描きます。"},{"id":{"d":375,"n":8,"t":4},"speaker":"B","text":"野球の場合と、原理は同じですね。"}];
INSERT INTO sentence [{"id":{"d":376,"n":1,"t":4},"speaker":"A","text":"スカッシュというスポーツをしたことがありますか？"},{"id":{"d":376,"n":2,"t":4},"speaker":"B","text":"したことがありません。まず、どのようなスポーツか教えてください。"},{"id":{"d":376,"n":3,"t":4},"speaker":"A","text":"2人で正方形の箱の中に入り、黒い小さなボールを、ラケットで壁に打ち続けるというスポーツです。"},{"id":{"d":376,"n":4,"t":4},"speaker":"B","text":"あなたはスカッシュをやったことがありますか？"},{"id":{"d":376,"n":5,"t":4},"speaker":"A","text":"はい。相手が打ちにくそうなところを考えて、壁に当てないといけないので、とても難しかったです。また、力の加減も難しいです。"},{"id":{"d":376,"n":6,"t":4},"speaker":"B","text":"実際に体験したいです。"},{"id":{"d":376,"n":7,"t":4},"speaker":"A","text":"大学の近くのスポーツジムで体験できますよ。一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":377,"n":1,"t":4},"speaker":"A","text":"よさこいを知っていますか？"},{"id":{"d":377,"n":2,"t":4},"speaker":"B","text":"もちろん知っています。幼稚園の時に踊ったことがあります。"},{"id":{"d":377,"n":3,"t":4},"speaker":"A","text":"よさこいにはさまざまな種類があることを知っていましたか？"},{"id":{"d":377,"n":4,"t":4},"speaker":"B","text":"1種類だけではないのですか？"},{"id":{"d":377,"n":5,"t":4},"speaker":"A","text":"実はよさこいには、高知のよさこい、北海道のYOSAKOIソーラン祭り、名古屋のよさこいなど、さまざまな種類があります。"},{"id":{"d":377,"n":6,"t":4},"speaker":"B","text":"いくつか種類があるのですね。何が違うのですか？"},{"id":{"d":377,"n":7,"t":4},"speaker":"A","text":"名古屋と北海道のよさこいの違いは、私は分かりませんが、高知と北海道のよさこいの違いははっきりしていて、高知のよさこいは練り歩くことがメインです。"},{"id":{"d":377,"n":8,"t":4},"speaker":"B","text":"それでは、北海道のYOSAKOIソーラン祭りでは、とても激しく動くということですか？"},{"id":{"d":377,"n":9,"t":4},"speaker":"A","text":"その通りです。100メートルほどの道を走り回ります。"}];
INSERT INTO sentence [{"id":{"d":378,"n":1,"t":4},"speaker":"A","text":"普段の生活で、癒しはありますか？"},{"id":{"d":378,"n":2,"t":4},"speaker":"B","text":"ストレスをあまり感じないので、癒しを求めることが少ないです。あなたは、何かありますか？"},{"id":{"d":378,"n":3,"t":4},"speaker":"A","text":"私は、子犬や子猫の動画を見ることが癒しです。"},{"id":{"d":378,"n":4,"t":4},"speaker":"B","text":"とてもかわいいですよね。子犬の犬種は何ですか？"},{"id":{"d":378,"n":5,"t":4},"speaker":"A","text":"私は豆柴が大好きなので、豆柴の動画をよく見ています。"},{"id":{"d":378,"n":6,"t":4},"speaker":"B","text":"豆柴とてもかわいいですね。家の中でちょこちょこ動いている動画を見てかわいいなと思っていました。"},{"id":{"d":378,"n":7,"t":4},"speaker":"A","text":"また、豆柴が好きなので、似ている柴犬も大好きです。"},{"id":{"d":378,"n":8,"t":4},"speaker":"B","text":"日本人は、柴犬を好きな人が多いですね。私も将来は、柴犬を飼いたいです。"}];
INSERT INTO sentence [{"id":{"d":379,"n":1,"t":4},"speaker":"A","text":"今日のリハビリテーションは、ゴムのチューブを使って行います。"},{"id":{"d":379,"n":2,"t":4},"speaker":"B","text":"どのようなことをするのですか？"},{"id":{"d":379,"n":3,"t":4},"speaker":"A","text":"このチューブを、壁に固定して引っ張るだけです。"},{"id":{"d":379,"n":4,"t":4},"speaker":"B","text":"簡単ですが、この運動はどこに効くものですか？"},{"id":{"d":379,"n":5,"t":4},"speaker":"A","text":"これは肩甲骨周りの、可動域を広げる運動です。"},{"id":{"d":379,"n":6,"t":4},"speaker":"B","text":"注意するべきことはありますか？"},{"id":{"d":379,"n":7,"t":4},"speaker":"A","text":"肘を固定して、引っ張るようにしてください。肘を固定しなければ、効果が出ません。"},{"id":{"d":379,"n":8,"t":4},"speaker":"B","text":"分かりました。注意しながらリハビリテーションをしてみます。"}];
INSERT INTO sentence [{"id":{"d":380,"n":1,"t":4},"speaker":"A","text":"今日はどうしましたか？"},{"id":{"d":380,"n":2,"t":4},"speaker":"B","text":"運動を指導している身として、勉強しにきました。"},{"id":{"d":380,"n":3,"t":4},"speaker":"A","text":"それでは、簡単なことから学んでください。"},{"id":{"d":380,"n":4,"t":4},"speaker":"B","text":"一番基本的なことを、教えていただけるとうれしいです。"},{"id":{"d":380,"n":5,"t":4},"speaker":"A","text":"脱水症状を回避するために、運動をしている時はしっかり給水をするようにしましょう。"},{"id":{"d":380,"n":6,"t":4},"speaker":"B","text":"スポーツをしている時にスポーツ飲料を飲みますが、他に良い飲み物はありますか？"},{"id":{"d":380,"n":7,"t":4},"speaker":"A","text":"実は、コーラも良いと最近の研究ではいわれています。"},{"id":{"d":380,"n":8,"t":4},"speaker":"B","text":"昔はコーラは骨を溶かすといわれていましたが、今は推奨されるのですね。"}];
INSERT INTO sentence [{"id":{"d":381,"n":1,"t":4},"speaker":"A","text":"昨日、プロ野球のキャンプが終了したようです。"},{"id":{"d":381,"n":2,"t":4},"speaker":"B","text":"ついこの間、キャンプインという報道を見た気がしますが、もう終わってしまったのですね。"},{"id":{"d":381,"n":3,"t":4},"speaker":"A","text":"私も、とても早かったような気がします。"},{"id":{"d":381,"n":4,"t":4},"speaker":"B","text":"今年は、どのチームに注目していますか？"},{"id":{"d":381,"n":5,"t":4},"speaker":"A","text":"元々、北海道日本ハムファイターズを応援していたので、今年も変わらず応援しようと思います。"},{"id":{"d":381,"n":6,"t":4},"speaker":"B","text":"BIGBOSS監督になり、チームが大きく変わったと聞いています。私も日ハムに期待しています。"},{"id":{"d":381,"n":7,"t":4},"speaker":"A","text":"昨日の映像では、どの選手もいらない脂肪は落ちて、動きやすい体になっているように感じました。"},{"id":{"d":381,"n":8,"t":4},"speaker":"B","text":"さまざまなコーチを招請した成果が出ているかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":382,"n":1,"t":4},"speaker":"A","text":"社会人になって健康を気遣った生活をするようになってから、学生の頃はいかに不健康な生活をしていたのかを思い知りました。"},{"id":{"d":382,"n":2,"t":4},"speaker":"B","text":"やはり学生時代は、不健康な生活になりますか？"},{"id":{"d":382,"n":3,"t":4},"speaker":"A","text":"ご飯はしっかり食べていましたが、1人暮らしだと、やはり栄養を考えて作るよりも自分の食べたいものや、簡単なものばかりを作ってしまう傾向にありました。"},{"id":{"d":382,"n":4,"t":4},"speaker":"B","text":"それは仕方ないことかもしれませんね。社会人になってからも、1人暮らしですか？"},{"id":{"d":382,"n":5,"t":4},"speaker":"A","text":"はい。ですが、社会人になってからは、バランスを考えて計画的に自炊をするようにしています。"},{"id":{"d":382,"n":6,"t":4},"speaker":"B","text":"それは素晴らしいです。具体的に、どのような工夫をしているのですか？"},{"id":{"d":382,"n":7,"t":4},"speaker":"A","text":"前もって作りたいメニューや、健康的なメニューをリストアップしています。リストを見ながら、栄養が偏らないように、献立を調整するようにしています。"}];
INSERT INTO sentence [{"id":{"d":383,"n":1,"t":4},"speaker":"A","text":"耳の近くのリンパ節が腫れているのですが、どの病院に行けば良いと思いますか？"},{"id":{"d":383,"n":2,"t":4},"speaker":"B","text":"何が原因か分かっていますか？"},{"id":{"d":383,"n":3,"t":4},"speaker":"A","text":"全く分かりません。いつの間にか腫れていて、唾液を飲むときに少し痛みを感じます。"},{"id":{"d":383,"n":4,"t":4},"speaker":"B","text":"それはだいぶ大変そうですね。原因が分からないのならば、大きな病院に行くことをお勧めします。"},{"id":{"d":383,"n":5,"t":4},"speaker":"A","text":"分かりました。すぐ近くの大学病院に行こうと思います。"},{"id":{"d":383,"n":6,"t":4},"speaker":"B","text":"あそこの大学病院は、紹介状がないと、基本的には診察を受けることができません。"},{"id":{"d":383,"n":7,"t":4},"speaker":"A","text":"そうなのですね。それでは、どこの病院がいいのでしょうか？"},{"id":{"d":383,"n":8,"t":4},"speaker":"B","text":"少し遠くなりますが、車で15分程度のところに総合病院がありますよ。"}];
INSERT INTO sentence [{"id":{"d":384,"n":1,"t":4},"speaker":"A","text":"あるネットの記事で見ましたが、適度なアルコール摂取は体に良いそうです。"},{"id":{"d":384,"n":2,"t":4},"speaker":"B","text":"それはよく聞きますね。ただ適度なという言葉が難しくて、どの程度なのか疑問に思います。"},{"id":{"d":384,"n":3,"t":4},"speaker":"A","text":"確かにそうですね。適度は人それぞれかもしれませんね。"},{"id":{"d":384,"n":4,"t":4},"speaker":"B","text":"しかし、毎日少しずつでもアルコールを摂取していると、アルコール依存症になるという記事も見たことがあります。"},{"id":{"d":384,"n":5,"t":4},"speaker":"A","text":"どちらが正しいのでしょうか？"},{"id":{"d":384,"n":6,"t":4},"speaker":"B","text":"両方ともネットの記事なので難しいですね。しっかり取捨選択する必要があります。"}];
INSERT INTO sentence [{"id":{"d":385,"n":1,"t":4},"speaker":"A","text":"最近、あなたは素振りをしませんね。なぜ素振りをしなくなったのですか？"},{"id":{"d":385,"n":2,"t":4},"speaker":"B","text":"「体に変な癖がつく」とコーチに言われたので、素振りをしなくなりました。"},{"id":{"d":385,"n":3,"t":4},"speaker":"A","text":"変な癖がつくことなど、あるのですか？"},{"id":{"d":385,"n":4,"t":4},"speaker":"B","text":"私はあまり感じていませんでしたが、一応コーチの忠告なので従おうかなと思っています。"},{"id":{"d":385,"n":5,"t":4},"speaker":"A","text":"それよりも、バットを動かす筋肉を少しでも使って、動きやすくする方がいいと思います。"},{"id":{"d":385,"n":6,"t":4},"speaker":"B","text":"私も、そのように考えて素振りをしていました。コーチに、質問してみます。"},{"id":{"d":385,"n":7,"t":4},"speaker":"A","text":"そうですね。疑問に思ったことは聞いてみるといいと思います。"},{"id":{"d":385,"n":8,"t":4},"speaker":"B","text":"今日の練習の際、質問してみます。"}];
INSERT INTO sentence [{"id":{"d":386,"n":1,"t":4},"speaker":"A","text":"現在、あなたはとても健康に毎日を過ごしていらっしゃるようですが、小さい頃から体が丈夫だったのですか？"},{"id":{"d":386,"n":2,"t":4},"speaker":"B","text":"全くそのようなことはありません。むしろ、体はとても弱かったです。"},{"id":{"d":386,"n":3,"t":4},"speaker":"A","text":"今からは、考えることができませんね。よく病院に行っていた、ということですか？"},{"id":{"d":386,"n":4,"t":4},"speaker":"B","text":"その通りです。幼稚園を休んで小児科によく行っていました。"},{"id":{"d":386,"n":5,"t":4},"speaker":"A","text":"何か病院の記憶はありますか？"},{"id":{"d":386,"n":6,"t":4},"speaker":"B","text":"全くありませんが、小児科の先生に「もう来ることはなくなるでしょう」と言われてから、1回も行かなくなったと、母から言われました。"},{"id":{"d":386,"n":7,"t":4},"speaker":"A","text":"それはすごいですね。なぜ、小児科の先生は、その後、あなたが小児科に行かずに済むと分かったのでしょうか？"},{"id":{"d":386,"n":8,"t":4},"speaker":"B","text":"何か、小児科の先生だからこそ分かることがあったのでしょう。しっかりと、虚弱体質が治って体が強くなって良かったです。"}];
INSERT INTO sentence [{"id":{"d":387,"n":1,"t":4},"speaker":"A","text":"今日近くの野球場で少年野球をしていました。"},{"id":{"d":387,"n":2,"t":4},"speaker":"B","text":"少年野球は見ていてすがすがしい気持ちになれますね。私もひと目見たかったです。"},{"id":{"d":387,"n":3,"t":4},"speaker":"A","text":"確かにすがすがしい気持ちにはなりますが、想像以上に本格的に練習をしていて少しびっくりしました。"},{"id":{"d":387,"n":4,"t":4},"speaker":"B","text":"やはり少年野球でも、強豪と呼ばれるチームは練習も厳しいのでしょうか？"},{"id":{"d":387,"n":5,"t":4},"speaker":"A","text":"そうですね。平日に練習をしているということはクラブチームなので、プロのコーチが教えているのでしょう。"},{"id":{"d":387,"n":6,"t":4},"speaker":"B","text":"それなら、本格的な練習をしていることにも合点がいきました。"},{"id":{"d":387,"n":7,"t":4},"speaker":"A","text":"私の母校の野球部よりも、よほどしっかりと練習していました。あのような少年が、阪神甲子園球場で行われる高校野球の試合に出場するのでしょうか？"},{"id":{"d":387,"n":8,"t":4},"speaker":"B","text":"そうかもしれませんね。彼らのこれからの活躍が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":388,"n":1,"t":4},"speaker":"A","text":"最近髪の毛が長すぎて、今まで持っていたワックスでは、髪の毛がまとまりません。"},{"id":{"d":388,"n":2,"t":4},"speaker":"B","text":"長い髪の毛用のワックスが、あるのですか？"},{"id":{"d":388,"n":3,"t":4},"speaker":"A","text":"もちろんあります。私はずっと髪の毛が短いので、長い髪の毛用のワックスは持っていませんが、買おうか迷っています。"},{"id":{"d":388,"n":4,"t":4},"speaker":"B","text":"しかし、常に髪の毛が短いのであれば今だけのために、ワックスを購入する必要はないように感じます。"},{"id":{"d":388,"n":5,"t":4},"speaker":"A","text":"確かに、そうですね。ただ、明日も外出するのですが、髪の毛のセットをする気になれません。"},{"id":{"d":388,"n":6,"t":4},"speaker":"B","text":"そのような時にはドライヤーと、ヘアアイロンを使うと、うまくセットできますよ。"},{"id":{"d":388,"n":7,"t":4},"speaker":"A","text":"今まで、ドライヤーしか使ったことがないので、ヘアアイロンの使い方が分かりません。"},{"id":{"d":388,"n":8,"t":4},"speaker":"B","text":"それでは、簡単に教えるので、今から試してみましょう。"}];
INSERT INTO sentence [{"id":{"d":389,"n":1,"t":4},"speaker":"A","text":"今まで、胃薬を飲んだことがないのですが、胃薬はどのような味ですか？"},{"id":{"d":389,"n":2,"t":4},"speaker":"B","text":"胃薬に味は、ないと思いますよ。味わって摂取するものではありません。"},{"id":{"d":389,"n":3,"t":4},"speaker":"A","text":"しかし、正露丸はとても独特な匂いがすると聞いています。"},{"id":{"d":389,"n":4,"t":4},"speaker":"B","text":"そうですね。正露丸の匂いが独特なので有名なだけだと思います。"},{"id":{"d":389,"n":5,"t":4},"speaker":"A","text":"正露丸は、どのような味なのですか？"},{"id":{"d":389,"n":6,"t":4},"speaker":"B","text":"正露丸も味はしませんよ。飲んだら口の中で、とても独特な匂いがするだけです。"},{"id":{"d":389,"n":7,"t":4},"speaker":"A","text":"あの匂いが独特といわれているのは、かんでいるのではなく、飲むだけでする匂いなのですね。"},{"id":{"d":389,"n":8,"t":4},"speaker":"B","text":"そうです。しかし、胃腸薬としては、とても効果はあるので匂いがしても売れるということです。"}];
INSERT INTO sentence [{"id":{"d":390,"n":1,"t":4},"speaker":"A","text":"手にまめが、できたことはありますか？"},{"id":{"d":390,"n":2,"t":4},"speaker":"B","text":"まめができたことはありませんが、たこならできたことがあります。"},{"id":{"d":390,"n":3,"t":4},"speaker":"A","text":"たこの方が大変な気がします。なぜできたのですか？"},{"id":{"d":390,"n":4,"t":4},"speaker":"B","text":"懸垂をしていたら、手にたこができました。"},{"id":{"d":390,"n":5,"t":4},"speaker":"A","text":"少し意味が分からないのですが、懸垂をしていて、なぜ手にたこができるのですか？"},{"id":{"d":390,"n":6,"t":4},"speaker":"B","text":"鉄棒を握っていると、どうしても手の皮が巻き込まれてしまい、その影響でたこになりました。"},{"id":{"d":390,"n":7,"t":4},"speaker":"A","text":"それでは、指の付け根のところにできたということですか？"},{"id":{"d":390,"n":8,"t":4},"speaker":"B","text":"その通りです。たこができたおかげで、もう全く痛くありませんが、皮膚の一部が爪のように硬くて気になります。"}];
INSERT INTO sentence [{"id":{"d":391,"n":1,"t":4},"speaker":"A","text":"何事もプロの世界は、私たち素人には理解できないことがたくさんありますね。"},{"id":{"d":391,"n":2,"t":4},"speaker":"B","text":"何か理解できないことがありましたか？"},{"id":{"d":391,"n":3,"t":4},"speaker":"A","text":"サッカーの業界では、世界有数のチームに所属している高成績の日本人選手が、普段の試合には全く出場することができていない場合もあるのです。"},{"id":{"d":391,"n":4,"t":4},"speaker":"B","text":"それはその選手以上に、素晴らしい選手がたくさんいるということではありませんか？"},{"id":{"d":391,"n":5,"t":4},"speaker":"A","text":"その通りですが、全盛期の今、しっかりと試合に出場することが大切なのではないかと、私は思ってしまいます。"},{"id":{"d":391,"n":6,"t":4},"speaker":"B","text":"確かに、そのように思いますね。しかし、レベルの高いチームに所属することで、質の高い練習をすることができ、試合に出るよりいいと考えているのかもしれません。"},{"id":{"d":391,"n":7,"t":4},"speaker":"A","text":"練習の質の問題については、考えていませんでした。確かに、そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":392,"n":1,"t":4},"speaker":"A","text":"北京2022パラリンピックが開幕しましたね。連日、日本人選手の活躍が報道されています。"},{"id":{"d":392,"n":2,"t":4},"speaker":"B","text":"素晴らしい活躍が続いていますね。2種目で金メダルを獲得した選手もいるようです。"},{"id":{"d":392,"n":3,"t":4},"speaker":"A","text":"素晴らしいですね。オリンピックで金メダルを獲得することは、とても大変なはずです。"},{"id":{"d":392,"n":4,"t":4},"speaker":"B","text":"しかし、その選手は「タイムをもっと伸ばしたかった」と金メダル獲得が決定した後で、コメントをしていました。"},{"id":{"d":392,"n":5,"t":4},"speaker":"A","text":"ただ、金メダルを取るのではなく、しっかりタイムも自分の満足するもので、優勝したいということですね。素晴らしいプロ意識です。"},{"id":{"d":392,"n":6,"t":4},"speaker":"B","text":"やはりプロは、私たちのような考えとは少し違う、高いレベルのものを常に持っていると痛感しました。"},{"id":{"d":392,"n":7,"t":4},"speaker":"A","text":"何かの第一線で活躍している人は、何か違うものが見えているのでしょう。"},{"id":{"d":392,"n":8,"t":4},"speaker":"B","text":"少しうらやましいような気もしますが、その選手たちの生い立ちも関係していると思うので、まねはできませんね。"}];
INSERT INTO sentence [{"id":{"d":393,"n":1,"t":4},"speaker":"A","text":"久しぶりですね。どうしましたか？"},{"id":{"d":393,"n":2,"t":4},"speaker":"B","text":"肋骨が1本飛び出ているように感じたので、来ました。"},{"id":{"d":393,"n":3,"t":4},"speaker":"A","text":"それでは確認しますので、どこに違和感があるのか教えてください。"},{"id":{"d":393,"n":4,"t":4},"speaker":"B","text":"左の背中側の肋骨に違和感があります。"},{"id":{"d":393,"n":5,"t":4},"speaker":"A","text":"確かに、少し出ていますね。これによって、日常生活に支障は出ていますか？"},{"id":{"d":393,"n":6,"t":4},"speaker":"B","text":"普通に座って課題をしているだけで、痛くて集中が切れてしまうことが多々あります。"},{"id":{"d":393,"n":7,"t":4},"speaker":"A","text":"日常生活に支障が出ているということですね。それはしっかりと確認して、治しましょう。"},{"id":{"d":393,"n":8,"t":4},"speaker":"B","text":"治してください。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":394,"n":1,"t":4},"speaker":"A","text":"毎日、ひげをそるのが面倒になってきました。ひげだけ脱毛しようか迷っています。"},{"id":{"d":394,"n":2,"t":4},"speaker":"B","text":"ひげだけ脱毛した場合、いくらくらいかかるのですか？"},{"id":{"d":394,"n":3,"t":4},"speaker":"A","text":"美容脱毛では、完全にひげ脱毛が終了するまで約10万円するようです。"},{"id":{"d":394,"n":4,"t":4},"speaker":"B","text":"想像していたよりも高いように感じます。永久脱毛をした場合、ひげはもう生えてこなくなるのですか？"},{"id":{"d":394,"n":5,"t":4},"speaker":"A","text":"全く生えてこなくなるわけでは、ないようです。少し考えないと分かりませんね。"},{"id":{"d":394,"n":6,"t":4},"speaker":"B","text":"そうですね。しっかり考えて、どのようにするのか決めた方がいいでしょう。"}];
INSERT INTO sentence [{"id":{"d":395,"n":1,"t":4},"speaker":"A","text":"日本では、サッカーと、野球のどちらに取り組む方が、異性から好意を持たれやすいか、ということがよく話題に上がりますね。"},{"id":{"d":395,"n":2,"t":4},"speaker":"B","text":"確かに、サッカー部と野球部でよく話になります。"},{"id":{"d":395,"n":3,"t":4},"speaker":"A","text":"私の周りでは、多くの人が「野球男子の方がいい」と言っていました。"},{"id":{"d":395,"n":4,"t":4},"speaker":"B","text":"そうですか。サッカーをしていた身としては少し悲しいです。何故でしょうか？"},{"id":{"d":395,"n":5,"t":4},"speaker":"A","text":"野球部員の方が誠実そう、真面目そうという意見が多く聞かれますね。"},{"id":{"d":395,"n":6,"t":4},"speaker":"B","text":"サッカー部員は、なぜ真面目、誠実ではないと思われるのでしょうか？"},{"id":{"d":395,"n":7,"t":4},"speaker":"A","text":"野球部員は、短髪の人が多くいます。そのため真面目そうと思うのでしょう。"},{"id":{"d":395,"n":8,"t":4},"speaker":"B","text":"サッカー部員も、短髪にしたら真面目そうと思ってもらえるでしょうか？"},{"id":{"d":395,"n":9,"t":4},"speaker":"A","text":"もしかしたら、そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":396,"n":1,"t":4},"speaker":"A","text":"大学に、献血をする車が来ていました。あなたも、献血をしに行きませんか？"},{"id":{"d":396,"n":2,"t":4},"speaker":"B","text":"今回は、献血に協力したら、何をもらえるのでしょうか？"},{"id":{"d":396,"n":3,"t":4},"speaker":"A","text":"まだ、確認していません。前回は、何をもらえたか覚えていますか？"},{"id":{"d":396,"n":4,"t":4},"speaker":"B","text":"確か、カップ焼きそばをたくさんもらいました。まだ、消費しきれていません。"},{"id":{"d":396,"n":5,"t":4},"speaker":"A","text":"北海道ならではの、カップ焼きそばをもらいましたね。加えて、大勢で協力した場合は、何か追加でもらえた気がします。"},{"id":{"d":396,"n":6,"t":4},"speaker":"B","text":"記憶が曖昧ですが、オレンジジュースを1ダースほどもらった記憶があります。"},{"id":{"d":396,"n":7,"t":4},"speaker":"A","text":"そうでしたね。それでは、今回も友達を誘って献血をしに行きましょう。"},{"id":{"d":396,"n":8,"t":4},"speaker":"B","text":"友達に連絡をしてみます。"}];
INSERT INTO sentence [{"id":{"d":397,"n":1,"t":4},"speaker":"A","text":"香水をつけている若者が、増えていますね。"},{"id":{"d":397,"n":2,"t":4},"speaker":"B","text":"私の友達も、香水をつけると張り切って買いに行っていました。"},{"id":{"d":397,"n":3,"t":4},"speaker":"A","text":"森谷さんは、香水を持っていますか？"},{"id":{"d":397,"n":4,"t":4},"speaker":"B","text":"私は、あのような匂いが少し苦手で持っていません。"},{"id":{"d":397,"n":5,"t":4},"speaker":"A","text":"それでは、洋服は無臭ということですか？"},{"id":{"d":397,"n":6,"t":4},"speaker":"B","text":"それも嫌なので、自分の好きなきつくない柔軟剤を使っています。"},{"id":{"d":397,"n":7,"t":4},"speaker":"A","text":"柔軟剤で、香りをつけるのはいいですね。しかし、きつい匂いの柔軟剤もあるので注意も必要ですね。"},{"id":{"d":397,"n":8,"t":4},"speaker":"B","text":"柔軟剤を買うときに、テスターで確認しているので大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":398,"n":1,"t":4},"speaker":"A","text":"この注射は痛くないと聞いていますが、本当ですか？"},{"id":{"d":398,"n":2,"t":4},"speaker":"B","text":"痛くないと思います。針がとても細いので、痛みを感じにくいと思いますよ。"},{"id":{"d":398,"n":3,"t":4},"speaker":"A","text":"それでは、安心して注射を受けることができます。"},{"id":{"d":398,"n":4,"t":4},"speaker":"B","text":"しかし、小さい子は大人に比べて、痛覚がとても敏感だといわれていますので、子どもには痛いかもしれません。"},{"id":{"d":398,"n":5,"t":4},"speaker":"A","text":"そのようなことがあるのですね。健康のために、この注射はしなくてはいけないと教えられたので、少し痛くても我慢します。"},{"id":{"d":398,"n":6,"t":4},"speaker":"B","text":"偉いですね。それでは、一瞬、痛むかもしれませんが我慢してくださいね。"}];
INSERT INTO sentence [{"id":{"d":399,"n":1,"t":4},"speaker":"A","text":"サッカーが、世界的に人気のあるスポーツである理由が少し分かってきました。"},{"id":{"d":399,"n":2,"t":4},"speaker":"B","text":"なぜだと思いますか？"},{"id":{"d":399,"n":3,"t":4},"speaker":"A","text":"まず、プロのレベルが高く、一般の人が憧れるというブランド力があると思います。"},{"id":{"d":399,"n":4,"t":4},"speaker":"B","text":"なるほど。確かに、彼らはたくさんのスーパープレーを見せてくれますね。"},{"id":{"d":399,"n":5,"t":4},"speaker":"A","text":"他にも、世界のどこかで何かが起きた場合、サッカー業界は積極的にチャリティー活動を行っています。私は、元々サッカーにはそれほど興味はありませんでしたが、非常時に素早くこのような活動をしていることを知り、興味を持つようになりました。"},{"id":{"d":399,"n":6,"t":4},"speaker":"B","text":"そのようなことをしていたとは知りませんでした。私も興味が湧いてきました。"}];
INSERT INTO sentence [{"id":{"d":400,"n":1,"t":4},"speaker":"A","text":"ディフェンスの練習中ですが、少し休憩がほしい人はいますか？"},{"id":{"d":400,"n":2,"t":4},"speaker":"B","text":"はい。喉が渇いたので、飲み物を飲む時間が欲しいです。"},{"id":{"d":400,"n":3,"t":4},"speaker":"A","text":"分かりました。しっかり給水してください。"},{"id":{"d":400,"n":4,"t":4},"speaker":"B","text":"練習中でも給水してよいですか？"},{"id":{"d":400,"n":5,"t":4},"speaker":"A","text":"もちろんです。今日は、とても暑いので熱中症になってしまう可能性もあります。体調管理には気を付けてください。"},{"id":{"d":400,"n":6,"t":4},"speaker":"B","text":"もし熱中症になった場合どうしたら良いですか？"},{"id":{"d":400,"n":7,"t":4},"speaker":"A","text":"具合が悪くなったらすぐに日陰に入って安静にしてください。落ち着いたら室内に入って、横になりながら体を冷やしてください。ひどい場合は病院へ行くのですぐに教えてくださいね。"},{"id":{"d":400,"n":8,"t":4},"speaker":"B","text":"分かりました。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":401,"n":1,"t":4},"speaker":"A","text":"昨日、食事をしている時に、口内をかんでしまいました。かんだ場所がとても痛いです。"},{"id":{"d":401,"n":2,"t":4},"speaker":"B","text":"昨日少し患部を見ましたが、流血がひどかったのできっと口内炎になっていると思いますよ。"},{"id":{"d":401,"n":3,"t":4},"speaker":"A","text":"どのようにしたら良いですか？"},{"id":{"d":401,"n":4,"t":4},"speaker":"B","text":"口の中のかんだ場所は、後々とても腫れることがあるので、ゆっくり食べることくらいしか対策はないと思います。"},{"id":{"d":401,"n":5,"t":4},"speaker":"A","text":"話していても腫れている部分をかみそうになるのですが大丈夫でしょうか？"},{"id":{"d":401,"n":6,"t":4},"speaker":"B","text":"話している時には口を閉じることも少ないので、食事の時ほど口内をかむことは多くないと思います。"},{"id":{"d":401,"n":7,"t":4},"speaker":"A","text":"それでは腫れがひくまで毎日ゆっくりご飯を食べることにします。"},{"id":{"d":401,"n":8,"t":4},"speaker":"B","text":"それがいいと思います。気を付けてください。"}];
INSERT INTO sentence [{"id":{"d":402,"n":1,"t":4},"speaker":"A","text":"サッカーの欧州クラブ選手権の試合が、昨日行われました。"},{"id":{"d":402,"n":2,"t":4},"speaker":"B","text":"ベスト16の試合ですね。とても見応えのある試合でした。"},{"id":{"d":402,"n":3,"t":4},"speaker":"A","text":"第1試合ではレアル・マドリードが0-1という僅差で負けていましたが、今回の第2試合は3-1と圧勝しました。"},{"id":{"d":402,"n":4,"t":4},"speaker":"B","text":"2試合合計で3-2となりましたので、レアル・マドリードがベスト8に進出ですね。"},{"id":{"d":402,"n":5,"t":4},"speaker":"A","text":"パリ・サン＝ジェルマンは、今季からとても強力な3トップを擁していましたが、それでも勝つことができませんでしたね。"},{"id":{"d":402,"n":6,"t":4},"speaker":"B","text":"昨年の大会は準優勝でしたので、今大会は注目されていた分、少し落ち込みました。"},{"id":{"d":402,"n":7,"t":4},"speaker":"A","text":"その分、レアル・マドリードに期待したいですね。"},{"id":{"d":402,"n":8,"t":4},"speaker":"B","text":"はい。これからも面白い試合を、たくさんしてほしいです。"}];
INSERT INTO sentence [{"id":{"d":403,"n":1,"t":4},"speaker":"A","text":"それでは、もう一度長距離走の作戦を確認しましょう。"},{"id":{"d":403,"n":2,"t":4},"speaker":"B","text":"分かりました。今回は何キロメートル走の作戦の確認をするのですか？"},{"id":{"d":403,"n":3,"t":4},"speaker":"A","text":"10キロメートル走の確認をします。最初は、先頭集団についていくということでしたね。"},{"id":{"d":403,"n":4,"t":4},"speaker":"B","text":"そうですね。ポケットされることがないように、位置取りには気を付けて、先頭集団に紛れようと思います。"},{"id":{"d":403,"n":5,"t":4},"speaker":"A","text":"そして、半分が過ぎた辺りで少しペースを上げます。"},{"id":{"d":403,"n":6,"t":4},"speaker":"B","text":"いきなり上げるのではなく、じわじわと上げていくということでしたね。"},{"id":{"d":403,"n":7,"t":4},"speaker":"A","text":"そうですね。そして最初にスパートをかける人には、しっかりついていくということですね。"},{"id":{"d":403,"n":8,"t":4},"speaker":"B","text":"そのスパートについて行き、最後に、私がもう一度スパートをかける、という作戦でしたね。"},{"id":{"d":403,"n":9,"t":4},"speaker":"A","text":"しっかり理解していますね。それでは、頑張って走ってきてください。"}];
INSERT INTO sentence [{"id":{"d":404,"n":1,"t":4},"speaker":"A","text":"ハードル走をしたことは、ありますか？"},{"id":{"d":404,"n":2,"t":4},"speaker":"B","text":"一度だけあります。小学生の時に、体育の授業でしたことがあります。"},{"id":{"d":404,"n":3,"t":4},"speaker":"A","text":"どうでしたか？うまくできましたか？"},{"id":{"d":404,"n":4,"t":4},"speaker":"B","text":"学校の中で、上位3人に入るくらいの結果は出していました。"},{"id":{"d":404,"n":5,"t":4},"speaker":"A","text":"それはすごいですね。大会もハードル走で出たのですか？"},{"id":{"d":404,"n":6,"t":4},"speaker":"B","text":"いいえ、ハードル走では出ませんでした。仲の良い友達が、私よりも速かったので、彼に負けるのが嫌だったのです。"},{"id":{"d":404,"n":7,"t":4},"speaker":"A","text":"練習をして、その彼に勝とうとは思わなかったのですか？"},{"id":{"d":404,"n":8,"t":4},"speaker":"B","text":"はい、思いませんでした。大会には、元々私が得意だった競技で出ました。"}];
INSERT INTO sentence [{"id":{"d":405,"n":1,"t":4},"speaker":"A","text":"あなたは、新型コロナワクチンを打ちましたか？"},{"id":{"d":405,"n":2,"t":4},"speaker":"B","text":"はい。ワクチンを打ちましたが、抗体は本当にできているのでしょうか？"},{"id":{"d":405,"n":3,"t":4},"speaker":"A","text":"このワクチンは、しっかり研究をした上で開発されているので、抗体は付きます。"},{"id":{"d":405,"n":4,"t":4},"speaker":"B","text":"それではこのワクチンによって、新型コロナウイルス感染症には、もうかかることはないということですか？"},{"id":{"d":405,"n":5,"t":4},"speaker":"A","text":"残念ですが、まだかかる可能性はあります。"},{"id":{"d":405,"n":6,"t":4},"speaker":"B","text":"それでは、抗体ができてもあまり意味がないということですか？"},{"id":{"d":405,"n":7,"t":4},"speaker":"A","text":"いえ、ワクチンを打って抗体が付くことによって、新型コロナウイルス感染症による重症化を防ぐことができます。"},{"id":{"d":405,"n":8,"t":4},"speaker":"B","text":"重症化を防ぐことができるならば、打つ必要がありますね。"}];
INSERT INTO sentence [{"id":{"d":406,"n":1,"t":4},"speaker":"A","text":"バドミントンをしませんか？"},{"id":{"d":406,"n":2,"t":4},"speaker":"B","text":"楽しそうなので、行きたいのです。どこでできますか？"},{"id":{"d":406,"n":3,"t":4},"speaker":"A","text":"近くの公園にコートがあります。そこでしましょう。"},{"id":{"d":406,"n":4,"t":4},"speaker":"B","text":"外でバドミントンをすると、シャトルが風の影響で飛んで行きませんか？"},{"id":{"d":406,"n":5,"t":4},"speaker":"A","text":"遊びなので仕方ありません。それとも、どこか体育館で本格的にしますか？"},{"id":{"d":406,"n":6,"t":4},"speaker":"B","text":"私は体育館で行うと思っていました。でも、今は少し運動する程度のバドミントンで良いと思っています。"},{"id":{"d":406,"n":7,"t":4},"speaker":"A","text":"お互いバドミントンを本格的にしていたわけではないので、少し難しそうですね。"},{"id":{"d":406,"n":8,"t":4},"speaker":"B","text":"確かに、そうですね。公園で楽しく行いましょう。"}];
INSERT INTO sentence [{"id":{"d":407,"n":1,"t":4},"speaker":"A","text":"あなたは、髪の毛を染めたことがありますか？"},{"id":{"d":407,"n":2,"t":4},"speaker":"B","text":"ありますよ。大学生になる時に初めて染めて、その後も何度か染めました。"},{"id":{"d":407,"n":3,"t":4},"speaker":"A","text":"髪の毛は、自分で染めましたか？"},{"id":{"d":407,"n":4,"t":4},"speaker":"B","text":"いいえ。毎回美容院で染めていました。自分で染めるとうまくいかないと聞いていたので、プロに任せました。"},{"id":{"d":407,"n":5,"t":4},"speaker":"A","text":"自分で染めると、どのようにうまくいかないか、知っていますか？"},{"id":{"d":407,"n":6,"t":4},"speaker":"B","text":"うまく染まっているところと、染まっていないところができてしまうそうです。"},{"id":{"d":407,"n":7,"t":4},"speaker":"A","text":"まだらになってしまう、ということですね。"},{"id":{"d":407,"n":8,"t":4},"speaker":"B","text":"他にも、染毛剤がいろいろな場所に付いてしまうので、処理も大変になるようです。"},{"id":{"d":407,"n":9,"t":4},"speaker":"A","text":"なるほど。やはり、プロに頼む方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":408,"n":1,"t":4},"speaker":"A","text":"見てください、すてきな爪でしょう？"},{"id":{"d":408,"n":2,"t":4},"speaker":"B","text":"指先の爪の、1つ1つが全部違う絵になっているのですね、きれいです。"},{"id":{"d":408,"n":3,"t":4},"speaker":"A","text":"新しくできたネイルサロンに行ってきたのです。"},{"id":{"d":408,"n":4,"t":4},"speaker":"B","text":"そういえば、駅前のビルに開店していましたね。"},{"id":{"d":408,"n":5,"t":4},"speaker":"A","text":"とても丁寧で、良いサロンでしたよ。"},{"id":{"d":408,"n":6,"t":4},"speaker":"B","text":"それほどきれいにしてもらったら、料金もお高いのでしょう？"},{"id":{"d":408,"n":7,"t":4},"speaker":"A","text":"それが、開店記念の割引券があったので、とても安くできました。"},{"id":{"d":408,"n":8,"t":4},"speaker":"B","text":"それは良いですね。もし割引券が余っていたら、私に頂けませんか？"}];
INSERT INTO sentence [{"id":{"d":409,"n":1,"t":4},"speaker":"A","text":"ママ、大変です。"},{"id":{"d":409,"n":2,"t":4},"speaker":"B","text":"何が起こったのですか？"},{"id":{"d":409,"n":3,"t":4},"speaker":"A","text":"膝を擦りむいてしてしまい、血がたくさん出ています。とても痛いです。"},{"id":{"d":409,"n":4,"t":4},"speaker":"B","text":"大変です。すぐに洗ってばんそうこうを貼らないといけませんね。"},{"id":{"d":409,"n":5,"t":4},"speaker":"A","text":"膝を洗ってきたので、ばんそうこうを貼ってください。"},{"id":{"d":409,"n":6,"t":4},"speaker":"B","text":"はい、貼りましたよ。"},{"id":{"d":409,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。でも、まだ痛いです。"},{"id":{"d":409,"n":8,"t":4},"speaker":"B","text":"しばらくすると痛くなくなりますよ。あまり触らないようにしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":410,"n":1,"t":4},"speaker":"A","text":"今年も、花粉が多く飛んでいるようです。"},{"id":{"d":410,"n":2,"t":4},"speaker":"B","text":"あなたは、花粉症なのですか？"},{"id":{"d":410,"n":3,"t":4},"speaker":"A","text":"はい。花粉の季節は、マスクと花粉症用眼鏡が欠かせません。"},{"id":{"d":410,"n":4,"t":4},"speaker":"B","text":"今年は、特に花粉が多いそうですから大変ですね。"},{"id":{"d":410,"n":5,"t":4},"speaker":"A","text":"鼻水と涙を止める良い薬が見つからなくて、困っています。"},{"id":{"d":410,"n":6,"t":4},"speaker":"B","text":"病院には行っていますか？"},{"id":{"d":410,"n":7,"t":4},"speaker":"A","text":"行っているのですが、薬があまり体に合わないのです。"},{"id":{"d":410,"n":8,"t":4},"speaker":"B","text":"一層の事、花粉がないところに移住できたら良いですね。"}];
INSERT INTO sentence [{"id":{"d":411,"n":1,"t":4},"speaker":"A","text":"ジョギングを始めて、3日目です。"},{"id":{"d":411,"n":2,"t":4},"speaker":"B","text":"毎朝のジョギングは、続けられそうですか？"},{"id":{"d":411,"n":3,"t":4},"speaker":"A","text":"今のところは、大丈夫だと思います。"},{"id":{"d":411,"n":4,"t":4},"speaker":"B","text":"毎日、何キロメートル走っているのですか？"},{"id":{"d":411,"n":5,"t":4},"speaker":"A","text":"5キロメートル、3キロメートル、1キロメートルです。"},{"id":{"d":411,"n":6,"t":4},"speaker":"B","text":"1日目、2日目、3日目ということですか？"},{"id":{"d":411,"n":7,"t":4},"speaker":"A","text":"そういうことです。"},{"id":{"d":411,"n":8,"t":4},"speaker":"B","text":"順調に減っていますね。"},{"id":{"d":411,"n":9,"t":4},"speaker":"A","text":"明日こそは、距離を増やせるように頑張りたいと思います。"}];
INSERT INTO sentence [{"id":{"d":412,"n":1,"t":4},"speaker":"A","text":"私は健康のために、断酒を始めました。"},{"id":{"d":412,"n":2,"t":4},"speaker":"B","text":"それは良いことですね。これまでは、主に何を飲んでいたのですか？"},{"id":{"d":412,"n":3,"t":4},"speaker":"A","text":"毎日飲んでいたのは、ビールです。"},{"id":{"d":412,"n":4,"t":4},"speaker":"B","text":"ビールは飲み過ぎなければ、健康を害することはないと思います。"},{"id":{"d":412,"n":5,"t":4},"speaker":"A","text":"そうかもしれませんが、せっかくやる気になったので、もう少し断酒を続けてみようと思っています。"}];
INSERT INTO sentence [{"id":{"d":413,"n":1,"t":4},"speaker":"A","text":"診察券と、健康保険証の提示をお願いします。"},{"id":{"d":413,"n":2,"t":4},"speaker":"B","text":"診察券はあるのですが、健康保険証が見当たりません。"},{"id":{"d":413,"n":3,"t":4},"speaker":"A","text":"今月の受診は、初めてですか？"},{"id":{"d":413,"n":4,"t":4},"speaker":"B","text":"こちらは初めてですが、別の診療科にかかったことがあります。"},{"id":{"d":413,"n":5,"t":4},"speaker":"A","text":"確かに、今月の初めに皮膚科にかかっていらっしゃいますね。"},{"id":{"d":413,"n":6,"t":4},"speaker":"B","text":"健康保険証がなくても大丈夫でしょうか？"},{"id":{"d":413,"n":7,"t":4},"speaker":"A","text":"今月は大丈夫です。来月は、忘れないようにしてください。"},{"id":{"d":413,"n":8,"t":4},"speaker":"B","text":"はい、気を付けます。すみません。"}];
INSERT INTO sentence [{"id":{"d":414,"n":1,"t":4},"speaker":"A","text":"採血をしますね。"},{"id":{"d":414,"n":2,"t":4},"speaker":"B","text":"はい、よろしくお願いします。"},{"id":{"d":414,"n":3,"t":4},"speaker":"A","text":"右腕と左腕、どちらの腕がいいですか？"},{"id":{"d":414,"n":4,"t":4},"speaker":"B","text":"左腕の方がいいです。"},{"id":{"d":414,"n":5,"t":4},"speaker":"A","text":"最近、他にも採血しましたか？"},{"id":{"d":414,"n":6,"t":4},"speaker":"B","text":"献血に行きました。"},{"id":{"d":414,"n":7,"t":4},"speaker":"A","text":"その時の痕がまだ残っていますので、右腕でも良いですか？"},{"id":{"d":414,"n":8,"t":4},"speaker":"B","text":"本当ですね、気が付きませんでした。では、右腕でお願いします。"}];
INSERT INTO sentence [{"id":{"d":415,"n":1,"t":4},"speaker":"A","text":"帰りに、シャンプーを買ってきてください。"},{"id":{"d":415,"n":2,"t":4},"speaker":"B","text":"どのシャンプーが良いのですか？"},{"id":{"d":415,"n":3,"t":4},"speaker":"A","text":"アンファー スカルプD ボーテ ナチュラスター スカルプシャンプーをいつも使っています。"},{"id":{"d":415,"n":4,"t":4},"speaker":"B","text":"その長い名前を覚えるのが、大変です。"},{"id":{"d":415,"n":5,"t":4},"speaker":"A","text":"では、頭皮に負担の少ないシャンプーでしたら、何でも構いません。"}];
INSERT INTO sentence [{"id":{"d":416,"n":1,"t":4},"speaker":"A","text":"夫のいびきがひどくて、最近はずっと寝不足です。"},{"id":{"d":416,"n":2,"t":4},"speaker":"B","text":"いつからですか？"},{"id":{"d":416,"n":3,"t":4},"speaker":"A","text":"1カ月くらい前からなのです。"},{"id":{"d":416,"n":4,"t":4},"speaker":"B","text":"急にいびきをかくようになったのだったら、ご病気の可能性も考えられますよ。"},{"id":{"d":416,"n":5,"t":4},"speaker":"A","text":"そうなのですか？"},{"id":{"d":416,"n":6,"t":4},"speaker":"B","text":"念のため、病院に行った方がいいかもしれません。"},{"id":{"d":416,"n":7,"t":4},"speaker":"A","text":"少し心配になってきました。"},{"id":{"d":416,"n":8,"t":4},"speaker":"B","text":"一緒に、病院を探しましょう。"}];
INSERT INTO sentence [{"id":{"d":417,"n":1,"t":4},"speaker":"A","text":"私は、少し食べ過ぎてしまいました。"},{"id":{"d":417,"n":2,"t":4},"speaker":"B","text":"胃薬なら持っていますよ。"},{"id":{"d":417,"n":3,"t":4},"speaker":"A","text":"消化が良くなる胃薬ですか？"},{"id":{"d":417,"n":4,"t":4},"speaker":"B","text":"そうです、私はよく胃もたれするので、いつも持っているのです。"},{"id":{"d":417,"n":5,"t":4},"speaker":"A","text":"1回分を分けてもらえますか？"},{"id":{"d":417,"n":6,"t":4},"speaker":"B","text":"よいですよ、粉薬は大丈夫ですか？"},{"id":{"d":417,"n":7,"t":4},"speaker":"A","text":"大丈夫です。"},{"id":{"d":417,"n":8,"t":4},"speaker":"B","text":"どうぞ飲んでください。食べ過ぎには気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":418,"n":1,"t":4},"speaker":"A","text":"公園に散歩に行きませんか？"},{"id":{"d":418,"n":2,"t":4},"speaker":"B","text":"いいですね、万歩計を持って行きます。"},{"id":{"d":418,"n":3,"t":4},"speaker":"A","text":"歩数計ですか。私は今まで、散歩で歩数を計ったことがありませんでした。"},{"id":{"d":418,"n":4,"t":4},"speaker":"B","text":"歩数が分かるのも、楽しいですよ。"},{"id":{"d":418,"n":5,"t":4},"speaker":"A","text":"あなたは、記録をつけたりするのですか？"},{"id":{"d":418,"n":6,"t":4},"speaker":"B","text":"昔は日記に書き留めていましたが、最近は、万歩計が積算の歩数を自動で記録してくれます。"},{"id":{"d":418,"n":7,"t":4},"speaker":"A","text":"優秀な万歩計なのですね。"},{"id":{"d":418,"n":8,"t":4},"speaker":"B","text":"最近の万歩計は便利です。そろそろ合計歩数が、東海道制覇になりそうなので、最近よく歩いています。"}];
INSERT INTO sentence [{"id":{"d":419,"n":1,"t":4},"speaker":"A","text":"肘から、血が出ていますよ。"},{"id":{"d":419,"n":2,"t":4},"speaker":"B","text":"ただのかすり傷ですよ。"},{"id":{"d":419,"n":3,"t":4},"speaker":"A","text":"駄目ですよ、きちんと消毒をしなくてはいけません。"},{"id":{"d":419,"n":4,"t":4},"speaker":"B","text":"もう乾いてきていますし、大丈夫です。"},{"id":{"d":419,"n":5,"t":4},"speaker":"A","text":"いえ、大丈夫には見えません。小さな傷から大変なことになることもあるのですよ。"},{"id":{"d":419,"n":6,"t":4},"speaker":"B","text":"さっき、少し転んで擦りむいただけです。"},{"id":{"d":419,"n":7,"t":4},"speaker":"A","text":"でも、念のため医務室で診てもらってください。"}];
INSERT INTO sentence [{"id":{"d":420,"n":1,"t":4},"speaker":"A","text":"昨日、私はクライミングジムに行ってきました。"},{"id":{"d":420,"n":2,"t":4},"speaker":"B","text":"あなたは、ボルダリングをされるのですか？"},{"id":{"d":420,"n":3,"t":4},"speaker":"A","text":"ボルダリングの体験教室に参加して、昨日初めて挑戦しました。あなたはボルダリングをしたことはありますか？"},{"id":{"d":420,"n":4,"t":4},"speaker":"B","text":"興味はあるのですが、したことはありません。"},{"id":{"d":420,"n":5,"t":4},"speaker":"A","text":"ぜひ挑戦してみてください。初心者でも楽しめましたよ。"},{"id":{"d":420,"n":6,"t":4},"speaker":"B","text":"そうなのですね。今度、私も連れて行ってください。"}];
INSERT INTO sentence [{"id":{"d":421,"n":1,"t":4},"speaker":"A","text":"ボルダリングをしてみて、どうですか？"},{"id":{"d":421,"n":2,"t":4},"speaker":"B","text":"私は、実は高所恐怖症なのだと初めて分かりました。"},{"id":{"d":421,"n":3,"t":4},"speaker":"A","text":"ボルダリングは、壁の方を見ているから、それほど高さを感じずに登ることができると思いますが、それでも怖かったですか？"},{"id":{"d":421,"n":4,"t":4},"speaker":"B","text":"足元の確認をするために、つい下を見てしまい、恐怖を感じました。"},{"id":{"d":421,"n":5,"t":4},"speaker":"A","text":"今までは、高いところが苦手だとは思わなかったのですか？"},{"id":{"d":421,"n":6,"t":4},"speaker":"B","text":"はい。乗り物や展望台は、自分の力で登っていないからか、大丈夫だったのですが、ボルダリングは手が震えてしまいました。"}];
INSERT INTO sentence [{"id":{"d":422,"n":1,"t":4},"speaker":"A","text":"おばあちゃん、目薬をさしてくれますか？"},{"id":{"d":422,"n":2,"t":4},"speaker":"B","text":"よいですよ。美子ちゃんは、自分で目薬をさせないのですか？"},{"id":{"d":422,"n":3,"t":4},"speaker":"A","text":"目薬が、全部頬に落ちてしまいます。"},{"id":{"d":422,"n":4,"t":4},"speaker":"B","text":"まだ、自分でさすのは難しいのですね。私の膝に頭を乗せて、寝てください。"},{"id":{"d":422,"n":5,"t":4},"speaker":"A","text":"はい。"},{"id":{"d":422,"n":6,"t":4},"speaker":"B","text":"はい、では、さしますよ。終わっても目をこすっては駄目ですよ。"}];
INSERT INTO sentence [{"id":{"d":423,"n":1,"t":4},"speaker":"A","text":"洗濯をする時に、柔軟剤を使いますか？"},{"id":{"d":423,"n":2,"t":4},"speaker":"B","text":"私は、必ず使います。"},{"id":{"d":423,"n":3,"t":4},"speaker":"A","text":"漂白剤はどうですか？"},{"id":{"d":423,"n":4,"t":4},"speaker":"B","text":"漂白剤は、時々使います。"},{"id":{"d":423,"n":5,"t":4},"speaker":"A","text":"両方を一度に使っても、良いものなのでしょうか？"},{"id":{"d":423,"n":6,"t":4},"speaker":"B","text":"私が使っているものは、一度に両方使っても大丈夫な製品です。"},{"id":{"d":423,"n":7,"t":4},"speaker":"A","text":"製品によって違うのですか？"},{"id":{"d":423,"n":8,"t":4},"speaker":"B","text":"恐らく、そうだと思います。調べてみてはいかがですか？"}];
INSERT INTO sentence [{"id":{"d":424,"n":1,"t":4},"speaker":"A","text":"漂白剤は、すごいですね。"},{"id":{"d":424,"n":2,"t":4},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":424,"n":3,"t":4},"speaker":"A","text":"以前から落ちなかった食べこぼしの染みが、落ちました。"},{"id":{"d":424,"n":4,"t":4},"speaker":"B","text":"漂白剤が、染みの状態に合っていたのでしょうね。"},{"id":{"d":424,"n":5,"t":4},"speaker":"A","text":"ずっと、悩みの種だったのです。"},{"id":{"d":424,"n":6,"t":4},"speaker":"B","text":"染みがあったのは、今着ているブラウスですか？"},{"id":{"d":424,"n":7,"t":4},"speaker":"A","text":"そうです、全体も白くなったように感じます。"},{"id":{"d":424,"n":8,"t":4},"speaker":"B","text":"とてもきれいです。"}];
INSERT INTO sentence [{"id":{"d":425,"n":1,"t":4},"speaker":"A","text":"虫除けスプレーを腕につけたら、かぶれてしまいました。"},{"id":{"d":425,"n":2,"t":4},"speaker":"B","text":"あなたは肌が弱いのですか？"},{"id":{"d":425,"n":3,"t":4},"speaker":"A","text":"そうですね。肌は弱いほうだと思います。"},{"id":{"d":425,"n":4,"t":4},"speaker":"B","text":"服の上からスプレーできる虫除けを、広告で見ましたよ。"},{"id":{"d":425,"n":5,"t":4},"speaker":"A","text":"そのようなものがあるのですか？それは良さそうです。"},{"id":{"d":425,"n":6,"t":4},"speaker":"B","text":"ドラッグストアでも売っているそうですよ。"},{"id":{"d":425,"n":7,"t":4},"speaker":"A","text":"そうなのですね。明日、ドラッグストアに行ってみます。"},{"id":{"d":425,"n":8,"t":4},"speaker":"B","text":"かぶれたところを治す薬も、探すといいですよ。"}];
INSERT INTO sentence [{"id":{"d":426,"n":1,"t":4},"speaker":"A","text":"おとといの運動会で久しぶりに走ったら、筋肉痛になりました。"},{"id":{"d":426,"n":2,"t":4},"speaker":"B","text":"張り切ったのですね。"},{"id":{"d":426,"n":3,"t":4},"speaker":"A","text":"少し、無理をしたかもしれません。"},{"id":{"d":426,"n":4,"t":4},"speaker":"B","text":"いきなり走れば、つらいと思います。"},{"id":{"d":426,"n":5,"t":4},"speaker":"A","text":"2日たってから、筋肉痛になるとは驚きました。湿布を貼って、様子を見ようと思っています。"},{"id":{"d":426,"n":6,"t":4},"speaker":"B","text":"長引くときには、まずは薬局で薬剤師さんに相談するといいですよ。"}];
INSERT INTO sentence [{"id":{"d":427,"n":1,"t":4},"speaker":"A","text":"最近、私は貧血気味のようです。"},{"id":{"d":427,"n":2,"t":4},"speaker":"B","text":"病院には、行きましたか？"},{"id":{"d":427,"n":3,"t":4},"speaker":"A","text":"病院に行くほど、ひどくはないと思います。"},{"id":{"d":427,"n":4,"t":4},"speaker":"B","text":"鉄分の多い食べ物を食べていますか？"},{"id":{"d":427,"n":5,"t":4},"speaker":"A","text":"何を食べたらよいのか、よく分かりません。"},{"id":{"d":427,"n":6,"t":4},"speaker":"B","text":"レバーや貝類が良いと聞きますよ。"},{"id":{"d":427,"n":7,"t":4},"speaker":"A","text":"そうなのですね。早速、今日の夕食にニラレバ炒めを食べようと思います。"}];
INSERT INTO sentence [{"id":{"d":428,"n":1,"t":4},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":428,"n":2,"t":4},"speaker":"B","text":"おかえりなさい。"},{"id":{"d":428,"n":3,"t":4},"speaker":"A","text":"今日のおやつは、何ですか？"},{"id":{"d":428,"n":4,"t":4},"speaker":"B","text":"おやつの前に、手洗いと、うがいをしなくてはいけませんね。"},{"id":{"d":428,"n":5,"t":4},"speaker":"A","text":"はい、そうですね。"},{"id":{"d":428,"n":6,"t":4},"speaker":"B","text":"手洗いと、うがいを済ませたら、テーブルの上のドーナツを食べてもよいですよ。"},{"id":{"d":428,"n":7,"t":4},"speaker":"A","text":"どのドーナツを食べてもよいのですか？"},{"id":{"d":428,"n":8,"t":4},"speaker":"B","text":"どの種類でも良いですが、1つだけですよ。"}];
INSERT INTO sentence [{"id":{"d":429,"n":1,"t":4},"speaker":"A","text":"アルコール消毒のし過ぎで、手がかぶれました。"},{"id":{"d":429,"n":2,"t":4},"speaker":"B","text":"最近はどこに行っても、消毒液がありますからね。"},{"id":{"d":429,"n":3,"t":4},"speaker":"A","text":"仕事で、今日1日で10軒回り、その都度、消毒をしていました。"},{"id":{"d":429,"n":4,"t":4},"speaker":"B","text":"手が荒れるほど消毒しなければならない、というのもどうかと思います。"},{"id":{"d":429,"n":5,"t":4},"speaker":"A","text":"ハンドクリームを塗れば、治るでしょうか？"},{"id":{"d":429,"n":6,"t":4},"speaker":"B","text":"保湿効果はあると思いますが、できればしばらくアルコール消毒をしなくても済むのなら、それが一番良いですね。"}];
INSERT INTO sentence [{"id":{"d":430,"n":1,"t":4},"speaker":"A","text":"昨日は、頭が痛かったのですが、今日は元気です。"},{"id":{"d":430,"n":2,"t":4},"speaker":"B","text":"頭痛が治って、良かったですね。"},{"id":{"d":430,"n":3,"t":4},"speaker":"A","text":"はい。最近は仕事が忙しくて、疲れていたようです。"},{"id":{"d":430,"n":4,"t":4},"speaker":"B","text":"病院に、行ったのですか？"},{"id":{"d":430,"n":5,"t":4},"speaker":"A","text":"いいえ、病院には行きませんでしたが、よく食べて、よく寝ました。"},{"id":{"d":430,"n":6,"t":4},"speaker":"B","text":"疲れているときは、それが一番の治療法ですね。"},{"id":{"d":430,"n":7,"t":4},"speaker":"A","text":"これからは、無理をしないようにします。"},{"id":{"d":430,"n":8,"t":4},"speaker":"B","text":"それが、良いと思います。"}];
INSERT INTO sentence [{"id":{"d":431,"n":1,"t":4},"speaker":"A","text":"せきが、ひどいですね？"},{"id":{"d":431,"n":2,"t":4},"speaker":"B","text":"ええ、少し風邪気味なのです。"},{"id":{"d":431,"n":3,"t":4},"speaker":"A","text":"今日はもう帰って、休むようにしてください。"},{"id":{"d":431,"n":4,"t":4},"speaker":"B","text":"私も、そろそろ帰ろうと思っていました。"},{"id":{"d":431,"n":5,"t":4},"speaker":"A","text":"時間があれば、病院に寄って行ってはいかがですか？"},{"id":{"d":431,"n":6,"t":4},"speaker":"B","text":"そうですね。分かりました。"},{"id":{"d":431,"n":7,"t":4},"speaker":"A","text":"この喉あめは、私からのお見舞いです。"},{"id":{"d":431,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":432,"n":1,"t":4},"speaker":"A","text":"風邪薬を下さい。"},{"id":{"d":432,"n":2,"t":4},"speaker":"B","text":"風邪のどのような症状に効く薬を、お探しですか？"},{"id":{"d":432,"n":3,"t":4},"speaker":"A","text":"せきがひどいのです。"},{"id":{"d":432,"n":4,"t":4},"speaker":"B","text":"せき止めのお薬ですね。"},{"id":{"d":432,"n":5,"t":4},"speaker":"A","text":"はい、喉も痛いのです。"},{"id":{"d":432,"n":6,"t":4},"speaker":"B","text":"喉あめも、ございますよ。"},{"id":{"d":432,"n":7,"t":4},"speaker":"A","text":"レモン味の喉あめと、かわいいパッケージのせき止めを下さい。"},{"id":{"d":432,"n":8,"t":4},"speaker":"B","text":"はい、こちらをどうぞ。お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":433,"n":1,"t":4},"speaker":"A","text":"美容院の予約アプリケーションを使ってみました。"},{"id":{"d":433,"n":2,"t":4},"speaker":"B","text":"使い勝手は、どうですか？"},{"id":{"d":433,"n":3,"t":4},"speaker":"A","text":"すぐに予約ができて、便利です。"},{"id":{"d":433,"n":4,"t":4},"speaker":"B","text":"割引クーポンも使えるのですか？"},{"id":{"d":433,"n":5,"t":4},"speaker":"A","text":"お店によっていろいろ割引がきくようですし、美容師さんとの「会話なし」というコースも選べましたよ。"},{"id":{"d":433,"n":6,"t":4},"speaker":"B","text":"美容師さんと会話しないということまで、選べるのですね、すごいです。"},{"id":{"d":433,"n":7,"t":4},"speaker":"A","text":"今の時代を象徴していますよね。"},{"id":{"d":433,"n":8,"t":4},"speaker":"B","text":"私も今度、使ってみます。"}];
INSERT INTO sentence [{"id":{"d":434,"n":1,"t":4},"speaker":"A","text":"今年の運動会の徒競走では、1番になりたいです。"},{"id":{"d":434,"n":2,"t":4},"speaker":"B","text":"では、パパと一緒に、公園で走る練習をしてきたらどうでしょうか？"},{"id":{"d":434,"n":3,"t":4},"speaker":"A","text":"走るだけで、早くなれますか？"},{"id":{"d":434,"n":4,"t":4},"speaker":"B","text":"走り方のコツがあるのですよ。陸上競技の選手が、ホームページでコツを紹介していました。"},{"id":{"d":434,"n":5,"t":4},"speaker":"A","text":"そのホームページを、見たいです。"},{"id":{"d":434,"n":6,"t":4},"speaker":"B","text":"リビングのパソコンで、ウェブページを開いておくので、後で見てください。"}];
INSERT INTO sentence [{"id":{"d":435,"n":1,"t":4},"speaker":"A","text":"陸上競技の選手が紹介していた走り方のコツを実践したら、本当に速く走れるようになりました。"},{"id":{"d":435,"n":2,"t":4},"speaker":"B","text":"それはすごいですね。パパと一緒に練習したかいがありましたね。"},{"id":{"d":435,"n":3,"t":4},"speaker":"A","text":"はい。今日も、パパにストップウォッチでタイムを計ってもらいました。"},{"id":{"d":435,"n":4,"t":4},"speaker":"B","text":"どのくらい速くなったのですか？"},{"id":{"d":435,"n":5,"t":4},"speaker":"A","text":"少しだけです。でも、いつも同じような良いタイムで走れるようになりました。"},{"id":{"d":435,"n":6,"t":4},"speaker":"B","text":"運動会が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":436,"n":1,"t":4},"speaker":"A","text":"最近、お肌が荒れ気味で困っています。"},{"id":{"d":436,"n":2,"t":4},"speaker":"B","text":"肌荒れには、ビタミンを取ると良いと聞きました。"},{"id":{"d":436,"n":3,"t":4},"speaker":"A","text":"サラダやフルーツなどを食べればよいのでしょうか？"},{"id":{"d":436,"n":4,"t":4},"speaker":"B","text":"それも間違ってはいないと思いますが、サラダはそれほど多くの量を食べられませんよね。"},{"id":{"d":436,"n":5,"t":4},"speaker":"A","text":"確かにそうですね。何か、良い方法をご存じですか？"},{"id":{"d":436,"n":6,"t":4},"speaker":"B","text":"ビタミンを取るには、野菜でスープや温野菜などを作ると良いです。"},{"id":{"d":436,"n":7,"t":4},"speaker":"A","text":"作るのが大変そうですね。"},{"id":{"d":436,"n":8,"t":4},"speaker":"B","text":"煮込むだけですから簡単ですよ。ぜひ、挑戦してみてください。"}];
INSERT INTO sentence [{"id":{"d":437,"n":1,"t":4},"speaker":"A","text":"風邪ですか？"},{"id":{"d":437,"n":2,"t":4},"speaker":"B","text":"いえ、風邪はひいていませんよ。"},{"id":{"d":437,"n":3,"t":4},"speaker":"A","text":"でも、薬を飲んでいたでしょう？"},{"id":{"d":437,"n":4,"t":4},"speaker":"B","text":"これは、健康食品ですよ。アロエの粉です。便秘に効くのです。"},{"id":{"d":437,"n":5,"t":4},"speaker":"A","text":"粉薬のようですね。苦くないのですか？"},{"id":{"d":437,"n":6,"t":4},"speaker":"B","text":"それほど苦くはありませんよ。おいしくもないですけどね。"}];
INSERT INTO sentence [{"id":{"d":438,"n":1,"t":4},"speaker":"A","text":"最近、視力が悪くて、大きな教室だと黒板が見えません。"},{"id":{"d":438,"n":2,"t":4},"speaker":"B","text":"それは大変ですね。眼鏡かコンタクトレンズを作った方がいいですよ。"},{"id":{"d":438,"n":3,"t":4},"speaker":"A","text":"一番前の席に座っていれば、眼鏡は必要ないと思っていたのですが、そういうわけにもいかなくなりました。"},{"id":{"d":438,"n":4,"t":4},"speaker":"B","text":"一番前だからか、最近よく先生に指名されていますよね。"},{"id":{"d":438,"n":5,"t":4},"speaker":"A","text":"はい。勉強熱心だと思われているのかもしれませんね。まずは、眼鏡を作ろうと思うのですが、お薦めのお店はありますか？"},{"id":{"d":438,"n":6,"t":4},"speaker":"B","text":"駅前の眼鏡屋さんでは、眼鏡が一律のお手頃な料金で販売されていますよ。"},{"id":{"d":438,"n":7,"t":4},"speaker":"A","text":"そうなのですね。来週、アルバイトのお給料が入ったら、行ってみます。"},{"id":{"d":438,"n":8,"t":4},"speaker":"B","text":"品揃えも豊富なので、きっと、あなたに似合う眼鏡を見つけられることと思います。"}];
INSERT INTO sentence [{"id":{"d":439,"n":1,"t":4},"speaker":"A","text":"人間ドックの結果が返ってきたのですが、再検査でした。"},{"id":{"d":439,"n":2,"t":4},"speaker":"B","text":"何が引っかかったのですか？"},{"id":{"d":439,"n":3,"t":4},"speaker":"A","text":"肝機能が悪いらしいのです。"},{"id":{"d":439,"n":4,"t":4},"speaker":"B","text":"お酒の飲み過ぎですか？"},{"id":{"d":439,"n":5,"t":4},"speaker":"A","text":"普通より、少し飲酒量が多いだけです。"},{"id":{"d":439,"n":6,"t":4},"speaker":"B","text":"検査の結果によっては、お酒を控えなくてはいけませんね。"},{"id":{"d":439,"n":7,"t":4},"speaker":"A","text":"そうですね。"},{"id":{"d":439,"n":8,"t":4},"speaker":"B","text":"最近はノンアルコールのビールも、おいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":440,"n":1,"t":4},"speaker":"A","text":"海水浴に行ったら、ひどい日焼けになってしまいました。"},{"id":{"d":440,"n":2,"t":4},"speaker":"B","text":"日焼け止めを、塗っていなかったのですか？"},{"id":{"d":440,"n":3,"t":4},"speaker":"A","text":"塗っていたのですが、足りなかったようです。"},{"id":{"d":440,"n":4,"t":4},"speaker":"B","text":"首のところの赤みが、ひどいですね。"},{"id":{"d":440,"n":5,"t":4},"speaker":"A","text":"タオルをぬらして冷やしたのですが、まだ赤いですか？"},{"id":{"d":440,"n":6,"t":4},"speaker":"B","text":"はい。冷蔵庫にケーキを買ったときに付いてきた、保冷剤がありますよ。"},{"id":{"d":440,"n":7,"t":4},"speaker":"A","text":"もらってもよいですか？"},{"id":{"d":440,"n":8,"t":4},"speaker":"B","text":"はい。保冷剤をタオルにくるみ、日焼けしたところを冷やすといいですよ。"}];
INSERT INTO sentence [{"id":{"d":441,"n":1,"t":4},"speaker":"A","text":"私は夏バテで、食欲がありません。"},{"id":{"d":441,"n":2,"t":4},"speaker":"B","text":"何も食べていないのですか？"},{"id":{"d":441,"n":3,"t":4},"speaker":"A","text":"今朝は、コーヒーを飲みました。"},{"id":{"d":441,"n":4,"t":4},"speaker":"B","text":"それだけですか？"},{"id":{"d":441,"n":5,"t":4},"speaker":"A","text":"それでは良くないとは、分かっているのですが、あまり食べたくないのです。"},{"id":{"d":441,"n":6,"t":4},"speaker":"B","text":"それでは、夏バテがひどくなるばかりですよ。"},{"id":{"d":441,"n":7,"t":4},"speaker":"A","text":"お昼はどうしようかと思っています。"},{"id":{"d":441,"n":8,"t":4},"speaker":"B","text":"では、今日は食欲がなくても食べられるものを、私がごちそうしますよ。"}];
INSERT INTO sentence [{"id":{"d":442,"n":1,"t":4},"speaker":"A","text":"娘が、高熱を出してしまいました。病院に連れて行きたいのですが、どうすれば良いでしょうか？"},{"id":{"d":442,"n":2,"t":4},"speaker":"B","text":"まずは、かかりつけの病院に、連絡しましょう。"},{"id":{"d":442,"n":3,"t":4},"speaker":"A","text":"かかりつけの病院に連絡をしたのですが、もう診療時間外なので電話がつながりませんでした。"},{"id":{"d":442,"n":4,"t":4},"speaker":"B","text":"では、小児科の救急診療をしてくれる病院を探しましょう。"},{"id":{"d":442,"n":5,"t":4},"speaker":"A","text":"どのようにして、探せば良いですか？"},{"id":{"d":442,"n":6,"t":4},"speaker":"B","text":"スマートフォンで検索すれば、情報が得られますよ。"},{"id":{"d":442,"n":7,"t":4},"speaker":"A","text":"検索する言葉は「小児科　救急」ですか？"},{"id":{"d":442,"n":8,"t":4},"speaker":"B","text":"はい。自宅の最寄り駅も入力すると、近くの病院を調べられますよ。"}];
INSERT INTO sentence [{"id":{"d":443,"n":1,"t":4},"speaker":"A","text":"漢方薬というのは、中国の薬ですよね？"},{"id":{"d":443,"n":2,"t":4},"speaker":"B","text":"そうですよ、昔の中国人の知恵が詰まっている薬です。"},{"id":{"d":443,"n":3,"t":4},"speaker":"A","text":"先日、病気になったパンダに、漢方薬を飲ませたら回復したという話をテレビで見ました。"},{"id":{"d":443,"n":4,"t":4},"speaker":"B","text":"パンダに、漢方薬を飲ませたのですか？"},{"id":{"d":443,"n":5,"t":4},"speaker":"A","text":"日本に、初めてパンダが来たころの話です。"},{"id":{"d":443,"n":6,"t":4},"speaker":"B","text":"カンカンとランランが来たころですね。"},{"id":{"d":443,"n":7,"t":4},"speaker":"A","text":"具合が悪いパンダの治療法が分からなくて、獣医さんが漢方薬を飲ませたらしいです。"},{"id":{"d":443,"n":8,"t":4},"speaker":"B","text":"本当に漢方薬が効いたのなら、中国の昔の知恵はすごいですね。"}];
INSERT INTO sentence [{"id":{"d":444,"n":1,"t":4},"speaker":"A","text":"蚊に刺されてしまいました。かゆくて我慢ができません。"},{"id":{"d":444,"n":2,"t":4},"speaker":"B","text":"かきむしったら、駄目ですよ。そこの引き出しに、かゆみ止めが入っていたと思います。"},{"id":{"d":444,"n":3,"t":4},"speaker":"A","text":"ありました。"},{"id":{"d":444,"n":4,"t":4},"speaker":"B","text":"たっぷりかゆみ止めを塗るといいですよ。"},{"id":{"d":444,"n":5,"t":4},"speaker":"A","text":"薬を塗ったら、かゆみが引いてきました。"},{"id":{"d":444,"n":6,"t":4},"speaker":"B","text":"また、かゆくなってきたら塗ってください。"}];
INSERT INTO sentence [{"id":{"d":445,"n":1,"t":4},"speaker":"A","text":"プールに行きませんか？"},{"id":{"d":445,"n":2,"t":4},"speaker":"B","text":"今日、行くのですか？"},{"id":{"d":445,"n":3,"t":4},"speaker":"A","text":"明日、行きたいと思っているのですが、いかがですか？"},{"id":{"d":445,"n":4,"t":4},"speaker":"B","text":"はい、一緒に行きましょう。久しぶりに泳げるのが楽しみです。"},{"id":{"d":445,"n":5,"t":4},"speaker":"A","text":"水着は、ありますか？"},{"id":{"d":445,"n":6,"t":4},"speaker":"B","text":"数年前のものしかないので、これから買いに行きます。"},{"id":{"d":445,"n":7,"t":4},"speaker":"A","text":"ちょうど私も買おうと思っていたところです。ご一緒してよいですか？"},{"id":{"d":445,"n":8,"t":4},"speaker":"B","text":"もちろんです。行きましょう。明日は2人とも新しい水着で泳げますね。"}];
INSERT INTO sentence [{"id":{"d":446,"n":1,"t":4},"speaker":"A","text":"ホットヨガ教室に行きませんか？"},{"id":{"d":446,"n":2,"t":4},"speaker":"B","text":"私は体が硬いので、ホットヨガは無理だと思います。"},{"id":{"d":446,"n":3,"t":4},"speaker":"A","text":"暑い部屋でヨガを行うので、体が柔らかくなりますから大丈夫ですよ。"},{"id":{"d":446,"n":4,"t":4},"speaker":"B","text":"そうでしょうか？お風呂に入ると、体が柔らかくなるのと同じでしょうか？"},{"id":{"d":446,"n":5,"t":4},"speaker":"A","text":"そうかもしれません。試しに一度、一緒に行ってみませんか？"},{"id":{"d":446,"n":6,"t":4},"speaker":"B","text":"せっかくお誘いいただいたのですから、行ってみます。"},{"id":{"d":446,"n":7,"t":4},"speaker":"A","text":"Tシャツとスパッツを用意してください。"},{"id":{"d":446,"n":8,"t":4},"speaker":"B","text":"わかりました。きっと、たくさん汗をかくので、タオルも必要ですね。"}];
INSERT INTO sentence [{"id":{"d":447,"n":1,"t":4},"speaker":"A","text":"さっきから、しゃっくりが止まりませんね。"},{"id":{"d":447,"n":2,"t":4},"speaker":"B","text":"甘いものをなめるといいと聞きましたので、あめをなめています。"},{"id":{"d":447,"n":3,"t":4},"speaker":"A","text":"でも、あまり効いていないようですね。"},{"id":{"d":447,"n":4,"t":4},"speaker":"B","text":"他に、しゃっくりの止め方を知りませんか？"},{"id":{"d":447,"n":5,"t":4},"speaker":"A","text":"知っています。耳に指を入れて、押してみるのはどうですか？"},{"id":{"d":447,"n":6,"t":4},"speaker":"B","text":"さっそく、試してみます。あ、止まったようです。"}];
INSERT INTO sentence [{"id":{"d":448,"n":1,"t":4},"speaker":"A","text":"髪型が変わりましたね。"},{"id":{"d":448,"n":2,"t":4},"speaker":"B","text":"昨日、美容院に行ってカットしてきたのです。"},{"id":{"d":448,"n":3,"t":4},"speaker":"A","text":"とても似合っています。"},{"id":{"d":448,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":448,"n":5,"t":4},"speaker":"A","text":"カラーリングもしたのですか？"},{"id":{"d":448,"n":6,"t":4},"speaker":"B","text":"少しだけ、茶色にしてきました。"},{"id":{"d":448,"n":7,"t":4},"speaker":"A","text":"明るい感じが良いですね。"},{"id":{"d":448,"n":8,"t":4},"speaker":"B","text":"とても気に入っています。心も明るくなりました。"}];
INSERT INTO sentence [{"id":{"d":449,"n":1,"t":4},"speaker":"A","text":"予防接種をしたら、予防接種前のアルコール消毒で皮膚がかぶれてしまいました。"},{"id":{"d":449,"n":2,"t":4},"speaker":"B","text":"注射を打つときに、アルコールについて聞かれませんでしたか？"},{"id":{"d":449,"n":3,"t":4},"speaker":"A","text":"「アルコールでかぶれたことはありませんか？」と聞かれました。"},{"id":{"d":449,"n":4,"t":4},"speaker":"B","text":"そのときに「かぶれる」と伝えたら良かったですね。"},{"id":{"d":449,"n":5,"t":4},"speaker":"A","text":"でも、今までは皮膚がかぶれたことなどなかったのです。"},{"id":{"d":449,"n":6,"t":4},"speaker":"B","text":"今回は、体調が悪かったのかもしれませんね。"},{"id":{"d":449,"n":7,"t":4},"speaker":"A","text":"そういう理由で、皮膚がかぶれてしまうときもあるのでしょうか？"},{"id":{"d":449,"n":8,"t":4},"speaker":"B","text":"そういうときも、あるかもしれませんね。今度、病院に行ったら、先生に伺ってみてはいかがですか？"}];
INSERT INTO sentence [{"id":{"d":450,"n":1,"t":4},"speaker":"A","text":"階段で滑って、捻挫をしました。"},{"id":{"d":450,"n":2,"t":4},"speaker":"B","text":"病院には、行きましたか？"},{"id":{"d":450,"n":3,"t":4},"speaker":"A","text":"行っていません。薬局で湿布を買って貼りました。"},{"id":{"d":450,"n":4,"t":4},"speaker":"B","text":"少しは良くなりましたか？"},{"id":{"d":450,"n":5,"t":4},"speaker":"A","text":"冷やしているのですが、時間がたって、腫れがひどくなったようです。"},{"id":{"d":450,"n":6,"t":4},"speaker":"B","text":"早く病院に行った方がいいですよ。"},{"id":{"d":450,"n":7,"t":4},"speaker":"A","text":"今日は日曜日で、休診日なのです。明日の朝、行ってみようと思います。"},{"id":{"d":450,"n":8,"t":4},"speaker":"B","text":"明日まで悪化しなければ良いですね。お大事に。"}];
INSERT INTO sentence [{"id":{"d":451,"n":1,"t":4},"speaker":"A","text":"眼鏡をかけるようになったのですか？"},{"id":{"d":451,"n":2,"t":4},"speaker":"B","text":"最近、老眼が進んできたので老眼鏡を作りました。本や新聞を読むときだけ使っています。"},{"id":{"d":451,"n":3,"t":4},"speaker":"A","text":"老眼鏡の使い心地はいかがですか？"},{"id":{"d":451,"n":4,"t":4},"speaker":"B","text":"今まで眼鏡をかけたことがないのでなかなか慣れません。"},{"id":{"d":451,"n":5,"t":4},"speaker":"A","text":"そのうち慣れると思います。"},{"id":{"d":451,"n":6,"t":4},"speaker":"B","text":"そうですね。ただ、字を読むときにいら立ちがなくなったことは良かったです。"},{"id":{"d":451,"n":7,"t":4},"speaker":"A","text":"目に合った眼鏡は、読書を楽にしてくれますね。"},{"id":{"d":451,"n":8,"t":4},"speaker":"B","text":"はい、読書のペースが進みます。"}];
INSERT INTO sentence [{"id":{"d":452,"n":1,"t":4},"speaker":"A","text":"最近、飲むサプリメントが増えてしまいました。"},{"id":{"d":452,"n":2,"t":4},"speaker":"B","text":"お医者さまに、「飲むように」と言われたのですか？"},{"id":{"d":452,"n":3,"t":4},"speaker":"A","text":"いいえ、自分で体に良さそうなものを選びました。"},{"id":{"d":452,"n":4,"t":4},"speaker":"B","text":"よく考えて、減らしてもよいのではありませんか？"},{"id":{"d":452,"n":5,"t":4},"speaker":"A","text":"どれも体に不足している栄養素のような気がして、やめられません。"},{"id":{"d":452,"n":6,"t":4},"speaker":"B","text":"どのようなものを飲んでいるのですか？"},{"id":{"d":452,"n":7,"t":4},"speaker":"A","text":"ビタミン、カルシウムを主に13種類くらいです。"},{"id":{"d":452,"n":8,"t":4},"speaker":"B","text":"飲み合わせが悪いものもありそうなので、よく調べた方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":453,"n":1,"t":4},"speaker":"A","text":"冬は、冷えがひどくてつらいです。"},{"id":{"d":453,"n":2,"t":4},"speaker":"B","text":"いつもたくさん着ているように見えますけれど、寒いのですか？"},{"id":{"d":453,"n":3,"t":4},"speaker":"A","text":"5枚重ね着しても寒いのです。"},{"id":{"d":453,"n":4,"t":4},"speaker":"B","text":"ひどい冷え性なのですね。"},{"id":{"d":453,"n":5,"t":4},"speaker":"A","text":"何か楽になる方法をご存知でしたら、教えてください。"},{"id":{"d":453,"n":6,"t":4},"speaker":"B","text":"薬もあるようですが、ショウガ湯やココアが体を温めるのに良さそうですよ。"},{"id":{"d":453,"n":7,"t":4},"speaker":"A","text":"最近、私はブラックコーヒーばかり飲んでいました。"},{"id":{"d":453,"n":8,"t":4},"speaker":"B","text":"まず、体が温まるドリンクに変えてみましょう。"}];
INSERT INTO sentence [{"id":{"d":454,"n":1,"t":4},"speaker":"A","text":"唇が荒れているようですね。"},{"id":{"d":454,"n":2,"t":4},"speaker":"B","text":"冬はひどく乾燥するので、すぐに唇が荒れてしまいます。"},{"id":{"d":454,"n":3,"t":4},"speaker":"A","text":"口紅も合っていないのではありませんか？"},{"id":{"d":454,"n":4,"t":4},"speaker":"B","text":"そうかもしれません。"},{"id":{"d":454,"n":5,"t":4},"speaker":"A","text":"口紅を、リップクリームに変えるのはどうでしょう？"},{"id":{"d":454,"n":6,"t":4},"speaker":"B","text":"この口紅はブランド物で高かったですし、とても気に入っているので、たくさん使いたいのですよね。"},{"id":{"d":454,"n":7,"t":4},"speaker":"A","text":"せっかく買った口紅を、使いたい気持ちは分かりますが、唇の荒れが治るまでは我慢してはどうでしょう？"},{"id":{"d":454,"n":8,"t":4},"speaker":"B","text":"そうですね、そうします。色があると気分が華やぐので、口紅の代わりに使える色付きリップクリームのお薦めがあったら教えて欲しいです。"}];
INSERT INTO sentence [{"id":{"d":455,"n":1,"t":4},"speaker":"A","text":"今日、デパートで、化粧水の試供品をもらいました。"},{"id":{"d":455,"n":2,"t":4},"speaker":"B","text":"使ってみたのですか？"},{"id":{"d":455,"n":3,"t":4},"speaker":"A","text":"はい。とても肌に合って良い感じです。"},{"id":{"d":455,"n":4,"t":4},"speaker":"B","text":"肌に合っているものが見つかって、良かったですね。"},{"id":{"d":455,"n":5,"t":4},"speaker":"A","text":"でも、どこにも売っていないのです。"},{"id":{"d":455,"n":6,"t":4},"speaker":"B","text":"試供品の袋が残っていたら、見せてもらえますか？"},{"id":{"d":455,"n":7,"t":4},"speaker":"A","text":"はい、これです。"},{"id":{"d":455,"n":8,"t":4},"speaker":"B","text":"これは、通信販売でしか扱われていない商品だったと思います。"}];
INSERT INTO sentence [{"id":{"d":456,"n":1,"t":4},"speaker":"A","text":"あなたは、風邪気味のようですね。"},{"id":{"d":456,"n":2,"t":4},"speaker":"B","text":"先日、雨に降られて、ぬれて帰ったときに風邪をひいてしまったようです。"},{"id":{"d":456,"n":3,"t":4},"speaker":"A","text":"もう寒くなってきましたから、気を付けないといけませんよ。"},{"id":{"d":456,"n":4,"t":4},"speaker":"B","text":"傘をコンビニエンスストアで買うか迷ったのですが、買わずにぬれてしまいました。風邪薬は飲んできたので、症状は抑えられていると思います。"},{"id":{"d":456,"n":5,"t":4},"speaker":"A","text":"つらくなったら、帰宅してください。"},{"id":{"d":456,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。今のところは大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":457,"n":1,"t":4},"speaker":"A","text":"膝のレントゲン写真は、こちらです。"},{"id":{"d":457,"n":2,"t":4},"speaker":"B","text":"どうでしょうか？やはり関節がどうかなっているのでしょうか？"},{"id":{"d":457,"n":3,"t":4},"speaker":"A","text":"変形性膝関節症といって、膝関節の軟骨がすり減っているようです。"},{"id":{"d":457,"n":4,"t":4},"speaker":"B","text":"確かに、膝の外側が狭くなっていますね。重症なのでしょうか？"},{"id":{"d":457,"n":5,"t":4},"speaker":"A","text":"まだ、それほど重症ではありません。でも、放っておくと、だんだん進行する場合はあります。"},{"id":{"d":457,"n":6,"t":4},"speaker":"B","text":"歩けなくなって、車いすの助けを借りなくてはいけないようなことになるのでしょうか？"},{"id":{"d":457,"n":7,"t":4},"speaker":"A","text":"その可能性はあります。でも、リハビリテーションを根気よく続けていけば、進行を抑えることもできます。"},{"id":{"d":457,"n":8,"t":4},"speaker":"B","text":"それでは、早速、リハビリテーションを始めたいと思います。どうぞよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":458,"n":1,"t":4},"speaker":"A","text":"私は、健康のために何か運動をしたいと思っているのですが、森山さんは何かしていますか？"},{"id":{"d":458,"n":2,"t":4},"speaker":"B","text":"私は犬を飼っているので、毎朝、散歩しています。運動といえば、そのくらいですね。"},{"id":{"d":458,"n":3,"t":4},"speaker":"A","text":"なるほど、犬を飼っていれば、毎日散歩が欠かせませんね。どうですか、やっぱり調子は良いですか？"},{"id":{"d":458,"n":4,"t":4},"speaker":"B","text":"はい、おかげさまで、体の調子は良いようです。"},{"id":{"d":458,"n":5,"t":4},"speaker":"A","text":"私もウオーキングをしようと思うのですが、これまで運動をしても続いたことがないのです。"},{"id":{"d":458,"n":6,"t":4},"speaker":"B","text":"根気よく続けていれば、そのうち体が自然に運動を求めるようになります。"},{"id":{"d":458,"n":7,"t":4},"speaker":"A","text":"森山さんのように犬でも飼っていれば、散歩が良い機会になって続けられるのでしょうね。"},{"id":{"d":458,"n":8,"t":4},"speaker":"B","text":"そうかもしれません。一層の事、大下さんも犬を飼ってみてはいかがですか？"},{"id":{"d":458,"n":9,"t":4},"speaker":"A","text":"犬を飼うのは、子どもを1人育てるのと同じくらい大変なのではありませんか？"},{"id":{"d":458,"n":10,"t":4},"speaker":"B","text":"確かに大変ですが、それから得られるものも多いですよ。この頃、それを実感します。"}];
INSERT INTO sentence [{"id":{"d":459,"n":1,"t":4},"speaker":"A","text":"最近、子どもが小麦アレルギーを発症して、通院するようになりました。"},{"id":{"d":459,"n":2,"t":4},"speaker":"B","text":"それは大変ですね。以前は何ともなかったのですか？"},{"id":{"d":459,"n":3,"t":4},"speaker":"A","text":"はい、特に何かにアレルギー反応を起こすということもなかったのですが、ある日、突然、小麦にアレルギーを起こすようになったのです。"},{"id":{"d":459,"n":4,"t":4},"speaker":"B","text":"それほど突然、アレルギーを発症するというようなことがあるのでしょうか？"},{"id":{"d":459,"n":5,"t":4},"speaker":"A","text":"お医者さんの診断では、学校給食でパンを食べた後、クラスの友達と激しい運動をしたのがきっかけだったようなのです。"},{"id":{"d":459,"n":6,"t":4},"speaker":"B","text":"へえ、そのようなことが発症の原因になるのですか？初めて聞きました。"},{"id":{"d":459,"n":7,"t":4},"speaker":"A","text":"私も驚いて、なぜそのようなことになるのか尋ねてみました。"},{"id":{"d":459,"n":8,"t":4},"speaker":"B","text":"お医者さんは、何とおっしゃったのですか？"},{"id":{"d":459,"n":9,"t":4},"speaker":"A","text":"「普段は、アレルギー物質をゆるやかに吸収するので大丈夫なのですが、急に激しい運動をすると、吸収が早まって、アレルギーが発症する」とおっしゃっていました。"},{"id":{"d":459,"n":10,"t":4},"speaker":"B","text":"運動が悪いとは、普通は思いませんよね。"},{"id":{"d":459,"n":11,"t":4},"speaker":"A","text":"そうでしょう。私も意外でした。何が原因になるのか、分からないものです。"}];
INSERT INTO sentence [{"id":{"d":460,"n":1,"t":4},"speaker":"A","text":"お父さんは、最近つまずくことがありますね。"},{"id":{"d":460,"n":2,"t":4},"speaker":"B","text":"時々、目がかすむことがあるのです。"},{"id":{"d":460,"n":3,"t":4},"speaker":"A","text":"それはいけませんね。病院に行って診てもらった方がいいですよ。"},{"id":{"d":460,"n":4,"t":4},"speaker":"B","text":"いや、大したことはないと思います。年を取ると、こういうことは、珍しいことではありません。"},{"id":{"d":460,"n":5,"t":4},"speaker":"A","text":"でも、お医者さんに診ていただいて、何事もなければ安心ではありませんか？"},{"id":{"d":460,"n":6,"t":4},"speaker":"B","text":"正直なところ、病院に行くのは気が進みません。"},{"id":{"d":460,"n":7,"t":4},"speaker":"A","text":"白内障や緑内障、加齢黄斑変性症などにかかると目がかすむようですよ。一度、お医者さんに診てもらいませんか？"},{"id":{"d":460,"n":8,"t":4},"speaker":"B","text":"心配してくれてありがとう。でも、もう少し様子を見て、少しずつひどくなるようだったら、お医者さんに診てもらうことにします。"}];
INSERT INTO sentence [{"id":{"d":461,"n":1,"t":4},"speaker":"A","text":"3回目の新型コロナワクチンの接種券を送っていただきました。交互接種について書かれていますが、詳しく説明していただけますか？"},{"id":{"d":461,"n":2,"t":4},"speaker":"B","text":"はい、交互接種とは、1回目と2回目に受けたワクチンと違う種類のワクチンを3回目に受けることです。例えば、1回目と2回目にファイザー社のワクチン、3回目にモデルナ社のワクチンを受けることです。"},{"id":{"d":461,"n":3,"t":4},"speaker":"A","text":"私は1回目と2回目にファイザー社のワクチンを接種しました。もし3回目のモデルナ社のワクチンを接種すると、有効性に違いがありますか？"},{"id":{"d":461,"n":4,"t":4},"speaker":"B","text":"臨床データでは、モデルナ社のワクチンを接種する方が、抗体の数が増えるようです。その分、有効性が少し高まります。"},{"id":{"d":461,"n":5,"t":4},"speaker":"A","text":"モデルナ社のワクチンは副反応が強いということを聞きましたが、どうなのでしょうか？"},{"id":{"d":461,"n":6,"t":4},"speaker":"B","text":"モデルナ社のワクチンの方が、副反応が少し強いといわれています。発熱や全身の倦怠感がファイザー社のワクチンよりも、若干強いようです。"},{"id":{"d":461,"n":7,"t":4},"speaker":"A","text":"そうですか。実をいいますと、1回目と2回目のワクチン接種で副反応がひどかったものですから、少し心配なのです。"},{"id":{"d":461,"n":8,"t":4},"speaker":"B","text":"それなら、ファイザー社のワクチンを選んでもよろしいですよ。有効性も若干モデルナ社のワクチンの方が高いというだけで、ファイザー社のワクチンでも十分な有効性があります。"},{"id":{"d":461,"n":9,"t":4},"speaker":"A","text":"そうですか。それではファイザー社のワクチンにすることにします。どうもありがとうございました。"},{"id":{"d":461,"n":10,"t":4},"speaker":"B","text":"どういたしまして。分からないことがありましたら、遠慮なくおっしゃってください。"}];
INSERT INTO sentence [{"id":{"d":462,"n":1,"t":4},"speaker":"A","text":"息子が少年野球のチームに入っており、日曜日ごとに試合があるのです。"},{"id":{"d":462,"n":2,"t":4},"speaker":"B","text":"実は、私の息子もサッカーのチームに入っていまして、やはり、毎週日曜日に試合があるのですよ。"},{"id":{"d":462,"n":3,"t":4},"speaker":"A","text":"息子にせがまれて、サッカーの試合の応援に行っていますが、あなたもやはり行かれますか？"},{"id":{"d":462,"n":4,"t":4},"speaker":"B","text":"はい、私も極力行くようにしています。少年チームといっても、レベルが高くて、見ていて面白いです。"},{"id":{"d":462,"n":5,"t":4},"speaker":"A","text":"息子さんのポジションはどこですか？"},{"id":{"d":462,"n":6,"t":4},"speaker":"B","text":"ゴールキーパーです。シュートを打たれたときは、見ていて心臓が止まりそうになります。"},{"id":{"d":462,"n":7,"t":4},"speaker":"A","text":"うちの息子は、ライトを守っているのですが、エラーするのではないかと、気が気ではありませんね。"},{"id":{"d":462,"n":8,"t":4},"speaker":"B","text":"勝ったときはいいのですが、負けたときは息子も意気消沈して、どのようにして慰めてやろうかと、考え込んでしまいます。"},{"id":{"d":462,"n":9,"t":4},"speaker":"A","text":"それは、私も同じですよ。特に、息子のエラーが原因で負けたときは、声を掛けるのもはばかられます。"},{"id":{"d":462,"n":10,"t":4},"speaker":"B","text":"それでも、気が付いたら無我夢中で応援しているのですから、良い気晴らしになりますね。"}];
INSERT INTO sentence [{"id":{"d":463,"n":1,"t":4},"speaker":"A","text":"花粉症の季節になりましたね。毎年、この季節になると、憂鬱になります。"},{"id":{"d":463,"n":2,"t":4},"speaker":"B","text":"今年は例年よりも、花粉が多いそうですから、大変ですね。あなたは、どのような症状が出るのですか？"},{"id":{"d":463,"n":3,"t":4},"speaker":"A","text":"鼻水、目のかゆみ、くしゃみ、咽喉の痛みがあり、本当につらいです。"},{"id":{"d":463,"n":4,"t":4},"speaker":"B","text":"幸いなことに、私は今のところ花粉症にかかっていませんが、誰でも発症する可能性があるそうですから、油断はできません。"},{"id":{"d":463,"n":5,"t":4},"speaker":"A","text":"今からでも、できるだけ予防を心掛ける方がいいですよ。"},{"id":{"d":463,"n":6,"t":4},"speaker":"B","text":"どのようなことに、気を付ければ良いのでしょうか？"},{"id":{"d":463,"n":7,"t":4},"speaker":"A","text":"外出するときは、眼鏡とマスクを着けること、毛織物を避け、化学繊維や綿でできた服を着ること、外出から帰ったら、玄関に入る前に、頭や服に付いた花粉を払い落すこと、などでしょうか。"},{"id":{"d":463,"n":8,"t":4},"speaker":"B","text":"結構、面倒ですね。私にはできそうもありません。"},{"id":{"d":463,"n":9,"t":4},"speaker":"A","text":"最初は誰でも続けるのが面倒ですが、慣れれば自然と身に付くものですよ。"},{"id":{"d":463,"n":10,"t":4},"speaker":"B","text":"そうした、花粉症対策の効果はありますか？"},{"id":{"d":463,"n":11,"t":4},"speaker":"A","text":"私の経験では、花粉症対策をするのとしないのとでは違うようです。それでも、花粉症の症状が全くなくなるわけではありません。"},{"id":{"d":463,"n":12,"t":4},"speaker":"B","text":"そうなのですね。私も、花粉症の兆候が出てきたら、そうするようにします。"}];
INSERT INTO sentence [{"id":{"d":464,"n":1,"t":4},"speaker":"A","text":"今日はどうされましたか？"},{"id":{"d":464,"n":2,"t":4},"speaker":"B","text":"健康診断で「血圧が高い」と言われまして。医者に診てもらうように勧められました。"},{"id":{"d":464,"n":3,"t":4},"speaker":"A","text":"収縮期血圧が153、弛緩期血圧が95ですから、確かに高いですね。降圧剤を処方しましょう。"},{"id":{"d":464,"n":4,"t":4},"speaker":"B","text":"生活習慣も関係があると聞きましたが、どのようなことに気を付ければよいのですか？"},{"id":{"d":464,"n":5,"t":4},"speaker":"A","text":"まず、塩分を控えることです。1日に6グラムを目標にしてください。"},{"id":{"d":464,"n":6,"t":4},"speaker":"B","text":"「1日に6グラム」と言われても、どの程度なのか分かりません。何か目安はありますか？"},{"id":{"d":464,"n":7,"t":4},"speaker":"A","text":"どの食べ物にどのくらい塩分が含まれているか、分かりやすく説明したパンフレットがありますから、これを参考にしてください。"},{"id":{"d":464,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。パンフレットを見る限り、確かに今の食事では塩分が多すぎるようです。私はどちらかというと、濃い味が好きなのです。私が何にでもしょうゆをかけるので、妻はいい顔をしないのですが、どうしてもやめられません。"},{"id":{"d":464,"n":9,"t":4},"speaker":"A","text":"だしを十分にとったり、酢をしっかり使ったりなど、いろいろと工夫をすればそれほどしょうゆを使わなくてもおいしく食べられますよ。"},{"id":{"d":464,"n":10,"t":4},"speaker":"B","text":"そうですか。妻と相談して、食事を変えてみようと思います。"},{"id":{"d":464,"n":11,"t":4},"speaker":"A","text":"そうしてください。それと、なるべく運動をするようにしてください。"},{"id":{"d":464,"n":12,"t":4},"speaker":"B","text":"はい、ウオーキングを始めようと思って、歩数計を買ったところです。"},{"id":{"d":464,"n":13,"t":4},"speaker":"A","text":"それはいいことですよ。始めからあまり無理をしないで、徐々に体を慣らすようにしてください。"}];
INSERT INTO sentence [{"id":{"d":465,"n":1,"t":4},"speaker":"A","text":"私は、不眠症にかかって困っています。何か良い方法を知りませんか？"},{"id":{"d":465,"n":2,"t":4},"speaker":"B","text":"不眠症といっても、いろいろありますが、あなたはどのような症状なのですか？"},{"id":{"d":465,"n":3,"t":4},"speaker":"A","text":"布団に入っても、なかなか寝付けないのです。夜中に、何度も起きてしまいます。"},{"id":{"d":465,"n":4,"t":4},"speaker":"B","text":"寝る前に、お茶やお酒を飲んだりしていませんか？"},{"id":{"d":465,"n":5,"t":4},"speaker":"A","text":"はい、食事の後はお茶を飲みますし、お酒を飲めば眠れるかと思って、寝る前に少し飲みます。"},{"id":{"d":465,"n":6,"t":4},"speaker":"B","text":"それが良くないのかもしれませんね。カフェインやアルコールを摂取すると、寝つきが悪くなるだけでなく、眠りが浅くなって夜中に起きてしまう原因になるそうですよ。"},{"id":{"d":465,"n":7,"t":4},"speaker":"A","text":"そうだったのですか。それでは、寝る前に、お茶や、お酒を飲むのをやめてみます。"},{"id":{"d":465,"n":8,"t":4},"speaker":"B","text":"まずは、それを実行してください。それでも不眠症が治らないようでしたら、また別の方法を考えましょう。"}];
INSERT INTO sentence [{"id":{"d":466,"n":1,"t":4},"speaker":"A","text":"以前、私は熱中症にかかったことがあります。"},{"id":{"d":466,"n":2,"t":4},"speaker":"B","text":"どのような状況で、熱中症にかかったのですか？"},{"id":{"d":466,"n":3,"t":4},"speaker":"A","text":"真夏の炎天下で、街を歩いていたのです。"},{"id":{"d":466,"n":4,"t":4},"speaker":"B","text":"水も飲まずに歩いていたのですか？"},{"id":{"d":466,"n":5,"t":4},"speaker":"A","text":"当時は、まだ今のように熱中症という症状が話題になっていませんでした。"},{"id":{"d":466,"n":6,"t":4},"speaker":"B","text":"それで、熱中症の危険性について知らなかったのですね？"},{"id":{"d":466,"n":7,"t":4},"speaker":"A","text":"はい。何となく体が怠くなって、どうしたのだろうと思っているうちに、立っていられなくなって、しばらくベンチで休みました。"},{"id":{"d":466,"n":8,"t":4},"speaker":"B","text":"救急車を、呼ばなかったのですか？"},{"id":{"d":466,"n":9,"t":4},"speaker":"A","text":"しばらく休んでいれば、治まるだろうと思っていたのです。"},{"id":{"d":466,"n":10,"t":4},"speaker":"B","text":"それで、治まったのですか？"},{"id":{"d":466,"n":11,"t":4},"speaker":"A","text":"自動販売機で、水を買って飲んでいるうちに治まりました。"},{"id":{"d":466,"n":12,"t":4},"speaker":"B","text":"危ないところでしたね。"}];
INSERT INTO sentence [{"id":{"d":467,"n":1,"t":4},"speaker":"A","text":"近ごろ足のむくみがひどくて、どのようにかならないものかと思っています。"},{"id":{"d":467,"n":2,"t":4},"speaker":"B","text":"良い方法がありますよ。私も以前、足がむくんでいたのですが、それで治りました。"},{"id":{"d":467,"n":3,"t":4},"speaker":"A","text":"そのような方法があるのですか。ぜひ教えてください。"},{"id":{"d":467,"n":4,"t":4},"speaker":"B","text":"足に適度な圧力をかける着圧ストッキングというのがあります。それを1日、はいておくのです。"},{"id":{"d":467,"n":5,"t":4},"speaker":"A","text":"それだけですか？"},{"id":{"d":467,"n":6,"t":4},"speaker":"B","text":"はい。足の血行やリンパの流れをよくするのだそうです。私は、1日目から効果がありました。"},{"id":{"d":467,"n":7,"t":4},"speaker":"A","text":"本当ですか？少し信じられません。"},{"id":{"d":467,"n":8,"t":4},"speaker":"B","text":"そう思うでしょう。私も驚いたくらいです。"},{"id":{"d":467,"n":9,"t":4},"speaker":"A","text":"それほど効くのなら、試してみる価値はありますね。どこで手に入れたのですか？"},{"id":{"d":467,"n":10,"t":4},"speaker":"B","text":"私はインターネット販売で買いましたが、ドラッグストアでも売っているのではないでしょうか？"},{"id":{"d":467,"n":11,"t":4},"speaker":"A","text":"良いことを教わりました。早速、着圧ストッキングを買って、試してみることにします。"}];
INSERT INTO sentence [{"id":{"d":468,"n":1,"t":4},"speaker":"A","text":"これほどひどい肩凝りになったのは、初めてです。"},{"id":{"d":468,"n":2,"t":4},"speaker":"B","text":"それほどに、ひどいのですか？"},{"id":{"d":468,"n":3,"t":4},"speaker":"A","text":"何しろ、首の上げ下げも自由にならないくらいなのです。"},{"id":{"d":468,"n":4,"t":4},"speaker":"B","text":"何か、思い当たることでもありますか？"},{"id":{"d":468,"n":5,"t":4},"speaker":"A","text":"最近、少し大工仕事をして、のこぎりを引いたり、ドライバーでネジを回したりしました。"},{"id":{"d":468,"n":6,"t":4},"speaker":"B","text":"たぶん、それが原因ですね。良い方法を教えましょう。以前、テレビの番組で放送していた方法です。"},{"id":{"d":468,"n":7,"t":4},"speaker":"A","text":"どのような方法ですか？"},{"id":{"d":468,"n":8,"t":4},"speaker":"B","text":"筋膜リリースといって、筋肉を包む膜をゆるめる方法です。"},{"id":{"d":468,"n":9,"t":4},"speaker":"A","text":"難しいのではありませんか？"},{"id":{"d":468,"n":10,"t":4},"speaker":"B","text":"いえ、少し根気は要りますが、難しいことはありません。"},{"id":{"d":468,"n":11,"t":4},"speaker":"A","text":"ぜひ、教えてください。"},{"id":{"d":468,"n":12,"t":4},"speaker":"B","text":"筋膜リリースの方法を解説した本がありますので、それを差し上げましょう。"},{"id":{"d":468,"n":13,"t":4},"speaker":"A","text":"いえ、頂くわけにはいきませんので、貸してください。"}];
INSERT INTO sentence [{"id":{"d":469,"n":1,"t":4},"speaker":"A","text":"あなたは数日前に「腰が痛い」と言っていましたけれど、治りましたか？"},{"id":{"d":469,"n":2,"t":4},"speaker":"B","text":"いや、相変わらずですよ。"},{"id":{"d":469,"n":3,"t":4},"speaker":"A","text":"そうなのではないかと思って、腰痛に効くという入浴剤を買っておきました。"},{"id":{"d":469,"n":4,"t":4},"speaker":"B","text":"そのようなもので、腰痛が治るとは思えませんがね。"},{"id":{"d":469,"n":5,"t":4},"speaker":"A","text":"物は試しですから、使ってみたらいかがですか？"},{"id":{"d":469,"n":6,"t":4},"speaker":"B","text":"あなたがそれほど言うのなら、使ってみましょう。"},{"id":{"d":469,"n":7,"t":4},"speaker":"A","text":"そうしてください。今、お風呂を入れますね。"},{"id":{"d":469,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。では、その間に、私は犬を連れて散歩でもしてきましょう。"}];
INSERT INTO sentence [{"id":{"d":470,"n":1,"t":4},"speaker":"A","text":"タトゥーが流行していますね。"},{"id":{"d":470,"n":2,"t":4},"speaker":"B","text":"今やファッションの一部になっていますね。"},{"id":{"d":470,"n":3,"t":4},"speaker":"A","text":"「入れ墨」と言っていた頃に比べると、隔世の感がします。"},{"id":{"d":470,"n":4,"t":4},"speaker":"B","text":"私が最初に驚いたのは、サッカーのデビッド・ベッカム元選手が首筋にタトゥーをしていたのを見たときです。"},{"id":{"d":470,"n":5,"t":4},"speaker":"A","text":"彼を見て、タトゥーに興味を持った人も少なくなさそうですね。"},{"id":{"d":470,"n":6,"t":4},"speaker":"B","text":"タトゥーには、元々呪術的な要素もあったそうです。"},{"id":{"d":470,"n":7,"t":4},"speaker":"A","text":"そうなのですね。欧米の人たちに、果たしてそのような感覚があるのかどうか、聞きたい気もします。"},{"id":{"d":470,"n":8,"t":4},"speaker":"B","text":"今のタトゥーの流行は、ヒッピー文化に源があるそうです。"},{"id":{"d":470,"n":9,"t":4},"speaker":"A","text":"ヒッピー文化はインドの宗教に影響を受けたのですから、そのような感覚があるのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":471,"n":1,"t":4},"speaker":"A","text":"あなたの、その口紅は良い色ですね。"},{"id":{"d":471,"n":2,"t":4},"speaker":"B","text":"そうですか？自分では少し派手かなと思っていたのですが。"},{"id":{"d":471,"n":3,"t":4},"speaker":"A","text":"そのようなことはありませんよ。よく似合っていますよ。"},{"id":{"d":471,"n":4,"t":4},"speaker":"B","text":"良かったです。少し冒険してみたかったのです。"},{"id":{"d":471,"n":5,"t":4},"speaker":"A","text":"とてもすてきです。気分が変わったでしょう？"},{"id":{"d":471,"n":6,"t":4},"speaker":"B","text":"ええ、少し前向きになった気がします。"},{"id":{"d":471,"n":7,"t":4},"speaker":"A","text":"いつもと違うことをしてみるのも、良いものですよね。"},{"id":{"d":471,"n":8,"t":4},"speaker":"B","text":"そうですね。もっといろんなことに挑戦してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":472,"n":1,"t":4},"speaker":"A","text":"息子が、少し虚弱なものですから、体を鍛えたいと思っています。"},{"id":{"d":472,"n":2,"t":4},"speaker":"B","text":"それなら、ぜひうちのサッカーチームに入りませんか？"},{"id":{"d":472,"n":3,"t":4},"speaker":"A","text":"人見知りをするところがあって、皆さんに溶け込んでいけるかどうか、心配なのですが。"},{"id":{"d":472,"n":4,"t":4},"speaker":"B","text":"誰でも初めはそうですよ。一緒に練習をしていると、自然に打ち解けていきます。"},{"id":{"d":472,"n":5,"t":4},"speaker":"A","text":"それでは、ぜひ見学に行かせてください。いつ、お伺いすればよいでしょうか？"},{"id":{"d":472,"n":6,"t":4},"speaker":"B","text":"いつでも、ご都合の良いときに来てください。毎週、日曜日の午前9時から練習をしています。"},{"id":{"d":472,"n":7,"t":4},"speaker":"A","text":"それでは今度の日曜日に伺います。何か持っていく物は、ありますか？"},{"id":{"d":472,"n":8,"t":4},"speaker":"B","text":"いえ、特にありません。運動ができるような服装をしていただければ十分です。"},{"id":{"d":472,"n":9,"t":4},"speaker":"A","text":"分かりました。当日は、どうぞよろしくお願いします。"},{"id":{"d":472,"n":10,"t":4},"speaker":"B","text":"はい、お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":473,"n":1,"t":4},"speaker":"A","text":"ヨガをしたいのですが、どこか良い教室を知りませんか？"},{"id":{"d":473,"n":2,"t":4},"speaker":"B","text":"それなら、スタジオ・ハタがいいですよ。実は私も通っているのです。"},{"id":{"d":473,"n":3,"t":4},"speaker":"A","text":"そうだったのですか。どのようなところですか？"},{"id":{"d":473,"n":4,"t":4},"speaker":"B","text":"雰囲気がすごく良くて、インストラクターの感じも良いですよ。"},{"id":{"d":473,"n":5,"t":4},"speaker":"A","text":"初めてなので、自分に合うかどうか、続けていけるかどうか心配なのですが。"},{"id":{"d":473,"n":6,"t":4},"speaker":"B","text":"体験レッスンというのがあって、それだと無料で受けられます。"},{"id":{"d":473,"n":7,"t":4},"speaker":"A","text":"では、その無料レッスンを受けてみます。予約が必要ですか？"},{"id":{"d":473,"n":8,"t":4},"speaker":"B","text":"いえ、予約は必要ありません。いつでも自由にレッスンを受けられます。"},{"id":{"d":473,"n":9,"t":4},"speaker":"A","text":"昼は都合がつかないので、夜にレッスンがあれば助かるのですが。"},{"id":{"d":473,"n":10,"t":4},"speaker":"B","text":"もちろん、夜もレッスンをしています。会社帰りにいつでも寄ることができます。"},{"id":{"d":473,"n":11,"t":4},"speaker":"A","text":"それは助かります。それでは、早速、明日の夜にでも行ってみます。"}];
INSERT INTO sentence [{"id":{"d":474,"n":1,"t":4},"speaker":"A","text":"どうしましたか？鼻をすすっていますね。"},{"id":{"d":474,"n":2,"t":4},"speaker":"B","text":"どうやら、風邪をひいたらしいのです。"},{"id":{"d":474,"n":3,"t":4},"speaker":"A","text":"ただの風邪なら良いのですが、今は新型コロナウイルス感染症のこともありますから、検査をしてもらった方がいいのではありませんか？"},{"id":{"d":474,"n":4,"t":4},"speaker":"B","text":"PCR検査は済んでいます。陰性でしたよ。"},{"id":{"d":474,"n":5,"t":4},"speaker":"A","text":"安心しました。実を言うと、私が濃厚接触者になったのではないかと少し不安になっていたのです。"},{"id":{"d":474,"n":6,"t":4},"speaker":"B","text":"すみません、先に言うべきでした。"},{"id":{"d":474,"n":7,"t":4},"speaker":"A","text":"いえ、こちらこそすみません。葛根湯なら持っていますよ。私は風邪のひき始めには、必ず飲むことにしています。"},{"id":{"d":474,"n":8,"t":4},"speaker":"B","text":"ちょうど切らしていたところなので、頂ければ助かります。"},{"id":{"d":474,"n":9,"t":4},"speaker":"A","text":"少し待っていてください。水と一緒に持ってきます。"},{"id":{"d":474,"n":10,"t":4},"speaker":"B","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":475,"n":1,"t":4},"speaker":"A","text":"日焼け止めクリームは、もう塗りましたか？"},{"id":{"d":475,"n":2,"t":4},"speaker":"B","text":"まだ塗っていません。"},{"id":{"d":475,"n":3,"t":4},"speaker":"A","text":"忘れてしまったのですね。外出するときは、必ず塗るように伝えていますよ。"},{"id":{"d":475,"n":4,"t":4},"speaker":"B","text":"分かっています。でも、面倒なので、つい忘れてしまうのです。"},{"id":{"d":475,"n":5,"t":4},"speaker":"A","text":"早く塗ってきてください。"},{"id":{"d":475,"n":6,"t":4},"speaker":"B","text":"日焼け止めクリームが見つかりません。いつもは化粧箱の中に入れてあるのに、見当たりません。"},{"id":{"d":475,"n":7,"t":4},"speaker":"A","text":"私は使っていませんよ。あなたが、どこかに置き忘れたのではありませんか？"},{"id":{"d":475,"n":8,"t":4},"speaker":"B","text":"ありました。歯磨きのそばに置いていました。歯磨き粉と、日焼け止めクリームの、パッケージがよく似ているから分かりませんでした。"}];
INSERT INTO sentence [{"id":{"d":476,"n":1,"t":4},"speaker":"A","text":"先月から、ウオーキングを始めました。"},{"id":{"d":476,"n":2,"t":4},"speaker":"B","text":"それは良いことですね。どのくらい歩いているのですか？"},{"id":{"d":476,"n":3,"t":4},"speaker":"A","text":"1日8,000歩を目標にしています。"},{"id":{"d":476,"n":4,"t":4},"speaker":"B","text":"1日1万歩はよく聞きますけれど、8,000歩という数に何か意味があるのですか？"},{"id":{"d":476,"n":5,"t":4},"speaker":"A","text":"最近読んだ本に、1日8,000歩歩けばアンチエイジング・プログラムが発動すると書いてあったのです。"},{"id":{"d":476,"n":6,"t":4},"speaker":"B","text":"それにしても、8,000歩歩くのは大変ではありませんか？"},{"id":{"d":476,"n":7,"t":4},"speaker":"A","text":"普段の生活をしていれば大体3,000歩くらいは歩いているそうですから、それも入れて8,000歩というのは、それほどきついものではありません。"},{"id":{"d":476,"n":8,"t":4},"speaker":"B","text":"それなら私にもできそうです。あなたを見習って、私も始めようと思います。"}];
INSERT INTO sentence [{"id":{"d":477,"n":1,"t":4},"speaker":"A","text":"あなたは、フィットネスクラブに、通い始めたそうですね。"},{"id":{"d":477,"n":2,"t":4},"speaker":"B","text":"はい、先月から、会社から帰る途中にある、駅前のクラブに通っています。"},{"id":{"d":477,"n":3,"t":4},"speaker":"A","text":"どうしてまた、急に思い立ったのですか？"},{"id":{"d":477,"n":4,"t":4},"speaker":"B","text":"最近、腰のベルトがきつくなったと実感していたところに、妻からも「おなかが出てきた」と言われました。これは良くないと思ったものですから、通うようになりました。"},{"id":{"d":477,"n":5,"t":4},"speaker":"A","text":"どうですか？効果はありますか？"},{"id":{"d":477,"n":6,"t":4},"speaker":"B","text":"まだ、通い始めて1カ月なので、目立った効果はありません。ただ、体調が良くなったような気がします。"},{"id":{"d":477,"n":7,"t":4},"speaker":"A","text":"それは良かったですね。どのような風に良くなったのですか？"},{"id":{"d":477,"n":8,"t":4},"speaker":"B","text":"寝つきが良くなりました。前は布団に入っても、なかなか眠れなくて困っていたのです。"},{"id":{"d":477,"n":9,"t":4},"speaker":"A","text":"不眠症が治ったとは、それは良いことを聞きました。実は、私も眠れなくて困っていたのです。"},{"id":{"d":477,"n":10,"t":4},"speaker":"B","text":"それなら、ぜひフィットネスクラブに通うといいですよ。私の通っているクラブに一緒に通いませんか？"},{"id":{"d":477,"n":11,"t":4},"speaker":"A","text":"そうします。紹介してください。"}];
INSERT INTO sentence [{"id":{"d":478,"n":1,"t":4},"speaker":"A","text":"あなたの目は、白内障がかなり進んでいます。なるべく早く手術を受ける方がいいと思います。"},{"id":{"d":478,"n":2,"t":4},"speaker":"B","text":"手術ですか。何だか怖い気もします。どのような手順で行うのですか？"},{"id":{"d":478,"n":3,"t":4},"speaker":"A","text":"まず、水晶体を包む袋の前面に、切れ目を入れます。"},{"id":{"d":478,"n":4,"t":4},"speaker":"B","text":"私は、痛みに弱いのです。注射の痛みでさえ怖いくらいです。"},{"id":{"d":478,"n":5,"t":4},"speaker":"A","text":"もちろん、麻酔をしますから、ほとんど痛みは感じませんよ。"},{"id":{"d":478,"n":6,"t":4},"speaker":"B","text":"そうですか。安心しました。"},{"id":{"d":478,"n":7,"t":4},"speaker":"A","text":"それから、超音波で水晶体を砕きながら、機械で吸引して取り除きます。取り除いた水晶体の代わりに、眼内レンズを入れて固定して、完了です。"},{"id":{"d":478,"n":8,"t":4},"speaker":"B","text":"手術時間は、どのくらいかかるのですか？"},{"id":{"d":478,"n":9,"t":4},"speaker":"A","text":"手術自体は、10分から15分くらいです。"},{"id":{"d":478,"n":10,"t":4},"speaker":"B","text":"手術の後は、安静にしなければいけないのでしょう？"},{"id":{"d":478,"n":11,"t":4},"speaker":"A","text":"以前のように、絶対安静ということはありません。最近だと、術後に気を付けるべきことは、目をこすったりしないこと、1週間は洗顔をしないこと、飲酒・喫煙を控えることくらいです。"}];
INSERT INTO sentence [{"id":{"d":479,"n":1,"t":4},"speaker":"A","text":"どうしましたか？"},{"id":{"d":479,"n":2,"t":4},"speaker":"B","text":"脇腹から背中にかけて、痛みがあるのです。"},{"id":{"d":479,"n":3,"t":4},"speaker":"A","text":"痛みは、いつごろから始まりましたか？"},{"id":{"d":479,"n":4,"t":4},"speaker":"B","text":"半年ほど前から、時々痛んでいました。"},{"id":{"d":479,"n":5,"t":4},"speaker":"A","text":"その間、何か市販薬を飲んだりしましたか？"},{"id":{"d":479,"n":6,"t":4},"speaker":"B","text":"いえ、何もしていません。強い痛みではなかったので、そのうち治るだろうと思っていたのです。"},{"id":{"d":479,"n":7,"t":4},"speaker":"A","text":"触診してみましょう。ベッドに横になってください。ここは、痛みますか？"},{"id":{"d":479,"n":8,"t":4},"speaker":"B","text":"いえ、痛みはありません。"},{"id":{"d":479,"n":9,"t":4},"speaker":"A","text":"ここはどうですか？"},{"id":{"d":479,"n":10,"t":4},"speaker":"B","text":"そこも、特に痛くありません。"},{"id":{"d":479,"n":11,"t":4},"speaker":"A","text":"念のため、CT検査をしてみましょう。この書類を持って、50番の検査室に行ってください。"},{"id":{"d":479,"n":12,"t":4},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":480,"n":1,"t":4},"speaker":"A","text":"今度の休みにキャンプに行きませんか？"},{"id":{"d":480,"n":2,"t":4},"speaker":"B","text":"キャンプに行くのは良いのですけれど、私はよく虫に刺されるのです。"},{"id":{"d":480,"n":3,"t":4},"speaker":"A","text":"虫除けスプレーをするなど、いろいろ対処法はありますよ。"},{"id":{"d":480,"n":4,"t":4},"speaker":"B","text":"以前、キャンプへ行ったとき、他の人は刺されなかったのに、私は体中を蚊に刺されたことがあります。"},{"id":{"d":480,"n":5,"t":4},"speaker":"A","text":"何か、蚊に刺されやすい人と、刺されにくい人は、体質の違いでもあるのですかね？"},{"id":{"d":480,"n":6,"t":4},"speaker":"B","text":"血液型に関係があると聞いたことがあります。"},{"id":{"d":480,"n":7,"t":4},"speaker":"A","text":"何型が、刺されやすいのですか？"},{"id":{"d":480,"n":8,"t":4},"speaker":"B","text":"O型が、刺されやすいのだそうです。"},{"id":{"d":480,"n":9,"t":4},"speaker":"A","text":"あなたは、O型ですか？"},{"id":{"d":480,"n":10,"t":4},"speaker":"B","text":"いえ、それがA型なのです。"},{"id":{"d":480,"n":11,"t":4},"speaker":"A","text":"血液型に関係があるという説も、当てにはならないということでしょうか？"},{"id":{"d":480,"n":12,"t":4},"speaker":"B","text":"そうかもしれませんね。それとも、何か他にも原因があるのかもしれません。"},{"id":{"d":480,"n":13,"t":4},"speaker":"A","text":"いずれにしろ、虫除けのスプレーを持って行けば、きっと大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":481,"n":1,"t":4},"speaker":"A","text":"私は、しゃくりが止まらなくなってしまいました。"},{"id":{"d":481,"n":2,"t":4},"speaker":"B","text":"それは大変です。もうすぐ映画が始まってしまいます。"},{"id":{"d":481,"n":3,"t":4},"speaker":"A","text":"困りました。みんなに迷惑をかけてしまいます。"},{"id":{"d":481,"n":4,"t":4},"speaker":"B","text":"「息を止めると、しゃっくりが止まる」と聞いたことがあります。"},{"id":{"d":481,"n":5,"t":4},"speaker":"A","text":"どうするのですか？"},{"id":{"d":481,"n":6,"t":4},"speaker":"B","text":"深く息を吸い込んで、ゆっくり吐くのです。"},{"id":{"d":481,"n":7,"t":4},"speaker":"A","text":"こうですか？"},{"id":{"d":481,"n":8,"t":4},"speaker":"B","text":"そうです。それを繰り返してください。"},{"id":{"d":481,"n":9,"t":4},"speaker":"A","text":"止まりました。助かりました。どうなることかと思いました。"},{"id":{"d":481,"n":10,"t":4},"speaker":"B","text":"ちょうど映画が始まりました。危ないところでしたね。"}];
INSERT INTO sentence [{"id":{"d":482,"n":1,"t":4},"speaker":"A","text":"友人が、パーキンソン病にかかってしまいました。"},{"id":{"d":482,"n":2,"t":4},"speaker":"B","text":"それはお気の毒です。何か兆候はなかったのですか？"},{"id":{"d":482,"n":3,"t":4},"speaker":"A","text":"前かがみの姿勢が少しずつひどくなり、歩く速さも遅くなって、どうしたのだろうと思っていたのです。"},{"id":{"d":482,"n":4,"t":4},"speaker":"B","text":"年を取れば、誰でも多かれ少なかれそうなるので、すぐにパーキンソン病だとは思いません。"},{"id":{"d":482,"n":5,"t":4},"speaker":"A","text":"友人は他の病気の診察で、かかりつけのクリニックに行ったのだそうです。"},{"id":{"d":482,"n":6,"t":4},"speaker":"B","text":"そのお医者さんが、パーキンソン病を疑ったのですか？"},{"id":{"d":482,"n":7,"t":4},"speaker":"A","text":"そうです。病室に入ってくる友人の歩き方を見てパーキンソン病ではないかと思って、精密検査を受けてみるように勧めたのだそうです。"},{"id":{"d":482,"n":8,"t":4},"speaker":"B","text":"パーキンソン病というのは、難しい病気だと聞いていますが、どのような治療をしているのですか？"},{"id":{"d":482,"n":9,"t":4},"speaker":"A","text":"脳内のドーパミンが不足しているので、それを補充する薬を飲んでいるそうです。"},{"id":{"d":482,"n":10,"t":4},"speaker":"B","text":"良くなるといいですね。"}];
INSERT INTO sentence [{"id":{"d":483,"n":1,"t":4},"speaker":"A","text":"2、3日前からまぶたが腫れて、押さえると痛みます。"},{"id":{"d":483,"n":2,"t":4},"speaker":"B","text":"麦粒腫のようですね。"},{"id":{"d":483,"n":3,"t":4},"speaker":"A","text":"麦粒腫というのは何ですか？初めて聞きました。"},{"id":{"d":483,"n":4,"t":4},"speaker":"B","text":"俗に言う、ものもらいです。まぶたの縁に、脂を出す腺があるのですが、そこに細菌が感染して炎症が起こったものです。"},{"id":{"d":483,"n":5,"t":4},"speaker":"A","text":"手術が必要ですか？"},{"id":{"d":483,"n":6,"t":4},"speaker":"B","text":"手術というほど大げさなものではなくて、少し切開して膿を出します。"},{"id":{"d":483,"n":7,"t":4},"speaker":"A","text":"痛いのでしょうか？私は痛みに弱いのです。"},{"id":{"d":483,"n":8,"t":4},"speaker":"B","text":"眼瞼麻酔で痛みを和らげますが、少しは痛むと思います。"},{"id":{"d":483,"n":9,"t":4},"speaker":"A","text":"薬で治すことはできませんか？"},{"id":{"d":483,"n":10,"t":4},"speaker":"B","text":"もう少し早ければ、薬で散らすこともできたでしょうが、ここまで腫れると難しいですね。"},{"id":{"d":483,"n":11,"t":4},"speaker":"A","text":"分かりました。では、仕方がありませんね。切開してください。"}];
INSERT INTO sentence [{"id":{"d":484,"n":1,"t":4},"speaker":"A","text":"冷たい水を飲むと、奥の方の歯がひどく痛みます。"},{"id":{"d":484,"n":2,"t":4},"speaker":"B","text":"確認しますね。これは、虫歯のようですね。いつごろから、痛んでいたのですか？"},{"id":{"d":484,"n":3,"t":4},"speaker":"A","text":"半年ほど前からです。それほどの痛みでもなかったので、だましだまししていたのですが、この2、3日、急に痛みが激しくなりました。"},{"id":{"d":484,"n":4,"t":4},"speaker":"B","text":"どうやら神経まで虫歯が進行しているようです。"},{"id":{"d":484,"n":5,"t":4},"speaker":"A","text":"そうでしたか。もっと早く、歯科医院へ来れば良かったです。"},{"id":{"d":484,"n":6,"t":4},"speaker":"B","text":"そうですね。もっと早く来てくれれば、治療も簡単でした。"},{"id":{"d":484,"n":7,"t":4},"speaker":"A","text":"歯を抜くことになるのでしょうか？"},{"id":{"d":484,"n":8,"t":4},"speaker":"B","text":"いえ、歯そのものは抜かなくても良いでしょう。神経を取って、内部を消毒し、薬を詰めて、クラウンというかぶせものをかぶせます。"},{"id":{"d":484,"n":9,"t":4},"speaker":"A","text":"分かりました。治療をお願いします。"}];
INSERT INTO sentence [{"id":{"d":485,"n":1,"t":4},"speaker":"A","text":"体が怠くて、何をする気にもなれません。"},{"id":{"d":485,"n":2,"t":4},"speaker":"B","text":"夏バテではありませんか？今年は、暑い日が続きましたから。"},{"id":{"d":485,"n":3,"t":4},"speaker":"A","text":"今夏は、猛暑日が続きましたね。天気予報番組では、「気象庁が統計を取り始めてから、今年の夏の猛暑日が、これまでの夏と比較して最も多かった」と言っていました。"},{"id":{"d":485,"n":4,"t":4},"speaker":"B","text":"夏バテは、冷房装置が効きすぎていることも原因の1つです。内外の温度差が大きいと、自律神経の働きが悪くなります。"},{"id":{"d":485,"n":5,"t":4},"speaker":"A","text":"何か対処する方法はないものでしょうか？"},{"id":{"d":485,"n":6,"t":4},"speaker":"B","text":"一番良いのは、水を飲むことで、それも一度に大量の水を飲むのではなくて、少しずつ、小まめに飲むことです。"},{"id":{"d":485,"n":7,"t":4},"speaker":"A","text":"そうだったのですか。私は、スポーツドリンクを一気に飲んでいました。"},{"id":{"d":485,"n":8,"t":4},"speaker":"B","text":"それから、汗をかくとエネルギーを消耗しますから、良質のたんぱく質を取ることです。"},{"id":{"d":485,"n":9,"t":4},"speaker":"A","text":"なるほど、地道な生活習慣が大事なのですね。"}];
INSERT INTO sentence [{"id":{"d":486,"n":1,"t":4},"speaker":"A","text":"数日前から、背中に痛みがあります。"},{"id":{"d":486,"n":2,"t":4},"speaker":"B","text":"背中の、どこが痛いですか？"},{"id":{"d":486,"n":3,"t":4},"speaker":"A","text":"首から背中、腰にかけて、痛みがあります。"},{"id":{"d":486,"n":4,"t":4},"speaker":"B","text":"しびれはありますか？"},{"id":{"d":486,"n":5,"t":4},"speaker":"A","text":"右足の指が少し、しびれています。"},{"id":{"d":486,"n":6,"t":4},"speaker":"B","text":"他に、気になるところはありますか？"},{"id":{"d":486,"n":7,"t":4},"speaker":"A","text":"右足の後ろ側もしびれています。"},{"id":{"d":486,"n":8,"t":4},"speaker":"B","text":"なるほど。それでは、まずはレントゲン写真を撮りましょう。"}];
INSERT INTO sentence [{"id":{"d":487,"n":1,"t":4},"speaker":"A","text":"明日は土曜日ですね。あなたは、何か予定はありますか？"},{"id":{"d":487,"n":2,"t":4},"speaker":"B","text":"サッカーの練習があります。"},{"id":{"d":487,"n":3,"t":4},"speaker":"A","text":"健康的な、休日の過ごし方ですね。"},{"id":{"d":487,"n":4,"t":4},"speaker":"B","text":"健康的といえば、健康的なのですが、このところ運動不足なので、バテそうです。"},{"id":{"d":487,"n":5,"t":4},"speaker":"A","text":"明日は暑くなりそうだから、熱中症の予防が必要ですね。"},{"id":{"d":487,"n":6,"t":4},"speaker":"B","text":"小まめに水分補給をするようにします。"},{"id":{"d":487,"n":7,"t":4},"speaker":"A","text":"でも、これほど夜遅くまでビールを飲んでいて大丈夫ですか？寝不足や二日酔いで激しい運動をすると、熱中症になりやすいそうですよ。"},{"id":{"d":487,"n":8,"t":4},"speaker":"B","text":"確かに、そうですね。そろそろ帰ります。"}];
INSERT INTO sentence [{"id":{"d":488,"n":1,"t":4},"speaker":"A","text":"来週、健康診断がありますね。あなたは、何曜日に受けますか？"},{"id":{"d":488,"n":2,"t":4},"speaker":"B","text":"来週ですか？知りませんでした。"},{"id":{"d":488,"n":3,"t":4},"speaker":"A","text":"大学からメールが来ていませんか？"},{"id":{"d":488,"n":4,"t":4},"speaker":"B","text":"ああ、ありました。これは、受けないといけないのですか？"},{"id":{"d":488,"n":5,"t":4},"speaker":"A","text":"受けないと、健康診断書を発行してもらえません。"},{"id":{"d":488,"n":6,"t":4},"speaker":"B","text":"健康診断書がないと、困りますか？"},{"id":{"d":488,"n":7,"t":4},"speaker":"A","text":"健康診断書がないと、学内のジムやプールの利用ができません。"},{"id":{"d":488,"n":8,"t":4},"speaker":"B","text":"それは困ります。それでは、水曜日に受けることにします。"}];
INSERT INTO sentence [{"id":{"d":489,"n":1,"t":4},"speaker":"A","text":"健康診断では、何の検査をしますか？"},{"id":{"d":489,"n":2,"t":4},"speaker":"B","text":"学年によって検査項目が違います。全学年に共通の項目は、身長、体重、視力、血圧の測定と、血液検査、尿検査、それからお医者さんによる診察です。"},{"id":{"d":489,"n":3,"t":4},"speaker":"A","text":"たくさんの検査をするのですね。私は1年生です。"},{"id":{"d":489,"n":4,"t":4},"speaker":"B","text":"新入生は、これ以外に胸部X線検査も受けますね。"},{"id":{"d":489,"n":5,"t":4},"speaker":"A","text":"分かりました。健康診断へは、どのような服装で行ったら良いですか？"},{"id":{"d":489,"n":6,"t":4},"speaker":"B","text":"脱ぎ着がしやすい服や靴が良いですよ。"},{"id":{"d":489,"n":7,"t":4},"speaker":"A","text":"靴も脱ぐのですか？"},{"id":{"d":489,"n":8,"t":4},"speaker":"B","text":"はい。身長、体重測定の時に、靴を脱ぐのです。"},{"id":{"d":489,"n":9,"t":4},"speaker":"A","text":"なるほど。よく分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":490,"n":1,"t":4},"speaker":"A","text":"目が赤いですね。どうしたのですか？"},{"id":{"d":490,"n":2,"t":4},"speaker":"B","text":"花粉症です。毎年、この季節になると、目が充血してしまうのです。それに、かゆくてたまりません。"},{"id":{"d":490,"n":3,"t":4},"speaker":"A","text":"それはつらいですね。"},{"id":{"d":490,"n":4,"t":4},"speaker":"B","text":"飲み薬を処方してもらったのですが、飲むと眠くなるので困ります。"},{"id":{"d":490,"n":5,"t":4},"speaker":"A","text":"それで、この頃、授業中に居眠りをしているのですね。"},{"id":{"d":490,"n":6,"t":4},"speaker":"B","text":"薬を飲まないと、くしゃみのしすぎで腹筋が痛くなります。スギ花粉症なのです。"},{"id":{"d":490,"n":7,"t":4},"speaker":"A","text":"このキャンパスには見事なスギ並木があるから、大変ですね。"},{"id":{"d":490,"n":8,"t":4},"speaker":"B","text":"教室にたどり着くまでに、100回くらいくしゃみが出ます。"}];
INSERT INTO sentence [{"id":{"d":491,"n":1,"t":4},"speaker":"A","text":"最近は、皇居の周りをランニングする人を、よく見かけますね。"},{"id":{"d":491,"n":2,"t":4},"speaker":"B","text":"皇居ランですね。私も走ったことがあります。"},{"id":{"d":491,"n":3,"t":4},"speaker":"A","text":"どのくらいの距離を走るのですか？"},{"id":{"d":491,"n":4,"t":4},"speaker":"B","text":"1周、5キロメートルくらいです。信号待ちがないので、気持ち良く走れました。"},{"id":{"d":491,"n":5,"t":4},"speaker":"A","text":"私も走りたいけれど、最近、運動不足なので自信がありません。"},{"id":{"d":491,"n":6,"t":4},"speaker":"B","text":"それなら、ウオーキングからはじめたらどうでしょう。"},{"id":{"d":491,"n":7,"t":4},"speaker":"A","text":"それが良さそうですね。休みの日に、近所の緑道を歩いてみます。"},{"id":{"d":491,"n":8,"t":4},"speaker":"B","text":"ウオーキングの前後にはストレッチ体操をしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":492,"n":1,"t":4},"speaker":"A","text":"不快そうな顔をして、どうしたのですか？"},{"id":{"d":492,"n":2,"t":4},"speaker":"B","text":"1時間くらい前から、頭が痛いのです。"},{"id":{"d":492,"n":3,"t":4},"speaker":"A","text":"発熱はありますか？"},{"id":{"d":492,"n":4,"t":4},"speaker":"B","text":"先ほど測ったら、36度2分で、平熱でした。"},{"id":{"d":492,"n":5,"t":4},"speaker":"A","text":"そうですか。頭痛があまりひどいようなら、頭痛薬を飲んだらどうですか？"},{"id":{"d":492,"n":6,"t":4},"speaker":"B","text":"20分くらい前に飲みました。"},{"id":{"d":492,"n":7,"t":4},"speaker":"A","text":"それなら、そろそろ効いてくる頃ですね。"},{"id":{"d":492,"n":8,"t":4},"speaker":"B","text":"そうだといいのですが。そういえば、頭痛が治まってきました。"},{"id":{"d":492,"n":9,"t":4},"speaker":"A","text":"良かったですね。"}];
INSERT INTO sentence [{"id":{"d":493,"n":1,"t":4},"speaker":"A","text":"ドラッグストアがありますね。少し寄ってもよいですか？"},{"id":{"d":493,"n":2,"t":4},"speaker":"B","text":"良いですよ。何を買うのですか？"},{"id":{"d":493,"n":3,"t":4},"speaker":"A","text":"ハワイ旅行に持参する薬を、買っておこうと思うのです。"},{"id":{"d":493,"n":4,"t":4},"speaker":"B","text":"飲み慣れた風邪薬や解熱剤を持って行くと、安心できますよね。"},{"id":{"d":493,"n":5,"t":4},"speaker":"A","text":"はい。旅行中は食べ過ぎてしまうので、胃腸薬も買っておきます。他に用意しておいた方がいいものはあるでしょうか？"},{"id":{"d":493,"n":6,"t":4},"speaker":"B","text":"旅行中は体調を崩しやすいので、私はビタミン剤を飲むようにしています。"},{"id":{"d":493,"n":7,"t":4},"speaker":"A","text":"それは良いですね。私は旅行中に口内炎ができやすいので、ビタミン剤を飲むと予防できるかもしれません。"},{"id":{"d":493,"n":8,"t":4},"speaker":"B","text":"それから、日焼け止めも持って行った方がいいですよ。"},{"id":{"d":493,"n":9,"t":4},"speaker":"A","text":"日焼け止めは、現地でも買えませんか？"},{"id":{"d":493,"n":10,"t":4},"speaker":"B","text":"現地でも買えるのですが、事前に買っておくことをお薦めします。以前、私がハワイへ行った時には、現地で日焼け止めを入手する前に、ひどく日焼けしてしまいました。"}];
INSERT INTO sentence [{"id":{"d":494,"n":1,"t":4},"speaker":"A","text":"インフルエンザの予防のために、あなたは何かしていますか？"},{"id":{"d":494,"n":2,"t":4},"speaker":"B","text":"今年は、弟の高校受験があるので、家族全員で予防接種を受けました。"},{"id":{"d":494,"n":3,"t":4},"speaker":"A","text":"私も、今年は妹が受験生なので、インフルエンザには気を付けています。"},{"id":{"d":494,"n":4,"t":4},"speaker":"B","text":"予防には「手洗い、うがい、マスク」とよく言われていますね。"},{"id":{"d":494,"n":5,"t":4},"speaker":"A","text":"うがいはインフルエンザには効果がないという意見もありますが、風邪の予防にはなりますよね。"},{"id":{"d":494,"n":6,"t":4},"speaker":"B","text":"そうですね。私は習慣で、家に帰ると手洗いと、うがいをしています。それから、部屋の乾燥対策に加湿器を使っています。"},{"id":{"d":494,"n":7,"t":4},"speaker":"A","text":"そういえば、先週、父が加湿器を買ってきました。これもインフルエンザ予防だったのですね。"},{"id":{"d":494,"n":8,"t":4},"speaker":"B","text":"息子さん思いのお父さんですね。"}];
INSERT INTO sentence [{"id":{"d":495,"n":1,"t":4},"speaker":"A","text":"手を、けがしたのですか？"},{"id":{"d":495,"n":2,"t":4},"speaker":"B","text":"雪道で転んで、手首を骨折しました。"},{"id":{"d":495,"n":3,"t":4},"speaker":"A","text":"それは大変でしたね。手術したのですか？"},{"id":{"d":495,"n":4,"t":4},"speaker":"B","text":"幸い、手術はせずに済みました。ギプスで固定して、三角巾でつっています。"},{"id":{"d":495,"n":5,"t":4},"speaker":"A","text":"これから、診察ですか？"},{"id":{"d":495,"n":6,"t":4},"speaker":"B","text":"いいえ、今日はリハビリテーションです。"},{"id":{"d":495,"n":7,"t":4},"speaker":"A","text":"ギプスが取れたら、リハビリテーションも終わるのですか？"},{"id":{"d":495,"n":8,"t":4},"speaker":"B","text":"多分、ギプスが取れた後も、しばらくリハビリテーションに通うことになると思います。"},{"id":{"d":495,"n":9,"t":4},"speaker":"A","text":"そうですか。お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":496,"n":1,"t":4},"speaker":"A","text":"今日は、ジョギングをしないのですか？"},{"id":{"d":496,"n":2,"t":4},"speaker":"B","text":"風邪気味なので、やめておきます。なんだか寒気がするのです。"},{"id":{"d":496,"n":3,"t":4},"speaker":"A","text":"そういえば、くしゃみをしていましたね。"},{"id":{"d":496,"n":4,"t":4},"speaker":"B","text":"鼻詰まりがひどいので、走ったら息切れしそうです。"},{"id":{"d":496,"n":5,"t":4},"speaker":"A","text":"それは、ゆっくり休んだ方がいいですね。喉の痛みはありますか？"},{"id":{"d":496,"n":6,"t":4},"speaker":"B","text":"喉も少し、違和感があります。"},{"id":{"d":496,"n":7,"t":4},"speaker":"A","text":"喉あめを持っています。お1つ、いかがですか？"},{"id":{"d":496,"n":8,"t":4},"speaker":"B","text":"ありがとう。これは効きますね。喉も鼻もすっきりします。"}];
INSERT INTO sentence [{"id":{"d":497,"n":1,"t":4},"speaker":"A","text":"スポーツは、お好きですか？"},{"id":{"d":497,"n":2,"t":4},"speaker":"B","text":"好きです。私は、小学生の頃から野球をしていて、今でも草野球チームに入っています。"},{"id":{"d":497,"n":3,"t":4},"speaker":"A","text":"それは良いですね。私はサッカー少年でした。今でも月に一度、職場の仲間と一緒に汗を流しています。"},{"id":{"d":497,"n":4,"t":4},"speaker":"B","text":"球技は楽しいですよね。"},{"id":{"d":497,"n":5,"t":4},"speaker":"A","text":"はい。球技は見るのも楽しいですね。私は、野球はしませんが、野球観戦は好きです。"},{"id":{"d":497,"n":6,"t":4},"speaker":"B","text":"プロ野球ですか？"},{"id":{"d":497,"n":7,"t":4},"speaker":"A","text":"プロ野球も好きですが、高校の野球の試合を観戦することが好きです。見ているだけで、元気が出ます。"},{"id":{"d":497,"n":8,"t":4},"speaker":"B","text":"そうですね。私は九州出身なので、九州の高校をつい応援してしまいます。"}];
INSERT INTO sentence [{"id":{"d":498,"n":1,"t":4},"speaker":"A","text":"今日の野球の試合はどうでしたか？"},{"id":{"d":498,"n":2,"t":4},"speaker":"B","text":"8対7で負けました。"},{"id":{"d":498,"n":3,"t":4},"speaker":"A","text":"それは惜しいですね。"},{"id":{"d":498,"n":4,"t":4},"speaker":"B","text":"相手のピッチャーがうますぎました。速いストレートの間に、カーブを投げてくるのです。"},{"id":{"d":498,"n":5,"t":4},"speaker":"A","text":"でも、8対7とは、頑張りましたね。"},{"id":{"d":498,"n":6,"t":4},"speaker":"B","text":"はい。チームワークでどのようにかして、点を積み上げました。力みすぎると、フライや空振り三振でアウトになるので、確実にランナーを送ることを考えました。"},{"id":{"d":498,"n":7,"t":4},"speaker":"A","text":"それにしても、泥だらけですね。"},{"id":{"d":498,"n":8,"t":4},"speaker":"B","text":"はい。送りバントで思い切り走って、スライディングをしました。自分はアウトになる覚悟でしたが、相手の送球ミスでセーフになりました。"}];
INSERT INTO sentence [{"id":{"d":499,"n":1,"t":4},"speaker":"A","text":"最近、私は腰痛に悩まされています。"},{"id":{"d":499,"n":2,"t":4},"speaker":"B","text":"立ち仕事や力仕事が多いのですか？"},{"id":{"d":499,"n":3,"t":4},"speaker":"A","text":"いいえ。デスクワークです。長時間パソコンの前に座っています。"},{"id":{"d":499,"n":4,"t":4},"speaker":"B","text":"デスクワークも腰痛になりやすいと言いますよね。"},{"id":{"d":499,"n":5,"t":4},"speaker":"A","text":"そのようですね。職業病といっても良いかもしれません。"},{"id":{"d":499,"n":6,"t":4},"speaker":"B","text":"早めに整形外科を受診した方がいいですよ。私も腰痛で通院しています。"},{"id":{"d":499,"n":7,"t":4},"speaker":"A","text":"あなたもデスクワークですか？"},{"id":{"d":499,"n":8,"t":4},"speaker":"B","text":"それもありますが、最近は父の介護を手伝っているので、腰に負担がかかっているのだと思います。"},{"id":{"d":499,"n":9,"t":4},"speaker":"A","text":"それは大変ですね。"}];
INSERT INTO sentence [{"id":{"d":500,"n":1,"t":4},"speaker":"A","text":"今日は暑いですね。"},{"id":{"d":500,"n":2,"t":4},"speaker":"B","text":"夏バテでしょうか？朝から目まいがして、あくびが出ます。"},{"id":{"d":500,"n":3,"t":4},"speaker":"A","text":"それは、熱中症ではありませんか？"},{"id":{"d":500,"n":4,"t":4},"speaker":"B","text":"そういえば、夜中に足がつりました。言われてみれば、熱中症かもしれません。"},{"id":{"d":500,"n":5,"t":4},"speaker":"A","text":"まずは、涼しいところで休みましょう。"},{"id":{"d":500,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。単なる夏バテかと思っていました。"},{"id":{"d":500,"n":7,"t":4},"speaker":"A","text":"中で休んでいてください。スポーツドリンクを買ってきます。"},{"id":{"d":500,"n":8,"t":4},"speaker":"B","text":"本当にありがとう。助かります。"}];
INSERT INTO sentence [{"id":{"d":501,"n":1,"t":4},"speaker":"A","text":"私は持病を持っていますが、新型コロナワクチンを接種することはできますか？"},{"id":{"d":501,"n":2,"t":4},"speaker":"B","text":"新型コロナワクチン接種は、体調の良いときに受けるのが基本ですが、かかっている病気によります。"},{"id":{"d":501,"n":3,"t":4},"speaker":"A","text":"私は、高血圧の薬を飲んでいるのです。"},{"id":{"d":501,"n":4,"t":4},"speaker":"B","text":"私も高血圧ですが、新型コロナワクチン接種しました。"},{"id":{"d":501,"n":5,"t":4},"speaker":"A","text":"そうですか。それでは、かかりつけ医に相談してみます。"},{"id":{"d":501,"n":6,"t":4},"speaker":"B","text":"高血圧などの持病がある人は、優先的に新型コロナワクチン接種を受けられるそうですよ。"},{"id":{"d":501,"n":7,"t":4},"speaker":"A","text":"そうなのですか。知りませんでした。"},{"id":{"d":501,"n":8,"t":4},"speaker":"B","text":"私は市役所に申請して、新型コロナワクチンの接種券を送ってもらいました。"}];
INSERT INTO sentence [{"id":{"d":502,"n":1,"t":4},"speaker":"A","text":"虫に刺されたところが、かゆくてたまりません。"},{"id":{"d":502,"n":2,"t":4},"speaker":"B","text":"虫刺されでしたら、この塗り薬が効きます。"},{"id":{"d":502,"n":3,"t":4},"speaker":"A","text":"かゆみ止めですか？"},{"id":{"d":502,"n":4,"t":4},"speaker":"B","text":"はい。かゆみにスッと効きますよ。"},{"id":{"d":502,"n":5,"t":4},"speaker":"A","text":"かきむしって傷になっていますが、その上から塗っても大丈夫ですか？"},{"id":{"d":502,"n":6,"t":4},"speaker":"B","text":"問題ありません。この薬には、殺菌成分も入っているので、傷口の化膿も予防します。"},{"id":{"d":502,"n":7,"t":4},"speaker":"A","text":"それでは、これを下さい。それから、虫除けスプレーもお願いします。子どもにも使えるものがいいです。"},{"id":{"d":502,"n":8,"t":4},"speaker":"B","text":"かしこまりました。お子さまにも使える虫除けスプレーでしたら、こちらに3種類ございます。"}];
INSERT INTO sentence [{"id":{"d":503,"n":1,"t":4},"speaker":"A","text":"このシャンプーのボトルには、細かい刻みがついています。"},{"id":{"d":503,"n":2,"t":4},"speaker":"B","text":"本当ですね。何のためについているのですか？"},{"id":{"d":503,"n":3,"t":4},"speaker":"A","text":"触っただけで、シャンプーとリンスを区別できるようにするためです。"},{"id":{"d":503,"n":4,"t":4},"speaker":"B","text":"それは便利ですね。私は老眼が進んできたので、シャンプーとリンスを間違えることがあります。"},{"id":{"d":503,"n":5,"t":4},"speaker":"A","text":"私も近視なので、以前は困っていました。"},{"id":{"d":503,"n":6,"t":4},"speaker":"B","text":"家で使っているシャンプーにも、刻み目がついているのかもしれませんね。気が付きませんでした。"},{"id":{"d":503,"n":7,"t":4},"speaker":"A","text":"最初に考案した会社が、実用新案の申請を取り下げて、業界に働きかけたそうです。それで、他の会社のシャンプーにも刻み目がついているそうですよ。"},{"id":{"d":503,"n":8,"t":4},"speaker":"B","text":"まさに、ユニバーサルデザインですね。"}];
INSERT INTO sentence [{"id":{"d":504,"n":1,"t":4},"speaker":"A","text":"子どもが、「おなかが痛い」と言っています。"},{"id":{"d":504,"n":2,"t":4},"speaker":"B","text":"腹痛の他に、どのような症状がありますか？"},{"id":{"d":504,"n":3,"t":4},"speaker":"A","text":"下痢をしていて、微熱があります。"},{"id":{"d":504,"n":4,"t":4},"speaker":"B","text":"食あたりかもしれませんね。"},{"id":{"d":504,"n":5,"t":4},"speaker":"A","text":"食あたりですか？"},{"id":{"d":504,"n":6,"t":4},"speaker":"B","text":"はい。食中毒のことです。"},{"id":{"d":504,"n":7,"t":4},"speaker":"A","text":"病院に行った方がいいでしょうか？"},{"id":{"d":504,"n":8,"t":4},"speaker":"B","text":"そうですね。小児科か内科を受診すると良いでしょう。"}];
INSERT INTO sentence [{"id":{"d":505,"n":1,"t":4},"speaker":"A","text":"今朝、ジョギングをしていたら、子どもたちが公園に集まって体操をしていました。"},{"id":{"d":505,"n":2,"t":4},"speaker":"B","text":"ああ、ラジオ体操ですね。夏休みになると、あちらこちらの公園で子ども会や町内会がラジオ体操をしていますね。"},{"id":{"d":505,"n":3,"t":4},"speaker":"A","text":"ラジオ体操とは、ラジオ番組ですか？"},{"id":{"d":505,"n":4,"t":4},"speaker":"B","text":"元々はラジオ番組ですが、今ではラジオでもテレビでも放送しています。CDやDVDも出ていますよ。"},{"id":{"d":505,"n":5,"t":4},"speaker":"A","text":"知りませんでした。ラジオやテレビでラジオ体操が放送されるのは、夏の間だけなのですか？"},{"id":{"d":505,"n":6,"t":4},"speaker":"B","text":"いえ、ラジオ体操は、1年中放送しています。90年以上も前から続いているのだそうですよ。"},{"id":{"d":505,"n":7,"t":4},"speaker":"A","text":"それほど前から、あるのですか？すごいですね。"},{"id":{"d":505,"n":8,"t":4},"speaker":"B","text":"小学校で習うので、日本で育った人はたいてい知っています。ラジオ体操第1とラジオ体操第2があり、第1は柔軟体操が中心です。3分ほどですが、しっかり体を動かせば、なかなか良い運動になります。"},{"id":{"d":505,"n":9,"t":4},"speaker":"A","text":"それなら、ジョギング前のストレッチ体操として取り入れたいと思います。"}];
INSERT INTO sentence [{"id":{"d":506,"n":1,"t":4},"speaker":"A","text":"醤油を取ってください。"},{"id":{"d":506,"n":2,"t":4},"speaker":"B","text":"はい、どうぞ。"},{"id":{"d":506,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":506,"n":4,"t":4},"speaker":"B","text":"そのようにたくさんかけたら塩辛くありませんか？"},{"id":{"d":506,"n":5,"t":4},"speaker":"A","text":"これくらいではあまり塩辛いとは感じませんが、私がかけた量は多いですか？"},{"id":{"d":506,"n":6,"t":4},"speaker":"B","text":"少し多いと思います。"},{"id":{"d":506,"n":7,"t":4},"speaker":"A","text":"つい、かけ過ぎてしまうのです。"},{"id":{"d":506,"n":8,"t":4},"speaker":"B","text":"塩分の取り過ぎは体に良くないので、気を付けた方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":507,"n":1,"t":4},"speaker":"A","text":"そろそろ寝ますね。"},{"id":{"d":507,"n":2,"t":4},"speaker":"B","text":"私はもう少し起きています。"},{"id":{"d":507,"n":3,"t":4},"speaker":"A","text":"昨夜も遅かったようですが、寝不足になりませんか？"},{"id":{"d":507,"n":4,"t":4},"speaker":"B","text":"少し寝不足気味です。でもこの本が面白くて、どんどん読み進めてしまうのです。"},{"id":{"d":507,"n":5,"t":4},"speaker":"A","text":"気持ちは分かりますが、朝起きるのが辛くなってしまうので、あまり夜更かししない方がいいですよ。"},{"id":{"d":507,"n":6,"t":4},"speaker":"B","text":"今読んでいる章を読み終えたら、寝ることにします。"},{"id":{"d":507,"n":7,"t":4},"speaker":"A","text":"それでは、先に休んでいますね。"},{"id":{"d":507,"n":8,"t":4},"speaker":"B","text":"はい。おやすみなさい。"}];
INSERT INTO sentence [{"id":{"d":508,"n":1,"t":4},"speaker":"A","text":"最近は、雨ばかりで運動不足気味です。"},{"id":{"d":508,"n":2,"t":4},"speaker":"B","text":"私も運動不足が気になっています。"},{"id":{"d":508,"n":3,"t":4},"speaker":"A","text":"家の中でもできる運動は何かないでしょうか？"},{"id":{"d":508,"n":4,"t":4},"speaker":"B","text":"ヨガはどうですか？"},{"id":{"d":508,"n":5,"t":4},"speaker":"A","text":"ヨガはやったことがありません。"},{"id":{"d":508,"n":6,"t":4},"speaker":"B","text":"私は前に少し習っていました。よければ、教えますので一緒にやりませんか？"},{"id":{"d":508,"n":7,"t":4},"speaker":"A","text":"それはありがたいです。初めてでも大丈夫でしょうか？"},{"id":{"d":508,"n":8,"t":4},"speaker":"B","text":"簡単なものから教えますので安心してください。"}];
