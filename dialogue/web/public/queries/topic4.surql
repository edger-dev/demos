INSERT INTO sentence [{"id":{"d":526,"n":1,"t":4},"speaker":"A","text":"血圧を測りますね。"},{"id":{"d":526,"n":2,"t":4},"speaker":"B","text":"お願いします。"},{"id":{"d":526,"n":3,"t":4},"speaker":"A","text":"この血圧計に腕を入れてください。"},{"id":{"d":526,"n":4,"t":4},"speaker":"B","text":"これでよいですか？"},{"id":{"d":526,"n":5,"t":4},"speaker":"A","text":"もう少し奥まで入れてください。"},{"id":{"d":526,"n":6,"t":4},"speaker":"B","text":"はい。これでどうでしょう？"},{"id":{"d":526,"n":7,"t":4},"speaker":"A","text":"それでよいです。それでは測定しますので、腕を動かさないでください。"},{"id":{"d":526,"n":8,"t":4},"speaker":"B","text":"分かりました。"}];
