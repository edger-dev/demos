INSERT INTO sentence [{"id":{"d":521,"n":1,"t":4},"speaker":"A","text":"ハンドクリームを家に忘れてきてしまいました。"},{"id":{"d":521,"n":2,"t":4},"speaker":"B","text":"よかったら私のものを使ってください。"},{"id":{"d":521,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。パッケージにフランス語が書いてありますね。"},{"id":{"d":521,"n":4,"t":4},"speaker":"B","text":"友達から、フランス旅行のお土産でもらったものです。"},{"id":{"d":521,"n":5,"t":4},"speaker":"A","text":"道理で、見たことのないハンドクリームだと思いました。"},{"id":{"d":521,"n":6,"t":4},"speaker":"B","text":"フランスでは人気の商品らしいです。"},{"id":{"d":521,"n":7,"t":4},"speaker":"A","text":"そうなのですね。とても華やかな香りがします。"},{"id":{"d":521,"n":8,"t":4},"speaker":"B","text":"香りもですが使い心地もいいので、気に入っています。"}];
