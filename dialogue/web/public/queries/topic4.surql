INSERT INTO sentence [{"id":{"d":1,"n":1,"t":4},"speaker":"A","text":"失礼します。"},{"id":{"d":1,"n":2,"t":4},"speaker":"B","text":"今日はどうされましたか？"},{"id":{"d":1,"n":3,"t":4},"speaker":"A","text":"3日ほど前から、せきが続いています。体温も高めです。"},{"id":{"d":1,"n":4,"t":4},"speaker":"B","text":"体温はどのくらいありますか？"},{"id":{"d":1,"n":5,"t":4},"speaker":"A","text":"自宅を出る前に測ったときは36度7分、昨日は37度5分ありました。"},{"id":{"d":1,"n":6,"t":4},"speaker":"B","text":"そうですか。それでは喉の腫れ具合を診ていきますね。"},{"id":{"d":1,"n":7,"t":4},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":2,"n":1,"t":4},"speaker":"A","text":"加藤君、サッカーをしませんか？"},{"id":{"d":2,"n":2,"t":4},"speaker":"B","text":"ごめんなさい。足が痛いので今日はやめておきます。"},{"id":{"d":2,"n":3,"t":4},"speaker":"A","text":"大丈夫ですか？どうしたのですか？"},{"id":{"d":2,"n":4,"t":4},"speaker":"B","text":"はい。家に帰るときに足をくじいてしまいました。"},{"id":{"d":2,"n":5,"t":4},"speaker":"A","text":"それは痛そうです。気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":3,"n":1,"t":4},"speaker":"A","text":"少し散歩に出かけてきます。"},{"id":{"d":3,"n":2,"t":4},"speaker":"B","text":"あら、珍しいですね。いつ頃になったら、戻りますか？"},{"id":{"d":3,"n":3,"t":4},"speaker":"A","text":"近所を一回りしてくるので、10分くらい後に戻ります。"},{"id":{"d":3,"n":4,"t":4},"speaker":"B","text":"分かりました。気を付けていってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":4,"n":1,"t":4},"speaker":"A","text":"先生、検査の結果はどうでしたか？"},{"id":{"d":4,"n":2,"t":4},"speaker":"B","text":"コレステロール値が高いですね。何か心当たりはありますか？"},{"id":{"d":4,"n":3,"t":4},"speaker":"A","text":"最近は、仕事が忙しくて食生活が乱れているのです。夕飯を取るのが夜遅くになることもしばしばあります。"},{"id":{"d":4,"n":4,"t":4},"speaker":"B","text":"恐らく、それが原因でしょう。今後は仕事をセーブして、バランスの良い食事を心掛けてください。"},{"id":{"d":4,"n":5,"t":4},"speaker":"A","text":"はい。分かりました。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":5,"n":1,"t":4},"speaker":"A","text":"最近太ってきてしまったので、筋力トレーニングを始めようかなと思っています。"},{"id":{"d":5,"n":2,"t":4},"speaker":"B","text":"良いですね。ジムに通うのはいかがですか？"},{"id":{"d":5,"n":3,"t":4},"speaker":"A","text":"いろいろなマシンがあり、使い方がよく分かりません。"},{"id":{"d":5,"n":4,"t":4},"speaker":"B","text":"それなら、パーソナルジムに通うのはいかがですか？トレーナーがついていろいろ教えてくれます。"},{"id":{"d":5,"n":5,"t":4},"speaker":"A","text":"それは魅力的ですね。検討してみます。"}];
INSERT INTO sentence [{"id":{"d":6,"n":1,"t":4},"speaker":"A","text":"すみません。少しお尋ねしたいことがあるのですが、佐藤病院に行くにはどのように行けば良いですか？"},{"id":{"d":6,"n":2,"t":4},"speaker":"B","text":"この通りを右に出てもらって、2つ目の信号を左に曲がってもらって、そのまま真っすぐ行ってもらうと佐藤病院に着きます。"},{"id":{"d":6,"n":3,"t":4},"speaker":"A","text":"歩くと、どのくらい時間がかかりますか？"},{"id":{"d":6,"n":4,"t":4},"speaker":"B","text":"10分くらいで着くと思います。"},{"id":{"d":6,"n":5,"t":4},"speaker":"A","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":7,"n":1,"t":4},"speaker":"A","text":"この間、野球の大会で手首にボールが当たってしまいました。痛みがひどいです。"},{"id":{"d":7,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？病院には行きましたか？"},{"id":{"d":7,"n":3,"t":4},"speaker":"A","text":"レントゲンを撮ってもらったところ、手首の骨にひびが入っていました。「野球の練習は、当分控えるように」と、お医者さんに言われてしまいました。"},{"id":{"d":7,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。日常生活にも支障が出そうですね。"},{"id":{"d":7,"n":5,"t":4},"speaker":"A","text":"はい。手伝ってもらうことが増えるかもしれませんが、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":8,"n":1,"t":4},"speaker":"A","text":"ごちそうさまでした。私は、おなかいっぱいになると眠くなってしまいます。"},{"id":{"d":8,"n":2,"t":4},"speaker":"B","text":"そのまま寝てしまうと、体重が増える原因になります。食事をするときは、腹八分目に抑えると良いですよ。"},{"id":{"d":8,"n":3,"t":4},"speaker":"A","text":"太りたくないので、寝てしまわないように少し運動をしてきます。"},{"id":{"d":8,"n":4,"t":4},"speaker":"B","text":"食べ終わった後は、激しい運動を控えた方がいいですよ。"},{"id":{"d":8,"n":5,"t":4},"speaker":"A","text":"では、少し休んでからウオーキングに行ってきます。"}];
INSERT INTO sentence [{"id":{"d":9,"n":1,"t":4},"speaker":"A","text":"この間、会社の同僚が人間ドックを受けてきたそうです。"},{"id":{"d":9,"n":2,"t":4},"speaker":"B","text":"結果については、どのように言っていましたか？"},{"id":{"d":9,"n":3,"t":4},"speaker":"A","text":"同僚は、「肝臓が少し悪い」と言っていました。お酒を飲む量を控えるそうです。"},{"id":{"d":9,"n":4,"t":4},"speaker":"B","text":"良い心掛けですね。健二さんも、人間ドックを受けたらどうですか？"},{"id":{"d":9,"n":5,"t":4},"speaker":"A","text":"はい。病気は早期発見が大切ですからね。"}];
INSERT INTO sentence [{"id":{"d":10,"n":1,"t":4},"speaker":"A","text":"先生、注射は痛いですか？"},{"id":{"d":10,"n":2,"t":4},"speaker":"B","text":"少し、チクッとしますがそれほど痛くありません。"},{"id":{"d":10,"n":3,"t":4},"speaker":"A","text":"患部が、赤く腫れたりしませんか？"},{"id":{"d":10,"n":4,"t":4},"speaker":"B","text":"赤くなる人もいますが、すぐに引くと思います。"}];
INSERT INTO sentence [{"id":{"d":11,"n":1,"t":4},"speaker":"A","text":"ごちそうさまでした。"},{"id":{"d":11,"n":2,"t":4},"speaker":"B","text":"食べ終わった後は、歯を磨かないと虫歯になりますよ。"},{"id":{"d":11,"n":3,"t":4},"speaker":"A","text":"虫歯になると、どうなりますか？"},{"id":{"d":11,"n":4,"t":4},"speaker":"B","text":"歯が痛くなって、ひどい時には抜けてしまうこともあるのですよ。"},{"id":{"d":11,"n":5,"t":4},"speaker":"A","text":"分かりました。きちんと歯磨きします。"}];
INSERT INTO sentence [{"id":{"d":12,"n":1,"t":4},"speaker":"A","text":"最近、睡眠時間がとれているせいか仕事にきちんと集中できているような気がします。上司に仕事の進展が速いと褒められました。"},{"id":{"d":12,"n":2,"t":4},"speaker":"B","text":"良かったですね。このままの調子で成果を上げられると、次の査定で評価が得られるかもしれません。"},{"id":{"d":12,"n":3,"t":4},"speaker":"A","text":"そうだと良いですね。もし、評価が上がったら給料も上がるのでしょうか？"},{"id":{"d":12,"n":4,"t":4},"speaker":"B","text":"恐らく上がると思います。その時は温泉旅行にでも行きましょうか？"},{"id":{"d":12,"n":5,"t":4},"speaker":"A","text":"良いですね。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":13,"n":1,"t":4},"speaker":"A","text":"失礼します。"},{"id":{"d":13,"n":2,"t":4},"speaker":"B","text":"鈴木君、どうしましたか？"},{"id":{"d":13,"n":3,"t":4},"speaker":"A","text":"友達と一緒に鬼ごっこをしていたら転んで、膝をけがしてしまいました。"},{"id":{"d":13,"n":4,"t":4},"speaker":"B","text":"けがしたところを洗って、消毒をしましょう。"},{"id":{"d":13,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":14,"n":1,"t":4},"speaker":"A","text":"ヨガが、健康に良いというのは本当ですか？"},{"id":{"d":14,"n":2,"t":4},"speaker":"B","text":"はい。ヨガは全身の凝りをほぐすことができるので、体が柔軟になり健康にもつながると思います。"},{"id":{"d":14,"n":3,"t":4},"speaker":"A","text":"確かに、体が柔らかくなれば、けがの予防にもつながりますよね。"},{"id":{"d":14,"n":4,"t":4},"speaker":"B","text":"そうですね。またヨガには呼吸法もあるので、リラックスできます。"},{"id":{"d":14,"n":5,"t":4},"speaker":"A","text":"近所で、ヨガ教室を開催しているところがあるので、今度行こうと思います。"}];
INSERT INTO sentence [{"id":{"d":15,"n":1,"t":4},"speaker":"A","text":"あなたは、歯磨きを何分くらいしますか？"},{"id":{"d":15,"n":2,"t":4},"speaker":"B","text":"私は3分程度、歯磨きをしています。佐藤さんはいかがですか？"},{"id":{"d":15,"n":3,"t":4},"speaker":"A","text":"私は、5分以上は時間をかけています。以前、虫歯になった経験があるので、それからは入念に歯を磨く事にしました。"},{"id":{"d":15,"n":4,"t":4},"speaker":"B","text":"それは、素晴らしい心掛けですね。"},{"id":{"d":15,"n":5,"t":4},"speaker":"A","text":"おかげで今は虫歯がありません。"}];
INSERT INTO sentence [{"id":{"d":16,"n":1,"t":4},"speaker":"A","text":"最近、家のお風呂の温度設定が高いのではありませんか？"},{"id":{"d":16,"n":2,"t":4},"speaker":"B","text":"お風呂に漬かって汗を流すのは、健康に良いらしいですよ。"},{"id":{"d":16,"n":3,"t":4},"speaker":"A","text":"それにしても、少し熱すぎる気がします。熱すぎても健康を害してしまいますよ。"},{"id":{"d":16,"n":4,"t":4},"speaker":"B","text":"そうですね。もう少し設定温度を下げましょう。"}];
INSERT INTO sentence [{"id":{"d":17,"n":1,"t":4},"speaker":"A","text":"もしもし、おはようございます。鈴木です。"},{"id":{"d":17,"n":2,"t":4},"speaker":"B","text":"おはようございます。どうしましたか？"},{"id":{"d":17,"n":3,"t":4},"speaker":"A","text":"今朝起きたら頭痛があり、体温を測ったら38度もありました。"},{"id":{"d":17,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。大丈夫ですか？"},{"id":{"d":17,"n":5,"t":4},"speaker":"A","text":"体温が高いので、今日は会社を休ませていただきたいのですが、よろしいでしょうか？"},{"id":{"d":17,"n":6,"t":4},"speaker":"B","text":"はい、分かりました。どうぞお大事にしてください。"},{"id":{"d":17,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":18,"n":1,"t":4},"speaker":"A","text":"佐藤さんは、あまりメークをしないのですか？"},{"id":{"d":18,"n":2,"t":4},"speaker":"B","text":"メークをしようとは思うのですが、上手にできないので困っています。良かったら教えてもらえませんか？"},{"id":{"d":18,"n":3,"t":4},"speaker":"A","text":"よいですよ。きっと始めればメークが楽しくなってくると思います。メークの方法は動画でもいろいろな人が紹介しているので、それを見るのも参考になりますよ。"},{"id":{"d":18,"n":4,"t":4},"speaker":"B","text":"そうなのですね。お薦めの動画があったら教えていただけますか？"},{"id":{"d":18,"n":5,"t":4},"speaker":"A","text":"はい、後でURLを送りますね。"}];
INSERT INTO sentence [{"id":{"d":19,"n":1,"t":4},"speaker":"A","text":"最近は、髪型が決まらないのです。"},{"id":{"d":19,"n":2,"t":4},"speaker":"B","text":"ヘアセットする際に、ワックスなどは使っていますか？"},{"id":{"d":19,"n":3,"t":4},"speaker":"A","text":"使っていません。水で整えているだけです。"},{"id":{"d":19,"n":4,"t":4},"speaker":"B","text":"ワックスを使った方がより、格好よく仕上がりますよ。"}];
INSERT INTO sentence [{"id":{"d":20,"n":1,"t":4},"speaker":"A","text":"私は、以前交通事故を起こして、首と手の骨を折るけがをしたことがあります。"},{"id":{"d":20,"n":2,"t":4},"speaker":"B","text":"それは大変でしたね。リハビリテーションは大変でしたか？"},{"id":{"d":20,"n":3,"t":4},"speaker":"A","text":"最初は自分で立つこともできなかったのですが、今は不自由なく日常生活を送ることができています。"},{"id":{"d":20,"n":4,"t":4},"speaker":"B","text":"今の状態になるまで、どのくらいリハビリテーションをしたのですか？"},{"id":{"d":20,"n":5,"t":4},"speaker":"A","text":"大体1年くらいです。ベッドの上での生活は大変でした。"}];
INSERT INTO sentence [{"id":{"d":21,"n":1,"t":4},"speaker":"A","text":"あなたは、最近よくマスクをしていますが、体調が優れないのですか？"},{"id":{"d":21,"n":2,"t":4},"speaker":"B","text":"いいえ、病気の予防のためにしているのです。"},{"id":{"d":21,"n":3,"t":4},"speaker":"A","text":"マスクをし続けていると、呼吸がしづらくはありませんか？"},{"id":{"d":21,"n":4,"t":4},"speaker":"B","text":"最初のうちは、そう感じましたが、慣れてしまえば平気です。病気になってしまうことに比べれば、苦ではありませんよ。"}];
INSERT INTO sentence [{"id":{"d":22,"n":1,"t":4},"speaker":"A","text":"また視力が弱くなったようで、眼鏡をしていても遠くのものが見えづらくなってきました。"},{"id":{"d":22,"n":2,"t":4},"speaker":"B","text":"視力検査をして、新しい眼鏡に変えたらどうですか？"},{"id":{"d":22,"n":3,"t":4},"speaker":"A","text":"はい。コンタクトレンズに変えようと思っています。眼鏡をしていると、かけている耳の部分が痛くなるのです。"},{"id":{"d":22,"n":4,"t":4},"speaker":"B","text":"コンタクトレンズは「目に入れる時が怖い」と、言っていましたよね？"},{"id":{"d":22,"n":5,"t":4},"speaker":"A","text":"最近になって、心変わりをしました。「コンタクトレンズは、目に入れることに慣れると楽だ」と聞きましたから。"}];
INSERT INTO sentence [{"id":{"d":23,"n":1,"t":4},"speaker":"A","text":"病院に行って検査をしたら、妊娠していることが分かりました。"},{"id":{"d":23,"n":2,"t":4},"speaker":"B","text":"良かったですね！とてもうれしいです。赤ちゃんの名前は何にしますか？"},{"id":{"d":23,"n":3,"t":4},"speaker":"A","text":"気が早いですね。まだ、男の子か女の子かも分かっていないのですよ。"},{"id":{"d":23,"n":4,"t":4},"speaker":"B","text":"出産予定日はいつですか？"},{"id":{"d":23,"n":5,"t":4},"speaker":"A","text":"8カ月後です。私の体調が優れないときは、食事作りや掃除をしていただけるとうれしいです。"},{"id":{"d":23,"n":6,"t":4},"speaker":"B","text":"もちろん食事作りや掃除をします。体を大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":24,"n":1,"t":4},"speaker":"A","text":"お客さんは、肩が凝っていますね。"},{"id":{"d":24,"n":2,"t":4},"speaker":"B","text":"仕事で、パソコンの画面ばかり見ているからだと思います。"},{"id":{"d":24,"n":3,"t":4},"speaker":"A","text":"仕事の合間に、簡単なマッサージでもした方がいいですよ。"},{"id":{"d":24,"n":4,"t":4},"speaker":"B","text":"仕事に集中していると時間を忘れて没頭してしまうのですよね。"},{"id":{"d":24,"n":5,"t":4},"speaker":"A","text":"肩凝りもひどくなると、頭痛につながったりするので、早めに対処した方がいいですよ。"},{"id":{"d":24,"n":6,"t":4},"speaker":"B","text":"そうなのですね。どのようなマッサージをしたら良いですか？"},{"id":{"d":24,"n":7,"t":4},"speaker":"A","text":"マッサージというよりは、肩を回すなどのストレッチ体操が効果的です。"},{"id":{"d":24,"n":8,"t":4},"speaker":"B","text":"分かりました。仕事の合間に試してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":25,"n":1,"t":4},"speaker":"A","text":"その傷は、どうしたのですか？"},{"id":{"d":25,"n":2,"t":4},"speaker":"B","text":"朝は、ひげそりをしていたら誤って、肌を切ってしまいました。"},{"id":{"d":25,"n":3,"t":4},"speaker":"A","text":"大丈夫ですか？ばんそうこうを貼った方がいいですよ。"},{"id":{"d":25,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":26,"n":1,"t":4},"speaker":"A","text":"バスケットボールで指をけがしました。コーチは「ただの突き指ではないか」とおっしゃるのですが、それにしては痛みが強いです。"},{"id":{"d":26,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？湿布を貼りましょうか？"},{"id":{"d":26,"n":3,"t":4},"speaker":"A","text":"骨折している可能性があるので、病院に連れて行ってもらえますか？"},{"id":{"d":26,"n":4,"t":4},"speaker":"B","text":"それは大変ですね。今からすぐ行くので準備してください。"},{"id":{"d":26,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":27,"n":1,"t":4},"speaker":"A","text":"来週の土曜日、海水浴に行きましょう。"},{"id":{"d":27,"n":2,"t":4},"speaker":"B","text":"急にどうしたのですか？"},{"id":{"d":27,"n":3,"t":4},"speaker":"A","text":"吉田さんは、「最近海に行ってない」と話していましたよね？土曜日はスケジュールが空いているので、お誘いしようと思ったのです。"},{"id":{"d":27,"n":4,"t":4},"speaker":"B","text":"もちろん、海に行くのはうれしいのですが、準備が必要です。"},{"id":{"d":27,"n":5,"t":4},"speaker":"A","text":"水着や着替えがあれば、十分ではありませんか？"},{"id":{"d":27,"n":6,"t":4},"speaker":"B","text":"海に行くのだったら、日焼け止めも必要ですね。"},{"id":{"d":27,"n":7,"t":4},"speaker":"A","text":"そうですね。水着と、着替えと、日焼け止めを持って行きましょう。"}];
INSERT INTO sentence [{"id":{"d":28,"n":1,"t":4},"speaker":"A","text":"最近になって、ジョギングを始めました。"},{"id":{"d":28,"n":2,"t":4},"speaker":"B","text":"それは、なぜですか？"},{"id":{"d":28,"n":3,"t":4},"speaker":"A","text":"10月に開催される、東京マラソンに出場したいと思ったからです。"},{"id":{"d":28,"n":4,"t":4},"speaker":"B","text":"それは、楽しみですね。毎日ジョギングをしているのですか？"},{"id":{"d":28,"n":5,"t":4},"speaker":"A","text":"はい。毎朝6時に起きて10キロメートル程度走っています。朝のジョギングは気持ちが良いですよ。"},{"id":{"d":28,"n":6,"t":4},"speaker":"B","text":"私も佐藤さんを見習って、ジョギングを始めてみようと思います。"},{"id":{"d":28,"n":7,"t":4},"speaker":"A","text":"短い距離から、始めると良いですよ。"}];
INSERT INTO sentence [{"id":{"d":29,"n":1,"t":4},"speaker":"A","text":"救急車をお願いします。"},{"id":{"d":29,"n":2,"t":4},"speaker":"B","text":"どうされましたか？"},{"id":{"d":29,"n":3,"t":4},"speaker":"A","text":"男性が倒れていて、意識がありません。"},{"id":{"d":29,"n":4,"t":4},"speaker":"B","text":"場所はどこですか？"},{"id":{"d":29,"n":5,"t":4},"speaker":"A","text":"羽田空港P1駐車場です。"},{"id":{"d":29,"n":6,"t":4},"speaker":"B","text":"他に何か外傷はありますか？"},{"id":{"d":29,"n":7,"t":4},"speaker":"A","text":"特に目立つ傷などはありません。"},{"id":{"d":29,"n":8,"t":4},"speaker":"B","text":"分かりました。すぐに、そちらへ向かいます。"}];
INSERT INTO sentence [{"id":{"d":30,"n":1,"t":4},"speaker":"A","text":"最近は、暑くてすぐ疲れてしまいます。"},{"id":{"d":30,"n":2,"t":4},"speaker":"B","text":"夏バテですか？"},{"id":{"d":30,"n":3,"t":4},"speaker":"A","text":"そうかもしれません。食欲もあまりありません。この暑さが、どうにかならないでしょうか？"},{"id":{"d":30,"n":4,"t":4},"speaker":"B","text":"夏だから、暑いのは仕方ありません。適度な運動もお勧めですよ。"},{"id":{"d":30,"n":5,"t":4},"speaker":"A","text":"そうなのですね。近所でヨガ教室を開催しているので、今度行ってみます。"}];
INSERT INTO sentence [{"id":{"d":31,"n":1,"t":4},"speaker":"A","text":"金田さんは、禁煙をしたのですか？"},{"id":{"d":31,"n":2,"t":4},"speaker":"B","text":"はい。今まで何度か禁煙しようと思っていてもできませんでしたが、今回こそ禁煙することに決めました。"},{"id":{"d":31,"n":3,"t":4},"speaker":"A","text":"何かきっかけが、あったのですか？"},{"id":{"d":31,"n":4,"t":4},"speaker":"B","text":"はい。喫煙を続けるとさまざまな病気のリスクがあるということを、テレビ番組で知ったからです。"},{"id":{"d":31,"n":5,"t":4},"speaker":"A","text":"それは良い心掛けですね。健康が第一ですからね。お互い健康に気を付けて長生きしたいですね。"},{"id":{"d":31,"n":6,"t":4},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":32,"n":1,"t":4},"speaker":"A","text":"昨日、山に行ったら虫に刺されました。"},{"id":{"d":32,"n":2,"t":4},"speaker":"B","text":"虫除けスプレーを準備していかなかったのですか？"},{"id":{"d":32,"n":3,"t":4},"speaker":"A","text":"虫除けスプレーも蚊取り線香も持って、準備は万端整えて行きましたが、それでも刺されてしまいました。"},{"id":{"d":32,"n":4,"t":4},"speaker":"B","text":"それは、お気の毒でしたね。"},{"id":{"d":32,"n":5,"t":4},"speaker":"A","text":"虫に刺された部分をかいてしまい、余計に悪化してしまいました。痛みも増してきたようです。"},{"id":{"d":32,"n":6,"t":4},"speaker":"B","text":"それは、本当にただの虫刺されでしょうか？痛みがひどいようであれば、早めに病院で診てもらった方がいいですよ。"},{"id":{"d":32,"n":7,"t":4},"speaker":"A","text":"そうですね。医者に診てもらおうと思います。"}];
INSERT INTO sentence [{"id":{"d":33,"n":1,"t":4},"speaker":"A","text":"最近、手が乾燥して、ひび割れした箇所から出血してしまいます。"},{"id":{"d":33,"n":2,"t":4},"speaker":"B","text":"それはつらいですね。ばんそうこうを貼ったらどうですか？"},{"id":{"d":33,"n":3,"t":4},"speaker":"A","text":"その都度、ばんそうこうを貼っていてもきりがないのです。乾燥から肌を守る必要があると思います。"},{"id":{"d":33,"n":4,"t":4},"speaker":"B","text":"どうすれば良いでしょう？"},{"id":{"d":33,"n":5,"t":4},"speaker":"A","text":"保湿クリームを、手に塗れば良いのですが、今使っているクリームは効果が薄いので、もっと効くものが欲しいのです。"},{"id":{"d":33,"n":6,"t":4},"speaker":"B","text":"肌に合ったクリームが見つかると良いですね。"}];
INSERT INTO sentence [{"id":{"d":34,"n":1,"t":4},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":34,"n":2,"t":4},"speaker":"B","text":"おかえりなさい。優一さん、少し飲み過ぎたようですね。"},{"id":{"d":34,"n":3,"t":4},"speaker":"A","text":"はい、今日は楽しい会だったので、つい飲み過ぎてしまいました。タバコの臭いもついてしまったようです。"},{"id":{"d":34,"n":4,"t":4},"speaker":"B","text":"受動喫煙のリスクもあるので気を付けてくださいね。体のために、アルコールも適量にしてください。"},{"id":{"d":34,"n":5,"t":4},"speaker":"A","text":"分かりました。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":35,"n":1,"t":4},"speaker":"A","text":"少し吐き気がします。"},{"id":{"d":35,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？"},{"id":{"d":35,"n":3,"t":4},"speaker":"A","text":"頭も痛いので、病院に連れて行ってもらえますか？"},{"id":{"d":35,"n":4,"t":4},"speaker":"B","text":"今日は暑かったので、熱中症かもしれません。すぐに病院に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":36,"n":1,"t":4},"speaker":"A","text":"父の介護で、腰を痛めてしまいました。"},{"id":{"d":36,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？仕事に影響が出てしまいますね。"},{"id":{"d":36,"n":3,"t":4},"speaker":"A","text":"はい。車に乗るのもままならない状態です。"},{"id":{"d":36,"n":4,"t":4},"speaker":"B","text":"明日の仕事は、休みますか？"},{"id":{"d":36,"n":5,"t":4},"speaker":"A","text":"いえ。恐らく明日になれば、少し回復すると思うので、湿布を貼って仕事に行くつもりです。"},{"id":{"d":36,"n":6,"t":4},"speaker":"B","text":"無理はしないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":37,"n":1,"t":4},"speaker":"A","text":"今日の春美さんは、花のような良い香りがしますね。"},{"id":{"d":37,"n":2,"t":4},"speaker":"B","text":"気が付いてくれたのですね。"},{"id":{"d":37,"n":3,"t":4},"speaker":"A","text":"良い香りがしたので、つい呼び止めてしまいました。"},{"id":{"d":37,"n":4,"t":4},"speaker":"B","text":"実は新しい香水をつけてみたのです。"},{"id":{"d":37,"n":5,"t":4},"speaker":"A","text":"どこの香水ですか？"},{"id":{"d":37,"n":6,"t":4},"speaker":"B","text":"海外の香水なのですが、名前は分からないのです。"}];
INSERT INTO sentence [{"id":{"d":38,"n":1,"t":4},"speaker":"A","text":"散歩は毎日しているのですか？"},{"id":{"d":38,"n":2,"t":4},"speaker":"B","text":"毎日ではないのですが、休日はできるだけ散歩するようにしています。"},{"id":{"d":38,"n":3,"t":4},"speaker":"A","text":"どこを散歩しているのですか？"},{"id":{"d":38,"n":4,"t":4},"speaker":"B","text":"家の周りや運動公園です。"},{"id":{"d":38,"n":5,"t":4},"speaker":"A","text":"そうなのですね。何時ごろ歩いているのですか？"},{"id":{"d":38,"n":6,"t":4},"speaker":"B","text":"家の周りを散歩する時は10時と午後4時の2回、運動公園を散歩する時は午後3時に散歩しています。"},{"id":{"d":38,"n":7,"t":4},"speaker":"A","text":"1日2回も、散歩をする時があるとはすごいですね。"}];
INSERT INTO sentence [{"id":{"d":39,"n":1,"t":4},"speaker":"A","text":"最近、私はジムに通い始めました。"},{"id":{"d":39,"n":2,"t":4},"speaker":"B","text":"どのようなトレーニングをしていますか？"},{"id":{"d":39,"n":3,"t":4},"speaker":"A","text":"チェストプレスで胸を鍛えたり、ショルダープレスで肩を鍛えたりしています。"},{"id":{"d":39,"n":4,"t":4},"speaker":"B","text":"主に上半身を鍛えているのですね。私もジムに興味があるのですが、どのようなコースがあるのですか？"},{"id":{"d":39,"n":5,"t":4},"speaker":"A","text":"入会したコースによっては、お風呂やサウナも利用できますし、マッサージチェアを使えるコースもあります。"},{"id":{"d":39,"n":6,"t":4},"speaker":"B","text":"それは魅力的ですね。私も通いたくなりました。"}];
INSERT INTO sentence [{"id":{"d":40,"n":1,"t":4},"speaker":"A","text":"最近、ひどい偏頭痛に悩まされています。"},{"id":{"d":40,"n":2,"t":4},"speaker":"B","text":"大丈夫ですか？つらそうですね。"},{"id":{"d":40,"n":3,"t":4},"speaker":"A","text":"気圧の変化が関係しているのかもしれません。"},{"id":{"d":40,"n":4,"t":4},"speaker":"B","text":"そうなのですね。他に症状はありますか？"},{"id":{"d":40,"n":5,"t":4},"speaker":"A","text":"吐き気がしたり、目の前がまぶしく感じることがあります。"},{"id":{"d":40,"n":6,"t":4},"speaker":"B","text":"あまりつらいようだったら、病院に行って診てもらってきた方がいいですよ。"},{"id":{"d":40,"n":7,"t":4},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":41,"n":1,"t":4},"speaker":"A","text":"今日は眼科に行って、目薬をもらってきました。"},{"id":{"d":41,"n":2,"t":4},"speaker":"B","text":"何か目のご病気でもあるのですか？"},{"id":{"d":41,"n":3,"t":4},"speaker":"A","text":"ドライアイだそうです。"},{"id":{"d":41,"n":4,"t":4},"speaker":"B","text":"そうなのですね。パソコンやスマートフォンを見る時間が増えたこともあって、ドライアイになる人が多いようです。"},{"id":{"d":41,"n":5,"t":4},"speaker":"A","text":"最近はパソコンを使った仕事を始めたし、家でもついスマートフォンばかり見てしまいます。"},{"id":{"d":41,"n":6,"t":4},"speaker":"B","text":"目が乾いた時や疲れた時は、遠くを見るといいですよ。"},{"id":{"d":41,"n":7,"t":4},"speaker":"A","text":"教えてくれてありがとうございます。試してみます。"}];
INSERT INTO sentence [{"id":{"d":42,"n":1,"t":4},"speaker":"A","text":"お母さん、私は手術をするのが怖いです。"},{"id":{"d":42,"n":2,"t":4},"speaker":"B","text":"怖がる気持ちは分かりますよ。でも、全身麻酔なので目が覚めた時には終わっていますよ。"},{"id":{"d":42,"n":3,"t":4},"speaker":"A","text":"お母さんも、手術をしたことがあるのですか？"},{"id":{"d":42,"n":4,"t":4},"speaker":"B","text":"全身麻酔ではなかったけれど、局所麻酔で手術をしたことがあります。"},{"id":{"d":42,"n":5,"t":4},"speaker":"A","text":"何の手術をしたのですか？"},{"id":{"d":42,"n":6,"t":4},"speaker":"B","text":"学生の頃に盲腸の手術をしました。手術中に執刀している先生の声や作業音が聞こえてきて不思議な感覚でしたが、痛みもなくあっという間に終わりましたよ。"},{"id":{"d":42,"n":7,"t":4},"speaker":"A","text":"そうなのですね。麻酔で痛みは感じないと分かっていても、やはり少し怖いです。"},{"id":{"d":42,"n":8,"t":4},"speaker":"B","text":"大丈夫ですよ。私がそばについていますからね。"}];
INSERT INTO sentence [{"id":{"d":43,"n":1,"t":4},"speaker":"A","text":"私は、風邪気味のようです。"},{"id":{"d":43,"n":2,"t":4},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":43,"n":3,"t":4},"speaker":"A","text":"喉が痛く、少し下痢もしています。"},{"id":{"d":43,"n":4,"t":4},"speaker":"B","text":"では、今日は消化の良い食事を作りますね。"},{"id":{"d":43,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":44,"n":1,"t":4},"speaker":"A","text":"鏡ばかり見て、どうしたのですか？"},{"id":{"d":44,"n":2,"t":4},"speaker":"B","text":"口内炎ができていて、気になるのです。"},{"id":{"d":44,"n":3,"t":4},"speaker":"A","text":"そうなのですね。私は口内炎になったことがないのですが、口内炎ができるとどうなるのですか？"},{"id":{"d":44,"n":4,"t":4},"speaker":"B","text":"口の中や周辺の粘膜が、炎症を起こすのです。私の場合は口の中をかんでしまったことが原因なのですが、ストレスや栄養不足も関係しているようです。"},{"id":{"d":44,"n":5,"t":4},"speaker":"A","text":"ストレスや栄養不足も関係していることを、初めて知りました。お大事にしてください。"},{"id":{"d":44,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":45,"n":1,"t":4},"speaker":"A","text":"高熱が出ました。"},{"id":{"d":45,"n":2,"t":4},"speaker":"B","text":"本当ですか？体温は何度ありますか？"},{"id":{"d":45,"n":3,"t":4},"speaker":"A","text":"38.5度です。"},{"id":{"d":45,"n":4,"t":4},"speaker":"B","text":"あなたの手元に、解熱剤はありますか？"},{"id":{"d":45,"n":5,"t":4},"speaker":"A","text":"先日ちょうど使い切ってしまいました。"},{"id":{"d":45,"n":6,"t":4},"speaker":"B","text":"では、急いで買ってきます。"},{"id":{"d":45,"n":7,"t":4},"speaker":"A","text":"ありがとうございます。気を付けて行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":46,"n":1,"t":4},"speaker":"A","text":"最近、私は低血圧で貧血気味なのです。"},{"id":{"d":46,"n":2,"t":4},"speaker":"B","text":"病院で、検査をしてもらった方がいいですよ。"},{"id":{"d":46,"n":3,"t":4},"speaker":"A","text":"明日、病院に行ってこようと思っています。"},{"id":{"d":46,"n":4,"t":4},"speaker":"B","text":"貧血がひどいと倒れてしまうこともあるので、起き上がる時などはゆっくり動いてください。"},{"id":{"d":46,"n":5,"t":4},"speaker":"A","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":47,"n":1,"t":4},"speaker":"A","text":"新しい靴を履いたら、靴擦れをしてしまいました。"},{"id":{"d":47,"n":2,"t":4},"speaker":"B","text":"痛そうですね。"},{"id":{"d":47,"n":3,"t":4},"speaker":"A","text":"このような事もあろうかと、ばんそうこうを持ってきて良かったです。"},{"id":{"d":47,"n":4,"t":4},"speaker":"B","text":"準備万全ですね。"},{"id":{"d":47,"n":5,"t":4},"speaker":"A","text":"新しい靴やハイヒールを履く時は、ばんそうこうが必需品です。"},{"id":{"d":47,"n":6,"t":4},"speaker":"B","text":"そうですね。私も新しい靴を履くときには、ばんそうこうを手放せません。"}];
INSERT INTO sentence [{"id":{"d":48,"n":1,"t":4},"speaker":"A","text":"もしもし、こんにちは。インフルエンザ予防接種の予約をお願いします。"},{"id":{"d":48,"n":2,"t":4},"speaker":"B","text":"こちらの病院には、かかったことはありますか？"},{"id":{"d":48,"n":3,"t":4},"speaker":"A","text":"いいえ。かかったことはありません。"},{"id":{"d":48,"n":4,"t":4},"speaker":"B","text":"そうでしたか。新規の方は、来週から予約開始となります。"},{"id":{"d":48,"n":5,"t":4},"speaker":"A","text":"そうですか。そうしましたら、その頃またお電話させていただこうと思います。"},{"id":{"d":48,"n":6,"t":4},"speaker":"B","text":"よろしくお願いします。では、失礼します。"}];
INSERT INTO sentence [{"id":{"d":49,"n":1,"t":4},"speaker":"A","text":"すみません、お尋ねしたいのですが、東京国際フォーラムへは、どの改札口から出れば良いでしょうか？"},{"id":{"d":49,"n":2,"t":4},"speaker":"B","text":"それなら、国際フォーラム口という改札口がありますので、そちらをご利用ください。"},{"id":{"d":49,"n":3,"t":4},"speaker":"A","text":"分かりました。"},{"id":{"d":49,"n":4,"t":4},"speaker":"B","text":"改札口は向かって右手になります。出ると、すぐに建物が見えると思います。"},{"id":{"d":49,"n":5,"t":4},"speaker":"A","text":"助かりました、ありがとうございました。"},{"id":{"d":49,"n":6,"t":4},"speaker":"B","text":"お気を付けて。"}];
INSERT INTO sentence [{"id":{"d":50,"n":1,"t":4},"speaker":"A","text":"この薬には、副作用はありますか？"},{"id":{"d":50,"n":2,"t":4},"speaker":"B","text":"まれに、副作用として吐き気などが出ることがあります。"},{"id":{"d":50,"n":3,"t":4},"speaker":"A","text":"そうなのですね。他に注意することはありますか？"},{"id":{"d":50,"n":4,"t":4},"speaker":"B","text":"グレープフルーツは、食べないようにしてください。"},{"id":{"d":50,"n":5,"t":4},"speaker":"A","text":"それはなぜですか？"},{"id":{"d":50,"n":6,"t":4},"speaker":"B","text":"薬の効果が薄れてしまうからです。"},{"id":{"d":50,"n":7,"t":4},"speaker":"A","text":"分かりました。気を付けます。"},{"id":{"d":50,"n":8,"t":4},"speaker":"B","text":"お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":51,"n":1,"t":4},"speaker":"A","text":"こんにちは、先生、失礼いたします。"},{"id":{"d":51,"n":2,"t":4},"speaker":"B","text":"こんにちは、今日はどうされましたか？"},{"id":{"d":51,"n":3,"t":4},"speaker":"A","text":"いつもの頭痛がひどいので、薬を頂けないでしょうか？"},{"id":{"d":51,"n":4,"t":4},"speaker":"B","text":"それは、おつらいですね、ひとまず、血圧と脈を測りましょう。腕を出していただけますか？"},{"id":{"d":51,"n":5,"t":4},"speaker":"A","text":"はい。"},{"id":{"d":51,"n":6,"t":4},"speaker":"B","text":"血圧、脈とも安定していますので、あまり心配なさらなくても大丈夫です。いつもの薬をお出ししますので、薬局で受け取ってください。"},{"id":{"d":51,"n":7,"t":4},"speaker":"A","text":"分かりました。"},{"id":{"d":51,"n":8,"t":4},"speaker":"B","text":"その他で、気になっている事はありませんか？"},{"id":{"d":51,"n":9,"t":4},"speaker":"A","text":"今は、特にありません。"},{"id":{"d":51,"n":10,"t":4},"speaker":"B","text":"それでは、お大事になさってください。"},{"id":{"d":51,"n":11,"t":4},"speaker":"A","text":"先生、ありがとうございました、失礼いたします。"}];
INSERT INTO sentence [{"id":{"d":52,"n":1,"t":4},"speaker":"A","text":"最近、ひげが気になります。そってもすぐに伸びてきます。"},{"id":{"d":52,"n":2,"t":4},"speaker":"B","text":"僕も同じです。"},{"id":{"d":52,"n":3,"t":4},"speaker":"A","text":"僕は毎日T字カミソリでそっていますが、君はどうしていますか？"},{"id":{"d":52,"n":4,"t":4},"speaker":"B","text":"僕は電気シェーバーを使っています。"},{"id":{"d":52,"n":5,"t":4},"speaker":"A","text":"電気シェーバーよりカミソリの方が、よくそれる気がするのですが、電気シェーバーの使い心地は良いですか？"},{"id":{"d":52,"n":6,"t":4},"speaker":"B","text":"僕は肌が弱いから、カミソリに負けてしまうのです。だから電気シェーバーを使っています。"},{"id":{"d":52,"n":7,"t":4},"speaker":"A","text":"そうなのですね。"}];
INSERT INTO sentence [{"id":{"d":53,"n":1,"t":4},"speaker":"A","text":"あなたは、健康に関する記事を読んでいるのですか？"},{"id":{"d":53,"n":2,"t":4},"speaker":"B","text":"はい、最近おなかが出てきたことを気にしているのです。"},{"id":{"d":53,"n":3,"t":4},"speaker":"A","text":"たくさんご飯を食べたり、お酒を飲んだりしているからですね。少し控えてみてはどうですか？"},{"id":{"d":53,"n":4,"t":4},"speaker":"B","text":"それができればよいのですが。あなたは何か運動でもしているのですか？"},{"id":{"d":53,"n":5,"t":4},"speaker":"A","text":"運動は嫌いなので特にしていません。"},{"id":{"d":53,"n":6,"t":4},"speaker":"B","text":"では、どのようにして痩せたのですか？"},{"id":{"d":53,"n":7,"t":4},"speaker":"A","text":"1日分の食べ物のカロリーを計算してみたら、食べ過ぎていたことが分かり、摂取するカロリーを決めてそれ以上は取らないようにしました。好きなものを控えたりもしました。"},{"id":{"d":53,"n":8,"t":4},"speaker":"B","text":"そうだったのですか。僕も少し食事の量を控えてみます。"},{"id":{"d":53,"n":9,"t":4},"speaker":"A","text":"お酒も控えると、早く効果が出るかもしれません。"},{"id":{"d":53,"n":10,"t":4},"speaker":"B","text":"それはなかなか難しいですが、なるべく努力します。"}];
INSERT INTO sentence [{"id":{"d":54,"n":1,"t":4},"speaker":"A","text":"松本部長、失礼いたします。"},{"id":{"d":54,"n":2,"t":4},"speaker":"B","text":"伊藤君、どうしましたか？"},{"id":{"d":54,"n":3,"t":4},"speaker":"A","text":"実は、今朝から体調が優れなかったのですが、担当案件の納期が気になり出社いたしました。休憩時間中に高熱が出てきたようなので、早退させていただきたく伺いました。"},{"id":{"d":54,"n":4,"t":4},"speaker":"B","text":"大丈夫ですか？仕事のことは気にせず、すぐに早退してください。"},{"id":{"d":54,"n":5,"t":4},"speaker":"A","text":"分かりました。そうさせていただきます。"},{"id":{"d":54,"n":6,"t":4},"speaker":"B","text":"その案件については、私から田中君に依頼しておきますので安心してください。"},{"id":{"d":54,"n":7,"t":4},"speaker":"A","text":"ありがとうございます、それではお先に失礼いたします。"},{"id":{"d":54,"n":8,"t":4},"speaker":"B","text":"はい、お疲れさまです。お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":55,"n":1,"t":4},"speaker":"A","text":"こんにちは。すみません、インフルエンザ予防接種を受けたいのですが。"},{"id":{"d":55,"n":2,"t":4},"speaker":"B","text":"こんにちは。インフルエンザ予防接種ですね。今の時期ですと予約をしていただいてから、接種をすることになります。それでよろしいですか？"},{"id":{"d":55,"n":3,"t":4},"speaker":"A","text":"分かりました。"},{"id":{"d":55,"n":4,"t":4},"speaker":"B","text":"それでは、こちらの問診票に必要事項をお書きください。"},{"id":{"d":55,"n":5,"t":4},"speaker":"A","text":"はい。書き終えました。"},{"id":{"d":55,"n":6,"t":4},"speaker":"B","text":"ご記入ありがとうございます。予防接種日は来週月曜日の午前中ですので、お昼前までに当院へいらしてください。"},{"id":{"d":55,"n":7,"t":4},"speaker":"A","text":"分かりました、よろしくお願いいたします。"},{"id":{"d":55,"n":8,"t":4},"speaker":"B","text":"はい、月曜日にお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":56,"n":1,"t":4},"speaker":"A","text":"あなたは、風邪をひきましたか？"},{"id":{"d":56,"n":2,"t":4},"speaker":"B","text":"風邪とは違うと思いますが、どうして風邪だと思ったのですか？"},{"id":{"d":56,"n":3,"t":4},"speaker":"A","text":"最近鼻声のようなので、風邪かなと思ったのです。"},{"id":{"d":56,"n":4,"t":4},"speaker":"B","text":"多分、花粉症のせいです。それで鼻詰まりをしているのです。"},{"id":{"d":56,"n":5,"t":4},"speaker":"A","text":"病院には行っていますか？"},{"id":{"d":56,"n":6,"t":4},"speaker":"B","text":"いえ、病院へは行かずに市販の薬を買って飲んでいます。"},{"id":{"d":56,"n":7,"t":4},"speaker":"A","text":"そうですか。良くならないようでしたら病院へ行って診てもらった方がいいですよ。"},{"id":{"d":56,"n":8,"t":4},"speaker":"B","text":"実は、病院が苦手なのです。"},{"id":{"d":56,"n":9,"t":4},"speaker":"A","text":"お気持ちは分かりますが、自分の体を第一に考えると、病院に行くことがベストだと思います。"},{"id":{"d":56,"n":10,"t":4},"speaker":"B","text":"確かにそうですね。アドバイスをありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":57,"n":1,"t":4},"speaker":"A","text":"今日は1日パソコン作業をしていたので、目が疲れました。"},{"id":{"d":57,"n":2,"t":4},"speaker":"B","text":"それは大変でしたね。"},{"id":{"d":57,"n":3,"t":4},"speaker":"A","text":"慣れないことを長時間すると疲れます。"},{"id":{"d":57,"n":4,"t":4},"speaker":"B","text":"休みを取りながら作業をしたり、目薬を差したりはしないのですか？"},{"id":{"d":57,"n":5,"t":4},"speaker":"A","text":"1時間に1回、少し手を離して一息ついたりはしていますが、目薬は使っていません。目薬は苦手なのです。"},{"id":{"d":57,"n":6,"t":4},"speaker":"B","text":"そうなのですね。どうして目薬が苦手なのですか？"},{"id":{"d":57,"n":7,"t":4},"speaker":"A","text":"目薬を差すことそのものは苦手ではないのですが、その後に目薬が鼻から喉に流れてくるあの味が嫌なので苦手なのです。"},{"id":{"d":57,"n":8,"t":4},"speaker":"B","text":"そうですか。言われてみれば薬の味はしますが、僕はあまり気になりません。"}];
INSERT INTO sentence [{"id":{"d":58,"n":1,"t":4},"speaker":"A","text":"もしもし、高橋です。鈴木さんですか？"},{"id":{"d":58,"n":2,"t":4},"speaker":"B","text":"はい、鈴木です。"},{"id":{"d":58,"n":3,"t":4},"speaker":"A","text":"申し訳ないのですが、今日、約束の時間に行けそうにありません。"},{"id":{"d":58,"n":4,"t":4},"speaker":"B","text":"どうかしましたか？"},{"id":{"d":58,"n":5,"t":4},"speaker":"A","text":"昨日、飲み過ぎてしまい頭が痛いのです。"},{"id":{"d":58,"n":6,"t":4},"speaker":"B","text":"そうですか。楽しみにしていたので残念です。"},{"id":{"d":58,"n":7,"t":4},"speaker":"A","text":"本当にすみません。"},{"id":{"d":58,"n":8,"t":4},"speaker":"B","text":"また、別の日に予定を変更しましょうか？"},{"id":{"d":58,"n":9,"t":4},"speaker":"A","text":"それは助かります。ではまた連絡します。"}];
INSERT INTO sentence [{"id":{"d":59,"n":1,"t":4},"speaker":"A","text":"石井君、おはようございます。"},{"id":{"d":59,"n":2,"t":4},"speaker":"B","text":"半田課長、おはようございます。もうお体は良いのですか？胃の不調でお休みをされていると伺っておりました。"},{"id":{"d":59,"n":3,"t":4},"speaker":"A","text":"心配をかけて申し訳ありません。もう、大丈夫です。"},{"id":{"d":59,"n":4,"t":4},"speaker":"B","text":"それは何よりです。課長はいつも夜遅くまで仕事されていましたので、お疲れではないかと気になっておりました。"},{"id":{"d":59,"n":5,"t":4},"speaker":"A","text":"医者からも、疲労が原因の胃痛ではないかと指摘されました。「今後は、体調に合わせて仕事を進めるにように」と言われました。"},{"id":{"d":59,"n":6,"t":4},"speaker":"B","text":"私ごときが差し出がましいのですが、半田課長のお体を考えると仕事の量を減らすのが順当と考えます。"},{"id":{"d":59,"n":7,"t":4},"speaker":"A","text":"心配してくれて、ありがとうございます。石井君にも少しずつ私の仕事を任せたいと考えています。"}];
INSERT INTO sentence [{"id":{"d":60,"n":1,"t":4},"speaker":"A","text":"随分と日焼けしていますね。どこかに行ったのですか？"},{"id":{"d":60,"n":2,"t":4},"speaker":"B","text":"日帰りですが、バイクでツーリングしてきました。"},{"id":{"d":60,"n":3,"t":4},"speaker":"A","text":"そうだったのですね。1日でだいぶ日焼けましたね。"},{"id":{"d":60,"n":4,"t":4},"speaker":"B","text":"バイクにまたがって、ハンドルを握っているので、腕や脚がずっと日に当たるからでしょう。"},{"id":{"d":60,"n":5,"t":4},"speaker":"A","text":"そうなのですね。"},{"id":{"d":60,"n":6,"t":4},"speaker":"B","text":"ヘルメットをかぶるから、顔だけは日焼けしないのです。"},{"id":{"d":60,"n":7,"t":4},"speaker":"A","text":"確かに、そうですね。"},{"id":{"d":60,"n":8,"t":4},"speaker":"B","text":"これは、バイク愛好者ならよくあることです。"}];
INSERT INTO sentence [{"id":{"d":61,"n":1,"t":4},"speaker":"A","text":"すみません、湿布を買いたいのですが売り場はどちらですか？"},{"id":{"d":61,"n":2,"t":4},"speaker":"B","text":"湿布はこちらにございます。"},{"id":{"d":61,"n":3,"t":4},"speaker":"A","text":"たくさん種類があるのですね。"},{"id":{"d":61,"n":4,"t":4},"speaker":"B","text":"そうですね、貼り付けるタイプの他、塗るタイプもあります。ご希望の物はございますか？"},{"id":{"d":61,"n":5,"t":4},"speaker":"A","text":"お薦めはありますか？"},{"id":{"d":61,"n":6,"t":4},"speaker":"B","text":"そうですね、当店ではこちらの貼り付けタイプが人気で、購入される方が多くいらっしゃいます。"},{"id":{"d":61,"n":7,"t":4},"speaker":"A","text":"ではこれを頂きます。"},{"id":{"d":61,"n":8,"t":4},"speaker":"B","text":"ありがとうございます。お会計レジはあちらにございます。"}];
INSERT INTO sentence [{"id":{"d":62,"n":1,"t":4},"speaker":"A","text":"けがをしたと聞きました。どうしたのですか？"},{"id":{"d":62,"n":2,"t":4},"speaker":"B","text":"先月、スキーに行った時に転んで足を骨折してしまいました。"},{"id":{"d":62,"n":3,"t":4},"speaker":"A","text":"それは大変ですね。どのくらいで治りそうですか？"},{"id":{"d":62,"n":4,"t":4},"speaker":"B","text":"「全治3カ月」と医者に言われました。"},{"id":{"d":62,"n":5,"t":4},"speaker":"A","text":"しばらくは不自由ですね。お大事になさってください。"},{"id":{"d":62,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。しばらくおとなしく過ごします。"}];
INSERT INTO sentence [{"id":{"d":63,"n":1,"t":4},"speaker":"A","text":"右目が痛いです。"},{"id":{"d":63,"n":2,"t":4},"speaker":"B","text":"どこかにぶつけたのですか？赤く腫れています。"},{"id":{"d":63,"n":3,"t":4},"speaker":"A","text":"いえ、ぶつけてはいません。"},{"id":{"d":63,"n":4,"t":4},"speaker":"B","text":"ではどうしたのでしょうね。眼科に行って診てもらいましょう。"}];
INSERT INTO sentence [{"id":{"d":64,"n":1,"t":4},"speaker":"A","text":"来週、インフルエンザの予防接種に行きましょう。"},{"id":{"d":64,"n":2,"t":4},"speaker":"B","text":"怖いから行きたくありません。"},{"id":{"d":64,"n":3,"t":4},"speaker":"A","text":"細い針だから、あまり痛みは感じないと思いますよ。"},{"id":{"d":64,"n":4,"t":4},"speaker":"B","text":"この前も予防接種をしましたが、今回で最後ですか？"},{"id":{"d":64,"n":5,"t":4},"speaker":"A","text":"はい、そうです。"}];
INSERT INTO sentence [{"id":{"d":65,"n":1,"t":4},"speaker":"A","text":"足つぼマッサージに行きませんか？"},{"id":{"d":65,"n":2,"t":4},"speaker":"B","text":"良いですね。私も行きたいと思っていました。最近立ち仕事が続いて足が疲れています。"},{"id":{"d":65,"n":3,"t":4},"speaker":"A","text":"今日の夜は、お時間はありますか？"},{"id":{"d":65,"n":4,"t":4},"speaker":"B","text":"今日はあいにく予定があります。明日の夜はいかがですか？"},{"id":{"d":65,"n":5,"t":4},"speaker":"A","text":"良いですよ。では、明日の夜に予約しておきますね。30分コースと1時間コースのどちらにしますか？"},{"id":{"d":65,"n":6,"t":4},"speaker":"B","text":"そうですね。では、1時間コースにします。予約をよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":66,"n":1,"t":4},"speaker":"A","text":"エステメニューで使える、クーポン券の使用期限は今月末です。ご予約されますか？"},{"id":{"d":66,"n":2,"t":4},"speaker":"B","text":"今月は日曜日しか予約を取ることができないのですが、まだすいていますか？"},{"id":{"d":66,"n":3,"t":4},"speaker":"A","text":"12月26日日曜日の15時からでしたら、予約が可能です。いかがでしょうか？"},{"id":{"d":66,"n":4,"t":4},"speaker":"B","text":"はい。そちらでお願いします。"},{"id":{"d":66,"n":5,"t":4},"speaker":"A","text":"かしこまりました。エステのメニューはどちらがよろしいでしょうか？"},{"id":{"d":66,"n":6,"t":4},"speaker":"B","text":"足のむくみを取るメニューでお願いします。"},{"id":{"d":66,"n":7,"t":4},"speaker":"A","text":"かしこまりました。当日は、お気を付けていらしてください。お待ちしております。"},{"id":{"d":66,"n":8,"t":4},"speaker":"B","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":67,"n":1,"t":4},"speaker":"A","text":"先日は、当サロンをご利用いただきありがとうございました。その後、気になる点はございませんか？"},{"id":{"d":67,"n":2,"t":4},"speaker":"B","text":"ご連絡いただきありがとうございます。カラーの色持ちもよく、とても満足しています。"},{"id":{"d":67,"n":3,"t":4},"speaker":"A","text":"それはありがとうございます。朝のセットなどで、困っていることはございませんか？"},{"id":{"d":67,"n":4,"t":4},"speaker":"B","text":"ありません。縮毛矯正をしていただいたおかげで、寝癖もつきにくく、朝のヘアセットがとても楽になりました。"},{"id":{"d":67,"n":5,"t":4},"speaker":"A","text":"そう言っていただけてうれしいです。もし気になる点がございましたら、いつでもご相談ください。すぐに手直しいたします。"},{"id":{"d":67,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。また、次もよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":68,"n":1,"t":4},"speaker":"A","text":"今回、当社ホームページに、お褒めのコメントをいただきありがとうございます。ささやかではございますが、次回来店時にヘッドマッサージをプレゼントいたします。"},{"id":{"d":68,"n":2,"t":4},"speaker":"B","text":"いつも丁寧な施術をしてくださるのでお礼の意味を込めて、投稿いたしました。"},{"id":{"d":68,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。小林さまのお話は話題も豊富で、我々もいつも楽しい時間をいただいております。"},{"id":{"d":68,"n":4,"t":4},"speaker":"B","text":"そう言っていただけて光栄です。私もスタッフの皆さまの対応の素晴らしさに、いつも癒やされています。"},{"id":{"d":68,"n":5,"t":4},"speaker":"A","text":"ありがとうございます。それでは、次回のお越しを心よりお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":69,"n":1,"t":4},"speaker":"A","text":"こんにちは。失礼します。"},{"id":{"d":69,"n":2,"t":4},"speaker":"B","text":"体の調子はいかがですか？"},{"id":{"d":69,"n":3,"t":4},"speaker":"A","text":"薬の効果も出ているようで、調子が良いです。"},{"id":{"d":69,"n":4,"t":4},"speaker":"B","text":"それは良かったです。それでは、このまま薬を続けてみましょう。"},{"id":{"d":69,"n":5,"t":4},"speaker":"A","text":"分かりました。少し風邪気味なので、風邪薬も一緒に処方していただけますか？"},{"id":{"d":69,"n":6,"t":4},"speaker":"B","text":"はい、いつもの薬と一緒に出しておきます。"}];
INSERT INTO sentence [{"id":{"d":70,"n":1,"t":4},"speaker":"A","text":"もしもし、夕日小学校の落合です。"},{"id":{"d":70,"n":2,"t":4},"speaker":"B","text":"落合先生、いつもお世話になっております。"},{"id":{"d":70,"n":3,"t":4},"speaker":"A","text":"実は、みかさんが休み時間に転んで足を擦りむいてしまいました。保健室で処置はしましたが、帰宅後も様子を見ていただけますか？"},{"id":{"d":70,"n":4,"t":4},"speaker":"B","text":"分かりました。わざわざご連絡いただきありがとうございます。失礼いたします。"}];
INSERT INTO sentence [{"id":{"d":71,"n":1,"t":4},"speaker":"A","text":"最近寒くなってきましたね。手が乾燥するので困ってしまいます。"},{"id":{"d":71,"n":2,"t":4},"speaker":"B","text":"私も同じです。洗濯物を干している時や、洗い物をする時など、手が痛くなるのを感じます。"},{"id":{"d":71,"n":3,"t":4},"speaker":"A","text":"大変ですよね。私は、そういう時にハンドクリームを塗るようにしています。"},{"id":{"d":71,"n":4,"t":4},"speaker":"B","text":"私も同じです。薬局で売っているハンドクリームには、良い香りのものが多く、選ぶのも楽しいです。"},{"id":{"d":71,"n":5,"t":4},"speaker":"A","text":"そうですね、私はバラの香りがするものが好きです。"},{"id":{"d":71,"n":6,"t":4},"speaker":"B","text":"私は、蜂蜜の香りのするクリームが好きです。"},{"id":{"d":71,"n":7,"t":4},"speaker":"A","text":"私も、蜂蜜の香りのものも好きですよ。今度ぜひ持ち寄りませんか？"},{"id":{"d":71,"n":8,"t":4},"speaker":"B","text":"良いですね。持ち寄ってより多くの香りを楽しみましょう。"}];
INSERT INTO sentence [{"id":{"d":72,"n":1,"t":4},"speaker":"A","text":"最近、鼻水が止まらなくて、困っています。"},{"id":{"d":72,"n":2,"t":4},"speaker":"B","text":"それは、大変ですね。大丈夫ですか？"},{"id":{"d":72,"n":3,"t":4},"speaker":"A","text":"はい、市販薬を飲んでどうにかして改善するように努力しています。"},{"id":{"d":72,"n":4,"t":4},"speaker":"B","text":"ぜひ、お大事になさってください。"}];
INSERT INTO sentence [{"id":{"d":73,"n":1,"t":4},"speaker":"A","text":"雰囲気が変わりましたが、髪型を変えましたか？"},{"id":{"d":73,"n":2,"t":4},"speaker":"B","text":"よく分かりましたね。パーマをかけました。"},{"id":{"d":73,"n":3,"t":4},"speaker":"A","text":"すてきですよ。とても似合っていらっしゃいます。"},{"id":{"d":73,"n":4,"t":4},"speaker":"B","text":"ありがとうございます。実はずっと前からパーマをかけてみたかったのです。"},{"id":{"d":73,"n":5,"t":4},"speaker":"A","text":"それでは、念願かなってという感じなのですね。おめでとうございます！"},{"id":{"d":73,"n":6,"t":4},"speaker":"B","text":"ありがとうございます。そのおかげで毎日がより楽しくなりました。"},{"id":{"d":73,"n":7,"t":4},"speaker":"A","text":"やはり髪型の威力は絶大ですね！私も、髪型を変えたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":74,"n":1,"t":4},"speaker":"A","text":"最近、何か運動をしていますか？"},{"id":{"d":74,"n":2,"t":4},"speaker":"B","text":"いいえ、何もしていません。実は最近3キログラム太ってしまいました。運動をした方がいいとは分かっているのですが、なかか始めることができません。"},{"id":{"d":74,"n":3,"t":4},"speaker":"A","text":"なるほど。私も体重が増えてしまったことがあったのですが、毎日往復1時間歩くように心掛けたら元に戻りました。"},{"id":{"d":74,"n":4,"t":4},"speaker":"B","text":"それはとても良いお話を聞きました。私も早速実行してみます。"},{"id":{"d":74,"n":5,"t":4},"speaker":"A","text":"はい、ぜひ試してみてください。"}];
INSERT INTO sentence [{"id":{"d":75,"n":1,"t":4},"speaker":"A","text":"最近、肩凝りと腰痛がひどくて、困っています。"},{"id":{"d":75,"n":2,"t":4},"speaker":"B","text":"それは、大変ですね。何か思い当たる原因はありますか？"},{"id":{"d":75,"n":3,"t":4},"speaker":"A","text":"仕事中にずっと座って、パソコンに向かっているのが原因かと思われます。"},{"id":{"d":75,"n":4,"t":4},"speaker":"B","text":"それは、ある種の職業病ですね。お大事になさってください。"},{"id":{"d":75,"n":5,"t":4},"speaker":"A","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":76,"n":1,"t":4},"speaker":"A","text":"石川さんのお肌は透明感がありますね。健康な肌を保つ秘訣が何かあるのでしょうか？"},{"id":{"d":76,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。特に何もしていませんが、強いて言えば、おいしいものを食べて、しっかりよく寝て、適宜運動をするなど健康的な生活を心掛けることです。"},{"id":{"d":76,"n":3,"t":4},"speaker":"A","text":"なるほど。私も心掛けているつもりでしたが、恐らく不十分なのだろうと思います。具体的には、何をされているのですか？"},{"id":{"d":76,"n":4,"t":4},"speaker":"B","text":"食べ物は、揚げ物やファーストフードを避けて、良質の睡眠をしっかり取るために日付が変わる前に寝床につくようにしています。また、運動を習慣づけるために、最寄りの駅の隣の駅で降りて歩くようにしています。"},{"id":{"d":76,"n":5,"t":4},"speaker":"A","text":"やはり努力されているのですね。私も1つでも取り入れて、頑張りたいと思います。"}];
INSERT INTO sentence [{"id":{"d":77,"n":1,"t":4},"speaker":"A","text":"こんにちは。ちかさんの風邪の具合はどうですか？"},{"id":{"d":77,"n":2,"t":4},"speaker":"B","text":"せきが止まって、だいぶ良くなりました。"},{"id":{"d":77,"n":3,"t":4},"speaker":"A","text":"皮膚の具合はどうですか？"},{"id":{"d":77,"n":4,"t":4},"speaker":"B","text":"最近は、肩や背中も痒そうにしています。"},{"id":{"d":77,"n":5,"t":4},"speaker":"A","text":"アレルギー体質なので、保湿クリームをたっぷり塗ってあげてください。"},{"id":{"d":77,"n":6,"t":4},"speaker":"B","text":"全身に塗っても構いませんか？"},{"id":{"d":77,"n":7,"t":4},"speaker":"A","text":"大丈夫です。足や、腕も痒くなりやすいので十分に塗ってください。"},{"id":{"d":77,"n":8,"t":4},"speaker":"B","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":78,"n":1,"t":4},"speaker":"A","text":"仕事で疲れた時、私はドリンク剤を飲むことがあります。"},{"id":{"d":78,"n":2,"t":4},"speaker":"B","text":"私も同じです。どのドリンク剤をよく飲みますか？"},{"id":{"d":78,"n":3,"t":4},"speaker":"A","text":"私は「リポビタンD」を飲むことが多いです。"},{"id":{"d":78,"n":4,"t":4},"speaker":"B","text":"私は「リポビタンD」も飲みますが、一番好きなのは「エスカップ」です。"},{"id":{"d":78,"n":5,"t":4},"speaker":"A","text":"「エスカップ」は初めて聞きました。今度、試してみます。"},{"id":{"d":78,"n":6,"t":4},"speaker":"B","text":"はい、お薦めなのでぜひ試してみてください。"}];
INSERT INTO sentence [{"id":{"d":79,"n":1,"t":4},"speaker":"A","text":"昔から低血圧気味なのですが、最近ますますひどくなりました。朝起きるのがつらくて仕方がありません。"},{"id":{"d":79,"n":2,"t":4},"speaker":"B","text":"それは、大変です。私も低血圧気味ではありますが、朝起きるのがつらいというほどではありません。ですので、どれほどつらいかは推して知るべし、という感じです。"},{"id":{"d":79,"n":3,"t":4},"speaker":"A","text":"ありがとうございます。低血圧を治すためにいろいろ試してみたのですが、なかなか効果があるものがなくて困ってしまいます。"},{"id":{"d":79,"n":4,"t":4},"speaker":"B","text":"なるほど。例えば、どのようなことを試されたのですか？"},{"id":{"d":79,"n":5,"t":4},"speaker":"A","text":"そうですね、食生活を改善したり、血液循環をよくする運動を行ったりしました。"},{"id":{"d":79,"n":6,"t":4},"speaker":"B","text":"確かに効果がありそうですが、あなたには効果がなかったということなのですか？"},{"id":{"d":79,"n":7,"t":4},"speaker":"A","text":"はい、そうなのです。人によるのかもしれません。"}];
INSERT INTO sentence [{"id":{"d":80,"n":1,"t":4},"speaker":"A","text":"インフルエンザの予防接種は受けましたか？"},{"id":{"d":80,"n":2,"t":4},"speaker":"B","text":"はい、受けました。今年は、例年以上にインフルエンザが流行しているようなので、早めに受けた方が良さそうです。"},{"id":{"d":80,"n":3,"t":4},"speaker":"A","text":"全くその通りです。インフルエンザにかかるといろいろな活動が滞ってしまうので、毎年受けるようにしています。"},{"id":{"d":80,"n":4,"t":4},"speaker":"B","text":"私も同じです。病気で活動が滞ってしまうのはもったいないですよね。"},{"id":{"d":80,"n":5,"t":4},"speaker":"A","text":"はい、そうですね。健康を保つための予防は、いくら行ってもしすぎることはないといえそうです。"}];
INSERT INTO sentence [{"id":{"d":81,"n":1,"t":4},"speaker":"A","text":"私は、健康のために毎日家でストレッチ体操をするようにしています。"},{"id":{"d":81,"n":2,"t":4},"speaker":"B","text":"それは、良い習慣ですね。見習いたいです。夜寝る前にされているのですか？"},{"id":{"d":81,"n":3,"t":4},"speaker":"A","text":"はい、そうです。夜寝る前に、健康増進とリラックスを兼ねて行うようにしています。"},{"id":{"d":81,"n":4,"t":4},"speaker":"B","text":"素晴らしいです！具体的にどのようなストレッチ体操を行うのでしょうか？"},{"id":{"d":81,"n":5,"t":4},"speaker":"A","text":"首と肩をほぐした後、腕と足を伸ばします。"},{"id":{"d":81,"n":6,"t":4},"speaker":"B","text":"意外と簡単にできそうですね。"},{"id":{"d":81,"n":7,"t":4},"speaker":"A","text":"はい、お勧めなので試してみてください。"},{"id":{"d":81,"n":8,"t":4},"speaker":"B","text":"そうですね、試してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":82,"n":1,"t":4},"speaker":"A","text":"小林さん、こんにちは。昨日のみかさんのけがの具合はどうですか？"},{"id":{"d":82,"n":2,"t":4},"speaker":"B","text":"ご丁寧にありがとうございます。少し擦りむいただけなので大丈夫です。"},{"id":{"d":82,"n":3,"t":4},"speaker":"A","text":"そうですか。悪化しなくて何よりでした。"},{"id":{"d":82,"n":4,"t":4},"speaker":"B","text":"足も全然痛くないようです。もう、いつも通り走り回っています。"},{"id":{"d":82,"n":5,"t":4},"speaker":"A","text":"そうですか。安心しました。"}];
INSERT INTO sentence [{"id":{"d":83,"n":1,"t":4},"speaker":"A","text":"子どものインフルエンザ予防接種の予約をしたいのですが、よろしいですか？"},{"id":{"d":83,"n":2,"t":4},"speaker":"B","text":"はい、こちらで受け付けています。お子さんの年齢と人数を教えてください。"},{"id":{"d":83,"n":3,"t":4},"speaker":"A","text":"8歳と5歳の2人です。"},{"id":{"d":83,"n":4,"t":4},"speaker":"B","text":"今年はワクチンが少ないので、病院で補充ができましたらご連絡いたします。"},{"id":{"d":83,"n":5,"t":4},"speaker":"A","text":"分かりました。いつ頃になるか分かりますか？"},{"id":{"d":83,"n":6,"t":4},"speaker":"B","text":"あいにく未定のためお伝えできません。"},{"id":{"d":83,"n":7,"t":4},"speaker":"A","text":"そうですか。では、ご連絡をお待ちしています。"}];
INSERT INTO sentence [{"id":{"d":84,"n":1,"t":4},"speaker":"A","text":"インフルエンザワクチンを入荷したので、ご連絡しました。"},{"id":{"d":84,"n":2,"t":4},"speaker":"B","text":"ありがとうございます。子ども2人分を予約します。"},{"id":{"d":84,"n":3,"t":4},"speaker":"A","text":"それでは、来週平日の午前と午後どちらがよろしいですか？"},{"id":{"d":84,"n":4,"t":4},"speaker":"B","text":"幼稚園と学校があるので、午後の予約をお願いします。"},{"id":{"d":84,"n":5,"t":4},"speaker":"A","text":"火曜日の午後4時がすいておりますが、ご都合はいかがでしょうか？"},{"id":{"d":84,"n":6,"t":4},"speaker":"B","text":"はい、その時間でしたらお伺いできます。"},{"id":{"d":84,"n":7,"t":4},"speaker":"A","text":"ご自宅を出る前に、体温を測ってきてください。母子健康手帳もご持参ください。"},{"id":{"d":84,"n":8,"t":4},"speaker":"B","text":"分かりました。よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":85,"n":1,"t":4},"speaker":"A","text":"このように暑い日が続くと、夏バテで物事がはかどりませんね。"},{"id":{"d":85,"n":2,"t":4},"speaker":"B","text":"私も同じです。やはり私は、あまりに暑いのは苦手です。"},{"id":{"d":85,"n":3,"t":4},"speaker":"A","text":"そうですよね。熱っぽくなって、意識も、もうろうとしてしまいますからね。"},{"id":{"d":85,"n":4,"t":4},"speaker":"B","text":"本当にそうです。暑すぎると何をするにも、いつもの倍くらい時間がかかってしまいます。"}];
INSERT INTO sentence [{"id":{"d":86,"n":1,"t":4},"speaker":"A","text":"冬には、うがいを習慣づけるようにしています。"},{"id":{"d":86,"n":2,"t":4},"speaker":"B","text":"私も同じです。風邪の予防に効果がありますよね。"},{"id":{"d":86,"n":3,"t":4},"speaker":"A","text":"はい、うがいを習慣にするようになってから、冬の間に風邪をひかなくなりました。"},{"id":{"d":86,"n":4,"t":4},"speaker":"B","text":"私もそうです。うがいを習慣にするまでは、よく喉が乾燥してつらかったのですが、最近はそういったこともなくなりました。"},{"id":{"d":86,"n":5,"t":4},"speaker":"A","text":"それは、良かったです。やはり、「継続は力なり」ですね。"}];
INSERT INTO sentence [{"id":{"d":87,"n":1,"t":4},"speaker":"A","text":"夏場には、日焼け止めが欠かせません。"},{"id":{"d":87,"n":2,"t":4},"speaker":"B","text":"私も同じです。夏場は外出時に、必ず日焼け止めを塗るようにしています。紫外線や強い日差しから肌を守ることができるので、重宝しています。"},{"id":{"d":87,"n":3,"t":4},"speaker":"A","text":"私も、紫外線が怖くて日焼け止めを塗っています。長期的に見て、紫外線を避けておくことは、皮膚がんの予防になると聞いたことがあります。"},{"id":{"d":87,"n":4,"t":4},"speaker":"B","text":"そうですね、私もそれは聞いたことがあります。"}];
INSERT INTO sentence [{"id":{"d":88,"n":1,"t":4},"speaker":"A","text":"最近、栄養療法を始めました。"},{"id":{"d":88,"n":2,"t":4},"speaker":"B","text":"ちょうど私も始めたいと考えていたのですが、いかがですか？"},{"id":{"d":88,"n":3,"t":4},"speaker":"A","text":"以前から悩んでいた、目まいや頭痛が改善されました。"},{"id":{"d":88,"n":4,"t":4},"speaker":"B","text":"それは、良かったです。目まいや頭痛は、主にどの栄養素で改善するのでしょうか？"},{"id":{"d":88,"n":5,"t":4},"speaker":"A","text":"私が飲んでいるのは、鉄分とビタミンBとCなのですが、恐らく一番目まいと頭痛に効果があったのは鉄分です。実は、お医者さんによると、私は鉄欠乏性貧血の気があるようです。"},{"id":{"d":88,"n":6,"t":4},"speaker":"B","text":"今度、その病院を紹介してもらえますか？"},{"id":{"d":88,"n":7,"t":4},"speaker":"A","text":"はい、もちろんです。"}];
INSERT INTO sentence [{"id":{"d":89,"n":1,"t":4},"speaker":"A","text":"先生、サッカーボールを狙ったところに蹴る方法を教えてください。"},{"id":{"d":89,"n":2,"t":4},"speaker":"B","text":"まず、サッカーボールを蹴りたい方向に、体を向けることが大切です。"},{"id":{"d":89,"n":3,"t":4},"speaker":"A","text":"そうなのですか。サッカーボールを蹴るときに違う方向を見て蹴っていました。"},{"id":{"d":89,"n":4,"t":4},"speaker":"B","text":"サッカーボールを蹴る際は、蹴りたい方向に体と目線を向けることが重要です。そのことを意識して練習に取り組んでください。"},{"id":{"d":89,"n":5,"t":4},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":90,"n":1,"t":4},"speaker":"A","text":"先生、湿布を貼ってから余計に腰が痛むのですが、湿布を貼らなくてもよいですか？"},{"id":{"d":90,"n":2,"t":4},"speaker":"B","text":"つらい体勢を長時間維持し続けたりしましたか？何か心当たりはありませんか？"},{"id":{"d":90,"n":3,"t":4},"speaker":"A","text":"いいえ。自宅で安静にしているので、特に心当たりはありません。"},{"id":{"d":90,"n":4,"t":4},"speaker":"B","text":"そうですか。湿布に含まれる成分などが関係している可能性もあるので、湿布は貼らなくて結構です。"},{"id":{"d":90,"n":5,"t":4},"speaker":"A","text":"分かりました。"}];
