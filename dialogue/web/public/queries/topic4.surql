INSERT INTO sentence [{"id":{"d":524,"n":1,"t":4},"speaker":"A","text":"熱があって、食欲がありません。"},{"id":{"d":524,"n":2,"t":4},"speaker":"B","text":"栄養を取った方が、回復が早いと思いますよ。"},{"id":{"d":524,"n":3,"t":4},"speaker":"A","text":"果物なら食べられそうです。"},{"id":{"d":524,"n":4,"t":4},"speaker":"B","text":"果物なら、バナナとリンゴがあります。"},{"id":{"d":524,"n":5,"t":4},"speaker":"A","text":"リンゴが食べたいです。"},{"id":{"d":524,"n":6,"t":4},"speaker":"B","text":"皮をむいて、食べやすい大きさに切りますね。"},{"id":{"d":524,"n":7,"t":4},"speaker":"A","text":"面倒をかけてすみません。"},{"id":{"d":524,"n":8,"t":4},"speaker":"B","text":"お互い様なので、気にしないでください。"}];
