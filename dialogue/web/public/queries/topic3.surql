INSERT INTO sentence [{"id":{"d":577,"n":1,"t":3},"speaker":"A","text":"すみません。ここは空港の第2ターミナルですか？"},{"id":{"d":577,"n":2,"t":3},"speaker":"B","text":"はい、そうです。どうかされましたか？"},{"id":{"d":577,"n":3,"t":3},"speaker":"A","text":"第1ターミナルに行くはずが、間違えてこちらに来てしまいました。"},{"id":{"d":577,"n":4,"t":3},"speaker":"B","text":"それでは、2つのターミナルを循環しているバスがありますので、ご利用ください。"},{"id":{"d":577,"n":5,"t":3},"speaker":"A","text":"集合時間が迫っているのですが、第1ターミナルまで移動するには、バスでどのくらいかかりますか？"},{"id":{"d":577,"n":6,"t":3},"speaker":"B","text":"5分くらいで到着します。"},{"id":{"d":577,"n":7,"t":3},"speaker":"A","text":"分かりました。バスはどこから出ていますか？"},{"id":{"d":577,"n":8,"t":3},"speaker":"B","text":"ターミナルの正面出入り口を出ていただくと、左側にバス乗り場があります。"},{"id":{"d":577,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":578,"n":1,"t":3},"speaker":"A","text":"申し訳ございませんが、ご予約をいただいたシングルのお部屋が本日満室となっております。"},{"id":{"d":578,"n":2,"t":3},"speaker":"B","text":"予約はできているのに、満室なのですか？"},{"id":{"d":578,"n":3,"t":3},"speaker":"A","text":"はい。こちらのミスですから、お部屋のアップグレードで対応をさせていただいてよろしいでしょうか？"},{"id":{"d":578,"n":4,"t":3},"speaker":"B","text":"はい。具体的には、どのような部屋になるのですか？"},{"id":{"d":578,"n":5,"t":3},"speaker":"A","text":"ツインのお部屋が空いておりますので、そちらをご利用いただけます。"},{"id":{"d":578,"n":6,"t":3},"speaker":"B","text":"料金はそのままということでしょうか？"},{"id":{"d":578,"n":7,"t":3},"speaker":"A","text":"はい。あくまでもこちらの手違いですので、ご予約をいただいたシングルの料金で結構です。"},{"id":{"d":578,"n":8,"t":3},"speaker":"B","text":"分かりました。それでお願いします。"}];
INSERT INTO sentence [{"id":{"d":579,"n":1,"t":3},"speaker":"A","text":"添乗員さん、すみません。"},{"id":{"d":579,"n":2,"t":3},"speaker":"B","text":"どうしましたか？"},{"id":{"d":579,"n":3,"t":3},"speaker":"A","text":"少し気分が悪くて、寒気がします。"},{"id":{"d":579,"n":4,"t":3},"speaker":"B","text":"まだツアー日程の途中なので、バスの中で横になりますか？"},{"id":{"d":579,"n":5,"t":3},"speaker":"A","text":"バスだと余計に具合が悪くなりそうなので、もし、可能ならホテルの部屋で休みたいです。"},{"id":{"d":579,"n":6,"t":3},"speaker":"B","text":"ホテルはどちらですか？"},{"id":{"d":579,"n":7,"t":3},"speaker":"A","text":"リバーサイドホテルです。"},{"id":{"d":579,"n":8,"t":3},"speaker":"B","text":"それでは、ホテルまでお送りしますのでゆっくりお休みください。"}];
INSERT INTO sentence [{"id":{"d":580,"n":1,"t":3},"speaker":"A","text":"ケビン、今日はどこへ行きたいですか？"},{"id":{"d":580,"n":2,"t":3},"speaker":"B","text":"日本に来たら絶対に訪問しようと思っていた所があるので、そこへ行きたいです。"},{"id":{"d":580,"n":3,"t":3},"speaker":"A","text":"どこでしょうか？"},{"id":{"d":580,"n":4,"t":3},"speaker":"B","text":"メイドカフェです。"},{"id":{"d":580,"n":5,"t":3},"speaker":"A","text":"実は私はメイドカフェに行ったことがありません。"},{"id":{"d":580,"n":6,"t":3},"speaker":"B","text":"本当ですか？それでは、案内していただくのは難しいでしょうか？"},{"id":{"d":580,"n":7,"t":3},"speaker":"A","text":"いえ、調べれば場所は分かりますし、案内することはできるので、今日はメイドカフェに行きましょう。"},{"id":{"d":580,"n":8,"t":3},"speaker":"B","text":"ずっと行きたかったので、とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":581,"n":1,"t":3},"speaker":"A","text":"河西さまですか？"},{"id":{"d":581,"n":2,"t":3},"speaker":"B","text":"はい。そうです。"},{"id":{"d":581,"n":3,"t":3},"speaker":"A","text":"初めまして。私はローマ滞在中にお世話をさせていただく、大越と申します。"},{"id":{"d":581,"n":4,"t":3},"speaker":"B","text":"初めまして。よろしくお願いします。取りあえずホテルへの案内をお願いします。"},{"id":{"d":581,"n":5,"t":3},"speaker":"A","text":"かしこまりました。マイクロバスでご案内いたします。"},{"id":{"d":581,"n":6,"t":3},"speaker":"B","text":"マイクロバスはどこに止まっているのでしょうか？"},{"id":{"d":581,"n":7,"t":3},"speaker":"A","text":"空港の駐車場です。運転手に出入り口まで来るように連絡しますので、少々お待ちください。"},{"id":{"d":581,"n":8,"t":3},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":582,"n":1,"t":3},"speaker":"A","text":"お待たせしました。こちらが、親子丼です。"},{"id":{"d":582,"n":2,"t":3},"speaker":"B","text":"私が注文したのは、カツ丼です。"},{"id":{"d":582,"n":3,"t":3},"speaker":"A","text":"失礼いたしました。注文に誤りが起きたようです。作り直します。"},{"id":{"d":582,"n":4,"t":3},"speaker":"B","text":"これから作ると時間がかかりますよね？"},{"id":{"d":582,"n":5,"t":3},"speaker":"A","text":"そうですね。急ぎますが、10分ほど調理に時間がかかってしまいます。"},{"id":{"d":582,"n":6,"t":3},"speaker":"B","text":"急いでいるので、作り直さなくてよいです。"},{"id":{"d":582,"n":7,"t":3},"speaker":"A","text":"それでは親子丼をお召し上がりいただいて、料金の方は割引させていただきます。"},{"id":{"d":582,"n":8,"t":3},"speaker":"B","text":"かえってすみません。それではいただきます。"}];
INSERT INTO sentence [{"id":{"d":583,"n":1,"t":3},"speaker":"A","text":"お父さん、カナダでお世話になったホストマザーが日本に遊びに来るそうです。"},{"id":{"d":583,"n":2,"t":3},"speaker":"B","text":"それは楽しみですね。"},{"id":{"d":583,"n":3,"t":3},"speaker":"A","text":"1週間くらい日本で過ごすらしいのですが、うちに泊まってもらってよいですか？"},{"id":{"d":583,"n":4,"t":3},"speaker":"B","text":"あなたがカナダでとてもお世話になった方ですし、構いませんよ。"},{"id":{"d":583,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。ホストマザーに伝えます。"},{"id":{"d":583,"n":6,"t":3},"speaker":"B","text":"空港まで車で迎えに行くので、そのことも伝えてください。"},{"id":{"d":583,"n":7,"t":3},"speaker":"A","text":"分かりました。"},{"id":{"d":583,"n":8,"t":3},"speaker":"B","text":"それから飛行機の到着時間を確認しておいてくださいね。"}];
INSERT INTO sentence [{"id":{"d":584,"n":1,"t":3},"speaker":"A","text":"この展望台は眺めが良いですね。"},{"id":{"d":584,"n":2,"t":3},"speaker":"B","text":"私は高い所が怖いので、あまり楽しめません。"},{"id":{"d":584,"n":3,"t":3},"speaker":"A","text":"高所恐怖症ですか？"},{"id":{"d":584,"n":4,"t":3},"speaker":"B","text":"そうですね。高いところが苦手です。"},{"id":{"d":584,"n":5,"t":3},"speaker":"A","text":"この展望台のさらに上にもう一つ展望台があるのですが、上るのは、やめておいた方がいいですね。"},{"id":{"d":584,"n":6,"t":3},"speaker":"B","text":"私はさらに高い展望台に上がることは無理ですが、もし行きたいようでしたら下で待っていますので、遠慮なく行って来てください。"},{"id":{"d":584,"n":7,"t":3},"speaker":"A","text":"私も高い所が好きなわけではないのでやめておきます。"},{"id":{"d":584,"n":8,"t":3},"speaker":"B","text":"せっかくタワーに来たのに、どうしたものか申し訳ありません。"}];
INSERT INTO sentence [{"id":{"d":585,"n":1,"t":3},"speaker":"A","text":"パンダの親子が見えますね。"},{"id":{"d":585,"n":2,"t":3},"speaker":"B","text":"あれはお父さんパンダでしょうか、それともお母さんパンダでしょうか？"},{"id":{"d":585,"n":3,"t":3},"speaker":"A","text":"看板にお父さんパンダだと書いてあります。"},{"id":{"d":585,"n":4,"t":3},"speaker":"B","text":"お父さんパンダは、上手にタケを持って食べていますね。"},{"id":{"d":585,"n":5,"t":3},"speaker":"A","text":"お母さんパンダと子どもパンダはどこでしょうか？"},{"id":{"d":585,"n":6,"t":3},"speaker":"B","text":"隣の柵の中にいます。"},{"id":{"d":585,"n":7,"t":3},"speaker":"A","text":"本当ですね。お昼寝をしているようです。"},{"id":{"d":585,"n":8,"t":3},"speaker":"B","text":"パンダは何をしていてもかわいいですね。"}];
INSERT INTO sentence [{"id":{"d":586,"n":1,"t":3},"speaker":"A","text":"乗る予定だったバスが、行ってしまいました。"},{"id":{"d":586,"n":2,"t":3},"speaker":"B","text":"私がトイレに行っていたせいです。すみません。"},{"id":{"d":586,"n":3,"t":3},"speaker":"A","text":"仕方ありませんよ。次のバスに乗りましょう。"},{"id":{"d":586,"n":4,"t":3},"speaker":"B","text":"時刻表で次のバスの時間を確認しますね。"},{"id":{"d":586,"n":5,"t":3},"speaker":"A","text":"どうでしたか？"},{"id":{"d":586,"n":6,"t":3},"speaker":"B","text":"次のバスは15分後に到着します。"},{"id":{"d":586,"n":7,"t":3},"speaker":"A","text":"あまり待たずに次のバスが来るようで、良かったです。"},{"id":{"d":586,"n":8,"t":3},"speaker":"B","text":"そうですね。バス停に移動しておきましょう。"}];
INSERT INTO sentence [{"id":{"d":587,"n":1,"t":3},"speaker":"A","text":"夏休みにフィリピンに行きます。"},{"id":{"d":587,"n":2,"t":3},"speaker":"B","text":"旅行ですか？"},{"id":{"d":587,"n":3,"t":3},"speaker":"A","text":"私の母がフィリピン人なので、母の実家に行くのです。"},{"id":{"d":587,"n":4,"t":3},"speaker":"B","text":"莉子ちゃんのお母さまがフィリピンの方だったとは、知りませんでした。"},{"id":{"d":587,"n":5,"t":3},"speaker":"A","text":"これまで話す機会がありませんでしたね。"},{"id":{"d":587,"n":6,"t":3},"speaker":"B","text":"それでは、英語も話せるのですか？"},{"id":{"d":587,"n":7,"t":3},"speaker":"A","text":"はい。日本語と同じくらい話せます。"},{"id":{"d":587,"n":8,"t":3},"speaker":"B","text":"うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":588,"n":1,"t":3},"speaker":"A","text":"こちらお済みでしたら、お下げしてよろしいですか？"},{"id":{"d":588,"n":2,"t":3},"speaker":"B","text":"はい。お願いします。"},{"id":{"d":588,"n":3,"t":3},"speaker":"A","text":"食後のデザートをお持ちしてよろしいでしょうか？"},{"id":{"d":588,"n":4,"t":3},"speaker":"B","text":"はい。それと、追加でコーヒーをお願いします。"},{"id":{"d":588,"n":5,"t":3},"speaker":"A","text":"それでしたら、デザートセットにした方がお得ですよ。"},{"id":{"d":588,"n":6,"t":3},"speaker":"B","text":"そうなのですか？"},{"id":{"d":588,"n":7,"t":3},"speaker":"A","text":"はい。デザートの料金に150円の追加で、お好きなお飲み物を付けることができます。"},{"id":{"d":588,"n":8,"t":3},"speaker":"B","text":"それでは、デザートセットにします。"},{"id":{"d":588,"n":9,"t":3},"speaker":"A","text":"かしこまりました。少々お待ちくださいませ。"}];
INSERT INTO sentence [{"id":{"d":589,"n":1,"t":3},"speaker":"A","text":"おしゃれなカフェですね。"},{"id":{"d":589,"n":2,"t":3},"speaker":"B","text":"雰囲気が好きで、私のお気に入りのお店です。"},{"id":{"d":589,"n":3,"t":3},"speaker":"A","text":"テラス席もあるのですね。"},{"id":{"d":589,"n":4,"t":3},"speaker":"B","text":"昼間は、テラス席を利用するととても気持ちが良いですよ。"},{"id":{"d":589,"n":5,"t":3},"speaker":"A","text":"次は昼間に来てみたいです。"},{"id":{"d":589,"n":6,"t":3},"speaker":"B","text":"何を頼みますか？"},{"id":{"d":589,"n":7,"t":3},"speaker":"A","text":"ケーキの中で何かお薦めはありますか？"},{"id":{"d":589,"n":8,"t":3},"speaker":"B","text":"チーズケーキがお薦めです。"}];
INSERT INTO sentence [{"id":{"d":590,"n":1,"t":3},"speaker":"A","text":"搭乗手続きをお願いします。"},{"id":{"d":590,"n":2,"t":3},"speaker":"B","text":"航空券を確認いたします。"},{"id":{"d":590,"n":3,"t":3},"speaker":"A","text":"これです。"},{"id":{"d":590,"n":4,"t":3},"speaker":"B","text":"座席のご希望はありますか？"},{"id":{"d":590,"n":5,"t":3},"speaker":"A","text":"窓側でお願いします。"},{"id":{"d":590,"n":6,"t":3},"speaker":"B","text":"預ける荷物がありましたら、こちらへ乗せてください。"},{"id":{"d":590,"n":7,"t":3},"speaker":"A","text":"2つあるのですが、大丈夫ですか？"},{"id":{"d":590,"n":8,"t":3},"speaker":"B","text":"合計で重さが20キログラム以内なら大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":591,"n":1,"t":3},"speaker":"A","text":"保険商品をお探しですか？"},{"id":{"d":591,"n":2,"t":3},"speaker":"B","text":"はい。旅行保険に入ろうと思っています。"},{"id":{"d":591,"n":3,"t":3},"speaker":"A","text":"旅行先はどちらですか？"},{"id":{"d":591,"n":4,"t":3},"speaker":"B","text":"オランダ王国です。"},{"id":{"d":591,"n":5,"t":3},"speaker":"A","text":"出国日と帰国日を教えてください。"},{"id":{"d":591,"n":6,"t":3},"speaker":"B","text":"7月21日に出国して、27日に帰国します。"},{"id":{"d":591,"n":7,"t":3},"speaker":"A","text":"携帯品に対する補償もお付けしますか？"},{"id":{"d":591,"n":8,"t":3},"speaker":"B","text":"はい。"},{"id":{"d":591,"n":9,"t":3},"speaker":"A","text":"そうしますと、こちらのプランがよろしいかと存じます。"}];
INSERT INTO sentence [{"id":{"d":592,"n":1,"t":3},"speaker":"A","text":"何か食べたいですが、出掛けるのはおっくうですね。"},{"id":{"d":592,"n":2,"t":3},"speaker":"B","text":"ルームサービスを頼みましょうか？"},{"id":{"d":592,"n":3,"t":3},"speaker":"A","text":"そうしましょう。ここにメニューがあります。"},{"id":{"d":592,"n":4,"t":3},"speaker":"B","text":"メインはそれぞれ1品注文して、サラダは1品を一緒に食べましょうか？"},{"id":{"d":592,"n":5,"t":3},"speaker":"A","text":"良いですね。私はメインをハヤシライスにします。"},{"id":{"d":592,"n":6,"t":3},"speaker":"B","text":"私は、ミックスサンドイッチがいいです。"},{"id":{"d":592,"n":7,"t":3},"speaker":"A","text":"サラダはシーザーサラダがおいしそうです。"},{"id":{"d":592,"n":8,"t":3},"speaker":"B","text":"では、それらを注文しましょう。"}];
INSERT INTO sentence [{"id":{"d":593,"n":1,"t":3},"speaker":"A","text":"バイクの免許を取りました。"},{"id":{"d":593,"n":2,"t":3},"speaker":"B","text":"バイクに乗って行ってみたいところはありますか？"},{"id":{"d":593,"n":3,"t":3},"speaker":"A","text":"北海道です。北海道を一周したいです。"},{"id":{"d":593,"n":4,"t":3},"speaker":"B","text":"それは夢がありますね。"},{"id":{"d":593,"n":5,"t":3},"speaker":"A","text":"アルバイトをして旅行費用を貯める予定です。"},{"id":{"d":593,"n":6,"t":3},"speaker":"B","text":"北海道一周ですと、旅行費用は相当かかるでしょうね。"},{"id":{"d":593,"n":7,"t":3},"speaker":"A","text":"節約して、費用は最低限に抑えるつもりです。"},{"id":{"d":593,"n":8,"t":3},"speaker":"B","text":"北海道を一周するのに何日くらいかかるのですか？"},{"id":{"d":593,"n":9,"t":3},"speaker":"A","text":"私は2週間で一周するつもりです。"}];
INSERT INTO sentence [{"id":{"d":594,"n":1,"t":3},"speaker":"A","text":"誠君は犬を飼っていますよね？"},{"id":{"d":594,"n":2,"t":3},"speaker":"B","text":"はい。チワワを飼っています。"},{"id":{"d":594,"n":3,"t":3},"speaker":"A","text":"うちはトイ・プードルを飼っているのですが、今度一緒にドッグランに行きませんか？"},{"id":{"d":594,"n":4,"t":3},"speaker":"B","text":"ドッグランには行ったことがありませんが、どのような所ですか？"},{"id":{"d":594,"n":5,"t":3},"speaker":"A","text":"僕がよく行くドッグランは、広い芝生広場があって犬がリードなしで遊べます。"},{"id":{"d":594,"n":6,"t":3},"speaker":"B","text":"リードなしで遊べるというのはいいですね。"},{"id":{"d":594,"n":7,"t":3},"speaker":"A","text":"犬が自由に走り回れるので、良い運動になりますよ。"},{"id":{"d":594,"n":8,"t":3},"speaker":"B","text":"ぜひ行ってみたいです。次の週末はどうでしょうか？"}];
INSERT INTO sentence [{"id":{"d":595,"n":1,"t":3},"speaker":"A","text":"はい、ペットホテル「スマイル」です。"},{"id":{"d":595,"n":2,"t":3},"speaker":"B","text":"もしもし、旅行に行く間、犬を預けたいので予約をお願いします。"},{"id":{"d":595,"n":3,"t":3},"speaker":"A","text":"はい。ご予約いただく日にちはいつでしょうか？"},{"id":{"d":595,"n":4,"t":3},"speaker":"B","text":"11月3日から6日までです。"},{"id":{"d":595,"n":5,"t":3},"speaker":"A","text":"犬種を教えてください。"},{"id":{"d":595,"n":6,"t":3},"speaker":"B","text":"ポメラニアンです。"},{"id":{"d":595,"n":7,"t":3},"speaker":"A","text":"餌はいつもあげているものをお預かりしたいので、当日は多めにお持ちください。"},{"id":{"d":595,"n":8,"t":3},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":596,"n":1,"t":3},"speaker":"A","text":"このホテルには、中国料理とフランス料理と和食のレストランがあります。夕食は、どのレストランにしますか？"},{"id":{"d":596,"n":2,"t":3},"speaker":"B","text":"今日は何を食べたい気分ですか？"},{"id":{"d":596,"n":3,"t":3},"speaker":"A","text":"昼食がうどんだったので、和食以外がいいです。"},{"id":{"d":596,"n":4,"t":3},"speaker":"B","text":"それでは、中国料理かフランス料理のレストランにしましょう。"},{"id":{"d":596,"n":5,"t":3},"speaker":"A","text":"ここにレストランのパンフレットがあるので、それらを見て、どちらのお店にするかを決めましょう。"},{"id":{"d":596,"n":6,"t":3},"speaker":"B","text":"どちらの料理の写真がおいしそうですか？"},{"id":{"d":596,"n":7,"t":3},"speaker":"A","text":"フランス料理のレストランのメインディッシュがとてもおいしそうです。"},{"id":{"d":596,"n":8,"t":3},"speaker":"B","text":"本当ですね。フランス料理のレストランに決めましょう。"}];
INSERT INTO sentence [{"id":{"d":597,"n":1,"t":3},"speaker":"A","text":"私は九州新幹線に乗ったことがありません。"},{"id":{"d":597,"n":2,"t":3},"speaker":"B","text":"私もありません。そもそも、九州新幹線がどこを走っているのか分かりません。"},{"id":{"d":597,"n":3,"t":3},"speaker":"A","text":"福岡県と鹿児島県の間を走っていますよ。"},{"id":{"d":597,"n":4,"t":3},"speaker":"B","text":"九州を縦断しているのですね。"},{"id":{"d":597,"n":5,"t":3},"speaker":"A","text":"そうです。もしかして九州新幹線の愛称もご存じありませんか？"},{"id":{"d":597,"n":6,"t":3},"speaker":"B","text":"ええ、知りません。"},{"id":{"d":597,"n":7,"t":3},"speaker":"A","text":"「つばめ」と「さくら」と「みずほ」ですよ。"},{"id":{"d":597,"n":8,"t":3},"speaker":"B","text":"よく知っていますね。"},{"id":{"d":597,"n":9,"t":3},"speaker":"A","text":"新幹線が好きなのです。だから九州新幹線にも早く乗ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":598,"n":1,"t":3},"speaker":"A","text":"次の夏休みに、一人でおばあちゃんの家に行ってみたいです。"},{"id":{"d":598,"n":2,"t":3},"speaker":"B","text":"一人で行って、おばあちゃんの家に泊まるということですか？"},{"id":{"d":598,"n":3,"t":3},"speaker":"A","text":"はい。当然、帰りも一人で帰ってきます。"},{"id":{"d":598,"n":4,"t":3},"speaker":"B","text":"大丈夫でしょうか。心配です。"},{"id":{"d":598,"n":5,"t":3},"speaker":"A","text":"私はもう12歳ですよ。ご心配には及びません。"},{"id":{"d":598,"n":6,"t":3},"speaker":"B","text":"おばあちゃんの家の最寄り駅から、おばあちゃんの家までの道は分かりますか？"},{"id":{"d":598,"n":7,"t":3},"speaker":"A","text":"それは少し曖昧です。"},{"id":{"d":598,"n":8,"t":3},"speaker":"B","text":"それではせめて、駅までおばあちゃんに迎えに来てもらいましょう。"}];
INSERT INTO sentence [{"id":{"d":599,"n":1,"t":3},"speaker":"A","text":"ゴールデンウイーク期間中に北海道へ行きます。"},{"id":{"d":599,"n":2,"t":3},"speaker":"B","text":"家族旅行ですか？"},{"id":{"d":599,"n":3,"t":3},"speaker":"A","text":"父が単身赴任しているので、会いに行くのです。"},{"id":{"d":599,"n":4,"t":3},"speaker":"B","text":"お父さまと離れて暮らしているのですね。"},{"id":{"d":599,"n":5,"t":3},"speaker":"A","text":"はい。1年半前に北海道に転勤になって、その時から単身赴任しています。"},{"id":{"d":599,"n":6,"t":3},"speaker":"B","text":"寂しいですね。"},{"id":{"d":599,"n":7,"t":3},"speaker":"A","text":"だいぶ慣れましたがやはり寂しいですね。でも、北海道なので会いに行った際は、ついでに観光もできるので楽しんでいます。"},{"id":{"d":599,"n":8,"t":3},"speaker":"B","text":"そうですね、北海道なら観光名所がたくさんありますね。"}];
INSERT INTO sentence [{"id":{"d":600,"n":1,"t":3},"speaker":"A","text":"パスポートの更新をお願いします。"},{"id":{"d":600,"n":2,"t":3},"speaker":"B","text":"新規ではなく更新ですか？"},{"id":{"d":600,"n":3,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":600,"n":4,"t":3},"speaker":"B","text":"現在お持ちのパスポートを拝見します。"},{"id":{"d":600,"n":5,"t":3},"speaker":"A","text":"これです。"},{"id":{"d":600,"n":6,"t":3},"speaker":"B","text":"有効期限が5年と10年が選べますが、どちらにしますか？"},{"id":{"d":600,"n":7,"t":3},"speaker":"A","text":"10年にします。"},{"id":{"d":600,"n":8,"t":3},"speaker":"B","text":"それでは、こちらの申請用紙に必要事項の記入をお願いします。"}];
INSERT INTO sentence [{"id":{"d":601,"n":1,"t":3},"speaker":"A","text":"今年は、明治神宮に初詣に行きました。"},{"id":{"d":601,"n":2,"t":3},"speaker":"B","text":"混雑していませんでしたか？"},{"id":{"d":601,"n":3,"t":3},"speaker":"A","text":"想像以上の人混みでした。"},{"id":{"d":601,"n":4,"t":3},"speaker":"B","text":"混雑している様子が、テレビのニュース番組で報道されることもあるくらいですよね。"},{"id":{"d":601,"n":5,"t":3},"speaker":"A","text":"はい。でも、明治神宮の初詣は有名なので一度行ってみたかったのです。"},{"id":{"d":601,"n":6,"t":3},"speaker":"B","text":"実際に行ってみてどうでしたか？"},{"id":{"d":601,"n":7,"t":3},"speaker":"A","text":"都会とは思えない雰囲気で、厳かな気分になりました。"},{"id":{"d":601,"n":8,"t":3},"speaker":"B","text":"私は行ったことがないので機会があれば行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":602,"n":1,"t":3},"speaker":"A","text":"17日にお父さんがタイ王国から一時帰国するそうです。"},{"id":{"d":602,"n":2,"t":3},"speaker":"B","text":"本当ですか？うれしいです。"},{"id":{"d":602,"n":3,"t":3},"speaker":"A","text":"お父さんに会えるのは久しぶりですね。"},{"id":{"d":602,"n":4,"t":3},"speaker":"B","text":"空港にお迎えに行きたいです。"},{"id":{"d":602,"n":5,"t":3},"speaker":"A","text":"そうですね。みんなで行きましょう。"},{"id":{"d":602,"n":6,"t":3},"speaker":"B","text":"どのくらい日本にいられるのですか？"},{"id":{"d":602,"n":7,"t":3},"speaker":"A","text":"2週間ほどいられるそうです。"},{"id":{"d":602,"n":8,"t":3},"speaker":"B","text":"その間、お父さんとあちこち出掛けたいです。"}];
INSERT INTO sentence [{"id":{"d":603,"n":1,"t":3},"speaker":"A","text":"月に旅行ができたら、何をしたいですか？"},{"id":{"d":603,"n":2,"t":3},"speaker":"B","text":"私は、月を一周したいです。"},{"id":{"d":603,"n":3,"t":3},"speaker":"A","text":"どのくらいかかるのか分かりませんね。"},{"id":{"d":603,"n":4,"t":3},"speaker":"B","text":"もし一日で周れないなら、野宿してみたいです。"},{"id":{"d":603,"n":5,"t":3},"speaker":"A","text":"月で眠るなんて、どのような感じか想像もつきませんね。"},{"id":{"d":603,"n":6,"t":3},"speaker":"B","text":"あなたは何をしてみたいですか？"},{"id":{"d":603,"n":7,"t":3},"speaker":"A","text":"私は、月にあるものを持ち帰りたいです。"},{"id":{"d":603,"n":8,"t":3},"speaker":"B","text":"地球に帰ったら、良い記念品になりますね。"}];
INSERT INTO sentence [{"id":{"d":604,"n":1,"t":3},"speaker":"A","text":"この飛行機の着陸まであと何時間でしょうか？"},{"id":{"d":604,"n":2,"t":3},"speaker":"B","text":"9時間くらいです。"},{"id":{"d":604,"n":3,"t":3},"speaker":"A","text":"機内ですることもないので少し寝ます。"},{"id":{"d":604,"n":4,"t":3},"speaker":"B","text":"後で映画が始まりますよ。"},{"id":{"d":604,"n":5,"t":3},"speaker":"A","text":"何という映画ですか？"},{"id":{"d":604,"n":6,"t":3},"speaker":"B","text":"『ジュラシック・ワールド』です。"},{"id":{"d":604,"n":7,"t":3},"speaker":"A","text":"映画が始まりそうになったら、起こしてもらえますか？"},{"id":{"d":604,"n":8,"t":3},"speaker":"B","text":"よいですよ。"}];
INSERT INTO sentence [{"id":{"d":605,"n":1,"t":3},"speaker":"A","text":"こんにちは。田畑さんは、週末はご自宅がお留守だったようですね？"},{"id":{"d":605,"n":2,"t":3},"speaker":"B","text":"こんにちは。はい。私は、家族と旅行をしていたのです。"},{"id":{"d":605,"n":3,"t":3},"speaker":"A","text":"そうでしたか。どちらへ行ったのですか？"},{"id":{"d":605,"n":4,"t":3},"speaker":"B","text":"大分県です。温泉に漬かってきました。"},{"id":{"d":605,"n":5,"t":3},"speaker":"A","text":"いいですね。大分県は温泉地がたくさんありますよね。"},{"id":{"d":605,"n":6,"t":3},"speaker":"B","text":"はい。温泉巡りをしてリフレッシュできました。"},{"id":{"d":605,"n":7,"t":3},"speaker":"A","text":"それは良かったです。飛行機で出掛けたのですか？"},{"id":{"d":605,"n":8,"t":3},"speaker":"B","text":"はい。久しぶりの家族旅行で良い思い出になりました。"}];
INSERT INTO sentence [{"id":{"d":606,"n":1,"t":3},"speaker":"A","text":"UFOを見たことはありますか？"},{"id":{"d":606,"n":2,"t":3},"speaker":"B","text":"私はありませんが、友達が「UFOを見た」と言っていました。"},{"id":{"d":606,"n":3,"t":3},"speaker":"A","text":"どのようなUFOを見たのでしょうか？"},{"id":{"d":606,"n":4,"t":3},"speaker":"B","text":"空で光り輝き、急に消えてしまうUFOだったそうです。"},{"id":{"d":606,"n":5,"t":3},"speaker":"A","text":"写真などはないのでしょうか？"},{"id":{"d":606,"n":6,"t":3},"speaker":"B","text":"はい。ありません。写真を撮ろうとしたら、もう消えていたそうです。"},{"id":{"d":606,"n":7,"t":3},"speaker":"A","text":"あなたはUFOを信じますか？"},{"id":{"d":606,"n":8,"t":3},"speaker":"B","text":"友達なので信じたいですが、何かを見間違えた可能性もあると思います。"}];
INSERT INTO sentence [{"id":{"d":607,"n":1,"t":3},"speaker":"A","text":"いよいよ離陸ですね。"},{"id":{"d":607,"n":2,"t":3},"speaker":"B","text":"何度経験しても、離陸時には緊張してしまいます。"},{"id":{"d":607,"n":3,"t":3},"speaker":"A","text":"私は耳の調子が悪くなるので離着陸が苦手です。"},{"id":{"d":607,"n":4,"t":3},"speaker":"B","text":"あなたは、気圧の変化に弱いのかもしれませんね。"},{"id":{"d":607,"n":5,"t":3},"speaker":"A","text":"はい。いろいろと対策をしたのですが、効果がありませんでした。"},{"id":{"d":607,"n":6,"t":3},"speaker":"B","text":"慣れることはありませんか？"},{"id":{"d":607,"n":7,"t":3},"speaker":"A","text":"はい。慣れませんね。毎回とても不快です。"},{"id":{"d":607,"n":8,"t":3},"speaker":"B","text":"今日もこれから離陸するので、しばらく我慢が必要ですね。"}];
INSERT INTO sentence [{"id":{"d":608,"n":1,"t":3},"speaker":"A","text":"海に泳ぎに行きましょう。"},{"id":{"d":608,"n":2,"t":3},"speaker":"B","text":"少し待ってください。"},{"id":{"d":608,"n":3,"t":3},"speaker":"A","text":"どうかしましたか？"},{"id":{"d":608,"n":4,"t":3},"speaker":"B","text":"ゴーグルが見つかりません。"},{"id":{"d":608,"n":5,"t":3},"speaker":"A","text":"ゴーグルを持って来た記憶はありますか？"},{"id":{"d":608,"n":6,"t":3},"speaker":"B","text":"はい。このバッグに入れたと思うのですが、見当たりません。"},{"id":{"d":608,"n":7,"t":3},"speaker":"A","text":"入れ忘れてしまったのかもしれませんね。"},{"id":{"d":608,"n":8,"t":3},"speaker":"B","text":"この旅行のために新しく買ったゴーグルなので、残念です。"}];
INSERT INTO sentence [{"id":{"d":609,"n":1,"t":3},"speaker":"A","text":"春休みに、息子の家族が遊びに来ることになりました。"},{"id":{"d":609,"n":2,"t":3},"speaker":"B","text":"それはにぎやかになりそうですね。"},{"id":{"d":609,"n":3,"t":3},"speaker":"A","text":"はい。孫を4人連れて来るのでとてもにぎやかになるでしょう。"},{"id":{"d":609,"n":4,"t":3},"speaker":"B","text":"それは大人数ですね。食事の支度などが大変ではありませんか？"},{"id":{"d":609,"n":5,"t":3},"speaker":"A","text":"正直、大変な面もありますが、息子夫婦が手伝ってくれます。"},{"id":{"d":609,"n":6,"t":3},"speaker":"B","text":"それは良かったです。お孫さんはおいくつですか？"},{"id":{"d":609,"n":7,"t":3},"speaker":"A","text":"14歳、11歳、5歳、4歳の4人です。"},{"id":{"d":609,"n":8,"t":3},"speaker":"B","text":"14歳の子も手伝ってくれそうですね。"}];
INSERT INTO sentence [{"id":{"d":610,"n":1,"t":3},"speaker":"A","text":"忘年会の幹事をしてもらえませんか？"},{"id":{"d":610,"n":2,"t":3},"speaker":"B","text":"今年の幹事は、秋山さんではありませんか？"},{"id":{"d":610,"n":3,"t":3},"speaker":"A","text":"その予定だったのですが、秋山さんが急に1カ月ほどタイ王国へ出張になってしまったのです。"},{"id":{"d":610,"n":4,"t":3},"speaker":"B","text":"そのようなことですか。他に幹事をできそうな人はいませんか？"},{"id":{"d":610,"n":5,"t":3},"speaker":"A","text":"いろいろと声を掛けたのですが、見つからなくて困っています。"},{"id":{"d":610,"n":6,"t":3},"speaker":"B","text":"分かりました。今年は私が幹事をします。"},{"id":{"d":610,"n":7,"t":3},"speaker":"A","text":"助かります。ありがとうございます。"},{"id":{"d":610,"n":8,"t":3},"speaker":"B","text":"なるべく楽しい忘年会になるように頑張りますね。"}];
INSERT INTO sentence [{"id":{"d":611,"n":1,"t":3},"speaker":"A","text":"おはようございます。高原の朝は冷えますね。"},{"id":{"d":611,"n":2,"t":3},"speaker":"B","text":"おはようございます。本当ですね。羽織るものが欲しいです。"},{"id":{"d":611,"n":3,"t":3},"speaker":"A","text":"朝食の前に散歩でもいかがですか？"},{"id":{"d":611,"n":4,"t":3},"speaker":"B","text":"良いですね。どこを歩きましょうか？"},{"id":{"d":611,"n":5,"t":3},"speaker":"A","text":"湖の周りを歩きましょう。林道の先に湖があるそうですよ。"},{"id":{"d":611,"n":6,"t":3},"speaker":"B","text":"それでは、湖を一周しましょう。"},{"id":{"d":611,"n":7,"t":3},"speaker":"A","text":"一周するのにどのくらい時間がかかるでしょうか？"},{"id":{"d":611,"n":8,"t":3},"speaker":"B","text":"ロッジのオーナーに聞いてみましょう。"}];
INSERT INTO sentence [{"id":{"d":612,"n":1,"t":3},"speaker":"A","text":"どちらがレンタカーを運転しますか？"},{"id":{"d":612,"n":2,"t":3},"speaker":"B","text":"申し訳ありませんが、あなたに自動車の運転をお願いします。"},{"id":{"d":612,"n":3,"t":3},"speaker":"A","text":"あなたは、「自動車の運転は好きだ」と話していましたよね？"},{"id":{"d":612,"n":4,"t":3},"speaker":"B","text":"はい。運転したいのですが、実は運転免許証を自宅に忘れてきてしまったのです。"},{"id":{"d":612,"n":5,"t":3},"speaker":"A","text":"そうなのですか？よく探しましたか？"},{"id":{"d":612,"n":6,"t":3},"speaker":"B","text":"もちろんです。妻に電話して聞いたところ、リビングルームに私の運転免許証が置かれていたそうです。"},{"id":{"d":612,"n":7,"t":3},"speaker":"A","text":"それではこの旅行中は私が運転しますね。"},{"id":{"d":612,"n":8,"t":3},"speaker":"B","text":"本当にすみません。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":613,"n":1,"t":3},"speaker":"A","text":"もしもし、先日そちらに宿泊した者ですが、少しよろしいですか？"},{"id":{"d":613,"n":2,"t":3},"speaker":"B","text":"はい。どのようなご用件でしょうか？"},{"id":{"d":613,"n":3,"t":3},"speaker":"A","text":"泊まった部屋に電気シェーバーを忘れてしまったのですが、そちらで見つかっていないでしょうか？"},{"id":{"d":613,"n":4,"t":3},"speaker":"B","text":"確認いたします。お泊りになった部屋番号は覚えていますか？"},{"id":{"d":613,"n":5,"t":3},"speaker":"A","text":"807号室です。"},{"id":{"d":613,"n":6,"t":3},"speaker":"B","text":"忘れ物としてお預かりしております。"},{"id":{"d":613,"n":7,"t":3},"speaker":"A","text":"良かったです。着払いでよいので送っていただけますか？"},{"id":{"d":613,"n":8,"t":3},"speaker":"B","text":"かしこまりました。お送りする住所をお願いします。"}];
INSERT INTO sentence [{"id":{"d":614,"n":1,"t":3},"speaker":"A","text":"東京スカイツリーに来たのは初めてですか？"},{"id":{"d":614,"n":2,"t":3},"speaker":"B","text":"はい、初めてです。ずっと来たいと思っていました。"},{"id":{"d":614,"n":3,"t":3},"speaker":"A","text":"天望デッキに行きましょう。"},{"id":{"d":614,"n":4,"t":3},"speaker":"B","text":"まずはお店を見て回って、最後に天望デッキに行きましょうよ。"},{"id":{"d":614,"n":5,"t":3},"speaker":"A","text":"いいですね、行きましょう。お店もたくさんあって楽しめますよね。"},{"id":{"d":614,"n":6,"t":3},"speaker":"B","text":"妹にお土産を頼まれているので、ここで買おうと思っています。"},{"id":{"d":614,"n":7,"t":3},"speaker":"A","text":"どのようなものを頼まれたのですか？"},{"id":{"d":614,"n":8,"t":3},"speaker":"B","text":"妹の好きなキャラクターのシールとメモ帳です。"}];
INSERT INTO sentence [{"id":{"d":615,"n":1,"t":3},"speaker":"A","text":"有給休暇を取って、ユニバーサル・スタジオ・ジャパンに行きます。"},{"id":{"d":615,"n":2,"t":3},"speaker":"B","text":"いつ行くのですか？"},{"id":{"d":615,"n":3,"t":3},"speaker":"A","text":"来月の9日です。"},{"id":{"d":615,"n":4,"t":3},"speaker":"B","text":"9日は平日なので、週末ほど混雑はしないでしょうね。"},{"id":{"d":615,"n":5,"t":3},"speaker":"A","text":"私は初めてユニバーサル・スタジオ・ジャパンに行くのですが、やはり混むのでしょうか？"},{"id":{"d":615,"n":6,"t":3},"speaker":"B","text":"はい。私が去年行った時は、歩くのも大変なくらい混んでいました。"},{"id":{"d":615,"n":7,"t":3},"speaker":"A","text":"そうですか。やはり人気のアトラクションは長時間待つのでしょうね。"},{"id":{"d":615,"n":8,"t":3},"speaker":"B","text":"2時間待ちや3時間待ちのアトラクションがいくつもありましたよ。"}];
INSERT INTO sentence [{"id":{"d":616,"n":1,"t":3},"speaker":"A","text":"菊池さん、起きてください。"},{"id":{"d":616,"n":2,"t":3},"speaker":"B","text":"どうしましたか？"},{"id":{"d":616,"n":3,"t":3},"speaker":"A","text":"大変です。降りる駅を乗り過ごしてしまいました。"},{"id":{"d":616,"n":4,"t":3},"speaker":"B","text":"本当ですか？今、この電車はどこを走っているのでしょうか？"},{"id":{"d":616,"n":5,"t":3},"speaker":"A","text":"本当です。もうすぐ京都駅という地点を走っています。"},{"id":{"d":616,"n":6,"t":3},"speaker":"B","text":"つい、深く眠ってしまいました。すみません。"},{"id":{"d":616,"n":7,"t":3},"speaker":"A","text":"いえ。私も眠っていて、先ほどふと目が覚めたのです。"},{"id":{"d":616,"n":8,"t":3},"speaker":"B","text":"ひとまず京都駅で降りて、反対行きの電車に乗るしかありませんね。"}];
INSERT INTO sentence [{"id":{"d":617,"n":1,"t":3},"speaker":"A","text":"スキーが上手ですね。"},{"id":{"d":617,"n":2,"t":3},"speaker":"B","text":"雪国出身で子どもの頃からスキーをしていたのです。"},{"id":{"d":617,"n":3,"t":3},"speaker":"A","text":"滑り方を教えてもらえませんか？"},{"id":{"d":617,"n":4,"t":3},"speaker":"B","text":"よいですよ。スキー板をハの字に開けますか？"},{"id":{"d":617,"n":5,"t":3},"speaker":"A","text":"立っていることがようやくなので、ハの字に開くことは無理です。"},{"id":{"d":617,"n":6,"t":3},"speaker":"B","text":"足をハの字に開いた方が、安定して立っていられますよ。"},{"id":{"d":617,"n":7,"t":3},"speaker":"A","text":"そうでしょうか？すみませんが、腕につかまっても良いですか？"},{"id":{"d":617,"n":8,"t":3},"speaker":"B","text":"はい。では私の腕につかまってください。"}];
INSERT INTO sentence [{"id":{"d":618,"n":1,"t":3},"speaker":"A","text":"英国から手紙が届きました。"},{"id":{"d":618,"n":2,"t":3},"speaker":"B","text":"誰からですか？"},{"id":{"d":618,"n":3,"t":3},"speaker":"A","text":"大学の友達です。"},{"id":{"d":618,"n":4,"t":3},"speaker":"B","text":"英国に行っているのですか？"},{"id":{"d":618,"n":5,"t":3},"speaker":"A","text":"はい。大学の夏休みを利用して英国をあちこち旅行しているのです。"},{"id":{"d":618,"n":6,"t":3},"speaker":"B","text":"うらやましいですね。"},{"id":{"d":618,"n":7,"t":3},"speaker":"A","text":"手紙からも、旅行を満喫している様子がうかがえます。"},{"id":{"d":618,"n":8,"t":3},"speaker":"B","text":"帰国したら、お土産話を聞きたいですね。"}];
INSERT INTO sentence [{"id":{"d":619,"n":1,"t":3},"speaker":"A","text":"夏休みに富士山に登りました。"},{"id":{"d":619,"n":2,"t":3},"speaker":"B","text":"無事に登頂できましたか？"},{"id":{"d":619,"n":3,"t":3},"speaker":"A","text":"はい。どのようにかして頂上までたどり着きました。"},{"id":{"d":619,"n":4,"t":3},"speaker":"B","text":"大変だったでしょうね。"},{"id":{"d":619,"n":5,"t":3},"speaker":"A","text":"頂上まで登るよりも、下山する時の方が大変でした。"},{"id":{"d":619,"n":6,"t":3},"speaker":"B","text":"どうしてですか？"},{"id":{"d":619,"n":7,"t":3},"speaker":"A","text":"下りの方が、膝への負担が大きくてつらかったのです。"},{"id":{"d":619,"n":8,"t":3},"speaker":"B","text":"それは意外な感想です。間違いなく登りの方がつらいのだと思っていました。"}];
INSERT INTO sentence [{"id":{"d":620,"n":1,"t":3},"speaker":"A","text":"温泉旅行はどうでしたか？"},{"id":{"d":620,"n":2,"t":3},"speaker":"B","text":"リフレッシュできました。"},{"id":{"d":620,"n":3,"t":3},"speaker":"A","text":"写真を撮っていたら、見せてください。"},{"id":{"d":620,"n":4,"t":3},"speaker":"B","text":"はい。これが泊まった旅館の写真です。"},{"id":{"d":620,"n":5,"t":3},"speaker":"A","text":"立派な建物ですね。部屋の写真もありますか？"},{"id":{"d":620,"n":6,"t":3},"speaker":"B","text":"これです。広くてきれいな部屋でした。"},{"id":{"d":620,"n":7,"t":3},"speaker":"A","text":"くつろげそうな雰囲気ですね。"},{"id":{"d":620,"n":8,"t":3},"speaker":"B","text":"これは夕食の写真です。"},{"id":{"d":620,"n":9,"t":3},"speaker":"A","text":"豪華ですね。盛り付けが美しいです。"}];
INSERT INTO sentence [{"id":{"d":621,"n":1,"t":3},"speaker":"A","text":"今度、兄が留学先から一時帰国します。"},{"id":{"d":621,"n":2,"t":3},"speaker":"B","text":"お兄さまは留学しているのですか？知りませんでした。"},{"id":{"d":621,"n":3,"t":3},"speaker":"A","text":"すみません。言いそびれていました。"},{"id":{"d":621,"n":4,"t":3},"speaker":"B","text":"どこに留学しているのですか？"},{"id":{"d":621,"n":5,"t":3},"speaker":"A","text":"英国です。もう2年以上たつので休みを利用して久しぶりに帰国することになりました。"},{"id":{"d":621,"n":6,"t":3},"speaker":"B","text":"それは楽しみですね。"},{"id":{"d":621,"n":7,"t":3},"speaker":"A","text":"はい。聞きたいことも話したいこともたくさんあります。"},{"id":{"d":621,"n":8,"t":3},"speaker":"B","text":"私は一人っ子なので、仲の良い兄弟がうらやましいです。"}];
INSERT INTO sentence [{"id":{"d":622,"n":1,"t":3},"speaker":"A","text":"6階に行きましょう。"},{"id":{"d":622,"n":2,"t":3},"speaker":"B","text":"エレベーターとエスカレーターのどちらで行きますか？"},{"id":{"d":622,"n":3,"t":3},"speaker":"A","text":"近い方で行きましょう。"},{"id":{"d":622,"n":4,"t":3},"speaker":"B","text":"そこにフロアマップがあるので確認しましょう。"},{"id":{"d":622,"n":5,"t":3},"speaker":"A","text":"エレベーターはあちらの隅にあるようです。エスカレーターは見つかりましたか？"},{"id":{"d":622,"n":6,"t":3},"speaker":"B","text":"エスカレーターはフロアの中央にあります。"},{"id":{"d":622,"n":7,"t":3},"speaker":"A","text":"それではエスカレーターの方が近いですね。"},{"id":{"d":622,"n":8,"t":3},"speaker":"B","text":"はい。エスカレーターで6階に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":623,"n":1,"t":3},"speaker":"A","text":"かわいい置物ですね。"},{"id":{"d":623,"n":2,"t":3},"speaker":"B","text":"トルコを旅行した時に買ったものです。"},{"id":{"d":623,"n":3,"t":3},"speaker":"A","text":"トルコに行ったことがあるのですね？"},{"id":{"d":623,"n":4,"t":3},"speaker":"B","text":"はい。4年くらい前に友達と行きました。"},{"id":{"d":623,"n":5,"t":3},"speaker":"A","text":"私も行ってみたいのですが、どうでしたか？"},{"id":{"d":623,"n":6,"t":3},"speaker":"B","text":"とても良かったです。私はトルコが大好きになりました。"},{"id":{"d":623,"n":7,"t":3},"speaker":"A","text":"どのようなところが良かったですか？"},{"id":{"d":623,"n":8,"t":3},"speaker":"B","text":"景色が壮大ですし、文化が独特で魅力的なところです。"},{"id":{"d":623,"n":9,"t":3},"speaker":"A","text":"今度、お薦めのトルコの観光スポットを教えてください。"}];
INSERT INTO sentence [{"id":{"d":624,"n":1,"t":3},"speaker":"A","text":"初めてスノーボードをしたのはいつですか？"},{"id":{"d":624,"n":2,"t":3},"speaker":"B","text":"小学生の時だと思います。"},{"id":{"d":624,"n":3,"t":3},"speaker":"A","text":"それは早いですね。"},{"id":{"d":624,"n":4,"t":3},"speaker":"B","text":"親戚の伯父さんが毎年のように、スノーボードをするためにスキー場へ連れて行ってくれたのです。"},{"id":{"d":624,"n":5,"t":3},"speaker":"A","text":"スノーボードの滑り方も教えてもらったのですか？"},{"id":{"d":624,"n":6,"t":3},"speaker":"B","text":"はい。その伯父さんがいとこと私に教えてくれました。"},{"id":{"d":624,"n":7,"t":3},"speaker":"A","text":"楽しかったでしょうね。"},{"id":{"d":624,"n":8,"t":3},"speaker":"B","text":"伯父のおかげでスノーボードが大好きになりました。"}];
INSERT INTO sentence [{"id":{"d":625,"n":1,"t":3},"speaker":"A","text":"この先、高速道路と一般道路と2つのルートがあります。"},{"id":{"d":625,"n":2,"t":3},"speaker":"B","text":"高速道路の方が、目的地に早く着くのでしょうか？"},{"id":{"d":625,"n":3,"t":3},"speaker":"A","text":"はい。インターネットで調べたら、高速道路を利用する方が目的地には約30分早く到着するようです。"},{"id":{"d":625,"n":4,"t":3},"speaker":"B","text":"でも、私は高速道路を運転する自信がありません。"},{"id":{"d":625,"n":5,"t":3},"speaker":"A","text":"それでは私が自動車の運転を代わります。"},{"id":{"d":625,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。それでは、この先で車を止められそうな所を見付け次第、駐車をします。"},{"id":{"d":625,"n":7,"t":3},"speaker":"A","text":"飲み物を買いたいので、できればコンビニエンスストアに止めて欲しいです。"},{"id":{"d":625,"n":8,"t":3},"speaker":"B","text":"分かりました。次にコンビニエンスストアが見付かれば、駐車をします。"}];
INSERT INTO sentence [{"id":{"d":626,"n":1,"t":3},"speaker":"A","text":"立派なお城ですね。"},{"id":{"d":626,"n":2,"t":3},"speaker":"B","text":"明治時代に今の姿に復元されたそうです。"},{"id":{"d":626,"n":3,"t":3},"speaker":"A","text":"そうでしたか。その後の地震や戦争を耐え抜いたのですね。"},{"id":{"d":626,"n":4,"t":3},"speaker":"B","text":"そう思って見ると感慨深いですね。"},{"id":{"d":626,"n":5,"t":3},"speaker":"A","text":"中に入ってみますか？"},{"id":{"d":626,"n":6,"t":3},"speaker":"B","text":"入れるのですか？"},{"id":{"d":626,"n":7,"t":3},"speaker":"A","text":"はい。お城に残されていた物なども展示されています。"},{"id":{"d":626,"n":8,"t":3},"speaker":"B","text":"とても興味があります。お城の中へ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":627,"n":1,"t":3},"speaker":"A","text":"部活動で遠征試合に行くことになりました。"},{"id":{"d":627,"n":2,"t":3},"speaker":"B","text":"どこに行くのですか？"},{"id":{"d":627,"n":3,"t":3},"speaker":"A","text":"長野県です。"},{"id":{"d":627,"n":4,"t":3},"speaker":"B","text":"そうですか。遠いので送り迎えが必要ですね。"},{"id":{"d":627,"n":5,"t":3},"speaker":"A","text":"学校がバスを用意してくれて、部員全員でそれに乗って行くそうです。"},{"id":{"d":627,"n":6,"t":3},"speaker":"B","text":"それは助かります。"},{"id":{"d":627,"n":7,"t":3},"speaker":"A","text":"ただバスの費用がかかるので、明日集金があります。"},{"id":{"d":627,"n":8,"t":3},"speaker":"B","text":"構いませんよ。いくらですか？"},{"id":{"d":627,"n":9,"t":3},"speaker":"A","text":"一人1,000円です。"},{"id":{"d":627,"n":10,"t":3},"speaker":"B","text":"分かりました。用意しておきます。"}];
INSERT INTO sentence [{"id":{"d":628,"n":1,"t":3},"speaker":"A","text":"この頃、海外旅行している夢ばかり見ます。"},{"id":{"d":628,"n":2,"t":3},"speaker":"B","text":"どの国を旅行している夢ですか？"},{"id":{"d":628,"n":3,"t":3},"speaker":"A","text":"それが、イタリアだったりブラジルだったり、毎回違うのです。"},{"id":{"d":628,"n":4,"t":3},"speaker":"B","text":"夢の中で海外旅行ができるなんて楽しそうです。"},{"id":{"d":628,"n":5,"t":3},"speaker":"A","text":"時々、窃盗の被害に遭うなど怖い思いもしますよ。"},{"id":{"d":628,"n":6,"t":3},"speaker":"B","text":"非常にリアルですね。"},{"id":{"d":628,"n":7,"t":3},"speaker":"A","text":"そうなのです。正夢だったら怖いと思うこともあります。"},{"id":{"d":628,"n":8,"t":3},"speaker":"B","text":"実際に海外で怖い体験をしてことはありますか？"},{"id":{"d":628,"n":9,"t":3},"speaker":"A","text":"いえ、特にありません。ドラマや映画で見たことがあるだけです。"}];
INSERT INTO sentence [{"id":{"d":629,"n":1,"t":3},"speaker":"A","text":"飛行機のチケットを購入したいのですが。"},{"id":{"d":629,"n":2,"t":3},"speaker":"B","text":"どちらからどちらまででしょうか？"},{"id":{"d":629,"n":3,"t":3},"speaker":"A","text":"成田国際空港からアルゼンチンまでです。"},{"id":{"d":629,"n":4,"t":3},"speaker":"B","text":"お日にちはお決まりですか？"},{"id":{"d":629,"n":5,"t":3},"speaker":"A","text":"今日か明日の便はありますか？"},{"id":{"d":629,"n":6,"t":3},"speaker":"B","text":"はい。直行便と乗継便があります。"},{"id":{"d":629,"n":7,"t":3},"speaker":"A","text":"直行便で行きたいですが、料金が高いですよね？"},{"id":{"d":629,"n":8,"t":3},"speaker":"B","text":"そうですね。乗継便より割高になります。"},{"id":{"d":629,"n":9,"t":3},"speaker":"A","text":"乗継便で、なるべくフライト時間が短く済むルートを教えてください。"},{"id":{"d":629,"n":10,"t":3},"speaker":"B","text":"分かりました。お調べしますので少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":630,"n":1,"t":3},"speaker":"A","text":"今度一人でハワイ州に行くそうですけど、目的は何ですか？"},{"id":{"d":630,"n":2,"t":3},"speaker":"B","text":"観光です。『HAWAIIFIVE-O』という作品を知っていますか？"},{"id":{"d":630,"n":3,"t":3},"speaker":"A","text":"知っています。確か、昔にそのような海外ドラマがありましたね。"},{"id":{"d":630,"n":4,"t":3},"speaker":"B","text":"どうして知っているのですか？"},{"id":{"d":630,"n":5,"t":3},"speaker":"A","text":"テーマ曲がとても有名ですよね。確かザ・ベンチャーズも演奏しています。その曲がきっかけで、ドラマを知りました。"},{"id":{"d":630,"n":6,"t":3},"speaker":"B","text":"それでは『私立探偵マグナム』も知っていますか？"},{"id":{"d":630,"n":7,"t":3},"speaker":"A","text":"はい、知っています。それもハワイ州が舞台の昔のドラマです。"},{"id":{"d":630,"n":8,"t":3},"speaker":"B","text":"実は、どちらもリブートされて新しいドラマになっています。"},{"id":{"d":630,"n":9,"t":3},"speaker":"A","text":"それは知りませんでした。"},{"id":{"d":630,"n":10,"t":3},"speaker":"B","text":"それらのドラマ内で放送されている風景が素晴らしいので、ハワイ州へ行ってみたくなったのです。"}];
INSERT INTO sentence [{"id":{"d":631,"n":1,"t":3},"speaker":"A","text":"そろそろガソリンスタンドに入ります。"},{"id":{"d":631,"n":2,"t":3},"speaker":"B","text":"ガソリンを入れるだけですか？"},{"id":{"d":631,"n":3,"t":3},"speaker":"A","text":"いいえ。そのあと高速道路に入って4時間くらいは走るでしょうから、しばらく休みましょう。"},{"id":{"d":631,"n":4,"t":3},"speaker":"B","text":"ドライブの休憩のタイミングは運転手にお任せします。休むのならカフェでもあるといいのですが。"},{"id":{"d":631,"n":5,"t":3},"speaker":"A","text":"カフェとコンビニエンスストアが近くに存在したはずです。車の整備もお願いして、私たちはカフェに入りましょう。"},{"id":{"d":631,"n":6,"t":3},"speaker":"B","text":"最近はカフェ併設型のガソリンスタンドが多いですね。"},{"id":{"d":631,"n":7,"t":3},"speaker":"A","text":"ふと思ったのですが、電気自動車隆盛となったらガソリンスタンドはどうなるのでしょうか？"},{"id":{"d":631,"n":8,"t":3},"speaker":"B","text":"確かに、ガソリン車を利用する人がいなくなれば、ガソリンスタンドは必要なくなりますね。"},{"id":{"d":631,"n":9,"t":3},"speaker":"A","text":"自動車が電気自動車だけになるということは本当にありえるのでしょうか？"},{"id":{"d":631,"n":10,"t":3},"speaker":"B","text":"州によって方針が異なると思いますが、米国では「あと10年で電気自動車だけになる」ということも言われているようです。"},{"id":{"d":631,"n":11,"t":3},"speaker":"A","text":"ガソリン車から電気自動車へと急激な変化ですね。地球のためには、変化を受け入れていかなければならないのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":632,"n":1,"t":3},"speaker":"A","text":"メキシコの骸骨祭りを知っていますか？"},{"id":{"d":632,"n":2,"t":3},"speaker":"B","text":"骸骨祭りという名前だけは聞いたことがあります。詳しく教えてもらえますか？"},{"id":{"d":632,"n":3,"t":3},"speaker":"A","text":"正式には「死者の日」と言って11月の1日と2日に行われます。映画の『リメンバー・ミー』でも話題になりました。"},{"id":{"d":632,"n":4,"t":3},"speaker":"B","text":"その『リメンバー・ミー』を見たので知っていたのです。骸骨祭りは具体的にはどのようなお祭りなのですか？"},{"id":{"d":632,"n":5,"t":3},"speaker":"A","text":"日本のお盆のように先祖を迎える行事ですが、音楽やダンスで先祖を迎えるので陽気なお祭りです。"},{"id":{"d":632,"n":6,"t":3},"speaker":"B","text":"映画にもそのようなシーンがありました。人々が骸骨の衣装を着て踊っていました。"},{"id":{"d":632,"n":7,"t":3},"speaker":"A","text":"骸骨の仮装をしたり、祭壇に骸骨のお菓子をお供えしたりします。"}];
INSERT INTO sentence [{"id":{"d":633,"n":1,"t":3},"speaker":"A","text":"今度、エジプトに行くそうですね。やはりピラミッドを見ることが目的ですか？"},{"id":{"d":633,"n":2,"t":3},"speaker":"B","text":"はい。でも、ピラミッドを見ること以外にも目的があります。"},{"id":{"d":633,"n":3,"t":3},"speaker":"A","text":"古代文明の宝庫といわれる国ですから、見どころも豊富でしょう。"},{"id":{"d":633,"n":4,"t":3},"speaker":"B","text":"博物館にも行きたいし、スフィンクスもナイル川も見たいです。"},{"id":{"d":633,"n":5,"t":3},"speaker":"A","text":"エジプトに行くなら、ぜひともラクダに乗ってください。"},{"id":{"d":633,"n":6,"t":3},"speaker":"B","text":"ラクダは日本でも乗れるのではありませんか？"},{"id":{"d":633,"n":7,"t":3},"speaker":"A","text":"はい。しかし、やはり雰囲気が違うと思います。ラクダには砂漠が似合います。"}];
INSERT INTO sentence [{"id":{"d":634,"n":1,"t":3},"speaker":"A","text":"博多どんたくについて、教えていただけますか？"},{"id":{"d":634,"n":2,"t":3},"speaker":"B","text":"博多出身ですから多少はお役に立てると思います。"},{"id":{"d":634,"n":3,"t":3},"speaker":"A","text":"私は「どんたく」の語源を知りたいのです。"},{"id":{"d":634,"n":4,"t":3},"speaker":"B","text":"「どんたく」は「休日」という意味のオランダ語が由来です。"},{"id":{"d":634,"n":5,"t":3},"speaker":"A","text":"そうでしたか。それでは、なぜ5月3日から開催するのですか？"},{"id":{"d":634,"n":6,"t":3},"speaker":"B","text":"日程は何度か変わっているのですが、新憲法発布後は憲法記念日に合わせたようです。"},{"id":{"d":634,"n":7,"t":3},"speaker":"A","text":"5月3日という日付そのものには、それほど意味はないのですか？"},{"id":{"d":634,"n":8,"t":3},"speaker":"B","text":"実際は2日からどんたく祭りは始まっています。日付そのものには特にはこだわっていないと思います。"}];
INSERT INTO sentence [{"id":{"d":635,"n":1,"t":3},"speaker":"A","text":"子どもの頃は、旅行することが嫌いでした。"},{"id":{"d":635,"n":2,"t":3},"speaker":"B","text":"意外ですね。今では、休日の度にあちらこちらへ旅行されているではありませんか？"},{"id":{"d":635,"n":3,"t":3},"speaker":"A","text":"はい。今では旅行が趣味の一つです。"},{"id":{"d":635,"n":4,"t":3},"speaker":"B","text":"子どもの頃の旅行は団体行動だったので、嫌いだったのではありませんか？"},{"id":{"d":635,"n":5,"t":3},"speaker":"A","text":"そうかもしれません。修学旅行や林間学校は、確かに旅行というより学校行事といえるかもしれませんね。"},{"id":{"d":635,"n":6,"t":3},"speaker":"B","text":"学校の旅行は学習の一環ですから、観光を楽しむための旅行とは、そもそも目的が違いますしね。"},{"id":{"d":635,"n":7,"t":3},"speaker":"A","text":"私にとっての旅行は自由で気ままなイメージです。"},{"id":{"d":635,"n":8,"t":3},"speaker":"B","text":"今でも団体旅行は苦手ですか？"},{"id":{"d":635,"n":9,"t":3},"speaker":"A","text":"いいえ、そうでもありません。大人になってからは、団体旅行でも魅力を感じます。"}];
INSERT INTO sentence [{"id":{"d":636,"n":1,"t":3},"speaker":"A","text":"この会員チケットを利用すると、何か利点がありますか？"},{"id":{"d":636,"n":2,"t":3},"speaker":"B","text":"期間中は表記された路線の列車を自由に利用できることが利点です。"},{"id":{"d":636,"n":3,"t":3},"speaker":"A","text":"新幹線も利用できますか？"},{"id":{"d":636,"n":4,"t":3},"speaker":"B","text":"はい。6回まで指定席券を使用できますし、新幹線にも対応しています。"},{"id":{"d":636,"n":5,"t":3},"speaker":"A","text":"この会員チケットには他にも何か良い点はありますか？"},{"id":{"d":636,"n":6,"t":3},"speaker":"B","text":"バスやレンタカーも、このチケットならお安くなります。"},{"id":{"d":636,"n":7,"t":3},"speaker":"A","text":"レンタカーも利用したいと思っていたので、私に合ったチケットです。"},{"id":{"d":636,"n":8,"t":3},"speaker":"B","text":"他にも特定の施設や店舗で特典を受けることができますよ。"},{"id":{"d":636,"n":9,"t":3},"speaker":"A","text":"それでは入会の手続きをします。"}];
INSERT INTO sentence [{"id":{"d":637,"n":1,"t":3},"speaker":"A","text":"新婚旅行は、どちらに行かれるのですか？"},{"id":{"d":637,"n":2,"t":3},"speaker":"B","text":"栃木県に行くと決めています。"},{"id":{"d":637,"n":3,"t":3},"speaker":"A","text":"外国に行くことは考えていませんか？"},{"id":{"d":637,"n":4,"t":3},"speaker":"B","text":"海外旅行では気が休まらないので、国内旅行の方がいいと思いました。"},{"id":{"d":637,"n":5,"t":3},"speaker":"A","text":"栃木県に決めているのはなぜですか？"},{"id":{"d":637,"n":6,"t":3},"speaker":"B","text":"私の会社の系列ホテルとゴルフ場があるからです。ゆっくり過ごしたいと思っています。"},{"id":{"d":637,"n":7,"t":3},"speaker":"A","text":"婚約者の方も国内旅行で納得されているのですか？"},{"id":{"d":637,"n":8,"t":3},"speaker":"B","text":"ええ、もちろんです。彼女とはゴルフ場で知り合ったのですが、どこかに行くよりゴルフがしたいようです。"},{"id":{"d":637,"n":9,"t":3},"speaker":"A","text":"仕事がお忙しそうですし、新婚旅行でゴルフざんまいも良いかもしれませんね。"},{"id":{"d":637,"n":10,"t":3},"speaker":"B","text":"はい。お互いに出張の多い仕事なので、新婚旅行くらいはゆっくりしたいのです。"}];
INSERT INTO sentence [{"id":{"d":638,"n":1,"t":3},"speaker":"A","text":"今度、ニューヨーク市に行くそうですね。治安は大丈夫ですか？"},{"id":{"d":638,"n":2,"t":3},"speaker":"B","text":"はい。銃の事件やヘイトクライムは少し心配ですが、やはりニューヨーク市は、一度は行ってみたい場所です。"},{"id":{"d":638,"n":3,"t":3},"speaker":"A","text":"ニューヨーク市のどこにそれほどの魅力があるのでしょうか？"},{"id":{"d":638,"n":4,"t":3},"speaker":"B","text":"私はジャズが好きなので、いつか本場のジャズを聴きたいと思っていたのです。"},{"id":{"d":638,"n":5,"t":3},"speaker":"A","text":"ジャズといえばニューオーリンズではありませんか？"},{"id":{"d":638,"n":6,"t":3},"speaker":"B","text":"初期のジャズならばニューオーリンズですが、メインストリームジャズならばニューヨーク市です。"},{"id":{"d":638,"n":7,"t":3},"speaker":"A","text":"具体的には、どのようなところに行かれるのですか？"},{"id":{"d":638,"n":8,"t":3},"speaker":"B","text":"ブルーノート、バードランド、ビレッジバンガードなどのジャズクラブに行きます。"},{"id":{"d":638,"n":9,"t":3},"speaker":"A","text":"ブルーノートは聞いたことのあるジャズクラブです。"},{"id":{"d":638,"n":10,"t":3},"speaker":"B","text":"日本にあるブルーノート東京はニューヨーク市にあるブルーノートの支店です。ブルーノートは、恐らく世界で一番有名なジャズクラブでしょう。"}];
INSERT INTO sentence [{"id":{"d":639,"n":1,"t":3},"speaker":"A","text":"このパスポートにはたくさんのスタンプが押されていますね。一体何カ国旅行しているのですか？"},{"id":{"d":639,"n":2,"t":3},"speaker":"B","text":"数えたことがないので分かりません。大学時代はバックパッカーで世界中を旅しました。"},{"id":{"d":639,"n":3,"t":3},"speaker":"A","text":"仕事でも海外出張が多いそうですね。"},{"id":{"d":639,"n":4,"t":3},"speaker":"B","text":"仕事で行く場合は、同じ国に行くことが多いですよ。"},{"id":{"d":639,"n":5,"t":3},"speaker":"A","text":"大学時代からのパスポートならば、そろそろ期限が切れるのではありませんか？"},{"id":{"d":639,"n":6,"t":3},"speaker":"B","text":"はい。表紙の色が赤いパスポートの期限は10年で、今年で期限切れとなります。"},{"id":{"d":639,"n":7,"t":3},"speaker":"A","text":"もう期限切れ間近ではありませんか？"},{"id":{"d":639,"n":8,"t":3},"speaker":"B","text":"はい。10年の期限ですが9年半を超えると入国できない国があるので、早めに更新手続きをしたいと考えています。"},{"id":{"d":639,"n":9,"t":3},"speaker":"A","text":"そのようなことから、今日は更新のためにパスポートを持っているわけですか？"},{"id":{"d":639,"n":10,"t":3},"speaker":"B","text":"はい、そうです。今日は仕事を早めに終えてパスポートの更新申請に行きます。"}];
INSERT INTO sentence [{"id":{"d":640,"n":1,"t":3},"speaker":"A","text":"ヨットで世界一周をする人は入国手続をどうしているのでしょう？"},{"id":{"d":640,"n":2,"t":3},"speaker":"B","text":"私もかつて同じ疑問を持っていました。以前、ヨットで世界一周をした人の映画で、入国手続きの方法を見たことがあります。"},{"id":{"d":640,"n":3,"t":3},"speaker":"A","text":"寄港してからパスポートを見せに行くのですか？"},{"id":{"d":640,"n":4,"t":3},"speaker":"B","text":"いいえ。映画では接岸する前に無線機で係官を呼んでいました。ヨットで係官と入国手続きをしてから上陸したはずです。"},{"id":{"d":640,"n":5,"t":3},"speaker":"A","text":"そのように係官は、すぐ来てくれるのですか？"},{"id":{"d":640,"n":6,"t":3},"speaker":"B","text":"日本の出国前に入国管理官と海上保安官の出国審査や船舶の整備確認を受けます。その時に寄港場所も伝えておくのだそうです。"},{"id":{"d":640,"n":7,"t":3},"speaker":"A","text":"到着前から入国管理局は情報を受けているのですね。接岸しないで他国を通過するときは何かするのですか？"},{"id":{"d":640,"n":8,"t":3},"speaker":"B","text":"調べてみて知ったことなのですが、上陸せずに海をただ通過するだけなら手続は不要なのだそうです。"},{"id":{"d":640,"n":9,"t":3},"speaker":"A","text":"それでも領海12海里の外にいなければならないと思いますが。"},{"id":{"d":640,"n":10,"t":3},"speaker":"B","text":"それが12海里以内でもその国の治安や安全を害しなければ通航は可能らしいのです。"},{"id":{"d":640,"n":11,"t":3},"speaker":"A","text":"それは初めて知りました。ヨットで世界一周するには、知っておかなければならないルールの一つかもしれません。"}];
INSERT INTO sentence [{"id":{"d":641,"n":1,"t":3},"speaker":"A","text":"今度金婚式の記念に旅行されるそうですね。どちらに行かれるのですか？"},{"id":{"d":641,"n":2,"t":3},"speaker":"B","text":"イタリア、フランス、スペイン地中海沿岸を旅行します。"},{"id":{"d":641,"n":3,"t":3},"speaker":"A","text":"何か目的はあるのですか？"},{"id":{"d":641,"n":4,"t":3},"speaker":"B","text":"妻は「ベネチアでゴンドラに乗りたい」と言っています。"},{"id":{"d":641,"n":5,"t":3},"speaker":"A","text":"それなら、日本から予約をしておくと良いですよ。"},{"id":{"d":641,"n":6,"t":3},"speaker":"B","text":"ゴンドラの予約は、現地で申し込んではいけませんか？"},{"id":{"d":641,"n":7,"t":3},"speaker":"A","text":"現地では、当日予約が取れないことがあります。"},{"id":{"d":641,"n":8,"t":3},"speaker":"B","text":"パックツアーなので、恐らく旅行会社が予約してくれていると思います。"},{"id":{"d":641,"n":9,"t":3},"speaker":"A","text":"それならきっと大丈夫だと思いますが、確認しておくことをお勧めします。"},{"id":{"d":641,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。事前に確認しておこうと思います。"}];
INSERT INTO sentence [{"id":{"d":642,"n":1,"t":3},"speaker":"A","text":"一人で登山をする上で、注意することはありますか？"},{"id":{"d":642,"n":2,"t":3},"speaker":"B","text":"日程を家族に伝えておくことです。詳しく紙に書いて、渡しておくといいでしょうね。"},{"id":{"d":642,"n":3,"t":3},"speaker":"A","text":"他に重要なことは何かありますか？"},{"id":{"d":642,"n":4,"t":3},"speaker":"B","text":"できる範囲で構いませんから、メールなどで家族に登山の現状を知らせておくと良いですよ。"},{"id":{"d":642,"n":5,"t":3},"speaker":"A","text":"一人登山では、それくらいした方がいいのですか？"},{"id":{"d":642,"n":6,"t":3},"speaker":"B","text":"はい。一人の登山だと、場合によっては精神が不安定になることもあるので、定期的な連絡は冷静さを保つ役に立ちます。"},{"id":{"d":642,"n":7,"t":3},"speaker":"A","text":"メールを頻繁に使うとなると、満充電しておくだけでは足りませんね。他にも予備電池が必要になりそうです。"},{"id":{"d":642,"n":8,"t":3},"speaker":"B","text":"寒いと電池の消耗は激しいので、必ず予備電池を持って行った方がいいですよ。"},{"id":{"d":642,"n":9,"t":3},"speaker":"A","text":"それは知りませんでした。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":643,"n":1,"t":3},"speaker":"A","text":"スペインの建築物というとサグラダファミリアを思い起こしますが、他にも有名な建築物はありますか？"},{"id":{"d":643,"n":2,"t":3},"speaker":"B","text":"はい。アルハンブラ宮殿は素晴らしいです。"},{"id":{"d":643,"n":3,"t":3},"speaker":"A","text":"クラシック曲の『アルハンブラの思い出』は知っています。アルハンブラ宮殿は、どこにあるのですか？"},{"id":{"d":643,"n":4,"t":3},"speaker":"B","text":"アルハンブラ宮殿はアンダルシア州のグラナダにあります。スペインの地中海側です。"},{"id":{"d":643,"n":5,"t":3},"speaker":"A","text":"建築物としての見どころはどこでしょうか？"},{"id":{"d":643,"n":6,"t":3},"speaker":"B","text":"くも巣状に張り巡らされたタイルの天井装飾、イスラム建築最高峰である「ナスル朝宮殿」など素晴らしいところばかりです。"},{"id":{"d":643,"n":7,"t":3},"speaker":"A","text":"スペインの国中にイスラムの建築物があるのですか？"},{"id":{"d":643,"n":8,"t":3},"speaker":"B","text":"グラナダはスペインの中にあるイスラム王朝最大の都市でした。"},{"id":{"d":643,"n":9,"t":3},"speaker":"A","text":"スペインにあるイスラム王朝の宮殿となると、さまざまな芸術作品の題材になるのも無理はありません。"},{"id":{"d":643,"n":10,"t":3},"speaker":"B","text":"アルハンブラ宮殿はどこを見ても素晴らしいです。機会があれば、ぜひ訪れてみてください。"}];
INSERT INTO sentence [{"id":{"d":644,"n":1,"t":3},"speaker":"A","text":"フランス観光というと、パリや地中海沿岸が人気ですが、他にも見るべきところがありますか？"},{"id":{"d":644,"n":2,"t":3},"speaker":"B","text":"私がお薦めするのはモン・サン＝ミシェルです。"},{"id":{"d":644,"n":3,"t":3},"speaker":"A","text":"モン・サン＝ミシェルは何が有名なのですか？"},{"id":{"d":644,"n":4,"t":3},"speaker":"B","text":"小島の中央にある修道院が有名です。修道院の周りを中世のさまざまな建築物が囲んでいます。"},{"id":{"d":644,"n":5,"t":3},"speaker":"A","text":"まるで要塞のようです。モン・サン＝ミシェルはどこにあるのですか？"},{"id":{"d":644,"n":6,"t":3},"speaker":"B","text":"英国に近いノルマンディー地方にあり、百年戦争の時は要塞の働きをしていました。"},{"id":{"d":644,"n":7,"t":3},"speaker":"A","text":"映画やアニメーションに出てきそうな島です。"},{"id":{"d":644,"n":8,"t":3},"speaker":"B","text":"ゲームやアニメーションによく出てくるような、島全体が街として描かれている様子は、大体モン・サン＝ミシェルをイメージしていると思います。"},{"id":{"d":644,"n":9,"t":3},"speaker":"A","text":"モン・サン＝ミシェルは随分歴史的価値があるのでしょうね。"},{"id":{"d":644,"n":10,"t":3},"speaker":"B","text":"はい。モン・サン＝ミシェルはカトリックの巡礼地であり世界遺産でもあり、その上ラムサール条約登録地でもあります。"}];
INSERT INTO sentence [{"id":{"d":645,"n":1,"t":3},"speaker":"A","text":"トルコ旅行をしていたそうですね？"},{"id":{"d":645,"n":2,"t":3},"speaker":"B","text":"はい。念願のカッパドキアに行ってきました。"},{"id":{"d":645,"n":3,"t":3},"speaker":"A","text":"カッパドキアというと地下都市を見てきたのですか？"},{"id":{"d":645,"n":4,"t":3},"speaker":"B","text":"よくご存じですね。カイマクルとデリンクユの地下都市を見てきました。"},{"id":{"d":645,"n":5,"t":3},"speaker":"A","text":"古代から残っている地下都市と聞きましたが、それが今でもあることが信じられません。"},{"id":{"d":645,"n":6,"t":3},"speaker":"B","text":"地下都市には何万人もの規模で住んでいたということですから驚きです。"},{"id":{"d":645,"n":7,"t":3},"speaker":"A","text":"そのような数の人間が、なぜ地下に住むようになったのでしょうか？"},{"id":{"d":645,"n":8,"t":3},"speaker":"B","text":"一般には、アラブ人の迫害から逃れるための、キリスト教徒の避難所だったからといわれています。"},{"id":{"d":645,"n":9,"t":3},"speaker":"A","text":"それだけの地下都市が作れるのなら、戦っても勝てるような気がします。"},{"id":{"d":645,"n":10,"t":3},"speaker":"B","text":"地下都市はどのように作られたかも、まだ分かっていません。本当に謎の多い都市です。"}];
INSERT INTO sentence [{"id":{"d":646,"n":1,"t":3},"speaker":"A","text":"カッパドキアに行ってきたそうですね。地下都市は見応えがありましたか？"},{"id":{"d":646,"n":2,"t":3},"speaker":"B","text":"はい。カッパドギアの地下都市は、信じられないほど深くまで住居空間が広がっていました。ただし、見どころは地下だけにとどまりません。"},{"id":{"d":646,"n":3,"t":3},"speaker":"A","text":"そうなのですか？"},{"id":{"d":646,"n":4,"t":3},"speaker":"B","text":"はい。地上では自然の力でできたとは思えない奇岩群、岩窟教会や石造りの民家など神秘的な光景を見ることができました。"},{"id":{"d":646,"n":5,"t":3},"speaker":"A","text":"それだけたくさんの光景を一度に見ることは難しいのではありませんか？"},{"id":{"d":646,"n":6,"t":3},"speaker":"B","text":"そこでお薦めなのは、カッパドキアの気球観光です。"},{"id":{"d":646,"n":7,"t":3},"speaker":"A","text":"気球で観光することは一般的なのですか？"},{"id":{"d":646,"n":8,"t":3},"speaker":"B","text":"はい。カッパドキアでは有名だそうです。実際にあの景観を一望するには、気球が最も適していると思います。"},{"id":{"d":646,"n":9,"t":3},"speaker":"A","text":"カッパドキアでは地下や地上を歩いても、空から見ても貴重な体験を得られますね。"},{"id":{"d":646,"n":10,"t":3},"speaker":"B","text":"本当に行ってきて良かったです。"}];
INSERT INTO sentence [{"id":{"d":647,"n":1,"t":3},"speaker":"A","text":"私は、埼玉にはあまり観光というイメージを持っていませんが、どこか良いところをご存知ですか？"},{"id":{"d":647,"n":2,"t":3},"speaker":"B","text":"ええ。イメージがないということは、これから楽しめる場所をたくさん発見できるということです。"},{"id":{"d":647,"n":3,"t":3},"speaker":"A","text":"埼玉にはどのような観光スポットがあるのですか？"},{"id":{"d":647,"n":4,"t":3},"speaker":"B","text":"例えば、吉見百穴というスポットがあります。ご存じですか？"},{"id":{"d":647,"n":5,"t":3},"speaker":"A","text":"それは知りません。たくさんの穴があるのですか？"},{"id":{"d":647,"n":6,"t":3},"speaker":"B","text":"吉見百穴は横穴古墳群の遺跡です。岩盤に200以上もの横穴があります。"},{"id":{"d":647,"n":7,"t":3},"speaker":"A","text":"横穴以外に見どころはあるのですか？"},{"id":{"d":647,"n":8,"t":3},"speaker":"B","text":"吉見百穴には、ヒカリゴケの自生している穴があります。この辺りでは見ることができない貴重なものです。"},{"id":{"d":647,"n":9,"t":3},"speaker":"A","text":"コケが光るのですか？それは、ぜひ見てみたいです。"},{"id":{"d":647,"n":10,"t":3},"speaker":"B","text":"ヒカリゴケは、自身が発光するのではなく、反射光によって淡く輝きます。とてもきれいですよ。ぜひ見に行ってください。"}];
INSERT INTO sentence [{"id":{"d":648,"n":1,"t":3},"speaker":"A","text":"クルーザーとはどのようなものですか？"},{"id":{"d":648,"n":2,"t":3},"speaker":"B","text":"居住できるレジャー用のヨットやボートのことです。"},{"id":{"d":648,"n":3,"t":3},"speaker":"A","text":"私はさらに大きな船舶をイメージしていました。"},{"id":{"d":648,"n":4,"t":3},"speaker":"B","text":"それは大型クルーザーとかクルーズ船といった方がいいかもしれません。"},{"id":{"d":648,"n":5,"t":3},"speaker":"A","text":"よく東京湾観光に使われている船は、クルーザーといえるのでしょうか？"},{"id":{"d":648,"n":6,"t":3},"speaker":"B","text":"クルーズの意味は巡航ですから、小さいヨットから大型船や屋形船までクルーザーと呼んで差し支えないと思います。"},{"id":{"d":648,"n":7,"t":3},"speaker":"A","text":"形は関係ないわけですね。フェリーはクルーズ船とはいいませんか？"},{"id":{"d":648,"n":8,"t":3},"speaker":"B","text":"フェリーは、海上を定期的に運行する、人や車両や荷物の運搬船ですね。"},{"id":{"d":648,"n":9,"t":3},"speaker":"A","text":"なるほど、居住空間を楽しむ船とは違うのですね。"},{"id":{"d":648,"n":10,"t":3},"speaker":"B","text":"最近は居住空間を楽しむフェリーもあるようですが、クルーズ船はまさに船での生活を楽しむための船です。"}];
INSERT INTO sentence [{"id":{"d":649,"n":1,"t":3},"speaker":"A","text":"山梨県の旅館に泊まった時に、面白いことを聞きました。"},{"id":{"d":649,"n":2,"t":3},"speaker":"B","text":"それは何でしょう。私は山梨県出身です。"},{"id":{"d":649,"n":3,"t":3},"speaker":"A","text":"旅館の夕食でお刺身が出てきたので、「内陸県でお刺身ですか？」と言ってしまったのです。"},{"id":{"d":649,"n":4,"t":3},"speaker":"B","text":"それではその後、旅館の人はなぜ山梨でお刺身が出るのかを説明されたことでしょうね。"},{"id":{"d":649,"n":5,"t":3},"speaker":"A","text":"その通りです。山梨県はおすし屋さんの数が全国一で、県民はマグロが大好きなのだそうです。"},{"id":{"d":649,"n":6,"t":3},"speaker":"B","text":"私の実家でもマグロのお刺身はよく夕食に出てきます。"},{"id":{"d":649,"n":7,"t":3},"speaker":"A","text":"生魚を運ぶためのルートもあって、昔でも駿河湾から一晩で甲府まで運ぶことができたそうです。"},{"id":{"d":649,"n":8,"t":3},"speaker":"B","text":"駿河湾のアサリやアジの干物もよく食べます。"},{"id":{"d":649,"n":9,"t":3},"speaker":"A","text":"旅館の朝食が、アサリのおみそ汁とアジの干物でした。"},{"id":{"d":649,"n":10,"t":3},"speaker":"B","text":"予期せぬ山梨の料理を堪能したことになりますね。"}];
INSERT INTO sentence [{"id":{"d":650,"n":1,"t":3},"speaker":"A","text":"東京メトロ大江戸線ができて、地下深くにある駅が増えました。"},{"id":{"d":650,"n":2,"t":3},"speaker":"B","text":"そうなのですか？私は国会議事堂前駅もかなり深いところにあると思います。"},{"id":{"d":650,"n":3,"t":3},"speaker":"A","text":"東京の地下駅の地上からの深さをランキングにしたものがあるのですが、それによると、国会議事堂前駅は第3位です。ベスト10に大江戸線の駅は5駅がランクインしています。"},{"id":{"d":650,"n":4,"t":3},"speaker":"B","text":"ベスト10のうちの半分ですか。それはかなり多いですね。"},{"id":{"d":650,"n":5,"t":3},"speaker":"A","text":"第1位は六本木駅です。ホームから地上まで移動するのに5分弱かかるようです。"},{"id":{"d":650,"n":6,"t":3},"speaker":"B","text":"5分もあれば、駅間の距離が短い路線なら2駅くらい進んでしまいますね。"},{"id":{"d":650,"n":7,"t":3},"speaker":"A","text":"はい。利用する路線を選ぶときは、大江戸線の場合は地上到達時間も考慮する必要があります。"},{"id":{"d":650,"n":8,"t":3},"speaker":"B","text":"私は大江戸線の深い駅ベスト5を撮影し、写真をInstagramに上げたくなりました。"},{"id":{"d":650,"n":9,"t":3},"speaker":"A","text":"面白いかもしれませんね。エレベーターの長さや数、地上までの移動時間なども載せると良いと思います。"}];
INSERT INTO sentence [{"id":{"d":651,"n":1,"t":3},"speaker":"A","text":"この写真を見てください。"},{"id":{"d":651,"n":2,"t":3},"speaker":"B","text":"どこかのショッピングモールの写真に見えます。どこが特別なのでしょうか？"},{"id":{"d":651,"n":3,"t":3},"speaker":"A","text":"ここを見てください。これは世界一短いエスカレーターです。"},{"id":{"d":651,"n":4,"t":3},"speaker":"B","text":"確かに短いですね。5段くらいしかありません。このエスカレーターはどこにあるのですか？"},{"id":{"d":651,"n":5,"t":3},"speaker":"A","text":"このエスカレーターは、川崎駅から徒歩4分のショッピングビルにあります。"},{"id":{"d":651,"n":6,"t":3},"speaker":"B","text":"なぜ、このようなエスカレーターがあるのですか？"},{"id":{"d":651,"n":7,"t":3},"speaker":"A","text":"エスカレーターの工事中に太いはりが途中にあることが分かり、短いエスカレーターと階段を組み合わせることになったそうです。"},{"id":{"d":651,"n":8,"t":3},"speaker":"B","text":"この短いエスカレーターを使っている人はいるのですか？"},{"id":{"d":651,"n":9,"t":3},"speaker":"A","text":"はい。ギネス世界記録にも認定されているからか、私が見た時は、このエスカレーターの利用者は多かったですよ。"}];
INSERT INTO sentence [{"id":{"d":652,"n":1,"t":3},"speaker":"A","text":"米国で行ってみたいところはありますか？"},{"id":{"d":652,"n":2,"t":3},"speaker":"B","text":"はい。ウィンチェスター・ミステリー・ハウスです。"},{"id":{"d":652,"n":3,"t":3},"speaker":"A","text":"ウィンチェスター・ミステリー・ハウスとはどのようなところでしょう。銃のウィンチェスターと関係があるのでしょうか？"},{"id":{"d":652,"n":4,"t":3},"speaker":"B","text":"その通りです。銃ビジネスで巨万の富を築いたウィリアム・ウィンチェスターの妻サラが、建築し続けた家のことです。"},{"id":{"d":652,"n":5,"t":3},"speaker":"A","text":"ミステリー・ハウスと言っていましたが、いわゆる幽霊屋敷ですか？"},{"id":{"d":652,"n":6,"t":3},"speaker":"B","text":"はい。サラは霊媒師の助言に従って、生きている間ずっと家を建築し続けたのです。"},{"id":{"d":652,"n":7,"t":3},"speaker":"A","text":"霊媒師の助言はどのような内容だったのですか？"},{"id":{"d":652,"n":8,"t":3},"speaker":"B","text":"「銃で亡くなった人たちのために家を建て続けなければならない。もしやめればあなたは死んでしまうだろう」と言ったそうです。"},{"id":{"d":652,"n":9,"t":3},"speaker":"A","text":"サラさんはその霊媒師の言う通り、1軒の家を建て続けたのですか？"},{"id":{"d":652,"n":10,"t":3},"speaker":"B","text":"そのようです。その巨大な家は霊から逃れるための隠し部屋や秘密通路がたくさんあるそうです。"},{"id":{"d":652,"n":11,"t":3},"speaker":"A","text":"それでミステリー・ハウスと呼ばれているのですね。"}];
INSERT INTO sentence [{"id":{"d":653,"n":1,"t":3},"speaker":"A","text":"マイケル・ジャクソンの家は今どうなっているのでしょうか？"},{"id":{"d":653,"n":2,"t":3},"speaker":"B","text":"ネバーランドのことですか？ネバーランドは遊園地だったはずです。"},{"id":{"d":653,"n":3,"t":3},"speaker":"A","text":"マイケル・ジャクソンの家は、ネバーランドと呼ばれているのですか？"},{"id":{"d":653,"n":4,"t":3},"speaker":"B","text":"はい。物語『ピーター・パン』に出てくるネバーランドから命名したマイケル・ジャクソンの自宅兼遊園地です。"},{"id":{"d":653,"n":5,"t":3},"speaker":"A","text":"ネバーランドは遊園地とのことですが、観覧車、メリーゴーラウンド、海賊船、ジェットコースターなどのアトラクションもあるのですか？"},{"id":{"d":653,"n":6,"t":3},"speaker":"B","text":"存在していたはずですよ。ネバーランドには動物園も併設されていたと思います。"},{"id":{"d":653,"n":7,"t":3},"speaker":"A","text":"楽しそうです。営業しているなら、ネバーランドにぜひとも行ってみたいです。"},{"id":{"d":653,"n":8,"t":3},"speaker":"B","text":"マイケルの死後しばらくは家族に相続されて運営は続きましたが、確か今は売り出されていたと思います。"},{"id":{"d":653,"n":9,"t":3},"speaker":"A","text":"それは残念です。最近マイケルの曲を聞いて彼のファンになったので、米国に行くなら見てみようと思ったのですが。"}];
INSERT INTO sentence [{"id":{"d":654,"n":1,"t":3},"speaker":"A","text":"ドイツ観光で行くべきところはありますか？"},{"id":{"d":654,"n":2,"t":3},"speaker":"B","text":"クラシック音楽が好きならノイシュバンシュタイン城はいかがですか？"},{"id":{"d":654,"n":3,"t":3},"speaker":"A","text":"クラシック音楽は好きです。バイロイトやベルリン国立歌劇場なら分かりますが、お城がお薦めなのですか？"},{"id":{"d":654,"n":4,"t":3},"speaker":"B","text":"はい。ノイシュバンシュタイン城はただの城ではありませんよ。19世紀後半にルートヴィッヒ2世によって建てられた城です。"},{"id":{"d":654,"n":5,"t":3},"speaker":"A","text":"ルートヴィッヒ2世はワーグナーのパトロンでしたね。"},{"id":{"d":654,"n":6,"t":3},"speaker":"B","text":"そうです。ワーグナーのオペラに魅入られたルートヴィッヒ2世はその音楽観をお城に注ぎ込んだのです。"},{"id":{"d":654,"n":7,"t":3},"speaker":"A","text":"思い出しました。ノイシュバンシュタイン城はクラシック音楽史ではとても有名な城でした。"},{"id":{"d":654,"n":8,"t":3},"speaker":"B","text":"ノイシュバンシュタイン城はおとぎ話に出てくるような外観で、白鳥城とも呼ばれています。"},{"id":{"d":654,"n":9,"t":3},"speaker":"A","text":"その話を聞いたら、ノイシュバンシュタイン城に行かざるを得ません。ぜひ一度、この目で見てみたいものです。"},{"id":{"d":654,"n":10,"t":3},"speaker":"B","text":"冬のノイシュバンシュタイン城は特にきれいです。きっと満足されるでしょう。"}];
INSERT INTO sentence [{"id":{"d":655,"n":1,"t":3},"speaker":"A","text":"パトカーに乗ったことはありますか？"},{"id":{"d":655,"n":2,"t":3},"speaker":"B","text":"はい、あります。子どもの頃、迷子になってしまったことがありました。パトカーには、親のところに連れていってもらう時に乗りました。"},{"id":{"d":655,"n":3,"t":3},"speaker":"A","text":"なぜ迷子になったのですか？"},{"id":{"d":655,"n":4,"t":3},"speaker":"B","text":"保育園の保護者同伴遠足で秋田市内を観光したのです。しかし、団体行動が苦手で、勝手に一人で歩いているうちに迷子になってしまいました。"},{"id":{"d":655,"n":5,"t":3},"speaker":"A","text":"それは心配でしたね。誰が警察を呼んだのですか？"},{"id":{"d":655,"n":6,"t":3},"speaker":"B","text":"私です。自分から交番に行って「迷子になりました」と言ったのです。"},{"id":{"d":655,"n":7,"t":3},"speaker":"A","text":"そこは、しっかりした子だったのですね。"},{"id":{"d":655,"n":8,"t":3},"speaker":"B","text":"そうですね。名札から通っていた保育園が特定されて、パトカーで連れて行ってもらったというわけです。"}];
INSERT INTO sentence [{"id":{"d":656,"n":1,"t":3},"speaker":"A","text":"浅草というと何を連想しますか？"},{"id":{"d":656,"n":2,"t":3},"speaker":"B","text":"浅草花やしきと浅草サンバカーニバルです。"},{"id":{"d":656,"n":3,"t":3},"speaker":"A","text":"私も浅草花やしきには何度も行きました。気軽に行ける遊園地ですよね。"},{"id":{"d":656,"n":4,"t":3},"speaker":"B","text":"日本最古のローラーコースターで有名です。私はよくデートの際に利用しました。"},{"id":{"d":656,"n":5,"t":3},"speaker":"A","text":"浅草サンバカーニバルは、それほど古くからは行っていませんよね？"},{"id":{"d":656,"n":6,"t":3},"speaker":"B","text":"それでも、初回開催から30年以上はたっていると思います。浅草サンバカーニバルは、パレードのコンテストなので、かなりにぎやかになりますね。"},{"id":{"d":656,"n":7,"t":3},"speaker":"A","text":"そうですね。テレビのニュース番組でも浅草のサンバカーニバルは毎年取り上げられています。それを見ていると、本場ブラジルのリオのカーニバルに行きたくなったりはしませんか？"},{"id":{"d":656,"n":8,"t":3},"speaker":"B","text":"いいえ。リオのカーニバルは規模が大きすぎるように思います。"},{"id":{"d":656,"n":9,"t":3},"speaker":"A","text":"リオのカーニバルでは逮捕者や死者が出ることもあるそうですから、恐らく大変なイベントでしょうね。"},{"id":{"d":656,"n":10,"t":3},"speaker":"B","text":"私にはリオより浅草が似合っていると思います。"}];
INSERT INTO sentence [{"id":{"d":657,"n":1,"t":3},"speaker":"A","text":"今度、両親を連れて横浜観光に行きたいのですが、どこかお薦めの場所はありますか？"},{"id":{"d":657,"n":2,"t":3},"speaker":"B","text":"定番は中華街でしょうね。その後、山下公園や赤レンガ倉庫に行くのもいいでしょう。"},{"id":{"d":657,"n":3,"t":3},"speaker":"A","text":"市内のホテルに泊まって、翌日も市内を回りたいのですが、他にも何かありますか？"},{"id":{"d":657,"n":4,"t":3},"speaker":"B","text":"年配の方でしたら、三渓園やイセザキ・モールが良いかもしれません。"},{"id":{"d":657,"n":5,"t":3},"speaker":"A","text":"興味深いです。横浜からは少し離れるのですが横浜・八景島シーパラダイスにも行ってみたいと思っているのです。"},{"id":{"d":657,"n":6,"t":3},"speaker":"B","text":"日本最大級の水族館ですから、ご両親が魚などに興味があるようでしたら、行ってみると良いですよ。"},{"id":{"d":657,"n":7,"t":3},"speaker":"A","text":"2日間で横浜市内と横浜・八景島シーパラダイス、全てを見ることは難しいでしょうか？"},{"id":{"d":657,"n":8,"t":3},"speaker":"B","text":"崎陽軒本店や新横浜ラーメン博物館のような食事処を多く入れると、2日間では難しいでしょうね。"},{"id":{"d":657,"n":9,"t":3},"speaker":"A","text":"なるほど、そうですか。両親の希望を聞いてみようと思います。また相談に乗ってください。"}];
INSERT INTO sentence [{"id":{"d":658,"n":1,"t":3},"speaker":"A","text":"今度、ポルトガルに住んでいる叔母に会いに行きます。"},{"id":{"d":658,"n":2,"t":3},"speaker":"B","text":"それは叔母さんも喜ぶことでしょう。"},{"id":{"d":658,"n":3,"t":3},"speaker":"A","text":"ついでにポルトガル観光をしたいのですが、どこに行くと良いのか分かりません。"},{"id":{"d":658,"n":4,"t":3},"speaker":"B","text":"叔母さんはポルトガルのどちらに住んでいるのですか？"},{"id":{"d":658,"n":5,"t":3},"speaker":"A","text":"リスボン郊外です。叔母からは「リスボン市内を案内する」と言われています。"},{"id":{"d":658,"n":6,"t":3},"speaker":"B","text":"リスボンは全体が観光地といえますから、住んでいる叔母さんにお任せすることが良さそうですね。"},{"id":{"d":658,"n":7,"t":3},"speaker":"A","text":"1週間ほど滞在するのですが、もし他の見どころもご存知でしたら教えていただけますか？"},{"id":{"d":658,"n":8,"t":3},"speaker":"B","text":"時間があるようでしたら、ポルトにも滞在するといいでしょう。"},{"id":{"d":658,"n":9,"t":3},"speaker":"A","text":"ポルトもポルトガルの大都市ですね。"},{"id":{"d":658,"n":10,"t":3},"speaker":"B","text":"はい。いろいろと見どころはありますが、ぜひレロ書店には足を運んでください。とても本屋とは思えないような空間が広がっていますよ。"}];
INSERT INTO sentence [{"id":{"d":659,"n":1,"t":3},"speaker":"A","text":"出張の時に何か楽しみにしていることはありますか？"},{"id":{"d":659,"n":2,"t":3},"speaker":"B","text":"はい。食べログで調べた飲食店に行くことです。"},{"id":{"d":659,"n":3,"t":3},"speaker":"A","text":"知らない土地で仕事をするのですから、食事くらいは楽しみたいですね。"},{"id":{"d":659,"n":4,"t":3},"speaker":"B","text":"事前にその土地の情報が全くないときに、食べログは役に立ちます。"},{"id":{"d":659,"n":5,"t":3},"speaker":"A","text":"自分の評価と食べログの評価が違っていることはありませんか？"},{"id":{"d":659,"n":6,"t":3},"speaker":"B","text":"もちろん評価が違っていることはありますが、それもまた旅の醍醐味です。"},{"id":{"d":659,"n":7,"t":3},"speaker":"A","text":"自分の食べた料理が、食べログの評価ほど高くなかったら、残念に思いませんか？"},{"id":{"d":659,"n":8,"t":3},"speaker":"B","text":"思いません。お店の紹介があるだけでありがたいものなので、特に評価にはこだわっていないのだと思います。"},{"id":{"d":659,"n":9,"t":3},"speaker":"A","text":"そうなのですね。行った飲食店の記録は残しているのですか？"},{"id":{"d":659,"n":10,"t":3},"speaker":"B","text":"Facebookに写真と感想を載せています。もう随分たまっていますから今度整理しなければなりません。"}];
INSERT INTO sentence [{"id":{"d":660,"n":1,"t":3},"speaker":"A","text":"ロンドン旅行をしたそうですね。ベーカー街には行ってきましたか？"},{"id":{"d":660,"n":2,"t":3},"speaker":"B","text":"もちろんです。シャーロキシャンなら当然です。シャーロック・ホームズ博物館で半日ほど過ごしてきました。"},{"id":{"d":660,"n":3,"t":3},"speaker":"A","text":"シャーロック・ホームズは架空の人物なのに博物館があるのですね。"},{"id":{"d":660,"n":4,"t":3},"speaker":"B","text":"架空の人物がテーマになっている博物館は珍しくないと思います。例えば、日本にも、全国各地にアンパンマンこどもミュージアムがあります。"},{"id":{"d":660,"n":5,"t":3},"speaker":"A","text":"確かにそうですね。シャーロック・ホームズ関連の場所を訪ねるためにロンドンに行ってきたのですか？"},{"id":{"d":660,"n":6,"t":3},"speaker":"B","text":"いいえ。もう一つ目的がありました。マダム・タッソー館を訪問することです。"},{"id":{"d":660,"n":7,"t":3},"speaker":"A","text":"有名なろう人形館ですね。私は東京タワーのろう人形館でマダム・タッソー館を知りました。今でも新しい人形を展示しているらしいですね？"},{"id":{"d":660,"n":8,"t":3},"speaker":"B","text":"はい。マダム・タッソー館で人形を作ってもらえると、大スターの仲間入りとなるそうです。"},{"id":{"d":660,"n":9,"t":3},"speaker":"A","text":"マダム・タッソー館に最近のろう人形はありましたか？"},{"id":{"d":660,"n":10,"t":3},"speaker":"B","text":"新しいかどうかは分かりませんが、ブラッド・ピットとアンジェリーナ・ジョリーの人形がありました。"}];
INSERT INTO sentence [{"id":{"d":661,"n":1,"t":3},"speaker":"A","text":"梅雨の時期になると、旅行をする気がうせてしまいます。"},{"id":{"d":661,"n":2,"t":3},"speaker":"B","text":"京都の宇治市では6月に県祭りと書く「あがたまつり」が開催されます。梅雨の時期ですが、このお祭りには行ってみる価値がありますよ。"},{"id":{"d":661,"n":3,"t":3},"speaker":"A","text":"県祭りとはどのような祭りですか？"},{"id":{"d":661,"n":4,"t":3},"speaker":"B","text":"県神社と書いて「あがたじんじゃ」と読む神社の祭りで、深夜の暗闇の中で儀式を行います。暗闇の奇祭と呼ばれているそうです。"},{"id":{"d":661,"n":5,"t":3},"speaker":"A","text":"何か独特の雰囲気がありそうなお祭りですね。京都なら他にも見どころはありそうですね。"},{"id":{"d":661,"n":6,"t":3},"speaker":"B","text":"はい。三室戸寺は鎌倉の明月院とともにアジサイで有名です。竜安寺や天龍寺の庭を見るのも良いでしょう。"},{"id":{"d":661,"n":7,"t":3},"speaker":"A","text":"京都の寺の庭は部屋の中から見ることができるところも多いですよね。それなら雨にぬれることもありません。"},{"id":{"d":661,"n":8,"t":3},"speaker":"B","text":"庭は雨が降っている方が趣深いです。良かったら梅雨時の京都に行ってみませんか？"},{"id":{"d":661,"n":9,"t":3},"speaker":"A","text":"奇祭には確かに心引かれます。行ってみることとしましょう。"}];
INSERT INTO sentence [{"id":{"d":662,"n":1,"t":3},"speaker":"A","text":"寒い日が続くと、どこか暖かい南国に行きたくなります。"},{"id":{"d":662,"n":2,"t":3},"speaker":"B","text":"南国といえばバンコクはどうですか？私は去年バンコクに行ってきたのですが、大変満喫できました。"},{"id":{"d":662,"n":3,"t":3},"speaker":"A","text":"バンコクというと、寺院や、人で込み合っている市場を連想します。"},{"id":{"d":662,"n":4,"t":3},"speaker":"B","text":"確かに寺や市場はバンコクの有名な観光スポットですが、近年は高層ビルやスパやカフェも人気です。"},{"id":{"d":662,"n":5,"t":3},"speaker":"A","text":"日本の高層ビルとは何か違いがありますか？"},{"id":{"d":662,"n":6,"t":3},"speaker":"B","text":"マハナコーン・スカイウォークの78階はガラス床で下を見ることができます。反対にルーフトップなスカイバーもあります。"},{"id":{"d":662,"n":7,"t":3},"speaker":"A","text":"高層階の床がガラス張りなんて、私は下を見て平気でいられる自信がありません。バンコクは近代化した都市になっているのですか？"},{"id":{"d":662,"n":8,"t":3},"speaker":"B","text":"はい。観光客が楽しめるお店も多いですよ。新しさも古さもあり魅力的な都市でした。"},{"id":{"d":662,"n":9,"t":3},"speaker":"A","text":"寒さしのぎに、ぜひバンコクへ行ってみたくなりました。"}];
INSERT INTO sentence [{"id":{"d":663,"n":1,"t":3},"speaker":"A","text":"藤沢駅で少し気になったことがあります。"},{"id":{"d":663,"n":2,"t":3},"speaker":"B","text":"どのようなことでしょうか？"},{"id":{"d":663,"n":3,"t":3},"speaker":"A","text":"江ノ島電鉄の藤沢駅が小田急電鉄のショッピングセンターの2階にあるのです。"},{"id":{"d":663,"n":4,"t":3},"speaker":"B","text":"そうですね。それのどのようなところが気になったのですか？"},{"id":{"d":663,"n":5,"t":3},"speaker":"A","text":"小田急電鉄の藤沢駅はJRの藤沢駅と隣接しています。私は、小田急電鉄の建物には江ノ島電鉄ではなくて、小田急線の駅があるべきだと思うのですが。"},{"id":{"d":663,"n":6,"t":3},"speaker":"B","text":"私の記憶に間違いがなければ、小田急電鉄の建物は以前、江ノ島電鉄のビルだったはずです。その後、小田急電鉄の建物になったのです。"},{"id":{"d":663,"n":7,"t":3},"speaker":"A","text":"建設された当初からビルの2階に江ノ島電鉄の藤沢駅が存在したのですか？"},{"id":{"d":663,"n":8,"t":3},"speaker":"B","text":"いいえ。最初は地上に存在した江ノ島電鉄藤沢駅は、江ノ島電鉄のビルが建設されたとき、その2階に移されたと思います。"},{"id":{"d":663,"n":9,"t":3},"speaker":"A","text":"なるほど。そのような経緯があったのですね。民家の軒下を走るレトロな電車がビルの2階から出発するので、私は何か不思議な印象を覚えたのです。"}];
INSERT INTO sentence [{"id":{"d":664,"n":1,"t":3},"speaker":"A","text":"娘がオークランドの大学に留学しているので、近々夫婦でニュージーランドに行こうと思っています。"},{"id":{"d":664,"n":2,"t":3},"speaker":"B","text":"ニュージーランドはすてきなところです。ぜひとも楽しんできてください。"},{"id":{"d":664,"n":3,"t":3},"speaker":"A","text":"ところが、私たちはニュージーランドのことを全く知りません。"},{"id":{"d":664,"n":4,"t":3},"speaker":"B","text":"ニュージーランドは自然の豊かな国です。滞在期間と体力に余裕があれば、いくらでも自然が満喫できます。"},{"id":{"d":664,"n":5,"t":3},"speaker":"A","text":"北と南の島のどちらに見どころがたくさんありますか？お薦めの観光スポットも教えていただけますか？"},{"id":{"d":664,"n":6,"t":3},"speaker":"B","text":"どちらにも見どころがありますよ。『ロード・オブ・ザ・リング』や『ホビット』に興味があれば、その映画の撮影地が、今は観光地となっています。"},{"id":{"d":664,"n":7,"t":3},"speaker":"A","text":"観光地となっている場所は『ロード・オブ・ザ・リング』のどのシーンに出てくるのですか？"},{"id":{"d":664,"n":8,"t":3},"speaker":"B","text":"ホビット村のシーンで使われた風景を撮影当時のまま残しています。ここ以外にも、美しい星空が見える砂浜やフィヨルドランド国立公園の観賞、土ボタルやキウイに出合うのも良いでしょう。"},{"id":{"d":664,"n":9,"t":3},"speaker":"A","text":"たくさんの観光スポットがあるのですね。すぐには、観光する場所を決められそうにありません。"},{"id":{"d":664,"n":10,"t":3},"speaker":"B","text":"期間に余裕があるのでしたら、娘さんと相談して訪ねる場所を決めてはいかがですか？"},{"id":{"d":664,"n":11,"t":3},"speaker":"A","text":"はい、そうしたいと思います。たくさん教えてくれてありがとうございました。"},{"id":{"d":664,"n":12,"t":3},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":665,"n":1,"t":3},"speaker":"A","text":"南国気分を味わえる旅行がしたいのですが、どこか良いところはありますか？"},{"id":{"d":665,"n":2,"t":3},"speaker":"B","text":"私なら、モルディブをお薦めします。海はどこよりもきれいだと思います。"},{"id":{"d":665,"n":3,"t":3},"speaker":"A","text":"私は趣味でスキューバダイビングをしているので、モルディブは行ってみたい場所の一つです。"},{"id":{"d":665,"n":4,"t":3},"speaker":"B","text":"宿泊施設やレストランも充実しています。水上ヴィラからはすぐにでも海に入れます。"},{"id":{"d":665,"n":5,"t":3},"speaker":"A","text":"日本からは遠いのでしょうが、モルディブへの直行便はありますか？"},{"id":{"d":665,"n":6,"t":3},"speaker":"B","text":"往路だけですが、成田国際空港から直行便が出ています。飛行時間は約7時間です。"},{"id":{"d":665,"n":7,"t":3},"speaker":"A","text":"アクセスも悪くないのですね。モルディブといえば、温暖化の影響で2030年には多くの島が海面に沈むといわれていたように思います。"},{"id":{"d":665,"n":8,"t":3},"speaker":"B","text":"そのような気がかりな話もありますね。今でしたら素晴らしいモルディブの海を楽しめると思います。"}];
INSERT INTO sentence [{"id":{"d":666,"n":1,"t":3},"speaker":"A","text":"ニューヨーク市に行ってきたそうですね。出張だったのですか？"},{"id":{"d":666,"n":2,"t":3},"speaker":"B","text":"いいえ、私的な旅行です。映画の『ジョーカー』をご存知ですか？そのロケ地を巡ってきました。"},{"id":{"d":666,"n":3,"t":3},"speaker":"A","text":"ホアキン・フェニックスが出演した映画ですね。その映画は私も見ました。"},{"id":{"d":666,"n":4,"t":3},"speaker":"B","text":"映画『バットマン』に出てくるゴッサム・シティのモデルがニューヨーク市です。"},{"id":{"d":666,"n":5,"t":3},"speaker":"A","text":"ジョーカーが踊りながら降りてくる階段は見られるのですか？"},{"id":{"d":666,"n":6,"t":3},"speaker":"B","text":"はい。ブロンクスにある「ステップ・ストリート」ですね。『ジョーカー』の階段のシーンは有名ですよね。ジョーカーの母がいたアーカム・アサイラムやジョーカーが争った地下鉄も見てきました。"},{"id":{"d":666,"n":7,"t":3},"speaker":"A","text":"ジョーカーと母親が住んでいたアパートは本当にあるのですか？"},{"id":{"d":666,"n":8,"t":3},"speaker":"B","text":"ありますよ。そのアパートはハイブリッジのアンダーソン街にあります。実際に人が住んでいるアパートです。"},{"id":{"d":666,"n":9,"t":3},"speaker":"A","text":"そのアパートが本当にあるのなら、観光客が押し寄せてしまったら地元の住民には迷惑なのではありませんか？"},{"id":{"d":666,"n":10,"t":3},"speaker":"B","text":"そうなのです。ですから、ツアーの前に住民の迷惑にならない行動をするよう、十分に注意を受けました。"}];
INSERT INTO sentence [{"id":{"d":667,"n":1,"t":3},"speaker":"A","text":"この間、軍艦島に行ってきました。"},{"id":{"d":667,"n":2,"t":3},"speaker":"B","text":"軍艦島は観光できるのですか？どちらにあるのですか？"},{"id":{"d":667,"n":3,"t":3},"speaker":"A","text":"長崎県にあります。軍艦島は部分的に観光を楽しむことができます。"},{"id":{"d":667,"n":4,"t":3},"speaker":"B","text":"軍艦島は石炭採掘のために小さな島に人工的に作られた町ですよね。"},{"id":{"d":667,"n":5,"t":3},"speaker":"A","text":"そうです。1974年に軍艦島が閉山となって以降は無人で、最近まで観光もできませんでした。"},{"id":{"d":667,"n":6,"t":3},"speaker":"B","text":"今は自由に見られるのですか？"},{"id":{"d":667,"n":7,"t":3},"speaker":"A","text":"いいえ。危険な場所がありますし、島全体を自由に観光できるわけではありません。観光をするには、船会社が運航しているツアーに参加する必要があります。"},{"id":{"d":667,"n":8,"t":3},"speaker":"B","text":"ツアーコースは1種類しかないのですか？"},{"id":{"d":667,"n":9,"t":3},"speaker":"A","text":"何種類かありますよ。ツアーを企画している船会社はいくつかあるので、比べてみても良いと思います。"}];
INSERT INTO sentence [{"id":{"d":668,"n":1,"t":3},"speaker":"A","text":"この間の連休中は、どこかに出掛けましたか？"},{"id":{"d":668,"n":2,"t":3},"speaker":"B","text":"2泊3日で鬼怒川に行ってきました。"},{"id":{"d":668,"n":3,"t":3},"speaker":"A","text":"それは良かったですね。温泉が好きなのですか？"},{"id":{"d":668,"n":4,"t":3},"speaker":"B","text":"はい。もちろん温泉も楽しんできましたが、鬼怒川には他にも堪能できる場所がありました。"},{"id":{"d":668,"n":5,"t":3},"speaker":"A","text":"鬼怒川はライン下りが有名でしたね。"},{"id":{"d":668,"n":6,"t":3},"speaker":"B","text":"はい。川を下るなら自分でオールを使うラフティングが体験できます。ヘルメットを着けて滝つぼに飛び込むキャニオニングもあります。"},{"id":{"d":668,"n":7,"t":3},"speaker":"A","text":"私はアクティブな活動よりも、変わった食べ物に興味があります。"},{"id":{"d":668,"n":8,"t":3},"speaker":"B","text":"それならばSLポップコーンをお薦めします。瞬間冷凍したポップコーンで食べると鼻から蒸気が出てきます。"},{"id":{"d":668,"n":9,"t":3},"speaker":"A","text":"鼻から蒸気が出るポップコーンならば、写真に収めたくなりますね。"},{"id":{"d":668,"n":10,"t":3},"speaker":"B","text":"鬼怒川近くの江戸ワンダーランド日光江戸村も、人気がある観光地の一つです。"}];
INSERT INTO sentence [{"id":{"d":669,"n":1,"t":3},"speaker":"A","text":"貯金箱にユーロ硬貨が入っていました。あなたが入れたのですか？"},{"id":{"d":669,"n":2,"t":3},"speaker":"B","text":"はい、私が入れました。これまでは財布に入れて持ち歩いていたのですが、少し邪魔になると思いましたので。"},{"id":{"d":669,"n":3,"t":3},"speaker":"A","text":"日本の硬貨とユーロ硬貨を混ぜないでください。このまま銀行に持って行ってしまうところでした。"},{"id":{"d":669,"n":4,"t":3},"speaker":"B","text":"すみません、気を付けます。ユーロ硬貨は使わないで取っておきたかったので、気付いてくれて良かったです。"},{"id":{"d":669,"n":5,"t":3},"speaker":"A","text":"私が見つけたのはこの3枚ですが、あなたが貯金箱に入れたのはこれで全部ですか？"},{"id":{"d":669,"n":6,"t":3},"speaker":"B","text":"はい。これらは、モナコとサンマリノとバチカンのユーロ硬貨です。どれもデザインが素晴らしいのです。"},{"id":{"d":669,"n":7,"t":3},"speaker":"A","text":"それほど大事なら自分でユーロ硬貨を保管しておいてください。ユーロ加盟国の数だけデザインが違うユーロ硬貨があるのですか？"},{"id":{"d":669,"n":8,"t":3},"speaker":"B","text":"具体的な数は分かりませんが、ユーロ硬貨は多様なデザインのものが流通しています。"},{"id":{"d":669,"n":9,"t":3},"speaker":"A","text":"では、これら以外のデザインの硬貨もあるのですね。"},{"id":{"d":669,"n":10,"t":3},"speaker":"B","text":"はい。来年またヨーロッパに行って、これら以外のユーロ硬貨も入手したいと思っています。アンドラのユーロ硬貨は特に欲しいところです。"}];
INSERT INTO sentence [{"id":{"d":670,"n":1,"t":3},"speaker":"A","text":"1週間ほど八丈島に行ってきたそうですね。随分長い滞在ですね。"},{"id":{"d":670,"n":2,"t":3},"speaker":"B","text":"休養と釣りと観光の旅にしたかったので、長期滞在となりました。"},{"id":{"d":670,"n":3,"t":3},"speaker":"A","text":"釣りで八丈島に行く人は珍しいのではありませんか？"},{"id":{"d":670,"n":4,"t":3},"speaker":"B","text":"そのようなことはありません。八丈島はたくさんの釣りスポットがあることでも有名な島です。"},{"id":{"d":670,"n":5,"t":3},"speaker":"A","text":"沖釣りと磯釣りを楽しまれたのですか？"},{"id":{"d":670,"n":6,"t":3},"speaker":"B","text":"はい。磯釣りよりは堤防釣りの方が釣果はありました。ヒラマサやカンパチを釣ることができました。"},{"id":{"d":670,"n":7,"t":3},"speaker":"A","text":"八丈島では陸でそれほどの釣果があるのですね。船釣りすれば大漁間違いないのではありませんか？"},{"id":{"d":670,"n":8,"t":3},"speaker":"B","text":"はい。船釣りもしました。大漁でしたがアオダイやヒメダイが主体でした。船長によると、水温の影響だそうです。"},{"id":{"d":670,"n":9,"t":3},"speaker":"A","text":"それほどまでに釣りを楽しんだのでしたら、観光に行く暇などなかったのではありませんか？"},{"id":{"d":670,"n":10,"t":3},"speaker":"B","text":"そのようなわけでもありませんよ。八丈富士、ふれあい牧場などの定番スポットは見てきました。"}];
INSERT INTO sentence [{"id":{"d":671,"n":1,"t":3},"speaker":"A","text":"日本の実業家が宇宙旅行をしていましたね。"},{"id":{"d":671,"n":2,"t":3},"speaker":"B","text":"宇宙旅行には無重力体験と宇宙ステーション滞在があります。無重力体験の方は、比較的、気軽にできると思いますよ。"},{"id":{"d":671,"n":3,"t":3},"speaker":"A","text":"私たちは将来、月に行けるようになるのでしょうか？"},{"id":{"d":671,"n":4,"t":3},"speaker":"B","text":"はい。月旅行ツアーの企画はあるようですね。火星にも行く予定もあるようですが、それが観光目的かどうかは分かりません。"},{"id":{"d":671,"n":5,"t":3},"speaker":"A","text":"無重力体験はテレビで見たことはありますが、宇宙に行くこととは違うような気がします。"},{"id":{"d":671,"n":6,"t":3},"speaker":"B","text":"無重力体験は宇宙飛行士の訓練といった方がいいかもしれません。"},{"id":{"d":671,"n":7,"t":3},"speaker":"A","text":"私は「地球は青かった」と言えるくらい高い位置から地球を見下ろしたいです。"},{"id":{"d":671,"n":8,"t":3},"speaker":"B","text":"宇宙ホテルの計画や、宇宙空間を移動する飛行計画もあるようです。"},{"id":{"d":671,"n":9,"t":3},"speaker":"A","text":"技術力の進化によって宇宙旅行は現実的になってきましたね。これからは、さらに経済的な面を考えてくれたらと思います。"}];
INSERT INTO sentence [{"id":{"d":672,"n":1,"t":3},"speaker":"A","text":"一度でよいので潜水艇に乗ってみたいと思っています。どこかで乗れるのでしょうか？"},{"id":{"d":672,"n":2,"t":3},"speaker":"B","text":"一般人にはそのような機会は提供されていないと思います。自衛隊に入隊するか、海洋学者になれば、潜水艇に乗る機会があるかもしれません。"},{"id":{"d":672,"n":3,"t":3},"speaker":"A","text":"私は、自衛隊員にも海洋学者にもなれそうにありません。"},{"id":{"d":672,"n":4,"t":3},"speaker":"B","text":"そういえば、子どもの頃に行った横浜ドリームランドに、潜水艦の疑似体験ができるアトラクションがありました。"},{"id":{"d":672,"n":5,"t":3},"speaker":"A","text":"半潜水艇ならば、今も観光設備としてあると思います。"},{"id":{"d":672,"n":6,"t":3},"speaker":"B","text":"外国に行けば、潜水艇の施設もあるかもしれませんね。"},{"id":{"d":672,"n":7,"t":3},"speaker":"A","text":"それは恐らくグアム島のアトランティス・サブマリンのことでしょうか？"},{"id":{"d":672,"n":8,"t":3},"speaker":"B","text":"はい。グアム島の潜水艦をご存じだったのですね。それに乗るのはどうですか？"},{"id":{"d":672,"n":9,"t":3},"speaker":"A","text":"自由に航路を進み、どこまででも潜れることが私の潜水艇のイメージなので、その潜水艦とは少し違うのです。"},{"id":{"d":672,"n":10,"t":3},"speaker":"B","text":"やはり潜水艇ですから海の深いところを潜航したいのですね。"}];
INSERT INTO sentence [{"id":{"d":673,"n":1,"t":3},"speaker":"A","text":"関所というと私は箱根くらいしか思い浮かびませんが、他の関所をご存知ですか？"},{"id":{"d":673,"n":2,"t":3},"speaker":"B","text":"はい。箱根以外にも、新居や碓氷などがありますね。"},{"id":{"d":673,"n":3,"t":3},"speaker":"A","text":"そうなのですね。関所は何のために設置されたのでしょうか？"},{"id":{"d":673,"n":4,"t":3},"speaker":"B","text":"関所とは、人や荷物の行き来を取り締まる場所のことです。江戸時代は、江戸に武器を入れないこと、江戸から大名の妻子を出さないことなどが関所の役目だったそうです。"},{"id":{"d":673,"n":5,"t":3},"speaker":"A","text":"それは「入鉄砲出女」といわれるものですね。各街道に1つは関所があることになります。"},{"id":{"d":673,"n":6,"t":3},"speaker":"B","text":"私が知っている関所は、東海道の箱根、中山道の碓氷、甲州街道の小仏、日光街道の栗橋です。"},{"id":{"d":673,"n":7,"t":3},"speaker":"A","text":"詳しいのですね。"},{"id":{"d":673,"n":8,"t":3},"speaker":"B","text":"大学入試は日本史を選択して勉強していたので、その頃に関所を暗記していたのですよ。"},{"id":{"d":673,"n":9,"t":3},"speaker":"A","text":"そうだったのですね。それを今もまだ覚えているなんて、素晴らしいです。"},{"id":{"d":673,"n":10,"t":3},"speaker":"B","text":"その頃と比べたらだいぶ忘れてしまっていますよ。まだ覚えているのは、交通情報を聞く時によく出てくる地名ばかりです。"},{"id":{"d":673,"n":11,"t":3},"speaker":"A","text":"そういえば、碓氷は聞いたことがありました。碓氷はアニメーションの聖地にもなっていて私も何度かドライブで行ったことがあります。"},{"id":{"d":673,"n":12,"t":3},"speaker":"B","text":"もしかして、そのアニメーションとは『頭文字D』のことですか？"},{"id":{"d":673,"n":13,"t":3},"speaker":"A","text":"そうです。あなたも行ったことがありますか？"},{"id":{"d":673,"n":14,"t":3},"speaker":"B","text":"私もドライブで行ったことがありますよ。聞きしに勝るカーブの数でした。"},{"id":{"d":673,"n":15,"t":3},"speaker":"A","text":"一般人が運転するには困難な道路です。高速道路の上信越自動車道が作られた理由もよく分かります。"}];
INSERT INTO sentence [{"id":{"d":674,"n":1,"t":3},"speaker":"A","text":"グラストンベリーに行ったそうですね。アーサー王伝説がお好きですか？"},{"id":{"d":674,"n":2,"t":3},"speaker":"B","text":"いいえ。グラストンベリーはアーサー王伝説と縁があるのですか？知りませんでした。"},{"id":{"d":674,"n":3,"t":3},"speaker":"A","text":"私は大学でアーサー王伝説についての研究をしているので、グラストンベリーには何度か行っています。"},{"id":{"d":674,"n":4,"t":3},"speaker":"B","text":"そうなのですね。私は野外音楽フェスティバルに参加するためにグラストンベリーへ行ってきたのです。"},{"id":{"d":674,"n":5,"t":3},"speaker":"A","text":"そのようなイベントが開催されているのですか？以前、旅行業者から「ある期間はグラストンベリーに行かない方がいい」と聞いたことがあるのですが、それはきっとそのフェスティバル期間を指していたのですね。"},{"id":{"d":674,"n":6,"t":3},"speaker":"B","text":"そうだと思います。アーサー王伝説についての研究のために行くなら、グラストンベリーフェスティバルのあの騒がしさには耐えられないかもしれません。"},{"id":{"d":674,"n":7,"t":3},"speaker":"A","text":"私の知っているグラストンベリーは都会から離れた静かなところですから、フェスティバルのイメージが湧きません。"},{"id":{"d":674,"n":8,"t":3},"speaker":"B","text":"野外フェスティバルの開催には広い場所が必要ですので、都会から離れたところが会場になることは多いのですよ。"}];
INSERT INTO sentence [{"id":{"d":675,"n":1,"t":3},"speaker":"A","text":"この夏の暑い時期に、なぜ苗場に行くのですか？苗場と言えば、冬にスキーを楽しむ場所ですよね？"},{"id":{"d":675,"n":2,"t":3},"speaker":"B","text":"フジロックフェスティバルをご存知ありませんか？フェスティバルを3日間楽しんできます。"},{"id":{"d":675,"n":3,"t":3},"speaker":"A","text":"フジロックフェスティバルは苗場で開かれているのですか？知りませんでした。"},{"id":{"d":675,"n":4,"t":3},"speaker":"B","text":"開催当時は違う場所でしたが、ここのところはずっと苗場スキー場で行われています。"},{"id":{"d":675,"n":5,"t":3},"speaker":"A","text":"苗場は交通の便が良いように思えませんが、車で見に来る人が多いのですか？"},{"id":{"d":675,"n":6,"t":3},"speaker":"B","text":"はい。車で来る人は多いですが、事前に入手した駐車券がないと駐車はできません。"},{"id":{"d":675,"n":7,"t":3},"speaker":"A","text":"それほどまでに多くの人が集まるのですね。"},{"id":{"d":675,"n":8,"t":3},"speaker":"B","text":"そう思います。駐車場もそうですが宿泊施設も事前に予約しておかなければならないほどです。"},{"id":{"d":675,"n":9,"t":3},"speaker":"A","text":"あなたは、もう何回か行かれているのですか？フジロックフェスティバルに慣れているようですね。"},{"id":{"d":675,"n":10,"t":3},"speaker":"B","text":"はい。毎年誘ってくれる友人がいるのです。彼がキャンピングカーを持っているので、ホテルには泊まらず車中泊をすることで、比較的安価にフジロックフェスティバルに参加できます。"}];
INSERT INTO sentence [{"id":{"d":676,"n":1,"t":3},"speaker":"A","text":"神奈川県の小中学校の遠足では、どの辺りへ行くのですか？"},{"id":{"d":676,"n":2,"t":3},"speaker":"B","text":"私が小中学生の時は、鎌倉山や丹沢のハイキングコースに行きました。"},{"id":{"d":676,"n":3,"t":3},"speaker":"A","text":"箱根には行かなかったのですか？"},{"id":{"d":676,"n":4,"t":3},"speaker":"B","text":"行きました。遠足で箱根へ行った記憶はありますが、何を見たかはあまり覚えていません。"},{"id":{"d":676,"n":5,"t":3},"speaker":"A","text":"鎌倉や丹沢のハイキングのことは覚えているのですか？"},{"id":{"d":676,"n":6,"t":3},"speaker":"B","text":"はい。山歩きが大変だったので記憶に残っているのだと思います。丹沢に行った時は雨が降ってきたので特によく覚えています。"},{"id":{"d":676,"n":7,"t":3},"speaker":"A","text":"鎌倉山はそれほど高い山ではないという印象がありますが、それほど大変だったのですか？"},{"id":{"d":676,"n":8,"t":3},"speaker":"B","text":"はい。自然の要塞としてその地に鎌倉幕府が開かれたほどですから、山道は険しいところが多いのです。"},{"id":{"d":676,"n":9,"t":3},"speaker":"A","text":"そうなのですね。日本の多くの山にはハイキングコースが作られているので、誰でも簡単に山歩きを楽しめるものだと思っていました。"},{"id":{"d":676,"n":10,"t":3},"speaker":"B","text":"子どもだったので、険しい山道が余計大変に感じられたのかもしれません。大人にとっては、ハイキングコースを歩くことは自然と接する楽しい機会なのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":677,"n":1,"t":3},"speaker":"A","text":"小学校の遠足で芦ノ湖に行ったときは、遊覧船に乗ったのですか？"},{"id":{"d":677,"n":2,"t":3},"speaker":"B","text":"いいえ。確か遊覧船も海賊船もロープウェイも使わなかったと思います。"},{"id":{"d":677,"n":3,"t":3},"speaker":"A","text":"海賊船があるのですか？遊覧船とは違うのですか？"},{"id":{"d":677,"n":4,"t":3},"speaker":"B","text":"海賊船の形をした遊覧船がありました。学校の遠足はバスでの移動が多いので、遊覧船やロープウェイは利用しなかったのでしょう。"},{"id":{"d":677,"n":5,"t":3},"speaker":"A","text":"なるほど。では、彫刻の森美術館には行きましたか？"},{"id":{"d":677,"n":6,"t":3},"speaker":"B","text":"はい、行きました。芸術に触れることができる美術館として当時から有名でした。"},{"id":{"d":677,"n":7,"t":3},"speaker":"A","text":"私が子どもの頃、テレビの天気予報番組の背景が彫刻の森美術館だったことがありました。"},{"id":{"d":677,"n":8,"t":3},"speaker":"B","text":"そうですね、私もテレビで見た記憶があります。それでヘンリー・ムーアの名前を知りました。"}];
INSERT INTO sentence [{"id":{"d":678,"n":1,"t":3},"speaker":"A","text":"函館で夜景を見るならば函館山が良いと聞きました。"},{"id":{"d":678,"n":2,"t":3},"speaker":"B","text":"その通りです。函館山は人気のある最もきれいな夜景スポットです。"},{"id":{"d":678,"n":3,"t":3},"speaker":"A","text":"函館の夜景を見に行くにあたって、注意しておくべきことはありますか？"},{"id":{"d":678,"n":4,"t":3},"speaker":"B","text":"日没後のロープウェイは混みますので、早めに山頂に登った方がいいかもしれません。"},{"id":{"d":678,"n":5,"t":3},"speaker":"A","text":"なるほど、その情報はありがたいです。霧で夜景が見られない時があると聞いたことがありますが、そのような日もあるのでしょうか？"},{"id":{"d":678,"n":6,"t":3},"speaker":"B","text":"はい。私は霧がかっている夜景の方が幻想的で好きですが、晴れた夜景を希望されるならば冬に行く方がいいでしょう。"},{"id":{"d":678,"n":7,"t":3},"speaker":"A","text":"霧がかっていると、それほどに幻想的な光景になるのですか？"},{"id":{"d":678,"n":8,"t":3},"speaker":"B","text":"はい。山から見下ろすとまるで雲の上にいるような気持ちになります。日没前に行けば、霧に夕日が溶けていく様子を見ることができます。"},{"id":{"d":678,"n":9,"t":3},"speaker":"A","text":"霧の日にはそのような素晴らしい光景が見られるのですね。やはり日没より早めに山頂へ行った方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":679,"n":1,"t":3},"speaker":"A","text":"今度、スコットランドに行くことにしました。"},{"id":{"d":679,"n":2,"t":3},"speaker":"B","text":"もしかすると、映画『ハリー・ポッター』の聖地巡礼ですか？"},{"id":{"d":679,"n":3,"t":3},"speaker":"A","text":"お察しの通りです。『ハリー・ポッター』で見たすてきなシーンの多くは、スコットランドにあると知ったのです。"},{"id":{"d":679,"n":4,"t":3},"speaker":"B","text":"ホグワーツに向かう蒸気機関車は本当にあるそうですね。"},{"id":{"d":679,"n":5,"t":3},"speaker":"A","text":"はい。ホグワーツ・エクスプレスといわれるジャコバイト号です。それには必ず乗ろうと思っています。"},{"id":{"d":679,"n":6,"t":3},"speaker":"B","text":"映画に出てくる場所を実際に見られるのはすばらしいですね。スカイ島には行かれるのですか？"},{"id":{"d":679,"n":7,"t":3},"speaker":"A","text":"もちろんです。スカイ島は翼の島といわれていて、島全体が映画のロケ地のような素晴らしい景観だそうです。"},{"id":{"d":679,"n":8,"t":3},"speaker":"B","text":"それは楽しみですね。恐らくどの景色を見ても映画やドラマで見たような気がするのでしょうね。"},{"id":{"d":679,"n":9,"t":3},"speaker":"A","text":"はい。旅行に行ってきた後で映画を見直すと、また別の楽しみが増えると思います。"},{"id":{"d":679,"n":10,"t":3},"speaker":"B","text":"帰ってきましたら、ぜひ旅行の話を聞かせてください。"}];
INSERT INTO sentence [{"id":{"d":680,"n":1,"t":3},"speaker":"A","text":"F1を観戦したいのですが、鈴鹿サーキットへ行くには車でなければいけませんか？"},{"id":{"d":680,"n":2,"t":3},"speaker":"B","text":"鈴鹿には車で行く人が多いと思いますが、ツアーを利用する方法もありますよ。"},{"id":{"d":680,"n":3,"t":3},"speaker":"A","text":"ツアーを利用する場合は、木曜日から行かなければなりませんか？"},{"id":{"d":680,"n":4,"t":3},"speaker":"B","text":"木曜日からのツアーだけではないと思います。土日開催のツアーもあるはずです。"},{"id":{"d":680,"n":5,"t":3},"speaker":"A","text":"ツアーの場合、鈴鹿への移動方法は電車とバスでしょうか？"},{"id":{"d":680,"n":6,"t":3},"speaker":"B","text":"新幹線とバスを利用するツアーと、バスのみを利用するツアーがあると思います。"},{"id":{"d":680,"n":7,"t":3},"speaker":"A","text":"F1観戦チケットは自分で購入するのですか？"},{"id":{"d":680,"n":8,"t":3},"speaker":"B","text":"そうですね。確かF1のチケットは自分で購入しなければならないはずです。あいにく詳細までは分かりません。"},{"id":{"d":680,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。自分で確認してみます。"}];
INSERT INTO sentence [{"id":{"d":681,"n":1,"t":3},"speaker":"A","text":"この間、旅行で小樽に行ってきました。"},{"id":{"d":681,"n":2,"t":3},"speaker":"B","text":"小樽というと洋館などの建築物が有名ですね。"},{"id":{"d":681,"n":3,"t":3},"speaker":"A","text":"はい。町全体が運河とレンガ造りの建築物で構成されていました。"},{"id":{"d":681,"n":4,"t":3},"speaker":"B","text":"確か、金融業も発展していたそうですね？"},{"id":{"d":681,"n":5,"t":3},"speaker":"A","text":"日本銀行旧小樽支店、旧北海道銀行本店、旧北海道拓殖銀行小樽支店などを見てきました。"},{"id":{"d":681,"n":6,"t":3},"speaker":"B","text":"確か、小樽にはオルゴールの専門店が存在したように思います。"},{"id":{"d":681,"n":7,"t":3},"speaker":"A","text":"それは、小樽オルゴール堂本館のことですね。歴史的建造物で入口には世界最大の蒸気時計がありました。"},{"id":{"d":681,"n":8,"t":3},"speaker":"B","text":"そうなのですね。小樽には造り酒屋の老舗も多いと聞いたことがあります。"},{"id":{"d":681,"n":9,"t":3},"speaker":"A","text":"その通りです。建造物とお酒好きには見逃せない場所が多くありましたよ。"}];
INSERT INTO sentence [{"id":{"d":682,"n":1,"t":3},"speaker":"A","text":"修学旅行で京都や日光以外の場所に行ったことはありますか？"},{"id":{"d":682,"n":2,"t":3},"speaker":"B","text":"はい、あります。高校の修学旅行で、山口県の萩市に行きました。"},{"id":{"d":682,"n":3,"t":3},"speaker":"A","text":"萩には松下村塾がありますね。行きましたか？"},{"id":{"d":682,"n":4,"t":3},"speaker":"B","text":"ええ、行きましたが、思っていたより小さかったのでびっくりしました。"},{"id":{"d":682,"n":5,"t":3},"speaker":"A","text":"江戸時代の地方の私塾ですから、現代人の住環境感覚とは違うのでしょうね。"},{"id":{"d":682,"n":6,"t":3},"speaker":"B","text":"山口県の美祢市にある秋芳洞にも行ってきましたが、こちらは思っていたよりも大きかったです。"},{"id":{"d":682,"n":7,"t":3},"speaker":"A","text":"洞窟は神秘的で異世界の雰囲気がしますね。島根県の津和野町には行きましたか？"},{"id":{"d":682,"n":8,"t":3},"speaker":"B","text":"はい。津和野町では森鴎外の生まれた場所を訪れました。落ち着いた静かなところでした。"},{"id":{"d":682,"n":9,"t":3},"speaker":"A","text":"学校の先生は、修学旅行としては静かなところを選んだようですね。"},{"id":{"d":682,"n":10,"t":3},"speaker":"B","text":"そうですね。行く前は行き先が有名な観光地ではないことを少し残念に思っていたのですが、実際に行ってみると想像していたよりも印象に残るところが多く、とても楽しめました。"}];
INSERT INTO sentence [{"id":{"d":683,"n":1,"t":3},"speaker":"A","text":"マラソンを始めたそうですね。健康に気を付けるようになったのですか？"},{"id":{"d":683,"n":2,"t":3},"speaker":"B","text":"いいえ、健康のためではありません。妻がマラソンに熱中していて、「来年はホノルルマラソンに出場したい」と言っているので、私も一緒に走っているのです。"},{"id":{"d":683,"n":3,"t":3},"speaker":"A","text":"もしかしたら、奥さまに付き合ってホノルルマラソンに出場するのですか？"},{"id":{"d":683,"n":4,"t":3},"speaker":"B","text":"はい。私はハワイ州には何回か行ったことがあるので、ハワイ州のことをいろいろ話していたら妻から「一緒に出場しよう」と誘われたのです。"},{"id":{"d":683,"n":5,"t":3},"speaker":"A","text":"素人が1年後にフルマラソンに挑戦するのは大変ではありませんか？"},{"id":{"d":683,"n":6,"t":3},"speaker":"B","text":"実は高校までは陸上部で中距離を走っていたので全くの素人というわけではありませんが、さすがにフルマラソンを走るのは難しいと考えて練習を始めたのです。"},{"id":{"d":683,"n":7,"t":3},"speaker":"A","text":"陸上種目の経験者とは知らず、失礼しました。奥さまは東京マラソンには出場しなかったのですか？"},{"id":{"d":683,"n":8,"t":3},"speaker":"B","text":"東京マラソンには出場していません。東京マラソンの出場の抽選に漏れたことが、ホノルルマラソン出場を考えるきっかけになったようです。"},{"id":{"d":683,"n":9,"t":3},"speaker":"A","text":"そうでしたか。ハワイ州の観光も楽しむことができるので、ホノルルマラソンに参加するのはむしろ魅力的かもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":684,"n":1,"t":3},"speaker":"A","text":"新婚旅行の行き先にモロッコを選んだそうですが、何か理由があるのですか？"},{"id":{"d":684,"n":2,"t":3},"speaker":"B","text":"私たちは趣味の映画を通じて出会ったので、二人の好きな映画にゆかりのあるモロッコに行くことにしました。"},{"id":{"d":684,"n":3,"t":3},"speaker":"A","text":"モロッコが舞台の映画には例えばどのようなものがあるのですか？"},{"id":{"d":684,"n":4,"t":3},"speaker":"B","text":"『モロッコ』、『カサブランカ』、『シェルタリング・スカイ』、『アレキサンダー』などです。"},{"id":{"d":684,"n":5,"t":3},"speaker":"A","text":"『カサブランカ』と『シェルタリング・スカイ』は私にも分かりました。"},{"id":{"d":684,"n":6,"t":3},"speaker":"B","text":"『アラビアのロレンス』のような映画の砂漠のシーンは、モロッコで撮影されていることが多いようです。"},{"id":{"d":684,"n":7,"t":3},"speaker":"A","text":"モロッコは映画のロケ地としても有名なところなのですね。"},{"id":{"d":684,"n":8,"t":3},"speaker":"B","text":"映画にも出てきますが、フェズの世界一複雑な迷路やマラケシュの広場にも行ってみたいと思っています。"},{"id":{"d":684,"n":9,"t":3},"speaker":"A","text":"映画ざんまいの新婚旅行になりそうですね。"},{"id":{"d":684,"n":10,"t":3},"speaker":"B","text":"はい。とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":685,"n":1,"t":3},"speaker":"A","text":"何度もヨーロッパに行かれているようですが、今回の目的は何ですか？"},{"id":{"d":685,"n":2,"t":3},"speaker":"B","text":"今回はスロベニアとクロアチアをじっくり見ることです。"},{"id":{"d":685,"n":3,"t":3},"speaker":"A","text":"スロベニアという国は知りませんが、クロアチアの近くですか？"},{"id":{"d":685,"n":4,"t":3},"speaker":"B","text":"はい。スロベニアはクロアチアの西にあり、オーストリアやイタリアに接しています。"},{"id":{"d":685,"n":5,"t":3},"speaker":"A","text":"スロベニアではどこを見てくる予定なのですか？"},{"id":{"d":685,"n":6,"t":3},"speaker":"B","text":"ヨーロッパ最大級のポストイナ鍾乳洞とブレッド湖の聖マリア教会は、スロベニアでは見てくる予定です。"},{"id":{"d":685,"n":7,"t":3},"speaker":"A","text":"スロベニアの聖マリア教会はどうして名所になっているのですか？"},{"id":{"d":685,"n":8,"t":3},"speaker":"B","text":"湖に浮かぶ島全体が教会で、岸辺から続く階段を上がって教会に入るのです。さらに、アルプス山脈を背景にした素晴らしい景観が見られるからだそうです。"},{"id":{"d":685,"n":9,"t":3},"speaker":"A","text":"そうなのですね。イタリアやオーストリアのように建築物も素晴らしいのでしょうか？"},{"id":{"d":685,"n":10,"t":3},"speaker":"B","text":"スロベニアの首都のリュブリアーナは、町全体が芸術的建築様式の宝庫といわれています。"}];
INSERT INTO sentence [{"id":{"d":686,"n":1,"t":3},"speaker":"A","text":"クロアチアのサッカーチームのことはよく知っているのですが、観光地としてはあまりよく知りません。どのような場所ですか？"},{"id":{"d":686,"n":2,"t":3},"speaker":"B","text":"クロアチアは2018年のFIFAワールドカップで準優勝だった強豪国ですよね。もちろん観光地としても有名な国ですよ。"},{"id":{"d":686,"n":3,"t":3},"speaker":"A","text":"あの辺りはボスニア・ヘルツェゴビナ紛争も起きたので、観光に行くには少し勇気が必要な気がします。"},{"id":{"d":686,"n":4,"t":3},"speaker":"B","text":"クロアチアでは紛争はもうすっかり落ち着いています。観光国としても力を入れているので十分楽しめますよ。"},{"id":{"d":686,"n":5,"t":3},"speaker":"A","text":"そうなのですね。クロアチアも、イタリアやオーストリアのように、ハプスブルク家の影響があるのでしょうか？"},{"id":{"d":686,"n":6,"t":3},"speaker":"B","text":"はい。クロアチアの首都のザグレブは、特にハプスブルク時代の影響があります。その時代に興味があるのであれば、ザグレブ聖母被昇天大聖堂や聖マルコ教会は必見です。"},{"id":{"d":686,"n":7,"t":3},"speaker":"A","text":"なるほど。クロアチアはアドリア海に接していますか？"},{"id":{"d":686,"n":8,"t":3},"speaker":"B","text":"はい。スプリットという都市はアドリア海沿岸で最大の港町です。魚市場や青空市場を楽しむことができます。"},{"id":{"d":686,"n":9,"t":3},"speaker":"A","text":"クロアチアには観光地としてもたくさんの魅力があるのですね。"},{"id":{"d":686,"n":10,"t":3},"speaker":"B","text":"はい。城壁で囲まれた島のトロギール、迷路のような旧市街で有名なザダールと、観光地としての見どころが満載な国です。"}];
INSERT INTO sentence [{"id":{"d":687,"n":1,"t":3},"speaker":"A","text":"オーストリアではどこが楽しかったですか？"},{"id":{"d":687,"n":2,"t":3},"speaker":"B","text":"一番楽しかったのは美術館です。私は大学生の頃から世紀末ウィーンの絵画に興味があります。"},{"id":{"d":687,"n":3,"t":3},"speaker":"A","text":"グスタフ・クリムトやエゴン・シーレなどの作品のことですね。"},{"id":{"d":687,"n":4,"t":3},"speaker":"B","text":"そうです。美術史美術館でクリムトの装飾画を見たかったので、目標が叶って嬉しかったです。"},{"id":{"d":687,"n":5,"t":3},"speaker":"A","text":"美術史美術館でしたらクリムト以外にも重要な作品をたくさん見ることができますね。"},{"id":{"d":687,"n":6,"t":3},"speaker":"B","text":"はい。カラヴァッジョ、ブリューゲル、ベラスケス、デューラーの作品もあります。クリムトの作品を見に行く人は少ないかもしれません。"},{"id":{"d":687,"n":7,"t":3},"speaker":"A","text":"世紀末ウィーン絵画といえば、ベルヴェデーレ宮殿の美術館を真っ先に挙げられるものと思っていました。"},{"id":{"d":687,"n":8,"t":3},"speaker":"B","text":"もちろんベルヴェデーレ宮殿の美術館にも行きました。"},{"id":{"d":687,"n":9,"t":3},"speaker":"A","text":"クリムトとエゴン・シーレとココシュカの代表作を見られる美術館と聞いたことがあります。"},{"id":{"d":687,"n":10,"t":3},"speaker":"B","text":"レオポルド美術館も世紀末ウィーン絵画とドイツ表現主義絵画にとって重要なところです。"}];
INSERT INTO sentence [{"id":{"d":688,"n":1,"t":3},"speaker":"A","text":"ヨーロッパを旅行するときには、どのような移動方法が便利ですか？"},{"id":{"d":688,"n":2,"t":3},"speaker":"B","text":"移動場所によって違うとは思いますが、絶対に外せない乗り物は列車です。"},{"id":{"d":688,"n":3,"t":3},"speaker":"A","text":"確かに、国外運転免許証がないと車には乗ることができませんね。"},{"id":{"d":688,"n":4,"t":3},"speaker":"B","text":"車は国外運転免許証を持っていて、外国生活と外国での運転に慣れた人が使う移動方法だと思います。"},{"id":{"d":688,"n":5,"t":3},"speaker":"A","text":"私は、列車も外国生活に慣れた人でないと使いこなすのが難しい移動手段のように思います。"},{"id":{"d":688,"n":6,"t":3},"speaker":"B","text":"そうとも言えませんよ。飛行機は値段が高い上に、空港が目的地から離れている場合が多いので、列車の方が便利な場合も多いです。"},{"id":{"d":688,"n":7,"t":3},"speaker":"A","text":"案内表示も英語ならば良いのですが、ドイツ語やハンガリー語で書かれているものは私には理解できません。"},{"id":{"d":688,"n":8,"t":3},"speaker":"B","text":"意外と英語表示は多いですよ。駅員さんがいたら恥ずかしがらずに聞けば、親切に教えてくれます。英語以外の言語も、地名くらいは聞き取れます。"},{"id":{"d":688,"n":9,"t":3},"speaker":"A","text":"そうですか。それでも外国旅行で列車に一人で乗るのは不安です。"},{"id":{"d":688,"n":10,"t":3},"speaker":"B","text":"では、まずは英語が使える英国で列車の旅を始めてはいかがですか？"},{"id":{"d":688,"n":11,"t":3},"speaker":"A","text":"それは良い考えです。ユーロスターには乗ってみたいと思っていました。"}];
INSERT INTO sentence [{"id":{"d":689,"n":1,"t":3},"speaker":"A","text":"今度のヨーロッパ旅行では、どこを訪れたのですか？"},{"id":{"d":689,"n":2,"t":3},"speaker":"B","text":"バルト三国のリトアニア、エストニア、ラトビアを巡ってきました。"},{"id":{"d":689,"n":3,"t":3},"speaker":"A","text":"バルト三国は非常に美しいところと聞いたことがあります。"},{"id":{"d":689,"n":4,"t":3},"speaker":"B","text":"はい。リトアニアの首都ヴィリニュス、エストニアの首都タリン、ラトビアの首都リガと、それぞれ素晴らしい景観でした。"},{"id":{"d":689,"n":5,"t":3},"speaker":"A","text":"バルト三国は歴史的に複雑なところですよね。"},{"id":{"d":689,"n":6,"t":3},"speaker":"B","text":"三国どの国も北欧やドイツ、旧ロシアやソビエト連邦に占領されていた時期があります。"},{"id":{"d":689,"n":7,"t":3},"speaker":"A","text":"バルト三国で特に見ておかなければならないところはどこですか？"},{"id":{"d":689,"n":8,"t":3},"speaker":"B","text":"リガのユーゲントシュティール建築群とバルトのヴェルサイユといわれるルンダーレ宮殿、シャウレイの十字架の丘はぜひ見ていただきたいです。"},{"id":{"d":689,"n":9,"t":3},"speaker":"A","text":"特にラトビアのリガに思い入れがあるのですか？"},{"id":{"d":689,"n":10,"t":3},"speaker":"B","text":"そうなのです。リガはバルト海の真珠といわれているだけあって、本当に素晴らしい町です。ぜひもう一度行きたいと思います。"}];
INSERT INTO sentence [{"id":{"d":690,"n":1,"t":3},"speaker":"A","text":"フィンランドに行ってきた、ということはオーロラを見に行ったのですね。"},{"id":{"d":690,"n":2,"t":3},"speaker":"B","text":"ええ、やはり一生に一度はオーロラを見たいと思いました。"},{"id":{"d":690,"n":3,"t":3},"speaker":"A","text":"それでオーロラを見ることはできたのですか？"},{"id":{"d":690,"n":4,"t":3},"speaker":"B","text":"はい。オーロラの見られる時期を選んで日程にも余裕を持たせたので、2回もはっきりと見ることができました。"},{"id":{"d":690,"n":5,"t":3},"speaker":"A","text":"それは良かったですね。フィンランドには他にどのような観光地があるのですか？"},{"id":{"d":690,"n":6,"t":3},"speaker":"B","text":"ウスペンスキー寺院、テンペリアウキオ教会、ヘルシンキ大聖堂はそれぞれ特徴があって素晴らしいところでした。"},{"id":{"d":690,"n":7,"t":3},"speaker":"A","text":"フィンランドですから、ムーミンやサンタクロースのところにも行きましたか？"},{"id":{"d":690,"n":8,"t":3},"speaker":"B","text":"もちろん。ムーミンワールドやサンタクロース村にも行ってきましたよ。"},{"id":{"d":690,"n":9,"t":3},"speaker":"A","text":"ムーミンとサンタクロースは外せませんね。"},{"id":{"d":690,"n":10,"t":3},"speaker":"B","text":"郷土料理でトナカイ肉のシチューも食べてきました。"},{"id":{"d":690,"n":11,"t":3},"speaker":"A","text":"それは私も食べてみたいものです。"}];
INSERT INTO sentence [{"id":{"d":691,"n":1,"t":3},"speaker":"A","text":"フィリピンに行ってきたそうですね。"},{"id":{"d":691,"n":2,"t":3},"speaker":"B","text":"はい。ルソン島にある首都のマニラに行ってきました。"},{"id":{"d":691,"n":3,"t":3},"speaker":"A","text":"マニラはどうでしたか？"},{"id":{"d":691,"n":4,"t":3},"speaker":"B","text":"マニラは驚くほどほど近代的な都市でした。高級ブランドショップが多くありました。"},{"id":{"d":691,"n":5,"t":3},"speaker":"A","text":"あなたは高級ブランドには興味がないと思っていましたが。"},{"id":{"d":691,"n":6,"t":3},"speaker":"B","text":"確かに、私は高級ブランドに興味はありません。目的はビガンの歴史都市やバロック様式の教会を見てくることでした。"},{"id":{"d":691,"n":7,"t":3},"speaker":"A","text":"なるほど。フィリピンはキリスト教国でしたね。"},{"id":{"d":691,"n":8,"t":3},"speaker":"B","text":"スペインが統治していた16世紀ころの雰囲気が残るイントラムロスは必見です。"},{"id":{"d":691,"n":9,"t":3},"speaker":"A","text":"そのように歴史的な街並みが残る一方で、セブ島などはリゾート地としても有名ですから、フィリピンはいろいろな面を持っているのですね。"},{"id":{"d":691,"n":10,"t":3},"speaker":"B","text":"はい。マニラの近代化した都市には本当に驚きました。"}];
INSERT INTO sentence [{"id":{"d":692,"n":1,"t":3},"speaker":"A","text":"夏休みに何か予定はありますか？"},{"id":{"d":692,"n":2,"t":3},"speaker":"B","text":"夏は、地方のローカル線に乗る旅行をするつもりです。"},{"id":{"d":692,"n":3,"t":3},"speaker":"A","text":"鉄道が趣味なのは知っていましたが、いつからそのような旅を始めたのですか？"},{"id":{"d":692,"n":4,"t":3},"speaker":"B","text":"去年からローカル線乗車の旅を始めました。アルバイトでお金を貯めて行くのです。"},{"id":{"d":692,"n":5,"t":3},"speaker":"A","text":"アルバイトでお金を貯めるといっても、結構お金はかかるのではないでしょうか？"},{"id":{"d":692,"n":6,"t":3},"speaker":"B","text":"一定期間内であれば全国の路線を利用できる乗車券を活用します。宿泊はユースホステルにしています。"},{"id":{"d":692,"n":7,"t":3},"speaker":"A","text":"ユースホステルは相部屋ではありませんでしたか？私は相部屋に少し抵抗感があります。"},{"id":{"d":692,"n":8,"t":3},"speaker":"B","text":"割高ですが個室のあるユースホステルもあります。私は話すことも好きなので相部屋の方が好きですよ。"},{"id":{"d":692,"n":9,"t":3},"speaker":"A","text":"知らない人と話すのは嫌ではありませんか？"},{"id":{"d":692,"n":10,"t":3},"speaker":"B","text":"いいえ。最初はみんな知らない人ですよ。それに、ユースホステルには海外からの旅行者も多く、彼らの国や文化についての話を聞くことは新鮮で面白いです。"}];
INSERT INTO sentence [{"id":{"d":693,"n":1,"t":3},"speaker":"A","text":"この割安の乗車券の有効期限はどのくらいですか？何日の間に何回乗車が可能なのでしょうか？"},{"id":{"d":693,"n":2,"t":3},"speaker":"B","text":"この乗車券は、利用期間中の任意の日に5回使用することができます。"},{"id":{"d":693,"n":3,"t":3},"speaker":"A","text":"1日に5回の乗車ですか？往復することを考えると、遠くまで行くのは難しいのではありませんか？"},{"id":{"d":693,"n":4,"t":3},"speaker":"B","text":"いいえ。5回分を1日で使う必要はありません。乗車券を使う日をあらかじめ決めておけばよく、日数まで決まっているわけではありません。"},{"id":{"d":693,"n":5,"t":3},"speaker":"A","text":"5回乗り降りした履歴は、乗車券に記載されるのですか？"},{"id":{"d":693,"n":6,"t":3},"speaker":"B","text":"はい。自動改札を利用せず、駅員さんに乗車印を押印してもらいます。"},{"id":{"d":693,"n":7,"t":3},"speaker":"A","text":"ローカル線では無人駅で駅員のいないところもありそうですね。"},{"id":{"d":693,"n":8,"t":3},"speaker":"B","text":"無人駅では、乗車した列車の車掌か運転手に乗車印を押してもらいます。"},{"id":{"d":693,"n":9,"t":3},"speaker":"A","text":"なるほど、この乗車券の使い方がよく分かりました。では早速、旅行スケジュールを立てましょう。"},{"id":{"d":693,"n":10,"t":3},"speaker":"B","text":"そうしましょう。旅行の日程を作ることも楽しみの一つですね。時刻表はありますか？"},{"id":{"d":693,"n":11,"t":3},"speaker":"A","text":"はい、ここにあります。"}];
INSERT INTO sentence [{"id":{"d":694,"n":1,"t":3},"speaker":"A","text":"秋田に詳しいとお聞きしました。なまはげの体験ができるような施設を知っていますか？"},{"id":{"d":694,"n":2,"t":3},"speaker":"B","text":"はい。男鹿半島になまはげ館があります。そこで、なまはげ習俗が体験できます。"},{"id":{"d":694,"n":3,"t":3},"speaker":"A","text":"その体験はいつでもできるのですか？"},{"id":{"d":694,"n":4,"t":3},"speaker":"B","text":"はい。なまはげ館に隣接する伝承館では、年間を通して体験ができるはずです。"},{"id":{"d":694,"n":5,"t":3},"speaker":"A","text":"伝承館はどのような場所なのですか？"},{"id":{"d":694,"n":6,"t":3},"speaker":"B","text":"正式名称は男鹿真山伝承館といって、秋田の伝統的な民家になっています。"},{"id":{"d":694,"n":7,"t":3},"speaker":"A","text":"男鹿半島には、他にも観光スポットがありそうですね。"},{"id":{"d":694,"n":8,"t":3},"speaker":"B","text":"入道崎や八郎潟も一度はご覧になった方がいいです。少しマニアックかもしれませんが、ゴジラ岩というものもありますよ。"},{"id":{"d":694,"n":9,"t":3},"speaker":"A","text":"ゴジラ岩とは何でしょうか？"},{"id":{"d":694,"n":10,"t":3},"speaker":"B","text":"文字通りゴジラによく似た岩です。男鹿半島に行くのならご覧になっておいた方がいいでしょう。"}];
INSERT INTO sentence [{"id":{"d":695,"n":1,"t":3},"speaker":"A","text":"国内旅行でかまくら体験ができるところはありますか？"},{"id":{"d":695,"n":2,"t":3},"speaker":"B","text":"長野県飯山市の「かまくらの里」、秋田県横田市の「横手の雪まつり」でかまくら体験ができます。「横手の雪まつり」は大規模なお祭りで見どころも多いですが、開催期間が限定されています。"},{"id":{"d":695,"n":3,"t":3},"speaker":"A","text":"「かまくらの里」はどのようなイベントなのですか？"},{"id":{"d":695,"n":4,"t":3},"speaker":"B","text":"かまくらのテーマパークのようなものです。"},{"id":{"d":695,"n":5,"t":3},"speaker":"A","text":"面白そうですが、利用できる時期は限定的なのですか？"},{"id":{"d":695,"n":6,"t":3},"speaker":"B","text":"「かまくらの里」は冬の約1カ月間です。積雪状況により開催期間は変わってきます。"},{"id":{"d":695,"n":7,"t":3},"speaker":"A","text":"「横手の雪まつり」はいつ開催されるのですか？"},{"id":{"d":695,"n":8,"t":3},"speaker":"B","text":"例年2月15日と16日に開催されます。"}];
INSERT INTO sentence [{"id":{"d":696,"n":1,"t":3},"speaker":"A","text":"コンパスポイントを知っていますか？"},{"id":{"d":696,"n":2,"t":3},"speaker":"B","text":"はい。カリブ海のバハマにあるリゾート地ですよね。"},{"id":{"d":696,"n":3,"t":3},"speaker":"A","text":"よく知っていますね。なぜ知っているのですか？"},{"id":{"d":696,"n":4,"t":3},"speaker":"B","text":"1970年から1980年代の音楽が好きの間では、コンパスポイントは有名だからです。"},{"id":{"d":696,"n":5,"t":3},"speaker":"A","text":"バハマのリゾート地と音楽に関係があるのですか？"},{"id":{"d":696,"n":6,"t":3},"speaker":"B","text":"コンパスポイントには有名なスタジオがあって、海外や日本の有名ミュージシャンが多く訪れています。"},{"id":{"d":696,"n":7,"t":3},"speaker":"A","text":"私もコンパスポイントに行ってきたのですが、そのようなことは知りませんでした。"},{"id":{"d":696,"n":8,"t":3},"speaker":"B","text":"スタジオはもう閉鎖されているので観光地ではないでしょう。"},{"id":{"d":696,"n":9,"t":3},"speaker":"A","text":"ええ。私はリゾート気分を味わいたかったのでとても満足しました。"},{"id":{"d":696,"n":10,"t":3},"speaker":"B","text":"それは良かったですね。ミュージシャンたちも保養が目的で、コンパスポイントのスタジオを利用したのかも知れません。"}];
INSERT INTO sentence [{"id":{"d":697,"n":1,"t":3},"speaker":"A","text":"日本近代文学館に行こうと思っています。"},{"id":{"d":697,"n":2,"t":3},"speaker":"B","text":"何かお目当ての展示品があるのですか？"},{"id":{"d":697,"n":3,"t":3},"speaker":"A","text":"はい。川端康成展を開催しているので、行ってみたいのです。あなたは以前、日本近代文学館に通われていたそうですね。"},{"id":{"d":697,"n":4,"t":3},"speaker":"B","text":"はい。私は、文学部の卒業論文を書いていた時、資料を集めるために通っていました。"},{"id":{"d":697,"n":5,"t":3},"speaker":"A","text":"日本近代文学館は東京の駒場にあって、交通の便は良さそうですね。"},{"id":{"d":697,"n":6,"t":3},"speaker":"B","text":"その通りです。確か、日本近代文学館には川端康成の記念室が存在したと思いますので、彼の特別展は何度か行われていると思います。"},{"id":{"d":697,"n":7,"t":3},"speaker":"A","text":"そのようですね。収蔵数はかなりあるようですが、誰でも閲覧できるのでしょうか？"},{"id":{"d":697,"n":8,"t":3},"speaker":"B","text":"はい。1日数百円で閲覧室が利用できます。そこで所蔵の図書や雑誌を検索して閲覧できます。"},{"id":{"d":697,"n":9,"t":3},"speaker":"A","text":"本や資料の貸し出しはしているのですか？"},{"id":{"d":697,"n":10,"t":3},"speaker":"B","text":"いいえ。貸し出しはしていないと思います。コピーや複写も制限があったはずです。"}];
INSERT INTO sentence [{"id":{"d":698,"n":1,"t":3},"speaker":"A","text":"シュノーケリングが趣味だそうですね。シュノーケリング目当ての旅行を計画することもあるのですか？"},{"id":{"d":698,"n":2,"t":3},"speaker":"B","text":"はい。シュノーケリングが目的で一人旅行によく出掛けます。この間は兄島に行ってきました。"},{"id":{"d":698,"n":3,"t":3},"speaker":"A","text":"小笠原諸島ですから海はきれいでしょうね。やはり南への旅行が多いのですか？"},{"id":{"d":698,"n":4,"t":3},"speaker":"B","text":"はい。遠出の旅行は、小笠原諸島や沖縄が多いです。水がきれいで海の生物をたくさん見ることができます。"},{"id":{"d":698,"n":5,"t":3},"speaker":"A","text":"しかし、遠出の旅行は、なかなか気軽にはできないのではありませんか？"},{"id":{"d":698,"n":6,"t":3},"speaker":"B","text":"そうですね。神奈川県や千葉県にもシュノーケリングスポットはあるので、遠出の旅行が難しい時はそちらに行きます。"},{"id":{"d":698,"n":7,"t":3},"speaker":"A","text":"近場の海ではシュノーケリングの楽しさも変わりますか？"},{"id":{"d":698,"n":8,"t":3},"speaker":"B","text":"いいえ。シュノーケリングはその場所にある楽しさを教えてくれます。近場なら四季の移り変わりを楽しむこともできます。"},{"id":{"d":698,"n":9,"t":3},"speaker":"A","text":"神奈川や千葉への移動手段は自動車ですか？"},{"id":{"d":698,"n":10,"t":3},"speaker":"B","text":"そうですね。スキューバダイビングほどではありませんが、普通の旅行よりは荷物が多くなるので、車での移動が便利です。"}];
INSERT INTO sentence [{"id":{"d":699,"n":1,"t":3},"speaker":"A","text":"夏は涼しいところへ行きたくなります。"},{"id":{"d":699,"n":2,"t":3},"speaker":"B","text":"そうですね。でも、時間をかけて避暑地へ行っても意外と暑いこともありますね。"},{"id":{"d":699,"n":3,"t":3},"speaker":"A","text":"私は千葉の勝浦が、夏でも涼しいと聞きました。"},{"id":{"d":699,"n":4,"t":3},"speaker":"B","text":"勝浦が避暑に適しているのですか？"},{"id":{"d":699,"n":5,"t":3},"speaker":"A","text":"私がニュースで聞いた情報では、勝浦では100年以上猛暑日がないそうですよ。"},{"id":{"d":699,"n":6,"t":3},"speaker":"B","text":"猛暑日がないと言っても、夜の気温が30度を超えるようならやはり寝苦しいのではないでしょうか？"},{"id":{"d":699,"n":7,"t":3},"speaker":"A","text":"それが勝浦では夜の気温は25度を下回ることが普通だと聞きました。"},{"id":{"d":699,"n":8,"t":3},"speaker":"B","text":"それは涼しそうですね。エアコンが苦手な人にとっては極楽です。"},{"id":{"d":699,"n":9,"t":3},"speaker":"A","text":"観光スポットやグルメスポットも多いと聞きましたので、今度一緒に行ってみませんか？"},{"id":{"d":699,"n":10,"t":3},"speaker":"B","text":"良いですね。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":700,"n":1,"t":3},"speaker":"A","text":"この間は、南米旅行のお土産をありがとうございます。"},{"id":{"d":700,"n":2,"t":3},"speaker":"B","text":"どういたしまして。ほんの少しばかりですが。ところであのキーホルダーが何でできているか分かりますか？"},{"id":{"d":700,"n":3,"t":3},"speaker":"A","text":"いいえ。私は、それをお聞きしようと思っていました。あれは貝のキーホルダーでしょうか？"},{"id":{"d":700,"n":4,"t":3},"speaker":"B","text":"いいえ。あれは、ピラルクのうろこのキーホルダーです。"},{"id":{"d":700,"n":5,"t":3},"speaker":"A","text":"あの大きさのうろこから想像すると、大変大きな魚ですね。"},{"id":{"d":700,"n":6,"t":3},"speaker":"B","text":"ピラルクは世界最大の淡水魚で体長3メートル以上といわれています。"},{"id":{"d":700,"n":7,"t":3},"speaker":"A","text":"人間の子どもの身長の2倍はあるのですね。"},{"id":{"d":700,"n":8,"t":3},"speaker":"B","text":"捕獲したピラルクが暴れて船が転覆することもあるそうです。"},{"id":{"d":700,"n":9,"t":3},"speaker":"A","text":"貴重な南米のお土産を頂いたのですね。どうもありがとうございます。"},{"id":{"d":700,"n":10,"t":3},"speaker":"B","text":"いえいえ、ピラルクのキーホルダーはそれほど珍しいものではありません。ブラジルではとてもよく知られたお土産です。"}];
INSERT INTO sentence [{"id":{"d":701,"n":1,"t":3},"speaker":"A","text":"マナティーを知っていますか？"},{"id":{"d":701,"n":2,"t":3},"speaker":"B","text":"はい。ジュゴンと同じで人魚伝説がある動物ですね。"},{"id":{"d":701,"n":3,"t":3},"speaker":"A","text":"そうです。それではマナティーとジュゴンの違いをご存知ですか？"},{"id":{"d":701,"n":4,"t":3},"speaker":"B","text":"いいえ、分かりません。ジュゴンとマナティーはどこが違うのですか？"},{"id":{"d":701,"n":5,"t":3},"speaker":"A","text":"尾びれがうちわのようになっているものがマナティーで、魚の尾びれのようになっている方がジュゴンです。"},{"id":{"d":701,"n":6,"t":3},"speaker":"B","text":"尾びれだけを見ると、マナティーの方にかわいらしさを感じます。"},{"id":{"d":701,"n":7,"t":3},"speaker":"A","text":"前びれが長いのもマナティーです。私はジュゴンとマナティーの両方が見られる水族館でそのことを知りました。"},{"id":{"d":701,"n":8,"t":3},"speaker":"B","text":"マナティーとジュゴンを見られる水族館はありますか？"},{"id":{"d":701,"n":9,"t":3},"speaker":"A","text":"日本でマナティーを見られるところは3カ所で、そのうちの1カ所ではジュゴンも見られるようです。"},{"id":{"d":701,"n":10,"t":3},"speaker":"B","text":"日本でジュゴンとマナティーの両方を見ることができる水族館は1つしかないのですね。それはどこですか？"},{"id":{"d":701,"n":11,"t":3},"speaker":"A","text":"三重県の鳥羽水族館です。"}];
INSERT INTO sentence [{"id":{"d":702,"n":1,"t":3},"speaker":"A","text":"先日、初めて熱海へ行ってきました。"},{"id":{"d":702,"n":2,"t":3},"speaker":"B","text":"熱海は観光地として有名ですね。どうでしたか？"},{"id":{"d":702,"n":3,"t":3},"speaker":"A","text":"温泉があり、海もあり、内陸は高原になっていて素晴らしい観光地でした。"},{"id":{"d":702,"n":4,"t":3},"speaker":"B","text":"熱海で特に印象に残ったところはありますか？"},{"id":{"d":702,"n":5,"t":3},"speaker":"A","text":"実は、熱川バナナワニ園に衝撃を受けました。"},{"id":{"d":702,"n":6,"t":3},"speaker":"B","text":"どのようなところに衝撃を受けたのでしょうか？"},{"id":{"d":702,"n":7,"t":3},"speaker":"A","text":"列車から外を見ていたら、多数のワニが餌を求めて争っているところを見てしまったのです。"},{"id":{"d":702,"n":8,"t":3},"speaker":"B","text":"確かに、列車から熱川バナナワニ園の園内が見られますね。"},{"id":{"d":702,"n":9,"t":3},"speaker":"A","text":"思わず次の駅で降りて1駅戻って熱川バナナワニ園に行きました。"},{"id":{"d":702,"n":10,"t":3},"speaker":"B","text":"実は私も熱川バナナワニ園が好きなのです。レッサーパンダやマナティーもいて、動物園としても見どころがあります。"},{"id":{"d":702,"n":11,"t":3},"speaker":"A","text":"その上に熱帯植物の植物園としても優れています。動植物好きにとっては、まさに一石二鳥の観光スポットです。"}];
INSERT INTO sentence [{"id":{"d":703,"n":1,"t":3},"speaker":"A","text":"東京近郊で1泊の旅行ならばどこが良いでしょうか？"},{"id":{"d":703,"n":2,"t":3},"speaker":"B","text":"私は三浦半島と城ヶ島の1泊旅行を推薦します。"},{"id":{"d":703,"n":3,"t":3},"speaker":"A","text":"三浦半島なら日帰りでもよいと思うのですが、泊まっても見るべきものがありますか？"},{"id":{"d":703,"n":4,"t":3},"speaker":"B","text":"逗子の近くの立石は安藤広重の浮世絵で有名で、葉山公園も相模湾と江ノ島そして富士山を一望することができます。それらは泊まってでも見る価値があると思います。"},{"id":{"d":703,"n":5,"t":3},"speaker":"A","text":"なるほど。横須賀も三笠公園やうみかぜ公園と景観の良い公園がありますね。"},{"id":{"d":703,"n":6,"t":3},"speaker":"B","text":"城ヶ島公園や安房埼灯台、城ヶ島灯台、馬の背洞門、水中観光船で海の中を見ることもできます。"},{"id":{"d":703,"n":7,"t":3},"speaker":"A","text":"しかし、東京からは京急線とバスで2時間もあれば城ヶ島に着きます。それでも宿泊した方がいいのでしょうか？"},{"id":{"d":703,"n":8,"t":3},"speaker":"B","text":"私は宿泊されることをお薦めします。三浦半島から見る夕日は非常に美しいです。特に城ヶ島からの景観と葉山公園の景観は素晴らしいです。"},{"id":{"d":703,"n":9,"t":3},"speaker":"A","text":"確かに、日帰りでは、落ち着いて夕日まで見ることは難しいかもしれませんね。"},{"id":{"d":703,"n":10,"t":3},"speaker":"B","text":"私は、初日に城ヶ島近辺を観光して夕日を見て宿泊し、翌日は横須賀や逗子を観光し、立石海岸か葉山公園で夕日を楽しんでから帰宅することが多いです。"},{"id":{"d":703,"n":11,"t":3},"speaker":"A","text":"それは魅力的な旅程ですね。"}];
INSERT INTO sentence [{"id":{"d":704,"n":1,"t":3},"speaker":"A","text":"チリンチリンアイスを知っていますか？"},{"id":{"d":704,"n":2,"t":3},"speaker":"B","text":"聞いたことがありません。コンビニエンスストアで売っているアイスクリームですか？"},{"id":{"d":704,"n":3,"t":3},"speaker":"A","text":"チリンチリンアイスは、長崎名物のアイスクリームです。"},{"id":{"d":704,"n":4,"t":3},"speaker":"B","text":"面白い名前ですね。"},{"id":{"d":704,"n":5,"t":3},"speaker":"A","text":"屋台を押してアイスクリームを売り歩くのですが、名前は、その時に鳴らす鐘の音から由来しているそうです。カランカランアイスと呼ばれることもあるそうですよ。"},{"id":{"d":704,"n":6,"t":3},"speaker":"B","text":"長崎のどこでチリンチリンアイスは売られているのですか？"},{"id":{"d":704,"n":7,"t":3},"speaker":"A","text":"運動会やイベントで出店することもありますし、駅や路上でも見かけることがあるそうです。"},{"id":{"d":704,"n":8,"t":3},"speaker":"B","text":"チリンチリンアイスはどのようなアイスクリームなのでしょうか？"},{"id":{"d":704,"n":9,"t":3},"speaker":"A","text":"見た目は、普通のアイスクリームです。コーンの上にアイスクリームがのっています。"},{"id":{"d":704,"n":10,"t":3},"speaker":"B","text":"アイスクリームは何味なのですか？"},{"id":{"d":704,"n":11,"t":3},"speaker":"A","text":"一番の定番はバニラでしょうか。他にも、チョコ味やバナナ味など、屋台によってさまざまな味があるようです。"}];
INSERT INTO sentence [{"id":{"d":705,"n":1,"t":3},"speaker":"A","text":"秋田のババヘラアイスは知っていますか？"},{"id":{"d":705,"n":2,"t":3},"speaker":"B","text":"分かりませんが、なんだか辛そうなアイスクリームですね。"},{"id":{"d":705,"n":3,"t":3},"speaker":"A","text":"それはハバネロですね。ババヘラアイスは秋田では有名なアイスクリームです。"},{"id":{"d":705,"n":4,"t":3},"speaker":"B","text":"ババヘラアイスは秋田のどこで売られているのですか？"},{"id":{"d":705,"n":5,"t":3},"speaker":"A","text":"道路脇やスーパーマーケットの駐車場、道の駅やイベント会場などで売られています。売り場にはビーチパラソルが立っていることが多いので、それが目印です。"},{"id":{"d":705,"n":6,"t":3},"speaker":"B","text":"秋田のいろいろなところで売られているのですね。"},{"id":{"d":705,"n":7,"t":3},"speaker":"A","text":"はい。ババヘラアイスを販売している業者は5社以上あるそうです。"},{"id":{"d":705,"n":8,"t":3},"speaker":"B","text":"どのようなアイスクリームなのですか？"},{"id":{"d":705,"n":9,"t":3},"speaker":"A","text":"いちご味、バナナ味などがあり、ジェラートのような食感です。一般には、バラの花びらのようにきれいに盛り付けられています。"},{"id":{"d":705,"n":10,"t":3},"speaker":"B","text":"一度食べてみたいものです。ババヘラアイスは秋田まで行かないと買えませんか？"},{"id":{"d":705,"n":11,"t":3},"speaker":"A","text":"通信販売をしている業者もありますよ。でも、ぜひとも現地で食べてみてほしいです。"}];
INSERT INTO sentence [{"id":{"d":706,"n":1,"t":3},"speaker":"A","text":"今度の週末に銚子までドライブをしませんか？"},{"id":{"d":706,"n":2,"t":3},"speaker":"B","text":"はい。いつも通り二人で運転を交代しながら目的地に向かうのですね。今回は日帰りですか？"},{"id":{"d":706,"n":3,"t":3},"speaker":"A","text":"いえ、地酒で有名な地元の料理店を予約したので、ビジネスホテルにでも1泊しましょう。"},{"id":{"d":706,"n":4,"t":3},"speaker":"B","text":"今回のドライブの目的はその料理店ですか？"},{"id":{"d":706,"n":5,"t":3},"speaker":"A","text":"はい。その料理店ではアブラボウズとメヌケといわれる深海魚を調理してくれます。"},{"id":{"d":706,"n":6,"t":3},"speaker":"B","text":"どちらも初めて聞く名前です。その深海魚はおいしいのですか？"},{"id":{"d":706,"n":7,"t":3},"speaker":"A","text":"はい。アブラボウズは脂が多いので食べ過ぎない方がいいのですが、おいしいと評判です。メヌケはめったに手に入らない高級魚ですが、とてもおいしいです。"},{"id":{"d":706,"n":8,"t":3},"speaker":"B","text":"銚子は漁港ですから海の食材が豊富なのですね。1泊してお酒を酌み交わすのも良いですね。"},{"id":{"d":706,"n":9,"t":3},"speaker":"A","text":"屛風ヶ浦や犬吠埼灯台にも行くつもりですが、他にも行きたいところはありますか？"},{"id":{"d":706,"n":10,"t":3},"speaker":"B","text":"銚子には展望台のあるツインタワーが存在したと思います。そこに行ってみたいです。"},{"id":{"d":706,"n":11,"t":3},"speaker":"A","text":"銚子ポートタワーですね、行きましょう。近くに水産物即売センターのウオッセ21もありますよ。"}];
INSERT INTO sentence [{"id":{"d":707,"n":1,"t":3},"speaker":"A","text":"修学旅行以外で日光に行ったことはありますか？"},{"id":{"d":707,"n":2,"t":3},"speaker":"B","text":"いいえ。小学校の修学旅行以来、日光には行っていません。"},{"id":{"d":707,"n":3,"t":3},"speaker":"A","text":"私もそうです。京都には修学旅行以外でも何度か訪れているのですが。"},{"id":{"d":707,"n":4,"t":3},"speaker":"B","text":"京都や奈良には、修学旅行の数日間だけでは見切れないほどの魅力的な観光スポットがありますからね。"},{"id":{"d":707,"n":5,"t":3},"speaker":"A","text":"そうですね。日光も修学旅行で使われるくらいですから魅力的な観光スポットがいくつもありそうですが、すぐに思い浮かぶものが日光東照宮くらいしかありません。"},{"id":{"d":707,"n":6,"t":3},"speaker":"B","text":"日光には、日光東照宮以外にも、戦場ヶ原や華厳の滝など、美しい景観が見られる観光スポットがありますよ。"},{"id":{"d":707,"n":7,"t":3},"speaker":"A","text":"詳しいのですね。修学旅行で行かれたのですか？"},{"id":{"d":707,"n":8,"t":3},"speaker":"B","text":"いいえ、修学旅行ではそれこそ日光東照宮にしか行きませんでした。大人になって、大河ドラマや時代劇を見るようになってから、知識が増えたのだと思います。日光は、徳川家の歴史に深く関わる土地なのですよ。"},{"id":{"d":707,"n":9,"t":3},"speaker":"A","text":"そうだったのですね。あなたと一緒に日光を観光したらとても楽しそうです。案内してもらえませんか？"},{"id":{"d":707,"n":10,"t":3},"speaker":"B","text":"私で良ければ喜んでご案内しますよ。ぜひ行きましょう。"},{"id":{"d":707,"n":11,"t":3},"speaker":"A","text":"ありがとうございます。日光へは、東京から私鉄でも2時間ほどで到着するのですね。"},{"id":{"d":707,"n":12,"t":3},"speaker":"B","text":"案外近いのですね。日光を観光した後、鬼怒川温泉に行くのも良いですね。"},{"id":{"d":707,"n":13,"t":3},"speaker":"A","text":"それはすてきな計画です。"}];
INSERT INTO sentence [{"id":{"d":708,"n":1,"t":3},"speaker":"A","text":"しょうゆに凝っているそうですね。"},{"id":{"d":708,"n":2,"t":3},"speaker":"B","text":"はい。一口にしょうゆといっても、その味は商品ごとに随分違うのですよ。"},{"id":{"d":708,"n":3,"t":3},"speaker":"A","text":"しょうゆに関しては、関東と関西で違いがあることを知っている程度です。"},{"id":{"d":708,"n":4,"t":3},"speaker":"B","text":"関東の濃口しょうゆと関西の薄口しょうゆのことですね。実は、しょうゆメーカーは全国で1,000社以上もあります。"},{"id":{"d":708,"n":5,"t":3},"speaker":"A","text":"スーパーなどで入手できる大手しょうゆメーカー以外のしょうゆは、どのようにして購入しているのですか？"},{"id":{"d":708,"n":6,"t":3},"speaker":"B","text":"通販サイトで、かなりの数のしょうゆが購入できます。"},{"id":{"d":708,"n":7,"t":3},"speaker":"A","text":"それではインターネットでしょうゆを購入しているのですか？"},{"id":{"d":708,"n":8,"t":3},"speaker":"B","text":"それもありますが、私は旅行も好きなので現地に行って直接しょうゆを購入することもあります。"},{"id":{"d":708,"n":9,"t":3},"speaker":"A","text":"しょうゆの街というと野田市、銚子市が有名ですよね？"},{"id":{"d":708,"n":10,"t":3},"speaker":"B","text":"そうですね。他に小豆島や金沢市、兵庫県のたつの市も有名です。富山や岐阜、京都や奈良、福岡には中小のしょうゆメーカーがたくさんあります。"},{"id":{"d":708,"n":11,"t":3},"speaker":"A","text":"しょうゆを求めるだけでも、いろいろな場所を旅行することになるのですね。"}];
INSERT INTO sentence [{"id":{"d":709,"n":1,"t":3},"speaker":"A","text":"レンタカーを返す前にガソリンを入れなければなりません。"},{"id":{"d":709,"n":2,"t":3},"speaker":"B","text":"はい。ガソリンスタンドは、どこにあるか分かりますか？"},{"id":{"d":709,"n":3,"t":3},"speaker":"A","text":"カーナビゲーションシステムで調べれば分かると思います。調べてみますね。"},{"id":{"d":709,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。近くにガソリンスタンドはありましたか？"},{"id":{"d":709,"n":5,"t":3},"speaker":"A","text":"ありましたが、ここから一番近いガソリンスタンドは、ガソリンの値段が高いです。"},{"id":{"d":709,"n":6,"t":3},"speaker":"B","text":"カーナビゲーションシステムでガソリンの値段まで分かるのですか？"},{"id":{"d":709,"n":7,"t":3},"speaker":"A","text":"はい。最近のカーナビゲーションシステムは優秀ですよね。"},{"id":{"d":709,"n":8,"t":3},"speaker":"B","text":"便利ですね。この近くに、もう少しガソリンの値段が安いガソリンスタンドはありますか？"},{"id":{"d":709,"n":9,"t":3},"speaker":"A","text":"いいえ。近くのガソリンスタンドはその1軒しかないようです。"},{"id":{"d":709,"n":10,"t":3},"speaker":"B","text":"そうですか。では、レンタカーを返す時間も迫っているので、そこに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":710,"n":1,"t":3},"speaker":"A","text":"ホテルで主催しているミニツアーに参加したいのですが。"},{"id":{"d":710,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。3コースございますが、どちらになさいますか？"},{"id":{"d":710,"n":3,"t":3},"speaker":"A","text":"お花畑満喫ツアーをお願いします。"},{"id":{"d":710,"n":4,"t":3},"speaker":"B","text":"何名さまのご参加ですか？"},{"id":{"d":710,"n":5,"t":3},"speaker":"A","text":"大人2名でお願いします。"},{"id":{"d":710,"n":6,"t":3},"speaker":"B","text":"承知いたしました。料金はお2人さまで2,000円でございます。"},{"id":{"d":710,"n":7,"t":3},"speaker":"A","text":"はい、2,000円ですね。"},{"id":{"d":710,"n":8,"t":3},"speaker":"B","text":"頂戴します。明日の朝9時にロビーに集合するようにお願いします。"}];
INSERT INTO sentence [{"id":{"d":711,"n":1,"t":3},"speaker":"A","text":"お花畑満喫ツアーは良かったですね。"},{"id":{"d":711,"n":2,"t":3},"speaker":"B","text":"ええ。すてきな花畑に何カ所も連れて行ってもらって、本当に満喫できました。"},{"id":{"d":711,"n":3,"t":3},"speaker":"A","text":"写真もたくさん撮れましたね。"},{"id":{"d":711,"n":4,"t":3},"speaker":"B","text":"100枚は撮ったと思いますよ。"},{"id":{"d":711,"n":5,"t":3},"speaker":"A","text":"お土産にお花の種をくれたのもうれしかったです。帰ったらどこに種をまこうかと考えています。"},{"id":{"d":711,"n":6,"t":3},"speaker":"B","text":"私はお土産のお花の形のクッキーがうれしかったです。"}];
INSERT INTO sentence [{"id":{"d":712,"n":1,"t":3},"speaker":"A","text":"今度の週末に、横浜の中華街に行きませんか？"},{"id":{"d":712,"n":2,"t":3},"speaker":"B","text":"行けません。土曜日は仕事が入っています。"},{"id":{"d":712,"n":3,"t":3},"speaker":"A","text":"では、日曜日はどうですか？"},{"id":{"d":712,"n":4,"t":3},"speaker":"B","text":"日曜日のお昼の時間帯でしたら大丈夫です。"},{"id":{"d":712,"n":5,"t":3},"speaker":"A","text":"上海ガニを食べたいのですが、私の希望するお店を予約しても大丈夫ですか？"},{"id":{"d":712,"n":6,"t":3},"speaker":"B","text":"はい、お願いします。予算はいくらくらいですか？"},{"id":{"d":712,"n":7,"t":3},"speaker":"A","text":"5,000円くらいになると思います。"},{"id":{"d":712,"n":8,"t":3},"speaker":"B","text":"分かりました。飲み放題コースも付けてください。"}];
INSERT INTO sentence [{"id":{"d":713,"n":1,"t":3},"speaker":"A","text":"そろそろ昼食にしませんか？"},{"id":{"d":713,"n":2,"t":3},"speaker":"B","text":"いいですね。昼食は何にしますか？"},{"id":{"d":713,"n":3,"t":3},"speaker":"A","text":"この先にうどん街道があるのですが、そこでうどんを食べるというのはどうですか？"},{"id":{"d":713,"n":4,"t":3},"speaker":"B","text":"うどん街道という道があるのですね。"},{"id":{"d":713,"n":5,"t":3},"speaker":"A","text":"はい。うどん屋さんがたくさん並んでいる道です。"},{"id":{"d":713,"n":6,"t":3},"speaker":"B","text":"どこのお店にするか迷いそうですね。"},{"id":{"d":713,"n":7,"t":3},"speaker":"A","text":"ガイドブックを開くので、目をつぶって指を指したところに行きましょう。"},{"id":{"d":713,"n":8,"t":3},"speaker":"B","text":"面白いですね。では、指しますよ。"},{"id":{"d":713,"n":9,"t":3},"speaker":"A","text":"はい、お願いします。"},{"id":{"d":713,"n":10,"t":3},"speaker":"B","text":"指しました。このお店に決定です。"}];
INSERT INTO sentence [{"id":{"d":714,"n":1,"t":3},"speaker":"A","text":"水沢うどんはおいしかったですね。"},{"id":{"d":714,"n":2,"t":3},"speaker":"B","text":"はい。水沢うどんは、讃岐と稲庭と並んで三大うどんといわれているそうですね。"},{"id":{"d":714,"n":3,"t":3},"speaker":"A","text":"有名なのですね。適当に選んだ店なのに絶品でした。"},{"id":{"d":714,"n":4,"t":3},"speaker":"B","text":"きっとどこのお店も同じくらいおいしいのだと思いますよ。"},{"id":{"d":714,"n":5,"t":3},"speaker":"A","text":"マイタケの天ぷらもおいしかったです。"},{"id":{"d":714,"n":6,"t":3},"speaker":"B","text":"食べたことがないほど肉厚のマイタケでしたね。"},{"id":{"d":714,"n":7,"t":3},"speaker":"A","text":"天然物なのでしょうか？"},{"id":{"d":714,"n":8,"t":3},"speaker":"B","text":"どうでしょうか。でも、天然物はあのお値段では出せないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":715,"n":1,"t":3},"speaker":"A","text":"伊香保温泉の街中は階段が多くて、歩くのが大変ですね。"},{"id":{"d":715,"n":2,"t":3},"speaker":"B","text":"そうですね。でも、昼食を食べ過ぎた私たちにとっては、ちょうど良い運動です。"},{"id":{"d":715,"n":3,"t":3},"speaker":"A","text":"昼食だけでなく、その後に温泉まんじゅうやらソフトクリームやらも食べましたよね。"},{"id":{"d":715,"n":4,"t":3},"speaker":"B","text":"はい。昼食の分は最初の150段、おまんじゅうは次の30段、ソフトクリームは次の30段で消化できますよ。"},{"id":{"d":715,"n":5,"t":3},"speaker":"A","text":"今日は、この後さらに150段は上る予定なので、その分でまた何かが食べられますね。"},{"id":{"d":715,"n":6,"t":3},"speaker":"B","text":"その通りです。次は何にしようか迷います。"},{"id":{"d":715,"n":7,"t":3},"speaker":"A","text":"やはりまだ食べる気なのですね。"},{"id":{"d":715,"n":8,"t":3},"speaker":"B","text":"もちろんです。あ、あそこで玉こんにゃくが売っていますよ。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":716,"n":1,"t":3},"speaker":"A","text":"もしもし？私です。"},{"id":{"d":716,"n":2,"t":3},"speaker":"B","text":"もしもし、真美さんですか？温泉は楽しんでいますか？"},{"id":{"d":716,"n":3,"t":3},"speaker":"A","text":"おかげさまで楽しんでいます。そちらは大丈夫ですか？"},{"id":{"d":716,"n":4,"t":3},"speaker":"B","text":"はい。何も変わったことはありませんよ。"},{"id":{"d":716,"n":5,"t":3},"speaker":"A","text":"それは良かったです。明日には帰るので、寂しくても我慢してくださいね。"},{"id":{"d":716,"n":6,"t":3},"speaker":"B","text":"ポチがいるから寂しくありません。気を付けて帰ってきてください。"},{"id":{"d":716,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。お土産においしいものをたくさん買って帰りますね。"},{"id":{"d":716,"n":8,"t":3},"speaker":"B","text":"うれしいです。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":717,"n":1,"t":3},"speaker":"A","text":"こちらでマス釣りができると聞いたのですが、受付はこちらですか？"},{"id":{"d":717,"n":2,"t":3},"speaker":"B","text":"はい。いらっしゃいませ。半日券と一日券がありますがどちらになさいますか？"},{"id":{"d":717,"n":3,"t":3},"speaker":"A","text":"半日券を、大人2人分お願いします。"},{"id":{"d":717,"n":4,"t":3},"speaker":"B","text":"かしこまりました。道具のレンタルと餌はいかがいたしましょう？"},{"id":{"d":717,"n":5,"t":3},"speaker":"A","text":"竿2本をお願いします。餌は虫ですか？"},{"id":{"d":717,"n":6,"t":3},"speaker":"B","text":"イクラもありますが、ブドウ虫もあります。"},{"id":{"d":717,"n":7,"t":3},"speaker":"A","text":"初心者なので虫は避けたいです。"},{"id":{"d":717,"n":8,"t":3},"speaker":"B","text":"それではイクラをご用意します。"}];
INSERT INTO sentence [{"id":{"d":718,"n":1,"t":3},"speaker":"A","text":"マス釣りは楽しかったですね。"},{"id":{"d":718,"n":2,"t":3},"speaker":"B","text":"はい。初めてなのに5匹ずつ釣れるなんて大漁ですね。"},{"id":{"d":718,"n":3,"t":3},"speaker":"A","text":"イクラでも釣れるものなのですね。"},{"id":{"d":718,"n":4,"t":3},"speaker":"B","text":"そうですね。私は虫が苦手なので、イクラがあって助かりました。"},{"id":{"d":718,"n":5,"t":3},"speaker":"A","text":"釣りの後に、釣った魚をバーベキューで食べたのも楽しかったです。"},{"id":{"d":718,"n":6,"t":3},"speaker":"B","text":"楽しかったですね。魚のさばき方も教えてもらえて勉強になりました。"},{"id":{"d":718,"n":7,"t":3},"speaker":"A","text":"バーベキューで食べきれず持って帰ってきたこの魚は、冷凍しておいても大丈夫でしょうか？"},{"id":{"d":718,"n":8,"t":3},"speaker":"B","text":"はい。大丈夫だと思います。"}];
INSERT INTO sentence [{"id":{"d":719,"n":1,"t":3},"speaker":"A","text":"天橋立に行ってきたそうですね？"},{"id":{"d":719,"n":2,"t":3},"speaker":"B","text":"はい。ツアーで行ってきたのですが、有名な股のぞきのところにも立ち寄りました。"},{"id":{"d":719,"n":3,"t":3},"speaker":"A","text":"実際に見た天橋立はいかがでしたか？"},{"id":{"d":719,"n":4,"t":3},"speaker":"B","text":"それが、残念ながら肉眼では見られませんでした。"},{"id":{"d":719,"n":5,"t":3},"speaker":"A","text":"なぜですか？霧でも出ていましたか？"},{"id":{"d":719,"n":6,"t":3},"speaker":"B","text":"いいえ、体が硬くて股の間から見られませんでした。"},{"id":{"d":719,"n":7,"t":3},"speaker":"A","text":"それは残念でしたね。"},{"id":{"d":719,"n":8,"t":3},"speaker":"B","text":"仕方がないのでスマートフォンで写真を撮って逆さにして見ましたよ。"}];
INSERT INTO sentence [{"id":{"d":720,"n":1,"t":3},"speaker":"A","text":"京都のお土産です。どうぞ。"},{"id":{"d":720,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。生八つ橋ですね。久しぶりに頂くのでうれしいです。"},{"id":{"d":720,"n":3,"t":3},"speaker":"A","text":"良かったら、こちらもどうぞ。"},{"id":{"d":720,"n":4,"t":3},"speaker":"B","text":"これはチョコバーですか？"},{"id":{"d":720,"n":5,"t":3},"speaker":"A","text":"そうです。有名なチョコレート菓子ですけれど、京都限定の商品なのです。"},{"id":{"d":720,"n":6,"t":3},"speaker":"B","text":"抹茶味なのですね。"},{"id":{"d":720,"n":7,"t":3},"speaker":"A","text":"1つ食べましたが、抹茶が濃くておいしいですよ。お好きな数だけどうぞ。"},{"id":{"d":720,"n":8,"t":3},"speaker":"B","text":"家族にも食べさせたいので3個頂いてもよろしいですか？"},{"id":{"d":720,"n":9,"t":3},"speaker":"A","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":721,"n":1,"t":3},"speaker":"A","text":"この間は京都のお土産をありがとうございました。"},{"id":{"d":721,"n":2,"t":3},"speaker":"B","text":"どういたしまして。抹茶味のチョコバーはご家族にも気に入っていただけましたか？"},{"id":{"d":721,"n":3,"t":3},"speaker":"A","text":"はい。夫が気に入って「さらにたくさん食べたい」と言うほどでした。"},{"id":{"d":721,"n":4,"t":3},"speaker":"B","text":"喜んでいただけて何よりです。"},{"id":{"d":721,"n":5,"t":3},"speaker":"A","text":"それで、夫の北海道出張のお土産がこれです。どうぞ。"},{"id":{"d":721,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。なんと、白いチョコバーですか！"},{"id":{"d":721,"n":7,"t":3},"speaker":"A","text":"はい。地域限定のチョコバーに夢中になってしまったようです。"},{"id":{"d":721,"n":8,"t":3},"speaker":"B","text":"ホワイトチョコレートもおいしそうですね。"}];
INSERT INTO sentence [{"id":{"d":722,"n":1,"t":3},"speaker":"A","text":"夏休みにどこかへ旅行に行きたいです。"},{"id":{"d":722,"n":2,"t":3},"speaker":"B","text":"パパの釣りに一緒に行きますか？"},{"id":{"d":722,"n":3,"t":3},"speaker":"A","text":"いいえ。釣りはしたくありません。"},{"id":{"d":722,"n":4,"t":3},"speaker":"B","text":"では、釣り宿のそばにある鋸山にハイキングに行くのはどうですか？"},{"id":{"d":722,"n":5,"t":3},"speaker":"A","text":"鋸山には何があるのですか？"},{"id":{"d":722,"n":6,"t":3},"speaker":"B","text":"大きな石でできた大仏様や地獄のぞきという怖い場所もありますよ。"},{"id":{"d":722,"n":7,"t":3},"speaker":"A","text":"怖そうですが、気になります。行ってみたいです。"},{"id":{"d":722,"n":8,"t":3},"speaker":"B","text":"では、一緒に行きましょう。でも、1日だけでよいので釣りに付き合ってくださいね。"},{"id":{"d":722,"n":9,"t":3},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":723,"n":1,"t":3},"speaker":"A","text":"昼食を食べるお店を探しているのですが、この辺りの名物といったら何ですか？"},{"id":{"d":723,"n":2,"t":3},"speaker":"B","text":"何といっても、ほうとうですよ。"},{"id":{"d":723,"n":3,"t":3},"speaker":"A","text":"ほうとうというのはどのようなものですか？"},{"id":{"d":723,"n":4,"t":3},"speaker":"B","text":"太めの麺をみそ味のつゆで煮込んだ煮込みうどんですよ。"},{"id":{"d":723,"n":5,"t":3},"speaker":"A","text":"体が温まりそうですね。"},{"id":{"d":723,"n":6,"t":3},"speaker":"B","text":"野菜もたっぷり入っていますからお薦めですよ。"},{"id":{"d":723,"n":7,"t":3},"speaker":"A","text":"ほうとうが食べられる、近くのお店を教えてください。"},{"id":{"d":723,"n":8,"t":3},"speaker":"B","text":"この先ののぼりが出ているところは、富士山を見ながらほうとうが食べられますよ。"},{"id":{"d":723,"n":9,"t":3},"speaker":"A","text":"教えていただきありがとうございます。行ってみます。"}];
INSERT INTO sentence [{"id":{"d":724,"n":1,"t":3},"speaker":"A","text":"晴れていたので富士山がきれいでしたね。"},{"id":{"d":724,"n":2,"t":3},"speaker":"B","text":"帰ったら富士山の写真をパソコンの壁紙にしようと思います。"},{"id":{"d":724,"n":3,"t":3},"speaker":"A","text":"それは良いですね。私も同じことをしても良いですか？"},{"id":{"d":724,"n":4,"t":3},"speaker":"B","text":"もちろん良いですよ。"},{"id":{"d":724,"n":5,"t":3},"speaker":"A","text":"仕事中も旅の思い出に浸ることができそうです。"},{"id":{"d":724,"n":6,"t":3},"speaker":"B","text":"そうですね。でも、ずっと浸っているわけにもいきませんがね。"},{"id":{"d":724,"n":7,"t":3},"speaker":"A","text":"もちろん、そこはきっちり仕事をした上でのことです。"},{"id":{"d":724,"n":8,"t":3},"speaker":"B","text":"それなら大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":725,"n":1,"t":3},"speaker":"A","text":"急いでどこに行かれるのですか？"},{"id":{"d":725,"n":2,"t":3},"speaker":"B","text":"羽田空港に友人を迎えに行くところです。"},{"id":{"d":725,"n":3,"t":3},"speaker":"A","text":"バスで行くのですか？"},{"id":{"d":725,"n":4,"t":3},"speaker":"B","text":"そのつもりです。時間が迫っているので失礼します。"},{"id":{"d":725,"n":5,"t":3},"speaker":"A","text":"少し待ってください。先ほど、テレビのニュース番組で、アナウンサーが「高速道路で事故がありました」と言っていましたよ。"},{"id":{"d":725,"n":6,"t":3},"speaker":"B","text":"本当ですか？それでは、電車で移動する方がいいでしょうか？"},{"id":{"d":725,"n":7,"t":3},"speaker":"A","text":"はい。確実に空港に着くためには、その方がいいと思います。"},{"id":{"d":725,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。そうします。"}];
INSERT INTO sentence [{"id":{"d":726,"n":1,"t":3},"speaker":"A","text":"お客さま、ご指定の駅に到着いたしました。"},{"id":{"d":726,"n":2,"t":3},"speaker":"B","text":"もう着いたのですね。少し眠ってしまいました。"},{"id":{"d":726,"n":3,"t":3},"speaker":"A","text":"料金は1,830円です。"},{"id":{"d":726,"n":4,"t":3},"speaker":"B","text":"プリペイドカードは使えますか？"},{"id":{"d":726,"n":5,"t":3},"speaker":"A","text":"お使いになれますよ。"},{"id":{"d":726,"n":6,"t":3},"speaker":"B","text":"では、プリペイドカードで支払いをお願いします。"},{"id":{"d":726,"n":7,"t":3},"speaker":"A","text":"領収書は必要ですか？"},{"id":{"d":726,"n":8,"t":3},"speaker":"B","text":"はい、お願いします。"}];
INSERT INTO sentence [{"id":{"d":727,"n":1,"t":3},"speaker":"A","text":"去年は、花火大会の開催が少なかったですね。"},{"id":{"d":727,"n":2,"t":3},"speaker":"B","text":"そうですね。今年は、去年よりも多く開催されるようですよ。"},{"id":{"d":727,"n":3,"t":3},"speaker":"A","text":"どこかの花火大会を一緒に見に行きませんか？"},{"id":{"d":727,"n":4,"t":3},"speaker":"B","text":"はい。確実に開催されるなら遠くまで行っても良いのですが、どうでしょうか？"},{"id":{"d":727,"n":5,"t":3},"speaker":"A","text":"もし中止になっても、花火大会の開催地付近で遊べるところに行けば良いですよ。"},{"id":{"d":727,"n":6,"t":3},"speaker":"B","text":"なるほど。例えば、どこですか？"},{"id":{"d":727,"n":7,"t":3},"speaker":"A","text":"横浜開港祭はどうでしょう？花火が中止でも横浜中華街で食事ができますよ。"},{"id":{"d":727,"n":8,"t":3},"speaker":"B","text":"それは良いですね。開催日時を調べましょう。"}];
INSERT INTO sentence [{"id":{"d":728,"n":1,"t":3},"speaker":"A","text":"先月予約したバスツアーに行ってきました。こちらはお土産です。"},{"id":{"d":728,"n":2,"t":3},"speaker":"B","text":"私にお土産を頂けるのですか？ありがとうございます。"},{"id":{"d":728,"n":3,"t":3},"speaker":"A","text":"どういたしまして。"},{"id":{"d":728,"n":4,"t":3},"speaker":"B","text":"ツアーはどうでしたか？"},{"id":{"d":728,"n":5,"t":3},"speaker":"A","text":"お花見ツアーだったのですが、今年は暖冬でしたでしょう？"},{"id":{"d":728,"n":6,"t":3},"speaker":"B","text":"そうですね。"},{"id":{"d":728,"n":7,"t":3},"speaker":"A","text":"京都の桜はもうほとんど散ってしまっていたのです。"},{"id":{"d":728,"n":8,"t":3},"speaker":"B","text":"それは残念でしたね。"},{"id":{"d":728,"n":9,"t":3},"speaker":"A","text":"はい。お花見の時期を予測するのは難しいですね。"},{"id":{"d":728,"n":10,"t":3},"speaker":"B","text":"そうですね。桜を見たいのであれば、青森や北海道なら開花時期にまだ間に合いますよ。"}];
INSERT INTO sentence [{"id":{"d":729,"n":1,"t":3},"speaker":"A","text":"バスツアーでは、昼食が食べ放題でした。"},{"id":{"d":729,"n":2,"t":3},"speaker":"B","text":"どのような食べ放題だったのですか？"},{"id":{"d":729,"n":3,"t":3},"speaker":"A","text":"カニやローストビーフもある豪華なビュッフェでした。"},{"id":{"d":729,"n":4,"t":3},"speaker":"B","text":"それは良いですね。ホテルのビュッフェですか？"},{"id":{"d":729,"n":5,"t":3},"speaker":"A","text":"そうです。鯉が泳いでいる池を見ながら食べてきました。"},{"id":{"d":729,"n":6,"t":3},"speaker":"B","text":"それは優雅ですね。"}];
INSERT INTO sentence [{"id":{"d":730,"n":1,"t":3},"speaker":"A","text":"お母さん、どうしてこの信玄餅はつぶれているのですか？"},{"id":{"d":730,"n":2,"t":3},"speaker":"B","text":"それは詰め放題の戦利品だからですよ。"},{"id":{"d":730,"n":3,"t":3},"speaker":"A","text":"バスツアーではそのようなサービスもあるのですか？"},{"id":{"d":730,"n":4,"t":3},"speaker":"B","text":"バスツアーで寄ったお店で、イベントをしていたのです。"},{"id":{"d":730,"n":5,"t":3},"speaker":"A","text":"なるほど。外装がつぶれていても味は変わりませんしね。"},{"id":{"d":730,"n":6,"t":3},"speaker":"B","text":"そうでしょう。でも、ご近所の佐藤さんや前田さんに差し上げる分は、きちんとしたものを買ってきました。"},{"id":{"d":730,"n":7,"t":3},"speaker":"A","text":"私の分はつぶれたものだけですか？"},{"id":{"d":730,"n":8,"t":3},"speaker":"B","text":"はい。数は4倍もあるのですから、それで良いでしょう。当分、毎日信玄餅を食べられますよ。"}];
INSERT INTO sentence [{"id":{"d":731,"n":1,"t":3},"speaker":"A","text":"今度、サークルでいすみ鉄道の撮影旅行に行く予定です。"},{"id":{"d":731,"n":2,"t":3},"speaker":"B","text":"撮り鉄研究会は遠出もするのですか？"},{"id":{"d":731,"n":3,"t":3},"speaker":"A","text":"はい。鉄の道があればどこへでも行く者が撮り鉄です。"},{"id":{"d":731,"n":4,"t":3},"speaker":"B","text":"昨年、時刻表の表紙に、撮影した写真が採用された先輩がいらしたそうですね。"},{"id":{"d":731,"n":5,"t":3},"speaker":"A","text":"はい。富士山と新幹線の写真ですね。きれいな写真でした。"},{"id":{"d":731,"n":6,"t":3},"speaker":"B","text":"表紙に採用されるほどの写真を撮影するのは難しいかもしれませんが、良い写真を撮ってきてくださいね。"},{"id":{"d":731,"n":7,"t":3},"speaker":"A","text":"桜と菜の花が一緒に撮れるらしいので頑張ります。"},{"id":{"d":731,"n":8,"t":3},"speaker":"B","text":"お土産も楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":732,"n":1,"t":3},"speaker":"A","text":"いすみ鉄道に乗ってきました。"},{"id":{"d":732,"n":2,"t":3},"speaker":"B","text":"楽しかったですか？"},{"id":{"d":732,"n":3,"t":3},"speaker":"A","text":"それはもう楽しかったです。この写真を見てください。"},{"id":{"d":732,"n":4,"t":3},"speaker":"B","text":"菜の花と桜と電車ですね。電車が手振れすることなく、よく撮れています。"},{"id":{"d":732,"n":5,"t":3},"speaker":"A","text":"今まで撮ったなかで一番うまく撮れました。"},{"id":{"d":732,"n":6,"t":3},"speaker":"B","text":"これは一眼レフカメラで撮ったのですか？"},{"id":{"d":732,"n":7,"t":3},"speaker":"A","text":"そうです。この時のために新しく買いました。"},{"id":{"d":732,"n":8,"t":3},"speaker":"B","text":"それは一眼レフカメラを買ったかいがありましたね。長く愛用してくださいね。"}];
INSERT INTO sentence [{"id":{"d":733,"n":1,"t":3},"speaker":"A","text":"温泉街を散策してきます。"},{"id":{"d":733,"n":2,"t":3},"speaker":"B","text":"かしこまりました。ルームキーをお預かりしますね。"},{"id":{"d":733,"n":3,"t":3},"speaker":"A","text":"温泉街でお薦めのバーはありませんか？"},{"id":{"d":733,"n":4,"t":3},"speaker":"B","text":"はい。クラフトビールの醸造所が併設されているバーがございます。"},{"id":{"d":733,"n":5,"t":3},"speaker":"A","text":"それは良いですね。場所はどこですか？"},{"id":{"d":733,"n":6,"t":3},"speaker":"B","text":"こちらの地図をご覧ください。当旅館はこちらで、バーはこの位置です。"},{"id":{"d":733,"n":7,"t":3},"speaker":"A","text":"それほど遠くはありませんね。行ってみます。"},{"id":{"d":733,"n":8,"t":3},"speaker":"B","text":"お気を付けて行ってらっしゃいませ。"}];
INSERT INTO sentence [{"id":{"d":734,"n":1,"t":3},"speaker":"A","text":"湯けむり横丁で飲み過ぎてしまいました。"},{"id":{"d":734,"n":2,"t":3},"speaker":"B","text":"はい。つまみもおいしくて、ついついお酒が進んでしましました。おなかがいっぱいです。"},{"id":{"d":734,"n":3,"t":3},"speaker":"A","text":"ホテルのディナーも引かれましたけど、素泊まりにして正解でしたね。"},{"id":{"d":734,"n":4,"t":3},"speaker":"B","text":"そうですね。"},{"id":{"d":734,"n":5,"t":3},"speaker":"A","text":"飲み屋で出会った地元の方にこの近くの見どころも教えていただいて、明日の予定も立ちました。"},{"id":{"d":734,"n":6,"t":3},"speaker":"B","text":"はい。明日は三薬師巡りですね。"}];
INSERT INTO sentence [{"id":{"d":735,"n":1,"t":3},"speaker":"A","text":"東京からの日帰り旅でこのようなコースがあるとは、驚きました。"},{"id":{"d":735,"n":2,"t":3},"speaker":"B","text":"私たち、日本酒好きには最高の旅でしょう？"},{"id":{"d":735,"n":3,"t":3},"speaker":"A","text":"まさか駅構内から出ないで1日過ごす旅だとは、思いつきませんよ。"},{"id":{"d":735,"n":4,"t":3},"speaker":"B","text":"面白いでしょう。越後湯沢駅のぽんしゅ館は最高ですよね。"},{"id":{"d":735,"n":5,"t":3},"speaker":"A","text":"はい。日本酒も飲みましたし、大きなおにぎりも食べましたし、お風呂にまで入りました。"},{"id":{"d":735,"n":6,"t":3},"speaker":"B","text":"新幹線を使えば、東京駅から1時間少しでたどり着く天国です。"},{"id":{"d":735,"n":7,"t":3},"speaker":"A","text":"お土産品にも日本酒味のものがたくさんありましたね。"},{"id":{"d":735,"n":8,"t":3},"speaker":"B","text":"ええ。お土産として購入してきた酒カステラを食べることが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":736,"n":1,"t":3},"speaker":"A","text":"昨夜はよく眠れました。充実した日帰り旅行のおかげでしょうか？"},{"id":{"d":736,"n":2,"t":3},"speaker":"B","text":"そうですね。旅行先で酒風呂に入ったことが良かったのではありませんか？"},{"id":{"d":736,"n":3,"t":3},"speaker":"A","text":"そう思いますか？実は私もそう思っていました。"},{"id":{"d":736,"n":4,"t":3},"speaker":"B","text":"私もよく眠れたのです。"},{"id":{"d":736,"n":5,"t":3},"speaker":"A","text":"酒風呂はぜいたく過ぎて、家ではできませんよね。"},{"id":{"d":736,"n":6,"t":3},"speaker":"B","text":"お肌の調子も良いようですし、できれば家でもしたいですよね。"},{"id":{"d":736,"n":7,"t":3},"speaker":"A","text":"ええ、そう思うほどに素晴らしい体験でした。日帰り旅行にもぜひまた出掛けましょうね。"},{"id":{"d":736,"n":8,"t":3},"speaker":"B","text":"賛成です。今度は観光もしましょう。"}];
INSERT INTO sentence [{"id":{"d":737,"n":1,"t":3},"speaker":"A","text":"ひたち海浜公園に行きたいのですが、どの電車に乗ったらよいですか？"},{"id":{"d":737,"n":2,"t":3},"speaker":"B","text":"JR常磐線に乗って勝田駅で降りてください。"},{"id":{"d":737,"n":3,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":737,"n":4,"t":3},"speaker":"B","text":"普通電車に乗るとかなり時間がかかりますので、特急ひたちに乗ることをお勧めします。"},{"id":{"d":737,"n":5,"t":3},"speaker":"A","text":"特急ですか？"},{"id":{"d":737,"n":6,"t":3},"speaker":"B","text":"はい。特急に乗っても、勝田駅まで行くには90分近くかかります。"},{"id":{"d":737,"n":7,"t":3},"speaker":"A","text":"結構遠いのですね。では、特急券を買いに行きます。"},{"id":{"d":737,"n":8,"t":3},"speaker":"B","text":"そちらの自動販売機で買えますよ。"}];
INSERT INTO sentence [{"id":{"d":738,"n":1,"t":3},"speaker":"A","text":"美術館に行きませんか？"},{"id":{"d":738,"n":2,"t":3},"speaker":"B","text":"良いですね。何か魅力的な展覧会が行われているのですか？"},{"id":{"d":738,"n":3,"t":3},"speaker":"A","text":"はい。上野にある美術館でゴッホ展が開かれるのです。"},{"id":{"d":738,"n":4,"t":3},"speaker":"B","text":"ゴッホは私も好きです。いつから始まるのですか？"},{"id":{"d":738,"n":5,"t":3},"speaker":"A","text":"来週の火曜日からです。"},{"id":{"d":738,"n":6,"t":3},"speaker":"B","text":"土曜日と日曜日は混雑するでしょうね。"},{"id":{"d":738,"n":7,"t":3},"speaker":"A","text":"そうだと思います。"},{"id":{"d":738,"n":8,"t":3},"speaker":"B","text":"有給休暇を取って、平日に行きましょうか？"}];
INSERT INTO sentence [{"id":{"d":739,"n":1,"t":3},"speaker":"A","text":"上野公園は見るところがたくさんあって楽しいですね。"},{"id":{"d":739,"n":2,"t":3},"speaker":"B","text":"そうですね。気づけば、美術館と博物館巡りをしてしまいましたね。"},{"id":{"d":739,"n":3,"t":3},"speaker":"A","text":"たくさん歩いたので足がむくんでしまいました。"},{"id":{"d":739,"n":4,"t":3},"speaker":"B","text":"では、お茶でも飲みながら休みませんか？"},{"id":{"d":739,"n":5,"t":3},"speaker":"A","text":"そうしましょう。私はおなかがすいているので、何かフードメニューもあるとうれしいです。"},{"id":{"d":739,"n":6,"t":3},"speaker":"B","text":"そこのレストランでは、パンダカレーというものが食べられるようですよ。"},{"id":{"d":739,"n":7,"t":3},"speaker":"A","text":"かわいらしいですね。そこへ行きましょう。"},{"id":{"d":739,"n":8,"t":3},"speaker":"B","text":"はい。私はカフェラテを注文しようと思います。"}];
INSERT INTO sentence [{"id":{"d":740,"n":1,"t":3},"speaker":"A","text":"東京ドイツ村は、東京にあるわけではないのですね。"},{"id":{"d":740,"n":2,"t":3},"speaker":"B","text":"そうなのです。千葉県にあるのですよ。"},{"id":{"d":740,"n":3,"t":3},"speaker":"A","text":"東京ドイツ村へ向かっていた車が、いきなり東京湾アクアラインに入ったときは驚きました。"},{"id":{"d":740,"n":4,"t":3},"speaker":"B","text":"良いドライブでしたでしょう？"},{"id":{"d":740,"n":5,"t":3},"speaker":"A","text":"確かに、そうですね。海ほたるでの休憩も楽しめました。目的地まではあとどのくらいですか？"},{"id":{"d":740,"n":6,"t":3},"speaker":"B","text":"もうすぐ着きますよ。ドイツ村ではソーセージを食べましょう。"},{"id":{"d":740,"n":7,"t":3},"speaker":"A","text":"良いですね。私はビールをいただいても良いですか？"},{"id":{"d":740,"n":8,"t":3},"speaker":"B","text":"はい。私はお酒が飲めないので飲みませんが、遠慮しないで飲んで良いですよ。"}];
INSERT INTO sentence [{"id":{"d":741,"n":1,"t":3},"speaker":"A","text":"東京ドイツ村は花壇もきれいなのですね。"},{"id":{"d":741,"n":2,"t":3},"speaker":"B","text":"はい、花壇は見どころの一つです。春先は特にきれいですよ。"},{"id":{"d":741,"n":3,"t":3},"speaker":"A","text":"子どもが遊ぶ場所が多いのかと思いましたが、お花は大人も楽しめますね。"},{"id":{"d":741,"n":4,"t":3},"speaker":"B","text":"そうですね。日も暮れてきましたし、そろそろ帰りましょうか？"},{"id":{"d":741,"n":5,"t":3},"speaker":"A","text":"そうしましょう。今日の記念に、先ほどレザークラフト体験で作ったキーホルダーを差し上げます。"},{"id":{"d":741,"n":6,"t":3},"speaker":"B","text":"良いのですか？私が作ったものよりずっときれいにできていますのに。"},{"id":{"d":741,"n":7,"t":3},"speaker":"A","text":"はい。ここまで自動車を運転してくださったことへのお礼です。今日はありがとうございました。夕飯も楽しみです。"},{"id":{"d":741,"n":8,"t":3},"speaker":"B","text":"お気遣いいただきありがとうございます。夕飯は、海ほたるで海鮮丼を食べましょう。"}];
INSERT INTO sentence [{"id":{"d":742,"n":1,"t":3},"speaker":"A","text":"小林さん、私はここにいます。"},{"id":{"d":742,"n":2,"t":3},"speaker":"B","text":"すみません、到着が約束の時間の間際になってしまいました。"},{"id":{"d":742,"n":3,"t":3},"speaker":"A","text":"大丈夫ですよ。寝坊でもしてしまったのですか？"},{"id":{"d":742,"n":4,"t":3},"speaker":"B","text":"いいえ。東京駅までは早く来たのですが、駅弁をどれにしようか迷い過ぎて遅くなりました。"},{"id":{"d":742,"n":5,"t":3},"speaker":"A","text":"小林さんらしいですね。結局、何のお弁当を買ったのですか？"},{"id":{"d":742,"n":6,"t":3},"speaker":"B","text":"いつものお弁当になってしまいました。"},{"id":{"d":742,"n":7,"t":3},"speaker":"A","text":"迷った挙句に、いつものお弁当を選んだのですか？"},{"id":{"d":742,"n":8,"t":3},"speaker":"B","text":"はい。いつもの味が一番安心ということですね。"}];
INSERT INTO sentence [{"id":{"d":743,"n":1,"t":3},"speaker":"A","text":"座間さんも、駅弁を買って来たのですか？"},{"id":{"d":743,"n":2,"t":3},"speaker":"B","text":"私も駅弁を買ってきましたよ。"},{"id":{"d":743,"n":3,"t":3},"speaker":"A","text":"いつものお弁当ですか？"},{"id":{"d":743,"n":4,"t":3},"speaker":"B","text":"いいえ。それは先週食べたので、今日は釜めしです。"},{"id":{"d":743,"n":5,"t":3},"speaker":"A","text":"東京駅で釜めしが売っているのですか？"},{"id":{"d":743,"n":6,"t":3},"speaker":"B","text":"はい。ありましたよ。特急で長野に行っていた頃は、駅のホームでよく買ったものです。"},{"id":{"d":743,"n":7,"t":3},"speaker":"A","text":"懐かしいですね。今は新幹線での移動ですから、ホームで買うことができなくなりました。"},{"id":{"d":743,"n":8,"t":3},"speaker":"B","text":"そうなのですよ。それで、懐かしくてつい買ってしまいました。"}];
INSERT INTO sentence [{"id":{"d":744,"n":1,"t":3},"speaker":"A","text":"電車の中で食べる駅弁は、格別ですね。"},{"id":{"d":744,"n":2,"t":3},"speaker":"B","text":"家で食べるのとは味が違って感じられます。"},{"id":{"d":744,"n":3,"t":3},"speaker":"A","text":"旅に心を躍らせているからではありませんか？"},{"id":{"d":744,"n":4,"t":3},"speaker":"B","text":"そうかもしれませんね。"},{"id":{"d":744,"n":5,"t":3},"speaker":"A","text":"シューマイを1つもらえませんか？"},{"id":{"d":744,"n":6,"t":3},"speaker":"B","text":"はい。鶏肉と交換していただけるならよいですよ。"},{"id":{"d":744,"n":7,"t":3},"speaker":"A","text":"分かりました。良かったら香の物もどうぞ。"},{"id":{"d":744,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。いただきます。"}];
INSERT INTO sentence [{"id":{"d":745,"n":1,"t":3},"speaker":"A","text":"イタリアに行ってみたいです。"},{"id":{"d":745,"n":2,"t":3},"speaker":"B","text":"イタリアに行かれたことはありませんか？"},{"id":{"d":745,"n":3,"t":3},"speaker":"A","text":"まだありません。本場のイタリア料理を食べてみたいです。あなたは行ったことがありますか？"},{"id":{"d":745,"n":4,"t":3},"speaker":"B","text":"私はローマとミラノに行ったことがあります。"},{"id":{"d":745,"n":5,"t":3},"speaker":"A","text":"イタリア料理はいかがでしたか？"},{"id":{"d":745,"n":6,"t":3},"speaker":"B","text":"素晴らしかったです。私はピザが食べたくて行ったので、毎日ピザざんまいでした。"},{"id":{"d":745,"n":7,"t":3},"speaker":"A","text":"良いですね。本場のピザはおいしかったですか？"},{"id":{"d":745,"n":8,"t":3},"speaker":"B","text":"それはもうおいしかったです。イタリアに行かれるなら、ピザを食べることは外せませんよ。"}];
INSERT INTO sentence [{"id":{"d":746,"n":1,"t":3},"speaker":"A","text":"友人から、新婚旅行のお土産でコアラのぬいぐるみをもらいました。"},{"id":{"d":746,"n":2,"t":3},"speaker":"B","text":"これがそのぬいぐるみですか？かわいいですね。"},{"id":{"d":746,"n":3,"t":3},"speaker":"A","text":"手の部分がクリップのようになっていて、物が挟めるようになっているので、木にしがみついたコアラのようになるそうです。"},{"id":{"d":746,"n":4,"t":3},"speaker":"B","text":"部屋のカーテンにも付けられそうですね。"},{"id":{"d":746,"n":5,"t":3},"speaker":"A","text":"それは良い案です。これ以外にも、カンガルージャーキーもらいました。"},{"id":{"d":746,"n":6,"t":3},"speaker":"B","text":"本物のカンガルーのお肉なのですか？"},{"id":{"d":746,"n":7,"t":3},"speaker":"A","text":"そのようです。これをお茶請けに休憩をしますか？"},{"id":{"d":746,"n":8,"t":3},"speaker":"B","text":"カンガルーが大好きなので、遠慮をさせてください。"}];
INSERT INTO sentence [{"id":{"d":747,"n":1,"t":3},"speaker":"A","text":"お父さん、今度の日曜日に水族館に連れて行ってください。"},{"id":{"d":747,"n":2,"t":3},"speaker":"B","text":"どこの水族館に行きたいのですか？"},{"id":{"d":747,"n":3,"t":3},"speaker":"A","text":"新江ノ島水族館に行きたいです。"},{"id":{"d":747,"n":4,"t":3},"speaker":"B","text":"新江ノ島水族館なら海が近くて、お魚が食べられますね。"},{"id":{"d":747,"n":5,"t":3},"speaker":"A","text":"はい。連れて行ってくれますか？"},{"id":{"d":747,"n":6,"t":3},"speaker":"B","text":"良いでしょう。土曜日までに宿題を済ませておくのですよ。"},{"id":{"d":747,"n":7,"t":3},"speaker":"A","text":"分かりました。ミケへのお土産に、お魚のぬいぐるみを買いたいです。"},{"id":{"d":747,"n":8,"t":3},"speaker":"B","text":"買って良いかどうかは、お母さんにも相談しましょう。"}];
INSERT INTO sentence [{"id":{"d":748,"n":1,"t":3},"speaker":"A","text":"この牧場のソフトクリームはおいしいですね。"},{"id":{"d":748,"n":2,"t":3},"speaker":"B","text":"絞りたての牛乳で作られているそうですよ。"},{"id":{"d":748,"n":3,"t":3},"speaker":"A","text":"先ほど体験した乳絞りで絞った牛乳ですか？"},{"id":{"d":748,"n":4,"t":3},"speaker":"B","text":"いいえ。その牛乳は使っていないと思いますよ。"},{"id":{"d":748,"n":5,"t":3},"speaker":"A","text":"自分で絞った牛乳でできたソフトクリームを食べられたら良いのに。"},{"id":{"d":748,"n":6,"t":3},"speaker":"B","text":"そうですね。でも、安全に食べるためには殺菌もしなければならないでしょうし、難しいかもしれませんね。"},{"id":{"d":748,"n":7,"t":3},"speaker":"A","text":"それもそうですね。"}];
INSERT INTO sentence [{"id":{"d":749,"n":1,"t":3},"speaker":"A","text":"次はふれあい広場に行きましょう。"},{"id":{"d":749,"n":2,"t":3},"speaker":"B","text":"私は、動物を触るのは初めてです。"},{"id":{"d":749,"n":3,"t":3},"speaker":"A","text":"そうなのですね。私は猫を飼っているので、猫だけは触ったことがあります。"},{"id":{"d":749,"n":4,"t":3},"speaker":"B","text":"動物に触れることは、怖くありませんか？"},{"id":{"d":749,"n":5,"t":3},"speaker":"A","text":"はい。小さい動物ですから大丈夫だと思いますよ。"},{"id":{"d":749,"n":6,"t":3},"speaker":"B","text":"ヤギのような大きい動物もいますよ。"},{"id":{"d":749,"n":7,"t":3},"speaker":"A","text":"確かに、ここにいるのはウサギやモルモットだけではありませんね。"},{"id":{"d":749,"n":8,"t":3},"speaker":"B","text":"私は柵の向こうに行くのは怖いので、柵の外で待っています。"}];
INSERT INTO sentence [{"id":{"d":750,"n":1,"t":3},"speaker":"A","text":"ふれあい広場で動物とたくさん触れ合えました。楽しかったですね。"},{"id":{"d":750,"n":2,"t":3},"speaker":"B","text":"良い体験ができました。ウサギは柔らかくて温かかったです。"},{"id":{"d":750,"n":3,"t":3},"speaker":"A","text":"それは良かったです。最初はとても怖がっていましたね。"},{"id":{"d":750,"n":4,"t":3},"speaker":"B","text":"はい。初めてだったのですから、笑わないでください。"},{"id":{"d":750,"n":5,"t":3},"speaker":"A","text":"今度はヤギに餌をあげてみましょうよ。"},{"id":{"d":750,"n":6,"t":3},"speaker":"B","text":"そうしましょう。売店で、餌が100円で売っていましたね。"},{"id":{"d":750,"n":7,"t":3},"speaker":"A","text":"お小遣いは足りそうですか？"},{"id":{"d":750,"n":8,"t":3},"speaker":"B","text":"はい。お母さんにたくさんもらったので大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":751,"n":1,"t":3},"speaker":"A","text":"ミステリーツアーに行きませんか？"},{"id":{"d":751,"n":2,"t":3},"speaker":"B","text":"はい。旅行会社のミステリーツアーですか？"},{"id":{"d":751,"n":3,"t":3},"speaker":"A","text":"いいえ、私の企画したミステリーツアーです。"},{"id":{"d":751,"n":4,"t":3},"speaker":"B","text":"私に行き先を告げないで、どこかに連れて行ってくれるのですか？"},{"id":{"d":751,"n":5,"t":3},"speaker":"A","text":"はい。バスタ新宿に行って空いている高速バスに適当に乗って、どこかに行こうという企画です。"},{"id":{"d":751,"n":6,"t":3},"speaker":"B","text":"それは行き当たりばったりというのではありませんか？"},{"id":{"d":751,"n":7,"t":3},"speaker":"A","text":"そうとも言います。"},{"id":{"d":751,"n":8,"t":3},"speaker":"B","text":"交通費がいくらかかるかも分からないような旅は不安ですので遠慮します。"}];
INSERT INTO sentence [{"id":{"d":752,"n":1,"t":3},"speaker":"A","text":"この駅で降りて、名物のうどんを食べましょう。"},{"id":{"d":752,"n":2,"t":3},"speaker":"B","text":"ちょうどお昼ですし、良い案ですね。"},{"id":{"d":752,"n":3,"t":3},"speaker":"A","text":"一応、次の電車が来る時間をチェックしておきましょう。"},{"id":{"d":752,"n":4,"t":3},"speaker":"B","text":"次の電車が来るのは14時40分です。"},{"id":{"d":752,"n":5,"t":3},"speaker":"A","text":"結構時間がありますね。"},{"id":{"d":752,"n":6,"t":3},"speaker":"B","text":"ゆっくり昼食が食べられそうです。"},{"id":{"d":752,"n":7,"t":3},"speaker":"A","text":"でも、その分、次の予定の美術館ではあまり時間が取れないかもしれません。"},{"id":{"d":752,"n":8,"t":3},"speaker":"B","text":"確かに、閉館時間まで1時間くらいしかなさそうです。"}];
INSERT INTO sentence [{"id":{"d":753,"n":1,"t":3},"speaker":"A","text":"東京ディズニーランドへ行きたいのですが、本当にこの道で良いのでしょうか？"},{"id":{"d":753,"n":2,"t":3},"speaker":"B","text":"はい。京葉線に乗り換えるのですね。この先で合っていますよ。"},{"id":{"d":753,"n":3,"t":3},"speaker":"A","text":"でも、もう随分歩いたのですが京葉線のホームにたどり着きません。"},{"id":{"d":753,"n":4,"t":3},"speaker":"B","text":"東京駅は、京葉線への乗り換えが遠いことで有名なのですよ。"},{"id":{"d":753,"n":5,"t":3},"speaker":"A","text":"そうなのですか？"},{"id":{"d":753,"n":6,"t":3},"speaker":"B","text":"はい。あの標識に従って行けば良いですよ。"},{"id":{"d":753,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。ちなみに、あとどれくらいの時間がかかるのでしょうか？"},{"id":{"d":753,"n":8,"t":3},"speaker":"B","text":"迷わなければ5分くらいで行けると思いますが、慣れない人は7、8分ほどかかるかもしれません。"}];
INSERT INTO sentence [{"id":{"d":754,"n":1,"t":3},"speaker":"A","text":"明日は一緒に遊びに行けなくてごめんなさい。"},{"id":{"d":754,"n":2,"t":3},"speaker":"B","text":"よいですよ。お父さんとどこかに出掛けるのでしょう？"},{"id":{"d":754,"n":3,"t":3},"speaker":"A","text":"はい。春休みの間に、駅のスタンプラリーをしているのです。"},{"id":{"d":754,"n":4,"t":3},"speaker":"B","text":"駅のスタンプラリーですか？"},{"id":{"d":754,"n":5,"t":3},"speaker":"A","text":"はい。地下鉄の駅全部に降りて、スタンプを集めるのです。"},{"id":{"d":754,"n":6,"t":3},"speaker":"B","text":"楽しそうですね。"},{"id":{"d":754,"n":7,"t":3},"speaker":"A","text":"もう20駅回りました。"},{"id":{"d":754,"n":8,"t":3},"speaker":"B","text":"今度、スタンプ帳を見せてください。"}];
INSERT INTO sentence [{"id":{"d":755,"n":1,"t":3},"speaker":"A","text":"せっかくの旅行なのに雨が降ってしまいましたね。"},{"id":{"d":755,"n":2,"t":3},"speaker":"B","text":"そうですね。傘は持って来ていますか？"},{"id":{"d":755,"n":3,"t":3},"speaker":"A","text":"はい。私は折り畳み傘を持って来ています。"},{"id":{"d":755,"n":4,"t":3},"speaker":"B","text":"私は持って来ませんでした。"},{"id":{"d":755,"n":5,"t":3},"speaker":"A","text":"宿の売店で、ビニール傘を販売していましたよ。"},{"id":{"d":755,"n":6,"t":3},"speaker":"B","text":"そうですか！すぐに買いに行ってきます。"},{"id":{"d":755,"n":7,"t":3},"speaker":"A","text":"雨が小止みになるまで宿にいますから、急がなくても大丈夫ですよ。"},{"id":{"d":755,"n":8,"t":3},"speaker":"B","text":"では、ゆっくりお土産も見てきます。"}];
INSERT INTO sentence [{"id":{"d":756,"n":1,"t":3},"speaker":"A","text":"アメリカ旅行中に骨折したのですか？"},{"id":{"d":756,"n":2,"t":3},"speaker":"B","text":"はい。転んでしまって足を骨折しました。"},{"id":{"d":756,"n":3,"t":3},"speaker":"A","text":"事件などに巻き込まれたのではなくて良かったです。"},{"id":{"d":756,"n":4,"t":3},"speaker":"B","text":"自分の不注意だったので、その点は良かったです。"},{"id":{"d":756,"n":5,"t":3},"speaker":"A","text":"病院で治療していただいたのでしょう？"},{"id":{"d":756,"n":6,"t":3},"speaker":"B","text":"はい。料金の高さに驚きましたよ。"},{"id":{"d":756,"n":7,"t":3},"speaker":"A","text":"お金は払えたのですか？"},{"id":{"d":756,"n":8,"t":3},"speaker":"B","text":"はい。旅行保険に入っていたので大丈夫でした。"}];
INSERT INTO sentence [{"id":{"d":757,"n":1,"t":3},"speaker":"A","text":"寒くありませんか？"},{"id":{"d":757,"n":2,"t":3},"speaker":"B","text":"少し寒いです。夏だと思って薄着し過ぎましたね。"},{"id":{"d":757,"n":3,"t":3},"speaker":"A","text":"山の上はこれほどに気温が低いのですね。"},{"id":{"d":757,"n":4,"t":3},"speaker":"B","text":"そうですね。昨日まで熱帯夜でしたのに。"},{"id":{"d":757,"n":5,"t":3},"speaker":"A","text":"羽織るものを持ってきましたか？"},{"id":{"d":757,"n":6,"t":3},"speaker":"B","text":"いいえ。スカーフならあります。"},{"id":{"d":757,"n":7,"t":3},"speaker":"A","text":"スカーフでは心許ないですね。ホテルの売店でウインドブレーカーが販売されていました。"},{"id":{"d":757,"n":8,"t":3},"speaker":"B","text":"風邪をひくといけないので、それを買ってしまいましょうか。"}];
INSERT INTO sentence [{"id":{"d":758,"n":1,"t":3},"speaker":"A","text":"今回の旅行は豪華ですね。"},{"id":{"d":758,"n":2,"t":3},"speaker":"B","text":"頑張って指定席を取りましたからね。"},{"id":{"d":758,"n":3,"t":3},"speaker":"A","text":"6人用の個室は珍しいのではありませんか？"},{"id":{"d":758,"n":4,"t":3},"speaker":"B","text":"はい。この列車には個室があることを知っていたので、いつか取ろうと思っていたのです。"},{"id":{"d":758,"n":5,"t":3},"speaker":"A","text":"とてもすてきな空間ですね。みんなも喜んでいます。"},{"id":{"d":758,"n":6,"t":3},"speaker":"B","text":"お食事も予約しているので、後で届くと思いますよ。"},{"id":{"d":758,"n":7,"t":3},"speaker":"A","text":"至れり尽くせりですね。ありがとうございます。"},{"id":{"d":758,"n":8,"t":3},"speaker":"B","text":"喜んでいただけて良かったです。でも、少しだけ旅行代金が高くなってしまったのは、申し訳ないと思っています。"}];
INSERT INTO sentence [{"id":{"d":759,"n":1,"t":3},"speaker":"A","text":"夕飯の舟盛りは素晴らしかったですね。"},{"id":{"d":759,"n":2,"t":3},"speaker":"B","text":"あのように大きな舟で出てくるとは思いませんでした。"},{"id":{"d":759,"n":3,"t":3},"speaker":"A","text":"食べきれるか心配でしたけれど、気が付いたら全て食べていました。"},{"id":{"d":759,"n":4,"t":3},"speaker":"B","text":"必ずやお酒の力ですよ。"},{"id":{"d":759,"n":5,"t":3},"speaker":"A","text":"お薦めされた地酒もおいしかったですからね。"},{"id":{"d":759,"n":6,"t":3},"speaker":"B","text":"この後、大浴場に行きますか？"},{"id":{"d":759,"n":7,"t":3},"speaker":"A","text":"いいえ。もう少し食後の休憩をしないと溺れてしまいそうです。"},{"id":{"d":759,"n":8,"t":3},"speaker":"B","text":"確かにそうですね。お風呂にはもう少し後に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":760,"n":1,"t":3},"speaker":"A","text":"今日は、この先のスーパーマーケットに寄っても良いですか？"},{"id":{"d":760,"n":2,"t":3},"speaker":"B","text":"良いですよ。でも、旅行に来てまでスーパーマーケットへ行くのですか？"},{"id":{"d":760,"n":3,"t":3},"speaker":"A","text":"はい。全国展開していない地元のスーパーマーケットには掘り出し物のお土産があるのですよ。"},{"id":{"d":760,"n":4,"t":3},"speaker":"B","text":"そうなのですか？"},{"id":{"d":760,"n":5,"t":3},"speaker":"A","text":"はい。ほら、見てください。このおはぎはとてもおいしそうです。"},{"id":{"d":760,"n":6,"t":3},"speaker":"B","text":"これがおはぎですか？このようなものは見たことがありません。"},{"id":{"d":760,"n":7,"t":3},"speaker":"A","text":"このスーパーの名物だそうですよ。おはぎに生クリームがのっています。"},{"id":{"d":760,"n":8,"t":3},"speaker":"B","text":"これはおいしそうです。ぜひ買ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":761,"n":1,"t":3},"speaker":"A","text":"レンタカーは軽自動車で良いのですか？"},{"id":{"d":761,"n":2,"t":3},"speaker":"B","text":"はい。今回行く場所は道が狭いところもあるので、あまり大きい車だと通りにくくて困るのです。"},{"id":{"d":761,"n":3,"t":3},"speaker":"A","text":"前にも行ったことがあるのですね？"},{"id":{"d":761,"n":4,"t":3},"speaker":"B","text":"ええ、でもそのときは途中までしか行きませんでした。"},{"id":{"d":761,"n":5,"t":3},"speaker":"A","text":"私は運転に自信がないので、狭い道の運転は、交代することができないかもしれません。"},{"id":{"d":761,"n":6,"t":3},"speaker":"B","text":"分かりました。今回は私が運転するので大丈夫ですよ。"},{"id":{"d":761,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。安全運転でお願いしますね。"},{"id":{"d":761,"n":8,"t":3},"speaker":"B","text":"承りました。"}];
INSERT INTO sentence [{"id":{"d":762,"n":1,"t":3},"speaker":"A","text":"あそこのお店は、駐車場にバイクがたくさん止まっていますね。"},{"id":{"d":762,"n":2,"t":3},"speaker":"B","text":"あのラーメン屋さんは、ツーリングをする人に有名なのだそうです。"},{"id":{"d":762,"n":3,"t":3},"speaker":"A","text":"バイクでなくても食べに行っても良いのでしょう？"},{"id":{"d":762,"n":4,"t":3},"speaker":"B","text":"もちろん、問題ありませんよ。"},{"id":{"d":762,"n":5,"t":3},"speaker":"A","text":"人気があると聞くと、食べたくなりませんか？"},{"id":{"d":762,"n":6,"t":3},"speaker":"B","text":"そうですね。行ってみましょうか？車を止めるスペースは空いていますか？"},{"id":{"d":762,"n":7,"t":3},"speaker":"A","text":"少し狭いかもしれませんが、端の方に車を止められそうです。"},{"id":{"d":762,"n":8,"t":3},"speaker":"B","text":"車を止めるには技術が必要な場所ですね。慎重に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":763,"n":1,"t":3},"speaker":"A","text":"明日のスケジュールはどうなっていますか？"},{"id":{"d":763,"n":2,"t":3},"speaker":"B","text":"明日は、まず朝市に行って、その後はお城見学に行きます。"},{"id":{"d":763,"n":3,"t":3},"speaker":"A","text":"朝市に行くのでしたら早起きしなくてはいけませんね。"},{"id":{"d":763,"n":4,"t":3},"speaker":"B","text":"朝市はお昼過ぎまでしているようですから、朝はゆっくりでも良いですよ。"},{"id":{"d":763,"n":5,"t":3},"speaker":"A","text":"それなら良かったです。少し飲み過ぎてしまい、早起きはつらいと思っていました。"},{"id":{"d":763,"n":6,"t":3},"speaker":"B","text":"そうですね。お酒がおいしくて、つい飲み過ぎましたね。"},{"id":{"d":763,"n":7,"t":3},"speaker":"A","text":"明日は何時に起きることにしますか？"},{"id":{"d":763,"n":8,"t":3},"speaker":"B","text":"7時30分でどうでしょうか？もっと遅い方がいいですか？"},{"id":{"d":763,"n":9,"t":3},"speaker":"A","text":"7時30分で大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":764,"n":1,"t":3},"speaker":"A","text":"朝市で、お土産をたくさん買うことができました。"},{"id":{"d":764,"n":2,"t":3},"speaker":"B","text":"買い食いもたくさんできましたね。満足です。"},{"id":{"d":764,"n":3,"t":3},"speaker":"A","text":"旅先の朝市での買い物は楽しいですね。"},{"id":{"d":764,"n":4,"t":3},"speaker":"B","text":"そうですね。買い物をしすぎて荷物が重くなってしまったので、宅配便で送っても良いですか？"},{"id":{"d":764,"n":5,"t":3},"speaker":"A","text":"もちろん。私もそうしたいと思っていました。"},{"id":{"d":764,"n":6,"t":3},"speaker":"B","text":"あちらのコンビニエンスストアから、荷物を送りましょう。"},{"id":{"d":764,"n":7,"t":3},"speaker":"A","text":"身軽になったら、このまま観光に行きたいですね。"},{"id":{"d":764,"n":8,"t":3},"speaker":"B","text":"賛成です。"}];
INSERT INTO sentence [{"id":{"d":765,"n":1,"t":3},"speaker":"A","text":"この辺りにバラの公園があると聞いてきたのですが。"},{"id":{"d":765,"n":2,"t":3},"speaker":"B","text":"河津バガテル公園のことですね。"},{"id":{"d":765,"n":3,"t":3},"speaker":"A","text":"そうです。そのような名前だったと思います。"},{"id":{"d":765,"n":4,"t":3},"speaker":"B","text":"名前が覚えにくいようで、よく「バラ公園」と呼ばれていますよ。"},{"id":{"d":765,"n":5,"t":3},"speaker":"A","text":"そうなのですね。バラ公園は、ここから歩いて行ける距離ですか？"},{"id":{"d":765,"n":6,"t":3},"speaker":"B","text":"いいえ。少し距離があるので、駅からシャトルバスに乗ると良いですよ。"},{"id":{"d":765,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。駅に行ってみます。"},{"id":{"d":765,"n":8,"t":3},"speaker":"B","text":"どういたしまして。バラが満開の良い季節にいらっしゃいましたね。"},{"id":{"d":765,"n":9,"t":3},"speaker":"A","text":"それは良かったです。"}];
INSERT INTO sentence [{"id":{"d":766,"n":1,"t":3},"speaker":"A","text":"旅行かばんがいっぱいになっていますね。"},{"id":{"d":766,"n":2,"t":3},"speaker":"B","text":"はい。お土産をたくさん買ってきましたから。"},{"id":{"d":766,"n":3,"t":3},"speaker":"A","text":"もしかして、私へのお土産ですか？"},{"id":{"d":766,"n":4,"t":3},"speaker":"B","text":"そうですよ。お留守番をしてくれたあなたには、特にたくさん買ってきました。"},{"id":{"d":766,"n":5,"t":3},"speaker":"A","text":"それはうれしいです。留守番をしたかいがあります。"},{"id":{"d":766,"n":6,"t":3},"speaker":"B","text":"今回は女子大学に通っていた時代のお友達同士で旅行させてくれて、ありがとうございます。"},{"id":{"d":766,"n":7,"t":3},"speaker":"A","text":"旅行は楽しめましたか？"},{"id":{"d":766,"n":8,"t":3},"speaker":"B","text":"はい。青春時代に戻れました。"}];
INSERT INTO sentence [{"id":{"d":767,"n":1,"t":3},"speaker":"A","text":"旅行のお土産をお披露目しましょう。"},{"id":{"d":767,"n":2,"t":3},"speaker":"B","text":"楽しみです。テーブルの上を片付けて、待っています。"},{"id":{"d":767,"n":3,"t":3},"speaker":"A","text":"まずはお酒です。日本酒とワインです。"},{"id":{"d":767,"n":4,"t":3},"speaker":"B","text":"重い瓶に入ったお酒を買ってきてくれてありがとうございます。"},{"id":{"d":767,"n":5,"t":3},"speaker":"A","text":"お次は燻製セットと生ハムと塩辛です。"},{"id":{"d":767,"n":6,"t":3},"speaker":"B","text":"どれもとてもおいしそうです。"},{"id":{"d":767,"n":7,"t":3},"speaker":"A","text":"ペアグラスも作ってきました。"},{"id":{"d":767,"n":8,"t":3},"speaker":"B","text":"素晴らしい出来ですね。今晩は、わが家の居酒屋が開店ですね。"}];
INSERT INTO sentence [{"id":{"d":768,"n":1,"t":3},"speaker":"A","text":"観光バスの予約をしたのですが、乗り場が分からないので教えていただけますか？"},{"id":{"d":768,"n":2,"t":3},"speaker":"B","text":"ご予約の際にお渡しした案内書はお持ちですか？"},{"id":{"d":768,"n":3,"t":3},"speaker":"A","text":"これのことですか？"},{"id":{"d":768,"n":4,"t":3},"speaker":"B","text":"はい。お客さまがご予約されたコースは「さくらんぼ狩り＆桃狩り食べ放題初夏満喫の旅」ですね。"},{"id":{"d":768,"n":5,"t":3},"speaker":"A","text":"そうです。バスがたくさんあって、どのバスか分かりません。"},{"id":{"d":768,"n":6,"t":3},"speaker":"B","text":"「さくらんぼ」と書いてあるバスは多いですからね。"},{"id":{"d":768,"n":7,"t":3},"speaker":"A","text":"どのバスに乗れば良いですか？"},{"id":{"d":768,"n":8,"t":3},"speaker":"B","text":"51番のバスにご乗車ください。数字の横にさくらんぼと桃の絵が描いてあるものが目印です。"}];
INSERT INTO sentence [{"id":{"d":769,"n":1,"t":3},"speaker":"A","text":"すみません、私の席はここで良いでしょうか？"},{"id":{"d":769,"n":2,"t":3},"speaker":"B","text":"あら、ごめんなさい。荷物を置いてしまっていました。すぐに退かしますね。"},{"id":{"d":769,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。皆さん、5人でご旅行ですか？"},{"id":{"d":769,"n":4,"t":3},"speaker":"B","text":"はい。地元で同級会があるのですよ。"},{"id":{"d":769,"n":5,"t":3},"speaker":"A","text":"そうなのですね。1人、関係のない者が交じってすみません。"},{"id":{"d":769,"n":6,"t":3},"speaker":"B","text":"気を使わせてしまってすみません。これでは居心地が悪いですよね？席を戻しましょうか？"},{"id":{"d":769,"n":7,"t":3},"speaker":"A","text":"いいえ、大丈夫ですから皆さんで楽しんでください。"},{"id":{"d":769,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。一緒の席になった好みです。もし良ければ一緒にお話しませんか？"}];
INSERT INTO sentence [{"id":{"d":770,"n":1,"t":3},"speaker":"A","text":"集合時間まで時間がありません。"},{"id":{"d":770,"n":2,"t":3},"speaker":"B","text":"手分けをして、お土産を買ってきましょう。"},{"id":{"d":770,"n":3,"t":3},"speaker":"A","text":"私はカステラとおまんじゅうを買ってきます。2箱ずつで良いですね？"},{"id":{"d":770,"n":4,"t":3},"speaker":"B","text":"はい、それで結構です。私は牛肉しぐれとビーフジャーキーを買ってきます。"},{"id":{"d":770,"n":5,"t":3},"speaker":"A","text":"私のビーフジャーキーは3つにしてください。"},{"id":{"d":770,"n":6,"t":3},"speaker":"B","text":"牛肉しぐれは2つで良いのですね？"},{"id":{"d":770,"n":7,"t":3},"speaker":"A","text":"はい。それは2つで大丈夫です。"},{"id":{"d":770,"n":8,"t":3},"speaker":"B","text":"それでは、集合時間に遅れないように急ぎましょう。"}];
INSERT INTO sentence [{"id":{"d":771,"n":1,"t":3},"speaker":"A","text":"電車が来ましたよ。"},{"id":{"d":771,"n":2,"t":3},"speaker":"B","text":"乗るのは待ってください。"},{"id":{"d":771,"n":3,"t":3},"speaker":"A","text":"この電車に乗るのではありませんか？"},{"id":{"d":771,"n":4,"t":3},"speaker":"B","text":"はい。これは特急です。私たちは、各駅停車の電車しか止まらない駅に行くのです。"},{"id":{"d":771,"n":5,"t":3},"speaker":"A","text":"なるほど、この電車では行けませんね。"},{"id":{"d":771,"n":6,"t":3},"speaker":"B","text":"そうです。次の電車を待ちましょう。"},{"id":{"d":771,"n":7,"t":3},"speaker":"A","text":"それでは、その間にトイレに行ってきますね。"},{"id":{"d":771,"n":8,"t":3},"speaker":"B","text":"次の電車は2分後です。急いでくださいね。"},{"id":{"d":771,"n":9,"t":3},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":772,"n":1,"t":3},"speaker":"A","text":"切符を買ってきます。"},{"id":{"d":772,"n":2,"t":3},"speaker":"B","text":"このカードは持っていませんか？"},{"id":{"d":772,"n":3,"t":3},"speaker":"A","text":"持っていません。それは定期券なのではありませんか？"},{"id":{"d":772,"n":4,"t":3},"speaker":"B","text":"違いますよ。プリペイドカードです。"},{"id":{"d":772,"n":5,"t":3},"speaker":"A","text":"プリペイドというのは何ですか？"},{"id":{"d":772,"n":6,"t":3},"speaker":"B","text":"先にお金を入れておいて、その金額内であれば切符を買わずに電車に乗れるカードです。"},{"id":{"d":772,"n":7,"t":3},"speaker":"A","text":"お金が足りなくなったらどうするのですか？"},{"id":{"d":772,"n":8,"t":3},"speaker":"B","text":"なくなりそうになったら、お金をチャージすれば引き続き使えますよ。"}];
INSERT INTO sentence [{"id":{"d":773,"n":1,"t":3},"speaker":"A","text":"初めてプリペイドカードを使いました。"},{"id":{"d":773,"n":2,"t":3},"speaker":"B","text":"持っていると便利でしょう？"},{"id":{"d":773,"n":3,"t":3},"speaker":"A","text":"はい。ところで、プリペイドカードの残額はどのようにしたら分かるのですか？"},{"id":{"d":773,"n":4,"t":3},"speaker":"B","text":"改札を通るときに金額が表示されているのを見ませんでしたか？"},{"id":{"d":773,"n":5,"t":3},"speaker":"A","text":"初めてプリペイドカードを使ったことに興奮して、気が付きませんでした。"},{"id":{"d":773,"n":6,"t":3},"speaker":"B","text":"今度、改札を通るときに見ておくと良いですよ。"},{"id":{"d":773,"n":7,"t":3},"speaker":"A","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":774,"n":1,"t":3},"speaker":"A","text":"この辺りはワサビが名物なのですか？"},{"id":{"d":774,"n":2,"t":3},"speaker":"B","text":"はい、そうです。水がきれいですから、おいしいワサビが育ちます。"},{"id":{"d":774,"n":3,"t":3},"speaker":"A","text":"そうなのですね。では、せっかくなのでわさびバーガーを注文します。"},{"id":{"d":774,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。お飲み物はどうされますか？"},{"id":{"d":774,"n":5,"t":3},"speaker":"A","text":"アイスコーヒーをと思いましたが、わさびスカッシュなるものもあるのですね？"},{"id":{"d":774,"n":6,"t":3},"speaker":"B","text":"はい。わさびスカッシュも爽やかでお薦めですよ。"},{"id":{"d":774,"n":7,"t":3},"speaker":"A","text":"では、飲み物はそれでお願いします。"},{"id":{"d":774,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。お席にお持ちしますので、番号札をテーブルに置いてお待ちください。"}];
INSERT INTO sentence [{"id":{"d":775,"n":1,"t":3},"speaker":"A","text":"さて、お土産を選びましょう。"},{"id":{"d":775,"n":2,"t":3},"speaker":"B","text":"父の好物がわさび漬けなので、私はこれにします。"},{"id":{"d":775,"n":3,"t":3},"speaker":"A","text":"私はわさびドレッシングとわさびふりかけを家族へのお土産にしようと思います。"},{"id":{"d":775,"n":4,"t":3},"speaker":"B","text":"それらもおいしそうですね。"},{"id":{"d":775,"n":5,"t":3},"speaker":"A","text":"はい。スーパーマーケットにあるものとは一味違う感じがします。"},{"id":{"d":775,"n":6,"t":3},"speaker":"B","text":"私もわさびドレッシングとわさびふりかけを追加しようと思います。"},{"id":{"d":775,"n":7,"t":3},"speaker":"A","text":"会社のみんなへのお土産はどうしますか？"},{"id":{"d":775,"n":8,"t":3},"speaker":"B","text":"先ほどのお店の温泉まんじゅうにしましょう。"}];
INSERT INTO sentence [{"id":{"d":776,"n":1,"t":3},"speaker":"A","text":"今回の台湾旅行は弾丸旅行でしたね。"},{"id":{"d":776,"n":2,"t":3},"speaker":"B","text":"そうですね。休みが長く取れませんでしたからね。"},{"id":{"d":776,"n":3,"t":3},"speaker":"A","text":"1泊2日で海外旅行ができるとは思いませんでした。"},{"id":{"d":776,"n":4,"t":3},"speaker":"B","text":"テレビ番組で見たのですが、台湾なら日帰りも可能だそうですよ。"},{"id":{"d":776,"n":5,"t":3},"speaker":"A","text":"さすがに日帰りは勘弁してほしいです。"},{"id":{"d":776,"n":6,"t":3},"speaker":"B","text":"忙しい旅行でしたけれど、下調べをしていたおかげで楽しめました。"},{"id":{"d":776,"n":7,"t":3},"speaker":"A","text":"食べたいものも食べ、買いたいものも買い、見たいものも見ましたよね。"},{"id":{"d":776,"n":8,"t":3},"speaker":"B","text":"はい。次は1泊2日で韓国に行ってみませんか？"}];
INSERT INTO sentence [{"id":{"d":777,"n":1,"t":3},"speaker":"A","text":"パスポートの写真撮影に失敗しました。"},{"id":{"d":777,"n":2,"t":3},"speaker":"B","text":"規格が合っていなかったということですか？"},{"id":{"d":777,"n":3,"t":3},"speaker":"A","text":"それは大丈夫だったのですが、証明写真機で撮ったらひどい表情になってしまったのです。"},{"id":{"d":777,"n":4,"t":3},"speaker":"B","text":"撮り直せばよかったではありませんか？"},{"id":{"d":777,"n":5,"t":3},"speaker":"A","text":"時間がなく、お金ももったいないと思って、そのまま提出してしまいました。"},{"id":{"d":777,"n":6,"t":3},"speaker":"B","text":"何年間有効のパスポートにしたのですか？"},{"id":{"d":777,"n":7,"t":3},"speaker":"A","text":"10年です。10年もあの顔で入国審査を受けなければなりません。"}];
INSERT INTO sentence [{"id":{"d":778,"n":1,"t":3},"speaker":"A","text":"このだるまの顔は、少しお父さんに似ています。"},{"id":{"d":778,"n":2,"t":3},"speaker":"B","text":"言われてみれば少し似ているかもしれません。お兄さんの合格祈願に、このだるまを買いましょうか？"},{"id":{"d":778,"n":3,"t":3},"speaker":"A","text":"はい。お父さんに見張られている気持ちで受験勉強をするのも良いかもしれません。"},{"id":{"d":778,"n":4,"t":3},"speaker":"B","text":"では、これを買いましょう。"},{"id":{"d":778,"n":5,"t":3},"speaker":"A","text":"夜中なのにだるま市はにぎわっていますね。"},{"id":{"d":778,"n":6,"t":3},"speaker":"B","text":"だるま市は、夜通し行っているそうですよ。"},{"id":{"d":778,"n":7,"t":3},"speaker":"A","text":"寒い中、お店の方も大変ですね。"},{"id":{"d":778,"n":8,"t":3},"speaker":"B","text":"そうですね。受験勉強を頑張っているお兄さんに、だるまのお弁当を買っていきましょう。"}];
INSERT INTO sentence [{"id":{"d":779,"n":1,"t":3},"speaker":"A","text":"お湯に入るまでは寒くて大変でしたが、雪の露天風呂も良いものですね。"},{"id":{"d":779,"n":2,"t":3},"speaker":"B","text":"はい、最高の気分です。"},{"id":{"d":779,"n":3,"t":3},"speaker":"A","text":"足の先が冷えていたので、最初はお湯が熱く感じられましたが、今はとても気持ちが良いです。"},{"id":{"d":779,"n":4,"t":3},"speaker":"B","text":"景色も良いですし、このまま食事もお酒も露天風呂でいただきたいくらいです。"},{"id":{"d":779,"n":5,"t":3},"speaker":"A","text":"私は景色を堪能したら、お風呂から上がってゆっくり夕飯をいただきます。"},{"id":{"d":779,"n":6,"t":3},"speaker":"B","text":"私を置いて、お風呂から先に出るつもりですか？"},{"id":{"d":779,"n":7,"t":3},"speaker":"A","text":"あなたはお風呂から出たくないのでしょう？"},{"id":{"d":779,"n":8,"t":3},"speaker":"B","text":"本当に食事を持って来ていただけるわけがないでしょう。私もお風呂から上がりますよ。"}];
INSERT INTO sentence [{"id":{"d":780,"n":1,"t":3},"speaker":"A","text":"お母さん、さっき見たパンダさんのぬいぐるみが欲しいです。"},{"id":{"d":780,"n":2,"t":3},"speaker":"B","text":"これですか？"},{"id":{"d":780,"n":3,"t":3},"speaker":"A","text":"いいえ。もう少し大きいものが良いです。"},{"id":{"d":780,"n":4,"t":3},"speaker":"B","text":"こっちのパンダさんですか？"},{"id":{"d":780,"n":5,"t":3},"speaker":"A","text":"違います。もっと大きいものです。"},{"id":{"d":780,"n":6,"t":3},"speaker":"B","text":"これ以上大きいものだと、卓ちゃんと同じくらいの大きさになってしまいますよ。"},{"id":{"d":780,"n":7,"t":3},"speaker":"A","text":"それでも僕は大きいものが良いのです。"},{"id":{"d":780,"n":8,"t":3},"speaker":"B","text":"大きすぎると持って帰るのが大変ですよ。リュックサックに入る大きさの物にしておきなさい。"},{"id":{"d":780,"n":9,"t":3},"speaker":"A","text":"分かりました。では、これにします。"}];
INSERT INTO sentence [{"id":{"d":781,"n":1,"t":3},"speaker":"A","text":"旅行を計画してくれて、ありがとうございました。"},{"id":{"d":781,"n":2,"t":3},"speaker":"B","text":"どういたしまして。子どもたちが大はしゃぎでしたね。"},{"id":{"d":781,"n":3,"t":3},"speaker":"A","text":"はい。私も子どものころから大好きだったアニメーションの世界を堪能できました。"},{"id":{"d":781,"n":4,"t":3},"speaker":"B","text":"東京ディズニーランドほど混んでいないし、夏休みに行くのにはちょうど良いと思ったのです。"},{"id":{"d":781,"n":5,"t":3},"speaker":"A","text":"とても良かったです。ホテルもかわいい部屋だったので、たくさん写真を撮ってしまいました。"},{"id":{"d":781,"n":6,"t":3},"speaker":"B","text":"旅行に向けて、子どもたちはアニメーションを見てきたのですか？"},{"id":{"d":781,"n":7,"t":3},"speaker":"A","text":"はい。DVDで作品を見て、予習をしてきました。"},{"id":{"d":781,"n":8,"t":3},"speaker":"B","text":"そうだったのですね。"}];
INSERT INTO sentence [{"id":{"d":782,"n":1,"t":3},"speaker":"A","text":"お土産をどうぞ。"},{"id":{"d":782,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。どらやきですか？"},{"id":{"d":782,"n":3,"t":3},"speaker":"A","text":"はい。有名な絵画の焼き印が付いたどら焼きです。"},{"id":{"d":782,"n":4,"t":3},"speaker":"B","text":"面白いですね。まるで、どら焼きが叫んでいるようです。"},{"id":{"d":782,"n":5,"t":3},"speaker":"A","text":"見た目も良いのですが、このどら焼きはとてもおいしいのですよ。"},{"id":{"d":782,"n":6,"t":3},"speaker":"B","text":"それは楽しみです。どちらに行かれたのですか？"},{"id":{"d":782,"n":7,"t":3},"speaker":"A","text":"徳島の大塚国際美術館です。"},{"id":{"d":782,"n":8,"t":3},"speaker":"B","text":"遠くまで行かれたのですね。"}];
INSERT INTO sentence [{"id":{"d":783,"n":1,"t":3},"speaker":"A","text":"大塚国際美術館は陶器の美術館で、本物の絵が展示されているわけではないのですよ。"},{"id":{"d":783,"n":2,"t":3},"speaker":"B","text":"それは知りませんでした。とても精巧で、言われなければ気づきませんね。"},{"id":{"d":783,"n":3,"t":3},"speaker":"A","text":"はい、遠くから見たら本物そっくりに見えます。"},{"id":{"d":783,"n":4,"t":3},"speaker":"B","text":"これはあなたが撮影した写真ですか？館内を撮影しても良いのですか？"},{"id":{"d":783,"n":5,"t":3},"speaker":"A","text":"はい。この美術館では、作品の写真を撮っても良いのですよ。"},{"id":{"d":783,"n":6,"t":3},"speaker":"B","text":"そうなのですね。まるでヨーロッパの美術館で撮った写真のようです。"},{"id":{"d":783,"n":7,"t":3},"speaker":"A","text":"そうでしょう。このような写真はなかなか他の美術館では撮れません。"},{"id":{"d":783,"n":8,"t":3},"speaker":"B","text":"すてきです。私も行ってみたくなりました。"}];
INSERT INTO sentence [{"id":{"d":784,"n":1,"t":3},"speaker":"A","text":"おはようございます。"},{"id":{"d":784,"n":2,"t":3},"speaker":"B","text":"おはようございます。大きな荷物を持っていますが、どうしたのですか？"},{"id":{"d":784,"n":3,"t":3},"speaker":"A","text":"旅行の帰りにそのまま出社したのです。"},{"id":{"d":784,"n":4,"t":3},"speaker":"B","text":"今朝、東京に着いたのですか？"},{"id":{"d":784,"n":5,"t":3},"speaker":"A","text":"そうです。土曜日の夜に夜行列車で出発して、日曜日の夜にまた夜行列車で帰って来たのです。"},{"id":{"d":784,"n":6,"t":3},"speaker":"B","text":"忙しい旅行ですね。"},{"id":{"d":784,"n":7,"t":3},"speaker":"A","text":"はい。どうしてもサンライズ瀬戸・出雲に乗りたかったのです。"},{"id":{"d":784,"n":8,"t":3},"speaker":"B","text":"行先は出雲だったのですか？遠くまでお疲れさまでした。"}];
INSERT INTO sentence [{"id":{"d":785,"n":1,"t":3},"speaker":"A","text":"出雲はいかがでしたか？"},{"id":{"d":785,"n":2,"t":3},"speaker":"B","text":"1日しか滞在時間がありませんでしたが、きちんと出雲大社にお参りしてきました。"},{"id":{"d":785,"n":3,"t":3},"speaker":"A","text":"良いご縁があると良いですね。"},{"id":{"d":785,"n":4,"t":3},"speaker":"B","text":"はい。お守りも買ってきました。ご利益があることを願います。"},{"id":{"d":785,"n":5,"t":3},"speaker":"A","text":"他にも観光をされたのですか？"},{"id":{"d":785,"n":6,"t":3},"speaker":"B","text":"はい。出雲そばを食べに行きました。出雲名物のぜんざいもいただきました。"},{"id":{"d":785,"n":7,"t":3},"speaker":"A","text":"グルメ旅ですね。"},{"id":{"d":785,"n":8,"t":3},"speaker":"B","text":"はい。旅行にはおいしいものが欠かせません。"}];
INSERT INTO sentence [{"id":{"d":786,"n":1,"t":3},"speaker":"A","text":"バスが来ませんね。"},{"id":{"d":786,"n":2,"t":3},"speaker":"B","text":"そうでうね。時刻表の通りなら、もう来ても良い頃だと思うのですが。"},{"id":{"d":786,"n":3,"t":3},"speaker":"A","text":"時刻表をもう一度見て来ます。"},{"id":{"d":786,"n":4,"t":3},"speaker":"B","text":"お願いします。"},{"id":{"d":786,"n":5,"t":3},"speaker":"A","text":"小林さん、すみません、私が時刻表を見間違えていました。"},{"id":{"d":786,"n":6,"t":3},"speaker":"B","text":"どういうことですか？"},{"id":{"d":786,"n":7,"t":3},"speaker":"A","text":"今日は平日なのに休日のところを見ていました。次のバスは1時間20分後に来ます。"},{"id":{"d":786,"n":8,"t":3},"speaker":"B","text":"そうでしたか。では、タクシーで行きましょう。"}];
INSERT INTO sentence [{"id":{"d":787,"n":1,"t":3},"speaker":"A","text":"あそこに、レンタサイクルの看板がありますよ。"},{"id":{"d":787,"n":2,"t":3},"speaker":"B","text":"ここから自転車で行くのですか？"},{"id":{"d":787,"n":3,"t":3},"speaker":"A","text":"その方が安く移動できるかと思いまして。タクシー代を割り勘するのと、レンタサイクルを利用するのと、どちらが安いでしょうか？"},{"id":{"d":787,"n":4,"t":3},"speaker":"B","text":"少々料金が高くても、私は楽な方がいいです。"},{"id":{"d":787,"n":5,"t":3},"speaker":"A","text":"今日は良いお天気なので、自転車も気持ちが良いと思いますよ。"},{"id":{"d":787,"n":6,"t":3},"speaker":"B","text":"私は自転車の運転に慣れていないので少し不安です。"},{"id":{"d":787,"n":7,"t":3},"speaker":"A","text":"自転車で移動することに不安があるのですね？"},{"id":{"d":787,"n":8,"t":3},"speaker":"B","text":"はい、最後に自転車に乗ったのは小学生の時なのです。タクシーに乗りましょう。"},{"id":{"d":787,"n":9,"t":3},"speaker":"A","text":"分かりました。では、駅前からタクシーに乗りましょう。"}];
INSERT INTO sentence [{"id":{"d":788,"n":1,"t":3},"speaker":"A","text":"明日の待ち合わせ場所はどこにしますか？"},{"id":{"d":788,"n":2,"t":3},"speaker":"B","text":"東京駅は分かりにくいですよね。"},{"id":{"d":788,"n":3,"t":3},"speaker":"A","text":"そうですよね。銀の鈴なら分かりやすいでしょうか？"},{"id":{"d":788,"n":4,"t":3},"speaker":"B","text":"銀の鈴は確かに分かりやすいですが、新幹線の改札とかなり離れていますよね。"},{"id":{"d":788,"n":5,"t":3},"speaker":"A","text":"そうですね。銀の鈴からホームまでの移動時間を考える必要があります。"},{"id":{"d":788,"n":6,"t":3},"speaker":"B","text":"指定席の予約はもう済んでいるので、発車時刻に3号車の前で待ち合わせをするのはどうでしょうか？"},{"id":{"d":788,"n":7,"t":3},"speaker":"A","text":"それは良い案ですね。お弁当は各自買ってから行きましょう。"},{"id":{"d":788,"n":8,"t":3},"speaker":"B","text":"分かりました。では、明日、新幹線のホームで会いましょう。"}];
INSERT INTO sentence [{"id":{"d":789,"n":1,"t":3},"speaker":"A","text":"3日間の旅行にしては、荷物が多いですね。"},{"id":{"d":789,"n":2,"t":3},"speaker":"B","text":"心配性なので、何でも持ってきてしまうのです。"},{"id":{"d":789,"n":3,"t":3},"speaker":"A","text":"ドライヤーは持っていますか？"},{"id":{"d":789,"n":4,"t":3},"speaker":"B","text":"はい、持ってきています。"},{"id":{"d":789,"n":5,"t":3},"speaker":"A","text":"ガイドブックや雨具は持っていますか？"},{"id":{"d":789,"n":6,"t":3},"speaker":"B","text":"はい、それも万全です。"},{"id":{"d":789,"n":7,"t":3},"speaker":"A","text":"私は持ってこなかったので、貸してくださいね。"},{"id":{"d":789,"n":8,"t":3},"speaker":"B","text":"良いですよ。でも、あなたような人のために荷物が多くなってしまうのですよ。"}];
INSERT INTO sentence [{"id":{"d":790,"n":1,"t":3},"speaker":"A","text":"沖縄旅行は初めてです。"},{"id":{"d":790,"n":2,"t":3},"speaker":"B","text":"そうですか。飛行機の窓からきれいな海が見えましたね。"},{"id":{"d":790,"n":3,"t":3},"speaker":"A","text":"素晴らしかったです。私は、きれいな海を見て、おいしい料理を食べるだけで満足です。"},{"id":{"d":790,"n":4,"t":3},"speaker":"B","text":"私は観光も楽しみです。"},{"id":{"d":790,"n":5,"t":3},"speaker":"A","text":"首里城は火事で焼けてしまって残念でしたね。"},{"id":{"d":790,"n":6,"t":3},"speaker":"B","text":"はい。でも、今も見られるところがあると聞いたので行ってみたいです。"},{"id":{"d":790,"n":7,"t":3},"speaker":"A","text":"そうなのですね。ぜひ行きましょう。"},{"id":{"d":790,"n":8,"t":3},"speaker":"B","text":"現地には募金箱もあるようです。私たちのお財布から出せるのは本当に少しの金額ですけれど、首里城復興に役立つのであれば協力したいです。"}];
INSERT INTO sentence [{"id":{"d":791,"n":1,"t":3},"speaker":"A","text":"沖縄は豚肉がおいしいですね。"},{"id":{"d":791,"n":2,"t":3},"speaker":"B","text":"「あぐー豚」というらしいですよ。"},{"id":{"d":791,"n":3,"t":3},"speaker":"A","text":"そうなのですね。あまりにもおいしくて、ラフテーだけを食べるつもりが、気が付いたら耳や足も食べてしまっていました。"},{"id":{"d":791,"n":4,"t":3},"speaker":"B","text":"良いことです。沖縄では「豚は鳴き声以外全て食べる」とも言われているそうですよ。"},{"id":{"d":791,"n":5,"t":3},"speaker":"A","text":"すてきな言葉ですね。明日は中身汁というのに挑戦してみませんか？"},{"id":{"d":791,"n":6,"t":3},"speaker":"B","text":"賛成です。私たちが旅行に来ると、食べ歩きばかりになってしまいますね。"},{"id":{"d":791,"n":7,"t":3},"speaker":"A","text":"仕方ありません。良いお酒があれば、おいしいものを食べたくなるものです。"},{"id":{"d":791,"n":8,"t":3},"speaker":"B","text":"そうですね。泡盛がおいしいから仕方がありませんね。"}];
INSERT INTO sentence [{"id":{"d":792,"n":1,"t":3},"speaker":"A","text":"沖縄土産を持って来ましたよ。"},{"id":{"d":792,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。お姉さん、沖縄に行ったのですか？"},{"id":{"d":792,"n":3,"t":3},"speaker":"A","text":"はい、そうです。お土産は島トウガラシです。あなたは辛いものが好きでしょう？"},{"id":{"d":792,"n":4,"t":3},"speaker":"B","text":"確かに辛いものは好きですが、お土産はこれだけですか？"},{"id":{"d":792,"n":5,"t":3},"speaker":"A","text":"荷物が多くなってしまって、これしか入らなかったのですよ。"},{"id":{"d":792,"n":6,"t":3},"speaker":"B","text":"本当ですか？"},{"id":{"d":792,"n":7,"t":3},"speaker":"A","text":"冗談ですよ。そのような悲しそうな顔をしないでください。はい、沖縄のお菓子セットです。"},{"id":{"d":792,"n":8,"t":3},"speaker":"B","text":"ちんすこう、紅芋タルト、サーターアンダギーですね。これほどまでにたくさんのお土産をありがとうございます。"},{"id":{"d":792,"n":9,"t":3},"speaker":"A","text":"どういたしまして。喜んでもらえて良かったです。"}];
INSERT INTO sentence [{"id":{"d":793,"n":1,"t":3},"speaker":"A","text":"いとこが、東京観光に来ることになりました。"},{"id":{"d":793,"n":2,"t":3},"speaker":"B","text":"どちらから来られるのですか？"},{"id":{"d":793,"n":3,"t":3},"speaker":"A","text":"山形です。"},{"id":{"d":793,"n":4,"t":3},"speaker":"B","text":"あなたが東京案内をしてあげるのですか？"},{"id":{"d":793,"n":5,"t":3},"speaker":"A","text":"はい。どこを案内したら良いと思いますか？"},{"id":{"d":793,"n":6,"t":3},"speaker":"B","text":"東京が初めてでしたら、東京スカイツリーと東京タワーと浅草寺はどうでしょうか？東京の定番の観光スポットです。"},{"id":{"d":793,"n":7,"t":3},"speaker":"A","text":"良いですね。高校生でもそのコースで楽しんでもらえるでしょうか？"},{"id":{"d":793,"n":8,"t":3},"speaker":"B","text":"高校生なら、その2つに加えて、東京ディズニーランドへ行っても良いかもしれません。東京ディズニーランドは東京の観光地ではありませんけれどね。"}];
INSERT INTO sentence [{"id":{"d":794,"n":1,"t":3},"speaker":"A","text":"昨日、高校生のいとこを連れて、東京スカイツリーに行ってきました。"},{"id":{"d":794,"n":2,"t":3},"speaker":"B","text":"そのいとこというのは、以前あなたが言っていた、山形の子ですね。"},{"id":{"d":794,"n":3,"t":3},"speaker":"A","text":"そうです。"},{"id":{"d":794,"n":4,"t":3},"speaker":"B","text":"東京観光はどうでしたか？"},{"id":{"d":794,"n":5,"t":3},"speaker":"A","text":"他にもいろいろ回ろうと思っていたのですが、東京スカイツリー周辺の観光だけで終わってしまいました。"},{"id":{"d":794,"n":6,"t":3},"speaker":"B","text":"東京スカイツリー周辺を時間をかけて見てきたのですね。"},{"id":{"d":794,"n":7,"t":3},"speaker":"A","text":"いとこの奈美ちゃんが東京スカイツリーのソラマチにあるすみだ水族館で動かなくなってしまいまして。"},{"id":{"d":794,"n":8,"t":3},"speaker":"B","text":"すみだ水族館が気に入ったのですね。"},{"id":{"d":794,"n":9,"t":3},"speaker":"A","text":"そのようです。ペンギンが気に入ったようでずっと見ていました。"},{"id":{"d":794,"n":10,"t":3},"speaker":"B","text":"その気持は私も分かります。すみだ水族館のペンギンはとてもかわいいですものね。"}];
INSERT INTO sentence [{"id":{"d":795,"n":1,"t":3},"speaker":"A","text":"奈美ちゃんはペンギンが気に入ったのですね。"},{"id":{"d":795,"n":2,"t":3},"speaker":"B","text":"はい。ペンギンを見たのは初めてです。かわいいですね。"},{"id":{"d":795,"n":3,"t":3},"speaker":"A","text":"ここの水族館は、特にペンギンが有名ですからね。"},{"id":{"d":795,"n":4,"t":3},"speaker":"B","text":"そうなのですね。ペンギンのぬいぐるみとお菓子を買ってくださって、ありがとうございました。"},{"id":{"d":795,"n":5,"t":3},"speaker":"A","text":"どういたしまして。ペンギン好きになった奈美ちゃんには、一番の東京土産になると思ったのですよ。"},{"id":{"d":795,"n":6,"t":3},"speaker":"B","text":"明日は叔母さんが東京を案内してくださるのですよね？明日も水族館がいいです。"},{"id":{"d":795,"n":7,"t":3},"speaker":"A","text":"せっかく東京まで来たのですから、ぜひいろいろ回ってみてください。母には「浅草寺に行くように」と言ってあります。"},{"id":{"d":795,"n":8,"t":3},"speaker":"B","text":"明日は叔母さんと浅草寺観光ですか。それも楽しみです。"}];
INSERT INTO sentence [{"id":{"d":796,"n":1,"t":3},"speaker":"A","text":"ゴールデンウイークは、海外旅行を計画しています。"},{"id":{"d":796,"n":2,"t":3},"speaker":"B","text":"良いですね。飛び石連休の間に有給休暇を入れて長期休暇にするのですか？"},{"id":{"d":796,"n":3,"t":3},"speaker":"A","text":"はい。そうすれば10連休になるので、ハワイ州に行こうかと思っているのです。"},{"id":{"d":796,"n":4,"t":3},"speaker":"B","text":"ゴールデンウイーク中は旅行代金が高くありませんか？"},{"id":{"d":796,"n":5,"t":3},"speaker":"A","text":"確かに少し高いです。でも、今回の旅行は新婚旅行の代わりなので、この際、旅行代金が高くても構いません。"},{"id":{"d":796,"n":6,"t":3},"speaker":"B","text":"そういえば、山田さんは新婚でしたね。"},{"id":{"d":796,"n":7,"t":3},"speaker":"A","text":"はい。結婚式の後は仕事が忙しすぎて休めなかったので、ようやく新婚旅行へ行くことができます。"},{"id":{"d":796,"n":8,"t":3},"speaker":"B","text":"楽しんできてくださいね。"},{"id":{"d":796,"n":9,"t":3},"speaker":"A","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":797,"n":1,"t":3},"speaker":"A","text":"明日は、ハイキングですね。お昼ごはんはどうしましょうか？"},{"id":{"d":797,"n":2,"t":3},"speaker":"B","text":"泊まっている民宿のおかみさんが、ハイキングに持参するためのおにぎりを作ってくださいますので、それをお昼ごはんにしましょう。"},{"id":{"d":797,"n":3,"t":3},"speaker":"A","text":"それはありがたいです。お昼ごはんが楽しみです。"},{"id":{"d":797,"n":4,"t":3},"speaker":"B","text":"水筒を事前に預けておけば、お茶も入れてくださるそうです。"},{"id":{"d":797,"n":5,"t":3},"speaker":"A","text":"それもぜひお願いしましょう。お菓子、タオル、雨具は持ちました。他に忘れ物はありませんか？"},{"id":{"d":797,"n":6,"t":3},"speaker":"B","text":"持っていくものはそれで良いと思います。スマートフォンの充電はしましたか？"},{"id":{"d":797,"n":7,"t":3},"speaker":"A","text":"忘れていました。"},{"id":{"d":797,"n":8,"t":3},"speaker":"B","text":"確認して良かったです。あとは、念のために紙の地図も持って行きましょう。"},{"id":{"d":797,"n":9,"t":3},"speaker":"A","text":"そうですね。バッテリーの残量がなくなってしまったらスマートフォンで地図は見られませんからね。"}];
INSERT INTO sentence [{"id":{"d":798,"n":1,"t":3},"speaker":"A","text":"コーヒーをください。"},{"id":{"d":798,"n":2,"t":3},"speaker":"B","text":"ホットコーヒーでよろしいですか？"},{"id":{"d":798,"n":3,"t":3},"speaker":"A","text":"はい。それと、後でお手洗いをお借りしてもよいでしょうか？"},{"id":{"d":798,"n":4,"t":3},"speaker":"B","text":"どうぞ。左手にドアがございます。"},{"id":{"d":798,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。コーヒーの良い香りがしますね。"},{"id":{"d":798,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。お客さまはご旅行ですか？"},{"id":{"d":798,"n":7,"t":3},"speaker":"A","text":"はい。この辺りは見どころが多くて、少し歩き疲れてしまいました。"},{"id":{"d":798,"n":8,"t":3},"speaker":"B","text":"そうでしたか。ゆっくり休んでくださいね。"}];
INSERT INTO sentence [{"id":{"d":799,"n":1,"t":3},"speaker":"A","text":"ここは、まるで食堂車のようですね。"},{"id":{"d":799,"n":2,"t":3},"speaker":"B","text":"そうですね。鉄道博物館の中にはこのようなレストランがあるのですね。"},{"id":{"d":799,"n":3,"t":3},"speaker":"A","text":"そういえば、昔、寝台特急の北斗星で北海道に行きましたよね？あそこに少し似ていませんか？"},{"id":{"d":799,"n":4,"t":3},"speaker":"B","text":"はい。確かに、あの時の食堂車によく似ています。"},{"id":{"d":799,"n":5,"t":3},"speaker":"A","text":"懐かしいです。"},{"id":{"d":799,"n":6,"t":3},"speaker":"B","text":"注文するメニューは決まりましたか？"},{"id":{"d":799,"n":7,"t":3},"speaker":"A","text":"まだ迷っています。あの時と同じく一番高いメニューを頼みませんか？"},{"id":{"d":799,"n":8,"t":3},"speaker":"B","text":"良いですね。昔、北斗星でも奮発してフレンチのコースを注文しましたね。"},{"id":{"d":799,"n":9,"t":3},"speaker":"A","text":"最近の列車には、食堂車がなくなってしまって悲しいです。"},{"id":{"d":799,"n":10,"t":3},"speaker":"B","text":"そうですね。今日はあの時のような豪華な食事で、昔を懐かしみましょう。"}];
INSERT INTO sentence [{"id":{"d":800,"n":1,"t":3},"speaker":"A","text":"出張で北海道へ行きましたが、忙しすぎてお土産が買えませんでした。"},{"id":{"d":800,"n":2,"t":3},"speaker":"B","text":"それで、手ぶらで帰宅したのですか？"},{"id":{"d":800,"n":3,"t":3},"speaker":"A","text":"はい。お土産を期待していた家族がへそを曲げてしまいました。"},{"id":{"d":800,"n":4,"t":3},"speaker":"B","text":"北海道土産をとても楽しみにされていたのですね。そういえば、今、近くのデパートで北海道フェアを行っているそうですよ。"},{"id":{"d":800,"n":5,"t":3},"speaker":"A","text":"なるほど、そこへ行けば北海道の名産品が手に入りますね。"},{"id":{"d":800,"n":6,"t":3},"speaker":"B","text":"はい。有名なお土産も販売されていると聞きました。"},{"id":{"d":800,"n":7,"t":3},"speaker":"A","text":"良いことを聞きました。早速今日にでも、そのデパートで北海道土産を買って帰ろうと思います。"},{"id":{"d":800,"n":8,"t":3},"speaker":"B","text":"ご家族の方もきっと喜ばれることでしょうね。"},{"id":{"d":800,"n":9,"t":3},"speaker":"A","text":"そうだと良いのですが。"}];
INSERT INTO sentence [{"id":{"d":801,"n":1,"t":3},"speaker":"A","text":"卒業旅行は、瀬戸内海に行ってきました。"},{"id":{"d":801,"n":2,"t":3},"speaker":"B","text":"海に行くことが目的だったのですか？"},{"id":{"d":801,"n":3,"t":3},"speaker":"A","text":"いいえ。瀬戸内海の直島が目的地でした。"},{"id":{"d":801,"n":4,"t":3},"speaker":"B","text":"直島ですか？どこかで聞いたことがあるような気がします。"},{"id":{"d":801,"n":5,"t":3},"speaker":"A","text":"直島は、美術館や野外作品がたくさんあるアートの島です。"},{"id":{"d":801,"n":6,"t":3},"speaker":"B","text":"思い出しました。雑誌で直島の作品を見たことがあります。"},{"id":{"d":801,"n":7,"t":3},"speaker":"A","text":"私たちは皆アートを専攻しているので、みんな大興奮でしたよ。"},{"id":{"d":801,"n":8,"t":3},"speaker":"B","text":"良い卒業旅行だったのですね。"}];
INSERT INTO sentence [{"id":{"d":802,"n":1,"t":3},"speaker":"A","text":"直島のお土産です。"},{"id":{"d":802,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":802,"n":3,"t":3},"speaker":"A","text":"直島は塩が特産だそうで、お土産品のお菓子には塩味のものが多くありました。"},{"id":{"d":802,"n":4,"t":3},"speaker":"B","text":"塩キャラメル、塩ドーナツ、塩せんべいに塩クッキーですね。"},{"id":{"d":802,"n":5,"t":3},"speaker":"A","text":"まだありますけれど、今日はこれだけお渡しします。"},{"id":{"d":802,"n":6,"t":3},"speaker":"B","text":"それほどたくさん頂いてよいのですか？"},{"id":{"d":802,"n":7,"t":3},"speaker":"A","text":"はい。全部塩味ですが、どれもおいしいですよ。"},{"id":{"d":802,"n":8,"t":3},"speaker":"B","text":"いただくのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":803,"n":1,"t":3},"speaker":"A","text":"私たちは旅行中でもファミリーレストランで食事をしていますね。"},{"id":{"d":803,"n":2,"t":3},"speaker":"B","text":"そうですね。ファミリーレストランは安くて安心できます。"},{"id":{"d":803,"n":3,"t":3},"speaker":"A","text":"私は、たまには別のところで食事をしたいです。"},{"id":{"d":803,"n":4,"t":3},"speaker":"B","text":"飽きてしまったのですか？"},{"id":{"d":803,"n":5,"t":3},"speaker":"A","text":"いいえ。せっかくなので、その土地でしか食べることのできないおいしいものを、そこにしかないお店で食べてみたいと思いました。"},{"id":{"d":803,"n":6,"t":3},"speaker":"B","text":"では、今日の昼食は、ファミリーレストランではなく、他のお店でいただきましょう。何を食べに行きますか？"},{"id":{"d":803,"n":7,"t":3},"speaker":"A","text":"まさかお母さんが承諾してくれると思わなかったので、まだ調べていません。"},{"id":{"d":803,"n":8,"t":3},"speaker":"B","text":"では、今から調べましょう。"}];
INSERT INTO sentence [{"id":{"d":804,"n":1,"t":3},"speaker":"A","text":"東京に来たからには、ぜひともパンケーキを食べたいです。"},{"id":{"d":804,"n":2,"t":3},"speaker":"B","text":"気合が入っていますね。"},{"id":{"d":804,"n":3,"t":3},"speaker":"A","text":"はい。地元では食べられないパンケーキを食べたいのです。"},{"id":{"d":804,"n":4,"t":3},"speaker":"B","text":"パンケーキが食べられるお店は、調べてあるのですか？"},{"id":{"d":804,"n":5,"t":3},"speaker":"A","text":"もちろん調べてきました。このビルの中にあるはずです。"},{"id":{"d":804,"n":6,"t":3},"speaker":"B","text":"もしかして、あの行列ができているお店ですか？"},{"id":{"d":804,"n":7,"t":3},"speaker":"A","text":"そのようです。列に並びましょう。"},{"id":{"d":804,"n":8,"t":3},"speaker":"B","text":"お店に入れるまでに1時間くらいはかかりそうですね。"}];
INSERT INTO sentence [{"id":{"d":805,"n":1,"t":3},"speaker":"A","text":"暗くなってきたので、夕飯を食べて帰りましょうか？"},{"id":{"d":805,"n":2,"t":3},"speaker":"B","text":"そうしましょう。"},{"id":{"d":805,"n":3,"t":3},"speaker":"A","text":"この商店街は閉まっているお店が多いですね。"},{"id":{"d":805,"n":4,"t":3},"speaker":"B","text":"観光地の商店は閉まる時間が早いということを忘れていました。"},{"id":{"d":805,"n":5,"t":3},"speaker":"A","text":"居酒屋なら開いていますけれど、どうしますか？"},{"id":{"d":805,"n":6,"t":3},"speaker":"B","text":"私はお酒を飲むより食事をしたい気分です。"},{"id":{"d":805,"n":7,"t":3},"speaker":"A","text":"私も同感です。"},{"id":{"d":805,"n":8,"t":3},"speaker":"B","text":"電車に乗って地元に帰れば、いつものお店が開いていますよ。そこで食事にしましょう。"},{"id":{"d":805,"n":9,"t":3},"speaker":"A","text":"そうですね。では駅へ向かいましょう。"}];
INSERT INTO sentence [{"id":{"d":806,"n":1,"t":3},"speaker":"A","text":"アイスクリームをお取り寄せしたのですか？"},{"id":{"d":806,"n":2,"t":3},"speaker":"B","text":"はい。先日、秋田に行ったときに食べたババヘラアイスが忘れられなくて、取り寄せてみました。"},{"id":{"d":806,"n":3,"t":3},"speaker":"A","text":"ババヘラアイスという言葉は、以前、テレビで聞いたことがある気がします。"},{"id":{"d":806,"n":4,"t":3},"speaker":"B","text":"秋田の名物ですよ。おばあさんがヘラで盛り付けてくれるバラの形のアイスです。"},{"id":{"d":806,"n":5,"t":3},"speaker":"A","text":"思い出しました。ヘラだけで、あのようなきれいな形にできるのですか？"},{"id":{"d":806,"n":6,"t":3},"speaker":"B","text":"任せてください。挑戦してみます。"},{"id":{"d":806,"n":7,"t":3},"speaker":"A","text":"なかなか苦戦していますね。"},{"id":{"d":806,"n":8,"t":3},"speaker":"B","text":"とても難しいです。バラの形にするのは私には無理なようです。"}];
INSERT INTO sentence [{"id":{"d":807,"n":1,"t":3},"speaker":"A","text":"きれいな景色ですね。"},{"id":{"d":807,"n":2,"t":3},"speaker":"B","text":"ええ。展望台まで登ってきたかいがありました。"},{"id":{"d":807,"n":3,"t":3},"speaker":"A","text":"あちらに有料の望遠鏡もありますよ。"},{"id":{"d":807,"n":4,"t":3},"speaker":"B","text":"何か拡大して見たいものがありますか？"},{"id":{"d":807,"n":5,"t":3},"speaker":"A","text":"山のふもとの動物園をここから見てみたいです。園内の動物も見えるでしょうか？"},{"id":{"d":807,"n":6,"t":3},"speaker":"B","text":"ここからだと、サルは見えるかもしれませんね。"},{"id":{"d":807,"n":7,"t":3},"speaker":"A","text":"では、サルを探してみます。"},{"id":{"d":807,"n":8,"t":3},"speaker":"B","text":"見えるといいですね。望遠鏡は100円で3分使えるようです。"}];
INSERT INTO sentence [{"id":{"d":808,"n":1,"t":3},"speaker":"A","text":"温泉街の夜はにぎやかですね。"},{"id":{"d":808,"n":2,"t":3},"speaker":"B","text":"みんな、夕涼みに出ているのでしょうか？"},{"id":{"d":808,"n":3,"t":3},"speaker":"A","text":"あちらの小川の方に人が集まっているような気がします。"},{"id":{"d":808,"n":4,"t":3},"speaker":"B","text":"行ってみますか？"},{"id":{"d":808,"n":5,"t":3},"speaker":"A","text":"そうですね。何か見るものがあるかもしれません。"},{"id":{"d":808,"n":6,"t":3},"speaker":"B","text":"あれは蛍ではありませんか？"},{"id":{"d":808,"n":7,"t":3},"speaker":"A","text":"蛍ですか？どこですか？"},{"id":{"d":808,"n":8,"t":3},"speaker":"B","text":"そこに光っていますよ。"}];
INSERT INTO sentence [{"id":{"d":809,"n":1,"t":3},"speaker":"A","text":"ヨットでクルージングとは、ぜいたくですね。"},{"id":{"d":809,"n":2,"t":3},"speaker":"B","text":"お父さんもお母さんも喜んでいるから良かったでしょう？"},{"id":{"d":809,"n":3,"t":3},"speaker":"A","text":"はい。徹さん、両親の金婚式のお祝いを計画してくれてありがとうございます。"},{"id":{"d":809,"n":4,"t":3},"speaker":"B","text":"どういたしまして。今日はランチクルーズを予約しているので、もう少ししたら、コース料理が出てきますよ。"},{"id":{"d":809,"n":5,"t":3},"speaker":"A","text":"楽しみです。そういえば、2人に渡すために用意した花束とプレゼントはどこにしまったのですか？"},{"id":{"d":809,"n":6,"t":3},"speaker":"B","text":"先ほど、船長さんに渡しておきました。"},{"id":{"d":809,"n":7,"t":3},"speaker":"A","text":"サプライズプレゼントが成功するか、少し緊張してきました。"},{"id":{"d":809,"n":8,"t":3},"speaker":"B","text":"大丈夫、必ず成功しますよ。せっかくの機会ですから、君もランチクルーズを楽しんでください。"}];
INSERT INTO sentence [{"id":{"d":810,"n":1,"t":3},"speaker":"A","text":"沖縄旅行の思い出を、絵日記に描いているのですね。"},{"id":{"d":810,"n":2,"t":3},"speaker":"B","text":"はい。今、作成している最中です。"},{"id":{"d":810,"n":3,"t":3},"speaker":"A","text":"この絵は何をしているところを描いたものですか？"},{"id":{"d":810,"n":4,"t":3},"speaker":"B","text":"ヘルメットのようなものをかぶって、海の中を歩いているところを描いたものです。"},{"id":{"d":810,"n":5,"t":3},"speaker":"A","text":"シーウォークをした時ですね。頭が随分大きいのですね。"},{"id":{"d":810,"n":6,"t":3},"speaker":"B","text":"はい。大きいものをかぶって歩きましたから。"},{"id":{"d":810,"n":7,"t":3},"speaker":"A","text":"まるで雪だるまのようです。"},{"id":{"d":810,"n":8,"t":3},"speaker":"B","text":"ええ。海の中ではみんな雪だるまのようでした。"}];
INSERT INTO sentence [{"id":{"d":811,"n":1,"t":3},"speaker":"A","text":"スパリゾートハワイアンズは、東京から無料の送迎バスがあるのですね。知りませんでした。"},{"id":{"d":811,"n":2,"t":3},"speaker":"B","text":"それはうれしいですね。家族で行くと交通費も馬鹿になりませんからね。"},{"id":{"d":811,"n":3,"t":3},"speaker":"A","text":"はい。これまでは公共交通機関で行っていましたが、今回は送迎バスを利用しようと思います。"},{"id":{"d":811,"n":4,"t":3},"speaker":"B","text":"そういえば、なぜ突然スパリゾートハワイアンズに行こうと思ったのですか？"},{"id":{"d":811,"n":5,"t":3},"speaker":"A","text":"CMを見て、久しぶりに行きたくなりました。"},{"id":{"d":811,"n":6,"t":3},"speaker":"B","text":"「こっちのハワイは温泉もある」というナレーションが入り、飛行機が戻ってくるCMですね。"},{"id":{"d":811,"n":7,"t":3},"speaker":"A","text":"そうです。実際、私はこちらののハワイの方が好きです。"},{"id":{"d":811,"n":8,"t":3},"speaker":"B","text":"そうなのですか？"},{"id":{"d":811,"n":9,"t":3},"speaker":"A","text":"はい。子どもたちを迷子の心配もなく遊ばせられますし、私ものんびり温泉に漬かって休めるのでとても良いです。"}];
INSERT INTO sentence [{"id":{"d":812,"n":1,"t":3},"speaker":"A","text":"バイキング料理を食べ過ぎました。"},{"id":{"d":812,"n":2,"t":3},"speaker":"B","text":"私も体が重くなりました。"},{"id":{"d":812,"n":3,"t":3},"speaker":"A","text":"どれも全部おいしいので、箸が止まらなくなりました。"},{"id":{"d":812,"n":4,"t":3},"speaker":"B","text":"食べ過ぎたのは食事がおいしかったからですか？"},{"id":{"d":812,"n":5,"t":3},"speaker":"A","text":"はい。そういうことにしておきましょう。"},{"id":{"d":812,"n":6,"t":3},"speaker":"B","text":"満腹になったことですし、この後は、サウナで汗を出して消化するのはどうですか？"},{"id":{"d":812,"n":7,"t":3},"speaker":"A","text":"いいえ。今、サウナに行ったら具合が悪くなりそうです。"},{"id":{"d":812,"n":8,"t":3},"speaker":"B","text":"そうですか。では、取りあえず部屋で休みましょう。"}];
INSERT INTO sentence [{"id":{"d":813,"n":1,"t":3},"speaker":"A","text":"ずっと街歩きをしていたので、歩き疲れました。"},{"id":{"d":813,"n":2,"t":3},"speaker":"B","text":"そこのお団子屋さんで休憩しましょう。"},{"id":{"d":813,"n":3,"t":3},"speaker":"A","text":"お店の中で食べられるのでしょうか？"},{"id":{"d":813,"n":4,"t":3},"speaker":"B","text":"いいえ。テイクアウトだけのようですね。"},{"id":{"d":813,"n":5,"t":3},"speaker":"A","text":"では、他に喫茶店を探しましょう。座って休みたいです。"},{"id":{"d":813,"n":6,"t":3},"speaker":"B","text":"そういえば、少し戻ったところに公園がありました。お団子を買って、そこの公園内で食べるのはどうですか？"},{"id":{"d":813,"n":7,"t":3},"speaker":"A","text":"それもいいですね。私はお茶を買ってきますから、みたらし団子を買ってきてください。"},{"id":{"d":813,"n":8,"t":3},"speaker":"B","text":"了解しました。"}];
INSERT INTO sentence [{"id":{"d":814,"n":1,"t":3},"speaker":"A","text":"写真を撮っていただけますか？"},{"id":{"d":814,"n":2,"t":3},"speaker":"B","text":"私はあまり写真撮影が上手ではありませんが、それでも構いませんか？"},{"id":{"d":814,"n":3,"t":3},"speaker":"A","text":"はい。二人一緒に写っていれば、それだけで十分です。"},{"id":{"d":814,"n":4,"t":3},"speaker":"B","text":"それでしたら撮りますよ。"},{"id":{"d":814,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。お願いします。"},{"id":{"d":814,"n":6,"t":3},"speaker":"B","text":"このカメラはシャッターが押せませんが、どうしたら良いのでしょう？"},{"id":{"d":814,"n":7,"t":3},"speaker":"A","text":"すみません。スイッチを入れていませんでした。"},{"id":{"d":814,"n":8,"t":3},"speaker":"B","text":"スイッチですか？"},{"id":{"d":814,"n":9,"t":3},"speaker":"A","text":"右上のボタンです。"},{"id":{"d":814,"n":10,"t":3},"speaker":"B","text":"これを押せば良いのですね。"},{"id":{"d":814,"n":11,"t":3},"speaker":"A","text":"そうです。すみません、ありがとうございます。"},{"id":{"d":814,"n":12,"t":3},"speaker":"B","text":"準備ができました。では、撮りますよ。"}];
INSERT INTO sentence [{"id":{"d":815,"n":1,"t":3},"speaker":"A","text":"仲居さん、これは灰皿ですか？"},{"id":{"d":815,"n":2,"t":3},"speaker":"B","text":"いいえ。これは「茶こぼし」といって、お茶を入れる際にお使いいただくものです。"},{"id":{"d":815,"n":3,"t":3},"speaker":"A","text":"どのようにして使うのですか？"},{"id":{"d":815,"n":4,"t":3},"speaker":"B","text":"急須にお湯を入れ過ぎた時は、こちらの穴の開いた蓋の上から捨てるのですよ。"},{"id":{"d":815,"n":5,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":815,"n":6,"t":3},"speaker":"B","text":"茶葉を捨てるときは穴の開いた蓋を取って、中に入れてください。"},{"id":{"d":815,"n":7,"t":3},"speaker":"A","text":"そのような使い方なのですね。灰皿かと思ってしまいました。"},{"id":{"d":815,"n":8,"t":3},"speaker":"B","text":"当館は全館禁煙でございますので、たばこはご遠慮くださいね。"}];
INSERT INTO sentence [{"id":{"d":816,"n":1,"t":3},"speaker":"A","text":"今度の連休に旅行をしようと思います。"},{"id":{"d":816,"n":2,"t":3},"speaker":"B","text":"どちらに行かれるのですか？"},{"id":{"d":816,"n":3,"t":3},"speaker":"A","text":"まだ決めていません。"},{"id":{"d":816,"n":4,"t":3},"speaker":"B","text":"ご夫婦での旅行なのですか？"},{"id":{"d":816,"n":5,"t":3},"speaker":"A","text":"はい、私の誕生日が近いので、行くことにしたのです。"},{"id":{"d":816,"n":6,"t":3},"speaker":"B","text":"三連休ですから、2泊3日ですか？"},{"id":{"d":816,"n":7,"t":3},"speaker":"A","text":"その予定です。"},{"id":{"d":816,"n":8,"t":3},"speaker":"B","text":"では、ここから近いところで、長野などはいかがです？今は新緑がきれいですよ。"},{"id":{"d":816,"n":9,"t":3},"speaker":"A","text":"良いですね。調べてみます。"}];
INSERT INTO sentence [{"id":{"d":817,"n":1,"t":3},"speaker":"A","text":"鎌倉で食べ歩きをしてきました。"},{"id":{"d":817,"n":2,"t":3},"speaker":"B","text":"小町通りは混んでいたでしょう？"},{"id":{"d":817,"n":3,"t":3},"speaker":"A","text":"小町通りの方ではなくて、御成通りを歩いてきました。"},{"id":{"d":817,"n":4,"t":3},"speaker":"B","text":"御成通りを選ぶとは、通ですね。"},{"id":{"d":817,"n":5,"t":3},"speaker":"A","text":"一度、小町通りに行こうとして、人の多さに尻込みしてしまっただけなのですけれどね。"},{"id":{"d":817,"n":6,"t":3},"speaker":"B","text":"何かおいしいものがありましたか？"},{"id":{"d":817,"n":7,"t":3},"speaker":"A","text":"はい。チョコレートや和菓子のお店がありました。さまよいながら、長谷まで歩いていました。"},{"id":{"d":817,"n":8,"t":3},"speaker":"B","text":"結構、歩きましたね。"}];
INSERT INTO sentence [{"id":{"d":818,"n":1,"t":3},"speaker":"A","text":"鎌倉のお土産です。"},{"id":{"d":818,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。これはクッキーですか？"},{"id":{"d":818,"n":3,"t":3},"speaker":"A","text":"はい。クッキーにラムレーズンとクリームを挟んだお菓子です。"},{"id":{"d":818,"n":4,"t":3},"speaker":"B","text":"おいしそうですね。"},{"id":{"d":818,"n":5,"t":3},"speaker":"A","text":"ラム酒の香りはしますが、酔うほどではないので仕事中でも食べられますよ。"},{"id":{"d":818,"n":6,"t":3},"speaker":"B","text":"休憩時間にコーヒーを入れて、お茶請けにいただきます。"},{"id":{"d":818,"n":7,"t":3},"speaker":"A","text":"私はお店でお茶もいただいてきました。他のお菓子もおいしかったですよ。"},{"id":{"d":818,"n":8,"t":3},"speaker":"B","text":"食べるのが楽しみです。今度行くときは、ぜひ誘ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":819,"n":1,"t":3},"speaker":"A","text":"これは、おいしいお茶ですね。"},{"id":{"d":819,"n":2,"t":3},"speaker":"B","text":"この間、妹が宇治に行ってきて、お土産にくれたものです。"},{"id":{"d":819,"n":3,"t":3},"speaker":"A","text":"宇治のお茶なのですね。"},{"id":{"d":819,"n":4,"t":3},"speaker":"B","text":"そうです。お茶のスイーツを目当てに、宇治まで行ってきたようですよ。"},{"id":{"d":819,"n":5,"t":3},"speaker":"A","text":"お茶菓子の土産はなかったのですか？"},{"id":{"d":819,"n":6,"t":3},"speaker":"B","text":"お団子と大福を買って来たらしいのですが、私に渡すまでに賞味期限切れになってしまったらしいです。"},{"id":{"d":819,"n":7,"t":3},"speaker":"A","text":"うっかり者の京子さんらしいですね。"},{"id":{"d":819,"n":8,"t":3},"speaker":"B","text":"はい。賞味期限が長いものにしてくれれば良かったのにと思いました。"}];
INSERT INTO sentence [{"id":{"d":820,"n":1,"t":3},"speaker":"A","text":"この先の小道は通り抜けられますか？"},{"id":{"d":820,"n":2,"t":3},"speaker":"B","text":"いいえ。そこは行き止まりになっています。"},{"id":{"d":820,"n":3,"t":3},"speaker":"A","text":"そうですか。この料理屋さんを探しているのですが、どこか分かりますか？"},{"id":{"d":820,"n":4,"t":3},"speaker":"B","text":"はい。その料理屋は、小道の突き当たりにありますよ。"},{"id":{"d":820,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。今から向かって、ランチタイムにはまだ間に合うでしょうか？"},{"id":{"d":820,"n":6,"t":3},"speaker":"B","text":"はい。まだ30分くらいは、大丈夫だと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":821,"n":1,"t":3},"speaker":"A","text":"桜が散ってしまいましたね。"},{"id":{"d":821,"n":2,"t":3},"speaker":"B","text":"今年は、お花見ができませんでした。"},{"id":{"d":821,"n":3,"t":3},"speaker":"A","text":"桜が満開の時期に、雨が多かったですからね。"},{"id":{"d":821,"n":4,"t":3},"speaker":"B","text":"そうですね。週末に箱根に行きませんか？"},{"id":{"d":821,"n":5,"t":3},"speaker":"A","text":"はい。温泉でゆっくりするのも良いですね。"},{"id":{"d":821,"n":6,"t":3},"speaker":"B","text":"温泉もですが、箱根は標高が高いので、桜もまだ散っていないと思うのです。"},{"id":{"d":821,"n":7,"t":3},"speaker":"A","text":"なるほど！それならお花見ができそうですね。"},{"id":{"d":821,"n":8,"t":3},"speaker":"B","text":"温泉と桜を見る旅にしましょう。"}];
INSERT INTO sentence [{"id":{"d":822,"n":1,"t":3},"speaker":"A","text":"自転車で県内一周旅行をしてきました。"},{"id":{"d":822,"n":2,"t":3},"speaker":"B","text":"日本一周ではなくて、県内一周旅行ですか？"},{"id":{"d":822,"n":3,"t":3},"speaker":"A","text":"ええ、時間がそれほどなかったのです。日本一周をするためには仕事を辞めなければなりません。"},{"id":{"d":822,"n":4,"t":3},"speaker":"B","text":"確かにそうですよね。県内だけなら、ゴールデンウイーク期間だけで回れるのですか？"},{"id":{"d":822,"n":5,"t":3},"speaker":"A","text":"はい。回ることができました。北海道のような広い県でなくて良かったです。"},{"id":{"d":822,"n":6,"t":3},"speaker":"B","text":"肌が真っ黒に日焼けされていたのは、自転車に乗っていたからなのですね。"},{"id":{"d":822,"n":7,"t":3},"speaker":"A","text":"そうだと思います。5月とはいえ、ずっと晴れていたので日差しがきつかったです。"},{"id":{"d":822,"n":8,"t":3},"speaker":"B","text":"天気に恵まれていたのですね。もし写真を撮っていたら見せてください。"}];
INSERT INTO sentence [{"id":{"d":823,"n":1,"t":3},"speaker":"A","text":"この後に行こうと思っていた博物館へは、今日は行かないことにしましょう。"},{"id":{"d":823,"n":2,"t":3},"speaker":"B","text":"どうしてですか？私は博物館にも行きたいです。"},{"id":{"d":823,"n":3,"t":3},"speaker":"A","text":"水族館の展示を見ることに夢中になり過ぎて、午前中だけの予定が午後まで長引いてしまったからです。"},{"id":{"d":823,"n":4,"t":3},"speaker":"B","text":"でも、まだ4時ですよ。"},{"id":{"d":823,"n":5,"t":3},"speaker":"A","text":"今から博物館へ向かっても、到着する頃には入場時間を過ぎてしまいます。"},{"id":{"d":823,"n":6,"t":3},"speaker":"B","text":"では、博物館へは来週行きましょう。"},{"id":{"d":823,"n":7,"t":3},"speaker":"A","text":"来週では、特別展示が終わってしまっていますよ。"},{"id":{"d":823,"n":8,"t":3},"speaker":"B","text":"私が見たいのは常設の恐竜の標本ですので、来週でも見られます。"}];
INSERT INTO sentence [{"id":{"d":824,"n":1,"t":3},"speaker":"A","text":"明日は叔母さんのお見舞いに行くのですよね？"},{"id":{"d":824,"n":2,"t":3},"speaker":"B","text":"はい。すみませんが、子どもたちと留守番をお願いします。"},{"id":{"d":824,"n":3,"t":3},"speaker":"A","text":"お安いご用です。もしよければ、明日、子どもたちのおにぎりと水筒の用意をしてくれますか？"},{"id":{"d":824,"n":4,"t":3},"speaker":"B","text":"それは構いませんが、明日はどこかに子どもたちを連れて行くのですか？"},{"id":{"d":824,"n":5,"t":3},"speaker":"A","text":"羽田空港のそばに、飛行機がよく見える公園があるのです。そこへ行こうかと思っています。"},{"id":{"d":824,"n":6,"t":3},"speaker":"B","text":"お弁当を持って、飛行機見物に行くのですね？"},{"id":{"d":824,"n":7,"t":3},"speaker":"A","text":"はい。子どもたちは飛行機が好きですからね。"},{"id":{"d":824,"n":8,"t":3},"speaker":"B","text":"私も、お見舞いが終わったらそちらに合流したいです。"}];
INSERT INTO sentence [{"id":{"d":825,"n":1,"t":3},"speaker":"A","text":"懸賞で旅行券10万円分が当たりました。"},{"id":{"d":825,"n":2,"t":3},"speaker":"B","text":"おめでとうございます。良かったですね。"},{"id":{"d":825,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。旅行先に悩んでいるのですが、どこへ行ったら良いと思いますか？"},{"id":{"d":825,"n":4,"t":3},"speaker":"B","text":"旅行へは一人で行くのですか？"},{"id":{"d":825,"n":5,"t":3},"speaker":"A","text":"できたら、母を連れて行こうと思っています。"},{"id":{"d":825,"n":6,"t":3},"speaker":"B","text":"お母さまとご一緒でしたら、近場の高級旅館に宿泊する方がいいと思いますよ。"},{"id":{"d":825,"n":7,"t":3},"speaker":"A","text":"なるほど。確かに旅行の目的地が遠いと、移動だけで疲れてしまいそうです。"},{"id":{"d":825,"n":8,"t":3},"speaker":"B","text":"はい。例えば、箱根や熱海などはいかがでしょうか？"}];
INSERT INTO sentence [{"id":{"d":826,"n":1,"t":3},"speaker":"A","text":"角館に行かれたそうですね？"},{"id":{"d":826,"n":2,"t":3},"speaker":"B","text":"はい。しだれ桜が満開できれいでしたよ。"},{"id":{"d":826,"n":3,"t":3},"speaker":"A","text":"良い時期に行かれましたね。"},{"id":{"d":826,"n":4,"t":3},"speaker":"B","text":"角館の武家屋敷には一度行ってみたかったのです。桜の時期でなくても良かったのですが、やはり満開の桜には感動しました。"},{"id":{"d":826,"n":5,"t":3},"speaker":"A","text":"武家屋敷に何か思い入れがあるのですか？"},{"id":{"d":826,"n":6,"t":3},"speaker":"B","text":"以前、時代物の映画を見てから憧れていたのです。"},{"id":{"d":826,"n":7,"t":3},"speaker":"A","text":"もしかして、少し前に賞をたくさん取った映画ですか？"},{"id":{"d":826,"n":8,"t":3},"speaker":"B","text":"はい。角館は映画のロケ地の一つなのです。映画を見ていつか行きたいと思っていました。"}];
INSERT INTO sentence [{"id":{"d":827,"n":1,"t":3},"speaker":"A","text":"きれいな写真ですね。"},{"id":{"d":827,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。先日、高知に行った時に撮影した写真です。"},{"id":{"d":827,"n":3,"t":3},"speaker":"A","text":"高知といえば、はりまや橋くらいしかないと思っていました。"},{"id":{"d":827,"n":4,"t":3},"speaker":"B","text":"高知は自然が豊かで良いところですよ。"},{"id":{"d":827,"n":5,"t":3},"speaker":"A","text":"水の色がこれほど青いのは、カメラにフィルターでもかけたのですか？"},{"id":{"d":827,"n":6,"t":3},"speaker":"B","text":"いいえ、これは自然の色なのです。「仁淀ブルー」という言葉を聞いたことはありませんか？"},{"id":{"d":827,"n":7,"t":3},"speaker":"A","text":"初めて聞きました。"},{"id":{"d":827,"n":8,"t":3},"speaker":"B","text":"高知の仁淀川の色のことをいうのです。仁淀川の鮮やかな青色は、本当に美しかったですよ。"}];
INSERT INTO sentence [{"id":{"d":828,"n":1,"t":3},"speaker":"A","text":"旅の写真はたくさんあるのですか？"},{"id":{"d":828,"n":2,"t":3},"speaker":"B","text":"はい。たくさんありますよ。お見せしますね。"},{"id":{"d":828,"n":3,"t":3},"speaker":"A","text":"タブレットで見ると、画面が大きくて見やすいですね。"},{"id":{"d":828,"n":4,"t":3},"speaker":"B","text":"スマートフォンで撮ったのですが、両親にも見せたくてタブレットで見られるようにしました。"},{"id":{"d":828,"n":5,"t":3},"speaker":"A","text":"私のデジカメでは画面が小さすぎて、お年寄りには見せられません。"},{"id":{"d":828,"n":6,"t":3},"speaker":"B","text":"デジカメの写真も、タブレットで見られるようにできますよ。"},{"id":{"d":828,"n":7,"t":3},"speaker":"A","text":"そうなのですね。今度タブレットを買いに行きたいです。"},{"id":{"d":828,"n":8,"t":3},"speaker":"B","text":"お買い物にお付き合いしますよ。"}];
INSERT INTO sentence [{"id":{"d":829,"n":1,"t":3},"speaker":"A","text":"最近、京都に良く行かれていますね。"},{"id":{"d":829,"n":2,"t":3},"speaker":"B","text":"はい。この写真集を買ってから、京都に夢中になっているのです。"},{"id":{"d":829,"n":3,"t":3},"speaker":"A","text":"「365日京都絶景の旅」という写真集ですね。"},{"id":{"d":829,"n":4,"t":3},"speaker":"B","text":"一日ごとに京都の絶景が紹介されている写真集です。同じ日付に同じ場所を探して歩くことが楽しいのです。"},{"id":{"d":829,"n":5,"t":3},"speaker":"A","text":"面白そうですね。でも、京都は日帰りで行けるとはいえ、毎日行くのは無理でしょう？"},{"id":{"d":829,"n":6,"t":3},"speaker":"B","text":"毎日はさすがに無理ですが、いつか全部の日付を埋められるかもしれないと思うだけで、心が躍ります。"},{"id":{"d":829,"n":7,"t":3},"speaker":"A","text":"しょっちゅうお土産も買ってきていただいていますが、毎回は要りませんよ。"},{"id":{"d":829,"n":8,"t":3},"speaker":"B","text":"おいしそうなものを見るとつい買ってしまうのです。おいしいものはみんなで食べましょう。"}];
INSERT INTO sentence [{"id":{"d":830,"n":1,"t":3},"speaker":"A","text":"横浜で、まるでサザンオールスターズの歌のようなデートをしてきました。"},{"id":{"d":830,"n":2,"t":3},"speaker":"B","text":"自分でデートプランを立てたのですか？"},{"id":{"d":830,"n":3,"t":3},"speaker":"A","text":"いいえ。その歌の歌詞を知ってか知らずか、彼がプランを立ててくれたのです。"},{"id":{"d":830,"n":4,"t":3},"speaker":"B","text":"マリーンルージュに乗って、大黒ふ頭に行って、ホテルニューグランドのシーガーディアンⅡでお食事をしたのですね？"},{"id":{"d":830,"n":5,"t":3},"speaker":"A","text":"大当たりです。彼は歌詞を知っていたと思いますか？"},{"id":{"d":830,"n":6,"t":3},"speaker":"B","text":"はい。多分、サザンオールスターズのファンなのだと思いますよ。"},{"id":{"d":830,"n":7,"t":3},"speaker":"A","text":"今回のことで気が付いたのですが、今まで一度もお互いの好きな音楽について話をしたことがなかったかもしれません。"},{"id":{"d":830,"n":8,"t":3},"speaker":"B","text":"今度、じっくり話してみたらいかがですか？"}];
INSERT INTO sentence [{"id":{"d":831,"n":1,"t":3},"speaker":"A","text":"この間乗っていた赤い車はどうされたのですか？"},{"id":{"d":831,"n":2,"t":3},"speaker":"B","text":"恥ずかしながら、廃車になりました。"},{"id":{"d":831,"n":3,"t":3},"speaker":"A","text":"新車で購入したばかりだということではなかったですか？"},{"id":{"d":831,"n":4,"t":3},"speaker":"B","text":"そうなのですが、購入後1週間で電柱にぶつけてしまいました。"},{"id":{"d":831,"n":5,"t":3},"speaker":"A","text":"けがはなかったのですか？"},{"id":{"d":831,"n":6,"t":3},"speaker":"B","text":"はい。少し首を痛めてしまいましたが、大きなけがはありませんでした。"},{"id":{"d":831,"n":7,"t":3},"speaker":"A","text":"命が無事で良かったです。"}];
INSERT INTO sentence [{"id":{"d":832,"n":1,"t":3},"speaker":"A","text":"このプリンはおいしいですね。"},{"id":{"d":832,"n":2,"t":3},"speaker":"B","text":"熱海の新しい名物らしいですよ。"},{"id":{"d":832,"n":3,"t":3},"speaker":"A","text":"お土産に買って、家に帰りたいです。"},{"id":{"d":832,"n":4,"t":3},"speaker":"B","text":"あなたのご家族は、あなたも入れて6人でしたよね？"},{"id":{"d":832,"n":5,"t":3},"speaker":"A","text":"そうです。祖父母と両親と兄と私です。"},{"id":{"d":832,"n":6,"t":3},"speaker":"B","text":"この瓶のプリンを6個持って帰るのは重くて大変かもしれません。"},{"id":{"d":832,"n":7,"t":3},"speaker":"A","text":"そうですよね。でもみんなにも食べさせてあげたいです。"},{"id":{"d":832,"n":8,"t":3},"speaker":"B","text":"送料がかかっても良いなら、宅配を利用する手もありますよ。"}];
INSERT INTO sentence [{"id":{"d":833,"n":1,"t":3},"speaker":"A","text":"雨が降り出しましたね。結構大雨です。"},{"id":{"d":833,"n":2,"t":3},"speaker":"B","text":"すてきなカフェだと思って長居をし過ぎましたね。"},{"id":{"d":833,"n":3,"t":3},"speaker":"A","text":"観光で疲れていましたし、紅茶がおいしかったですからね。"},{"id":{"d":833,"n":4,"t":3},"speaker":"B","text":"傘を売っているところはないでしょうか？"},{"id":{"d":833,"n":5,"t":3},"speaker":"A","text":"近くにはお店はなさそうですね。ホテルまで走りますか？"},{"id":{"d":833,"n":6,"t":3},"speaker":"B","text":"いいえ。少し距離があるので、無理がありますよ。"},{"id":{"d":833,"n":7,"t":3},"speaker":"A","text":"ホテルに電話したら迎えに来てくれないでしょうか？"},{"id":{"d":833,"n":8,"t":3},"speaker":"B","text":"そういえば、ホテルと駅の間は送迎バスがありましたね。電話してみましょう。"}];
INSERT INTO sentence [{"id":{"d":834,"n":1,"t":3},"speaker":"A","text":"キャンプ場までは、まだ遠いのですか？"},{"id":{"d":834,"n":2,"t":3},"speaker":"B","text":"いいえ。もう、そろそろ着く頃だと思います。"},{"id":{"d":834,"n":3,"t":3},"speaker":"A","text":"お昼過ぎには着くはずだったのですよね？"},{"id":{"d":834,"n":4,"t":3},"speaker":"B","text":"はい。そのはずでした。道を間違えてしまったのかもしれません。"},{"id":{"d":834,"n":5,"t":3},"speaker":"A","text":"キャンプ場を通り過ぎた可能性はありませんか？"},{"id":{"d":834,"n":6,"t":3},"speaker":"B","text":"すみません。カーナビゲーションシステムが故障してしまったので、分かりません。"},{"id":{"d":834,"n":7,"t":3},"speaker":"A","text":"より一層山奥に入っていくのは不安です。"},{"id":{"d":834,"n":8,"t":3},"speaker":"B","text":"そうですよね。Uターンして、来た道を戻ってみます。"}];
INSERT INTO sentence [{"id":{"d":835,"n":1,"t":3},"speaker":"A","text":"周遊バスの乗り場はここですか？"},{"id":{"d":835,"n":2,"t":3},"speaker":"B","text":"そうです。横浜は初めてですか？"},{"id":{"d":835,"n":3,"t":3},"speaker":"A","text":"はい。横浜中華街に行ったことはありますが、横浜市全体を観光しようと思ったのは初めてです。"},{"id":{"d":835,"n":4,"t":3},"speaker":"B","text":"それなら、周遊バスに乗るのは良いですね。あちこちで降りると楽しいと思いますよ。"},{"id":{"d":835,"n":5,"t":3},"speaker":"A","text":"一番のお薦めはどこですか？"},{"id":{"d":835,"n":6,"t":3},"speaker":"B","text":"私のお薦めは山下公園です。今の時期はバラが見事に咲いていますよ。"},{"id":{"d":835,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。行ってみます。"},{"id":{"d":835,"n":8,"t":3},"speaker":"B","text":"横浜山手西洋館にも行かれると良いですよ。"}];
INSERT INTO sentence [{"id":{"d":836,"n":1,"t":3},"speaker":"A","text":"今年のお盆休みは海外旅行をやめて、国内旅行にします。"},{"id":{"d":836,"n":2,"t":3},"speaker":"B","text":"国内のどこに行くのですか？"},{"id":{"d":836,"n":3,"t":3},"speaker":"A","text":"涼しい高原に行きたいと思っています。"},{"id":{"d":836,"n":4,"t":3},"speaker":"B","text":"毎年、暑い南の島ばかりを訪問していましたから、それも良いかもしれませんね。"},{"id":{"d":836,"n":5,"t":3},"speaker":"A","text":"今は行き先として志賀高原を考えています。"},{"id":{"d":836,"n":6,"t":3},"speaker":"B","text":"長野ですね？"},{"id":{"d":836,"n":7,"t":3},"speaker":"A","text":"はい。長野県です。きっと涼しくて良いですよ。"},{"id":{"d":836,"n":8,"t":3},"speaker":"B","text":"涼しいところでしたら、ホテルでのんびり読書をしたいです。"}];
INSERT INTO sentence [{"id":{"d":837,"n":1,"t":3},"speaker":"A","text":"いちごのかき氷を1つください。"},{"id":{"d":837,"n":2,"t":3},"speaker":"B","text":"はい。ミルクはどうしますか？"},{"id":{"d":837,"n":3,"t":3},"speaker":"A","text":"お願いします。夏の鎌倉は暑いですね。"},{"id":{"d":837,"n":4,"t":3},"speaker":"B","text":"これでも都心よりは涼しいのですよ。"},{"id":{"d":837,"n":5,"t":3},"speaker":"A","text":"そうですか？東京ではクーラーが効いているところにばかりいますからね。"},{"id":{"d":837,"n":6,"t":3},"speaker":"B","text":"昔は、江戸の人たちが鎌倉に避暑に来ていたそうですよ。"},{"id":{"d":837,"n":7,"t":3},"speaker":"A","text":"この暑さで避暑ですか？"},{"id":{"d":837,"n":8,"t":3},"speaker":"B","text":"はい。地球温暖化が進んでいなかった分、昔は今よりずっと涼しかったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":838,"n":1,"t":3},"speaker":"A","text":"草津温泉は良いお湯でしたね。"},{"id":{"d":838,"n":2,"t":3},"speaker":"B","text":"ええ、残念なのは草津白根山に行けなかったことです。"},{"id":{"d":838,"n":3,"t":3},"speaker":"A","text":"昔、お父さんが撮った草津白根山の写真はきれいでしたよね。"},{"id":{"d":838,"n":4,"t":3},"speaker":"B","text":"湯釜の写真ですね。"},{"id":{"d":838,"n":5,"t":3},"speaker":"A","text":"はい。あと、コマクサの写真も撮っていました。"},{"id":{"d":838,"n":6,"t":3},"speaker":"B","text":"まさか自分が旅の計画をした時に、草津白根山の噴火警戒レベルが上がるなんて思いませんでした。"},{"id":{"d":838,"n":7,"t":3},"speaker":"A","text":"噴火警戒レベルが落ち着いたら、また来ましょうね。"},{"id":{"d":838,"n":8,"t":3},"speaker":"B","text":"はい。それまで、楽しみを取っておきます。"}];
INSERT INTO sentence [{"id":{"d":839,"n":1,"t":3},"speaker":"A","text":"札幌ドームで行われるコンサートのチケットが当たりました。"},{"id":{"d":839,"n":2,"t":3},"speaker":"B","text":"北海道まで行かれるのですか？"},{"id":{"d":839,"n":3,"t":3},"speaker":"A","text":"はい。競争率が高いチケットなので、全国どこでも良いと思って申し込んだら、北海道で開催されるコンサートのチケットが当たったのです。"},{"id":{"d":839,"n":4,"t":3},"speaker":"B","text":"交通費がかかるし、大変ですね。"},{"id":{"d":839,"n":5,"t":3},"speaker":"A","text":"そうなのですが、しばらく旅行に行っていませんから、コンサートと同じくらい北海道旅行が楽しみなのです。"},{"id":{"d":839,"n":6,"t":3},"speaker":"B","text":"ついでに何泊かされるのですか？"},{"id":{"d":839,"n":7,"t":3},"speaker":"A","text":"はい。コンサートの前の日に行って1泊、コンサートの後に観光で2泊しようと思っています。"},{"id":{"d":839,"n":8,"t":3},"speaker":"B","text":"ゆっくり観光できそうですね。良い旅にしてください。"}];
INSERT INTO sentence [{"id":{"d":840,"n":1,"t":3},"speaker":"A","text":"おかえりなさい。北海道はどうでしたか？"},{"id":{"d":840,"n":2,"t":3},"speaker":"B","text":"ただいま帰りました。残念ながら、ゆっくり観光を楽しむどころではありませんでした。"},{"id":{"d":840,"n":3,"t":3},"speaker":"A","text":"スケジュールに余裕を持たせて行ったのですよね？"},{"id":{"d":840,"n":4,"t":3},"speaker":"B","text":"はい、そうなのですが晩秋の北海道を甘くみていました。初日は好天だったのに、2日目から大雪になってしまったのです。"},{"id":{"d":840,"n":5,"t":3},"speaker":"A","text":"それは大変でしたね。都民は雪に慣れていませんからね。"},{"id":{"d":840,"n":6,"t":3},"speaker":"B","text":"防寒着も足りなくて、観光どころではなくなってしまいました。"},{"id":{"d":840,"n":7,"t":3},"speaker":"A","text":"そうですか。よく東京へ帰って来られましたね。"},{"id":{"d":840,"n":8,"t":3},"speaker":"B","text":"そう思います。帰りの飛行機が予定通りに飛んだのは奇跡です。"}];
INSERT INTO sentence [{"id":{"d":841,"n":1,"t":3},"speaker":"A","text":"名古屋に出張に行くと、体重が増えてしまいます。"},{"id":{"d":841,"n":2,"t":3},"speaker":"B","text":"どうしてですか？"},{"id":{"d":841,"n":3,"t":3},"speaker":"A","text":"仕事はきちんとしていましたよ。でも、喫茶店で毎朝モーニングを食べていたからだと思います。"},{"id":{"d":841,"n":4,"t":3},"speaker":"B","text":"朝食を食べるくらいで体重は増えないと思いますよ。"},{"id":{"d":841,"n":5,"t":3},"speaker":"A","text":"普段は朝食を食べない人が、毎朝、満腹になるまで食べれば太ってしまいます。"},{"id":{"d":841,"n":6,"t":3},"speaker":"B","text":"それほどの量を食べなければ良いではありませんか？"},{"id":{"d":841,"n":7,"t":3},"speaker":"A","text":"分かってはいるのですが、つい食べたくなるほどおいしいのですよ。名古屋メシのおいしさは罪深いです。"},{"id":{"d":841,"n":8,"t":3},"speaker":"B","text":"さては、昼も夜も名古屋メシで満腹になっていましたね？"}];
INSERT INTO sentence [{"id":{"d":842,"n":1,"t":3},"speaker":"A","text":"東京にはこのような展望室もあるのですね。海がとてもきれいです。"},{"id":{"d":842,"n":2,"t":3},"speaker":"B","text":"楽しんでいただけて良かったです。「東京スカイツリーには、前に行ったことがある」と言っていたので、今回はこの展望室にお誘いしました。"},{"id":{"d":842,"n":3,"t":3},"speaker":"A","text":"素晴らしい景色です。ここはテレビ局のビルなのでしょう？"},{"id":{"d":842,"n":4,"t":3},"speaker":"B","text":"そうです。外から見えたと思いますが、建物の丸い部分まで昇ってきたのですよ。"},{"id":{"d":842,"n":5,"t":3},"speaker":"A","text":"スタジオ見学はできませんか？"},{"id":{"d":842,"n":6,"t":3},"speaker":"B","text":"今はできないようですね。"},{"id":{"d":842,"n":7,"t":3},"speaker":"A","text":"芸能人に会えるかと思っていたので、少しだけ残念です。"},{"id":{"d":842,"n":8,"t":3},"speaker":"B","text":"景色を満喫したら、お土産を買って帰りましょうか。"}];
INSERT INTO sentence [{"id":{"d":843,"n":1,"t":3},"speaker":"A","text":"このようなお弁当は、初めて見ました。"},{"id":{"d":843,"n":2,"t":3},"speaker":"B","text":"そうですか？これは「品川貝づくし」という駅弁です。"},{"id":{"d":843,"n":3,"t":3},"speaker":"A","text":"私が東京駅でお弁当を選ぶ際には、シウマイ弁当を買うことが多いですから。"},{"id":{"d":843,"n":4,"t":3},"speaker":"B","text":"他のものは目に入りませんか。このお弁当はお酒にとても合うのですよ。"},{"id":{"d":843,"n":5,"t":3},"speaker":"A","text":"合いそうですね。茶飯の上にいろいろな貝がびっしりと敷き詰められていて、とてもおいしそうです。"}];
INSERT INTO sentence [{"id":{"d":844,"n":1,"t":3},"speaker":"A","text":"お母さん、昨日はおばあちゃんのところで何をしてきたのですか？"},{"id":{"d":844,"n":2,"t":3},"speaker":"B","text":"オンライン旅行に申し込んできたのですよ。"},{"id":{"d":844,"n":3,"t":3},"speaker":"A","text":"おばあちゃんの家のパソコンで申し込んだのですか？"},{"id":{"d":844,"n":4,"t":3},"speaker":"B","text":"そうです。おばあちゃんがパソコンに詳しい人で良かったです。"},{"id":{"d":844,"n":5,"t":3},"speaker":"A","text":"旅行先はどちらにしたのですか？"},{"id":{"d":844,"n":6,"t":3},"speaker":"B","text":"ハワイ州です。おばあちゃんは「あなたたちの結婚式の時に、親戚みんなで行ったことが懐かしい」と言っていました。"},{"id":{"d":844,"n":7,"t":3},"speaker":"A","text":"お母さんの結婚式は、ハワイ州で挙げたのですか？"},{"id":{"d":844,"n":8,"t":3},"speaker":"B","text":"そうです。知らなかったのですか？アルバムを見せますよ。"},{"id":{"d":844,"n":9,"t":3},"speaker":"A","text":"知りませんでした。写真があるのですね。ぜひ見せてください。"}];
INSERT INTO sentence [{"id":{"d":845,"n":1,"t":3},"speaker":"A","text":"あちらにいる外国人の方は、観光客でしょうか。道が分からないのでしょうか？"},{"id":{"d":845,"n":2,"t":3},"speaker":"B","text":"そうかもしれません。先ほどから地図とスマートフォンを持ってうろついていますね。"},{"id":{"d":845,"n":3,"t":3},"speaker":"A","text":"道を教えて差し上げたらいかがですか？"},{"id":{"d":845,"n":4,"t":3},"speaker":"B","text":"おぼつかない英語しか話せないので、私には無理ですよ。"},{"id":{"d":845,"n":5,"t":3},"speaker":"A","text":"このようなことは度胸が大切ですよ。"},{"id":{"d":845,"n":6,"t":3},"speaker":"B","text":"私には度胸がありません。"},{"id":{"d":845,"n":7,"t":3},"speaker":"A","text":"では、私があちらの外国人の方へ、道を教えに行きます。"},{"id":{"d":845,"n":8,"t":3},"speaker":"B","text":"あなたは日本語しか分からないし英語も全く話せないのに、度胸があるのですね。"}];
INSERT INTO sentence [{"id":{"d":846,"n":1,"t":3},"speaker":"A","text":"この辺りにコインパーキングはありますか？"},{"id":{"d":846,"n":2,"t":3},"speaker":"B","text":"あのお店の隣にあったと思います。空きがあるかは分かりませんが。"},{"id":{"d":846,"n":3,"t":3},"speaker":"A","text":"奇跡的に空いていますね。"},{"id":{"d":846,"n":4,"t":3},"speaker":"B","text":"では、ここにしますか？"},{"id":{"d":846,"n":5,"t":3},"speaker":"A","text":"少し待ってください。10分550円ということは1時間いくらですか？"},{"id":{"d":846,"n":6,"t":3},"speaker":"B","text":"3,300円ですね。"},{"id":{"d":846,"n":7,"t":3},"speaker":"A","text":"それは高すぎます。もう少し駐車料金が安いコインパーキングを探しましょう。"},{"id":{"d":846,"n":8,"t":3},"speaker":"B","text":"そうしましょう。駐車料金がこれほどに高いとは、驚きです。"}];
INSERT INTO sentence [{"id":{"d":847,"n":1,"t":3},"speaker":"A","text":"お母さん、2階建てバスですよ。今からこれに乗って東京観光をするのですよ。"},{"id":{"d":847,"n":2,"t":3},"speaker":"B","text":"2階建てバスに乗るのは初めてなので楽しみです。"},{"id":{"d":847,"n":3,"t":3},"speaker":"A","text":"2階部分の屋根がないから天気が心配でしたが、晴れて良かったですね。"},{"id":{"d":847,"n":4,"t":3},"speaker":"B","text":"そうですね。屋根がないと、高いビルも全部見えるのですね。"},{"id":{"d":847,"n":5,"t":3},"speaker":"A","text":"素晴らしい景色ですよね。このバスで東京をぐるりと回ったら、昼食にしましょう。リクエストはありますか？"},{"id":{"d":847,"n":6,"t":3},"speaker":"B","text":"洋食が良いです。"},{"id":{"d":847,"n":7,"t":3},"speaker":"A","text":"それなら駅前のデパート内のレストランコーナーに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":848,"n":1,"t":3},"speaker":"A","text":"今日はドライブに行きましょう。"},{"id":{"d":848,"n":2,"t":3},"speaker":"B","text":"どこに行くのですか？"},{"id":{"d":848,"n":3,"t":3},"speaker":"A","text":"目的地は、高速道路のサービスエリアです。"},{"id":{"d":848,"n":4,"t":3},"speaker":"B","text":"高速道路のサービスエリアが目的地なのですか？"},{"id":{"d":848,"n":5,"t":3},"speaker":"A","text":"はい。高速のサービスエリアが、最近はかなり充実しているでしょう？"},{"id":{"d":848,"n":6,"t":3},"speaker":"B","text":"はい。よくテレビ番組でサービスエリアのグルメレポートをしている番組を放送していますね。"},{"id":{"d":848,"n":7,"t":3},"speaker":"A","text":"今日は、サービスエリアのグルメを楽しむ旅にしたいと思います。"},{"id":{"d":848,"n":8,"t":3},"speaker":"B","text":"それは楽しみです。しっかりおなかをすかせてから行きましょう。"}];
INSERT INTO sentence [{"id":{"d":849,"n":1,"t":3},"speaker":"A","text":"今日は僕の母をわが家へ招く予定でしたのに、急な仕事が入ってしまいすみませんでした。"},{"id":{"d":849,"n":2,"t":3},"speaker":"B","text":"いいえ、大丈夫ですよ。おかげでお母さまと2人でお出掛けできて楽しかったです。"},{"id":{"d":849,"n":3,"t":3},"speaker":"A","text":"母と2人でどこに行ったのですか？"},{"id":{"d":849,"n":4,"t":3},"speaker":"B","text":"お母さまはお花がお好きですから勝沼のハーブ庭園旅日記に行ってきました。"},{"id":{"d":849,"n":5,"t":3},"speaker":"A","text":"今頃、花が咲いているのですか？"},{"id":{"d":849,"n":6,"t":3},"speaker":"B","text":"ええ、ヒマワリもきれいでしたよ。カフェでラベンダーのソフトクリームを食べました。"},{"id":{"d":849,"n":7,"t":3},"speaker":"A","text":"ゆっくりできたようで良かったです。"},{"id":{"d":849,"n":8,"t":3},"speaker":"B","text":"ぶどう園にも寄ってぶどう狩りをしてきました。これが収穫物です。"}];
INSERT INTO sentence [{"id":{"d":850,"n":1,"t":3},"speaker":"A","text":"立派なキャンピングカーですね。"},{"id":{"d":850,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。これで日本一周の旅をしているのです。"},{"id":{"d":850,"n":3,"t":3},"speaker":"A","text":"奥さまとご一緒なのですか？"},{"id":{"d":850,"n":4,"t":3},"speaker":"B","text":"はい。妻は今、道の駅に食材を買いに行っています。この子たちも旅の仲間です。"},{"id":{"d":850,"n":5,"t":3},"speaker":"A","text":"かわいいワンちゃんたちですね。"},{"id":{"d":850,"n":6,"t":3},"speaker":"B","text":"はい。この子たちがいると、より楽しく旅ができるのですよ。"},{"id":{"d":850,"n":7,"t":3},"speaker":"A","text":"自由な旅ができて、うらやましいです。"},{"id":{"d":850,"n":8,"t":3},"speaker":"B","text":"とても楽しいです。あなたも、リタイア後の楽しみに取っておくと良いですよ。"}];
INSERT INTO sentence [{"id":{"d":851,"n":1,"t":3},"speaker":"A","text":"この牧場では、乗馬体験ができるのですか？"},{"id":{"d":851,"n":2,"t":3},"speaker":"B","text":"はい。3歳以上の方でしたら可能です。"},{"id":{"d":851,"n":3,"t":3},"speaker":"A","text":"大人も大丈夫なのですよね？"},{"id":{"d":851,"n":4,"t":3},"speaker":"B","text":"はい。体重が90キログラム以下の方でしたら大丈夫です。"},{"id":{"d":851,"n":5,"t":3},"speaker":"A","text":"夫の体重だと、90キログラム寸前かもしれません。"},{"id":{"d":851,"n":6,"t":3},"speaker":"B","text":"ご主人さまは乗馬体験を希望されているのですか？"},{"id":{"d":851,"n":7,"t":3},"speaker":"A","text":"はい。「子どもと一緒に乗りたい」と言っているのですが、子どもの体重と合わせて90キログラムを超える場合は乗れませんよね？"},{"id":{"d":851,"n":8,"t":3},"speaker":"B","text":"はい。申し訳ありませんが、その場合は2人一緒に1頭には乗ることはできません。それぞれでお乗りいただくのであれば、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":852,"n":1,"t":3},"speaker":"A","text":"真理さん、本当にバンジージャンプをするのですか？"},{"id":{"d":852,"n":2,"t":3},"speaker":"B","text":"これほど景色の良いところで空を飛べるのですよ。絶対に挑戦します。"},{"id":{"d":852,"n":3,"t":3},"speaker":"A","text":"ここでバンジージャンプができることを、誰から教えてもらったのですか？"},{"id":{"d":852,"n":4,"t":3},"speaker":"B","text":"サークルの友人が教えてくれました。美鈴さんです、彼女を知っていますか？"},{"id":{"d":852,"n":5,"t":3},"speaker":"A","text":"はい。彼女の地元は茨城でしたね。"},{"id":{"d":852,"n":6,"t":3},"speaker":"B","text":"そうです。ところで、裕子さんは飛ばないのですか？"},{"id":{"d":852,"n":7,"t":3},"speaker":"A","text":"私は見学だけにします。絶対に飛びません。"},{"id":{"d":852,"n":8,"t":3},"speaker":"B","text":"それでしたら、裕子さんは私が飛ぶところを動画で撮影してくれませんか？"},{"id":{"d":852,"n":9,"t":3},"speaker":"A","text":"よいですが、バンジージャンプを撮影するのは初めてなので、上手に撮影できないかもしれませんよ。"},{"id":{"d":852,"n":10,"t":3},"speaker":"B","text":"そのときは2回目も飛びますよ。"}];
INSERT INTO sentence [{"id":{"d":853,"n":1,"t":3},"speaker":"A","text":"お皿に絵が描けました。"},{"id":{"d":853,"n":2,"t":3},"speaker":"B","text":"上手に描けましたね。"},{"id":{"d":853,"n":3,"t":3},"speaker":"A","text":"これはこのまま持って帰ってよいのですか？"},{"id":{"d":853,"n":4,"t":3},"speaker":"B","text":"いいえ。この後、陶芸家の先生にお願いして窯で焼いてもらったら完成です。"},{"id":{"d":853,"n":5,"t":3},"speaker":"A","text":"では、今日は持って帰れませんね。"},{"id":{"d":853,"n":6,"t":3},"speaker":"B","text":"はい。完成まであと1週間待ってください。1週間後にご自宅に届きますよ。"},{"id":{"d":853,"n":7,"t":3},"speaker":"A","text":"楽しみに待っています。"},{"id":{"d":853,"n":8,"t":3},"speaker":"B","text":"もしよければ、今度は手びねり教室にも来てください。お皿の形から自分で作れますよ。"}];
INSERT INTO sentence [{"id":{"d":854,"n":1,"t":3},"speaker":"A","text":"セグウェイに乗るのは初めてなのですが、大丈夫でしょうか？"},{"id":{"d":854,"n":2,"t":3},"speaker":"B","text":"はい。ツアーの前に講習会がございますので、初めての方でも心配はないかと思います。"},{"id":{"d":854,"n":3,"t":3},"speaker":"A","text":"私は運動神経がとても悪いので、乗りこなせるか不安です。"},{"id":{"d":854,"n":4,"t":3},"speaker":"B","text":"そう言われる方も多いのですが、皆さまがセグウェイを乗りこなしていらっしゃいますよ。"},{"id":{"d":854,"n":5,"t":3},"speaker":"A","text":"そうなのですね。少し安心しました。"},{"id":{"d":854,"n":6,"t":3},"speaker":"B","text":"書類の記入欄に必要事項を書いていただけますか？"},{"id":{"d":854,"n":7,"t":3},"speaker":"A","text":"はい、記入します。"},{"id":{"d":854,"n":8,"t":3},"speaker":"B","text":"あと5分ほどで講習会が始まります。楽しんでいらしてください。"}];
INSERT INTO sentence [{"id":{"d":855,"n":1,"t":3},"speaker":"A","text":"ヒロ君が高熱を出してしまいました。"},{"id":{"d":855,"n":2,"t":3},"speaker":"B","text":"今日の旅行はできませんね。"},{"id":{"d":855,"n":3,"t":3},"speaker":"A","text":"そうですね。予約したホテルをキャンセルします。"},{"id":{"d":855,"n":4,"t":3},"speaker":"B","text":"お願いします。当日キャンセルは残念ですね。"},{"id":{"d":855,"n":5,"t":3},"speaker":"A","text":"仕方がありません。車で行く予定でしたから、電車のキャンセルが必要ないことがせめてもの救いです。"},{"id":{"d":855,"n":6,"t":3},"speaker":"B","text":"病院には連絡しましたか？"},{"id":{"d":855,"n":7,"t":3},"speaker":"A","text":"はい。11時ごろに病院に行くことにしました。"},{"id":{"d":855,"n":8,"t":3},"speaker":"B","text":"分かりました。では、その間に私はスポーツドリンクと熱冷ましを買ってきますね。"}];
INSERT INTO sentence [{"id":{"d":856,"n":1,"t":3},"speaker":"A","text":"長崎に出張して、1泊延泊してきたそうですね。"},{"id":{"d":856,"n":2,"t":3},"speaker":"B","text":"はい。長崎まで行ったらどうしても行きたい場所があって、もう1泊してきました。"},{"id":{"d":856,"n":3,"t":3},"speaker":"A","text":"ハウステンボスですか？"},{"id":{"d":856,"n":4,"t":3},"speaker":"B","text":"いいえ、私が行きたかったのは佐世保のハンバーガー屋です。"},{"id":{"d":856,"n":5,"t":3},"speaker":"A","text":"佐世保バーガーですか？有名ですよね。"},{"id":{"d":856,"n":6,"t":3},"speaker":"B","text":"ええ、そうです。あなたは佐世保バーガーを食べたことがありますか？"},{"id":{"d":856,"n":7,"t":3},"speaker":"A","text":"いいえ。食べたことがありません。"},{"id":{"d":856,"n":8,"t":3},"speaker":"B","text":"ぜひ一度食べてみてほしいです。"},{"id":{"d":856,"n":9,"t":3},"speaker":"A","text":"それほどまでにおいしかったのですか？"},{"id":{"d":856,"n":10,"t":3},"speaker":"B","text":"はい。あまりにもおいし過ぎて、3軒も次から次へと食べ歩いてしまったほどです。"}];
INSERT INTO sentence [{"id":{"d":857,"n":1,"t":3},"speaker":"A","text":"旅行は楽しかったですか？"},{"id":{"d":857,"n":2,"t":3},"speaker":"B","text":"はい。とても楽しかったです。これは、あなたへのお土産です。"},{"id":{"d":857,"n":3,"t":3},"speaker":"A","text":"十勝ワインとチーズですね。ありがとうございます。"},{"id":{"d":857,"n":4,"t":3},"speaker":"B","text":"公子がガイドブックのイラストレーターのお仕事をしているおかげで、普通は見られないところも見てきました。貴重な経験ができて面白かったです。"},{"id":{"d":857,"n":5,"t":3},"speaker":"A","text":"今回は公子さんのお仕事に付いて行ったのですか？"},{"id":{"d":857,"n":6,"t":3},"speaker":"B","text":"そうです。「『楽しい女子旅』がテーマなので付き合って」と言われたのです。"},{"id":{"d":857,"n":7,"t":3},"speaker":"A","text":"取材も兼ねての旅行だったのですね。"},{"id":{"d":857,"n":8,"t":3},"speaker":"B","text":"はい。そのため、旅費の一部は公子に出してもらえたのですよ。"},{"id":{"d":857,"n":9,"t":3},"speaker":"A","text":"それは良かったですね。"}];
INSERT INTO sentence [{"id":{"d":858,"n":1,"t":3},"speaker":"A","text":"もしもし？そちらの宿は、ペットも泊まれると聞いたのですが、本当でしょうか？"},{"id":{"d":858,"n":2,"t":3},"speaker":"B","text":"お問い合せありがとうございます。私共のホテルは中型犬、小型犬でしたら、飼い主さまと一緒に泊まることができる専用ルームがございます。"},{"id":{"d":858,"n":3,"t":3},"speaker":"A","text":"ビーグルは大丈夫ということですか？"},{"id":{"d":858,"n":4,"t":3},"speaker":"B","text":"はい、大丈夫です。"},{"id":{"d":858,"n":5,"t":3},"speaker":"A","text":"3月20日に予約をしたいのですが、お部屋は空いていますか？"},{"id":{"d":858,"n":6,"t":3},"speaker":"B","text":"朝食付き、大人2名さま、ワンちゃん1匹で36,000円のお部屋でしたらご用意できます。"},{"id":{"d":858,"n":7,"t":3},"speaker":"A","text":"良かったです。予約をお願いします。"},{"id":{"d":858,"n":8,"t":3},"speaker":"B","text":"承りました。ワンちゃんの宿泊のためのガイドブックをお送りしますので、宿泊日までにお読みください。"}];
INSERT INTO sentence [{"id":{"d":859,"n":1,"t":3},"speaker":"A","text":"観光の前に、荷物をコインロッカーに預けましょう。"},{"id":{"d":859,"n":2,"t":3},"speaker":"B","text":"この辺りを観光したら、また駅に戻ってくるのですか？"},{"id":{"d":859,"n":3,"t":3},"speaker":"A","text":"はい。ホテルの送迎バスが夕方駅に迎えに来てくれますので、その時間には戻ってくる予定です。"},{"id":{"d":859,"n":4,"t":3},"speaker":"B","text":"なるほど。では、駅に荷物を置いて行った方がいいですね。"},{"id":{"d":859,"n":5,"t":3},"speaker":"A","text":"必要なものまでコインロッカーに入れないように、気を付けてくださいね。"},{"id":{"d":859,"n":6,"t":3},"speaker":"B","text":"ご忠告ありがとうございます。小型のバッグごと荷物の全部をコンロッカーに入れるところでした。"},{"id":{"d":859,"n":7,"t":3},"speaker":"A","text":"お財布もなしで観光するつもりだったのですか？"},{"id":{"d":859,"n":8,"t":3},"speaker":"B","text":"はい。少し、ぼんやりとしていました。"}];
INSERT INTO sentence [{"id":{"d":860,"n":1,"t":3},"speaker":"A","text":"バスガイドさんは、歌がお上手ですね。"},{"id":{"d":860,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":860,"n":3,"t":3},"speaker":"A","text":"私の若い頃によく聞いた歌を、若いガイドさんが歌ってくださったのには感動しました。"},{"id":{"d":860,"n":4,"t":3},"speaker":"B","text":"このご当地ソングは、先輩からずっと受け継がれているのです。"},{"id":{"d":860,"n":5,"t":3},"speaker":"A","text":"これからも歌い続けていってくださいね。"},{"id":{"d":860,"n":6,"t":3},"speaker":"B","text":"はい、頑張ります。"},{"id":{"d":860,"n":7,"t":3},"speaker":"A","text":"ところで、集合時間までにお土産を買いに行きたいのですが、何かお薦めのものはありますか？"},{"id":{"d":860,"n":8,"t":3},"speaker":"B","text":"この辺りでは抹茶やみかんのお菓子が人気ですよ。"}];
INSERT INTO sentence [{"id":{"d":861,"n":1,"t":3},"speaker":"A","text":"箱根に日帰り旅をしてきました。"},{"id":{"d":861,"n":2,"t":3},"speaker":"B","text":"今はアジサイがきれいでしたでしょう？"},{"id":{"d":861,"n":3,"t":3},"speaker":"A","text":"はい。登山電車からアジサイがきれいに見えました。"},{"id":{"d":861,"n":4,"t":3},"speaker":"B","text":"日帰りでは、温泉には入れなかったのですか？"},{"id":{"d":861,"n":5,"t":3},"speaker":"A","text":"温泉には入りませんでしたが、足湯には漬かってきましたよ。カフェに足湯が存在したのです。"},{"id":{"d":861,"n":6,"t":3},"speaker":"B","text":"お茶を飲みながら足湯に漬かったのですか？"},{"id":{"d":861,"n":7,"t":3},"speaker":"A","text":"そうです。歩き疲れていたので気持ちが良かったですよ。"},{"id":{"d":861,"n":8,"t":3},"speaker":"B","text":"サービス満点のカフェですね。"}];
INSERT INTO sentence [{"id":{"d":862,"n":1,"t":3},"speaker":"A","text":"小田原でかまぼこ作り体験をしてきました。"},{"id":{"d":862,"n":2,"t":3},"speaker":"B","text":"テレビ番組で見たことがあります。難しいのでしょう？"},{"id":{"d":862,"n":3,"t":3},"speaker":"A","text":"はい。板にのせるかまぼこ作りは難しいそうです。そこで、私は揚げかまぼこを作りました。"},{"id":{"d":862,"n":4,"t":3},"speaker":"B","text":"揚げかまぼことはどのようなものですか？"},{"id":{"d":862,"n":5,"t":3},"speaker":"A","text":"いわゆるさつま揚げです。好きな具材を練りこんで、お店の人に揚げてもらうのです。"},{"id":{"d":862,"n":6,"t":3},"speaker":"B","text":"揚げたてを食べたらおいしそうですね。"},{"id":{"d":862,"n":7,"t":3},"speaker":"A","text":"はい、とてもおいしかったです。"}];
INSERT INTO sentence [{"id":{"d":863,"n":1,"t":3},"speaker":"A","text":"この駅に、車いす用のエレベーターはありますか？"},{"id":{"d":863,"n":2,"t":3},"speaker":"B","text":"はい。少し奥まったところになってしまいますが、ございます。"},{"id":{"d":863,"n":3,"t":3},"speaker":"A","text":"どの辺りでしょうか？"},{"id":{"d":863,"n":4,"t":3},"speaker":"B","text":"ご案内いたします。"},{"id":{"d":863,"n":5,"t":3},"speaker":"A","text":"お手数をおかけしてすみません。"},{"id":{"d":863,"n":6,"t":3},"speaker":"B","text":"こちらのエレベーターをお使いください。"},{"id":{"d":863,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":863,"n":8,"t":3},"speaker":"B","text":"恐れ入ります。お気を付けていってらっしゃいませ。"}];
INSERT INTO sentence [{"id":{"d":864,"n":1,"t":3},"speaker":"A","text":"卓球部の同窓会を開催するとしたら、やはり温泉卓球ができる旅館に泊まるのでしょうか？"},{"id":{"d":864,"n":2,"t":3},"speaker":"B","text":"はい。幹事がそのような旅館を探していましたよ。"},{"id":{"d":864,"n":3,"t":3},"speaker":"A","text":"そうなのですね。卒業しても卓球を続けていますか？"},{"id":{"d":864,"n":4,"t":3},"speaker":"B","text":"いいえ。私は暇がなくてしていません。美野里さんはどうですか？"},{"id":{"d":864,"n":5,"t":3},"speaker":"A","text":"私は時々ストレス解消のために、公民館で卓球をしています。"},{"id":{"d":864,"n":6,"t":3},"speaker":"B","text":"それでは、もし美野里さんと対戦したら確実に私が負けますね。"},{"id":{"d":864,"n":7,"t":3},"speaker":"A","text":"試合をしてみなければ、勝敗は分かりませんよ。"},{"id":{"d":864,"n":8,"t":3},"speaker":"B","text":"そうでしょうか？"},{"id":{"d":864,"n":9,"t":3},"speaker":"A","text":"そうですよ。現役時代はあなたの方が強かったのですから。"}];
INSERT INTO sentence [{"id":{"d":865,"n":1,"t":3},"speaker":"A","text":"ホテルに着く前に見えた公園には、夜も入れますか？"},{"id":{"d":865,"n":2,"t":3},"speaker":"B","text":"はい、入れますよ。"},{"id":{"d":865,"n":3,"t":3},"speaker":"A","text":"夜桜見物に行きたいのです。"},{"id":{"d":865,"n":4,"t":3},"speaker":"B","text":"それは良いですね。でも、安全のためには、お一人では行かない方がいいですよ。"},{"id":{"d":865,"n":5,"t":3},"speaker":"A","text":"夫と二人なら大丈夫ですよね？"},{"id":{"d":865,"n":6,"t":3},"speaker":"B","text":"はい。お二人でしたら大丈夫だと思います。"},{"id":{"d":865,"n":7,"t":3},"speaker":"A","text":"それでは、後で行ってみます。"},{"id":{"d":865,"n":8,"t":3},"speaker":"B","text":"くれぐれも気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":866,"n":1,"t":3},"speaker":"A","text":"こちらのスープカレーはおいしいですね。"},{"id":{"d":866,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":866,"n":3,"t":3},"speaker":"A","text":"私はスープカレーが大好きで、今日はこれで2軒目なのです。"},{"id":{"d":866,"n":4,"t":3},"speaker":"B","text":"ご旅行でいらしたのですか？"},{"id":{"d":866,"n":5,"t":3},"speaker":"A","text":"はい。一応旅行ですが、スープカレーを食べたら日帰りします。"},{"id":{"d":866,"n":6,"t":3},"speaker":"B","text":"日帰りですか。どちらからいらしたのですか？"},{"id":{"d":866,"n":7,"t":3},"speaker":"A","text":"東京です。どうしてもスープカレーを食べたくなって、飛行機に乗って来てしまいました。"},{"id":{"d":866,"n":8,"t":3},"speaker":"B","text":"わざわざスープカレーのために札幌まで来られたのですか！どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":867,"n":1,"t":3},"speaker":"A","text":"このボタンを押したら、船が来るのですか？"},{"id":{"d":867,"n":2,"t":3},"speaker":"B","text":"そうです。対岸への渡し舟が来ます。"},{"id":{"d":867,"n":3,"t":3},"speaker":"A","text":"ボタンを押しても反応がないように思います。"},{"id":{"d":867,"n":4,"t":3},"speaker":"B","text":"もう少し待っていれば、あちらから船が来ますよ。"},{"id":{"d":867,"n":5,"t":3},"speaker":"A","text":"あちらに見える船ですか？"},{"id":{"d":867,"n":6,"t":3},"speaker":"B","text":"ええ、あの船です。料金は船頭に渡してください。"},{"id":{"d":867,"n":7,"t":3},"speaker":"A","text":"料金はこの看板に書いてある通りですね。小銭がないのですが、お釣りを頂けるのでしょうか？"},{"id":{"d":867,"n":8,"t":3},"speaker":"B","text":"はい。でも、1,000円であれば、私が両替できますよ。"}];
INSERT INTO sentence [{"id":{"d":868,"n":1,"t":3},"speaker":"A","text":"お土産を買い過ぎてしまい、荷物が大量になってしまいました。歩くのが大変です。"},{"id":{"d":868,"n":2,"t":3},"speaker":"B","text":"大変そうですね。キャリーバッグを引きずって、エスカレーターに乗るのは危ないですよ。"},{"id":{"d":868,"n":3,"t":3},"speaker":"A","text":"そうですよね。エレベーターはありますか？"},{"id":{"d":868,"n":4,"t":3},"speaker":"B","text":"はい。この階段の裏にあるようです。"},{"id":{"d":868,"n":5,"t":3},"speaker":"A","text":"では、そちらに回ります。"},{"id":{"d":868,"n":6,"t":3},"speaker":"B","text":"私は荷物が少ないので、エスカレーターで行きますね。"},{"id":{"d":868,"n":7,"t":3},"speaker":"A","text":"分かりました。上で合流しましょう。"},{"id":{"d":868,"n":8,"t":3},"speaker":"B","text":"キャリーバッグの上に載せた袋が落ちそうですよ。気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":869,"n":1,"t":3},"speaker":"A","text":"スーパーマーケットで懐かしいパイが売っていたので買ってきました。"},{"id":{"d":869,"n":2,"t":3},"speaker":"B","text":"今日のおやつはうなぎパイですね。去年、家族で工場見学に行きましたね。"},{"id":{"d":869,"n":3,"t":3},"speaker":"A","text":"ええ、お土産にたくさん買ってきましたよね。"},{"id":{"d":869,"n":4,"t":3},"speaker":"B","text":"カフェで食べたパイ入りのパフェはおいしかったです。"},{"id":{"d":869,"n":5,"t":3},"speaker":"A","text":"また家族で行きたいですね。"},{"id":{"d":869,"n":6,"t":3},"speaker":"B","text":"今度は名物のうなぎを食べに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":870,"n":1,"t":3},"speaker":"A","text":"このお菓子は東京ディズニーランドのものですか？"},{"id":{"d":870,"n":2,"t":3},"speaker":"B","text":"いいえ、違います。ウォルト・ディズニー・ワールド・リゾートのものです。"},{"id":{"d":870,"n":3,"t":3},"speaker":"A","text":"アメリカへ行かれたのですか？"},{"id":{"d":870,"n":4,"t":3},"speaker":"B","text":"はい、そうです。娘がいるフロリダを訪ねて、娘の家族と一緒にウォルト・ディズニー・ワールド・リゾートに行きました。"},{"id":{"d":870,"n":5,"t":3},"speaker":"A","text":"お嬢さまはフロリダに住んでいらっしゃるのですか？"},{"id":{"d":870,"n":6,"t":3},"speaker":"B","text":"はい。語学留学が終わったら帰国するはずが、結婚相手との出会いがありまして。それからずっとフロリダに住んでいるのです。"},{"id":{"d":870,"n":7,"t":3},"speaker":"A","text":"離れていて寂しくはありませんか？"},{"id":{"d":870,"n":8,"t":3},"speaker":"B","text":"いいえ、それほど寂しさは感じていません。今回は孫にも会えましたし、楽しい滞在でした。"}];
INSERT INTO sentence [{"id":{"d":871,"n":1,"t":3},"speaker":"A","text":"京都のお寺巡りをする予定が、いつの間にかスイーツ巡りになっていますね。"},{"id":{"d":871,"n":2,"t":3},"speaker":"B","text":"そうですね。この周辺には有名な甘味処がたくさんあるようです。"},{"id":{"d":871,"n":3,"t":3},"speaker":"A","text":"昔、修学旅行で京都に来たときにはことを知りませんでした。今は何でもスマートフォンのインターネット検索を利用し、調べながら歩けるので便利ですね。"},{"id":{"d":871,"n":4,"t":3},"speaker":"B","text":"ええ。インターネットの検索エンジンに、「この近くのカフェは？」とか「この近くのケーキ屋さんは？」と聞けば、すぐに答えが返ってきます。"},{"id":{"d":871,"n":5,"t":3},"speaker":"A","text":"スマートフォンでインターネット検索をすれば、何でも教えてくれますから、つい検索してしまいますね。"},{"id":{"d":871,"n":6,"t":3},"speaker":"B","text":"はい。そしてインターネット検索で見つけたお店の商品は、おいしいものばかりでした。"},{"id":{"d":871,"n":7,"t":3},"speaker":"A","text":"今となっては、スマートフォンは観光に欠かせないものになりましたね。"},{"id":{"d":871,"n":8,"t":3},"speaker":"B","text":"さて、次はどこに行きましょうか？"},{"id":{"d":871,"n":9,"t":3},"speaker":"A","text":"平安神宮の近くのカフェにしましょう。ここからの行き方を検索します。"}];
INSERT INTO sentence [{"id":{"d":872,"n":1,"t":3},"speaker":"A","text":"スキー旅行が修学旅行の代わりだなんて悲しいです。"},{"id":{"d":872,"n":2,"t":3},"speaker":"B","text":"私も、スキーよりもユニバーサル・スタジオ・ジャパンに行きたかったです。"},{"id":{"d":872,"n":3,"t":3},"speaker":"A","text":"スキー旅行は、修学旅行というよりは、体育の授業のようですよね。"},{"id":{"d":872,"n":4,"t":3},"speaker":"B","text":"同感です。あなたはスキーをしたことはありますか？"},{"id":{"d":872,"n":5,"t":3},"speaker":"A","text":"ありません。今回が初めてです。"},{"id":{"d":872,"n":6,"t":3},"speaker":"B","text":"私も初めてです。"},{"id":{"d":872,"n":7,"t":3},"speaker":"A","text":"初心者クラスで転んでばかりになりそうです。"},{"id":{"d":872,"n":8,"t":3},"speaker":"B","text":"けがだけはしないように、注意しないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":873,"n":1,"t":3},"speaker":"A","text":"今、読んでいらっしゃるのは中国語の新聞ですか？"},{"id":{"d":873,"n":2,"t":3},"speaker":"B","text":"はい、勉強のために読むようにしているのです。"},{"id":{"d":873,"n":3,"t":3},"speaker":"A","text":"年が明けたら、中国に転勤されるのでしたね。"},{"id":{"d":873,"n":4,"t":3},"speaker":"B","text":"そうです。その前にも、来週、中国への短期出張が入っています。"},{"id":{"d":873,"n":5,"t":3},"speaker":"A","text":"中国語は話せるのですよね？言葉に不自由しないのはうらやましいです。"},{"id":{"d":873,"n":6,"t":3},"speaker":"B","text":"はい。しかし、言葉ができるだけでは、仕事はできません。"},{"id":{"d":873,"n":7,"t":3},"speaker":"A","text":"新聞で勉強しているのは、言葉だけではありませんね。素晴らしいです。"},{"id":{"d":873,"n":8,"t":3},"speaker":"B","text":"なるべく現地の情報を得ることを心掛けています。出張に行ったらお土産を買ってきますよ。"}];
