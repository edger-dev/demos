INSERT INTO sentence [{"id":{"d":575,"n":1,"t":3},"speaker":"A","text":"すみませんが、この空港内で仮眠できるところはありますか？"},{"id":{"d":575,"n":2,"t":3},"speaker":"B","text":"はい。有料ですが、仮眠室があります。"},{"id":{"d":575,"n":3,"t":3},"speaker":"A","text":"料金はどのくらいですか？"},{"id":{"d":575,"n":4,"t":3},"speaker":"B","text":"1時間当たり1,200円です。"},{"id":{"d":575,"n":5,"t":3},"speaker":"A","text":"そうですか。できればシャワーを浴びたいのですが、仮眠室に付いていますか？"},{"id":{"d":575,"n":6,"t":3},"speaker":"B","text":"シャワーは別になっていて、シャワー室があります。料金も別で800円です。"},{"id":{"d":575,"n":7,"t":3},"speaker":"A","text":"分かりました。場所を教えてください。"},{"id":{"d":575,"n":8,"t":3},"speaker":"B","text":"ただいま、地図をお持ちします。"}];
