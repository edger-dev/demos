INSERT INTO sentence [{"id":{"d":26,"n":1,"t":3},"speaker":"A","text":"エアコンが動きません。どうしたらよいですか？"},{"id":{"d":26,"n":2,"t":3},"speaker":"B","text":"エアコンのプラグはコンセントに入っていますか？"},{"id":{"d":26,"n":3,"t":3},"speaker":"A","text":"はい、入っています。"},{"id":{"d":26,"n":4,"t":3},"speaker":"B","text":"では一度抜いてからまた差し込んでみてください。"},{"id":{"d":26,"n":5,"t":3},"speaker":"A","text":"分かりました。やってみます。"},{"id":{"d":26,"n":6,"t":3},"speaker":"B","text":"差し込みましたか？"},{"id":{"d":26,"n":7,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":26,"n":8,"t":3},"speaker":"B","text":"それでは電源ボタンを押してみてください。"},{"id":{"d":26,"n":9,"t":3},"speaker":"A","text":"動きました！ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":27,"n":1,"t":3},"speaker":"A","text":"東京の友達と遊びに行きたいですが、インフルエンザが心配です。"},{"id":{"d":27,"n":2,"t":3},"speaker":"B","text":"どこへ遊びに行くつもりですか？"},{"id":{"d":27,"n":3,"t":3},"speaker":"A","text":"埼玉の大宮です。初めて行くのでどの飲食店が良いかもわかりません。"},{"id":{"d":27,"n":4,"t":3},"speaker":"B","text":"それでは口コミサイトを見てみたらよいでしょう。口コミサイトは参考になりますよ。いろいろな飲食店の混雑状況や感染対策の有無も把握できると思います。"},{"id":{"d":27,"n":5,"t":3},"speaker":"A","text":"なるほど、それは良い案ですね。口コミサイトを見てみます。アドバイスありがとうございました。"},{"id":{"d":27,"n":6,"t":3},"speaker":"B","text":"どういたしまして。インフルエンザの流行が早く収まるといいですね。"}];
INSERT INTO sentence [{"id":{"d":28,"n":1,"t":3},"speaker":"A","text":"何かお探しですか？"},{"id":{"d":28,"n":2,"t":3},"speaker":"B","text":"日本の友達へのお土産を探しています。"},{"id":{"d":28,"n":3,"t":3},"speaker":"A","text":"どういったものがよろしいでしょうか？"},{"id":{"d":28,"n":4,"t":3},"speaker":"B","text":"洋服がいいのですが。"},{"id":{"d":28,"n":5,"t":3},"speaker":"A","text":"それではこちらのTシャツはいかがですか？日本人の旅行客にも人気です。"},{"id":{"d":28,"n":6,"t":3},"speaker":"B","text":"かわいいですね。いくらですか？"},{"id":{"d":28,"n":7,"t":3},"speaker":"A","text":"1枚10ドルです。"},{"id":{"d":28,"n":8,"t":3},"speaker":"B","text":"では2枚ください。"},{"id":{"d":28,"n":9,"t":3},"speaker":"A","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":29,"n":1,"t":3},"speaker":"A","text":"私はいつか船で世界一周の旅に行くのが夢です。"},{"id":{"d":29,"n":2,"t":3},"speaker":"B","text":"数カ月かけて船で世界をゆっくり1周できるなんてぜいたくですよね。"},{"id":{"d":29,"n":3,"t":3},"speaker":"A","text":"はい。以前テレビで船の旅を見てから、ずっと主人と行きたいと思っているのです。"},{"id":{"d":29,"n":4,"t":3},"speaker":"B","text":"行けるといいですね。ご主人もきっと喜ぶでしょうね。"}];
INSERT INTO sentence [{"id":{"d":30,"n":1,"t":3},"speaker":"A","text":"ドルへの両替をお願いします。"},{"id":{"d":30,"n":2,"t":3},"speaker":"B","text":"かしこまりました。おいくら分でしょうか？"},{"id":{"d":30,"n":3,"t":3},"speaker":"A","text":"5万円分お願いします。1ドル札を多めでお願いします。"},{"id":{"d":30,"n":4,"t":3},"speaker":"B","text":"かしこまりました。こちらでいかがですか？お確かめください。"},{"id":{"d":30,"n":5,"t":3},"speaker":"A","text":"大丈夫です。ありがとうございます。"},{"id":{"d":30,"n":6,"t":3},"speaker":"B","text":"良い旅をお祈りいたします。"}];
INSERT INTO sentence [{"id":{"d":31,"n":1,"t":3},"speaker":"A","text":"ハチ公と一緒に写真を撮ってもらえませんか？"},{"id":{"d":31,"n":2,"t":3},"speaker":"B","text":"よいですよ。シャッターはどこですか？"},{"id":{"d":31,"n":3,"t":3},"speaker":"A","text":"右上にあります。"},{"id":{"d":31,"n":4,"t":3},"speaker":"B","text":"右上ですね。では撮りますよ。このような感じでどうですか？"},{"id":{"d":31,"n":5,"t":3},"speaker":"A","text":"はい、ありがとうございました。"},{"id":{"d":31,"n":6,"t":3},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":32,"n":1,"t":3},"speaker":"A","text":"今度一緒に水上スキーをしませんか？"},{"id":{"d":32,"n":2,"t":3},"speaker":"B","text":"ぜひ、体験してみたいです。どこでできるのでしょうか？"},{"id":{"d":32,"n":3,"t":3},"speaker":"A","text":"猪苗代湖でできますよ。"},{"id":{"d":32,"n":4,"t":3},"speaker":"B","text":"そうなのですね。でも、私は何も道具を持っていません。"},{"id":{"d":32,"n":5,"t":3},"speaker":"A","text":"猪苗代湖では必要な道具を全てレンタルできます。水上スキーの他に、バナナボートもできるようです。"},{"id":{"d":32,"n":6,"t":3},"speaker":"B","text":"楽しそうですね。そのようなアクティビティは海外でしかできないものかと思っていました。"}];
INSERT INTO sentence [{"id":{"d":33,"n":1,"t":3},"speaker":"A","text":"温かい飲み物をもらえますか？"},{"id":{"d":33,"n":2,"t":3},"speaker":"B","text":"コーヒーと紅茶がございます。"},{"id":{"d":33,"n":3,"t":3},"speaker":"A","text":"では紅茶をください。"},{"id":{"d":33,"n":4,"t":3},"speaker":"B","text":"ミルクと砂糖はいかがなさいますか？"},{"id":{"d":33,"n":5,"t":3},"speaker":"A","text":"結構です。"},{"id":{"d":33,"n":6,"t":3},"speaker":"B","text":"かしこまりました。熱いのでお気を付けてお召し上がりくださいませ。"},{"id":{"d":33,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":34,"n":1,"t":3},"speaker":"A","text":"すみません。"},{"id":{"d":34,"n":2,"t":3},"speaker":"B","text":"はい、どうしましたか？"},{"id":{"d":34,"n":3,"t":3},"speaker":"A","text":"座席の背もたれを倒してもよいですか？"},{"id":{"d":34,"n":4,"t":3},"speaker":"B","text":"構いませんよ。"},{"id":{"d":34,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。倒しますね。"},{"id":{"d":34,"n":6,"t":3},"speaker":"B","text":"どうぞ。"}];
INSERT INTO sentence [{"id":{"d":35,"n":1,"t":3},"speaker":"A","text":"パスポートを見せてください。滞在の目的は何ですか？"},{"id":{"d":35,"n":2,"t":3},"speaker":"B","text":"観光です。"},{"id":{"d":35,"n":3,"t":3},"speaker":"A","text":"何泊ですか？"},{"id":{"d":35,"n":4,"t":3},"speaker":"B","text":"3泊です。"},{"id":{"d":35,"n":5,"t":3},"speaker":"A","text":"宿泊場所はどこですか？"},{"id":{"d":35,"n":6,"t":3},"speaker":"B","text":"友人の家です。"},{"id":{"d":35,"n":7,"t":3},"speaker":"A","text":"分かりました。良い旅をお祈りいたします。"}];
INSERT INTO sentence [{"id":{"d":36,"n":1,"t":3},"speaker":"A","text":"予約してありませんが入店できますか？"},{"id":{"d":36,"n":2,"t":3},"speaker":"B","text":"何名さまですか？"},{"id":{"d":36,"n":3,"t":3},"speaker":"A","text":"2人です。"},{"id":{"d":36,"n":4,"t":3},"speaker":"B","text":"かしこまりました。ただ今確認いたしますので少々お待ちください。"},{"id":{"d":36,"n":5,"t":3},"speaker":"A","text":"はい。分かりました。"},{"id":{"d":36,"n":6,"t":3},"speaker":"B","text":"カウンターでしたらご案内できますが、いかがでしょうか？"},{"id":{"d":36,"n":7,"t":3},"speaker":"A","text":"大丈夫です。お願いします。"},{"id":{"d":36,"n":8,"t":3},"speaker":"B","text":"それではご案内いたします。"}];
INSERT INTO sentence [{"id":{"d":37,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。何名さまですか？"},{"id":{"d":37,"n":2,"t":3},"speaker":"B","text":"3人です。"},{"id":{"d":37,"n":3,"t":3},"speaker":"A","text":"こちらの窓際のお席でよろしいでしょうか？"},{"id":{"d":37,"n":4,"t":3},"speaker":"B","text":"はい。ありがとうございます。この席からは夜景が見えるのですね。"},{"id":{"d":37,"n":5,"t":3},"speaker":"A","text":"はい。こちらは横浜の夜景が一望できる特別なお席です。"},{"id":{"d":37,"n":6,"t":3},"speaker":"B","text":"すてきですね。お料理も楽しみです。"},{"id":{"d":37,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。それではご注文が決まりましたらお声がけください。"}];
INSERT INTO sentence [{"id":{"d":38,"n":1,"t":3},"speaker":"A","text":"今日はとても良い天気で、気持ち良かったですね。"},{"id":{"d":38,"n":2,"t":3},"speaker":"B","text":"本当に、気持ち良かったですね。今日は何をしていましたか？"},{"id":{"d":38,"n":3,"t":3},"speaker":"A","text":"あまりにも良いお天気だったので、今日はホテルに残って洗濯をしていました。長期旅行で洗濯物がたまってしまっていたのですが、すぐに乾いて助かりました。"},{"id":{"d":38,"n":4,"t":3},"speaker":"B","text":"私もだいぶたまってきているので、そろそろ洗濯をしたいです。どこで洗濯したのですか？"},{"id":{"d":38,"n":5,"t":3},"speaker":"A","text":"1階にランドリーコーナーがあり、そこで洗濯をしました。"},{"id":{"d":38,"n":6,"t":3},"speaker":"B","text":"ランドリーコーナーがあったのですね。知りませんでした。"},{"id":{"d":38,"n":7,"t":3},"speaker":"A","text":"他にもいろいろ設備がありますので、フロントで確認することをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":39,"n":1,"t":3},"speaker":"A","text":"どちらから栃木にいらっしゃいましたか？"},{"id":{"d":39,"n":2,"t":3},"speaker":"B","text":"京都です。"},{"id":{"d":39,"n":3,"t":3},"speaker":"A","text":"京都といえば、八つ橋や宇治抹茶が有名ですね。"},{"id":{"d":39,"n":4,"t":3},"speaker":"B","text":"はい。その他にも、湯豆腐や京野菜で作った漬物が有名です。栃木といえばイチゴやギョーザが有名ですよね？"},{"id":{"d":39,"n":5,"t":3},"speaker":"A","text":"はい。佐野ラーメンや日光ゆばも有名です。"}];
INSERT INTO sentence [{"id":{"d":40,"n":1,"t":3},"speaker":"A","text":"いつも休みの日には何をしていますか？"},{"id":{"d":40,"n":2,"t":3},"speaker":"B","text":"横浜の中華街においしいものを食べに行きます。"},{"id":{"d":40,"n":3,"t":3},"speaker":"A","text":"いいですね。お勧めの食べ物は何かありますか？"},{"id":{"d":40,"n":4,"t":3},"speaker":"B","text":"たくさんありますが、一番は豚まんです。豚まんを食べながら街の雰囲気を見て歩くのが特に好きです。"},{"id":{"d":40,"n":5,"t":3},"speaker":"A","text":"確かに食べ歩きは魅力的ですね。"},{"id":{"d":40,"n":6,"t":3},"speaker":"B","text":"はい。加藤さんは、休みの日にはいつも何をしますか？"},{"id":{"d":40,"n":7,"t":3},"speaker":"A","text":"私はよく由比ケ浜でサーフィンをします。鈴木さんはサーフィンをされますか？"},{"id":{"d":40,"n":8,"t":3},"speaker":"B","text":"サーフィンとは格好良いですね。私はしたことがありませんが、難しいのでしょうか？"},{"id":{"d":40,"n":9,"t":3},"speaker":"A","text":"ロングボードなら初心者にもお勧めですよ。"},{"id":{"d":40,"n":10,"t":3},"speaker":"B","text":"ロングボードとは何ですか？"},{"id":{"d":40,"n":11,"t":3},"speaker":"A","text":"ボードの種類の一つです。少し長めの板で、安定しやすいのですよ。"},{"id":{"d":40,"n":12,"t":3},"speaker":"B","text":"そうでしたか。勉強になります。"},{"id":{"d":40,"n":13,"t":3},"speaker":"A","text":"由比ケ浜では道具をレンタルできるので、今度ご一緒にいかがですか？"},{"id":{"d":40,"n":14,"t":3},"speaker":"B","text":"楽しそうですね。ぜひ行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":41,"n":1,"t":3},"speaker":"A","text":"手を挙げているのが見えましたが、どうされましたか？"},{"id":{"d":41,"n":2,"t":3},"speaker":"B","text":"旅をしています。宗谷岬まで乗せてくれませんか？"},{"id":{"d":41,"n":3,"t":3},"speaker":"A","text":"宗谷岬でしたらちょうど今から向かうところなのでご一緒しましょう。"},{"id":{"d":41,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":41,"n":5,"t":3},"speaker":"A","text":"宗谷岬まで行って何をするのですか？"},{"id":{"d":41,"n":6,"t":3},"speaker":"B","text":"観光です。日本の最北端に行ってみたいのです。"},{"id":{"d":41,"n":7,"t":3},"speaker":"A","text":"私もです。日本の最南端には去年行ったので、今回は、最北端に行きたいと思いまして。"},{"id":{"d":41,"n":8,"t":3},"speaker":"B","text":"それはすごいですね。日本縦断ということですね。私もやってみたいですね。"},{"id":{"d":41,"n":9,"t":3},"speaker":"A","text":"いろいろな場所を回ってみると、その土地その土地の文化の違いがあって、同じ日本なのに面白いと思いました。"},{"id":{"d":41,"n":10,"t":3},"speaker":"B","text":"ぜひ、日本縦断の話を聞かせてください。"},{"id":{"d":41,"n":11,"t":3},"speaker":"A","text":"喜んでお話しさせていただきます。"},{"id":{"d":41,"n":12,"t":3},"speaker":"B","text":"ありがとうございます。道中楽しくなりそうです。"},{"id":{"d":41,"n":13,"t":3},"speaker":"A","text":"私もそう思います。"}];
INSERT INTO sentence [{"id":{"d":42,"n":1,"t":3},"speaker":"A","text":"今まで乗ったエレベーターの中で印象的だったものは何ですか？私は帝国ホテルのエレベーターが印象に残っています。"},{"id":{"d":42,"n":2,"t":3},"speaker":"B","text":"帝国ホテルですか。私は福島の水族館で乗ったエレベーターが1番印象に残っていますね。"},{"id":{"d":42,"n":3,"t":3},"speaker":"A","text":"それは一体どのようなエレベーターなのですか？"},{"id":{"d":42,"n":4,"t":3},"speaker":"B","text":"エレベーターの周りに水槽があって、魚が泳いでいる様子が見られるのです。"},{"id":{"d":42,"n":5,"t":3},"speaker":"A","text":"それはすごいですね。一度見てみたいものです。"}];
INSERT INTO sentence [{"id":{"d":43,"n":1,"t":3},"speaker":"A","text":"聞いてください。実は私、ガソリンスタンド巡りという趣味を持っているのです。"},{"id":{"d":43,"n":2,"t":3},"speaker":"B","text":"それは珍しい趣味ですね。初めて聞きました。"},{"id":{"d":43,"n":3,"t":3},"speaker":"A","text":"そうでしょう。ガソリンスタンドに出会うたびに、少しずつ給油をして旅をするのです。"},{"id":{"d":43,"n":4,"t":3},"speaker":"B","text":"途中でガソリンがなくなってしまうことはないのですか？"},{"id":{"d":43,"n":5,"t":3},"speaker":"A","text":"大丈夫です。全てのガソリンスタンドを調べているので。"},{"id":{"d":43,"n":6,"t":3},"speaker":"B","text":"とても熱心ですね！ガソリンスタンド巡りの魅力は何ですか？"},{"id":{"d":43,"n":7,"t":3},"speaker":"A","text":"その土地やスタンドによっていろいろな違いがあるところですね。"},{"id":{"d":43,"n":8,"t":3},"speaker":"B","text":"例えばどのような違いがあるのですか？"},{"id":{"d":43,"n":9,"t":3},"speaker":"A","text":"一言では語れませんが、立地によって設備やサービス・接客に違いがあるのですよ。"},{"id":{"d":43,"n":10,"t":3},"speaker":"B","text":"なるほど。あまり気にしていませんでしたが、そういった目線で見ると確かに面白そうですね。"},{"id":{"d":43,"n":11,"t":3},"speaker":"A","text":"次回の給油時には、ぜひそのようなところも気にして見てください。"},{"id":{"d":43,"n":12,"t":3},"speaker":"B","text":"最近はガソリン代が上がっているので行くのをためらっていましたが、そのような楽しみがあるとガソリンスタンドに行きたくなりますね。"}];
INSERT INTO sentence [{"id":{"d":44,"n":1,"t":3},"speaker":"A","text":"田中さんはよくサイクリングをなさるのですか？私は今日が初めてです。"},{"id":{"d":44,"n":2,"t":3},"speaker":"B","text":"私は普段からよくしていますよ。始めて10年ほどになります。"},{"id":{"d":44,"n":3,"t":3},"speaker":"A","text":"素晴らしいですね。ぜひいろいろと教えてください。"},{"id":{"d":44,"n":4,"t":3},"speaker":"B","text":"もちろんですよ。では、出発しましょうか。付いてきてくださいね。"},{"id":{"d":44,"n":5,"t":3},"speaker":"A","text":"頼もしいです。"}];
INSERT INTO sentence [{"id":{"d":45,"n":1,"t":3},"speaker":"A","text":"こんにちは。"},{"id":{"d":45,"n":2,"t":3},"speaker":"B","text":"いらっしゃいませ。"},{"id":{"d":45,"n":3,"t":3},"speaker":"A","text":"浅草では、こちらの天丼が有名と聞いてきました。表の看板にあった、天丼定食を2人前お願いします。"},{"id":{"d":45,"n":4,"t":3},"speaker":"B","text":"かしこまりました。天丼定食を2人前ですね。"},{"id":{"d":45,"n":5,"t":3},"speaker":"A","text":"はい。お願いします。お時間はどれくらいかかりますか？"},{"id":{"d":45,"n":6,"t":3},"speaker":"B","text":"ちょうどお昼時で混み合っておりますので、30分ほどお時間をいただきますが、よろしいでしょうか？"},{"id":{"d":45,"n":7,"t":3},"speaker":"A","text":"はい。時間には余裕がありますので、大丈夫です。歩き回って、おなかがとてもすいたので、待ち遠しくて時間が気になってしまいました。"},{"id":{"d":45,"n":8,"t":3},"speaker":"B","text":"そうでしたか。それほどまでに楽しみにしていただけるのは、とてもうれしいです。ありがとうございます。では、なるべく急ぎでお作りいたしますね。"},{"id":{"d":45,"n":9,"t":3},"speaker":"A","text":"急がなくても大丈夫です。待っている時間もおいしく食べるには大切ですから。"},{"id":{"d":45,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。そう言っていただけると、とても助かります。"}];
INSERT INTO sentence [{"id":{"d":46,"n":1,"t":3},"speaker":"A","text":"どちらにお住まいですか？"},{"id":{"d":46,"n":2,"t":3},"speaker":"B","text":"福島県です。"},{"id":{"d":46,"n":3,"t":3},"speaker":"A","text":"福島県には昔行ったことがあります。会津若松に行きました。"},{"id":{"d":46,"n":4,"t":3},"speaker":"B","text":"会津には見どころがたくさんありますね。例えば、猪苗代湖は、澄んだ水面に映る自然豊かな景色が素晴らしいです。"},{"id":{"d":46,"n":5,"t":3},"speaker":"A","text":"そうですね。猪苗代湖も美しいですが、五色沼も大変幻想的でした。"},{"id":{"d":46,"n":6,"t":3},"speaker":"B","text":"五色沼もいいですね。鶴ヶ城には行かれましたか？"},{"id":{"d":46,"n":7,"t":3},"speaker":"A","text":"はい。白虎隊の話を聞いた時は悲しい気持ちになりました。"},{"id":{"d":46,"n":8,"t":3},"speaker":"B","text":"私も白虎隊が登場するドラマを見るたびに涙が止まりません。ところで、会津の名物料理こづゆはお召し上がりになられましたか？"},{"id":{"d":46,"n":9,"t":3},"speaker":"A","text":"はい、頂きました。とてもおいしかったです。"},{"id":{"d":46,"n":10,"t":3},"speaker":"B","text":"よかったです。会津のおもてなし料理ですからね。福島には会津だけでなく他にも観光スポットがたくさんあるのですよ。"},{"id":{"d":46,"n":11,"t":3},"speaker":"A","text":"特にお薦めの場所や食べ物はありますか？"},{"id":{"d":46,"n":12,"t":3},"speaker":"B","text":"私のお薦めは、大内宿のネギそばです。味はもちろん、驚くような見た目もすてきですよ。ネギが1本添えてあって、それを箸代わりにしておそばを食べるのです。"},{"id":{"d":46,"n":13,"t":3},"speaker":"A","text":"ネギ1本ですか。それはすごいですね。"},{"id":{"d":46,"n":14,"t":3},"speaker":"B","text":"ネギは薬味としておそばと一緒に食べてもよいのです。辛みがくせになります。"}];
INSERT INTO sentence [{"id":{"d":47,"n":1,"t":3},"speaker":"A","text":"お電話ありがとうございます。旅行のご予約を承ります。"},{"id":{"d":47,"n":2,"t":3},"speaker":"B","text":"すみません。年末に旅行を考えておりまして、ホテルと飛行機の予約をお願いしたいのですが。"},{"id":{"d":47,"n":3,"t":3},"speaker":"A","text":"かしこまりました。ご旅行先や、期間などはお決まりですか？"},{"id":{"d":47,"n":4,"t":3},"speaker":"B","text":"海外に行きたいと思っているのですが、初めてなので、何も分かりません。何から始めたらよいか教えていただけますか？"},{"id":{"d":47,"n":5,"t":3},"speaker":"A","text":"そうしましたら、まずはパスポートをご申請ください。"},{"id":{"d":47,"n":6,"t":3},"speaker":"B","text":"パスポートはどこで申請できますか？"},{"id":{"d":47,"n":7,"t":3},"speaker":"A","text":"お住まいの市町村の役所で申請できます。"},{"id":{"d":47,"n":8,"t":3},"speaker":"B","text":"申請には何が必要ですか？"},{"id":{"d":47,"n":9,"t":3},"speaker":"A","text":"申請には、戸籍謄本と住民票の写し、写真、運転免許証などの本人確認書類が必要になります。"},{"id":{"d":47,"n":10,"t":3},"speaker":"B","text":"いろいろ準備が必要なのですね。"},{"id":{"d":47,"n":11,"t":3},"speaker":"A","text":"そうですね。しかし、成人であれば、10年間有効のパスポートを申請できますので、一度取ってしまえば、しばらくはそれが使えますよ。"}];
INSERT INTO sentence [{"id":{"d":48,"n":1,"t":3},"speaker":"A","text":"今度の3連休に、一緒に台湾旅行をしませんか？"},{"id":{"d":48,"n":2,"t":3},"speaker":"B","text":"いいですね。行きましょう。ツアーではなく個人旅行で行きたいですね。"},{"id":{"d":48,"n":3,"t":3},"speaker":"A","text":"個人旅行ということはタクシーで回るのですか？"},{"id":{"d":48,"n":4,"t":3},"speaker":"B","text":"タクシーは値段が高いので、バスや電車など、公共交通機関を使いたいと思っています。乗り放題の便利なプリペイドカードも発行されているようです。"},{"id":{"d":48,"n":5,"t":3},"speaker":"A","text":"公共の交通機関を使うと確かに安く済みますが、移動に時間がかかるので、あまり多くの観光スポットを見ることはできませんね。"},{"id":{"d":48,"n":6,"t":3},"speaker":"B","text":"そうですね。3連休で行くとなると2泊3日しか時間がないので、移動の一部はタクシーを利用することも考えないといけませんね。"},{"id":{"d":48,"n":7,"t":3},"speaker":"A","text":"2泊3日の旅ならツアーに参加するのもいいかもしれませんよ。自由度は低いですが、ツアー会社で用意したバスで回れるので公共の交通機関を使うよりたくさん見られるかもしれません。"},{"id":{"d":48,"n":8,"t":3},"speaker":"B","text":"それも一つの方法ですね。行きたい所がツアーのコースに入っているものを探してみますね。"},{"id":{"d":48,"n":9,"t":3},"speaker":"A","text":"私も探してみます。良さげなツアーが見つかったら連絡します。"}];
INSERT INTO sentence [{"id":{"d":49,"n":1,"t":3},"speaker":"A","text":"すみません。このバスは天狗山へ行きますか？"},{"id":{"d":49,"n":2,"t":3},"speaker":"B","text":"行きますよ。私はそこでスノーボードをする予定です。"},{"id":{"d":49,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。もしご存じでしたらもう一つ教えていただきたいのですが、スキー用品一式を山のスキー場で借りることはできますか？"},{"id":{"d":49,"n":4,"t":3},"speaker":"B","text":"できます。ただ、レンタル店は夕方の4時で閉店だったと思うので、今からでは遅過ぎるかもしれません。もちろんスキーが無理でも山からの夜景を楽しむことはできますよ。"},{"id":{"d":49,"n":5,"t":3},"speaker":"A","text":"それでは今回はスキーではなく夜景を楽しむことにします。ありがとうございます。"},{"id":{"d":49,"n":6,"t":3},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":50,"n":1,"t":3},"speaker":"A","text":"このバスは何時に出発しますか？"},{"id":{"d":50,"n":2,"t":3},"speaker":"B","text":"13時に出発いたします。"},{"id":{"d":50,"n":3,"t":3},"speaker":"A","text":"千葉駅まではいくらかかりますか？"},{"id":{"d":50,"n":4,"t":3},"speaker":"B","text":"500円でございます。"},{"id":{"d":50,"n":5,"t":3},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":51,"n":1,"t":3},"speaker":"A","text":"部屋が暗いので別の部屋に変えていただくことはできますか？"},{"id":{"d":51,"n":2,"t":3},"speaker":"B","text":"もちろん大丈夫ですよ。どのようなお部屋をご希望ですか？"},{"id":{"d":51,"n":3,"t":3},"speaker":"A","text":"窓のあるお部屋がいいのですが。"},{"id":{"d":51,"n":4,"t":3},"speaker":"B","text":"空いております。追加で5,000円頂きますがよろしいでしょうか？"},{"id":{"d":51,"n":5,"t":3},"speaker":"A","text":"大丈夫です。お願いします。"},{"id":{"d":51,"n":6,"t":3},"speaker":"B","text":"ではこちらにサインをお願いいたします。"},{"id":{"d":51,"n":7,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":51,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。こちらが新しいお部屋の鍵になります。ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":52,"n":1,"t":3},"speaker":"A","text":"このカップはどこで作られたものですか？"},{"id":{"d":52,"n":2,"t":3},"speaker":"B","text":"ドイツ製です。作りも丈夫なのでお薦めです。"},{"id":{"d":52,"n":3,"t":3},"speaker":"A","text":"1つおいくらですか？"},{"id":{"d":52,"n":4,"t":3},"speaker":"B","text":"3,000円です。"},{"id":{"d":52,"n":5,"t":3},"speaker":"A","text":"ではプレゼント用にラッピングをお願いします。"},{"id":{"d":52,"n":6,"t":3},"speaker":"B","text":"かしこまりました。リボンは何色にしますか？"},{"id":{"d":52,"n":7,"t":3},"speaker":"A","text":"ピンクでお願いします。"},{"id":{"d":52,"n":8,"t":3},"speaker":"B","text":"かしこまりました。ではお包みしますので、こちらの番号札を持ってお待ちくださいませ。"},{"id":{"d":52,"n":9,"t":3},"speaker":"A","text":"はい。お願いします。"}];
INSERT INTO sentence [{"id":{"d":53,"n":1,"t":3},"speaker":"A","text":"エスタはどのように申請したらよいですか？"},{"id":{"d":53,"n":2,"t":3},"speaker":"B","text":"こちらの申請ページの案内に従って情報を入力してください。"},{"id":{"d":53,"n":3,"t":3},"speaker":"A","text":"支払方法はどのようなものがありますか？"},{"id":{"d":53,"n":4,"t":3},"speaker":"B","text":"クレジット決済がございます。"},{"id":{"d":53,"n":5,"t":3},"speaker":"A","text":"分かりました。この後はどうすればよいですか？"},{"id":{"d":53,"n":6,"t":3},"speaker":"B","text":"メールアドレスに申請の結果を送信いたします。許可が下りましたらエスタでの渡米が可能となります。"},{"id":{"d":53,"n":7,"t":3},"speaker":"A","text":"許可が下りたらどうしたらよいですか？"},{"id":{"d":53,"n":8,"t":3},"speaker":"B","text":"旅行の際に必ず申請IDをお持ちください。"},{"id":{"d":53,"n":9,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":53,"n":10,"t":3},"speaker":"B","text":"良い旅をお祈りします。"}];
INSERT INTO sentence [{"id":{"d":54,"n":1,"t":3},"speaker":"A","text":"このお店には、どのようなお土産がありますか？"},{"id":{"d":54,"n":2,"t":3},"speaker":"B","text":"チャイナドレスや紹興酒、中国茶などがございます。"},{"id":{"d":54,"n":3,"t":3},"speaker":"A","text":"では中国茶をください。"},{"id":{"d":54,"n":4,"t":3},"speaker":"B","text":"おいくつですか？"},{"id":{"d":54,"n":5,"t":3},"speaker":"A","text":"2つお願いします。"},{"id":{"d":54,"n":6,"t":3},"speaker":"B","text":"60元です。ご一緒にこちらの茶器もいかがですか？"},{"id":{"d":54,"n":7,"t":3},"speaker":"A","text":"かわいいですね。おいくらですか？"},{"id":{"d":54,"n":8,"t":3},"speaker":"B","text":"280元です。"},{"id":{"d":54,"n":9,"t":3},"speaker":"A","text":"ではそちらも1つください。"},{"id":{"d":54,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":55,"n":1,"t":3},"speaker":"A","text":"大阪にはおいしい食べ物がたくさんありますね。あなたは何が好きですか？"},{"id":{"d":55,"n":2,"t":3},"speaker":"B","text":"私はたこ焼きが好きです。特に白だし汁がお薦めです。"},{"id":{"d":55,"n":3,"t":3},"speaker":"A","text":"たこ焼きは定番ですね。私は豚まんが好きです。大阪に行くと必ず買って帰ります。"},{"id":{"d":55,"n":4,"t":3},"speaker":"B","text":"豚まんは私も大好きです。他にもてっちりが有名ですよね。"},{"id":{"d":55,"n":5,"t":3},"speaker":"A","text":"そうですね。でも、実は、私はてっちりを食べたことがありません。"},{"id":{"d":55,"n":6,"t":3},"speaker":"B","text":"そうなのですか？とてもおいしいですよ。よかったら今度食べに行きませんか？"},{"id":{"d":55,"n":7,"t":3},"speaker":"A","text":"はい。ぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":56,"n":1,"t":3},"speaker":"A","text":"福岡でおいしいランチが食べられるお店はありますか？"},{"id":{"d":56,"n":2,"t":3},"speaker":"B","text":"福岡でしたらこの店はいかがですか？おいしいイカ刺しが食べられます。"},{"id":{"d":56,"n":3,"t":3},"speaker":"A","text":"イカですか？福岡といえば、めんたいこの印象がありました。"},{"id":{"d":56,"n":4,"t":3},"speaker":"B","text":"福岡はイカもおいしいですよ。生けすがあって目の前で刺し身に調理してくれます。"},{"id":{"d":56,"n":5,"t":3},"speaker":"A","text":"すごいですね。それはおいしそうです。"},{"id":{"d":56,"n":6,"t":3},"speaker":"B","text":"さらに、お願いすれば、そのイカの半分は天ぷらにしてくれるのですよ。"},{"id":{"d":56,"n":7,"t":3},"speaker":"A","text":"一度でたくさん楽しめますね。そのお店はどこにありますか？"},{"id":{"d":56,"n":8,"t":3},"speaker":"B","text":"博多駅から歩いて5分の所にあります。"},{"id":{"d":56,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。行ってみます。"}];
INSERT INTO sentence [{"id":{"d":57,"n":1,"t":3},"speaker":"A","text":"韓国旅行のツアーを申し込みたいのですが。"},{"id":{"d":57,"n":2,"t":3},"speaker":"B","text":"何泊をご希望ですか？"},{"id":{"d":57,"n":3,"t":3},"speaker":"A","text":"3泊です。"},{"id":{"d":57,"n":4,"t":3},"speaker":"B","text":"それでしたらこちらのツアーはいかがですか？3食付いて主要な観光スポットも楽しめます。"},{"id":{"d":57,"n":5,"t":3},"speaker":"A","text":"いくらですか？"},{"id":{"d":57,"n":6,"t":3},"speaker":"B","text":"お1人さま3万9,800円でございます。"},{"id":{"d":57,"n":7,"t":3},"speaker":"A","text":"なるほど。3名で申し込めますか？"},{"id":{"d":57,"n":8,"t":3},"speaker":"B","text":"もちろんです。こちらの用紙にご記入ください。"},{"id":{"d":57,"n":9,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":57,"n":10,"t":3},"speaker":"B","text":"ご記入が終わりましたら、お声がけください。"}];
INSERT INTO sentence [{"id":{"d":58,"n":1,"t":3},"speaker":"A","text":"修学旅行はどこに行きましたか？"},{"id":{"d":58,"n":2,"t":3},"speaker":"B","text":"私は沖縄に行きました。海がとてもきれいでした。佐藤さんはどこへ行きましたか？"},{"id":{"d":58,"n":3,"t":3},"speaker":"A","text":"私は京都と奈良に行きました。お寺巡りが楽しかったです。"},{"id":{"d":58,"n":4,"t":3},"speaker":"B","text":"それは良いですね。食事は何を食べたのですか？"},{"id":{"d":58,"n":5,"t":3},"speaker":"A","text":"京野菜の懐石料理を食べました。野菜本来の甘さが際立っていてとてもおいしかったです。"}];
INSERT INTO sentence [{"id":{"d":59,"n":1,"t":3},"speaker":"A","text":"鹿児島に行きたいのですが、どうやって行けばよいでしょうか？"},{"id":{"d":59,"n":2,"t":3},"speaker":"B","text":"今は交通手段がいろいろありますからね。飛行機、フェリー、電車やバスもあります。どの交通機関を利用して行きますか？"},{"id":{"d":59,"n":3,"t":3},"speaker":"A","text":"いろいろな行き方があって悩みますね。"},{"id":{"d":59,"n":4,"t":3},"speaker":"B","text":"そうですね。急いで行くなら飛行機や新幹線、ゆっくり行くなら船やバスがいいと思いますよ。"},{"id":{"d":59,"n":5,"t":3},"speaker":"A","text":"のんびり行きたいので、船で行くことにします。"}];
INSERT INTO sentence [{"id":{"d":60,"n":1,"t":3},"speaker":"A","text":"電車で北海道まで行きたいのですが、鹿沼市からはどのようにして行けますか？"},{"id":{"d":60,"n":2,"t":3},"speaker":"B","text":"それでしたら、今は旅行会社でいろいろなツアーがあります。調べてみるといいですよ。"},{"id":{"d":60,"n":3,"t":3},"speaker":"A","text":"そうなのですね。ありがとうございます。調べてみます。"},{"id":{"d":60,"n":4,"t":3},"speaker":"B","text":"良いツアーが見つかるといいですね。"}];
INSERT INTO sentence [{"id":{"d":61,"n":1,"t":3},"speaker":"A","text":"今週末は東京に夜桜を見に行こうと思います。"},{"id":{"d":61,"n":2,"t":3},"speaker":"B","text":"いいですね。東京のどちらに行かれるのですか？"},{"id":{"d":61,"n":3,"t":3},"speaker":"A","text":"神楽坂に行こうと思っています。"},{"id":{"d":61,"n":4,"t":3},"speaker":"B","text":"なるほど。川沿いにサクラ並木があってきれいですよね。"},{"id":{"d":61,"n":5,"t":3},"speaker":"A","text":"はい。先ほどニュースで週末にちょうどサクラが満開になると言っていましたので、桜吹雪も見られそうでとても楽しみです。"},{"id":{"d":61,"n":6,"t":3},"speaker":"B","text":"桜吹雪ですか、きれいですよね。あとで感想を聞かせてくださいね。"},{"id":{"d":61,"n":7,"t":3},"speaker":"A","text":"はい。楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":62,"n":1,"t":3},"speaker":"A","text":"この休みは1人で四国まで行ってみようと思っています。初めて行く場所ですし、あまり事前調査ができていないので少し不安ですが。"},{"id":{"d":62,"n":2,"t":3},"speaker":"B","text":"いいですね。自由気ままに旅をするのもいいと思います。きっと楽しめると思いますよ。"},{"id":{"d":62,"n":3,"t":3},"speaker":"A","text":"そうですよね。向こうで何かお土産を買ってきますね。"},{"id":{"d":62,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。でも、あまり気を使わず、ゆっくり楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":63,"n":1,"t":3},"speaker":"A","text":"ねぶた祭を見に、青森へ行きたいと思っています。"},{"id":{"d":63,"n":2,"t":3},"speaker":"B","text":"ねぶた祭ならテレビで見たことがあります。大変な迫力ですよね。"},{"id":{"d":63,"n":3,"t":3},"speaker":"A","text":"そうですね。鹿沼から行くとしたら、交通手段は何がいいのでしょうか？"},{"id":{"d":63,"n":4,"t":3},"speaker":"B","text":"電車がいいと思います。車では時間がかかりますし、飛行機では遠回りになってしまいます。"},{"id":{"d":63,"n":5,"t":3},"speaker":"A","text":"そうなのですね。今年の夏に、電車で青森へ行ってみようと思います。"},{"id":{"d":63,"n":6,"t":3},"speaker":"B","text":"私もご一緒してよろしいですか？"},{"id":{"d":63,"n":7,"t":3},"speaker":"A","text":"はい、ぜひ一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":64,"n":1,"t":3},"speaker":"A","text":"新潟の観光スポットと言ったらどこでしょうか？"},{"id":{"d":64,"n":2,"t":3},"speaker":"B","text":"瀬波海岸はきれいですよ。あの辺りは、お米もお酒もおいしいです。"},{"id":{"d":64,"n":3,"t":3},"speaker":"A","text":"行ったことがあるのですか？"},{"id":{"d":64,"n":4,"t":3},"speaker":"B","text":"はい、若い頃に一度だけ行ったことがあります。"},{"id":{"d":64,"n":5,"t":3},"speaker":"A","text":"そうなのですね。夏に新潟へ行く予定があるので、瀬波海岸に行ってこようと思います。"}];
INSERT INTO sentence [{"id":{"d":65,"n":1,"t":3},"speaker":"A","text":"顔色が真っ青ですが、どうされましたか？"},{"id":{"d":65,"n":2,"t":3},"speaker":"B","text":"実は具合が悪いのです。どうやら、船酔いしたようです。"},{"id":{"d":65,"n":3,"t":3},"speaker":"A","text":"大丈夫ですか？売店に酔い止めの薬が売っていますよ。よろしければ買っていらしたらいかがですか？"},{"id":{"d":65,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。では、ちょっと買ってきます。"},{"id":{"d":65,"n":5,"t":3},"speaker":"A","text":"少しでも良くなるといいですね。"}];
INSERT INTO sentence [{"id":{"d":66,"n":1,"t":3},"speaker":"A","text":"この近くに洋服屋さんはありますか？"},{"id":{"d":66,"n":2,"t":3},"speaker":"B","text":"歩いて行ける範囲にはありません。コンビニエンスストアなら徒歩5分の所にあるのですが。"},{"id":{"d":66,"n":3,"t":3},"speaker":"A","text":"そのコンビニエンスストアには靴下は売っていますか？靴下が汚れてしまったのです。"},{"id":{"d":66,"n":4,"t":3},"speaker":"B","text":"綿の靴下か使い捨ての紙の靴下ならあるかもしれません。行ってみてはいかがですか？"},{"id":{"d":66,"n":5,"t":3},"speaker":"A","text":"コンビニエンスストアはどこにありますか？"},{"id":{"d":66,"n":6,"t":3},"speaker":"B","text":"この通りをまっすぐ進んで、2つ目の信号を右に曲がると、左側にあります。"},{"id":{"d":66,"n":7,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":67,"n":1,"t":3},"speaker":"A","text":"将来、どのような旅行をしてみたいですか？"},{"id":{"d":67,"n":2,"t":3},"speaker":"B","text":"豪華船クルーズで世界一周の旅に憧れています。"},{"id":{"d":67,"n":3,"t":3},"speaker":"A","text":"すてきですね。船内にはドレスコードのある豪華なレストランが複数あったり、フィットネスジムが完備していたりするようです。世界各国の港に泊まり、お土産を買うこともできるそうですよね。"},{"id":{"d":67,"n":4,"t":3},"speaker":"B","text":"聞いているだけでうっとりしてしまうようなお話です。機会があればぜひ行ってみたいものです。"}];
INSERT INTO sentence [{"id":{"d":68,"n":1,"t":3},"speaker":"A","text":"このバスはショッピングモールに行きますか？"},{"id":{"d":68,"n":2,"t":3},"speaker":"B","text":"申し訳ございません。このバスはショッピングモールへは行きません。"},{"id":{"d":68,"n":3,"t":3},"speaker":"A","text":"ショッピングモールに行くのはどのバスですか？"},{"id":{"d":68,"n":4,"t":3},"speaker":"B","text":"こちらとは反対側の2番乗り場のバスです。"},{"id":{"d":68,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":69,"n":1,"t":3},"speaker":"A","text":"今年の冬休みはニューヨークへ旅行に行ってきました。"},{"id":{"d":69,"n":2,"t":3},"speaker":"B","text":"すてきですね。何が印象に残っていますか？"},{"id":{"d":69,"n":3,"t":3},"speaker":"A","text":"ミュージカルが楽しかったです。"},{"id":{"d":69,"n":4,"t":3},"speaker":"B","text":"何を見ましたか？"},{"id":{"d":69,"n":5,"t":3},"speaker":"A","text":"『キャッツ』や『オペラ座の怪人』を見ました。"},{"id":{"d":69,"n":6,"t":3},"speaker":"B","text":"いいですね。私も行ってみたいです。"},{"id":{"d":69,"n":7,"t":3},"speaker":"A","text":"とても良い体験ができました。機会があればぜひ行ってみてください。"}];
INSERT INTO sentence [{"id":{"d":70,"n":1,"t":3},"speaker":"A","text":"今度、初めて京都に行きます。"},{"id":{"d":70,"n":2,"t":3},"speaker":"B","text":"京都のどこに行くのですか？"},{"id":{"d":70,"n":3,"t":3},"speaker":"A","text":"主には、金閣寺などのお寺を巡ることを考えています。"},{"id":{"d":70,"n":4,"t":3},"speaker":"B","text":"金閣寺は修学旅行で行ったことがあります。とてもきれいでしたよ。"},{"id":{"d":70,"n":5,"t":3},"speaker":"A","text":"そうでしたか。さらに楽しみになってきました。"},{"id":{"d":70,"n":6,"t":3},"speaker":"B","text":"楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":71,"n":1,"t":3},"speaker":"A","text":"浅草に到着後、昼食はどうされますか？"},{"id":{"d":71,"n":2,"t":3},"speaker":"B","text":"大黒屋の天丼を食べようと思っています。"},{"id":{"d":71,"n":3,"t":3},"speaker":"A","text":"そうですか。私は神谷バーでハンバーグかビーフシチューを食べようと思っています。"},{"id":{"d":71,"n":4,"t":3},"speaker":"B","text":"そこならこの間行きましたよ。ビーフシチューがとてもおいしかったです。"},{"id":{"d":71,"n":5,"t":3},"speaker":"A","text":"そうですか。楽しみです。後で天丼の感想も教えてくださいね。"}];
INSERT INTO sentence [{"id":{"d":72,"n":1,"t":3},"speaker":"A","text":"日本の居酒屋は初めてです。まずは何をすればよいですか？"},{"id":{"d":72,"n":2,"t":3},"speaker":"B","text":"はじめに飲み物をお伺いいたします。"},{"id":{"d":72,"n":3,"t":3},"speaker":"A","text":"分かりました。では日本酒を1杯お願いします。ところでこれは何ですか？まだ何も注文していませんが。"},{"id":{"d":72,"n":4,"t":3},"speaker":"B","text":"これはお通しです。皆さんに一律で400円で付けています。"},{"id":{"d":72,"n":5,"t":3},"speaker":"A","text":"居酒屋にはこのようなシステムがあるのですね。"}];
INSERT INTO sentence [{"id":{"d":73,"n":1,"t":3},"speaker":"A","text":"メインのお料理は、チキンとビーフのどちらになさいますか？"},{"id":{"d":73,"n":2,"t":3},"speaker":"B","text":"すみません、お魚料理はありますか？"},{"id":{"d":73,"n":3,"t":3},"speaker":"A","text":"ございます。お時間を頂きますがよろしいでしょうか？"},{"id":{"d":73,"n":4,"t":3},"speaker":"B","text":"はい。大丈夫です。"},{"id":{"d":73,"n":5,"t":3},"speaker":"A","text":"では、しばらくお待ちくださいませ。"},{"id":{"d":73,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":74,"n":1,"t":3},"speaker":"A","text":"クロークはありますか？"},{"id":{"d":74,"n":2,"t":3},"speaker":"B","text":"申し訳ありませんが、クロークはございません。その代わりに各お部屋にセーフティーボックスが付いております。"},{"id":{"d":74,"n":3,"t":3},"speaker":"A","text":"鍵はどのようなものですか？"},{"id":{"d":74,"n":4,"t":3},"speaker":"B","text":"ダイヤルロックでございます。お好きな3桁の番号を設定できます。"},{"id":{"d":74,"n":5,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":74,"n":6,"t":3},"speaker":"B","text":"ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":75,"n":1,"t":3},"speaker":"A","text":"今度、東京ディズニーシーに行くことになりましたが、どのようにして行けばよいでしょう？"},{"id":{"d":75,"n":2,"t":3},"speaker":"B","text":"東京駅からでしたら、電車で行けると思いますよ。"},{"id":{"d":75,"n":3,"t":3},"speaker":"A","text":"そうなのですね。何かお土産を買うつもりなのですが、お薦めのお土産はありますか？"},{"id":{"d":75,"n":4,"t":3},"speaker":"B","text":"被り物やぬいぐるみは人気があるようです。"},{"id":{"d":75,"n":5,"t":3},"speaker":"A","text":"なるほど、それらは確かに魅力的です。"},{"id":{"d":75,"n":6,"t":3},"speaker":"B","text":"食べるものがよろしければ、クッキーやチョコレートなどもお薦めです。"},{"id":{"d":75,"n":7,"t":3},"speaker":"A","text":"参考になります。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":76,"n":1,"t":3},"speaker":"A","text":"ホテルでの滞在を1日延長したいのですが、可能ですか？"},{"id":{"d":76,"n":2,"t":3},"speaker":"B","text":"かしこまりました。7,000円の追加料金がかかりますがよろしいでしょうか？"},{"id":{"d":76,"n":3,"t":3},"speaker":"A","text":"はい、構いません。"},{"id":{"d":76,"n":4,"t":3},"speaker":"B","text":"引き続き現在のお部屋をご利用いただけます。"},{"id":{"d":76,"n":5,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":76,"n":6,"t":3},"speaker":"B","text":"ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":77,"n":1,"t":3},"speaker":"A","text":"アメリカ国内での移動は何がお薦めですか？"},{"id":{"d":77,"n":2,"t":3},"speaker":"B","text":"安く済ませたいなら電車とバスを乗り継ぐのがお薦めです。"},{"id":{"d":77,"n":3,"t":3},"speaker":"A","text":"電車とバスですか。アメリカ旅行は初めてなので迷わないか心配です。"},{"id":{"d":77,"n":4,"t":3},"speaker":"B","text":"それでしたらウーバーはどうですか？"},{"id":{"d":77,"n":5,"t":3},"speaker":"A","text":"それはどのようなものですか？"},{"id":{"d":77,"n":6,"t":3},"speaker":"B","text":"指定した場所まで迎えに来てくれて希望の場所まで車で連れていってくれる、配車サービスです。"},{"id":{"d":77,"n":7,"t":3},"speaker":"A","text":"タクシーみたいな感じですね。"},{"id":{"d":77,"n":8,"t":3},"speaker":"B","text":"はい。タクシーよりも時間に正確で、運転手の情報も分かるので安全です。"},{"id":{"d":77,"n":9,"t":3},"speaker":"A","text":"そうなのですね。教えてくださってありがとうございます。"},{"id":{"d":77,"n":10,"t":3},"speaker":"B","text":"どういたしまして。楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":78,"n":1,"t":3},"speaker":"A","text":"ホテルの近くにコンビニエンスストアはありますか？"},{"id":{"d":78,"n":2,"t":3},"speaker":"B","text":"はい。ございます。"},{"id":{"d":78,"n":3,"t":3},"speaker":"A","text":"徒歩何分くらいでしょうか？"},{"id":{"d":78,"n":4,"t":3},"speaker":"B","text":"徒歩でしたら2、3分ほどの距離にございます。"},{"id":{"d":78,"n":5,"t":3},"speaker":"A","text":"安心しました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":79,"n":1,"t":3},"speaker":"A","text":"館内ツアーは何時からですか？"},{"id":{"d":79,"n":2,"t":3},"speaker":"B","text":"次の館内ツアーでしたら15時からでございます。"},{"id":{"d":79,"n":3,"t":3},"speaker":"A","text":"日本語の音声ガイドはありますか？"},{"id":{"d":79,"n":4,"t":3},"speaker":"B","text":"はい、ございます。レンタル料は1,000円でございます。"},{"id":{"d":79,"n":5,"t":3},"speaker":"A","text":"では、2つお願いします。"},{"id":{"d":79,"n":6,"t":3},"speaker":"B","text":"かしこまりました。"},{"id":{"d":79,"n":7,"t":3},"speaker":"A","text":"操作はどのようにしたらよいですか？"},{"id":{"d":79,"n":8,"t":3},"speaker":"B","text":"まずは、こちらのボタンを押して日本語を選択してください。その後はこちらのボタンで再生と一時停止ができます。"},{"id":{"d":79,"n":9,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":79,"n":10,"t":3},"speaker":"B","text":"ごゆっくりお楽しみください。"}];
INSERT INTO sentence [{"id":{"d":80,"n":1,"t":3},"speaker":"A","text":"今度、フランス旅行に行きます。"},{"id":{"d":80,"n":2,"t":3},"speaker":"B","text":"フランスのどこへ行くのですか？"},{"id":{"d":80,"n":3,"t":3},"speaker":"A","text":"パリへ行きます。"},{"id":{"d":80,"n":4,"t":3},"speaker":"B","text":"いいですね。パリは芸術がさかんで素晴らしい街並みですよ。"},{"id":{"d":80,"n":5,"t":3},"speaker":"A","text":"行ったことがあるのですか？"},{"id":{"d":80,"n":6,"t":3},"speaker":"B","text":"はい。初めてエッフェル塔を見た時は、感動しました。"},{"id":{"d":80,"n":7,"t":3},"speaker":"A","text":"エッフェル塔へはぜひ行きたいと思っています。他にお薦めの場所はありますか？"},{"id":{"d":80,"n":8,"t":3},"speaker":"B","text":"休日はマルシェに行って買い物をするのも楽しいですよ。"},{"id":{"d":80,"n":9,"t":3},"speaker":"A","text":"楽しそうですね。ぜひ行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":81,"n":1,"t":3},"speaker":"A","text":"私は飛行機に乗ることがとても好きです。"},{"id":{"d":81,"n":2,"t":3},"speaker":"B","text":"特に印象に残っているフライトはありますか？"},{"id":{"d":81,"n":3,"t":3},"speaker":"A","text":"一番印象に残っているのは初めてカナダに行った時のことです。"},{"id":{"d":81,"n":4,"t":3},"speaker":"B","text":"それはどうしてですか？"},{"id":{"d":81,"n":5,"t":3},"speaker":"A","text":"それが私の初めての海外旅行だったのです。飛行機に乗ったのも初めてだったので、よく覚えています。"},{"id":{"d":81,"n":6,"t":3},"speaker":"B","text":"そうなのですね。怖くはなかったですか？"},{"id":{"d":81,"n":7,"t":3},"speaker":"A","text":"離陸するまでは怖かったのですが、離陸後は雲の上を飛んでいることにずっと興奮していました。それから飛行機のとりこなのです。"},{"id":{"d":81,"n":8,"t":3},"speaker":"B","text":"良い思い出ですね。"}];
INSERT INTO sentence [{"id":{"d":82,"n":1,"t":3},"speaker":"A","text":"部屋に電子レンジは置いていますか？"},{"id":{"d":82,"n":2,"t":3},"speaker":"B","text":"はい、ございます。ご自由にお使いください。"},{"id":{"d":82,"n":3,"t":3},"speaker":"A","text":"安心しました。ありがとうございます。"},{"id":{"d":82,"n":4,"t":3},"speaker":"B","text":"ポットなどもありますので、こちらもお使いください。"},{"id":{"d":82,"n":5,"t":3},"speaker":"A","text":"それは助かります。"},{"id":{"d":82,"n":6,"t":3},"speaker":"B","text":"他に気になることなどはございませんか？"},{"id":{"d":82,"n":7,"t":3},"speaker":"A","text":"今のところ大丈夫です。"},{"id":{"d":82,"n":8,"t":3},"speaker":"B","text":"承知いたしました。それでは当日のお越しをお待ちしております。"},{"id":{"d":82,"n":9,"t":3},"speaker":"A","text":"はい、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":83,"n":1,"t":3},"speaker":"A","text":"年末に北海道旅行へ行きます。"},{"id":{"d":83,"n":2,"t":3},"speaker":"B","text":"冬の北海道とは楽しみですね。"},{"id":{"d":83,"n":3,"t":3},"speaker":"A","text":"北海道へ行くことも初めてなので、今から胸が高鳴ります。"},{"id":{"d":83,"n":4,"t":3},"speaker":"B","text":"何が一番の楽しみですか？"},{"id":{"d":83,"n":5,"t":3},"speaker":"A","text":"流氷です！ぜひ見てみたいとずっと思っていました。"},{"id":{"d":83,"n":6,"t":3},"speaker":"B","text":"流氷ですか。寒いので体に気を付けて楽しんできてくださいね。"},{"id":{"d":83,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":84,"n":1,"t":3},"speaker":"A","text":"先日、韓国旅行に行ってきました。これはそのお土産です。"},{"id":{"d":84,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。もしかしてこれは韓国の美容グッズですか？ちょうど欲しかったのでとてもうれしいです。"},{"id":{"d":84,"n":3,"t":3},"speaker":"A","text":"はい。韓国の美容パックと、マスカラやアイシャドウなどの化粧品です。"},{"id":{"d":84,"n":4,"t":3},"speaker":"B","text":"本当にうれしいです。特に美容パックは、先日雑誌で特集をしていたので、使ってみたくて仕方ありませんでした。楽しみです。"},{"id":{"d":84,"n":5,"t":3},"speaker":"A","text":"ぜひ使ってみてください。使った後はお肌がつるつるになるのが分かりますよ。"},{"id":{"d":84,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。使ったら感想をお伝えしますね。"}];
INSERT INTO sentence [{"id":{"d":85,"n":1,"t":3},"speaker":"A","text":"卒業旅行は、イタリアへ行こうと思います。"},{"id":{"d":85,"n":2,"t":3},"speaker":"B","text":"それは楽しそうですね。イタリアのどちらに行かれるご予定ですか？"},{"id":{"d":85,"n":3,"t":3},"speaker":"A","text":"ローマとベネチア、フィレンツェです。先日予約してきました。"},{"id":{"d":85,"n":4,"t":3},"speaker":"B","text":"フィレンツェなら以前行ったことがありますが、食事がとてもおいしかったですよ。"},{"id":{"d":85,"n":5,"t":3},"speaker":"A","text":"フィレンツェはそれが楽しみで行くことにしました。"},{"id":{"d":85,"n":6,"t":3},"speaker":"B","text":"そうでしたか。ローマとベネチアも、史跡と美術館巡りがとても楽しいと聞いたことがあります。"},{"id":{"d":85,"n":7,"t":3},"speaker":"A","text":"はい。実は、大学でイタリア美術を研究していましたので、その勉強も兼ねて行くことにしたのです。"},{"id":{"d":85,"n":8,"t":3},"speaker":"B","text":"それではますます楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":86,"n":1,"t":3},"speaker":"A","text":"今年の冬は、北海道に行きたいと思います。"},{"id":{"d":86,"n":2,"t":3},"speaker":"B","text":"いいですね。冬の北海道は雪景色が美しいでしょうね。"},{"id":{"d":86,"n":3,"t":3},"speaker":"A","text":"はい。まさに美しい雪景色が見たくて北海道へ行くことにしたのです。"},{"id":{"d":86,"n":4,"t":3},"speaker":"B","text":"以前、私も冬に北海道へ行きましたが、ちょうど雪が降り始めた時期で、しんしんと降り積もる雪の美しさにとても感動しました。"},{"id":{"d":86,"n":5,"t":3},"speaker":"A","text":"私が行くのもちょうど降り始める時期の予定なので、ますます楽しみになってきました。"},{"id":{"d":86,"n":6,"t":3},"speaker":"B","text":"ぜひ楽しんでいらしてください。帰られたら、お話を聞かせてくださいね。"},{"id":{"d":86,"n":7,"t":3},"speaker":"A","text":"もちろんです。お土産も楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":87,"n":1,"t":3},"speaker":"A","text":"今度のゴールデンウイークは、出張のついでに、関西で一人旅をしてこようと思います。"},{"id":{"d":87,"n":2,"t":3},"speaker":"B","text":"それは楽しそうです。関西のどちらに行かれるのですか？"},{"id":{"d":87,"n":3,"t":3},"speaker":"A","text":"平日大阪で仕事をした後、京都に移動して、休日は主に京都と滋賀を巡ろうと思っています。"},{"id":{"d":87,"n":4,"t":3},"speaker":"B","text":"楽しそうですね。京都と滋賀は意外と近いのですよね。"},{"id":{"d":87,"n":5,"t":3},"speaker":"A","text":"はい。滋賀は京都と比べてあまり混んでいない上に観光スポットも多いので、穴場の旅行先です。"},{"id":{"d":87,"n":6,"t":3},"speaker":"B","text":"滋賀には琵琶湖もありますし神社仏閣も多いです。現地のおいしい店も多いので、本当に楽しめますよね。"},{"id":{"d":87,"n":7,"t":3},"speaker":"A","text":"そうなのです。京都とはまた違った良さがあり、魅力的です。"}];
INSERT INTO sentence [{"id":{"d":88,"n":1,"t":3},"speaker":"A","text":"フィンランドではどのようにクリスマスを過ごすのですか？"},{"id":{"d":88,"n":2,"t":3},"speaker":"B","text":"ごちそうを作って家族や恋人と過ごします。"},{"id":{"d":88,"n":3,"t":3},"speaker":"A","text":"日本とあまり変わりませんね。"},{"id":{"d":88,"n":4,"t":3},"speaker":"B","text":"そうですね。あとは、アドベントカレンダーやクリスマスサウナなどもあります。"},{"id":{"d":88,"n":5,"t":3},"speaker":"A","text":"アドベントカレンダーとは何ですか？"},{"id":{"d":88,"n":6,"t":3},"speaker":"B","text":"クリスマスまでの日数を数えるためのカレンダーです。日付が書かれた小窓を開けると、中にいろいろな種類のお菓子や化粧品が入っているのです。"},{"id":{"d":88,"n":7,"t":3},"speaker":"A","text":"かわいらしいカレンダーですね。"}];
INSERT INTO sentence [{"id":{"d":89,"n":1,"t":3},"speaker":"A","text":"思い出に残っている機内食はありますか？"},{"id":{"d":89,"n":2,"t":3},"speaker":"B","text":"カナダへ旅行した時の機内食です。"},{"id":{"d":89,"n":3,"t":3},"speaker":"A","text":"どのような機内食だったのですか？"},{"id":{"d":89,"n":4,"t":3},"speaker":"B","text":"シュリンプカクテルと、フルーツがたくさん入った機内食でした。"},{"id":{"d":89,"n":5,"t":3},"speaker":"A","text":"それは豪華ですね。"},{"id":{"d":89,"n":6,"t":3},"speaker":"B","text":"はい。朝食だったのですが、とてもおいしく目にも鮮やかだったので、印象に残っています。"},{"id":{"d":89,"n":7,"t":3},"speaker":"A","text":"私もその機内食を食べてみたいです。"}];
INSERT INTO sentence [{"id":{"d":90,"n":1,"t":3},"speaker":"A","text":"ハロッズベアはどこにあるのでしょうか？"},{"id":{"d":90,"n":2,"t":3},"speaker":"B","text":"地下1階のギフトショップにあるようですよ。"},{"id":{"d":90,"n":3,"t":3},"speaker":"A","text":"地下1階ですか。ここは1階なので、下の階へ降りましょう。"},{"id":{"d":90,"n":4,"t":3},"speaker":"B","text":"はい。地図によると、このエスカレーターを降りてすぐのところにあるそうです。"},{"id":{"d":90,"n":5,"t":3},"speaker":"A","text":"あそこではありませんか？大きなかわいいクマが見えます。"},{"id":{"d":90,"n":6,"t":3},"speaker":"B","text":"そうですね。ハロッズベア以外にもバッグやマグカップも置いてあるようです。"},{"id":{"d":90,"n":7,"t":3},"speaker":"A","text":"お土産で買っていったら喜ばれそうですね。"},{"id":{"d":90,"n":8,"t":3},"speaker":"B","text":"そうですね。喜んでくれるといいですね。"}];
INSERT INTO sentence [{"id":{"d":91,"n":1,"t":3},"speaker":"A","text":"当店では、50ポンド以上購入されたお客さまは免税手続きを行うことができます。"},{"id":{"d":91,"n":2,"t":3},"speaker":"B","text":"どのように手続きをすればよいですか？"},{"id":{"d":91,"n":3,"t":3},"speaker":"A","text":"あちらの機械でチケットを発券し、順番をお待ちください。"},{"id":{"d":91,"n":4,"t":3},"speaker":"B","text":"分かりました。ありがとうございます。"},{"id":{"d":91,"n":5,"t":3},"speaker":"A","text":"この時期は混み合いますので、お時間に余裕をもってお手続きをお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":92,"n":1,"t":3},"speaker":"A","text":"カナダのバンクーバー行きのバスはどこから乗ることができますか？"},{"id":{"d":92,"n":2,"t":3},"speaker":"B","text":"シアトルからバンクーバー行きの長距離バスが出ております。"},{"id":{"d":92,"n":3,"t":3},"speaker":"A","text":"時間はどれくらいかかりますか？"},{"id":{"d":92,"n":4,"t":3},"speaker":"B","text":"約4時間弱でございます。"},{"id":{"d":92,"n":5,"t":3},"speaker":"A","text":"片道の料金を教えていただけますか？"},{"id":{"d":92,"n":6,"t":3},"speaker":"B","text":"20ドルでございます。"},{"id":{"d":92,"n":7,"t":3},"speaker":"A","text":"途中、トイレ休憩はありますか？"},{"id":{"d":92,"n":8,"t":3},"speaker":"B","text":"ございません。車内にトイレが付いておりますのでそちらをご利用ください。なお、乗車中に入国審査がございますので、パスポートをご用意ください。"},{"id":{"d":92,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。陸路での国境越えは初めてなので、とても緊張しています。"},{"id":{"d":92,"n":10,"t":3},"speaker":"B","text":"そうでしたか。カナダへの陸路の旅を、ぜひお楽しみくださいね。"}];
INSERT INTO sentence [{"id":{"d":93,"n":1,"t":3},"speaker":"A","text":"来週、ホストファミリーとバンフへ旅行に行くことになりました。"},{"id":{"d":93,"n":2,"t":3},"speaker":"B","text":"バンフは、自然豊かでとてもきれいな場所ですよね。"},{"id":{"d":93,"n":3,"t":3},"speaker":"A","text":"行かれたことがあるのですか？"},{"id":{"d":93,"n":4,"t":3},"speaker":"B","text":"はい、2年前に家族旅行で行きました。"},{"id":{"d":93,"n":5,"t":3},"speaker":"A","text":"お薦めの場所はありますか？"},{"id":{"d":93,"n":6,"t":3},"speaker":"B","text":"バーミリオン湖です。水面が鏡のようになっていて、山々を鮮やかに映します。とても幻想的ですよ。"},{"id":{"d":93,"n":7,"t":3},"speaker":"A","text":"それはさぞ絶景なのでしょうね。ぜひ訪れてみようと思います。"},{"id":{"d":93,"n":8,"t":3},"speaker":"B","text":"高性能のカメラでなくてもプロのカメラマンのような写真が撮れますよ。もし写真を撮ったら、ぜひ後ほど見せてください。"},{"id":{"d":93,"n":9,"t":3},"speaker":"A","text":"分かりました。いっぱい写真を撮ってきますね。"}];
INSERT INTO sentence [{"id":{"d":94,"n":1,"t":3},"speaker":"A","text":"先月、出張でアメリカに行って驚いたことがありました。"},{"id":{"d":94,"n":2,"t":3},"speaker":"B","text":"何に驚いたのですか？"},{"id":{"d":94,"n":3,"t":3},"speaker":"A","text":"現地のマクドナルドは、日本のマクドナルドとメニューが違うのです。"},{"id":{"d":94,"n":4,"t":3},"speaker":"B","text":"マクドナルドは国によってメニューを変えているので、日本のものとは違いますよね。何に一番驚きましたか？"},{"id":{"d":94,"n":5,"t":3},"speaker":"A","text":"メニューにブリトーがあったことです。"},{"id":{"d":94,"n":6,"t":3},"speaker":"B","text":"確かに、日本のマクドナルドにはありませんね。"},{"id":{"d":94,"n":7,"t":3},"speaker":"A","text":"あとは、飲み物のサイズにも驚きました。日本のSサイズの大きさはなく、一番小さいものは日本のMサイズでした。一番大きいものはなんとバケツのようなサイズで、とても驚きました。"},{"id":{"d":94,"n":8,"t":3},"speaker":"B","text":"それほど大きいサイズがあるのですか！一度バケツサイズのジュースを試してみたいものです。"}];
INSERT INTO sentence [{"id":{"d":95,"n":1,"t":3},"speaker":"A","text":"行ってみたい国はありますか？"},{"id":{"d":95,"n":2,"t":3},"speaker":"B","text":"オーストラリアに行ってみたいです。"},{"id":{"d":95,"n":3,"t":3},"speaker":"A","text":"なぜオーストラリアに行ってみたいのですか？"},{"id":{"d":95,"n":4,"t":3},"speaker":"B","text":"新婚旅行で行く予定だったのですが、諸事情で行けなくなってしまったので、いつかは行ってみたいとずっと思っています。"},{"id":{"d":95,"n":5,"t":3},"speaker":"A","text":"そうでしたか。オーストラリアのどこに行きたいですか？"},{"id":{"d":95,"n":6,"t":3},"speaker":"B","text":"シドニーです。有名な観光地を一通り見て回りたいですね。"},{"id":{"d":95,"n":7,"t":3},"speaker":"A","text":"シドニーに行くとなると日数が必要になりそうですね。"},{"id":{"d":95,"n":8,"t":3},"speaker":"B","text":"はい。退職してからの夢というところでしょうかね。"},{"id":{"d":95,"n":9,"t":3},"speaker":"A","text":"すてきな夢ですね。"},{"id":{"d":95,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":96,"n":1,"t":3},"speaker":"A","text":"明日、サウスモールに買い物に行きませんか？"},{"id":{"d":96,"n":2,"t":3},"speaker":"B","text":"良いですね！"},{"id":{"d":96,"n":3,"t":3},"speaker":"A","text":"サウスモール行きのバスがパシフィック・ルーテラン大学前の停留所から出るので、バス停に10時集合でよいでしょうか？"},{"id":{"d":96,"n":4,"t":3},"speaker":"B","text":"はい、大丈夫です。どのようなお店があるのですか？"},{"id":{"d":96,"n":5,"t":3},"speaker":"A","text":"若者に人気のブランドやおしゃれなお店が多いです。フードコートも充実していていろいろな国の食べ物が食べられますよ。"},{"id":{"d":96,"n":6,"t":3},"speaker":"B","text":"明日がとても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":97,"n":1,"t":3},"speaker":"A","text":"初めて飛行機に乗ったのはいつですか？"},{"id":{"d":97,"n":2,"t":3},"speaker":"B","text":"小学6年生で沖縄に旅行に行った時でしたね。"},{"id":{"d":97,"n":3,"t":3},"speaker":"A","text":"その時、どう感じましたか？"},{"id":{"d":97,"n":4,"t":3},"speaker":"B","text":"このような大きな機体が本当に飛ぶのかと思って、無事に着陸するまで緊張していました。"},{"id":{"d":97,"n":5,"t":3},"speaker":"A","text":"確かに、大きな機体が飛ぶのは不思議ですね。"},{"id":{"d":97,"n":6,"t":3},"speaker":"B","text":"それから、気圧で耳がおかしくなってしまったので、それが不快でした。"},{"id":{"d":97,"n":7,"t":3},"speaker":"A","text":"耳抜きはされましたか？"},{"id":{"d":97,"n":8,"t":3},"speaker":"B","text":"はい。何度も耳抜きをしてしまい、そのせいで余計耳がおかしくなりました。"},{"id":{"d":97,"n":9,"t":3},"speaker":"A","text":"それは大変でしたね。"},{"id":{"d":97,"n":10,"t":3},"speaker":"B","text":"ええ。でも、それはそれで良い思い出になりました。"}];
INSERT INTO sentence [{"id":{"d":98,"n":1,"t":3},"speaker":"A","text":"電車の旅をしたことはありますか？"},{"id":{"d":98,"n":2,"t":3},"speaker":"B","text":"はい。学生の頃、電車を乗り継いで長野へ旅行に行きました。"},{"id":{"d":98,"n":3,"t":3},"speaker":"A","text":"どのような旅でしたか？"},{"id":{"d":98,"n":4,"t":3},"speaker":"B","text":"友達10人くらいで川沿いのバンガローに泊まって、バーベキューや川遊びをしました。"},{"id":{"d":98,"n":5,"t":3},"speaker":"A","text":"楽しそうですね。"},{"id":{"d":98,"n":6,"t":3},"speaker":"B","text":"とても楽しかったです。しかし、夜に大雨になってしまって花火ができなかったのは残念でした。"},{"id":{"d":98,"n":7,"t":3},"speaker":"A","text":"それもまた思い出になりましたね。"}];
INSERT INTO sentence [{"id":{"d":99,"n":1,"t":3},"speaker":"A","text":"このあたりに古着屋はありますか？"},{"id":{"d":99,"n":2,"t":3},"speaker":"B","text":"人気の古着屋がイーストステーションにあります。"},{"id":{"d":99,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。イギリスにはセンスの良い古着屋が多くあると聞いたのです。"},{"id":{"d":99,"n":4,"t":3},"speaker":"B","text":"確かに古着屋はたくさんありますよ。あなたと同じように尋ねる方も多いです。"},{"id":{"d":99,"n":5,"t":3},"speaker":"A","text":"そうなのですね。お店に行くのがとても楽しみです。"},{"id":{"d":99,"n":6,"t":3},"speaker":"B","text":"良い服が見つかるといいですね。"},{"id":{"d":99,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":100,"n":1,"t":3},"speaker":"A","text":"せっかく旅行に来たのですから、何かおいしいものが食べたいです。"},{"id":{"d":100,"n":2,"t":3},"speaker":"B","text":"どのお店が良いでしょうか。ガイドブックに載っているお店はどれも高級そうなお店ですね。注文の仕方も分かりませんので心配です。"},{"id":{"d":100,"n":3,"t":3},"speaker":"A","text":"しかし、日本にもあるようなファストフードばかりを食べていると、旅行の気分を味わうことができません。"},{"id":{"d":100,"n":4,"t":3},"speaker":"B","text":"そうですね。いろいろ考えると入りにくくなってしまいそうなので、とりあえず、あそこのお店に行ってみませんか？"},{"id":{"d":100,"n":5,"t":3},"speaker":"A","text":"そうしましょう。旅の恥はかき捨てという言葉もありますし、挑戦してみましょう。"},{"id":{"d":100,"n":6,"t":3},"speaker":"B","text":"はい。行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":101,"n":1,"t":3},"speaker":"A","text":"お電話ありがとうございます。一柳閣旅館です。"},{"id":{"d":101,"n":2,"t":3},"speaker":"B","text":"鈴木と申しますが、12月18日に大人2名で1泊2日の予約をお願いできますか？"},{"id":{"d":101,"n":3,"t":3},"speaker":"A","text":"かしこまりました。12月18日ですね。ただ今空室情報をお調べいたしますので、少しお待ちください。"},{"id":{"d":101,"n":4,"t":3},"speaker":"B","text":"はい。お願いします。"},{"id":{"d":101,"n":5,"t":3},"speaker":"A","text":"お待たせいたしました。12月18日は空きがございます。"},{"id":{"d":101,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。よかったです。その日は妻の誕生日なので、記念に旅行をプレゼントしようと思いまして。"},{"id":{"d":101,"n":7,"t":3},"speaker":"A","text":"そうでしたか。それはおめでとうございます。それでは、よろしければ食後のデザートにバースデーケーキをご用意いたします。奥さまのお名前を教えていただけますか？"},{"id":{"d":101,"n":8,"t":3},"speaker":"B","text":"よいのですか？それでは、ぜひお願いします。妻の名前は理恵です。"},{"id":{"d":101,"n":9,"t":3},"speaker":"A","text":"かしこまりました。それでは、12月18日大人2名さまでご予約いたします。お気を付けてお越しになってください。すてきなお誕生日になりますよう、スタッフ一同、心を込めて準備させていただきます。"},{"id":{"d":101,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":102,"n":1,"t":3},"speaker":"A","text":"あなたが旅行へ行く時に、必ず持っていくものはありますか？"},{"id":{"d":102,"n":2,"t":3},"speaker":"B","text":"私の必需品はカメラです。"},{"id":{"d":102,"n":3,"t":3},"speaker":"A","text":"カメラがお好きなのですか？"},{"id":{"d":102,"n":4,"t":3},"speaker":"B","text":"はい。趣味の一つです。"},{"id":{"d":102,"n":5,"t":3},"speaker":"A","text":"旅先ではどういったものを撮るのですか？"},{"id":{"d":102,"n":6,"t":3},"speaker":"B","text":"旅先で出会った人の笑顔の写真を撮るのが好きです。"},{"id":{"d":102,"n":7,"t":3},"speaker":"A","text":"すてきですね。今度見せていただけますか？"},{"id":{"d":102,"n":8,"t":3},"speaker":"B","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":103,"n":1,"t":3},"speaker":"A","text":"キャンプに興味はありますか？"},{"id":{"d":103,"n":2,"t":3},"speaker":"B","text":"はい。最近興味があるのですが、何から用意すればよいのか分かりません。"},{"id":{"d":103,"n":3,"t":3},"speaker":"A","text":"そうですか。実は私は最近キャンプを始めたのです。"},{"id":{"d":103,"n":4,"t":3},"speaker":"B","text":"初めは何を準備したのですか？"},{"id":{"d":103,"n":5,"t":3},"speaker":"A","text":"私はテントと寝袋を最初に準備しました。"},{"id":{"d":103,"n":6,"t":3},"speaker":"B","text":"それらはどこで購入したのですか？"},{"id":{"d":103,"n":7,"t":3},"speaker":"A","text":"インターネットで安く購入しましたが、キャンプ場によっては貸し出しをしている所もあるので、調べてみるといいかと思いますよ。"},{"id":{"d":103,"n":8,"t":3},"speaker":"B","text":"そうですか。調べてみます。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":104,"n":1,"t":3},"speaker":"A","text":"連休は何をされる予定ですか？"},{"id":{"d":104,"n":2,"t":3},"speaker":"B","text":"家族で静岡でのキャンプを予定しています。"},{"id":{"d":104,"n":3,"t":3},"speaker":"A","text":"何泊の予定なのですか？"},{"id":{"d":104,"n":4,"t":3},"speaker":"B","text":"3泊の予定です。"},{"id":{"d":104,"n":5,"t":3},"speaker":"A","text":"同じキャンプ場に3泊されるのですか？"},{"id":{"d":104,"n":6,"t":3},"speaker":"B","text":"いいえ。1カ所は2日間しか予約が取れなかったので、最終日は別のキャンプ場に泊まる予定です。"},{"id":{"d":104,"n":7,"t":3},"speaker":"A","text":"1カ所目と2カ所目のキャンプ場は近いのですか？"},{"id":{"d":104,"n":8,"t":3},"speaker":"B","text":"いえ、違った風景を楽しみたいので少し離れた場所にしました。"},{"id":{"d":104,"n":9,"t":3},"speaker":"A","text":"そうですか。楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":105,"n":1,"t":3},"speaker":"A","text":"来週、子どもが修学旅行に行く予定です。"},{"id":{"d":105,"n":2,"t":3},"speaker":"B","text":"どこに行かれるのですか？"},{"id":{"d":105,"n":3,"t":3},"speaker":"A","text":"福島です。"},{"id":{"d":105,"n":4,"t":3},"speaker":"B","text":"修学旅行の行き先として最も人気のある京都ではなく、福島なのですね。"},{"id":{"d":105,"n":5,"t":3},"speaker":"A","text":"はい。修学旅行に行けること自体が楽しみのようで、とても喜んでいます。"},{"id":{"d":105,"n":6,"t":3},"speaker":"B","text":"そうですか。楽しい思い出が増えるといいですね。"},{"id":{"d":105,"n":7,"t":3},"speaker":"A","text":"ええ。たくさん楽しんできてほしいです。"}];
INSERT INTO sentence [{"id":{"d":106,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。何名さまですか？"},{"id":{"d":106,"n":2,"t":3},"speaker":"B","text":"2名です。窓側の席は空いていますか？"},{"id":{"d":106,"n":3,"t":3},"speaker":"A","text":"申し訳ございません。窓側のお席はご予約で埋まっております。窓から近いお席でよろしければご案内できます。"},{"id":{"d":106,"n":4,"t":3},"speaker":"B","text":"ではそれでお願いします。"},{"id":{"d":106,"n":5,"t":3},"speaker":"A","text":"かしこまりました。ご案内いたします。"}];
INSERT INTO sentence [{"id":{"d":107,"n":1,"t":3},"speaker":"A","text":"ディナーショーのチケットを大人2枚ください。"},{"id":{"d":107,"n":2,"t":3},"speaker":"B","text":"ご希望のお席はございますか？"},{"id":{"d":107,"n":3,"t":3},"speaker":"A","text":"前方、通路側でお願いします。"},{"id":{"d":107,"n":4,"t":3},"speaker":"B","text":"前方、通路側でございますね。かしこまりました。1万8,000円頂戴いたします。"},{"id":{"d":107,"n":5,"t":3},"speaker":"A","text":"クレジットカードは使えますか？"},{"id":{"d":107,"n":6,"t":3},"speaker":"B","text":"はい。ご利用いただけます。"},{"id":{"d":107,"n":7,"t":3},"speaker":"A","text":"では一括払いでお願いします。"},{"id":{"d":107,"n":8,"t":3},"speaker":"B","text":"かしこまりました。暗証番号のご入力をお願いいたします。"},{"id":{"d":107,"n":9,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":107,"n":10,"t":3},"speaker":"B","text":"クレジットカードと領収書をお返しいたします。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":108,"n":1,"t":3},"speaker":"A","text":"商品の交換をお願いできますか？"},{"id":{"d":108,"n":2,"t":3},"speaker":"B","text":"どうなさいましたか？"},{"id":{"d":108,"n":3,"t":3},"speaker":"A","text":"こちらのTシャツなのですが、サイズを間違って購入してしまいました。"},{"id":{"d":108,"n":4,"t":3},"speaker":"B","text":"かしこまりました。どのサイズに変更なさいますか？"},{"id":{"d":108,"n":5,"t":3},"speaker":"A","text":"Mサイズでお願いします。"},{"id":{"d":108,"n":6,"t":3},"speaker":"B","text":"レシートはお持ちですか？"},{"id":{"d":108,"n":7,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":108,"n":8,"t":3},"speaker":"B","text":"お預かりいたします。こちらがMサイズのTシャツでございます。ご確認くださいませ。"},{"id":{"d":108,"n":9,"t":3},"speaker":"A","text":"はい。大丈夫です。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":109,"n":1,"t":3},"speaker":"A","text":"ご注文はお決まりですか？"},{"id":{"d":109,"n":2,"t":3},"speaker":"B","text":"はい。オムライスとハンバーグをお願いします。"},{"id":{"d":109,"n":3,"t":3},"speaker":"A","text":"ご一緒にお飲み物はいかがですか？"},{"id":{"d":109,"n":4,"t":3},"speaker":"B","text":"オレンジジュースを2つお願いします。"},{"id":{"d":109,"n":5,"t":3},"speaker":"A","text":"お飲み物はいつお持ちいたしますか？"},{"id":{"d":109,"n":6,"t":3},"speaker":"B","text":"食後でお願いします。"},{"id":{"d":109,"n":7,"t":3},"speaker":"A","text":"かしこまりました。ご注文を繰り返します。オムライスが1つ、ハンバーグが1つ、食後にオレンジジュースが2つ、以上でお間違えありませんか？"},{"id":{"d":109,"n":8,"t":3},"speaker":"B","text":"はい。大丈夫です。"},{"id":{"d":109,"n":9,"t":3},"speaker":"A","text":"それではお食事からお持ちいたしますのでお待ちください。ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":110,"n":1,"t":3},"speaker":"A","text":"すみません。切符の買い方が分からないので、教えていただけますか？"},{"id":{"d":110,"n":2,"t":3},"speaker":"B","text":"はい。どちらへのお出かけでしょうか？"},{"id":{"d":110,"n":3,"t":3},"speaker":"A","text":"東京駅まで行きたいです。"},{"id":{"d":110,"n":4,"t":3},"speaker":"B","text":"かしこまりました。こちらの販売機で東京駅を選択し人数をご入力ください。そうしましたら画面に料金が表示されますのでこちらにお金を投入してください。"},{"id":{"d":110,"n":5,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":110,"n":6,"t":3},"speaker":"B","text":"お気を付けて行ってらっしゃいませ。"}];
INSERT INTO sentence [{"id":{"d":111,"n":1,"t":3},"speaker":"A","text":"予約をしていませんが、部屋は空いていますか？"},{"id":{"d":111,"n":2,"t":3},"speaker":"B","text":"はい、空いております。何名さまで何泊でしょうか？"},{"id":{"d":111,"n":3,"t":3},"speaker":"A","text":"大人2名、2泊です。"},{"id":{"d":111,"n":4,"t":3},"speaker":"B","text":"ダブルとツインのお部屋が空いておりますが、どちらがよろしいでしょうか？"},{"id":{"d":111,"n":5,"t":3},"speaker":"A","text":"料金に違いはありますか？"},{"id":{"d":111,"n":6,"t":3},"speaker":"B","text":"ダブルのお部屋は2名さまで2万1,000円、ツインのお部屋は2名さまで2万8,000円でございます。"},{"id":{"d":111,"n":7,"t":3},"speaker":"A","text":"ではツインの部屋でお願いします。"},{"id":{"d":111,"n":8,"t":3},"speaker":"B","text":"かしこまりました。2名さまでツインのお部屋を2泊のご利用でございますね。202号室をご用意いたしました。こちらがお部屋の鍵でございます。エレベーターで2階まで上がっていただいて、すぐ右のお部屋でございます。"},{"id":{"d":111,"n":9,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":111,"n":10,"t":3},"speaker":"B","text":"ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":112,"n":1,"t":3},"speaker":"A","text":"先日のテレビで聖地巡礼に関する特集が放映されていて、今度私もぜひしてみたいと思いました。"},{"id":{"d":112,"n":2,"t":3},"speaker":"B","text":"いいですね。どこに行きたいですか？"},{"id":{"d":112,"n":3,"t":3},"speaker":"A","text":"フランスが舞台の作品が好きなので、それに関連する場所に行きたいと思います。オペラ座やノートルダム寺院やモンマルトルなどが特に好きです。"},{"id":{"d":112,"n":4,"t":3},"speaker":"B","text":"なるほど、実現できるといいですね。人生は一度なので、思いきり楽しんだ方がいいですよ。"},{"id":{"d":112,"n":5,"t":3},"speaker":"A","text":"そうですね。ありがとうございます。まずは計画を立ててみます。"}];
INSERT INTO sentence [{"id":{"d":113,"n":1,"t":3},"speaker":"A","text":"私は飛行機の離陸の瞬間が好きです。"},{"id":{"d":113,"n":2,"t":3},"speaker":"B","text":"どうして離陸の瞬間がお好きなのですか？"},{"id":{"d":113,"n":3,"t":3},"speaker":"A","text":"住み慣れた地を離れてこれからどのようなことが待っているかと想像するのが、とてもワクワクするのです。それから、機内で映画を見たり機内食を食べたりするのも好きです。鈴木さんは離陸と着陸のどちらがお好きですか？"},{"id":{"d":113,"n":4,"t":3},"speaker":"B","text":"私は着陸の瞬間が好きです。窓の景色を見ながら知らない土地がどんどん近づいてくる様子に、ワクワクします。長時間のフライトから解放される感じも好きです。"},{"id":{"d":113,"n":5,"t":3},"speaker":"A","text":"分かります。立ち上がると足がしびれてうまく歩けないのですよね。そのようなところも楽しいです。"},{"id":{"d":113,"n":6,"t":3},"speaker":"B","text":"はい。飛行機の離陸の瞬間、着陸の瞬間ともに、甲乙つけがたい良さがあるということですね。"},{"id":{"d":113,"n":7,"t":3},"speaker":"A","text":"その通りですね。"}];
INSERT INTO sentence [{"id":{"d":114,"n":1,"t":3},"speaker":"A","text":"スイス旅行に行きたいのですが、お薦めの観光地はありますか？"},{"id":{"d":114,"n":2,"t":3},"speaker":"B","text":"マイエンフェルトはどうですか？自然豊かな田舎町でハイジの家という名のついた博物館もあります。"},{"id":{"d":114,"n":3,"t":3},"speaker":"A","text":"どうやって行くのですか？"},{"id":{"d":114,"n":4,"t":3},"speaker":"B","text":"チューリヒから鉄道で90分程です。"}];
INSERT INTO sentence [{"id":{"d":115,"n":1,"t":3},"speaker":"A","text":"私の一番の思い出は日本一周をしたことです。"},{"id":{"d":115,"n":2,"t":3},"speaker":"B","text":"素晴らしいですね。どこから出発したのですか？"},{"id":{"d":115,"n":3,"t":3},"speaker":"A","text":"青森から出発して青森へ帰りました。"},{"id":{"d":115,"n":4,"t":3},"speaker":"B","text":"何日くらいかかったのですか？"},{"id":{"d":115,"n":5,"t":3},"speaker":"A","text":"バイクで1年かけて旅しました。"},{"id":{"d":115,"n":6,"t":3},"speaker":"B","text":"寝る所やお金はどうされたのですか？"},{"id":{"d":115,"n":7,"t":3},"speaker":"A","text":"テントを立てたり現地で知り合った人の家に泊めてもらったりしました。お金はその土地でアルバイトをしました。"},{"id":{"d":115,"n":8,"t":3},"speaker":"B","text":"それは素晴らしい体験ができましたね。"},{"id":{"d":115,"n":9,"t":3},"speaker":"A","text":"はい、自分自身を成長させてくれた素晴らしい旅でした。"}];
INSERT INTO sentence [{"id":{"d":116,"n":1,"t":3},"speaker":"A","text":"宿泊の予約をしたいのですが。"},{"id":{"d":116,"n":2,"t":3},"speaker":"B","text":"かしこまりました。いつになさいますか？"},{"id":{"d":116,"n":3,"t":3},"speaker":"A","text":"1月10日から13日までの4日間でお願いします。"},{"id":{"d":116,"n":4,"t":3},"speaker":"B","text":"かしこまりました。何名さまのご利用でしょうか？"},{"id":{"d":116,"n":5,"t":3},"speaker":"A","text":"1人です。"},{"id":{"d":116,"n":6,"t":3},"speaker":"B","text":"お1人さまのご利用ですと、宿泊代金は2名さま分頂戴いたしますがよろしいでしょうか？"},{"id":{"d":116,"n":7,"t":3},"speaker":"A","text":"構いません。"},{"id":{"d":116,"n":8,"t":3},"speaker":"B","text":"かしこまりました。1月10日から13日までの4日間でお取りいたします。当日はご本人さま確認証をお忘れなくお持ちください。何かご不明な点はございますか？"},{"id":{"d":116,"n":9,"t":3},"speaker":"A","text":"大丈夫です。"},{"id":{"d":116,"n":10,"t":3},"speaker":"B","text":"では当日お待ちしております。お気を付けてお越しくださいませ。"}];
INSERT INTO sentence [{"id":{"d":117,"n":1,"t":3},"speaker":"A","text":"私は羽田空港の雰囲気が好きです。"},{"id":{"d":117,"n":2,"t":3},"speaker":"B","text":"なぜですか？"},{"id":{"d":117,"n":3,"t":3},"speaker":"A","text":"いろいろな人がいるのが面白いですし、これから旅先に飛び立つことに高揚している人々を見ていると自分も同じ気持ちになれるからです。"},{"id":{"d":117,"n":4,"t":3},"speaker":"B","text":"なるほど。空港の中にはお店もたくさんあるので、お店を見て歩くだけでも楽しいですよね。"},{"id":{"d":117,"n":5,"t":3},"speaker":"A","text":"私もそう思います。空港限定の商品や地方のお土産などを買うことができるのも良いですね。"},{"id":{"d":117,"n":6,"t":3},"speaker":"B","text":"そうですね。飛行機を見たり、買い物をしたり、食事をしたりと、楽しい1日を過ごすことができますね。"}];
INSERT INTO sentence [{"id":{"d":118,"n":1,"t":3},"speaker":"A","text":"お土産用の袋はお付けいたしますか？"},{"id":{"d":118,"n":2,"t":3},"speaker":"B","text":"はい。2枚お願いします。"},{"id":{"d":118,"n":3,"t":3},"speaker":"A","text":"かしこまりました。紙袋とビニール袋どちらがよろしいでしょうか？"},{"id":{"d":118,"n":4,"t":3},"speaker":"B","text":"ビニール袋でお願いします。"},{"id":{"d":118,"n":5,"t":3},"speaker":"A","text":"かしこまりました。ではこちらの袋にまとめてお入れいたします。"},{"id":{"d":118,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":118,"n":7,"t":3},"speaker":"A","text":"お買い上げありがとうございました。またお越しくださいませ。"}];
INSERT INTO sentence [{"id":{"d":119,"n":1,"t":3},"speaker":"A","text":"この前イルミネーションを見に行きました。"},{"id":{"d":119,"n":2,"t":3},"speaker":"B","text":"どこに誰と行かれたのですか？"},{"id":{"d":119,"n":3,"t":3},"speaker":"A","text":"1人で東武ワールドスクウェアに行きました。クリスマスシーズンだったので、家族連れやカップルが多かったです。"},{"id":{"d":119,"n":4,"t":3},"speaker":"B","text":"そうでしたか。東武ワールドスクウェアへは何で行かれたのですか？"},{"id":{"d":119,"n":5,"t":3},"speaker":"A","text":"電車とバスで行きました。帰りはバスがなくなってしまったので、駅まで30分程歩きました。"},{"id":{"d":119,"n":6,"t":3},"speaker":"B","text":"それは大変でしたね。暗くて寒かったのではありませんか？"},{"id":{"d":119,"n":7,"t":3},"speaker":"A","text":"はい。慣れない道というのもあって少し大変でしたが、それを踏まえても楽しかったです。"},{"id":{"d":119,"n":8,"t":3},"speaker":"B","text":"そうですか。それなら良かったですね。"}];
INSERT INTO sentence [{"id":{"d":120,"n":1,"t":3},"speaker":"A","text":"ロンドンへ行ったのですが、大英博物館が特に素晴らしかったです。"},{"id":{"d":120,"n":2,"t":3},"speaker":"B","text":"どのように素晴らしかったのですか？"},{"id":{"d":120,"n":3,"t":3},"speaker":"A","text":"まずイギリスの博物館自体が無料なのですが、そこが素晴らしいと思いました。"},{"id":{"d":120,"n":4,"t":3},"speaker":"B","text":"無料で入れるのですか？それは驚きです。"},{"id":{"d":120,"n":5,"t":3},"speaker":"A","text":"それから、800万点もの作品が所蔵されていて、アフリカやエジプトなど世界中の作品を見ることができました。"},{"id":{"d":120,"n":6,"t":3},"speaker":"B","text":"そこまでたくさんの作品が所蔵されている所はなかなかありませんね。"},{"id":{"d":120,"n":7,"t":3},"speaker":"A","text":"はい。私が知る限り、所蔵数は世界一だと思います。"}];
INSERT INTO sentence [{"id":{"d":121,"n":1,"t":3},"speaker":"A","text":"先週末はゴッホ展に行きました。"},{"id":{"d":121,"n":2,"t":3},"speaker":"B","text":"どこで開催していたのですか？"},{"id":{"d":121,"n":3,"t":3},"speaker":"A","text":"東京都美術館でした。どの作品も素晴らしかったです。"},{"id":{"d":121,"n":4,"t":3},"speaker":"B","text":"その他にはどこか行きましたか？"},{"id":{"d":121,"n":5,"t":3},"speaker":"A","text":"秋葉原を観光して職場へのお土産を買いました。"},{"id":{"d":121,"n":6,"t":3},"speaker":"B","text":"お土産は何を買われたのですか？"},{"id":{"d":121,"n":7,"t":3},"speaker":"A","text":"クッキーを買いました。職場のみんなが喜んでくれたのでうれしかったです。"},{"id":{"d":121,"n":8,"t":3},"speaker":"B","text":"良い休日を過ごされたようで、よかったですね。"}];
INSERT INTO sentence [{"id":{"d":122,"n":1,"t":3},"speaker":"A","text":"栃木県で有名な場所はありますか？今度、観光へ行こうと思っているのです。"},{"id":{"d":122,"n":2,"t":3},"speaker":"B","text":"日光東照宮はいかがですか？装飾にもそれぞれ意味があるので、それを知ると面白いですよ。"},{"id":{"d":122,"n":3,"t":3},"speaker":"A","text":"三猿なら知っています。悪いことは見ない、聞かない、しゃべらないという意味が込められていますね。"},{"id":{"d":122,"n":4,"t":3},"speaker":"B","text":"よくご存じですね。実は4匹いたなどの説もあるのですよ。"},{"id":{"d":122,"n":5,"t":3},"speaker":"A","text":"それは初めて知りました。その他にも見どころはあるのですか？"},{"id":{"d":122,"n":6,"t":3},"speaker":"B","text":"日光東照宮には、眠り猫という有名な彫刻作品もあります。平和を象徴していると言われています。"},{"id":{"d":122,"n":7,"t":3},"speaker":"A","text":"そうなのですね。私はネコが好きなので、今度注意して見てみます。"},{"id":{"d":122,"n":8,"t":3},"speaker":"B","text":"ぜひ、そうしてください。"}];
INSERT INTO sentence [{"id":{"d":123,"n":1,"t":3},"speaker":"A","text":"先月出張で関西へ行ったのですが、その時に初めて、本場大阪でお笑いを見ました。"},{"id":{"d":123,"n":2,"t":3},"speaker":"B","text":"どうでしたか？"},{"id":{"d":123,"n":3,"t":3},"speaker":"A","text":"面白かったです。エチュードといって即興でネタを披露するという舞台でした。演劇でしかエチュードを知らなかったので驚きました。"},{"id":{"d":123,"n":4,"t":3},"speaker":"B","text":"そのようなものがあるのですか。初めて知りました。"},{"id":{"d":123,"n":5,"t":3},"speaker":"A","text":"充実した時間を過ごすことができました。"}];
INSERT INTO sentence [{"id":{"d":124,"n":1,"t":3},"speaker":"A","text":"今度の夏休み、どこかに行きたいと思うのですが、同時にネコとたくさん戯れたいと思っています。どこかお薦めの場所はありますか？"},{"id":{"d":124,"n":2,"t":3},"speaker":"B","text":"それでしたら田代島はいかがですか？人口よりネコの方が多いみたいですよ。"},{"id":{"d":124,"n":3,"t":3},"speaker":"A","text":"田代島はどこにあるのですか？"},{"id":{"d":124,"n":4,"t":3},"speaker":"B","text":"宮城県石巻市にあります。『ひょっこりひょうたん島』のモデルになった島ともいわれています。"},{"id":{"d":124,"n":5,"t":3},"speaker":"A","text":"それは初めて聞きました。ぜひ行ってみたいです。"},{"id":{"d":124,"n":6,"t":3},"speaker":"B","text":"漁業や観光業も盛んなので楽しめると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":125,"n":1,"t":3},"speaker":"A","text":"アメニティーはどのようなものがありますか？"},{"id":{"d":125,"n":2,"t":3},"speaker":"B","text":"シャンプーやボディーソープなどの洗剤類や、歯ブラシ、かみそりがございます。"},{"id":{"d":125,"n":3,"t":3},"speaker":"A","text":"爪切りはありますか？"},{"id":{"d":125,"n":4,"t":3},"speaker":"B","text":"爪切りはフロントで貸し出しております。"},{"id":{"d":125,"n":5,"t":3},"speaker":"A","text":"1つ貸してください。"},{"id":{"d":125,"n":6,"t":3},"speaker":"B","text":"かしこまりました。こちらにお名前のご記入をお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":126,"n":1,"t":3},"speaker":"A","text":"こちらの劇場ではオペラグラスは借りられますか？"},{"id":{"d":126,"n":2,"t":3},"speaker":"B","text":"はい。こちらをどうぞ。見え方の確認をお願いいたします。"},{"id":{"d":126,"n":3,"t":3},"speaker":"A","text":"はい。問題なく見えます。"},{"id":{"d":126,"n":4,"t":3},"speaker":"B","text":"ではレンタル料5,500円を頂戴いたします。"},{"id":{"d":126,"n":5,"t":3},"speaker":"A","text":"クレジットカードは使えますか？"},{"id":{"d":126,"n":6,"t":3},"speaker":"B","text":"申し訳ございません。お支払い方法は現金のみでございます。"},{"id":{"d":126,"n":7,"t":3},"speaker":"A","text":"分かりました。では1万円からお願いします。"},{"id":{"d":126,"n":8,"t":3},"speaker":"B","text":"1万円のお預かりで4,500円お返しいたします。こちらの5,000円分は保証金になりますので、オペラグラスをご返却いただいた際にお返しいたします。"},{"id":{"d":126,"n":9,"t":3},"speaker":"A","text":"分かりました。"},{"id":{"d":126,"n":10,"t":3},"speaker":"B","text":"ごゆっくりどうぞ。"}];
INSERT INTO sentence [{"id":{"d":127,"n":1,"t":3},"speaker":"A","text":"こちらのホテルに日本人スタッフはいますか？"},{"id":{"d":127,"n":2,"t":3},"speaker":"B","text":"申し訳ございません。日本人スタッフはおりませんが、日本語で応対ができるスタッフがおります。"},{"id":{"d":127,"n":3,"t":3},"speaker":"A","text":"そのスタッフさんは常にいらっしゃいますか？"},{"id":{"d":127,"n":4,"t":3},"speaker":"B","text":"はい。日本語で応対ができるスタッフは20人以上おりますので、基本的には大丈夫です。"},{"id":{"d":127,"n":5,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":128,"n":1,"t":3},"speaker":"A","text":"タクシーに乗るのが大好きなので、世界中のタクシーに乗ってみたいです。"},{"id":{"d":128,"n":2,"t":3},"speaker":"B","text":"楽しそうですね。今まで乗ったタクシーの中でどのようなタクシーが良かったですか？"},{"id":{"d":128,"n":3,"t":3},"speaker":"A","text":"イギリスで乗ったタクシーです。ロールスロイスで運転も丁寧で、住所を言ったらすぐに理解して運転してくれました。"},{"id":{"d":128,"n":4,"t":3},"speaker":"B","text":"それは素晴らしいですね。"},{"id":{"d":128,"n":5,"t":3},"speaker":"A","text":"はい。運転手さんが気さくだったので、道中お話しするのも楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":129,"n":1,"t":3},"speaker":"A","text":"私は遺跡を巡るのが好きです。加藤さんは遺跡に興味はありますか？"},{"id":{"d":129,"n":2,"t":3},"speaker":"B","text":"私も好きですよ。アンコールワットは特に印象に残っています。"},{"id":{"d":129,"n":3,"t":3},"speaker":"A","text":"アンコールワットは良いですよね。早朝に行くと本当にきれいです。"},{"id":{"d":129,"n":4,"t":3},"speaker":"B","text":"そうですね。水に映る逆さアンコールワットも魅力的ですよね。"},{"id":{"d":129,"n":5,"t":3},"speaker":"A","text":"はい。とても感動しました。"}];
INSERT INTO sentence [{"id":{"d":130,"n":1,"t":3},"speaker":"A","text":"この前、初めて富士山の頂上まで登りました。"},{"id":{"d":130,"n":2,"t":3},"speaker":"B","text":"それは素晴らしいですね。つらくはなかったですか？"},{"id":{"d":130,"n":3,"t":3},"speaker":"A","text":"何度もくじけそうになりましたが、仲間たちと励まし合い何とか登ることができました。"},{"id":{"d":130,"n":4,"t":3},"speaker":"B","text":"そうですか。仲間がいると心強いですね。景色はどうでしたか？"},{"id":{"d":130,"n":5,"t":3},"speaker":"A","text":"御来光がとてもきれいで、感動して疲れが吹き飛びました。"},{"id":{"d":130,"n":6,"t":3},"speaker":"B","text":"なかなかできない貴重な体験ですね。"}];
INSERT INTO sentence [{"id":{"d":131,"n":1,"t":3},"speaker":"A","text":"旦那さんとはどこで知り合いましたか？"},{"id":{"d":131,"n":2,"t":3},"speaker":"B","text":"団体旅行で岐阜県に行った時、バスでたまたま隣の席でした。"},{"id":{"d":131,"n":3,"t":3},"speaker":"A","text":"そうですか。どうやって意気投合したのですか？"},{"id":{"d":131,"n":4,"t":3},"speaker":"B","text":"お互い旅行が趣味だったのですぐ打ち解けました。"},{"id":{"d":131,"n":5,"t":3},"speaker":"A","text":"住まいは近かったのですか？"},{"id":{"d":131,"n":6,"t":3},"speaker":"B","text":"私は埼玉、彼は長野でした。お互い旅行が趣味だったので、週末には会っていろいろな所へ出かけていました。"},{"id":{"d":131,"n":7,"t":3},"speaker":"A","text":"そうですか。すてきなお話をありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":132,"n":1,"t":3},"speaker":"A","text":"紅葉の季節ですね。"},{"id":{"d":132,"n":2,"t":3},"speaker":"B","text":"そうですね。那須の紅葉はとても素晴らしいですよ。"},{"id":{"d":132,"n":3,"t":3},"speaker":"A","text":"行ってみたいものです。那須は自然豊かですよね。"},{"id":{"d":132,"n":4,"t":3},"speaker":"B","text":"はい。外観も色が統一されていて落ち着きます。今度ぜひ行ってみてください。"},{"id":{"d":132,"n":5,"t":3},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":133,"n":1,"t":3},"speaker":"A","text":"サグラダファミリアはいつ完成するのでしょうか？"},{"id":{"d":133,"n":2,"t":3},"speaker":"B","text":"2026年には完成すると言われていますね。"},{"id":{"d":133,"n":3,"t":3},"speaker":"A","text":"2026年ですか。長いようで短いですね。"},{"id":{"d":133,"n":4,"t":3},"speaker":"B","text":"そうですね。現在の工事中の姿も貴重ですが、次はぜひ完成形を見たいです。"},{"id":{"d":133,"n":5,"t":3},"speaker":"A","text":"そうですね。2026年にまた来られるといいですね。"},{"id":{"d":133,"n":6,"t":3},"speaker":"B","text":"はい。ぜひまた来ましょう！"}];
INSERT INTO sentence [{"id":{"d":134,"n":1,"t":3},"speaker":"A","text":"中華街は世界中にありますね。"},{"id":{"d":134,"n":2,"t":3},"speaker":"B","text":"そうですね。海外出張の際にはよくその土地の中華街へ行きますが、私は日本の中華街が一番好きです。"},{"id":{"d":134,"n":3,"t":3},"speaker":"A","text":"なぜですか？"},{"id":{"d":134,"n":4,"t":3},"speaker":"B","text":"やはり日本人好みの味付けになっているからか、私は一番おいしく感じます。食べ歩きも楽しいです。"},{"id":{"d":134,"n":5,"t":3},"speaker":"A","text":"なるほど、そう言われてみればそのような気がします。"}];
INSERT INTO sentence [{"id":{"d":135,"n":1,"t":3},"speaker":"A","text":"ロシアは何が有名ですか？"},{"id":{"d":135,"n":2,"t":3},"speaker":"B","text":"食べ物でしたらピロシキ、ボルシチなどが有名です。"},{"id":{"d":135,"n":3,"t":3},"speaker":"A","text":"ピロシキは日本で食べたことがあります。本場でも食べてみたいです。建物は何がありますか？"},{"id":{"d":135,"n":4,"t":3},"speaker":"B","text":"ロシアの建造物は独特で面白いですよ。聖ワシリー寺院や血の上の教会などがあります。"},{"id":{"d":135,"n":5,"t":3},"speaker":"A","text":"血の上の教会ですか？恐ろしい名前ですね。"},{"id":{"d":135,"n":6,"t":3},"speaker":"B","text":"名前は恐ろしいですがとてもかわいらしい建物ですよ。他にもたくさん魅力的な建物があるので調べてみるといいですよ。"},{"id":{"d":135,"n":7,"t":3},"speaker":"A","text":"とても興味が湧きました。調べてみます。"}];
INSERT INTO sentence [{"id":{"d":136,"n":1,"t":3},"speaker":"A","text":"パンが好きで、休日はよくパン屋巡りをします。"},{"id":{"d":136,"n":2,"t":3},"speaker":"B","text":"そうですか。どのようなパンが好きですか？"},{"id":{"d":136,"n":3,"t":3},"speaker":"A","text":"私はハードパンが好きで、特にドイツパンが好きです。"},{"id":{"d":136,"n":4,"t":3},"speaker":"B","text":"ドイツパンは黒くて酸っぱいイメージがあります。"},{"id":{"d":136,"n":5,"t":3},"speaker":"A","text":"それがまたおいしいのです。また、プレッツェルやクリスマスのシュトーレンなども好きです。"},{"id":{"d":136,"n":6,"t":3},"speaker":"B","text":"プレッツェルは食べたことがあります。おいしいですよね。シュトーレンは食べたことがありませんが、どのようなものなのですか？"},{"id":{"d":136,"n":7,"t":3},"speaker":"A","text":"シュトーレンとはドイツの菓子パンで、主にクリスマスに食べられています。ドライフルーツがたくさん入っていておいしいですよ。ちょうど時期なので食べてみてはいかがですか？"},{"id":{"d":136,"n":8,"t":3},"speaker":"B","text":"おいしそうですね。食べてみます！"}];
INSERT INTO sentence [{"id":{"d":137,"n":1,"t":3},"speaker":"A","text":"すみません。予約をキャンセルしたいのですが。"},{"id":{"d":137,"n":2,"t":3},"speaker":"B","text":"かしこまりました。お名前とお電話番号をお伺いしてもよろしいでしょうか？"},{"id":{"d":137,"n":3,"t":3},"speaker":"A","text":"名前は鈴木太郎です。電話番号は080-1111-1111です。"},{"id":{"d":137,"n":4,"t":3},"speaker":"B","text":"鈴木太郎さまですね。確認が取れました。12月18日から20日のご予約でお間違いありませんか？"},{"id":{"d":137,"n":5,"t":3},"speaker":"A","text":"はい、そうです。"},{"id":{"d":137,"n":6,"t":3},"speaker":"B","text":"本日は宿泊予約日の1週間前なので、キャンセル料が8,000円かかってしまいますが、よろしいでしょうか？"},{"id":{"d":137,"n":7,"t":3},"speaker":"A","text":"はい。構いません。"},{"id":{"d":137,"n":8,"t":3},"speaker":"B","text":"かしこまりました。ではこちらのご予約をキャンセルいたします。"},{"id":{"d":137,"n":9,"t":3},"speaker":"A","text":"すみません。よろしくお願いします。"},{"id":{"d":137,"n":10,"t":3},"speaker":"B","text":"またの機会のご利用をお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":138,"n":1,"t":3},"speaker":"A","text":"夏休みの間に、沖縄でダイビングの免許を取りました。"},{"id":{"d":138,"n":2,"t":3},"speaker":"B","text":"素晴らしいですね。期間はどれくらいかかったのですか？"},{"id":{"d":138,"n":3,"t":3},"speaker":"A","text":"案外短い期間で取ることができますよ。私の場合は4日間でした。"},{"id":{"d":138,"n":4,"t":3},"speaker":"B","text":"4日間で取れるのですね、驚きました。なぜダイビングの免許を取得したのですか？何かやりたいことがあったのですか？"},{"id":{"d":138,"n":5,"t":3},"speaker":"A","text":"世界中のさまざまな海に潜って、イルカと泳ぎたかったのです。"},{"id":{"d":138,"n":6,"t":3},"speaker":"B","text":"すてきな夢ですね。かなうといいですね。"},{"id":{"d":138,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":139,"n":1,"t":3},"speaker":"A","text":"先週の日曜日に、鳥取砂丘でパラグライダーの体験をしました。"},{"id":{"d":139,"n":2,"t":3},"speaker":"B","text":"怖くはなかったですか？"},{"id":{"d":139,"n":3,"t":3},"speaker":"A","text":"最初は怖かったのですが、スタッフさんが親切で、景色もきれいだったのでだんだん怖さがなくなりました。最終的には、とても楽しかったです。"},{"id":{"d":139,"n":4,"t":3},"speaker":"B","text":"それを聞いて私もやってみたくなりました。砂丘という場所も、また斬新ですね。"},{"id":{"d":139,"n":5,"t":3},"speaker":"A","text":"そうですね。パラグライダーが終わったあとは、砂丘でたくさん遊びました。"},{"id":{"d":139,"n":6,"t":3},"speaker":"B","text":"充実した1日でしたね。"},{"id":{"d":139,"n":7,"t":3},"speaker":"A","text":"はい。とても楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":140,"n":1,"t":3},"speaker":"A","text":"ヘリコプターで一緒に夜景を見ませんか？"},{"id":{"d":140,"n":2,"t":3},"speaker":"B","text":"すてきですね。ぜひ行きたいです。どこを飛ぶのですか？"},{"id":{"d":140,"n":3,"t":3},"speaker":"A","text":"東京上空を飛び、そこから都内の夜景を見るのはどうでしょうか？この日は花火が打ち上がる予定もあります。"},{"id":{"d":140,"n":4,"t":3},"speaker":"B","text":"空から夜景と花火を見るなんてロマンチックですね。いつ行きますか？"},{"id":{"d":140,"n":5,"t":3},"speaker":"A","text":"1週間後は空いていますか？"},{"id":{"d":140,"n":6,"t":3},"speaker":"B","text":"空いています。"},{"id":{"d":140,"n":7,"t":3},"speaker":"A","text":"では1週間後、東京駅で17時に待ち合わせしましょう。"},{"id":{"d":140,"n":8,"t":3},"speaker":"B","text":"分かりました。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":141,"n":1,"t":3},"speaker":"A","text":"卒業旅行はどこに行きましょうか？"},{"id":{"d":141,"n":2,"t":3},"speaker":"B","text":"沖縄や北海道にも行きたいのですが、思い切って海外に行くのはどうでしょう？"},{"id":{"d":141,"n":3,"t":3},"speaker":"A","text":"良い案ですね。それならぜひアジアに行きたいです。"},{"id":{"d":141,"n":4,"t":3},"speaker":"B","text":"ベトナムや台湾、韓国など魅力的な国がたくさんありますね。どうしますか？"},{"id":{"d":141,"n":5,"t":3},"speaker":"A","text":"韓国に行ってみたいです。韓国料理や写真映えするスポットがたくさんありますよ。"},{"id":{"d":141,"n":6,"t":3},"speaker":"B","text":"それは魅力的です。では韓国にしましょう。何泊にしますか？"},{"id":{"d":141,"n":7,"t":3},"speaker":"A","text":"5泊程度はどうでしょうか？"},{"id":{"d":141,"n":8,"t":3},"speaker":"B","text":"いいですね。では早速旅行会社に連絡しましょう。"},{"id":{"d":141,"n":9,"t":3},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":142,"n":1,"t":3},"speaker":"A","text":"今度、東京湾で屋形船に乗ってきます。"},{"id":{"d":142,"n":2,"t":3},"speaker":"B","text":"それは楽しそうです。"},{"id":{"d":142,"n":3,"t":3},"speaker":"A","text":"はい。東京湾の屋形船で花火を見たいと思います。"},{"id":{"d":142,"n":4,"t":3},"speaker":"B","text":"優雅ですね。私も一度行ってみたいです。"},{"id":{"d":142,"n":5,"t":3},"speaker":"A","text":"私は会社の社員旅行として行くのですが、今度ぜひ行ってみてください。"},{"id":{"d":142,"n":6,"t":3},"speaker":"B","text":"はい、今度私も家族と旅行で行きたいと思います。"}];
INSERT INTO sentence [{"id":{"d":143,"n":1,"t":3},"speaker":"A","text":"バックパックで世界一周をするのが夢です。"},{"id":{"d":143,"n":2,"t":3},"speaker":"B","text":"すてきな夢ですね。移動手段はどうするのですか？"},{"id":{"d":143,"n":3,"t":3},"speaker":"A","text":"主に歩きとヒッチハイクを考えています。"},{"id":{"d":143,"n":4,"t":3},"speaker":"B","text":"素晴らしい挑戦ですね。"},{"id":{"d":143,"n":5,"t":3},"speaker":"A","text":"はい、すぐには達成できない目標を立ててそれを達成するのが好きなのです。"},{"id":{"d":143,"n":6,"t":3},"speaker":"B","text":"それは素晴らしいです！見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":144,"n":1,"t":3},"speaker":"A","text":"今度の休みにサイクリングなどいかがですか？"},{"id":{"d":144,"n":2,"t":3},"speaker":"B","text":"すみません。今度の休みは用事があって行けません。"},{"id":{"d":144,"n":3,"t":3},"speaker":"A","text":"そうですか。残念です。空いている日はありますか？"},{"id":{"d":144,"n":4,"t":3},"speaker":"B","text":"しばらく予定が埋まっていて分からないので、しばらく先になりそうです。"},{"id":{"d":144,"n":5,"t":3},"speaker":"A","text":"分かりました。もし予定が空いたら教えてください。"},{"id":{"d":144,"n":6,"t":3},"speaker":"B","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":145,"n":1,"t":3},"speaker":"A","text":"朝からずっと歩いて移動しているので、少し疲れてしまいました。休んでもよいですか？"},{"id":{"d":145,"n":2,"t":3},"speaker":"B","text":"そうですね。ちょうどあそこに足湯があるので、そこで休憩しましょう。"},{"id":{"d":145,"n":3,"t":3},"speaker":"A","text":"はい。ありがとうございます。"},{"id":{"d":145,"n":4,"t":3},"speaker":"B","text":"タオルの自動販売機もありますね。ここなら安心してゆっくり休憩できますね。"},{"id":{"d":145,"n":5,"t":3},"speaker":"A","text":"ここはワイファイもつながっているようなので、足湯に漬かりながら、次にどこに行くか調べましょう。"},{"id":{"d":145,"n":6,"t":3},"speaker":"B","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":146,"n":1,"t":3},"speaker":"A","text":"おはようございます。今日は晴れてよかったですね。"},{"id":{"d":146,"n":2,"t":3},"speaker":"B","text":"そうですね。よかったです。一応、折り畳み傘を持って来たので準備万端です。"},{"id":{"d":146,"n":3,"t":3},"speaker":"A","text":"楽しみにしていましたものね。バスの座席は前の方ですか？後ろの方ですか？"},{"id":{"d":146,"n":4,"t":3},"speaker":"B","text":"後ろの方は酔ってしまうので、前の方に座ろうと思っています。"},{"id":{"d":146,"n":5,"t":3},"speaker":"A","text":"そうですか。隣に座ってもよいですか？"},{"id":{"d":146,"n":6,"t":3},"speaker":"B","text":"もちろん、よいですよ。"}];
INSERT INTO sentence [{"id":{"d":147,"n":1,"t":3},"speaker":"A","text":"修学旅行はどこに行きましたか？"},{"id":{"d":147,"n":2,"t":3},"speaker":"B","text":"鎌倉に行ってきました。"},{"id":{"d":147,"n":3,"t":3},"speaker":"A","text":"いいですね。鎌倉で何をされたのですか？"},{"id":{"d":147,"n":4,"t":3},"speaker":"B","text":"大仏を見た後、長谷寺に行ってきました。"},{"id":{"d":147,"n":5,"t":3},"speaker":"A","text":"有名な観光スポットを巡られたのですね。"},{"id":{"d":147,"n":6,"t":3},"speaker":"B","text":"はい。あとは、あんみつを食べました。とてもおいしかったです。"},{"id":{"d":147,"n":7,"t":3},"speaker":"A","text":"それはよかったです。"}];
INSERT INTO sentence [{"id":{"d":148,"n":1,"t":3},"speaker":"A","text":"おはようございます。今日1日ガイドを務めさせていただきます、佐藤です。よろしくお願いします。"},{"id":{"d":148,"n":2,"t":3},"speaker":"B","text":"こちらこそ、よろしくお願いします。"},{"id":{"d":148,"n":3,"t":3},"speaker":"A","text":"何かお困りのことがございましたら、ご遠慮なく仰ってください。"},{"id":{"d":148,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。早速ですみませんが、指を切ってしまいましたのでばんそうこうを頂けませんか？"},{"id":{"d":148,"n":5,"t":3},"speaker":"A","text":"どうぞ、こちらをお使いください。"},{"id":{"d":148,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":149,"n":1,"t":3},"speaker":"A","text":"このホテルの近くに、ベジタリアン専門のレストランはありますか？"},{"id":{"d":149,"n":2,"t":3},"speaker":"B","text":"はい、ベジタリアンにお薦めのタイ料理店がございます。"},{"id":{"d":149,"n":3,"t":3},"speaker":"A","text":"どこにありますか？"},{"id":{"d":149,"n":4,"t":3},"speaker":"B","text":"セントラル駅から東に10分ほど歩いた所にございます。"},{"id":{"d":149,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。行ってみます。"}];
INSERT INTO sentence [{"id":{"d":150,"n":1,"t":3},"speaker":"A","text":"今日の夜は夜景の見えるレストランで食事をしたいと思っています。"},{"id":{"d":150,"n":2,"t":3},"speaker":"B","text":"私も同意見です。今から予約ができるか、いくつかのお店に聞いてみましょう。"},{"id":{"d":150,"n":3,"t":3},"speaker":"A","text":"お願いします。"},{"id":{"d":150,"n":4,"t":3},"speaker":"B","text":"19時から赤坂ヒルズのレストランが予約できました。イタリア料理のお店です。"},{"id":{"d":150,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。楽しみです。"},{"id":{"d":150,"n":6,"t":3},"speaker":"B","text":"どういたしまして。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":151,"n":1,"t":3},"speaker":"A","text":"サービスエリアに着いたら、何か買いに行きませんか？"},{"id":{"d":151,"n":2,"t":3},"speaker":"B","text":"そうですね。おなかもすいてきたので、私も何か買おうと思います。"},{"id":{"d":151,"n":3,"t":3},"speaker":"A","text":"確か、ここのサービスエリアの名物はたこ焼きでしたよね？"},{"id":{"d":151,"n":4,"t":3},"speaker":"B","text":"はい、そうです。私もたこ焼きを買ってこようかと思います。"},{"id":{"d":151,"n":5,"t":3},"speaker":"A","text":"以前雑誌で見たのですが、ここのたこ焼きは種類が豊富でソースも選べるので、大人気とのことですよ。"},{"id":{"d":151,"n":6,"t":3},"speaker":"B","text":"それではますます楽しみです。"}];
INSERT INTO sentence [{"id":{"d":152,"n":1,"t":3},"speaker":"A","text":"宮城には初めて来ましたが、おいしいものがたくさんありますね。"},{"id":{"d":152,"n":2,"t":3},"speaker":"B","text":"はい。私のお薦めはずんだ餅です。"},{"id":{"d":152,"n":3,"t":3},"speaker":"A","text":"おいしいですよね。ずんだシェイクも有名ですよね。"},{"id":{"d":152,"n":4,"t":3},"speaker":"B","text":"そうですね。他にもお米や牛タンなども有名です。"},{"id":{"d":152,"n":5,"t":3},"speaker":"A","text":"本当にたくさんのおいしいものがありますよね。今回の旅行でぜひいろいろ食べてみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":153,"n":1,"t":3},"speaker":"A","text":"夏休みに自転車で日本を一周しようと計画を立てています。"},{"id":{"d":153,"n":2,"t":3},"speaker":"B","text":"それは素晴らしい挑戦ですね。"},{"id":{"d":153,"n":3,"t":3},"speaker":"A","text":"以前テレビで見たことがあり、自分も挑戦してみたいと思ったのです。"},{"id":{"d":153,"n":4,"t":3},"speaker":"B","text":"そのテレビなら私も見たことがあります。海岸沿いを走る姿がとても気持ち良さそうでした。"},{"id":{"d":153,"n":5,"t":3},"speaker":"A","text":"そうですよね。海岸沿いを走ることも楽しみですが、いろいろな人と出会えることが最も楽しみです。"},{"id":{"d":153,"n":6,"t":3},"speaker":"B","text":"お気を付けて楽しんできてください。応援しています。"}];
INSERT INTO sentence [{"id":{"d":154,"n":1,"t":3},"speaker":"A","text":"すみません、教えてほしいことがあります。栃木県で入浴のできる道の駅はいくつありますか？"},{"id":{"d":154,"n":2,"t":3},"speaker":"B","text":"栃木県には3カ所ございます。"},{"id":{"d":154,"n":3,"t":3},"speaker":"A","text":"3カ所の場所と道の駅の名前を教えていただけますか？"},{"id":{"d":154,"n":4,"t":3},"speaker":"B","text":"宇都宮市に「道の駅うつのみやろまんちっく村」、芳賀町に「道の駅はがロマンの湯」、さくら市に「道の駅きつれがわ」がございます。"},{"id":{"d":154,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":155,"n":1,"t":3},"speaker":"A","text":"今度の遠足はどこに行くのですか？"},{"id":{"d":155,"n":2,"t":3},"speaker":"B","text":"那須の茶臼岳に登ります。"},{"id":{"d":155,"n":3,"t":3},"speaker":"A","text":"登山ですか？"},{"id":{"d":155,"n":4,"t":3},"speaker":"B","text":"そうです。でも、一般の人が想像するような木が生えている山ではなく、噴火の後にできたために大小さまざまな石が転がっている山なのです。"},{"id":{"d":155,"n":5,"t":3},"speaker":"A","text":"それは面白いですね。私も那須に観光に行った際には、ぜひ茶臼岳に登ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":156,"n":1,"t":3},"speaker":"A","text":"修学旅行はどこに行きますか？"},{"id":{"d":156,"n":2,"t":3},"speaker":"B","text":"福島と宮城です。"},{"id":{"d":156,"n":3,"t":3},"speaker":"A","text":"京都ではないのですか？"},{"id":{"d":156,"n":4,"t":3},"speaker":"B","text":"宿が埋まっていた関係で京都に行けなくなってしまい、東北方面に変更になりました。"},{"id":{"d":156,"n":5,"t":3},"speaker":"A","text":"そうですか。京都に行けないのは残念ですが、東北にも魅力はいっぱいありますから、東北の旅を楽しんできてください。"},{"id":{"d":156,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。友達と一緒ならどこでも楽しい思い出になると思います。"}];
INSERT INTO sentence [{"id":{"d":157,"n":1,"t":3},"speaker":"A","text":"海外旅行では必ず英語が必要ですか？"},{"id":{"d":157,"n":2,"t":3},"speaker":"B","text":"行き先によります。韓国などは日本語で対応してくれるお店もありますので、日本人が多い観光地を選んで旅行をすれば大丈夫ですよ。"},{"id":{"d":157,"n":3,"t":3},"speaker":"A","text":"そうですか。英語が全くできないので、海外旅行を諦めなくてはいけないかと思っていました。"},{"id":{"d":157,"n":4,"t":3},"speaker":"B","text":"そのようなことはありません。例えば、ツアーに参加すれば、英語が分からなくても添乗員さんが助けてくれるので安心して旅行を楽しめると思いますよ。"},{"id":{"d":157,"n":5,"t":3},"speaker":"A","text":"なるほど、それはうれしいですね。"}];
INSERT INTO sentence [{"id":{"d":158,"n":1,"t":3},"speaker":"A","text":"来月ドイツで行われる工業機械の展示会に行くのですが、お薦めのドイツ料理はありますか？"},{"id":{"d":158,"n":2,"t":3},"speaker":"B","text":"料理ではありませんが、ドイツと言えば、まずビールですね。ソーセージ料理も有名です。"},{"id":{"d":158,"n":3,"t":3},"speaker":"A","text":"ビールとソーセージですか、とても良い響きです。"},{"id":{"d":158,"n":4,"t":3},"speaker":"B","text":"はい、とても良く合う組み合わせです。"},{"id":{"d":158,"n":5,"t":3},"speaker":"A","text":"仕事で行くのですが、とても楽しみになってきました。ぜひ本場の味を体験してこようと思います。"},{"id":{"d":158,"n":6,"t":3},"speaker":"B","text":"帰国したらドイツのお話をぜひ聞かせてくださいね。"},{"id":{"d":158,"n":7,"t":3},"speaker":"A","text":"はい。日本に帰ってきたらすぐに連絡しますね。"}];
INSERT INTO sentence [{"id":{"d":159,"n":1,"t":3},"speaker":"A","text":"新幹線のグリーン車に乗ったことはありますか？"},{"id":{"d":159,"n":2,"t":3},"speaker":"B","text":"ありません。"},{"id":{"d":159,"n":3,"t":3},"speaker":"A","text":"私もありません。でも、いつかは乗ってみたいと思っています。グリーン車で旅行という響きにも憧れます。"},{"id":{"d":159,"n":4,"t":3},"speaker":"B","text":"確かにグリーン車での旅行はぜいたくな感じがします。おいしい駅弁とビールも欲しくなりますね。"},{"id":{"d":159,"n":5,"t":3},"speaker":"A","text":"はい。いつかグリーン車でぜいたく旅行をするために、今日も仕事を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":160,"n":1,"t":3},"speaker":"A","text":"先週、バンクーバーに旅行に行かれたと聞きました。いかがでしたか？"},{"id":{"d":160,"n":2,"t":3},"speaker":"B","text":"とてもよかったです。日本語も通じましたし、食事もおいしかったです。"},{"id":{"d":160,"n":3,"t":3},"speaker":"A","text":"日本語が通じるのですか？"},{"id":{"d":160,"n":4,"t":3},"speaker":"B","text":"はい、ほとんどのお店に日本人の店員さんがいて、お店に入ると「いらっしゃいませ」と言われました。カナダに来たのに不思議な感覚になりましたが、日本語で接客していただいたので買い物がスムーズにできました。"},{"id":{"d":160,"n":5,"t":3},"speaker":"A","text":"それほどたくさん日本人がいるのですね。"},{"id":{"d":160,"n":6,"t":3},"speaker":"B","text":"そうですね。留学生やワーキングホリデーの方も多い印象でした。ビザが取りやすいのもその一因かもしれません。"},{"id":{"d":160,"n":7,"t":3},"speaker":"A","text":"英語が苦手な私でも旅行がしやすそうです。次の旅先はカナダに決めました！"}];
INSERT INTO sentence [{"id":{"d":161,"n":1,"t":3},"speaker":"A","text":"先日の連休中に、主人が新潟に行ってきました。"},{"id":{"d":161,"n":2,"t":3},"speaker":"B","text":"何をしに行かれたのですか？"},{"id":{"d":161,"n":3,"t":3},"speaker":"A","text":"スノーボードです。冬は休みがあればスノーボードをするために毎年山に行っています。"},{"id":{"d":161,"n":4,"t":3},"speaker":"B","text":"そうですか。先日はどこに泊まったのですか？"},{"id":{"d":161,"n":5,"t":3},"speaker":"A","text":"スキー場近くの道の駅で車内泊をしたそうです。"},{"id":{"d":161,"n":6,"t":3},"speaker":"B","text":"そういった方法もあるのですね。車内泊も面白そうですね。"},{"id":{"d":161,"n":7,"t":3},"speaker":"A","text":"車内で快適に過ごせる方法を考えることも楽しいようですね。挑戦してみてください。"},{"id":{"d":161,"n":8,"t":3},"speaker":"B","text":"はい、ぜひ挑戦してみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":162,"n":1,"t":3},"speaker":"A","text":"旅先での楽しみは何ですか？"},{"id":{"d":162,"n":2,"t":3},"speaker":"B","text":"直売所に行くことです。その地域でしか食べることのできないものを食べることが楽しみです。"},{"id":{"d":162,"n":3,"t":3},"speaker":"A","text":"今まで行った中で、何か珍しいものはありますか？"},{"id":{"d":162,"n":4,"t":3},"speaker":"B","text":"何年か前に静岡で白いナスを見つけました。調理してみるとトロトロでとてもおいしかったです。今はいろいろな所で見かけるので珍しくはありませんが、当時は驚きました。"},{"id":{"d":162,"n":5,"t":3},"speaker":"A","text":"白いナスですか。ぜひ私も食べてみたいです。"}];
INSERT INTO sentence [{"id":{"d":163,"n":1,"t":3},"speaker":"A","text":"ハワイに行ったことはありますか？"},{"id":{"d":163,"n":2,"t":3},"speaker":"B","text":"はい、今から20年前に新婚旅行で行きました。"},{"id":{"d":163,"n":3,"t":3},"speaker":"A","text":"良いですね。楽しめましたか？"},{"id":{"d":163,"n":4,"t":3},"speaker":"B","text":"それが、到着したその日に夫が熱を出してしまい、ほとんどホテルから出られませんでした。"},{"id":{"d":163,"n":5,"t":3},"speaker":"A","text":"それは残念でしたね。"},{"id":{"d":163,"n":6,"t":3},"speaker":"B","text":"はい。その時はハワイを全然楽しめなかったので、銀婚式にもう一度行こうと約束しています。5年後が待ち遠しいです。"},{"id":{"d":163,"n":7,"t":3},"speaker":"A","text":"すてきですね。今度こそ楽しめるといいですね。"}];
INSERT INTO sentence [{"id":{"d":164,"n":1,"t":3},"speaker":"A","text":"今年の夏は家族でキャンプへ行くことを計画しています。お薦めのキャンプ場はありますか？"},{"id":{"d":164,"n":2,"t":3},"speaker":"B","text":"山形の三瀬海岸のキャンプ場がお薦めですよ。"},{"id":{"d":164,"n":3,"t":3},"speaker":"A","text":"初めて聞きました。"},{"id":{"d":164,"n":4,"t":3},"speaker":"B","text":"あまり知られていないため、いつも利用客が3組くらいのみなのです。砂浜にテントを立てるので砂まみれになりますが、日本海が目の前に広がっていてゆったりと過ごせます。"},{"id":{"d":164,"n":5,"t":3},"speaker":"A","text":"これまで砂浜でキャンプをしたことはありませんでしたが、楽しそうですね。"},{"id":{"d":164,"n":6,"t":3},"speaker":"B","text":"山とはまた違う経験ができて楽しいですよ。一度そこでキャンプをすると、必ずまた行きたくなると思います。"},{"id":{"d":164,"n":7,"t":3},"speaker":"A","text":"興味が湧いてきました。後でまた詳しく教えてください。"},{"id":{"d":164,"n":8,"t":3},"speaker":"B","text":"はい。今度ゆっくり三瀬海岸キャンプ場の楽しみ方をお教えしますね。"},{"id":{"d":164,"n":9,"t":3},"speaker":"A","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":165,"n":1,"t":3},"speaker":"A","text":"次の土曜日は古本屋巡りをしようと思うのですが、よければご一緒にどうですか？"},{"id":{"d":165,"n":2,"t":3},"speaker":"B","text":"面白そうです。ぜひご一緒させてください。"},{"id":{"d":165,"n":3,"t":3},"speaker":"A","text":"もちろん。東京には3大古本屋街と呼ばれる所があるのですが、ご存じですか？"},{"id":{"d":165,"n":4,"t":3},"speaker":"B","text":"神保町は知っているのですが、他の2つはどこでしょうか？"},{"id":{"d":165,"n":5,"t":3},"speaker":"A","text":"早稲田と本郷です。"},{"id":{"d":165,"n":6,"t":3},"speaker":"B","text":"そうですか。今回はどの街を回るのですか？"},{"id":{"d":165,"n":7,"t":3},"speaker":"A","text":"神保町からスタートして、本郷、早稲田の順で回ります。"},{"id":{"d":165,"n":8,"t":3},"speaker":"B","text":"3カ所全てを回るのですね。"},{"id":{"d":165,"n":9,"t":3},"speaker":"A","text":"はい。それぞれの街で古本屋の雰囲気も違いますので、その違いを楽しむことも古本屋巡りの醍醐味の一つです。"}];
INSERT INTO sentence [{"id":{"d":166,"n":1,"t":3},"speaker":"A","text":"ドライブをするなら、どのような車が良いですか？"},{"id":{"d":166,"n":2,"t":3},"speaker":"B","text":"私はスポーツカーが良いです。いろいろな道をハイスピードで走り抜けるのが気持ちよくて好きです。"},{"id":{"d":166,"n":3,"t":3},"speaker":"A","text":"そうですか。私はゆったりドライブを楽しみたいので、ミニバンタイプが好きです。"},{"id":{"d":166,"n":4,"t":3},"speaker":"B","text":"それぞれ好みが違うので面白いですね。"},{"id":{"d":166,"n":5,"t":3},"speaker":"A","text":"本当ですね。他の人にも聞いてみるともっと面白くなるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":167,"n":1,"t":3},"speaker":"A","text":"来週、宮島へ旅行に行きます。"},{"id":{"d":167,"n":2,"t":3},"speaker":"B","text":"もみじまんじゅうが有名な宮島ですか？"},{"id":{"d":167,"n":3,"t":3},"speaker":"A","text":"そうです。もみじまんじゅうも有名ですが、宮島と言えば厳島神社の鳥居が有名です。"},{"id":{"d":167,"n":4,"t":3},"speaker":"B","text":"それもそうですね。つい、食べ物が先に浮かんできてしまいましたが。"},{"id":{"d":167,"n":5,"t":3},"speaker":"A","text":"食べものに詳しいですね。確かに、もみじまんじゅうはいろいろ種類があってどれを買うか迷ってしまいます。"},{"id":{"d":167,"n":6,"t":3},"speaker":"B","text":"私はオーソドックスな小倉が好きです。"},{"id":{"d":167,"n":7,"t":3},"speaker":"A","text":"私はチョコレートなど変わり種も好きですよ。"},{"id":{"d":167,"n":8,"t":3},"speaker":"B","text":"お互い、食いしん坊ですね。"}];
INSERT INTO sentence [{"id":{"d":168,"n":1,"t":3},"speaker":"A","text":"イギリスで「Where is the bathroom?」と尋ねたら、通じはしたのですが不思議な顔をされました。"},{"id":{"d":168,"n":2,"t":3},"speaker":"B","text":"なるほど。"},{"id":{"d":168,"n":3,"t":3},"speaker":"A","text":"なぜ「なるほど」なのですか？"},{"id":{"d":168,"n":4,"t":3},"speaker":"B","text":"イギリスでは、トイレのことを「bathroom」ではなく「toilet」と呼ぶことが一般的なのです。"},{"id":{"d":168,"n":5,"t":3},"speaker":"A","text":"そうだったのですね。それで不思議な顔をされたのですね。"},{"id":{"d":168,"n":6,"t":3},"speaker":"B","text":"「bathroom」はアメリカ英語なので、「Where is the toilet?」と尋ねるとスムーズに伝わります。"},{"id":{"d":168,"n":7,"t":3},"speaker":"A","text":"それは知りませんでした。次はそのように尋ねてみます。"}];
INSERT INTO sentence [{"id":{"d":169,"n":1,"t":3},"speaker":"A","text":"今度の春休みはサクラを見に日本へ行ってきます。"},{"id":{"d":169,"n":2,"t":3},"speaker":"B","text":"それは楽しそうですね。日本でサクラを見るなんて、まさに日本フリークのあなたらしい過ごし方ですね。"},{"id":{"d":169,"n":3,"t":3},"speaker":"A","text":"はい。日本にはサクラの季節特有の文化がたくさんあるので、それも楽しみたいと思っています。"},{"id":{"d":169,"n":4,"t":3},"speaker":"B","text":"例えばどのようなものがありますか？"},{"id":{"d":169,"n":5,"t":3},"speaker":"A","text":"お花見のような行事のほかにも、サクラの季節特有の食事や、サクラをかたどった和菓子があります。"},{"id":{"d":169,"n":6,"t":3},"speaker":"B","text":"それは楽しみですね。またぜひ、日本の感想を聞かせてください。"},{"id":{"d":169,"n":7,"t":3},"speaker":"A","text":"はい、帰国したらまたお伝えしますね。"}];
INSERT INTO sentence [{"id":{"d":170,"n":1,"t":3},"speaker":"A","text":"車椅子でそちらのホテルを利用したいのですが？"},{"id":{"d":170,"n":2,"t":3},"speaker":"B","text":"さようでございますか。私どものホテルは全館バリアフリーとなっております。"},{"id":{"d":170,"n":3,"t":3},"speaker":"A","text":"そうですか。それはありがたいです。居室のドアは引き戸でしょうか？"},{"id":{"d":170,"n":4,"t":3},"speaker":"B","text":"申し訳ございません。全居室ドア仕様でございます。"},{"id":{"d":170,"n":5,"t":3},"speaker":"A","text":"そうですか。居室への出入りの際にお手伝いしてもらうことは可能でしょうか？"},{"id":{"d":170,"n":6,"t":3},"speaker":"B","text":"もちろんでございます。出入りの際以外のことでもご対応いたします。"},{"id":{"d":170,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。検討させていただきますので再度ご連絡します。"},{"id":{"d":170,"n":8,"t":3},"speaker":"B","text":"かしこまりました。ご連絡をお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":171,"n":1,"t":3},"speaker":"A","text":"旅行に便利なお薦めの100円グッズはありますか？"},{"id":{"d":171,"n":2,"t":3},"speaker":"B","text":"私のお薦めのグッズは圧縮袋です。洋服などをコンパクトに収納できるので重宝しています。"},{"id":{"d":171,"n":3,"t":3},"speaker":"A","text":"それは便利そうですね。他にも何かありますか？"},{"id":{"d":171,"n":4,"t":3},"speaker":"B","text":"ネットバッグが便利かと思います。行きは下着などを入れて帰りは洗濯物を入れれば、そのまま洗濯ができますよ。"},{"id":{"d":171,"n":5,"t":3},"speaker":"A","text":"それは便利ですね。早速購入したいと思います。"}];
INSERT INTO sentence [{"id":{"d":172,"n":1,"t":3},"speaker":"A","text":"年末の休暇には山梨へ行こうと思っています。"},{"id":{"d":172,"n":2,"t":3},"speaker":"B","text":"それは楽しみですね。山梨のどちらへ行かれるのですか？"},{"id":{"d":172,"n":3,"t":3},"speaker":"A","text":"富士急ハイランドに行く予定です。"},{"id":{"d":172,"n":4,"t":3},"speaker":"B","text":"富士急ハイランドですか、FUJIYAMAにも乗るのですか？"},{"id":{"d":172,"n":5,"t":3},"speaker":"A","text":"はい。以前に一度乗った時の感動をもう一度味わおうと思っています。"},{"id":{"d":172,"n":6,"t":3},"speaker":"B","text":"それは楽しみですね！また今度感想を教えてください。"},{"id":{"d":172,"n":7,"t":3},"speaker":"A","text":"はい、またお伝えしますね。"}];
INSERT INTO sentence [{"id":{"d":173,"n":1,"t":3},"speaker":"A","text":"来月、東京ディズニーシーへ行く予定なのですが、そこでぜひ食べてみたいものがあります。"},{"id":{"d":173,"n":2,"t":3},"speaker":"B","text":"何を食べてみたいのですか？"},{"id":{"d":173,"n":3,"t":3},"speaker":"A","text":"スペシャルメニューのシェパーズパイ風ブレッドです。"},{"id":{"d":173,"n":4,"t":3},"speaker":"B","text":"それはどういったものなのですか？"},{"id":{"d":173,"n":5,"t":3},"speaker":"A","text":"お皿がパンで作られているマッシュポテトです。チーズがミッキーの形になっているそうです。"},{"id":{"d":173,"n":6,"t":3},"speaker":"B","text":"それはかわいらしいですね。他にも気になるものがあったらぜひ教えてください。"},{"id":{"d":173,"n":7,"t":3},"speaker":"A","text":"分かりました。楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":174,"n":1,"t":3},"speaker":"A","text":"こちらのホテルの近くに、食事ができる所はありますか？"},{"id":{"d":174,"n":2,"t":3},"speaker":"B","text":"かっぱ寿司とケンタッキーフライドチキン、マクドナルドがございます。ファミリーレストランもあるのですが、現在、改装中のため、ご利用できません。"},{"id":{"d":174,"n":3,"t":3},"speaker":"A","text":"かっぱ寿司があるのですね。初めてなので、行ってみたいです。ホテルからどれくらいかかりますか？"},{"id":{"d":174,"n":4,"t":3},"speaker":"B","text":"ここからは徒歩で5分ほどです。"},{"id":{"d":174,"n":5,"t":3},"speaker":"A","text":"近いですね。それでは、夕飯はかっぱ寿司にします。ありがとうございました。"},{"id":{"d":174,"n":6,"t":3},"speaker":"B","text":"お気を付けて行ってらっしゃいませ。"}];
INSERT INTO sentence [{"id":{"d":175,"n":1,"t":3},"speaker":"A","text":"今年の秋は京都に行こうと思います。"},{"id":{"d":175,"n":2,"t":3},"speaker":"B","text":"楽しそうですね。秋に行かれるということは紅葉狩りですか？"},{"id":{"d":175,"n":3,"t":3},"speaker":"A","text":"そのつもりです。特に嵐山がとても好きなので、嵐山の紅葉を見る予定です。"},{"id":{"d":175,"n":4,"t":3},"speaker":"B","text":"それなら嵐山の渡月橋近辺でしょうか。あそこは桂川もあるので、紅葉の季節は本当にきれいですよね。"},{"id":{"d":175,"n":5,"t":3},"speaker":"A","text":"はい。その近辺で紅葉狩りをする予定です。近くにある竹林の道も大好きなので、ついでに足を延ばす予定です。"},{"id":{"d":175,"n":6,"t":3},"speaker":"B","text":"すてきですね。ぜひ楽しんでいらしてください。"},{"id":{"d":175,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。お土産を買ってくるので楽しみにしていらしてください。"}];
INSERT INTO sentence [{"id":{"d":176,"n":1,"t":3},"speaker":"A","text":"今度の誕生日はどうしますか？何かしてみたいことはありますか？"},{"id":{"d":176,"n":2,"t":3},"speaker":"B","text":"ヘリコプターで東京湾を1周してみたいです。"},{"id":{"d":176,"n":3,"t":3},"speaker":"A","text":"それは面白いですね。東京湾上空から見る夜景はきれいでしょうね。"},{"id":{"d":176,"n":4,"t":3},"speaker":"B","text":"そう思います。夜景を見ながら、高級なシャンパンで乾杯をしてみたいです。"},{"id":{"d":176,"n":5,"t":3},"speaker":"A","text":"分かりました。誕生日のプランは決まりましたね。では当日を楽しみにしていてください。"},{"id":{"d":176,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。今から待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":177,"n":1,"t":3},"speaker":"A","text":"昔からの夢なのですが、豪華客船で世界一周をしてみたいと思っています。"},{"id":{"d":177,"n":2,"t":3},"speaker":"B","text":"すてきな夢ですね。費用が高額と聞いたことがあるのですが、どのくらいかかるのですか？"},{"id":{"d":177,"n":3,"t":3},"speaker":"A","text":"グレードによって変わりますが、200万円から850万円と言われています。"},{"id":{"d":177,"n":4,"t":3},"speaker":"B","text":"簡単に行ける額ではありませんね。"},{"id":{"d":177,"n":5,"t":3},"speaker":"A","text":"そうなのです。そのため、数年前から少しずつ費用をためています。妻とは「定年になったら行こう」と話しています。"},{"id":{"d":177,"n":6,"t":3},"speaker":"B","text":"すてきですね。私も船での世界一周旅行を定年後の夢にしたくなってきました。"},{"id":{"d":177,"n":7,"t":3},"speaker":"A","text":"お互い頑張りましょうね。"},{"id":{"d":177,"n":8,"t":3},"speaker":"B","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":178,"n":1,"t":3},"speaker":"A","text":"こんにちは。日本の方ですか？"},{"id":{"d":178,"n":2,"t":3},"speaker":"B","text":"はい、私は日本人です。"},{"id":{"d":178,"n":3,"t":3},"speaker":"A","text":"私はマイケルです。カナダ人です。あなたは日本のどこからいらっしゃいましたか？"},{"id":{"d":178,"n":4,"t":3},"speaker":"B","text":"栃木から来ました。"},{"id":{"d":178,"n":5,"t":3},"speaker":"A","text":"栃木？"},{"id":{"d":178,"n":6,"t":3},"speaker":"B","text":"栃木と言っても分からないかもしれませんね。東京の近くです。日光がある所です。"},{"id":{"d":178,"n":7,"t":3},"speaker":"A","text":"日光なら分かります。日本の有名な歴史的な建造物がありますよね。"},{"id":{"d":178,"n":8,"t":3},"speaker":"B","text":"はい、日光東照宮ですね。"},{"id":{"d":178,"n":9,"t":3},"speaker":"A","text":"そうです。日光東照宮には一度行ってみたいと思っています。"},{"id":{"d":178,"n":10,"t":3},"speaker":"B","text":"それなら日光を旅行するときにはご連絡ください。案内しますよ。"}];
INSERT INTO sentence [{"id":{"d":179,"n":1,"t":3},"speaker":"A","text":"週に2回、宮城県の松島に仕事で行っています。"},{"id":{"d":179,"n":2,"t":3},"speaker":"B","text":"松島とは、あの、小さな島がいくつもある観光地のことですか？"},{"id":{"d":179,"n":3,"t":3},"speaker":"A","text":"そうです。"},{"id":{"d":179,"n":4,"t":3},"speaker":"B","text":"いいですね。毎週観光に行っている気分になるのではありませんか？"},{"id":{"d":179,"n":5,"t":3},"speaker":"A","text":"最初はうれしかったのですが、見慣れてきてしまいました。仕事が目的ですし、今はあまり観光という気分にはならなくなってしまっています。"},{"id":{"d":179,"n":6,"t":3},"speaker":"B","text":"そうですか。観光地というのは、たまに行くからこそ感動するものなのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":180,"n":1,"t":3},"speaker":"A","text":"ナイトサファリの予約をお願いします。"},{"id":{"d":180,"n":2,"t":3},"speaker":"B","text":"かしこまりました。何名さまでご予約をお取りいたしますか？"},{"id":{"d":180,"n":3,"t":3},"speaker":"A","text":"大人2名、子ども2名でよろしくお願いします。"},{"id":{"d":180,"n":4,"t":3},"speaker":"B","text":"大人2名さま、子ども2名さまですね。入場料は大人が1人50シンガポールドル、子どもが1人33シンガポールドルですので、合計166シンガポールドルになります。"},{"id":{"d":180,"n":5,"t":3},"speaker":"A","text":"食事もできると聞いたのですが、そちらは料金には含まれていますか？"},{"id":{"d":180,"n":6,"t":3},"speaker":"B","text":"お食事は別料金になっております。現地でのご精算になります。"},{"id":{"d":180,"n":7,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":180,"n":8,"t":3},"speaker":"B","text":"入場料のお支払いは、クレジットカードとキャッシュのどちらになさいますか？"},{"id":{"d":180,"n":9,"t":3},"speaker":"A","text":"クレジットカードでお願いします。"}];
INSERT INTO sentence [{"id":{"d":181,"n":1,"t":3},"speaker":"A","text":"東京から仙台までの高速バスのチケットを1枚お願いします。"},{"id":{"d":181,"n":2,"t":3},"speaker":"B","text":"かしこまりました。片道3,000円になります。"},{"id":{"d":181,"n":3,"t":3},"speaker":"A","text":"時間はどれくらいかかりますか？"},{"id":{"d":181,"n":4,"t":3},"speaker":"B","text":"約5時間50分を予定しております。23時40分に新宿を出発し、5時30分に仙台駅に到着予定です。"},{"id":{"d":181,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。新幹線より時間はかかりますが、高速バスは旅費を抑えられるので安く移動したいときには便利ですね。"},{"id":{"d":181,"n":6,"t":3},"speaker":"B","text":"そうですね。私も度々高速バスを利用しております。"}];
INSERT INTO sentence [{"id":{"d":182,"n":1,"t":3},"speaker":"A","text":"3泊4日の海外旅行に、スーツケースはどれくらいの大きさのものが必要ですか？"},{"id":{"d":182,"n":2,"t":3},"speaker":"B","text":"あまり大きなものは必要ないかと思いますが、お土産を入れてくることを考えると、少し大きめがいいかもしれません。"},{"id":{"d":182,"n":3,"t":3},"speaker":"A","text":"そうですね、お土産のことも考えないといけませんね。初めての海外旅行なので、何を用意してよいか分からなくて困っています。"},{"id":{"d":182,"n":4,"t":3},"speaker":"B","text":"最低限必要なものを持っていき、あとは現地で調達するのもいいですよ。"},{"id":{"d":182,"n":5,"t":3},"speaker":"A","text":"なるほど。これは必ず持っていった方がいいというものはありますか？"},{"id":{"d":182,"n":6,"t":3},"speaker":"B","text":"変圧器と変換プラグは持っていくことをお勧めします。"},{"id":{"d":182,"n":7,"t":3},"speaker":"A","text":"なぜですか？"},{"id":{"d":182,"n":8,"t":3},"speaker":"B","text":"国によって電圧が違うので、携帯の充電器など日本から持参した電気機器が壊れてしまう可能性があるのですよ。"},{"id":{"d":182,"n":9,"t":3},"speaker":"A","text":"とても勉強になります。ありがとうございます。"},{"id":{"d":182,"n":10,"t":3},"speaker":"B","text":"良い旅になることを願っています。"}];
INSERT INTO sentence [{"id":{"d":183,"n":1,"t":3},"speaker":"A","text":"カヌーに乗ったことはありますか？"},{"id":{"d":183,"n":2,"t":3},"speaker":"B","text":"はい、あります。カナダに旅行に行った際に乗りました。"},{"id":{"d":183,"n":3,"t":3},"speaker":"A","text":"海外でカヌー体験とはうらやましいです。"},{"id":{"d":183,"n":4,"t":3},"speaker":"B","text":"とてもきれいな川で、下をのぞくと魚が見えました。"},{"id":{"d":183,"n":5,"t":3},"speaker":"A","text":"素晴らしい体験でしたね。"},{"id":{"d":183,"n":6,"t":3},"speaker":"B","text":"はい、とても良かったです。"}];
INSERT INTO sentence [{"id":{"d":184,"n":1,"t":3},"speaker":"A","text":"この近くに、初心者向けのコースがあるスキー場はありますか？"},{"id":{"d":184,"n":2,"t":3},"speaker":"B","text":"はい、ございます。"},{"id":{"d":184,"n":3,"t":3},"speaker":"A","text":"スキー場の名前を教えていただけますか？"},{"id":{"d":184,"n":4,"t":3},"speaker":"B","text":"スノーマウンテンスキー場という所です。当ホテルの玄関より直通バスが出ておりますので、そちらをご利用ください。"},{"id":{"d":184,"n":5,"t":3},"speaker":"A","text":"それは助かります。スキー道具やウェアのレンタルはありますか？"},{"id":{"d":184,"n":6,"t":3},"speaker":"B","text":"はい。当ホテルの1階に、道具とウエアのレンタルショップがございます。ご案内いたしましょうか？"},{"id":{"d":184,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":185,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。"},{"id":{"d":185,"n":2,"t":3},"speaker":"B","text":"こんばんは。予約はしていませんが、今晩宿泊できますか？4名で、2部屋利用できるといいのですが。"},{"id":{"d":185,"n":3,"t":3},"speaker":"A","text":"少々お待ちください。ただ今、お調べいたします。"},{"id":{"d":185,"n":4,"t":3},"speaker":"B","text":"よろしくお願いします。"},{"id":{"d":185,"n":5,"t":3},"speaker":"A","text":"お待たせいたしました。2部屋ご用意できます。"},{"id":{"d":185,"n":6,"t":3},"speaker":"B","text":"よかった！ありがとうございます。"},{"id":{"d":185,"n":7,"t":3},"speaker":"A","text":"恐れ入りますが、クレジットカードはお持ちでしょうか？"},{"id":{"d":185,"n":8,"t":3},"speaker":"B","text":"クレジットカードではなく、現金で支払いたいのですが。"},{"id":{"d":185,"n":9,"t":3},"speaker":"A","text":"現金でお支払いいただく場合も、身分確認のためにクレジットカードの確認をさせていただいております。"},{"id":{"d":185,"n":10,"t":3},"speaker":"B","text":"そうでしたか。分かりました。こちらでお願いします。"},{"id":{"d":185,"n":11,"t":3},"speaker":"A","text":"ありがとうございます。ご確認いたしました。それではこちらがルームキーでございます。ごゆっくりおくつろぎください。"},{"id":{"d":185,"n":12,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":186,"n":1,"t":3},"speaker":"A","text":"海外旅行に行くとき、現金を持ち歩くのが心配です。"},{"id":{"d":186,"n":2,"t":3},"speaker":"B","text":"クレジットカードを作るのはいかがですか？身分の保証にもなりますからお勧めですよ。"},{"id":{"d":186,"n":3,"t":3},"speaker":"A","text":"そうですね。でも、まだ学生なのでクレジットカードは作れません。"},{"id":{"d":186,"n":4,"t":3},"speaker":"B","text":"それなら、トラベラーズチェックはいかがですか？"},{"id":{"d":186,"n":5,"t":3},"speaker":"A","text":"トラベラーズチェックとはどのようなものですか？"},{"id":{"d":186,"n":6,"t":3},"speaker":"B","text":"海外旅行で使える小切手のようなものです。支払う金額を書いてサインして渡しますので、現金より使いやすくて安心です。"},{"id":{"d":186,"n":7,"t":3},"speaker":"A","text":"それはどこで手に入りますか？"},{"id":{"d":186,"n":8,"t":3},"speaker":"B","text":"銀行で作ることができますよ。"},{"id":{"d":186,"n":9,"t":3},"speaker":"A","text":"なるほど。ありがとうございます。銀行に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":187,"n":1,"t":3},"speaker":"A","text":"スカイツリーと東京タワー、どちらが好きですか？"},{"id":{"d":187,"n":2,"t":3},"speaker":"B","text":"どちらも好きです。スカイツリーは技術の進歩が感じられますし、東京タワーは人力であそこまで造ったことを思うと感慨深いものがあります。"},{"id":{"d":187,"n":3,"t":3},"speaker":"A","text":"確かにそうですね。どちらも魅力的で、比べる物ではないのかもしれません。"},{"id":{"d":187,"n":4,"t":3},"speaker":"B","text":"そうかもしれませんね。今度、1日で両方のタワーに昇るツアーをしてみませんか？"},{"id":{"d":187,"n":5,"t":3},"speaker":"A","text":"いいですね。今度の日曜日はいかがですか？"},{"id":{"d":187,"n":6,"t":3},"speaker":"B","text":"すみません。今度の日曜日は恋人との予定があるので、別の日で予定を立てましょう。"},{"id":{"d":187,"n":7,"t":3},"speaker":"A","text":"分かりました。では、土曜日はいかがですか？"},{"id":{"d":187,"n":8,"t":3},"speaker":"B","text":"土曜日なら大丈夫です。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":188,"n":1,"t":3},"speaker":"A","text":"これまで訪れた国や地域で、特に印象に残っている場所はありますか？"},{"id":{"d":188,"n":2,"t":3},"speaker":"B","text":"中国ですね。"},{"id":{"d":188,"n":3,"t":3},"speaker":"A","text":"中国ですか。どのようなところが印象に残っているのですか？"},{"id":{"d":188,"n":4,"t":3},"speaker":"B","text":"食事です。食事に招待されたのですが、ほぼ生のカエルが出てきました。"},{"id":{"d":188,"n":5,"t":3},"speaker":"A","text":"生のカエルですか？"},{"id":{"d":188,"n":6,"t":3},"speaker":"B","text":"はい、生のカエルです。現地ではおもてなし料理のようです。"},{"id":{"d":188,"n":7,"t":3},"speaker":"A","text":"なるほど。それを食べましたか？"},{"id":{"d":188,"n":8,"t":3},"speaker":"B","text":"いいえ、食べることはできませんでした。相手の方の気持ちはうれしかったのですが、「ごめんなさい」と謝って、カエルは下げてもらいました。"},{"id":{"d":188,"n":9,"t":3},"speaker":"A","text":"文化が違うといろいろな料理があるのですね。"},{"id":{"d":188,"n":10,"t":3},"speaker":"B","text":"そうですね。あれには本当に驚きました。"}];
INSERT INTO sentence [{"id":{"d":189,"n":1,"t":3},"speaker":"A","text":"フランスに旅行に行くのですが、お薦めの場所はありますか？"},{"id":{"d":189,"n":2,"t":3},"speaker":"B","text":"ルーブル美術館がお薦めです。以前、フランスを旅行した時に訪れたことがあるのですが、『モナ・リザ』が素晴らしかったです。"},{"id":{"d":189,"n":3,"t":3},"speaker":"A","text":"それはすてきですね。私もぜひ行ってみたいと思います。"},{"id":{"d":189,"n":4,"t":3},"speaker":"B","text":"はい、ぜひ足を延ばしてみてください。近くにセーヌ川もあって、周りの景観もとてもきれいです。"},{"id":{"d":189,"n":5,"t":3},"speaker":"A","text":"それはますます楽しみです。教えていただいてありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":190,"n":1,"t":3},"speaker":"A","text":"今週末に東京ディズニーシーへ行くのですが、お土産は何が良いですか？"},{"id":{"d":190,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。もしよろしければお菓子をお願いします。"},{"id":{"d":190,"n":3,"t":3},"speaker":"A","text":"どのようなお菓子が好きですか？"},{"id":{"d":190,"n":4,"t":3},"speaker":"B","text":"チョコレートのお菓子が好きです。クッキーも捨てがたいですね。"},{"id":{"d":190,"n":5,"t":3},"speaker":"A","text":"ディズニーシーのお菓子は何を食べてもおいしいですものね。パッケージもかわいくて本当にすてきです。"},{"id":{"d":190,"n":6,"t":3},"speaker":"B","text":"はい。魅力的なお菓子がたくさんあるので、迷ってしまいます。空き箱を道具入れに使いたいので、かわいいパッケージのお菓子をお願いしてもよいですか？"},{"id":{"d":190,"n":7,"t":3},"speaker":"A","text":"分かりました。チョコレートとクッキーが両方味わえる、かわいいパッケージのお菓子を買ってきます。"},{"id":{"d":190,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":191,"n":1,"t":3},"speaker":"A","text":"飛行機の空席照会をお願いします。"},{"id":{"d":191,"n":2,"t":3},"speaker":"B","text":"はい。出発地と目的地を教えてください。"},{"id":{"d":191,"n":3,"t":3},"speaker":"A","text":"羽田空港から福岡空港までです。"},{"id":{"d":191,"n":4,"t":3},"speaker":"B","text":"往復でよろしいですか？"},{"id":{"d":191,"n":5,"t":3},"speaker":"A","text":"いえ、片道でよいです。"},{"id":{"d":191,"n":6,"t":3},"speaker":"B","text":"出発日はいつでしょうか？"},{"id":{"d":191,"n":7,"t":3},"speaker":"A","text":"来週の土曜日です。"},{"id":{"d":191,"n":8,"t":3},"speaker":"B","text":"何名さまでしょうか？"},{"id":{"d":191,"n":9,"t":3},"speaker":"A","text":"大人2人です。"},{"id":{"d":191,"n":10,"t":3},"speaker":"B","text":"午後出発の便でしたら、まだ空席があります。ご予約をお取りしますか？"},{"id":{"d":191,"n":11,"t":3},"speaker":"A","text":"はい、お願いします。午後の一番早い時間の便でお願いできますか？"},{"id":{"d":191,"n":12,"t":3},"speaker":"B","text":"かしこましました。"}];
INSERT INTO sentence [{"id":{"d":192,"n":1,"t":3},"speaker":"A","text":"サクラが咲き始めましたね。"},{"id":{"d":192,"n":2,"t":3},"speaker":"B","text":"次の日曜日にお花見に行きましょうか？"},{"id":{"d":192,"n":3,"t":3},"speaker":"A","text":"いいですね。どこに行きましょう？"},{"id":{"d":192,"n":4,"t":3},"speaker":"B","text":"混んでいない所がいいですよね。"},{"id":{"d":192,"n":5,"t":3},"speaker":"A","text":"原山公園はどうですか？小さい公園なのであまり混雑しないと思います。"},{"id":{"d":192,"n":6,"t":3},"speaker":"B","text":"確かに、あの公園なら、芝生広場にサクラの木がありますね。"},{"id":{"d":192,"n":7,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":192,"n":8,"t":3},"speaker":"B","text":"いいですね。お昼はお弁当を持っていきましょう。"},{"id":{"d":192,"n":9,"t":3},"speaker":"A","text":"レジャーシートも必要ですね。"}];
INSERT INTO sentence [{"id":{"d":193,"n":1,"t":3},"speaker":"A","text":"だいぶ歩いたので疲れました。"},{"id":{"d":193,"n":2,"t":3},"speaker":"B","text":"そうですね。少し休憩しましょう。"},{"id":{"d":193,"n":3,"t":3},"speaker":"A","text":"近くにお店があるといいのですが。"},{"id":{"d":193,"n":4,"t":3},"speaker":"B","text":"ガイドブックで調べてみます。"},{"id":{"d":193,"n":5,"t":3},"speaker":"A","text":"どうでしたか？"},{"id":{"d":193,"n":6,"t":3},"speaker":"B","text":"次の信号を右に曲がった先に喫茶店があるようです。"},{"id":{"d":193,"n":7,"t":3},"speaker":"A","text":"では、そこで休憩しましょう。"},{"id":{"d":193,"n":8,"t":3},"speaker":"B","text":"ケーキがおいしいと紹介されていますよ。"},{"id":{"d":193,"n":9,"t":3},"speaker":"A","text":"ちょうど甘いものが食べたかったのでうれしいです。"}];
INSERT INTO sentence [{"id":{"d":194,"n":1,"t":3},"speaker":"A","text":"これは、お土産です。よろしかったらどうぞ。"},{"id":{"d":194,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。どこかへ旅行に行かれたのですか？"},{"id":{"d":194,"n":3,"t":3},"speaker":"A","text":"家族で草津に行ってきました。"},{"id":{"d":194,"n":4,"t":3},"speaker":"B","text":"草津は温泉が有名ですね。"},{"id":{"d":194,"n":5,"t":3},"speaker":"A","text":"はい。温泉巡りをしてきました。疲れがとれましたよ。"},{"id":{"d":194,"n":6,"t":3},"speaker":"B","text":"寒くはなかったですか？"},{"id":{"d":194,"n":7,"t":3},"speaker":"A","text":"ここよりは気温が低いですが、温泉に漬かると体の芯から温まったので気になりませんでした。"},{"id":{"d":194,"n":8,"t":3},"speaker":"B","text":"それはいいですね。私も機会があったら行きたいです。"}];
INSERT INTO sentence [{"id":{"d":195,"n":1,"t":3},"speaker":"A","text":"私は世界一周旅行をすることが夢です。"},{"id":{"d":195,"n":2,"t":3},"speaker":"B","text":"憧れますよね。飛行機で行きたいのですか？"},{"id":{"d":195,"n":3,"t":3},"speaker":"A","text":"船旅がいいなと思っています。"},{"id":{"d":195,"n":4,"t":3},"speaker":"B","text":"船で世界一周となると、かなり日数がかかりそうですね。"},{"id":{"d":195,"n":5,"t":3},"speaker":"A","text":"はい。でも豪華客船なら設備が整っているので、長旅も楽しめそうです。"},{"id":{"d":195,"n":6,"t":3},"speaker":"B","text":"テレビで見たことがありますが、高級ホテルのような船がありますよね。"},{"id":{"d":195,"n":7,"t":3},"speaker":"A","text":"はい、それに乗ってみたいのです。いつか必ず行きたいので、少しずつ貯金しています。"},{"id":{"d":195,"n":8,"t":3},"speaker":"B","text":"夢が実現するといいですね。"}];
INSERT INTO sentence [{"id":{"d":196,"n":1,"t":3},"speaker":"A","text":"海外旅行へ行ったことはありますか？"},{"id":{"d":196,"n":2,"t":3},"speaker":"B","text":"はい。イタリアとカナダ、それから韓国に行ったことがあります。"},{"id":{"d":196,"n":3,"t":3},"speaker":"A","text":"どの国が一番印象に残っていますか？"},{"id":{"d":196,"n":4,"t":3},"speaker":"B","text":"カナダです。もう一度行きたいくらいです。"},{"id":{"d":196,"n":5,"t":3},"speaker":"A","text":"どのようなことが印象的でしたか？"},{"id":{"d":196,"n":6,"t":3},"speaker":"B","text":"イエローナイフでオーロラを見たことです。想像していた以上に神秘的で美しかったです。"},{"id":{"d":196,"n":7,"t":3},"speaker":"A","text":"オーロラは見られないこともあると聞いたことがあります。あなたは運が良かったのですね。"},{"id":{"d":196,"n":8,"t":3},"speaker":"B","text":"はい。私は滞在中に2回見ることができたので、とてもラッキーでした。"}];
INSERT INTO sentence [{"id":{"d":197,"n":1,"t":3},"speaker":"A","text":"イチゴ狩りは初めてです。たくさん実っていますね。"},{"id":{"d":197,"n":2,"t":3},"speaker":"B","text":"はい。どれを採るか迷ってしまいますね。"},{"id":{"d":197,"n":3,"t":3},"speaker":"A","text":"私はこれにします。甘くておいしいです。"},{"id":{"d":197,"n":4,"t":3},"speaker":"B","text":"本当に甘いですね。ミルクを付けなくても十分おいしいです。"},{"id":{"d":197,"n":5,"t":3},"speaker":"A","text":"これほどおいしいイチゴが食べ放題とは夢のようです。"},{"id":{"d":197,"n":6,"t":3},"speaker":"B","text":"そうですね。いくらでも食べられそうです。"},{"id":{"d":197,"n":7,"t":3},"speaker":"A","text":"どちらが多く食べられるか競争しましょうか？"},{"id":{"d":197,"n":8,"t":3},"speaker":"B","text":"よいですよ。負ける気がしません。"},{"id":{"d":197,"n":9,"t":3},"speaker":"A","text":"私も負けませんよ。100個くらい食べられそうです。"}];
INSERT INTO sentence [{"id":{"d":198,"n":1,"t":3},"speaker":"A","text":"荷造りをしているのですか？"},{"id":{"d":198,"n":2,"t":3},"speaker":"B","text":"はい。今度、友達と登山に行くので、その準備をしています。"},{"id":{"d":198,"n":3,"t":3},"speaker":"A","text":"登山が趣味なのですね。"},{"id":{"d":198,"n":4,"t":3},"speaker":"B","text":"はい。高校で登山部に所属して以来、趣味になりました。"},{"id":{"d":198,"n":5,"t":3},"speaker":"A","text":"そうですか。今までいろいろな山に登ったことがあるのでしょうね。"},{"id":{"d":198,"n":6,"t":3},"speaker":"B","text":"高校を卒業してからはなかなか登山する機会がなかったので、登山経験はあまり多くありません。"},{"id":{"d":198,"n":7,"t":3},"speaker":"A","text":"では、今回は久しぶりの登山ですか？"},{"id":{"d":198,"n":8,"t":3},"speaker":"B","text":"はい。心配なので、初心者向けの山にしました。"},{"id":{"d":198,"n":9,"t":3},"speaker":"A","text":"賢明な判断です。けがなどしないように気を付けて、楽しんできてくださいね。"},{"id":{"d":198,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":199,"n":1,"t":3},"speaker":"A","text":"飲み物を買いたいので、次のサービスエリアに寄ってもよいですか？"},{"id":{"d":199,"n":2,"t":3},"speaker":"B","text":"はい。私も何か飲みたいです。"},{"id":{"d":199,"n":3,"t":3},"speaker":"A","text":"ガソリンが残り少なくなってきたので、給油も必要ですね。"},{"id":{"d":199,"n":4,"t":3},"speaker":"B","text":"だいぶ長距離を走りましたね。運転を代わりましょうか？"},{"id":{"d":199,"n":5,"t":3},"speaker":"A","text":"少し疲れたので、お願いします。"},{"id":{"d":199,"n":6,"t":3},"speaker":"B","text":"では、次のサービスエリアからは私が運転しますね。"},{"id":{"d":199,"n":7,"t":3},"speaker":"A","text":"助かります。"},{"id":{"d":199,"n":8,"t":3},"speaker":"B","text":"目的地まではあと半分くらいでしょうか？"},{"id":{"d":199,"n":9,"t":3},"speaker":"A","text":"そうですね。渋滞がなければ夕方までには到着できそうです。"}];
INSERT INTO sentence [{"id":{"d":200,"n":1,"t":3},"speaker":"A","text":"チェックインをお願いします。"},{"id":{"d":200,"n":2,"t":3},"speaker":"B","text":"ご予約のお名前をおうかがいできますか？"},{"id":{"d":200,"n":3,"t":3},"speaker":"A","text":"坂下です。"},{"id":{"d":200,"n":4,"t":3},"speaker":"B","text":"坂下さまですね。お部屋は5階にご用意いたしました、こちらが鍵でございます。"},{"id":{"d":200,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。エレベーターはどこにありますか？"},{"id":{"d":200,"n":6,"t":3},"speaker":"B","text":"右手奥にございます。"},{"id":{"d":200,"n":7,"t":3},"speaker":"A","text":"分かりました。それから、明日の朝食は何時から食べられますか？"},{"id":{"d":200,"n":8,"t":3},"speaker":"B","text":"6時半からでございます。"},{"id":{"d":200,"n":9,"t":3},"speaker":"A","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":201,"n":1,"t":3},"speaker":"A","text":"友達と卒業旅行に行くことになりました。"},{"id":{"d":201,"n":2,"t":3},"speaker":"B","text":"どこへ行くのですか？"},{"id":{"d":201,"n":3,"t":3},"speaker":"A","text":"ヨーロッパです。"},{"id":{"d":201,"n":4,"t":3},"speaker":"B","text":"ヨーロッパのどの国にするかは決まりましたか？"},{"id":{"d":201,"n":5,"t":3},"speaker":"A","text":"いいえ、今相談しているところです。"},{"id":{"d":201,"n":6,"t":3},"speaker":"B","text":"旅行は計画する時間も楽しいですよね。"},{"id":{"d":201,"n":7,"t":3},"speaker":"A","text":"はい。インターネットでいろいろ調べながら盛り上がっています。"},{"id":{"d":201,"n":8,"t":3},"speaker":"B","text":"旅行は何日間の予定ですか？"},{"id":{"d":201,"n":9,"t":3},"speaker":"A","text":"10日間の予定です。"},{"id":{"d":201,"n":10,"t":3},"speaker":"B","text":"それなら、複数の国を訪れることもできますね。"},{"id":{"d":201,"n":11,"t":3},"speaker":"A","text":"そうですね、なるべくいろいろな国に行きたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":202,"n":1,"t":3},"speaker":"A","text":"お土産を選んでいるのですか？"},{"id":{"d":202,"n":2,"t":3},"speaker":"B","text":"はい。会社の同じ課の人にお世話になっているので、何か買おうと思います。"},{"id":{"d":202,"n":3,"t":3},"speaker":"A","text":"なるほど。私は妹にお土産を買います。"},{"id":{"d":202,"n":4,"t":3},"speaker":"B","text":"このお菓子はおいしそうですね。"},{"id":{"d":202,"n":5,"t":3},"speaker":"A","text":"そうですね。この地方で昔から食べられていると聞いたことがあります。"},{"id":{"d":202,"n":6,"t":3},"speaker":"B","text":"試食ができるのですね。いただきましょう。"},{"id":{"d":202,"n":7,"t":3},"speaker":"A","text":"一口サイズで食べやすいですね。"},{"id":{"d":202,"n":8,"t":3},"speaker":"B","text":"はい。それに、しっとりとしていておいしいです。私はこれを買います。"},{"id":{"d":202,"n":9,"t":3},"speaker":"A","text":"私も同じものにします。"}];
INSERT INTO sentence [{"id":{"d":203,"n":1,"t":3},"speaker":"A","text":"次はどのアトラクションに乗りますか？"},{"id":{"d":203,"n":2,"t":3},"speaker":"B","text":"ジェットコースターに乗りたいです。"},{"id":{"d":203,"n":3,"t":3},"speaker":"A","text":"40分待ちのようですよ。"},{"id":{"d":203,"n":4,"t":3},"speaker":"B","text":"それなら違うアトラクションにしましょう。"},{"id":{"d":203,"n":5,"t":3},"speaker":"A","text":"観覧車はどうですか？"},{"id":{"d":203,"n":6,"t":3},"speaker":"B","text":"すみません。高い所は苦手なので遠慮しておきます。"},{"id":{"d":203,"n":7,"t":3},"speaker":"A","text":"では、コーヒーカップはどうですか？"},{"id":{"d":203,"n":8,"t":3},"speaker":"B","text":"いいですね。どこにありますか？"},{"id":{"d":203,"n":9,"t":3},"speaker":"A","text":"あちらの方です。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":204,"n":1,"t":3},"speaker":"A","text":"出張、お疲れさまでした。"},{"id":{"d":204,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。時差ボケで眠いです。"},{"id":{"d":204,"n":3,"t":3},"speaker":"A","text":"それはつらいですね。"},{"id":{"d":204,"n":4,"t":3},"speaker":"B","text":"出張先との時差は何時間ですか？"},{"id":{"d":204,"n":5,"t":3},"speaker":"A","text":"14時間です。"},{"id":{"d":204,"n":6,"t":3},"speaker":"B","text":"では、今は夜中の12時くらいに感じているのですね。"},{"id":{"d":204,"n":7,"t":3},"speaker":"A","text":"そういうことになります。"},{"id":{"d":204,"n":8,"t":3},"speaker":"B","text":"今日は早めに退勤してゆっくり休んでください。"},{"id":{"d":204,"n":9,"t":3},"speaker":"A","text":"お気遣いありがとうございます。そうさせていただきます。"}];
INSERT INTO sentence [{"id":{"d":205,"n":1,"t":3},"speaker":"A","text":"遠足のお弁当はどのようなものがいいですか？"},{"id":{"d":205,"n":2,"t":3},"speaker":"B","text":"おにぎりがいいです。"},{"id":{"d":205,"n":3,"t":3},"speaker":"A","text":"具は何にしますか？"},{"id":{"d":205,"n":4,"t":3},"speaker":"B","text":"梅干しと昆布にしてください。"},{"id":{"d":205,"n":5,"t":3},"speaker":"A","text":"分かりました。おかずは鶏の唐揚げにしようと思っています。"},{"id":{"d":205,"n":6,"t":3},"speaker":"B","text":"唐揚げは大好物なのでうれしいです。"},{"id":{"d":205,"n":7,"t":3},"speaker":"A","text":"デザートは果物にしますね。"},{"id":{"d":205,"n":8,"t":3},"speaker":"B","text":"はい。できればイチゴがいいです。"},{"id":{"d":205,"n":9,"t":3},"speaker":"A","text":"分かりました。後で買い物に行くので、イチゴを買ってきますね。"}];
INSERT INTO sentence [{"id":{"d":206,"n":1,"t":3},"speaker":"A","text":"年末年始はどこかへ行きますか？"},{"id":{"d":206,"n":2,"t":3},"speaker":"B","text":"実家へ帰省するつもりです。"},{"id":{"d":206,"n":3,"t":3},"speaker":"A","text":"実家はどちらですか？"},{"id":{"d":206,"n":4,"t":3},"speaker":"B","text":"札幌です。"},{"id":{"d":206,"n":5,"t":3},"speaker":"A","text":"年末年始は、雪が積もっているでしょうね。"},{"id":{"d":206,"n":6,"t":3},"speaker":"B","text":"はい。帰省したら子どもを連れてスキーに行こうと思っています。"},{"id":{"d":206,"n":7,"t":3},"speaker":"A","text":"近くにスキー場があるのですか？"},{"id":{"d":206,"n":8,"t":3},"speaker":"B","text":"実はい。家から車で15分くらいの所にスキー場があります。"},{"id":{"d":206,"n":9,"t":3},"speaker":"A","text":"それはいいですね。楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":207,"n":1,"t":3},"speaker":"A","text":"ツアーを申し込んだ山本ですが、人数の変更をお願いします。"},{"id":{"d":207,"n":2,"t":3},"speaker":"B","text":"山本さまですね。お申し込みいただいたのはどのツアーでしょうか？"},{"id":{"d":207,"n":3,"t":3},"speaker":"A","text":"九州へ2泊3日で行くツアーです。"},{"id":{"d":207,"n":4,"t":3},"speaker":"B","text":"出発日はいつですか？"},{"id":{"d":207,"n":5,"t":3},"speaker":"A","text":"3月20日です。"},{"id":{"d":207,"n":6,"t":3},"speaker":"B","text":"かしこまりました。お調べいたしますので、少々お待ちください。"},{"id":{"d":207,"n":7,"t":3},"speaker":"A","text":"よろしくお願いします。"},{"id":{"d":207,"n":8,"t":3},"speaker":"B","text":"確認できました。人数はどのように変更しますか？"},{"id":{"d":207,"n":9,"t":3},"speaker":"A","text":"2名から3名に増やしたいのですが、大丈夫でしょうか？"},{"id":{"d":207,"n":10,"t":3},"speaker":"B","text":"はい、大丈夫ですよ。3名さまへの人数変更を承りました。"}];
INSERT INTO sentence [{"id":{"d":208,"n":1,"t":3},"speaker":"A","text":"明日から息子が修学旅行へ行きます。"},{"id":{"d":208,"n":2,"t":3},"speaker":"B","text":"どこへ行くのですか？"},{"id":{"d":208,"n":3,"t":3},"speaker":"A","text":"沖縄です。"},{"id":{"d":208,"n":4,"t":3},"speaker":"B","text":"それは楽しそうですね。"},{"id":{"d":208,"n":5,"t":3},"speaker":"A","text":"現地で自由時間が多いらしく、息子はとても楽しみにしています。"},{"id":{"d":208,"n":6,"t":3},"speaker":"B","text":"沖縄だともう海水浴ができるのでしょうか？"},{"id":{"d":208,"n":7,"t":3},"speaker":"A","text":"いいえ、まだ海開きはしていないようです。"},{"id":{"d":208,"n":8,"t":3},"speaker":"B","text":"そうですか。でも、きれいな海は見るだけでもよいですよね。"},{"id":{"d":208,"n":9,"t":3},"speaker":"A","text":"はい、私もうらやましく思っています。"}];
INSERT INTO sentence [{"id":{"d":209,"n":1,"t":3},"speaker":"A","text":"このホテルの近くに免税店はありますか？"},{"id":{"d":209,"n":2,"t":3},"speaker":"B","text":"海岸沿いのショッピングモールの中に、大型の免税店があります。"},{"id":{"d":209,"n":3,"t":3},"speaker":"A","text":"そのショッピングモールへの行き方を教えてください。"},{"id":{"d":209,"n":4,"t":3},"speaker":"B","text":"ホテルからショッピングモールまで無料のシャトルバスがあります。ぜひご利用ください。"},{"id":{"d":209,"n":5,"t":3},"speaker":"A","text":"それは助かります。"},{"id":{"d":209,"n":6,"t":3},"speaker":"B","text":"バスはホテルの車寄せに止まりますので、そこからお乗りください。"},{"id":{"d":209,"n":7,"t":3},"speaker":"A","text":"帰りもショッピングモールからシャトルバスが出ていますか？"},{"id":{"d":209,"n":8,"t":3},"speaker":"B","text":"もちろんです。時刻表をお渡ししますので、少々お待ちください。"},{"id":{"d":209,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":210,"n":1,"t":3},"speaker":"A","text":"そろそろ夜になりますね。ホテルに戻りましょうか？"},{"id":{"d":210,"n":2,"t":3},"speaker":"B","text":"そうですね。朝からいろいろと観光して、歩き疲れました。"},{"id":{"d":210,"n":3,"t":3},"speaker":"A","text":"夕食はどうしましょうか？"},{"id":{"d":210,"n":4,"t":3},"speaker":"B","text":"今からレストランを探すのはおっくうですね。"},{"id":{"d":210,"n":5,"t":3},"speaker":"A","text":"何か食べるものを買って、部屋で食べましょうか？"},{"id":{"d":210,"n":6,"t":3},"speaker":"B","text":"でも、この近くにお店はなさそうですよ。"},{"id":{"d":210,"n":7,"t":3},"speaker":"A","text":"それではホテルでルームサービスをとりましょうか？"},{"id":{"d":210,"n":8,"t":3},"speaker":"B","text":"それがいいですね。"}];
INSERT INTO sentence [{"id":{"d":211,"n":1,"t":3},"speaker":"A","text":"台風が近づいていますね。"},{"id":{"d":211,"n":2,"t":3},"speaker":"B","text":"明日、飛行機に乗る予定なのですが、欠航になってしまうかもしれません。"},{"id":{"d":211,"n":3,"t":3},"speaker":"A","text":"それは心配ですね。旅行ですか？"},{"id":{"d":211,"n":4,"t":3},"speaker":"B","text":"はい。高知へ行くのですが、欠航になったら困ってしまいます。"},{"id":{"d":211,"n":5,"t":3},"speaker":"A","text":"あいにくですね。ニュースで台風の進路を確認してみましょう。"},{"id":{"d":211,"n":6,"t":3},"speaker":"B","text":"はい。どうやら、台風は今夜のうちに日本から離れていくようです。"},{"id":{"d":211,"n":7,"t":3},"speaker":"A","text":"予定通りの飛行機に乗れるといいですね。"},{"id":{"d":211,"n":8,"t":3},"speaker":"B","text":"念のため、旅行会社に状況を問い合わせてみます。"}];
INSERT INTO sentence [{"id":{"d":212,"n":1,"t":3},"speaker":"A","text":"坂本さんはフランスに行ったことがあるそうですね。"},{"id":{"d":212,"n":2,"t":3},"speaker":"B","text":"はい、5年くらい前に旅行で行きました。"},{"id":{"d":212,"n":3,"t":3},"speaker":"A","text":"実は夏休みに友達とフランス旅行を計画しているのですが、フランス語が話せなくても大丈夫でしょうか？"},{"id":{"d":212,"n":4,"t":3},"speaker":"B","text":"大丈夫だと思いますよ。私もフランス語は全く話せません。"},{"id":{"d":212,"n":5,"t":3},"speaker":"A","text":"そうなのですか。旅行中、困ったことはありませんでしたか？"},{"id":{"d":212,"n":6,"t":3},"speaker":"B","text":"特にありませんでした。有名な観光地なら英語が通じるので、英語を話すことができれば大丈夫ですよ。"},{"id":{"d":212,"n":7,"t":3},"speaker":"A","text":"英語なら友達が堪能なので安心しました。"},{"id":{"d":212,"n":8,"t":3},"speaker":"B","text":"私は一応簡単なフランス語会話の本を買ったので、よかったら貸しましょうか？"},{"id":{"d":212,"n":9,"t":3},"speaker":"A","text":"ぜひお借りしたいです。"}];
INSERT INTO sentence [{"id":{"d":213,"n":1,"t":3},"speaker":"A","text":"部屋の空気がとても乾燥しているので、加湿器をお借りすることはできますか？"},{"id":{"d":213,"n":2,"t":3},"speaker":"B","text":"大変申し訳ございませんでした。ご用意いたしますので、少々お待ちください。"},{"id":{"d":213,"n":3,"t":3},"speaker":"A","text":"今から外出するので、部屋に運んでおいてもらえますか？"},{"id":{"d":213,"n":4,"t":3},"speaker":"B","text":"かしこまりました。お部屋はどちらですか？"},{"id":{"d":213,"n":5,"t":3},"speaker":"A","text":"836号室です。"},{"id":{"d":213,"n":6,"t":3},"speaker":"B","text":"836号室ですね。お部屋の前に加湿器をお届けいたします。もし使い方がご不明でしたら、ご連絡ください。"},{"id":{"d":213,"n":7,"t":3},"speaker":"A","text":"分かりました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":214,"n":1,"t":3},"speaker":"A","text":"明日から研修旅行ですね。"},{"id":{"d":214,"n":2,"t":3},"speaker":"B","text":"はい。集合時間が早いので、明日の朝は早起きしなければなりません。"},{"id":{"d":214,"n":3,"t":3},"speaker":"A","text":"家を出るのは何時ですか？"},{"id":{"d":214,"n":4,"t":3},"speaker":"B","text":"6時40分です。"},{"id":{"d":214,"n":5,"t":3},"speaker":"A","text":"それでは朝ご飯は6時でよいですか？"},{"id":{"d":214,"n":6,"t":3},"speaker":"B","text":"はい。早い時間ですみませんが、お願いします。"},{"id":{"d":214,"n":7,"t":3},"speaker":"A","text":"私は早起きが得意なので大丈夫です。"},{"id":{"d":214,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。私は早起きが苦手なので、今夜は早く寝ることにします。"}];
INSERT INTO sentence [{"id":{"d":215,"n":1,"t":3},"speaker":"A","text":"近い将来、宇宙旅行が身近になりそうですね。"},{"id":{"d":215,"n":2,"t":3},"speaker":"B","text":"あなたは宇宙へ行ってみたいですか？"},{"id":{"d":215,"n":3,"t":3},"speaker":"A","text":"はい。宇宙から地球を見てみたいです。"},{"id":{"d":215,"n":4,"t":3},"speaker":"B","text":"私も見てみたいのですが、実際には行かないと思います。"},{"id":{"d":215,"n":5,"t":3},"speaker":"A","text":"どうしてですか？"},{"id":{"d":215,"n":6,"t":3},"speaker":"B","text":"無重力がどのような状態なのか分からないので怖いのです。"},{"id":{"d":215,"n":7,"t":3},"speaker":"A","text":"私は無重力でいろいろと試してみたいことがあります。"},{"id":{"d":215,"n":8,"t":3},"speaker":"B","text":"好奇心があっていいですね。"},{"id":{"d":215,"n":9,"t":3},"speaker":"A","text":"手軽に宇宙に行ける日が早くきてほしいです。"}];
INSERT INTO sentence [{"id":{"d":216,"n":1,"t":3},"speaker":"A","text":"兄がサッカーの全国大会に出場することになりました。"},{"id":{"d":216,"n":2,"t":3},"speaker":"B","text":"素晴らしいですね。おめでとうございます。"},{"id":{"d":216,"n":3,"t":3},"speaker":"A","text":"一生懸命練習していたので、努力が報われて良かったです。"},{"id":{"d":216,"n":4,"t":3},"speaker":"B","text":"大会を見に行くのですか？"},{"id":{"d":216,"n":5,"t":3},"speaker":"A","text":"はい。家族全員で応援に行く予定です。"},{"id":{"d":216,"n":6,"t":3},"speaker":"B","text":"どこで開催されるのですか？"},{"id":{"d":216,"n":7,"t":3},"speaker":"A","text":"東京です。ちょうど夏休み中なので都合が良いです。"},{"id":{"d":216,"n":8,"t":3},"speaker":"B","text":"それはよかったですね。お兄さんが所属するチームが、勝ち進めるといいですね。"},{"id":{"d":216,"n":9,"t":3},"speaker":"A","text":"はい。精いっぱい応援してきます。"}];
INSERT INTO sentence [{"id":{"d":217,"n":1,"t":3},"speaker":"A","text":"私は釣りが趣味です。"},{"id":{"d":217,"n":2,"t":3},"speaker":"B","text":"海釣りですか？"},{"id":{"d":217,"n":3,"t":3},"speaker":"A","text":"はい。明日も仲間と釣りに行きます。"},{"id":{"d":217,"n":4,"t":3},"speaker":"B","text":"船を借りるのですか？"},{"id":{"d":217,"n":5,"t":3},"speaker":"A","text":"釣り仲間に船を持っている人がいるので、彼の船に乗せてもらいます。"},{"id":{"d":217,"n":6,"t":3},"speaker":"B","text":"釣りというと、朝が早いのでしょうね。"},{"id":{"d":217,"n":7,"t":3},"speaker":"A","text":"はい。夜明け前には出航します。"},{"id":{"d":217,"n":8,"t":3},"speaker":"B","text":"早起きするのは大変ではありませんか？"},{"id":{"d":217,"n":9,"t":3},"speaker":"A","text":"好きなことのための早起きなので、全く苦になりません。"}];
INSERT INTO sentence [{"id":{"d":218,"n":1,"t":3},"speaker":"A","text":"紅葉が美しいですね。"},{"id":{"d":218,"n":2,"t":3},"speaker":"B","text":"ええ。時期がちょうど良かったですね。"},{"id":{"d":218,"n":3,"t":3},"speaker":"A","text":"どこかで写真を撮りましょうか？"},{"id":{"d":218,"n":4,"t":3},"speaker":"B","text":"そうしましょう。あの湖はどうでしょうか？"},{"id":{"d":218,"n":5,"t":3},"speaker":"A","text":"いいですね。橋が架かっていますから橋の上で撮りましょう。"},{"id":{"d":218,"n":6,"t":3},"speaker":"B","text":"交代で撮影しますか？"},{"id":{"d":218,"n":7,"t":3},"speaker":"A","text":"せっかくなので一緒に写りましょう。"},{"id":{"d":218,"n":8,"t":3},"speaker":"B","text":"それでは、あちらのご夫婦に撮ってもらいましょうか？"},{"id":{"d":218,"n":9,"t":3},"speaker":"A","text":"そうしましょう。私がお願いしてきますね。"}];
INSERT INTO sentence [{"id":{"d":219,"n":1,"t":3},"speaker":"A","text":"旅先で宿泊するなら、ホテルと旅館のどちらがいいですか？"},{"id":{"d":219,"n":2,"t":3},"speaker":"B","text":"私は旅館がいいです。いつも旅館に泊まるようにしています。あなたはどうですか？"},{"id":{"d":219,"n":3,"t":3},"speaker":"A","text":"私はホテルが好きです。駅近くなどのアクセスの良い場所にあるのでよく利用します。"},{"id":{"d":219,"n":4,"t":3},"speaker":"B","text":"旅館だと大浴場があってのんびりできますよ。部屋に食事を運んでくれるのも楽です。"},{"id":{"d":219,"n":5,"t":3},"speaker":"A","text":"それは魅力的ですが、ベッドで寝たいのでホテルがいいです。アメニティーが充実しているなど、機能的なところも好ましいです。"},{"id":{"d":219,"n":6,"t":3},"speaker":"B","text":"確かにアメニティーはホテルの方が充実していそうです。"}];
INSERT INTO sentence [{"id":{"d":220,"n":1,"t":3},"speaker":"A","text":"このツアーは自由時間が多いですね。"},{"id":{"d":220,"n":2,"t":3},"speaker":"B","text":"はい。自由時間には、お好みに応じてオプショナルツアーをご提案させていただきますよ。"},{"id":{"d":220,"n":3,"t":3},"speaker":"A","text":"どのようなオプショナルツアーがありますか？"},{"id":{"d":220,"n":4,"t":3},"speaker":"B","text":"こちらに一覧表がございます。"},{"id":{"d":220,"n":5,"t":3},"speaker":"A","text":"たくさんありますね。お薦めはどれですか？"},{"id":{"d":220,"n":6,"t":3},"speaker":"B","text":"イルカを見に行くツアーが人気です。"},{"id":{"d":220,"n":7,"t":3},"speaker":"A","text":"楽しそうですね。時間はどのくらいかかりますか？"},{"id":{"d":220,"n":8,"t":3},"speaker":"B","text":"朝9時に出発して、3時頃に解散となります。"},{"id":{"d":220,"n":9,"t":3},"speaker":"A","text":"それではこちらの申し込みをお願いします。"},{"id":{"d":220,"n":10,"t":3},"speaker":"B","text":"承知しました。ご希望の日にちを教えてください。"}];
INSERT INTO sentence [{"id":{"d":221,"n":1,"t":3},"speaker":"A","text":"最近車を運転していませんので、ドライブに行きたいです。"},{"id":{"d":221,"n":2,"t":3},"speaker":"B","text":"私も行きたいです。一緒に行きましょう。"},{"id":{"d":221,"n":3,"t":3},"speaker":"A","text":"いいですね。海を見ながら運転したいです。"},{"id":{"d":221,"n":4,"t":3},"speaker":"B","text":"海岸沿いの道があるといいですね。国道30号なら海の近くを走っていますよね。"},{"id":{"d":221,"n":5,"t":3},"speaker":"A","text":"そうですね。景色が良さそうです。"},{"id":{"d":221,"n":6,"t":3},"speaker":"B","text":"次の週末に行きましょうか。ご都合はいかがですか？"},{"id":{"d":221,"n":7,"t":3},"speaker":"A","text":"大丈夫です。私の車で迎えに行きますね。"},{"id":{"d":221,"n":8,"t":3},"speaker":"B","text":"どうもありがとうございます。私の家の場所はご存じですよね。"},{"id":{"d":221,"n":9,"t":3},"speaker":"A","text":"はい。以前にお邪魔したことがあるので場所は分かります。迎えに行く時間は何時にしましょうか？"},{"id":{"d":221,"n":10,"t":3},"speaker":"B","text":"8時くらいに来ていただけますか？"},{"id":{"d":221,"n":11,"t":3},"speaker":"A","text":"分かりました。当日が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":222,"n":1,"t":3},"speaker":"A","text":"潮干狩りの季節だとニュースで報じられていました。"},{"id":{"d":222,"n":2,"t":3},"speaker":"B","text":"去年もこの時期に潮干狩りに行きましたね。"},{"id":{"d":222,"n":3,"t":3},"speaker":"A","text":"たくさんとれましたよね。"},{"id":{"d":222,"n":4,"t":3},"speaker":"B","text":"ボンゴレにしたり、あさりご飯にしたり、おいしかったですね。"},{"id":{"d":222,"n":5,"t":3},"speaker":"A","text":"今年も行きましょうか？"},{"id":{"d":222,"n":6,"t":3},"speaker":"B","text":"去年と同じ場所に行きますか？"},{"id":{"d":222,"n":7,"t":3},"speaker":"A","text":"遠くて時間がかかったのでもう少し近場がいいです。"},{"id":{"d":222,"n":8,"t":3},"speaker":"B","text":"近くに潮干狩りできる場所があるかどうか調べてみます。"}];
INSERT INTO sentence [{"id":{"d":223,"n":1,"t":3},"speaker":"A","text":"これは外国のお金ですか？"},{"id":{"d":223,"n":2,"t":3},"speaker":"B","text":"はい。カナダドルです。"},{"id":{"d":223,"n":3,"t":3},"speaker":"A","text":"どうしてカナダドルを持っているのですか？"},{"id":{"d":223,"n":4,"t":3},"speaker":"B","text":"大学生の頃にカナダに旅行に行って、記念にとっておいたのです。"},{"id":{"d":223,"n":5,"t":3},"speaker":"A","text":"そうですか。紙幣もあるのですか？"},{"id":{"d":223,"n":6,"t":3},"speaker":"B","text":"紙幣はありません。紙幣は全て日本円に両替してしまいました。"},{"id":{"d":223,"n":7,"t":3},"speaker":"A","text":"どこで両替できるのですか？"},{"id":{"d":223,"n":8,"t":3},"speaker":"B","text":"私は空港で両替しました。"}];
INSERT INTO sentence [{"id":{"d":224,"n":1,"t":3},"speaker":"A","text":"この写真はどこで撮ったのですか？"},{"id":{"d":224,"n":2,"t":3},"speaker":"B","text":"オーストラリアのシドニーです。"},{"id":{"d":224,"n":3,"t":3},"speaker":"A","text":"何年前に行ったのですか？"},{"id":{"d":224,"n":4,"t":3},"speaker":"B","text":"10年前です。新婚旅行で行きました。"},{"id":{"d":224,"n":5,"t":3},"speaker":"A","text":"すてきな写真ですね。私も新婚旅行は外国に行けば良かったと後悔しています。"},{"id":{"d":224,"n":6,"t":3},"speaker":"B","text":"国内旅行にしたのですか？"},{"id":{"d":224,"n":7,"t":3},"speaker":"A","text":"はい。夫の希望で国内旅行にしました。"},{"id":{"d":224,"n":8,"t":3},"speaker":"B","text":"今度、夫婦で海外旅行に行ってみてはどうですか？"},{"id":{"d":224,"n":9,"t":3},"speaker":"A","text":"そうですね。夫に提案してみます。"}];
INSERT INTO sentence [{"id":{"d":225,"n":1,"t":3},"speaker":"A","text":"新幹線の出発は何時ですか？"},{"id":{"d":225,"n":2,"t":3},"speaker":"B","text":"10時半です。"},{"id":{"d":225,"n":3,"t":3},"speaker":"A","text":"出発まであと20分ありますね。"},{"id":{"d":225,"n":4,"t":3},"speaker":"B","text":"20分あればお弁当が買えそうです。お弁当を買って、車内で食べましょうか？"},{"id":{"d":225,"n":5,"t":3},"speaker":"A","text":"そうですね。そこに売店がありますよ。"},{"id":{"d":225,"n":6,"t":3},"speaker":"B","text":"どれもおいしそうですね。"},{"id":{"d":225,"n":7,"t":3},"speaker":"A","text":"はい、どれにするか迷ってしまいます。"},{"id":{"d":225,"n":8,"t":3},"speaker":"B","text":"飲み物も買いますか？"},{"id":{"d":225,"n":9,"t":3},"speaker":"A","text":"はい。私はお茶にします。"}];
INSERT INTO sentence [{"id":{"d":226,"n":1,"t":3},"speaker":"A","text":"大学生になったら、留学をしたいです。"},{"id":{"d":226,"n":2,"t":3},"speaker":"B","text":"行きたい国は決まっているのですか？"},{"id":{"d":226,"n":3,"t":3},"speaker":"A","text":"イギリスです。"},{"id":{"d":226,"n":4,"t":3},"speaker":"B","text":"どうしてイギリスに行きたいのですか？"},{"id":{"d":226,"n":5,"t":3},"speaker":"A","text":"英語の先生がイギリスに留学した時の経験談を聞いて、興味を持ちました。"},{"id":{"d":226,"n":6,"t":3},"speaker":"B","text":"そうでしたか。留学してどのようなことを勉強したいのですか？"},{"id":{"d":226,"n":7,"t":3},"speaker":"A","text":"異文化コミュニケーションについて勉強したいです。"},{"id":{"d":226,"n":8,"t":3},"speaker":"B","text":"実現するといいですね。応援していますよ。"}];
INSERT INTO sentence [{"id":{"d":227,"n":1,"t":3},"speaker":"A","text":"良い天気ですね。ピクニックに行きましょうか？"},{"id":{"d":227,"n":2,"t":3},"speaker":"B","text":"はい。せっかくなので、何か遊ぶ物も持っていきましょう。"},{"id":{"d":227,"n":3,"t":3},"speaker":"A","text":"フリスビーがありますよ。"},{"id":{"d":227,"n":4,"t":3},"speaker":"B","text":"いいですね。バドミントンのラケットも持っていきましょう。"},{"id":{"d":227,"n":5,"t":3},"speaker":"A","text":"ラケットはどこにありますか？"},{"id":{"d":227,"n":6,"t":3},"speaker":"B","text":"物置にあると思います。"},{"id":{"d":227,"n":7,"t":3},"speaker":"A","text":"バドミントンの羽根も同じ場所にありますか？"},{"id":{"d":227,"n":8,"t":3},"speaker":"B","text":"もしあればそこに置いてあると思いますが、前に遊んだ時になくしてしまったかもしれません。"},{"id":{"d":227,"n":9,"t":3},"speaker":"A","text":"では、行く途中で羽根を買って行きましょう。"}];
INSERT INTO sentence [{"id":{"d":228,"n":1,"t":3},"speaker":"A","text":"先ほど、旅行中の外国の人に道を聞かれました。"},{"id":{"d":228,"n":2,"t":3},"speaker":"B","text":"どの国の人ですか？"},{"id":{"d":228,"n":3,"t":3},"speaker":"A","text":"ニュージーランドから来たそうです。"},{"id":{"d":228,"n":4,"t":3},"speaker":"B","text":"英語で聞かれたのですか？"},{"id":{"d":228,"n":5,"t":3},"speaker":"A","text":"いいえ、とても上手な日本語でした。"},{"id":{"d":228,"n":6,"t":3},"speaker":"B","text":"その人はどこに行こうとしていたのですか？"},{"id":{"d":228,"n":7,"t":3},"speaker":"A","text":"駅に行きたかったようなので、一緒に歩いて案内しました。"},{"id":{"d":228,"n":8,"t":3},"speaker":"B","text":"親切ですね。"},{"id":{"d":228,"n":9,"t":3},"speaker":"A","text":"道中いろいろな話ができて楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":229,"n":1,"t":3},"speaker":"A","text":"今夜の花火大会は混雑しそうですね。"},{"id":{"d":229,"n":2,"t":3},"speaker":"B","text":"はい。毎年、ニュースになるほどの人出ですね。"},{"id":{"d":229,"n":3,"t":3},"speaker":"A","text":"私たちは、どこから花火を見ましょうか？"},{"id":{"d":229,"n":4,"t":3},"speaker":"B","text":"あまり混まない所で見たいですね。"},{"id":{"d":229,"n":5,"t":3},"speaker":"A","text":"そうですね。少し離れた所に川があるので、川岸から見ましょうか？"},{"id":{"d":229,"n":6,"t":3},"speaker":"B","text":"良さそうですね。川岸に遊歩道はありますか？"},{"id":{"d":229,"n":7,"t":3},"speaker":"A","text":"はい。遊歩道を歩きながら花火を見ましょう。"},{"id":{"d":229,"n":8,"t":3},"speaker":"B","text":"その川への行き方は分かりますか？"},{"id":{"d":229,"n":9,"t":3},"speaker":"A","text":"一度行ったことがあるので分かると思いますが、一応、確認しておきます。"}];
INSERT INTO sentence [{"id":{"d":230,"n":1,"t":3},"speaker":"A","text":"これはあなたの自転車ですか？"},{"id":{"d":230,"n":2,"t":3},"speaker":"B","text":"はい。サイクリング用の自転車です。"},{"id":{"d":230,"n":3,"t":3},"speaker":"A","text":"格好良いですね。サイクリングが好きなのですか？"},{"id":{"d":230,"n":4,"t":3},"speaker":"B","text":"はい。週末はだいたいサイクリングに出かけるほど、好きです。"},{"id":{"d":230,"n":5,"t":3},"speaker":"A","text":"サイクリングへは1人で行くのですか？"},{"id":{"d":230,"n":6,"t":3},"speaker":"B","text":"サイクリングのサークルに入っているので、そのメンバーと一緒に行くことが多いです。"},{"id":{"d":230,"n":7,"t":3},"speaker":"A","text":"健康的で良い趣味ですね。"},{"id":{"d":230,"n":8,"t":3},"speaker":"B","text":"そうですね。足の筋肉がよく鍛えられますよ。"}];
INSERT INTO sentence [{"id":{"d":231,"n":1,"t":3},"speaker":"A","text":"今度の夏休みに、一緒に北海道へ行きませんか？"},{"id":{"d":231,"n":2,"t":3},"speaker":"B","text":"いいですね。あなたは、北海道へ行くのは初めてですか？"},{"id":{"d":231,"n":3,"t":3},"speaker":"A","text":"はい、初めてです。ずっと行きたいと思っていました。佐藤さんは北海道に行ったことはありますか？"},{"id":{"d":231,"n":4,"t":3},"speaker":"B","text":"5年くらい前に行きましたが、その時はスキーが目的だったので、観光はしていません。"},{"id":{"d":231,"n":5,"t":3},"speaker":"A","text":"そうですか。それでは今回は観光名所をいろいろと見て回りたいですね。"},{"id":{"d":231,"n":6,"t":3},"speaker":"B","text":"北海道は広いし観光名所がたくさんありますよ。どこか行きたい所はありますか？"},{"id":{"d":231,"n":7,"t":3},"speaker":"A","text":"迷いますが、旭山動物園に行ってみたいです。"},{"id":{"d":231,"n":8,"t":3},"speaker":"B","text":"いいですね。私も行きたいです。あと、確か夜景が有名な場所がありましたよね。"},{"id":{"d":231,"n":9,"t":3},"speaker":"A","text":"夜景というと、函館でしょうか？"},{"id":{"d":231,"n":10,"t":3},"speaker":"B","text":"そうです、函館です。旭山動物園と函館はどのくらい離れていますか？1日で回りきれるでしょうか？"},{"id":{"d":231,"n":11,"t":3},"speaker":"A","text":"どうでしょうか。私もよく分からないので、インターネットやガイドブックでいろいろと調べましょう。"},{"id":{"d":231,"n":12,"t":3},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":232,"n":1,"t":3},"speaker":"A","text":"今日泊まるホテルは夕食が豪華だそうですね。"},{"id":{"d":232,"n":2,"t":3},"speaker":"B","text":"そのようです。とてもおいしそうな夕食の写真が、ホテルのホームページに掲載されていました。"},{"id":{"d":232,"n":3,"t":3},"speaker":"A","text":"でも、これからイチゴ狩りへ行く予定ですよね。イチゴをおなかいっぱい食べてしまったら、夕食を食べられなくなってしまいそうです。"},{"id":{"d":232,"n":4,"t":3},"speaker":"B","text":"そうですね。でも、せっかくイチゴ狩りへ行くのに、イチゴを控えめ食べるのも残念ですよね。"},{"id":{"d":232,"n":5,"t":3},"speaker":"A","text":"はい。そこで、提案なのですが、今日と明日の予定を入れ替えるのはどうでしょうか？"},{"id":{"d":232,"n":6,"t":3},"speaker":"B","text":"イチゴ狩りを明日に変更するということでしょうか？もしそれができるのなら、そうしたいです。"},{"id":{"d":232,"n":7,"t":3},"speaker":"A","text":"イチゴ狩りの農園に電話して、日程を変更できるか問い合わせてみます。"},{"id":{"d":232,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。変更できるといいですね。"}];
INSERT INTO sentence [{"id":{"d":233,"n":1,"t":3},"speaker":"A","text":"旅行に行くならどこへ行きたいですか？"},{"id":{"d":233,"n":2,"t":3},"speaker":"B","text":"京都に行きたいです。"},{"id":{"d":233,"n":3,"t":3},"speaker":"A","text":"京都ですか。大人たちの旅行先として人気がありますね。"},{"id":{"d":233,"n":4,"t":3},"speaker":"B","text":"そうですね。歴史的な街並みや建物は大人好みなのかもしれません。"},{"id":{"d":233,"n":5,"t":3},"speaker":"A","text":"京都は修学旅行で行ったことがありますが、当時はあまり京都という土地に魅力を感じていなかった気がします。"},{"id":{"d":233,"n":6,"t":3},"speaker":"B","text":"私もそうでした。"},{"id":{"d":233,"n":7,"t":3},"speaker":"A","text":"友達と一緒に旅行をするということが、とにかく楽しかった記憶があります。"},{"id":{"d":233,"n":8,"t":3},"speaker":"B","text":"その通りですね。どこを観光したかもあまり覚えていませんが、友達とおそろいで買ったキーホルダーは覚えています。"},{"id":{"d":233,"n":9,"t":3},"speaker":"A","text":"最近になって京都の土地や文化そのものに興味が湧いてきたので、改めて旅行にいきたいと思っています。"},{"id":{"d":233,"n":10,"t":3},"speaker":"B","text":"私も大人になってから、京都の魅力に気づきました。"}];
INSERT INTO sentence [{"id":{"d":234,"n":1,"t":3},"speaker":"A","text":"海外旅行で食事に困ったことはありますか？"},{"id":{"d":234,"n":2,"t":3},"speaker":"B","text":"あります。新婚旅行で南ヨーロッパを巡ったのですが、そこで少し困ったことがありました。"},{"id":{"d":234,"n":3,"t":3},"speaker":"A","text":"観光地も多く、食事もすてきな土地というイメージですが、何に困ったのですか？"},{"id":{"d":234,"n":4,"t":3},"speaker":"B","text":"あの辺りの国はどこもオリーブオイルをたくさん使う文化なのですよ。"},{"id":{"d":234,"n":5,"t":3},"speaker":"A","text":"そうですね。じゃぶじゃぶと使いますね。"},{"id":{"d":234,"n":6,"t":3},"speaker":"B","text":"最初の2、3日はよかったのですが、あまりにも多過ぎてだんだん胃がもたれてきてしまいました。"},{"id":{"d":234,"n":7,"t":3},"speaker":"A","text":"私はオリーブオイルが大好きなのでうらやましいくらいですが、確かに毎食となると話は違うかもしれませんね。"},{"id":{"d":234,"n":8,"t":3},"speaker":"B","text":"私もそれまでは好きな方だと思っていたのです。"},{"id":{"d":234,"n":9,"t":3},"speaker":"A","text":"新婚旅行で新しい自分を発見したのですね。"},{"id":{"d":234,"n":10,"t":3},"speaker":"B","text":"そうかもしれません。それ以来、オリーブオイルが苦手になってしまいました。"}];
INSERT INTO sentence [{"id":{"d":235,"n":1,"t":3},"speaker":"A","text":"日本国内で行ってみたい所はありますか？"},{"id":{"d":235,"n":2,"t":3},"speaker":"B","text":"北海道に行ってみたいです。実は、まだ一度も行ったことがありません。"},{"id":{"d":235,"n":3,"t":3},"speaker":"A","text":"人気がある観光地ですよね。なぜ二の足を踏んでいるのですか？"},{"id":{"d":235,"n":4,"t":3},"speaker":"B","text":"行くならいろいろな所を訪れたいのですが、私は車の免許を持っていません。北海道はとても広いので、多くの場所を訪れたいなら車での観光がお薦めと聞いたことがあり、運転ができないうちに行っても楽しめるか不安だったのです。"},{"id":{"d":235,"n":5,"t":3},"speaker":"A","text":"北海道は広くて電車の本数は少ないですし、バスに至っては走っていない期間もありますからね。"},{"id":{"d":235,"n":6,"t":3},"speaker":"B","text":"しかも、電車の時刻表を使って日程を組むと、移動に時間がかかるので、数日の旅行というわけにはいかなくなるのです。"},{"id":{"d":235,"n":7,"t":3},"speaker":"A","text":"私は、北海道全体を周遊するのではなく函館近辺、小樽近辺とスポット的に旅行しています。それでも十分楽しめますよ。"},{"id":{"d":235,"n":8,"t":3},"speaker":"B","text":"そうなのですね。それなら車がなくても十分に観光できそうです。"}];
INSERT INTO sentence [{"id":{"d":236,"n":1,"t":3},"speaker":"A","text":"大学生になったら何がしたいですか？"},{"id":{"d":236,"n":2,"t":3},"speaker":"B","text":"外国に行ってみたいです。"},{"id":{"d":236,"n":3,"t":3},"speaker":"A","text":"留学したいということですか？"},{"id":{"d":236,"n":4,"t":3},"speaker":"B","text":"そこまでは考えていません。留学となると語学の勉強をしなければならず、それは大変そうです。"},{"id":{"d":236,"n":5,"t":3},"speaker":"A","text":"具体的にどこの国に行きたいですか？"},{"id":{"d":236,"n":6,"t":3},"speaker":"B","text":"スペインには何カ所か行ってみたい所があります。"},{"id":{"d":236,"n":7,"t":3},"speaker":"A","text":"そういえば、君はサッカーファンでしたね。"},{"id":{"d":236,"n":8,"t":3},"speaker":"B","text":"ご明察です。イングランドやドイツも良いですが、スペインは気候が温暖ですし、観客も他の国に比べればおとなしいと思うので、日本人でも観戦しやすいかと思います。"},{"id":{"d":236,"n":9,"t":3},"speaker":"A","text":"そういうことなら僕も誘ってください。一度エル・クラシコを見たいです。"},{"id":{"d":236,"n":10,"t":3},"speaker":"B","text":"誘うのは構いませんが、レアル・マドリードとバルセロナではそう簡単にチケットは取れないでしょうね。"},{"id":{"d":236,"n":11,"t":3},"speaker":"A","text":"そうなのですか。それは残念です。"}];
INSERT INTO sentence [{"id":{"d":237,"n":1,"t":3},"speaker":"A","text":"すみません。県立美術館への行き方を教えてください。"},{"id":{"d":237,"n":2,"t":3},"speaker":"B","text":"8番線のバスに乗って、県立公園前で降りてください。"},{"id":{"d":237,"n":3,"t":3},"speaker":"A","text":"どれくらいバスに乗るのでしょうか？"},{"id":{"d":237,"n":4,"t":3},"speaker":"B","text":"15分ほどです。混んでいる時はもう少しかかります。"},{"id":{"d":237,"n":5,"t":3},"speaker":"A","text":"バス停から県立美術館へはすぐですか？"},{"id":{"d":237,"n":6,"t":3},"speaker":"B","text":"いいえ。5分ほど歩きますが、バス停の近くにある大きな建物は1つだけですし、標識がありますので迷うことはないでしょう。"},{"id":{"d":237,"n":7,"t":3},"speaker":"A","text":"それなら安心です。バスの運賃はいくらですか？"},{"id":{"d":237,"n":8,"t":3},"speaker":"B","text":"200円です。次のバスがもうすぐ来ますので、こちらのバス停に並んでお待ちください。"},{"id":{"d":237,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":238,"n":1,"t":3},"speaker":"A","text":"夏休みはどこかに行きましたか？"},{"id":{"d":238,"n":2,"t":3},"speaker":"B","text":"聞いたらきっと驚くと思いますよ。NASAのジョンソン宇宙センターに行ってきたのです。"},{"id":{"d":238,"n":3,"t":3},"speaker":"A","text":"アメリカのNASAのことですか？なぜ行くことになったのですか？"},{"id":{"d":238,"n":4,"t":3},"speaker":"B","text":"叔母さん一家がヒューストンに住んでいて、誘ってくれたのです。"},{"id":{"d":238,"n":5,"t":3},"speaker":"A","text":"ヒューストンへは、あなた1人で行ったのですか？"},{"id":{"d":238,"n":6,"t":3},"speaker":"B","text":"いいえ。お父さんとお母さんは仕事だったので、姉と2人で行ってきました。"},{"id":{"d":238,"n":7,"t":3},"speaker":"A","text":"姉妹で旅行とは楽しそうですね。NASAの中にも入れたのですか？"},{"id":{"d":238,"n":8,"t":3},"speaker":"B","text":"はい。叔母さんの夫がNASAの職員なので、連れて行ってくれました。"},{"id":{"d":238,"n":9,"t":3},"speaker":"A","text":"NASAには誰でも入れるものなのですか？"},{"id":{"d":238,"n":10,"t":3},"speaker":"B","text":"観光用スペースもあるので誰でも入れますよ。宇宙食も売っていましたし、飛行管制室も見られましたよ。"}];
INSERT INTO sentence [{"id":{"d":239,"n":1,"t":3},"speaker":"A","text":"外国人の知り合いが日本に来ています。案内を頼まれているのですが、どこか良い所を知りませんか？"},{"id":{"d":239,"n":2,"t":3},"speaker":"B","text":"テレビ番組で見ましたが、相撲部屋の朝稽古の見学ツアーがあるようです。日本らしいですし、面白いと思ってくれるのではないでしょうか。"},{"id":{"d":239,"n":3,"t":3},"speaker":"A","text":"面白そうですね。具体的な内容を教えてください。"},{"id":{"d":239,"n":4,"t":3},"speaker":"B","text":"朝稽古を見たり、力士と相撲を取ったり、ちゃんこ鍋を食べたりしていましたよ。"},{"id":{"d":239,"n":5,"t":3},"speaker":"A","text":"彼は興味を持ってくれそうです。朝稽古は何時から始まりますか？"},{"id":{"d":239,"n":6,"t":3},"speaker":"B","text":"見学時間は7時頃から始まるそうです。ちょっと早いでしょうか？"},{"id":{"d":239,"n":7,"t":3},"speaker":"A","text":"彼は時差ボケで昼眠くなるそうですから、むしろちょうどいいかもしれません。"},{"id":{"d":239,"n":8,"t":3},"speaker":"B","text":"詳細がインターネットに出ているはずなので、そのサイトで他のツアーもついでに見ておくといいと思います。"},{"id":{"d":239,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。早速見てみます。"}];
INSERT INTO sentence [{"id":{"d":240,"n":1,"t":3},"speaker":"A","text":"すみません。ヒースロー空港への到着は何時と言っていましたか？"},{"id":{"d":240,"n":2,"t":3},"speaker":"B","text":"9時50分だそうです。"},{"id":{"d":240,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。アナウンスを聞き逃してしまったので、助かりました。"},{"id":{"d":240,"n":4,"t":3},"speaker":"B","text":"どういたしまして。お若いようですが、1人で外国旅行なんてあなたはしっかりしていますね。"},{"id":{"d":240,"n":5,"t":3},"speaker":"A","text":"ヒースロー空港では叔父さんが迎えに来てくれます。僕はただ乗って降りるだけですよ。"},{"id":{"d":240,"n":6,"t":3},"speaker":"B","text":"それでも簡単なことではありませんよ。到着口では英語を話さなくてはならないでしょう。"},{"id":{"d":240,"n":7,"t":3},"speaker":"A","text":"それもそうですね。父や母と一緒に何度か行ったことがありますので、それは大丈夫だと思います。"},{"id":{"d":240,"n":8,"t":3},"speaker":"B","text":"私よりも旅慣れしているかもしれませんね。"},{"id":{"d":240,"n":9,"t":3},"speaker":"A","text":"あなたはロンドンには仕事で行くのですか？"},{"id":{"d":240,"n":10,"t":3},"speaker":"B","text":"はい。ロンドンで乗り換えてリーズに向かいます。"},{"id":{"d":240,"n":11,"t":3},"speaker":"A","text":"お仕事は何をなさっているのですか？"},{"id":{"d":240,"n":12,"t":3},"speaker":"B","text":"研究者です。リーズの大学が私の職場です。"}];
INSERT INTO sentence [{"id":{"d":241,"n":1,"t":3},"speaker":"A","text":"お久しぶりです。しばらく会えませんでしたが、どこかに行っていたのですか？"},{"id":{"d":241,"n":2,"t":3},"speaker":"B","text":"昨日まで旅行でスコットランドに行っていたのです。"},{"id":{"d":241,"n":3,"t":3},"speaker":"A","text":"そうだったのですね。ネス湖のある所ですか？"},{"id":{"d":241,"n":4,"t":3},"speaker":"B","text":"残念ながらそこまでは行っていません。"},{"id":{"d":241,"n":5,"t":3},"speaker":"A","text":"そうですか。何をしてきたのですか？"},{"id":{"d":241,"n":6,"t":3},"speaker":"B","text":"グラスゴーでセルティックの試合を見てきたのです。"},{"id":{"d":241,"n":7,"t":3},"speaker":"A","text":"この間の試合に行ったのですか？それなら私もニュースで見ました。日本人選手も何人か出場していましたね。"},{"id":{"d":241,"n":8,"t":3},"speaker":"B","text":"はい。彼らもたくさん点を決めてくれましたし、応援しているチームが勝ってくれてうれしかったです。"},{"id":{"d":241,"n":9,"t":3},"speaker":"A","text":"それはよかったですね。レプリカユニフォームは買いましたか？"},{"id":{"d":241,"n":10,"t":3},"speaker":"B","text":"もちろんです。今度会ったらお見せしますね。楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":242,"n":1,"t":3},"speaker":"A","text":"叔父さんとばかり出かけていないで、今度は私とパリに行ってみませんか？"},{"id":{"d":242,"n":2,"t":3},"speaker":"B","text":"ぜひとも行きたいです。でも、どうやって行くのですか？"},{"id":{"d":242,"n":3,"t":3},"speaker":"A","text":"ユーロスターに乗ったことはないでしょう？この機会に乗ってみませんか？"},{"id":{"d":242,"n":4,"t":3},"speaker":"B","text":"ユーロスターに乗るのは初めてなので、楽しみです。パリまでどのくらい時間がかかるのですか？"},{"id":{"d":242,"n":5,"t":3},"speaker":"A","text":"ここからだと2時間15分ほどです。パリで行きたい所はありますか？"},{"id":{"d":242,"n":6,"t":3},"speaker":"B","text":"パリ・サンジェルマンの試合が見たいのですが、今からチケットを取るのは無理ですよね。"},{"id":{"d":242,"n":7,"t":3},"speaker":"A","text":"今からでは難しいでしょうね。ところが、ここにチケットがあるのです。"},{"id":{"d":242,"n":8,"t":3},"speaker":"B","text":"本当ですか！もう用意してくれていたのですね。うれしいです。ありがとうございます。"},{"id":{"d":242,"n":9,"t":3},"speaker":"A","text":"ちょっとしたサプライズです。喜んでもらえてよかったです。"}];
INSERT INTO sentence [{"id":{"d":243,"n":1,"t":3},"speaker":"A","text":"もしもし。お元気ですか？フランスはどうですか？"},{"id":{"d":243,"n":2,"t":3},"speaker":"B","text":"元気ですよ。今日はサッカーの試合を見てきました。"},{"id":{"d":243,"n":3,"t":3},"speaker":"A","text":"それはよかったです。今はパリにいるのでしたか？"},{"id":{"d":243,"n":4,"t":3},"speaker":"B","text":"はい、そうです。パリは今日で3日目です。"},{"id":{"d":243,"n":5,"t":3},"speaker":"A","text":"いいですね。ルーブル美術館には行きましたか？"},{"id":{"d":243,"n":6,"t":3},"speaker":"B","text":"はい。1日目に行ってきました。とても素晴らしい芸術を見ることができて、本当に来てよかったです。"},{"id":{"d":243,"n":7,"t":3},"speaker":"A","text":"それは何よりです。残りの滞在も楽しんでくださいね。"},{"id":{"d":243,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。では、また帰国するときに連絡しますね。"}];
INSERT INTO sentence [{"id":{"d":244,"n":1,"t":3},"speaker":"A","text":"あなたは建造物に興味があるのですか？"},{"id":{"d":244,"n":2,"t":3},"speaker":"B","text":"はい。建造物の中でも、特にタワーが大好きです。タワーを見るために旅行先を選ぶほどです。"},{"id":{"d":244,"n":3,"t":3},"speaker":"A","text":"確かに、タワーは地域のランドマークになっていることも多く、大抵の観光地にありますね。"},{"id":{"d":244,"n":4,"t":3},"speaker":"B","text":"そうですね。有名なものは、神戸ポートタワー、大阪の通天閣、京都タワー、東京のスカイツリーあたりでしょうか。行ったことはありますか？"},{"id":{"d":244,"n":5,"t":3},"speaker":"A","text":"残念ながら、その中で行ったことがあるのは、スカイツリーだけです。東京タワーには行ったことがあります。"},{"id":{"d":244,"n":6,"t":3},"speaker":"B","text":"東京タワーも有名ですね。私にとっては親戚の家よりも近い場所です。月に一度は東京タワーの周辺を散策したり、上ったりしています。"},{"id":{"d":244,"n":7,"t":3},"speaker":"A","text":"そうなのですね。今後、行ってみたいタワーはありますか？"},{"id":{"d":244,"n":8,"t":3},"speaker":"B","text":"倉敷のラ・レインボータワーは、ぜひ見に行きたいと思っています。"},{"id":{"d":244,"n":9,"t":3},"speaker":"A","text":"私は初めて聞いた所なのですが、観光名所なのでしょうか？"},{"id":{"d":244,"n":10,"t":3},"speaker":"B","text":"いいえ。だいぶ前に閉鎖されて、今は廃虚状態です。取り壊される前に、写真を撮りに行こうと思っています。"}];
INSERT INTO sentence [{"id":{"d":245,"n":1,"t":3},"speaker":"A","text":"乗り物に乗ることも旅行の楽しみの一つですが、これまでに珍しい乗り物に乗ったことはありますか？"},{"id":{"d":245,"n":2,"t":3},"speaker":"B","text":"ホバークラフトに乗ったことがあります。大分空港から大分市内までをホバークラフトで移動しました。"},{"id":{"d":245,"n":3,"t":3},"speaker":"A","text":"それは珍しいですね。今もあるのですか？"},{"id":{"d":245,"n":4,"t":3},"speaker":"B","text":"残念ながら、今は廃止されてしまいました。恐らく、今、日本で乗れる所はないと思います。"},{"id":{"d":245,"n":5,"t":3},"speaker":"A","text":"採算が合わないのでしょうか。私も乗ってみたかったです。"},{"id":{"d":245,"n":6,"t":3},"speaker":"B","text":"最近は大分空港の利用者が増えたので、ホバークラフトが復活するという話があるそうですよ。"},{"id":{"d":245,"n":7,"t":3},"speaker":"A","text":"それはうれしいです。復活したらぜひとも乗ってみたいです。"},{"id":{"d":245,"n":8,"t":3},"speaker":"B","text":"陸から海に入るときの浮遊感がとても楽しかったので、あなたにもぜひ体験してほしいです。"}];
INSERT INTO sentence [{"id":{"d":246,"n":1,"t":3},"speaker":"A","text":"明日の自由時間に、私たちはどこへ行きましょうか？"},{"id":{"d":246,"n":2,"t":3},"speaker":"B","text":"厳島神社はいかがですか？"},{"id":{"d":246,"n":3,"t":3},"speaker":"A","text":"なぜそこに行きたいのですか？"},{"id":{"d":246,"n":4,"t":3},"speaker":"B","text":"厳島神社自体は以前から知っていましたが、最近、源平の合戦との結び付きを知ったのです。それで、より一層、厳島神社に魅力を感じるようになりました。"},{"id":{"d":246,"n":5,"t":3},"speaker":"A","text":"平家物語のアニメーションを見ていたので、私もその話は知っています。確かに厳島神社は一度行ってみたい観光地ですね。"},{"id":{"d":246,"n":6,"t":3},"speaker":"B","text":"行くときは干潮時の歩いて行ける時間にしたいです。"},{"id":{"d":246,"n":7,"t":3},"speaker":"A","text":"自由行動時間と干潮時間が重なるか、私が調べておきます。"},{"id":{"d":246,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":247,"n":1,"t":3},"speaker":"A","text":"すみません、動物園に行きたいのですが、どうやって行けばよいか教えてもらえますか？"},{"id":{"d":247,"n":2,"t":3},"speaker":"B","text":"三宮駅まではポートライナーに乗ります。三宮駅からは阪急線に乗って王子公園駅で降りてください。"},{"id":{"d":247,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。時間はどれくらいかかりますか？"},{"id":{"d":247,"n":4,"t":3},"speaker":"B","text":"三ノ宮駅までは20分くらい、そこから王子公園駅までは5分くらいです。"},{"id":{"d":247,"n":5,"t":3},"speaker":"A","text":"運賃はどのくらいでしょうか？"},{"id":{"d":247,"n":6,"t":3},"speaker":"B","text":"確か、500円くらいです。もしたくさん電車を利用するのでしたら、旅行案内所などで割安の切符がないか聞いた方がいいかもしれません。"},{"id":{"d":247,"n":7,"t":3},"speaker":"A","text":"ご親切にありがとうございます。まずは動物園に行ってから、割安の切符を求めるかどうか考えようと思います。"},{"id":{"d":247,"n":8,"t":3},"speaker":"B","text":"ちなみに、もしパンダを見るなら早めに行った方がいいですよ。とても人気があるので、行列になっている可能性があります。"},{"id":{"d":247,"n":9,"t":3},"speaker":"A","text":"やはりパンダは人気があるのですね。急いでいきます。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":248,"n":1,"t":3},"speaker":"A","text":"森保さんの車は大きなワゴン車ですね。キャンプ仕様になっているのですか？"},{"id":{"d":248,"n":2,"t":3},"speaker":"B","text":"そうです。アウトドアが趣味で、連休にはよくキャンプへ出かけます。"},{"id":{"d":248,"n":3,"t":3},"speaker":"A","text":"ご家族も一緒ですか？"},{"id":{"d":248,"n":4,"t":3},"speaker":"B","text":"昔はよく家族で出かけましたが、最近は誰も付き合ってくれません。"},{"id":{"d":248,"n":5,"t":3},"speaker":"A","text":"皆さんお忙しくなったのですね。ワゴン車の上に載せているのはカヌーですか？"},{"id":{"d":248,"n":6,"t":3},"speaker":"B","text":"はい。キャンプ地の近くに湖や海、川があるときには持っていきます。"},{"id":{"d":248,"n":7,"t":3},"speaker":"A","text":"カヌーはすぐひっくり返るイメージなのですが、危なくはありませんか？"},{"id":{"d":248,"n":8,"t":3},"speaker":"B","text":"もう20年以上も乗っているので大丈夫です。カヌーに乗ってこいでいるだけで幸せな気分になります。"},{"id":{"d":248,"n":9,"t":3},"speaker":"A","text":"興味が湧いてきました。私もカヌーに挑戦してみたいのですが、道具を借りられる所はあるのでしょうか？"},{"id":{"d":248,"n":10,"t":3},"speaker":"B","text":"ありますよ。今度案内しますね。楽しいですよ。"}];
INSERT INTO sentence [{"id":{"d":249,"n":1,"t":3},"speaker":"A","text":"青函連絡船に乗りたいのですが、どうすればよいですか？"},{"id":{"d":249,"n":2,"t":3},"speaker":"B","text":"青函連絡船はもう廃止されていますが、青函フェリーのことでしょうか？"},{"id":{"d":249,"n":3,"t":3},"speaker":"A","text":"はい、そうです。青函連絡船の印象が強いので、ついそう言ってしまいました。"},{"id":{"d":249,"n":4,"t":3},"speaker":"B","text":"大丈夫ですよ。お客さまお1人でのご利用でしょうか？お車はありますか？"},{"id":{"d":249,"n":5,"t":3},"speaker":"A","text":"私1人です。車はありません。"},{"id":{"d":249,"n":6,"t":3},"speaker":"B","text":"そうしましたら、フェリーターミナルまではタクシーを利用されるのがよろしいかと思います。フェリーターミナルはここから少し離れています。バスもありますが、バス停から少し距離がありますのでタクシーがお薦めです。"},{"id":{"d":249,"n":7,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。タクシー乗り場は、あそこですか？"},{"id":{"d":249,"n":8,"t":3},"speaker":"B","text":"はい、そうです。良い旅をお祈りします。"}];
INSERT INTO sentence [{"id":{"d":250,"n":1,"t":3},"speaker":"A","text":"今度の旅行はどこへ行きたいか決めましたか？"},{"id":{"d":250,"n":2,"t":3},"speaker":"B","text":"現地のカフェに行く時間を作ってくれるなら、私はどこでもよいです。"},{"id":{"d":250,"n":3,"t":3},"speaker":"A","text":"あなたは本当にカフェが好きですね。郷土料理なら分かりますが、カフェは珍しいと思います。"},{"id":{"d":250,"n":4,"t":3},"speaker":"B","text":"そうでしょうか？ご当地カフェは、その地域の名産品を取り扱っていることも多く、面白いですよ。"},{"id":{"d":250,"n":5,"t":3},"speaker":"A","text":"確かにそうですね。郷土の食材を使ったスイーツなども、よく見る気がします。"},{"id":{"d":250,"n":6,"t":3},"speaker":"B","text":"旅行先の話ですが、年に1度の旅行のために積み立てているお金もあることだし、今年は海外へ行きませんか？"},{"id":{"d":250,"n":7,"t":3},"speaker":"A","text":"それは私も考えていました。東アジアや東南アジアなら可能だと思います。"},{"id":{"d":250,"n":8,"t":3},"speaker":"B","text":"海外は大手外資系のカフェでさえ特色があるので、とても楽しみです。どこの国にするかはあなたにお任せしますね。"},{"id":{"d":250,"n":9,"t":3},"speaker":"A","text":"分かりました。いくつか候補を考えて、メールしますね。"},{"id":{"d":250,"n":10,"t":3},"speaker":"B","text":"はい。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":251,"n":1,"t":3},"speaker":"A","text":"お互いに出張が多いですが、新幹線はよく利用しますか？"},{"id":{"d":251,"n":2,"t":3},"speaker":"B","text":"よく使いますよ。飛行機よりも安いですし、日帰りならば宿泊費も浮くので、ほとんど新幹線で移動しています。"},{"id":{"d":251,"n":3,"t":3},"speaker":"A","text":"全部の路線に乗りましたか？"},{"id":{"d":251,"n":4,"t":3},"speaker":"B","text":"私用で利用した路線も含めれば、かなり乗りましたね。乗っていないのは九州新幹線くらいです。"},{"id":{"d":251,"n":5,"t":3},"speaker":"A","text":"私も九州新幹線はありません。会社が東京ですから、博多から鹿児島の路線を利用することはありません。"},{"id":{"d":251,"n":6,"t":3},"speaker":"B","text":"九州内を移動するような出張でないと九州新幹線には乗らないでしょうね。"},{"id":{"d":251,"n":7,"t":3},"speaker":"A","text":"そうですね。今後も、新幹線の路線は増える予定なのでしょうか？"},{"id":{"d":251,"n":8,"t":3},"speaker":"B","text":"北海道新幹線や北陸新幹線は路線が伸びるはずです。九州新幹線もまだ延長工事をしているはずですよ。"},{"id":{"d":251,"n":9,"t":3},"speaker":"A","text":"完成したらぜひ乗ってみたいものですね。"}];
INSERT INTO sentence [{"id":{"d":252,"n":1,"t":3},"speaker":"A","text":"アメリカで行ってみたい所はありますか？"},{"id":{"d":252,"n":2,"t":3},"speaker":"B","text":"ワシントンD.C.に行ってみたいです。"},{"id":{"d":252,"n":3,"t":3},"speaker":"A","text":"ロサンゼルスやニューヨークではないのですか？なぜワシントンD.C.なのですか？"},{"id":{"d":252,"n":4,"t":3},"speaker":"B","text":"ワシントンD.C.にある博物館に行きたいからです。英語の教科書に載っていたのですが、その博物館にはアフリカン=アメリカンの文化と歴史に関するたくさんの資料があるそうです。"},{"id":{"d":252,"n":5,"t":3},"speaker":"A","text":"そのような面白い博物館があるのですね。"},{"id":{"d":252,"n":6,"t":3},"speaker":"B","text":"あなたがアメリカに住んでいたときに、ワシントンD.C.へは行きませんでしたか？"},{"id":{"d":252,"n":7,"t":3},"speaker":"A","text":"私はサンフランシスコで育ちましたが、残念ながら、ワシントンD.C.には行ったことがありません。"}];
INSERT INTO sentence [{"id":{"d":253,"n":1,"t":3},"speaker":"A","text":"ドイツに留学すると聞いたのですが、本当ですか？"},{"id":{"d":253,"n":2,"t":3},"speaker":"B","text":"はい。先日のコンテストで良い成績を収めることができたので、ドイツにある提携校で勉強させてもらえることになりました。"},{"id":{"d":253,"n":3,"t":3},"speaker":"A","text":"君のチェロは素晴らしいので、ドイツ留学も納得です。僕もいつか行ってみたいです。"},{"id":{"d":253,"n":4,"t":3},"speaker":"B","text":"ドイツには優れた音楽がたくさんあるので、私はそれを体験できることを楽しみにしています。ベルリン・フィルやバイロイト音楽祭はぜひ見てみたいです。"},{"id":{"d":253,"n":5,"t":3},"speaker":"A","text":"一流どころの演奏はなかなか見られるものではありませんし、貴重な経験ができそうですね。"},{"id":{"d":253,"n":6,"t":3},"speaker":"B","text":"はい。このチャンスを逃さず、現地でしかできない経験をたくさんしてくるつもりです。そのためには、ドイツ語の勉強やチェロの練習も一生懸命しなければなりません。"},{"id":{"d":253,"n":7,"t":3},"speaker":"A","text":"応援していますよ。"},{"id":{"d":253,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":254,"n":1,"t":3},"speaker":"A","text":"ベニスへの旅行を考えています。町の状態はどうなっていますか？"},{"id":{"d":254,"n":2,"t":3},"speaker":"B","text":"高潮や地盤沈下を心配されているのでしょうか？"},{"id":{"d":254,"n":3,"t":3},"speaker":"A","text":"はい、ニュースを見ているとたびたび高潮に遭っているようで心配です。水没することもあると聞いたことがあるので、町の状態を知っておきたいのです。"},{"id":{"d":254,"n":4,"t":3},"speaker":"B","text":"そうでしたか。ベネチアの水没問題はイタリア国内でも問題になっていて、イタリア政府は防潮堤を造るなどの対策を実施しております。"},{"id":{"d":254,"n":5,"t":3},"speaker":"A","text":"対策がされているのですね。観光客は受け入れていますか？"},{"id":{"d":254,"n":6,"t":3},"speaker":"B","text":"もちろんです。常に観光客の方を受け入れられる状態にしております。"},{"id":{"d":254,"n":7,"t":3},"speaker":"A","text":"少し安心しました。では、こちらのツアーへの申し込みをお願いします。もし、高潮などで希望している場所を見られないときは、キャンセルできますか？"},{"id":{"d":254,"n":8,"t":3},"speaker":"B","text":"可能でございます。その場合はキャンセル手続きをしていただくことになります。"},{"id":{"d":254,"n":9,"t":3},"speaker":"A","text":"分かりました。ところでベニスとベネチアはどう違うのですか？"},{"id":{"d":254,"n":10,"t":3},"speaker":"B","text":"どちらも同じ都市を指しています。ベネチアはイタリア語で、それを英語にするとベニスになります。"}];
INSERT INTO sentence [{"id":{"d":255,"n":1,"t":3},"speaker":"A","text":"金婚式のお祝いに子どもたちから旅行券を頂きました。どこか行きたい所はありますか？"},{"id":{"d":255,"n":2,"t":3},"speaker":"B","text":"沖縄か北海道はどうですか？"},{"id":{"d":255,"n":3,"t":3},"speaker":"A","text":"それが、外国旅行ができるくらいの旅行券なのですよ。海外で行きたい所はありますか？"},{"id":{"d":255,"n":4,"t":3},"speaker":"B","text":"思いつきません。あなたはどこか行きたい所はありますか？"},{"id":{"d":255,"n":5,"t":3},"speaker":"A","text":"私は『フランダースの犬』が好きなので、物語の舞台になったベルギーに行ってみたいです。"},{"id":{"d":255,"n":6,"t":3},"speaker":"B","text":"そうでしたね。ネロがルーベンスの作品を見ていた教会に行きたいという話は、何度か聞いたことがあります。"},{"id":{"d":255,"n":7,"t":3},"speaker":"A","text":"アントワープ聖母大聖堂です。でも、旅行先が私の行きたい所で良いのですか？"},{"id":{"d":255,"n":8,"t":3},"speaker":"B","text":"構いませんよ。"},{"id":{"d":255,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":256,"n":1,"t":3},"speaker":"A","text":"先ほど、釣り船の船長に電話しました。今日は天候が悪くて船は出せないそうです。"},{"id":{"d":256,"n":2,"t":3},"speaker":"B","text":"それは残念です。出られれば3カ月ぶりの海釣りでした。"},{"id":{"d":256,"n":3,"t":3},"speaker":"A","text":"私は1年ぶりでした。無理をしても駄目でしょうから、今日は諦めましょう。"},{"id":{"d":256,"n":4,"t":3},"speaker":"B","text":"しかし、今の今まで船長と連絡を取れなかったのは問題でした。"},{"id":{"d":256,"n":5,"t":3},"speaker":"A","text":"雨の中の船作業で気が付かなかったそうですよ。"},{"id":{"d":256,"n":6,"t":3},"speaker":"B","text":"もっともらしい言い訳ですが、出航を取りやめた時点で船客に連絡するのが当然の義務だと思います。"},{"id":{"d":256,"n":7,"t":3},"speaker":"A","text":"船の方が大切だったのでしょう。それよりもこれからどうしましょうか？"},{"id":{"d":256,"n":8,"t":3},"speaker":"B","text":"このままドライブして、前に私が話したそば屋に行きませんか？"},{"id":{"d":256,"n":9,"t":3},"speaker":"A","text":"構いませんが、今からだと早く着きすぎて開店していないのではありませんか？"},{"id":{"d":256,"n":10,"t":3},"speaker":"B","text":"少し手前に道の駅がありますからそこで少し時間をつぶしましょう。"}];
INSERT INTO sentence [{"id":{"d":257,"n":1,"t":3},"speaker":"A","text":"聞いてください。この間、UFO博物館に行ってきました。"},{"id":{"d":257,"n":2,"t":3},"speaker":"B","text":"そのような博物館があるのですか。どこまで行ってきたのですか？"},{"id":{"d":257,"n":3,"t":3},"speaker":"A","text":"石川県の羽咋市です。コスモアイル羽咋というのが正式名です。"},{"id":{"d":257,"n":4,"t":3},"speaker":"B","text":"では、北陸新幹線で行ったのですか？"},{"id":{"d":257,"n":5,"t":3},"speaker":"A","text":"そうです。金沢駅までは北陸新幹線で行きました。"},{"id":{"d":257,"n":6,"t":3},"speaker":"B","text":"博物館までの所要時間はどれくらいでしたか？"},{"id":{"d":257,"n":7,"t":3},"speaker":"A","text":"4時間もかかりませんでしたよ。金沢駅で電車に乗り換えてそこから1時間、羽咋駅から徒歩数分で目的地のUFO博物館に到着しました。"},{"id":{"d":257,"n":8,"t":3},"speaker":"B","text":"UFO博物館はどうでしたか？"},{"id":{"d":257,"n":9,"t":3},"speaker":"A","text":"旧ソ連やアメリカの宇宙船も展示されている本格的な博物館でした。とても楽しめました。"},{"id":{"d":257,"n":10,"t":3},"speaker":"B","text":"それはよかったですね。ところで、君の手にあるそれは何ですか？"},{"id":{"d":257,"n":11,"t":3},"speaker":"A","text":"博物館のゆるキャラで、宇宙人サンダー君です。握ると舌を出すのですよ、ほら。"},{"id":{"d":257,"n":12,"t":3},"speaker":"B","text":"もしかして、その人形を買いに石川県まで一人旅をしたのですか？"}];
INSERT INTO sentence [{"id":{"d":258,"n":1,"t":3},"speaker":"A","text":"最近のホテルのサービスはとても充実していますよね。"},{"id":{"d":258,"n":2,"t":3},"speaker":"B","text":"出張から帰ると皆さんそう言いますね。"},{"id":{"d":258,"n":3,"t":3},"speaker":"A","text":"宿泊費が変わらないのに、いろいろなお得なチケットを受け取ることができるのですよ。"},{"id":{"d":258,"n":4,"t":3},"speaker":"B","text":"食事券だけではないと聞いています。サウナやマッサージのチケットも多いようですね。"},{"id":{"d":258,"n":5,"t":3},"speaker":"A","text":"私が受け取ったチケットは、商店街の飲食無料券でした。2食分あったので、食費はかかりませんでした。"},{"id":{"d":258,"n":6,"t":3},"speaker":"B","text":"それはいいですね。ご当地の名物料理を無料で食べられますから、飲食無料券はうれしいと思います。"},{"id":{"d":258,"n":7,"t":3},"speaker":"A","text":"はい、私もそう思います。とてもありがたかったです。"}];
INSERT INTO sentence [{"id":{"d":259,"n":1,"t":3},"speaker":"A","text":"最近のサービスエリアはパンを売りにしているところが多いと思いませんか？"},{"id":{"d":259,"n":2,"t":3},"speaker":"B","text":"そうですね。昔はラーメンやそばが多かった印象です。"},{"id":{"d":259,"n":3,"t":3},"speaker":"A","text":"なぜこれほどまでにもパンが人気になったのでしょうか？"},{"id":{"d":259,"n":4,"t":3},"speaker":"B","text":"パンは持ち運びができて、お土産にもできるからでしょうか。"},{"id":{"d":259,"n":5,"t":3},"speaker":"A","text":"なるほど。そのサービスエリアでしか入手できないメロンパンなど、ご当地ならではのパンは、お土産としても魅力的ですね。"},{"id":{"d":259,"n":6,"t":3},"speaker":"B","text":"はい。郷土愛があふれたカレーパンやクリームパン、焼きそばパンなどがお手軽価格で入手できるのは、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":260,"n":1,"t":3},"speaker":"A","text":"高橋さんはホテルのアメニティーを使いますか？"},{"id":{"d":260,"n":2,"t":3},"speaker":"B","text":"使っていますよ。"},{"id":{"d":260,"n":3,"t":3},"speaker":"A","text":"たまにT字カミソリが入っていることがありますが、あれを使ったことはありますか？"},{"id":{"d":260,"n":4,"t":3},"speaker":"B","text":"いいえ、私は使ったことがありません。"},{"id":{"d":260,"n":5,"t":3},"speaker":"A","text":"そうですか。T字カミソリの使い心地についてお伺いしたかったのですが、別の人に聞くことにします。ありがとうございました。"},{"id":{"d":260,"n":6,"t":3},"speaker":"B","text":"お力になれずすみません。"}];
INSERT INTO sentence [{"id":{"d":261,"n":1,"t":3},"speaker":"A","text":"海外で車に乗っていると、通行車線が左右どちらか分からなくなることがあります。"},{"id":{"d":261,"n":2,"t":3},"speaker":"B","text":"それは危険ですね。レンタカーに乗る時ですか？"},{"id":{"d":261,"n":3,"t":3},"speaker":"A","text":"はい。"},{"id":{"d":261,"n":4,"t":3},"speaker":"B","text":"ハンドルの位置で直感的に通行車線が分かりませんか？右ハンドルなら左側通行、という具合に。"},{"id":{"d":261,"n":5,"t":3},"speaker":"A","text":"実は、日本で乗っているマイカーが左ハンドルなのです。"},{"id":{"d":261,"n":6,"t":3},"speaker":"B","text":"なるほど、それは厄介ですね。"},{"id":{"d":261,"n":7,"t":3},"speaker":"A","text":"はい。アメリカは右側通行で左ハンドルの車が一般的です。しかし、日本では左側通行で左ハンドルの車を運転しているので、アメリカでもつい左側通行してしまいそうになるのです。"},{"id":{"d":261,"n":8,"t":3},"speaker":"B","text":"それは本当に危ないですね。気を付けてくださいよ。"}];
INSERT INTO sentence [{"id":{"d":262,"n":1,"t":3},"speaker":"A","text":"ロサンゼルスで大谷翔平選手のプレーを見てきたと聞きました。"},{"id":{"d":262,"n":2,"t":3},"speaker":"B","text":"はい、エンゼルスタジアムで見てきました。ピッチングとバッティングの両方を見たかったので、3連戦全て見ました。"},{"id":{"d":262,"n":3,"t":3},"speaker":"A","text":"あなたがそれほどまでに彼のファンとは知りませんでした。巨人のファンかと思っていましたよ。"},{"id":{"d":262,"n":4,"t":3},"speaker":"B","text":"もちろん巨人のファンですが、大谷選手も応援しています。彼は日本の宝ですから。"},{"id":{"d":262,"n":5,"t":3},"speaker":"A","text":"大谷選手のホームランは見ることができましたか？"},{"id":{"d":262,"n":6,"t":3},"speaker":"B","text":"見ました。右中間に弾丸ライナーで飛び込みました。きれいな花火も上がりましたよ。"},{"id":{"d":262,"n":7,"t":3},"speaker":"A","text":"エンゼルスタジアムは噴水と花火で有名ですものね。"},{"id":{"d":262,"n":8,"t":3},"speaker":"B","text":"大谷選手は打者として出場していたのですが、途中からリリーフとして登板し、時速160キロメートルの球を投げていたのですよ。鳥肌が立ちました。"},{"id":{"d":262,"n":9,"t":3},"speaker":"A","text":"本当に規格外の選手です。私もいつか見に行きたいものです。"},{"id":{"d":262,"n":10,"t":3},"speaker":"B","text":"スポーツ選手の寿命は短いと言いますから、すぐにでも見に行くべきですよ。"}];
INSERT INTO sentence [{"id":{"d":263,"n":1,"t":3},"speaker":"A","text":"夏休みに卒業論文も書かないで、どこに旅行するのですか？"},{"id":{"d":263,"n":2,"t":3},"speaker":"B","text":"書かないわけではありません。卒論のために行くのです。秋田、青森、北海道を巡ります。"},{"id":{"d":263,"n":3,"t":3},"speaker":"A","text":"そうだったのですね。失礼しました。卒論のテーマは何ですか？"},{"id":{"d":263,"n":4,"t":3},"speaker":"B","text":"イザベラ・バードというイギリス女性です。明治の時に日本に来て、東京から北海道まで徒歩で踏破した人です。"},{"id":{"d":263,"n":5,"t":3},"speaker":"A","text":"当時の道は整備されておらず、大変な旅だったことでしょうね。夏の旅行は、彼女に関する資料を集めるためのものですか？"},{"id":{"d":263,"n":6,"t":3},"speaker":"B","text":"はい。彼女の足跡をたどり、資料を集めてくる予定です。"},{"id":{"d":263,"n":7,"t":3},"speaker":"A","text":"どのような所に資料があるのですか？"},{"id":{"d":263,"n":8,"t":3},"speaker":"B","text":"民俗博物館、民俗資料館、郷土館などです。バードの歩いた所には記念碑があるので、それも撮影したいです。"},{"id":{"d":263,"n":9,"t":3},"speaker":"A","text":"面白そうですね。ところで、資料を集めた後、その資料を用いて論文を書く時間はあるのですか？"},{"id":{"d":263,"n":10,"t":3},"speaker":"B","text":"卒論の骨子は既にまとめているので、問題ありません。今回の旅はその肉付けとなるものです。"}];
INSERT INTO sentence [{"id":{"d":264,"n":1,"t":3},"speaker":"A","text":"1カ月近く休暇を取るのですか？珍しいですね。どこかへ行くのですか？"},{"id":{"d":264,"n":2,"t":3},"speaker":"B","text":"家族で軽井沢に行きます。父の別荘をしばらく使っていなかったので、様子見がてら行くことになりました。"},{"id":{"d":264,"n":3,"t":3},"speaker":"A","text":"使っていない所に急に行っても大丈夫なのですか？"},{"id":{"d":264,"n":4,"t":3},"speaker":"B","text":"去年までは人に貸していたので、ずっと空けていたわけではありません。それに管理人もいます。"},{"id":{"d":264,"n":5,"t":3},"speaker":"A","text":"そうでしたか。お父さまは使わなかったのですか？"},{"id":{"d":264,"n":6,"t":3},"speaker":"B","text":"父は5年前に体調を崩して、それ以降行けていなかったのです。私と妻も仕事が忙しくて、別荘まで行く時間がなかなか取れませんでした。"},{"id":{"d":264,"n":7,"t":3},"speaker":"A","text":"そうだったのですね。家族水入らずで、避暑地暮らしを楽しんできてくださいね。"},{"id":{"d":264,"n":8,"t":3},"speaker":"B","text":"はい、ありがとうございます。岡田さんは休暇をどう過ごされますか？"},{"id":{"d":264,"n":9,"t":3},"speaker":"A","text":"町内の納涼夏祭りで幹事となっていますので、この夏は町に奉公してきます。"},{"id":{"d":264,"n":10,"t":3},"speaker":"B","text":"相変わらず祭り好きですね。夏祭りの成功をお祈りしています。"}];
INSERT INTO sentence [{"id":{"d":265,"n":1,"t":3},"speaker":"A","text":"夏休みはどこかへ行きますか？"},{"id":{"d":265,"n":2,"t":3},"speaker":"B","text":"ヨーロッパをバックパッカーで回ってこようと思っています。"},{"id":{"d":265,"n":3,"t":3},"speaker":"A","text":"大丈夫ですか？危険はありませんか？"},{"id":{"d":265,"n":4,"t":3},"speaker":"B","text":"危険も込みで旅行だと思っています。若いうちにしかできないことをしたいと思ったのです。"},{"id":{"d":265,"n":5,"t":3},"speaker":"A","text":"言葉の心配はないのでしょうか？"},{"id":{"d":265,"n":6,"t":3},"speaker":"B","text":"旅行のために、しばらく英会話教室に通っていたので大丈夫ですよ。"},{"id":{"d":265,"n":7,"t":3},"speaker":"A","text":"それは素晴らしいですね。具体的にどの国へ行くかは決めているのですか？"},{"id":{"d":265,"n":8,"t":3},"speaker":"B","text":"今のところ、スペイン、フランス、イタリアに行くつもりです。"}];
INSERT INTO sentence [{"id":{"d":266,"n":1,"t":3},"speaker":"A","text":"佐々木さん、定年まであとどれくらいですか？"},{"id":{"d":266,"n":2,"t":3},"speaker":"B","text":"もうあと1年で定年です。"},{"id":{"d":266,"n":3,"t":3},"speaker":"A","text":"定年後はどのように過ごされるのですか？"},{"id":{"d":266,"n":4,"t":3},"speaker":"B","text":"しばらく休んでから、もう一度復職しようと考えています。"},{"id":{"d":266,"n":5,"t":3},"speaker":"A","text":"佐々木さんはまだお元気ですので、それもいいでしょうね。しばらく休むのは何か目的があるのですか？"},{"id":{"d":266,"n":6,"t":3},"speaker":"B","text":"四国八十八カ所の巡礼をしたいと考えています。ずっと家族と会社のために働いてきたので、何か自分だけの達成感が欲しいと思ったのです。"},{"id":{"d":266,"n":7,"t":3},"speaker":"A","text":"それはすてきですね。ぜひ、旅のお話を聞かせてください。"},{"id":{"d":266,"n":8,"t":3},"speaker":"B","text":"分かりました。でも、まだ1年以上先の話ですよ。"}];
INSERT INTO sentence [{"id":{"d":267,"n":1,"t":3},"speaker":"A","text":"青井さん、自動車の運転はできますか？"},{"id":{"d":267,"n":2,"t":3},"speaker":"B","text":"運転免許は持っているのですが、運転はあまりしていません。移動するときは電車を使うことが多いです。"},{"id":{"d":267,"n":3,"t":3},"speaker":"A","text":"そうなのですね。私は今、自動車学校に通っているのですが、自動車の運転に苦戦しています。"},{"id":{"d":267,"n":4,"t":3},"speaker":"B","text":"慣れるまでは大変ですよね。頑張ってくださいね。"},{"id":{"d":267,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":267,"n":6,"t":3},"speaker":"B","text":"免許を取った後はすぐに運転をした方がいいですよ。"},{"id":{"d":267,"n":7,"t":3},"speaker":"A","text":"そうなのですか？"},{"id":{"d":267,"n":8,"t":3},"speaker":"B","text":"はい。私は自家用車を持っていなかったので、免許を取ってから運転をする機会がなかなかなく、そのせいで運転することに苦手意識を持つようになってしまいました。あなたはそうならないようにしてください。"},{"id":{"d":267,"n":9,"t":3},"speaker":"A","text":"なるほど、ご助言ありがとうございます。免許を取ったら、すぐに運転するようにします。"},{"id":{"d":267,"n":10,"t":3},"speaker":"B","text":"運転がうまくなったら、一緒にドライブしましょう。それまでに、私もレンタカーを借りて練習しておきます。"}];
INSERT INTO sentence [{"id":{"d":268,"n":1,"t":3},"speaker":"A","text":"夏休みの計画を立てましょう。"},{"id":{"d":268,"n":2,"t":3},"speaker":"B","text":"はい。せっかくなので旅行に行きたいですね。"},{"id":{"d":268,"n":3,"t":3},"speaker":"A","text":"そうですね。どこに行きましょうか？"},{"id":{"d":268,"n":4,"t":3},"speaker":"B","text":"北海道はどうですか？おいしい海鮮丼が食べたいです。"},{"id":{"d":268,"n":5,"t":3},"speaker":"A","text":"いいですね。そうしましょう。北海道への移動は飛行機がよさそうですね。"},{"id":{"d":268,"n":6,"t":3},"speaker":"B","text":"ええ、航空機を予約しましょう。"},{"id":{"d":268,"n":7,"t":3},"speaker":"A","text":"北海道に着いたらレンタカーを借りましょう。北海道は広いので、車があると便利だと思います。"},{"id":{"d":268,"n":8,"t":3},"speaker":"B","text":"私もそう思います。空港の近くでレンタカーを借りましょう。"}];
INSERT INTO sentence [{"id":{"d":269,"n":1,"t":3},"speaker":"A","text":"野田さんは、高校の修学旅行でどこに行きましたか？"},{"id":{"d":269,"n":2,"t":3},"speaker":"B","text":"沖縄です。クラスのみんなと海に入りました。"},{"id":{"d":269,"n":3,"t":3},"speaker":"A","text":"いいですね。私は沖縄にはまだ行ったことがありません。"},{"id":{"d":269,"n":4,"t":3},"speaker":"B","text":"もし沖縄へ行くなら、夏はとても混んでいるので、春休みに行くのがお勧めですよ。"},{"id":{"d":269,"n":5,"t":3},"speaker":"A","text":"なるほど。覚えておきます。"},{"id":{"d":269,"n":6,"t":3},"speaker":"B","text":"伊藤さんは修学旅行でどこに行きましたか？"},{"id":{"d":269,"n":7,"t":3},"speaker":"A","text":"台湾に行きました。"},{"id":{"d":269,"n":8,"t":3},"speaker":"B","text":"海外とはうらやましいです。中国語で会話をしたのですか？"},{"id":{"d":269,"n":9,"t":3},"speaker":"A","text":"いいえ、日本語です。日本語を話せる人が多いので助かりました。次に行くときは中国語で会話したいです。"},{"id":{"d":269,"n":10,"t":3},"speaker":"B","text":"伊藤さんは大学で中国語を学んでいますよね。"},{"id":{"d":269,"n":11,"t":3},"speaker":"A","text":"はい。中国語を習得して、必ずまた台湾に行きたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":270,"n":1,"t":3},"speaker":"A","text":"道路が混んでいますね。"},{"id":{"d":270,"n":2,"t":3},"speaker":"B","text":"はい。行楽シーズンなので、高速道路がかなり渋滞していますね。"},{"id":{"d":270,"n":3,"t":3},"speaker":"A","text":"車での旅行だと、移動時間を正確に予測することが難しいですね。"},{"id":{"d":270,"n":4,"t":3},"speaker":"B","text":"はい。宿のチェックインの時間は何時でしたか？"},{"id":{"d":270,"n":5,"t":3},"speaker":"A","text":"17時です。カーナビゲーションシステムの予測では、到着は18時となっています。"},{"id":{"d":270,"n":6,"t":3},"speaker":"B","text":"宿に電話してチェックインの時間に遅れることを伝えた方がいいでしょうか？"},{"id":{"d":270,"n":7,"t":3},"speaker":"A","text":"そうしましょう。私は運転しているので、電話をお願いできますか？"},{"id":{"d":270,"n":8,"t":3},"speaker":"B","text":"もちろんです。ホームページに記載されていた番号に電話をかけてみますね。"}];
INSERT INTO sentence [{"id":{"d":271,"n":1,"t":3},"speaker":"A","text":"紅葉シーズンになりましたね。"},{"id":{"d":271,"n":2,"t":3},"speaker":"B","text":"はい。私はこの時期が1年の中で一番好きです。"},{"id":{"d":271,"n":3,"t":3},"speaker":"A","text":"私もそうです。鈴木さんは紅葉狩りをしたことはありますか？"},{"id":{"d":271,"n":4,"t":3},"speaker":"B","text":"はい、毎年必ずしています。紅葉狩りをして写真を撮ることが私の趣味です。"},{"id":{"d":271,"n":5,"t":3},"speaker":"A","text":"そうですか。今年はもう行きましたか？"},{"id":{"d":271,"n":6,"t":3},"speaker":"B","text":"来週、京都の嵐山に行きます。初めて行くので今から楽しみです。"},{"id":{"d":271,"n":7,"t":3},"speaker":"A","text":"嵐山ですか。紅葉の名所ですね。すてきな写真が撮れたらぜひ見せてください。"},{"id":{"d":271,"n":8,"t":3},"speaker":"B","text":"はい。例年以上に気合を入れて撮影してきます。"}];
INSERT INTO sentence [{"id":{"d":272,"n":1,"t":3},"speaker":"A","text":"新婚旅行の行き先は決まりましたか？"},{"id":{"d":272,"n":2,"t":3},"speaker":"B","text":"今、夫と話し合っているところです。先輩はどこに行きましたか？"},{"id":{"d":272,"n":3,"t":3},"speaker":"A","text":"ヨーロッパです。大満足の旅行でした。"},{"id":{"d":272,"n":4,"t":3},"speaker":"B","text":"特に何が良かったのですか？"},{"id":{"d":272,"n":5,"t":3},"speaker":"A","text":"街並みが美しく、料理もおいしかったです。見たい所がたくさんあって、時間が足りないくらいでした。"},{"id":{"d":272,"n":6,"t":3},"speaker":"B","text":"そうなのですね。旅行会社のツアーを利用したのですか？"},{"id":{"d":272,"n":7,"t":3},"speaker":"A","text":"はい。ホテルも移動手段も手配されているので、楽でしたよ。"},{"id":{"d":272,"n":8,"t":3},"speaker":"B","text":"慣れない土地ではツアーに参加する方が良さそうですね。ヨーロッパのツアーを詳しく調べてみます。"}];
INSERT INTO sentence [{"id":{"d":273,"n":1,"t":3},"speaker":"A","text":"東京の地下鉄は種類が多いですね。"},{"id":{"d":273,"n":2,"t":3},"speaker":"B","text":"はい。アプリケーションで調べないと乗り換えが分からなくなることがあります。"},{"id":{"d":273,"n":3,"t":3},"speaker":"A","text":"分かります。私も、上京したての時よりは慣れましたが、今でも迷ってしまうことがあります。"},{"id":{"d":273,"n":4,"t":3},"speaker":"B","text":"これは、外国人観光客の方はもっと大変ですね。"},{"id":{"d":273,"n":5,"t":3},"speaker":"A","text":"そう思います。中国人の友人が日本に旅行しに来たとき、東京駅は路線が多過ぎると言っていました。"},{"id":{"d":273,"n":6,"t":3},"speaker":"B","text":"東京駅はとても広いですね。飲食店や書店も多く入っていますし、人も多いです。"},{"id":{"d":273,"n":7,"t":3},"speaker":"A","text":"はい。東京駅で乗り換えるときは時間に余裕を持たなければなりませんね。"},{"id":{"d":273,"n":8,"t":3},"speaker":"B","text":"そうですね。私は、アプリケーションに表示される乗り換え時間よりも多く見積もっています。"},{"id":{"d":273,"n":9,"t":3},"speaker":"A","text":"私も同じです。"}];
INSERT INTO sentence [{"id":{"d":274,"n":1,"t":3},"speaker":"A","text":"今日は大雪ですね。"},{"id":{"d":274,"n":2,"t":3},"speaker":"B","text":"はい。東北では既にかなりの積雪があるそうです。明日も大雪が続くそうですよ。"},{"id":{"d":274,"n":3,"t":3},"speaker":"A","text":"明日から宮城県に出張に行く予定なのですが、この天候では中止になるかもしれません。"},{"id":{"d":274,"n":4,"t":3},"speaker":"B","text":"交通機関に乱れが出そうですね。"},{"id":{"d":274,"n":5,"t":3},"speaker":"A","text":"はい。既に乱れ始めているかもしれません。"},{"id":{"d":274,"n":6,"t":3},"speaker":"B","text":"今調べたところ、航空機は相次いで欠航していますよ。東北新幹線も運休しています。"},{"id":{"d":274,"n":7,"t":3},"speaker":"A","text":"早くも大雪の影響が出始めていますね。明日の出張について上司に確認をとってみます。"},{"id":{"d":274,"n":8,"t":3},"speaker":"B","text":"それがいいと思います。明日宮城県に行くのは難しそうですよ。"}];
INSERT INTO sentence [{"id":{"d":275,"n":1,"t":3},"speaker":"A","text":"後藤さんの自転車は格好良いですね。"},{"id":{"d":275,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。サイクリング用の自転車です。"},{"id":{"d":275,"n":3,"t":3},"speaker":"A","text":"サイクリングがお好きなのですか？"},{"id":{"d":275,"n":4,"t":3},"speaker":"B","text":"はい。休みの日は大学時代の仲間とサイクリングに出かけています。"},{"id":{"d":275,"n":5,"t":3},"speaker":"A","text":"すてきですね。サイクリングの魅力は何ですか？"},{"id":{"d":275,"n":6,"t":3},"speaker":"B","text":"風を感じながら移動できることです。疾走感があります。"},{"id":{"d":275,"n":7,"t":3},"speaker":"A","text":"なるほど。長い時間こぎ続けることは大変ではありませんか？"},{"id":{"d":275,"n":8,"t":3},"speaker":"B","text":"疲れはしますが、良い運動になっていますよ。"}];
INSERT INTO sentence [{"id":{"d":276,"n":1,"t":3},"speaker":"A","text":"駅員さん、すみません。"},{"id":{"d":276,"n":2,"t":3},"speaker":"B","text":"はい。どうされましたか？"},{"id":{"d":276,"n":3,"t":3},"speaker":"A","text":"先ほど降りた急行列車に、旅行バッグを置き忘れてしまいました。"},{"id":{"d":276,"n":4,"t":3},"speaker":"B","text":"それは大変ですね。列車の行き先は分かりますか？"},{"id":{"d":276,"n":5,"t":3},"speaker":"A","text":"はい。日光行きでした。"},{"id":{"d":276,"n":6,"t":3},"speaker":"B","text":"日光行きの列車ですね。バッグの特徴を教えてください。"},{"id":{"d":276,"n":7,"t":3},"speaker":"A","text":"黒色のボストンバッグです。"},{"id":{"d":276,"n":8,"t":3},"speaker":"B","text":"分かりました。連絡を取ってみます。"}];
INSERT INTO sentence [{"id":{"d":277,"n":1,"t":3},"speaker":"A","text":"ここのホテルのアメニティーは充実していますね。"},{"id":{"d":277,"n":2,"t":3},"speaker":"B","text":"はい。化粧水や保湿クリームまであります。タオルもふわふわです。"},{"id":{"d":277,"n":3,"t":3},"speaker":"A","text":"ただ、このパジャマはサイズが少し小さいですね。"},{"id":{"d":277,"n":4,"t":3},"speaker":"B","text":"フロントに行って、サイズが交換できるか聞いてみましょう。"},{"id":{"d":277,"n":5,"t":3},"speaker":"A","text":"そうですね。夕飯を食べる前に、フロントに寄りましょう。"},{"id":{"d":277,"n":6,"t":3},"speaker":"B","text":"はい。夕飯はホテル内のレストランで食べる予定ですね。"},{"id":{"d":277,"n":7,"t":3},"speaker":"A","text":"そうです。18時の予約なので、今部屋を出るとちょうどいい時間に着きそうです。"},{"id":{"d":277,"n":8,"t":3},"speaker":"B","text":"それでは向かいましょうか。カードキーは持ちましたか？"},{"id":{"d":277,"n":9,"t":3},"speaker":"A","text":"持ちました。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":278,"n":1,"t":3},"speaker":"A","text":"お母さん、今度アメリカ人の友人に東京を案内することになっているのですが、お薦めの場所はありますか？"},{"id":{"d":278,"n":2,"t":3},"speaker":"B","text":"浅草はどうですか？雷門は人気の観光スポットです。"},{"id":{"d":278,"n":3,"t":3},"speaker":"A","text":"そうですね。最近は浴衣を着て散策する人もよく見かけます。"},{"id":{"d":278,"n":4,"t":3},"speaker":"B","text":"きっと楽しめると思います。屋形船に乗ることもできるのですよ。"},{"id":{"d":278,"n":5,"t":3},"speaker":"A","text":"屋形船とはどのような船のことですか？"},{"id":{"d":278,"n":6,"t":3},"speaker":"B","text":"屋根と座敷を備えた船のことです。船の上で宴会や食事を楽しむことができます。"},{"id":{"d":278,"n":7,"t":3},"speaker":"A","text":"そうなのですね。知りませんでした。"},{"id":{"d":278,"n":8,"t":3},"speaker":"B","text":"ただ、値段が高いかもしれませんので、料金を調べてみてください。"},{"id":{"d":278,"n":9,"t":3},"speaker":"A","text":"分かりました。調べてみます。"}];
INSERT INTO sentence [{"id":{"d":279,"n":1,"t":3},"speaker":"A","text":"坂本さん、ヨーロッパに行ったことはありますか？"},{"id":{"d":279,"n":2,"t":3},"speaker":"B","text":"はい。スペインとフランスとイタリアに行ったことがあります。谷口さんはどうですか？"},{"id":{"d":279,"n":3,"t":3},"speaker":"A","text":"私はドイツに行ったことがあります。"},{"id":{"d":279,"n":4,"t":3},"speaker":"B","text":"ドイツですか。次に旅行するときはドイツに行きたいと思っていました。"},{"id":{"d":279,"n":5,"t":3},"speaker":"A","text":"そうですか。ドイツはビールとソーセージがとてもおいしかったですよ。"},{"id":{"d":279,"n":6,"t":3},"speaker":"B","text":"ビールとソーセージの相性は抜群ですよね。ドイツに行ったらたくさん食べてしまいそうです。"},{"id":{"d":279,"n":7,"t":3},"speaker":"A","text":"そうです。現地でたくさん食べて、お土産もたっぷり購入しました。"},{"id":{"d":279,"n":8,"t":3},"speaker":"B","text":"私もドイツに行ったら谷口さんと同じことをすると思います。"}];
INSERT INTO sentence [{"id":{"d":280,"n":1,"t":3},"speaker":"A","text":"太田さんは海と山のどちらが好きですか？"},{"id":{"d":280,"n":2,"t":3},"speaker":"B","text":"私は海が好きです。"},{"id":{"d":280,"n":3,"t":3},"speaker":"A","text":"どうしてですか？"},{"id":{"d":280,"n":4,"t":3},"speaker":"B","text":"趣味が釣りだからです。休みの日は自前の釣り船に乗って海に出ています。"},{"id":{"d":280,"n":5,"t":3},"speaker":"A","text":"そうですか。釣り船まで持っているほど好きだなんて、知りませんでした。"},{"id":{"d":280,"n":6,"t":3},"speaker":"B","text":"竹内さんは、海と山のどちらが好きですか？"},{"id":{"d":280,"n":7,"t":3},"speaker":"A","text":"私は山が好きです。大人になってから山登りを初めましたが、すっかり夢中になっています。"},{"id":{"d":280,"n":8,"t":3},"speaker":"B","text":"山登りですか。私は全くしたことがありません。"},{"id":{"d":280,"n":9,"t":3},"speaker":"A","text":"山頂からの景色は絶景ですよ。山道は険しいですが、その分、大きな達成感が得られます。"}];
INSERT INTO sentence [{"id":{"d":281,"n":1,"t":3},"speaker":"A","text":"久美子さんは韓国ドラマがお好きと聞きました。"},{"id":{"d":281,"n":2,"t":3},"speaker":"B","text":"はい。最近、一気に好きになりました。"},{"id":{"d":281,"n":3,"t":3},"speaker":"A","text":"韓国ドラマを見ていると、やはり韓国に行きたくなりますか？"},{"id":{"d":281,"n":4,"t":3},"speaker":"B","text":"はい。ドラマの舞台となった街を訪れてみたいと思っています。"},{"id":{"d":281,"n":5,"t":3},"speaker":"A","text":"日本から韓国への移動時間は、どれくらいかかるのですか？"},{"id":{"d":281,"n":6,"t":3},"speaker":"B","text":"東京からソウルまで、飛行機だと3時間前後で行けるそうです。"},{"id":{"d":281,"n":7,"t":3},"speaker":"A","text":"3時間で行けるのですか。東京から沖縄まで飛行機で行くのと同じくらいですね。"},{"id":{"d":281,"n":8,"t":3},"speaker":"B","text":"はい。そう考えると近く感じますよね。"}];
INSERT INTO sentence [{"id":{"d":282,"n":1,"t":3},"speaker":"A","text":"商店街の福引が当たりました。"},{"id":{"d":282,"n":2,"t":3},"speaker":"B","text":"何等が当たったのですか？"},{"id":{"d":282,"n":3,"t":3},"speaker":"A","text":"1等です。"},{"id":{"d":282,"n":4,"t":3},"speaker":"B","text":"素晴らしいですね。景品は何ですか？"},{"id":{"d":282,"n":5,"t":3},"speaker":"A","text":"神戸港のクルーズ客船のチケットです。"},{"id":{"d":282,"n":6,"t":3},"speaker":"B","text":"それは豪華ですね。クルーズ客船は初めてですか？"},{"id":{"d":282,"n":7,"t":3},"speaker":"A","text":"はい、初めて乗ります。2人1組のチケットなので、一緒にどうですか？"},{"id":{"d":282,"n":8,"t":3},"speaker":"B","text":"うれしいです。私も初めて乗ります。楽しみにしていますね。"}];
INSERT INTO sentence [{"id":{"d":283,"n":1,"t":3},"speaker":"A","text":"今度の日曜日、日帰りでどこかに行きませんか？"},{"id":{"d":283,"n":2,"t":3},"speaker":"B","text":"はい。行きましょう。"},{"id":{"d":283,"n":3,"t":3},"speaker":"A","text":"どこか行きたい所はありますか？"},{"id":{"d":283,"n":4,"t":3},"speaker":"B","text":"少しだけ遠出して、ブドウ狩りをするのはどうですか？秋にぴったりだと思います。"},{"id":{"d":283,"n":5,"t":3},"speaker":"A","text":"いいですね。場所はどこにしましょうか？"},{"id":{"d":283,"n":6,"t":3},"speaker":"B","text":"私も加奈子さんも運転免許を持っていませんので、バスか電車で行ける所がいいです。"},{"id":{"d":283,"n":7,"t":3},"speaker":"A","text":"はい。電車だと駅からかなり歩くかもしれません。バスにしましょうか？"},{"id":{"d":283,"n":8,"t":3},"speaker":"B","text":"そうですね。それでは、ブドウ狩りのバスツアーをインターネットで探してみましょう。"},{"id":{"d":283,"n":9,"t":3},"speaker":"A","text":"はい。良いツアーを見つけたら、また連絡します。"}];
INSERT INTO sentence [{"id":{"d":284,"n":1,"t":3},"speaker":"A","text":"桃子さんの家も、丸山公園のすぐそばでしたよね。"},{"id":{"d":284,"n":2,"t":3},"speaker":"B","text":"はい。そうです。"},{"id":{"d":284,"n":3,"t":3},"speaker":"A","text":"丸山公園に行って、散歩をすることはありますか？"},{"id":{"d":284,"n":4,"t":3},"speaker":"B","text":"いいえ。高校生になってから、めっきり行かなくなりました。"},{"id":{"d":284,"n":5,"t":3},"speaker":"A","text":"私もです。小学生のときはよく丸山公園で遊んでいたのですが、しばらく行っていません。"},{"id":{"d":284,"n":6,"t":3},"speaker":"B","text":"そうですよね。私も小学校の遠足で行ったときが最後です。"},{"id":{"d":284,"n":7,"t":3},"speaker":"A","text":"遠足で行きましたね。懐かしいです。"},{"id":{"d":284,"n":8,"t":3},"speaker":"B","text":"今度、久しぶりに丸山公園に行って散歩をしませんか？"},{"id":{"d":284,"n":9,"t":3},"speaker":"A","text":"いいですね。丸山公園へ行くのは小学生以来なので楽しみです。"}];
INSERT INTO sentence [{"id":{"d":285,"n":1,"t":3},"speaker":"A","text":"松田さんの出身はどちらですか？"},{"id":{"d":285,"n":2,"t":3},"speaker":"B","text":"大阪です。"},{"id":{"d":285,"n":3,"t":3},"speaker":"A","text":"そうですか。松田さんは標準語を話されているので、大阪出身だとは思いませんでした。"},{"id":{"d":285,"n":4,"t":3},"speaker":"B","text":"関西地方の方言は特徴的ですよね。私は東京で過ごす時間が長いので、いつの間にか標準語が定着しました。"},{"id":{"d":285,"n":5,"t":3},"speaker":"A","text":"東京で暮らして何年になるのですか？"},{"id":{"d":285,"n":6,"t":3},"speaker":"B","text":"10年です。大阪弁を話すのは、年末年始に大阪の実家に帰ったときだけです。"},{"id":{"d":285,"n":7,"t":3},"speaker":"A","text":"実家では大阪弁を話されるのですね。"},{"id":{"d":285,"n":8,"t":3},"speaker":"B","text":"はい。家族がみんな大阪弁を話すので、自然と私も大阪弁で会話をしています。"},{"id":{"d":285,"n":9,"t":3},"speaker":"A","text":"なるほど。使い分けをされているのですね。"}];
INSERT INTO sentence [{"id":{"d":286,"n":1,"t":3},"speaker":"A","text":"お兄さんは今年の3月で大学を卒業しますよね。"},{"id":{"d":286,"n":2,"t":3},"speaker":"B","text":"はい。4月からは社会人です。"},{"id":{"d":286,"n":3,"t":3},"speaker":"A","text":"卒業旅行はどこへ行くのですか？"},{"id":{"d":286,"n":4,"t":3},"speaker":"B","text":"行き先の候補を2つにまで絞ったのですが、まだ決まっていません。"},{"id":{"d":286,"n":5,"t":3},"speaker":"A","text":"どことどこですか？"},{"id":{"d":286,"n":6,"t":3},"speaker":"B","text":"福岡と香川です。"},{"id":{"d":286,"n":7,"t":3},"speaker":"A","text":"どうしてその2つなのですか？"},{"id":{"d":286,"n":8,"t":3},"speaker":"B","text":"博多ラーメンを食べるか、讃岐うどんを食べるかで迷っているのです。"},{"id":{"d":286,"n":9,"t":3},"speaker":"A","text":"なるほど。何を食べたいかは重要な選択ですね。"}];
INSERT INTO sentence [{"id":{"d":287,"n":1,"t":3},"speaker":"A","text":"森田さんはサイクリングがお好きでしたよね。"},{"id":{"d":287,"n":2,"t":3},"speaker":"B","text":"はい。そうです。"},{"id":{"d":287,"n":3,"t":3},"speaker":"A","text":"隣町の大きな丸山公園には行ったことがありますか？"},{"id":{"d":287,"n":4,"t":3},"speaker":"B","text":"いいえ、ありません。"},{"id":{"d":287,"n":5,"t":3},"speaker":"A","text":"ぜひ行ってみてください。その公園にはすてきなサイクリングロードがありますよ。"},{"id":{"d":287,"n":6,"t":3},"speaker":"B","text":"そうなのですね。木下さんは行ったことがあるのですか？"},{"id":{"d":287,"n":7,"t":3},"speaker":"A","text":"はい。先日、めいっ子とサイクリングロードを走りました。とても心地よかったです。"},{"id":{"d":287,"n":8,"t":3},"speaker":"B","text":"良い情報をありがとうございます。次の休みに行ってみます。"}];
INSERT INTO sentence [{"id":{"d":288,"n":1,"t":3},"speaker":"A","text":"この辺りは、ヘリコプターや飛行機の音が多くしますね。"},{"id":{"d":288,"n":2,"t":3},"speaker":"B","text":"はい。近くに自衛隊の基地があるのですよ。"},{"id":{"d":288,"n":3,"t":3},"speaker":"A","text":"なるほど。音が気になりませんか？"},{"id":{"d":288,"n":4,"t":3},"speaker":"B","text":"初めは気になりましたが、何年も住んでいるうちに、この音にも慣れました。"},{"id":{"d":288,"n":5,"t":3},"speaker":"A","text":"この町には何年住んでいるのですか？"},{"id":{"d":288,"n":6,"t":3},"speaker":"B","text":"今年で10年になります。"},{"id":{"d":288,"n":7,"t":3},"speaker":"A","text":"10年ですか。それは長いですね。"},{"id":{"d":288,"n":8,"t":3},"speaker":"B","text":"はい。この町にも自分の家にも愛着が湧きました。"}];
INSERT INTO sentence [{"id":{"d":289,"n":1,"t":3},"speaker":"A","text":"林さん、海外出張の予定が入りましたよ。"},{"id":{"d":289,"n":2,"t":3},"speaker":"B","text":"突然ですね。時期はいつですか？"},{"id":{"d":289,"n":3,"t":3},"speaker":"A","text":"来月の1週目です。出張期間は1週間です。"},{"id":{"d":289,"n":4,"t":3},"speaker":"B","text":"場所はどこですか？"},{"id":{"d":289,"n":5,"t":3},"speaker":"A","text":"イギリスのロンドンです。"},{"id":{"d":289,"n":6,"t":3},"speaker":"B","text":"ロンドンですか。東京からは距離がありますね。"},{"id":{"d":289,"n":7,"t":3},"speaker":"A","text":"はい。時差も9時間あります。"},{"id":{"d":289,"n":8,"t":3},"speaker":"B","text":"時差ぼけが心配ですね。イギリスに着いてからすぐに仕事をするのですか？"},{"id":{"d":289,"n":9,"t":3},"speaker":"A","text":"いえ、着いてからは少し時間があります。余裕を持ってスケジュールを立てました。"},{"id":{"d":289,"n":10,"t":3},"speaker":"B","text":"それならよかったです。安心しました。"}];
INSERT INTO sentence [{"id":{"d":290,"n":1,"t":3},"speaker":"A","text":"山下さんは、休みができたらどこに行きたいですか？"},{"id":{"d":290,"n":2,"t":3},"speaker":"B","text":"種子島に行きたいです。"},{"id":{"d":290,"n":3,"t":3},"speaker":"A","text":"種子島ですか。なぜですか？"},{"id":{"d":290,"n":4,"t":3},"speaker":"B","text":"種子島宇宙センターに行きたいのです。"},{"id":{"d":290,"n":5,"t":3},"speaker":"A","text":"日本最大のロケット発射場がある所ですよね。"},{"id":{"d":290,"n":6,"t":3},"speaker":"B","text":"その通りです。ロケットが発射されるところを一度見てみたいです。"},{"id":{"d":290,"n":7,"t":3},"speaker":"A","text":"いいですね。宇宙に関心があるのですか？"},{"id":{"d":290,"n":8,"t":3},"speaker":"B","text":"はい。いつかアメリカのヒューストン宇宙センターにも行ってみたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":291,"n":1,"t":3},"speaker":"A","text":"最近、熱中していることはありますか？"},{"id":{"d":291,"n":2,"t":3},"speaker":"B","text":"私は最近一人旅に熱中しています。"},{"id":{"d":291,"n":3,"t":3},"speaker":"A","text":"私は一人旅をしたことがありませんが、楽しいのですか？"},{"id":{"d":291,"n":4,"t":3},"speaker":"B","text":"気の赴くままに旅ができて楽しいですよ。"},{"id":{"d":291,"n":5,"t":3},"speaker":"A","text":"最近はどこに行きましたか？"},{"id":{"d":291,"n":6,"t":3},"speaker":"B","text":"沖縄の離島に行きました。"},{"id":{"d":291,"n":7,"t":3},"speaker":"A","text":"離島へは船で行ったのですか？"},{"id":{"d":291,"n":8,"t":3},"speaker":"B","text":"はい。沖縄の本島から出ているフェリーに乗って行きました。"},{"id":{"d":291,"n":9,"t":3},"speaker":"A","text":"私は沖縄本島には行ったことがありますが、離島までは行ったことがありませんでした。"},{"id":{"d":291,"n":10,"t":3},"speaker":"B","text":"美しい自然がたくさんあってすてきな場所でした。機会があれば行ってみてください、お勧めですよ。"}];
INSERT INTO sentence [{"id":{"d":292,"n":1,"t":3},"speaker":"A","text":"冬休みの予定は決まっていますか？"},{"id":{"d":292,"n":2,"t":3},"speaker":"B","text":"はい。スキー旅行へ行きます。"},{"id":{"d":292,"n":3,"t":3},"speaker":"A","text":"スキー旅行へは毎年行っているのですか？"},{"id":{"d":292,"n":4,"t":3},"speaker":"B","text":"はい。毎年、家族全員で行っています。"},{"id":{"d":292,"n":5,"t":3},"speaker":"A","text":"いいですね。どこのスキー場に行くのですか？"},{"id":{"d":292,"n":6,"t":3},"speaker":"B","text":"新潟です。駅直結のゲレンデがあるので、そこへ行っています。"},{"id":{"d":292,"n":7,"t":3},"speaker":"A","text":"それは便利ですね。"},{"id":{"d":292,"n":8,"t":3},"speaker":"B","text":"飯田さんは、スキーはしませんか？"},{"id":{"d":292,"n":9,"t":3},"speaker":"A","text":"はい。私は運動も寒い所も苦手なのでスキーの経験はありません。"},{"id":{"d":292,"n":10,"t":3},"speaker":"B","text":"そうですか。冬休みは何をするのですか？"},{"id":{"d":292,"n":11,"t":3},"speaker":"A","text":"こたつに入って温まりながら、ゆっくりテレビを見ます。"},{"id":{"d":292,"n":12,"t":3},"speaker":"B","text":"その過ごし方もすてきですね。"}];
INSERT INTO sentence [{"id":{"d":293,"n":1,"t":3},"speaker":"A","text":"暖かくなりましたね。"},{"id":{"d":293,"n":2,"t":3},"speaker":"B","text":"はい。もうすぐ春ですね。"},{"id":{"d":293,"n":3,"t":3},"speaker":"A","text":"九州の方ではサクラが開花し始めたそうですよ。"},{"id":{"d":293,"n":4,"t":3},"speaker":"B","text":"そうですか。それでは東京のサクラももうすぐ咲きますね。"},{"id":{"d":293,"n":5,"t":3},"speaker":"A","text":"満開になるのが楽しみですね。今年こそは花見に行きたいです。"},{"id":{"d":293,"n":6,"t":3},"speaker":"B","text":"いいですね。去年はなかなかタイミングが合わず、花見ができませんでした。"},{"id":{"d":293,"n":7,"t":3},"speaker":"A","text":"そうでしたね。今年は行けそうですか？"},{"id":{"d":293,"n":8,"t":3},"speaker":"B","text":"はい。昨年よりは仕事のスケジュールに余裕があります。"},{"id":{"d":293,"n":9,"t":3},"speaker":"A","text":"よかったです。あなたと一緒にきれいなサクラが見られることを、楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":294,"n":1,"t":3},"speaker":"A","text":"良さんは運転が上手ですね。"},{"id":{"d":294,"n":2,"t":3},"speaker":"B","text":"うれしいです。ありがとうございます。"},{"id":{"d":294,"n":3,"t":3},"speaker":"A","text":"普段よく車を運転しているのですか？"},{"id":{"d":294,"n":4,"t":3},"speaker":"B","text":"はい。これまでは家で過ごすことが多かったのですが、免許を取ってからは週末にドライブをすることが多くなりました。"},{"id":{"d":294,"n":5,"t":3},"speaker":"A","text":"そうでしたか。ところで、最近、ガソリンは入れましたか？"},{"id":{"d":294,"n":6,"t":3},"speaker":"B","text":"そういえば、しばらく入れていませんね。そろそろ入れた方がいいかもしれません。"},{"id":{"d":294,"n":7,"t":3},"speaker":"A","text":"それでは近くのガソリンスタンドに寄りましょう。"},{"id":{"d":294,"n":8,"t":3},"speaker":"B","text":"はい。カーナビゲーションシステムで検索してもらえますか？"},{"id":{"d":294,"n":9,"t":3},"speaker":"A","text":"分かりました。調べてみます。"}];
INSERT INTO sentence [{"id":{"d":295,"n":1,"t":3},"speaker":"A","text":"やっと旅館に着きました。道が混んでいて大変でしたね。"},{"id":{"d":295,"n":2,"t":3},"speaker":"B","text":"そうですね。早速ですが、まずは温泉に行きましょう。"},{"id":{"d":295,"n":3,"t":3},"speaker":"A","text":"行きましょう。着替えの準備をします。"},{"id":{"d":295,"n":4,"t":3},"speaker":"B","text":"私もします。"},{"id":{"d":295,"n":5,"t":3},"speaker":"A","text":"あら、私は着替えの下着を家に忘れてきてしまったようです。"},{"id":{"d":295,"n":6,"t":3},"speaker":"B","text":"それは大変ですね。"},{"id":{"d":295,"n":7,"t":3},"speaker":"A","text":"うっかりしていました。近くのコンビニエンスストアに行って買ってきますね。"},{"id":{"d":295,"n":8,"t":3},"speaker":"B","text":"分かりました。私は部屋で待っていますね。"},{"id":{"d":295,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。急いで行ってきます。"}];
INSERT INTO sentence [{"id":{"d":296,"n":1,"t":3},"speaker":"A","text":"山中さんは、夜行バスを使って旅行をしたことはありますか？"},{"id":{"d":296,"n":2,"t":3},"speaker":"B","text":"はい。何度もありますよ。"},{"id":{"d":296,"n":3,"t":3},"speaker":"A","text":"そうですか。夜行バスで寝づらい思いをしたことはありませんか？"},{"id":{"d":296,"n":4,"t":3},"speaker":"B","text":"いいえ、ありません。私は横にならなくてもすぐに眠ることができますから。"},{"id":{"d":296,"n":5,"t":3},"speaker":"A","text":"それはいいですね。うらやましいです。"},{"id":{"d":296,"n":6,"t":3},"speaker":"B","text":"江藤さんは夜行バスだと寝づらいのですか？"},{"id":{"d":296,"n":7,"t":3},"speaker":"A","text":"はい。振動や音が気になってしまいます。"},{"id":{"d":296,"n":8,"t":3},"speaker":"B","text":"そうなのですね。飛行機や新幹線ではどうですか？"},{"id":{"d":296,"n":9,"t":3},"speaker":"A","text":"飛行機や新幹線でもすぐには眠れません。寝ようと思ってから眠るまでにだいぶ時間がかかってしまいます。"},{"id":{"d":296,"n":10,"t":3},"speaker":"B","text":"それではベッド付きの客室がある旅客船が一番良いですね。"},{"id":{"d":296,"n":11,"t":3},"speaker":"A","text":"確かに、それなら眠れると思います。"}];
INSERT INTO sentence [{"id":{"d":297,"n":1,"t":3},"speaker":"A","text":"酒井さんは将来、都会と田舎のどちらで暮らしたいですか？"},{"id":{"d":297,"n":2,"t":3},"speaker":"B","text":"私は田舎で暮らしたいです。"},{"id":{"d":297,"n":3,"t":3},"speaker":"A","text":"それはなぜですか？"},{"id":{"d":297,"n":4,"t":3},"speaker":"B","text":"田舎は、夜の星空がきれいだからです。きれいな風景に囲まれた生活に憧れます。"},{"id":{"d":297,"n":5,"t":3},"speaker":"A","text":"確かにきれいですね。私も田舎の町に旅行に行ったときは星空のきれいさに驚きました。"},{"id":{"d":297,"n":6,"t":3},"speaker":"B","text":"高い建物がないと星がよく見えますよね。"},{"id":{"d":297,"n":7,"t":3},"speaker":"A","text":"はい。都会では味わえない景色でした。"},{"id":{"d":297,"n":8,"t":3},"speaker":"B","text":"それでは山崎さんも田舎で暮らしたいですか？"},{"id":{"d":297,"n":9,"t":3},"speaker":"A","text":"いいえ、私は都会暮らしがいいです。"},{"id":{"d":297,"n":10,"t":3},"speaker":"B","text":"そうですか。なぜですか？"},{"id":{"d":297,"n":11,"t":3},"speaker":"A","text":"ショッピングやカフェ巡りが趣味だからです。私はお店がたくさんある町で生活したいです。"},{"id":{"d":297,"n":12,"t":3},"speaker":"B","text":"なるほど。それでは田舎より都会がいいですね。"}];
INSERT INTO sentence [{"id":{"d":298,"n":1,"t":3},"speaker":"A","text":"福井さんは、ブドウ狩りに行ったことはありますか？"},{"id":{"d":298,"n":2,"t":3},"speaker":"B","text":"はい。あります。橋本さんはどうですか？"},{"id":{"d":298,"n":3,"t":3},"speaker":"A","text":"私はまだありません。今年行ってみようかと思っているのです。"},{"id":{"d":298,"n":4,"t":3},"speaker":"B","text":"そうでしたか。"},{"id":{"d":298,"n":5,"t":3},"speaker":"A","text":"はい。ブドウ狩りに行ったときの感想を聞かせていただけませんか？"},{"id":{"d":298,"n":6,"t":3},"speaker":"B","text":"おいしいブドウを好きなだけ食べることができてとても楽しかったですよ。ただ、思っていたよりもすぐにおなかがいっぱいになってしまいました。"},{"id":{"d":298,"n":7,"t":3},"speaker":"A","text":"そうですか。ではおなかをすかせていく方が良さそうですね。"},{"id":{"d":298,"n":8,"t":3},"speaker":"B","text":"はい。そうするといいと思います。"}];
INSERT INTO sentence [{"id":{"d":299,"n":1,"t":3},"speaker":"A","text":"今回の旅行は大変でした。"},{"id":{"d":299,"n":2,"t":3},"speaker":"B","text":"何があったのですか？"},{"id":{"d":299,"n":3,"t":3},"speaker":"A","text":"帰りの飛行機の時間に間に合わなかったのです。"},{"id":{"d":299,"n":4,"t":3},"speaker":"B","text":"それは災難でしたね。どうして間に合わなかったのですか？"},{"id":{"d":299,"n":5,"t":3},"speaker":"A","text":"ホテルから空港までタクシーに乗ったのですが、道が渋滞していたのです。"},{"id":{"d":299,"n":6,"t":3},"speaker":"B","text":"なるほど。移動に時間がかかってしまったのですね。"},{"id":{"d":299,"n":7,"t":3},"speaker":"A","text":"そうです。30分のはずが2時間もかかってしまいました。"},{"id":{"d":299,"n":8,"t":3},"speaker":"B","text":"大渋滞に巻き込まれてしまったのですね。"},{"id":{"d":299,"n":9,"t":3},"speaker":"A","text":"はい。時間に余裕を持ってスケジュールを立てなければいけないと痛感しました。"}];
INSERT INTO sentence [{"id":{"d":300,"n":1,"t":3},"speaker":"A","text":"横山さんは海外旅行をしたことがありますか？"},{"id":{"d":300,"n":2,"t":3},"speaker":"B","text":"はい。インドに行ったことがあります。"},{"id":{"d":300,"n":3,"t":3},"speaker":"A","text":"インドですか。私は行ったことがありません。インドはどうでしたか？"},{"id":{"d":300,"n":4,"t":3},"speaker":"B","text":"珍しい経験がたくさんできて楽しかったですよ。"},{"id":{"d":300,"n":5,"t":3},"speaker":"A","text":"どのようなことをしたのですか？"},{"id":{"d":300,"n":6,"t":3},"speaker":"B","text":"象に乗ったりサリーを着たりしました。"},{"id":{"d":300,"n":7,"t":3},"speaker":"A","text":"インドならではの体験がたくさんできたのですね。"},{"id":{"d":300,"n":8,"t":3},"speaker":"B","text":"はい。もちろん、カレーも食べましたよ。日本で食べる味とは違いました。"},{"id":{"d":300,"n":9,"t":3},"speaker":"A","text":"そうですか。横山さんのお話を聞いていたら、インドに行きたくなりました。"}];
INSERT INTO sentence [{"id":{"d":301,"n":1,"t":3},"speaker":"A","text":"馬場さんは英語の発音がとても流ちょうですね。"},{"id":{"d":301,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":301,"n":3,"t":3},"speaker":"A","text":"英語圏で生活されたことがあるのですか？"},{"id":{"d":301,"n":4,"t":3},"speaker":"B","text":"はい。大学3年生のときに1年間留学をしていました。"},{"id":{"d":301,"n":5,"t":3},"speaker":"A","text":"そうですか。どこに留学したのですか？"},{"id":{"d":301,"n":6,"t":3},"speaker":"B","text":"カナダです。"},{"id":{"d":301,"n":7,"t":3},"speaker":"A","text":"会話は全て英語だったのですか？"},{"id":{"d":301,"n":8,"t":3},"speaker":"B","text":"はい。大学の授業も英語だったので初めは苦労しました。"},{"id":{"d":301,"n":9,"t":3},"speaker":"A","text":"そうでしょうね。私は想像するだけで心が折れてしまいます。"}];
INSERT INTO sentence [{"id":{"d":302,"n":1,"t":3},"speaker":"A","text":"今日は高速道路が混んでいませんね。"},{"id":{"d":302,"n":2,"t":3},"speaker":"B","text":"はい。順調に進んでいます。"},{"id":{"d":302,"n":3,"t":3},"speaker":"A","text":"運転も快適ですか？"},{"id":{"d":302,"n":4,"t":3},"speaker":"B","text":"はい。混んでいないときの運転はいっそう楽しいですね。"},{"id":{"d":302,"n":5,"t":3},"speaker":"A","text":"それはよかったです。運転し始めてからどのくらい時間がたったでしょうか？"},{"id":{"d":302,"n":6,"t":3},"speaker":"B","text":"2時間はたった気がします。"},{"id":{"d":302,"n":7,"t":3},"speaker":"A","text":"だいぶ長く運転していますね。そろそろ休憩所に寄りませんか？"},{"id":{"d":302,"n":8,"t":3},"speaker":"B","text":"そうですね。次のサービスエリアに寄りましょうか？"},{"id":{"d":302,"n":9,"t":3},"speaker":"A","text":"そうしましょう。そこでお昼も食べましょう。"}];
INSERT INTO sentence [{"id":{"d":303,"n":1,"t":3},"speaker":"A","text":"旅館の雰囲気がとてもすてきですね。布団も柔らかくて、ゆっくり眠れそうです。"},{"id":{"d":303,"n":2,"t":3},"speaker":"B","text":"そうですね。すてきな旅館でよかったです。"},{"id":{"d":303,"n":3,"t":3},"speaker":"A","text":"おばあちゃんは明日の朝何時に起きますか？"},{"id":{"d":303,"n":4,"t":3},"speaker":"B","text":"6時に起きようと思います。"},{"id":{"d":303,"n":5,"t":3},"speaker":"A","text":"早いですね。何かするのですか？"},{"id":{"d":303,"n":6,"t":3},"speaker":"B","text":"せっかく温泉付きの旅館に来たので、朝風呂をしようと思います。"},{"id":{"d":303,"n":7,"t":3},"speaker":"A","text":"温泉は朝早くから開いているのですか？"},{"id":{"d":303,"n":8,"t":3},"speaker":"B","text":"はい。朝の6時から開いているそうですよ。"},{"id":{"d":303,"n":9,"t":3},"speaker":"A","text":"そうですか。それでは私も6時起きますので、一緒に温泉に行きましょう。"},{"id":{"d":303,"n":10,"t":3},"speaker":"B","text":"はい。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":304,"n":1,"t":3},"speaker":"A","text":"理子さん、今年の春休みに私と一緒に卒業旅行をしませんか？"},{"id":{"d":304,"n":2,"t":3},"speaker":"B","text":"もちろんです。どこに行きましょうか？"},{"id":{"d":304,"n":3,"t":3},"speaker":"A","text":"理子さんの行きたい場所はありますか？"},{"id":{"d":304,"n":4,"t":3},"speaker":"B","text":"せっかくの機会なので海外に行きたいです。"},{"id":{"d":304,"n":5,"t":3},"speaker":"A","text":"それではフランスはどうですか？"},{"id":{"d":304,"n":6,"t":3},"speaker":"B","text":"いいですね。私たちは大学でフランス語の授業を取っていたので、卒業旅行の行き先としてぴったりだと思います。"},{"id":{"d":304,"n":7,"t":3},"speaker":"A","text":"よかったです。では、フランスに行きましょう。"},{"id":{"d":304,"n":8,"t":3},"speaker":"B","text":"そうしましょう。良いツアーがないか調べてみますね。"},{"id":{"d":304,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。私も調べてみます。"}];
INSERT INTO sentence [{"id":{"d":305,"n":1,"t":3},"speaker":"A","text":"今月末の週末は一緒に釣りに行く予定でしたよね。"},{"id":{"d":305,"n":2,"t":3},"speaker":"B","text":"はい。船も予約しました。"},{"id":{"d":305,"n":3,"t":3},"speaker":"A","text":"申し訳ないのですが、行けなくなってしまいました。"},{"id":{"d":305,"n":4,"t":3},"speaker":"B","text":"そうですか。それは残念です。"},{"id":{"d":305,"n":5,"t":3},"speaker":"A","text":"本当にすみません。とても楽しみにしていたので、私も残念です。"},{"id":{"d":305,"n":6,"t":3},"speaker":"B","text":"仕事が入ってしまったのですか？"},{"id":{"d":305,"n":7,"t":3},"speaker":"A","text":"はい。仕事で、新人研修の仕切りを任されることになったのです。"},{"id":{"d":305,"n":8,"t":3},"speaker":"B","text":"それは大事な役目ですね。魚釣りはいつでも行けるので、また予定を合わせて行きましょう。"},{"id":{"d":305,"n":9,"t":3},"speaker":"A","text":"すみません。ありがとうございます。"},{"id":{"d":305,"n":10,"t":3},"speaker":"B","text":"船の予約もキャンセルしておきますね。また仕事の休みの日が分かったら教えてください。"},{"id":{"d":305,"n":11,"t":3},"speaker":"A","text":"はい。予定が分かり次第すぐに伝えます。"}];
INSERT INTO sentence [{"id":{"d":306,"n":1,"t":3},"speaker":"A","text":"日曜日に子どもたちを連れてどこかに行きませんか？"},{"id":{"d":306,"n":2,"t":3},"speaker":"B","text":"そうですね。久しぶりに家族で出かけましょうか？"},{"id":{"d":306,"n":3,"t":3},"speaker":"A","text":"はい。日帰りで行ける所で、何か思いつく場所はありますか？"},{"id":{"d":306,"n":4,"t":3},"speaker":"B","text":"動物園か水族館はどうですか？"},{"id":{"d":306,"n":5,"t":3},"speaker":"A","text":"いいですね。日帰りで行くにはちょうど良さそうです。"},{"id":{"d":306,"n":6,"t":3},"speaker":"B","text":"はい。どちらが良いですか？"},{"id":{"d":306,"n":7,"t":3},"speaker":"A","text":"子どもたちは保育園の遠足で動物園に行く予定があるので、水族館にしましょうか？"},{"id":{"d":306,"n":8,"t":3},"speaker":"B","text":"そうですか。それでは水族館に行きましょう。私が車を出します。"},{"id":{"d":306,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":307,"n":1,"t":3},"speaker":"A","text":"今は行くことができませんが、海外旅行ができるようになった場合、行きたい国はどこですか？"},{"id":{"d":307,"n":2,"t":3},"speaker":"B","text":"私はサッカーが大好きなのでヨーロッパに行きたいです。特にスペインとイギリスに行きたいです。"},{"id":{"d":307,"n":3,"t":3},"speaker":"A","text":"外国のサッカーの試合はとても白熱していると聞いたことがあります。"},{"id":{"d":307,"n":4,"t":3},"speaker":"B","text":"国内では感じることができないほど盛り上がるようです。木村さんはどこに行きたいですか？"},{"id":{"d":307,"n":5,"t":3},"speaker":"A","text":"私もヨーロッパに行きたいです。特に北欧に行きたいと思っています。"},{"id":{"d":307,"n":6,"t":3},"speaker":"B","text":"北欧は建物もきれいでどの風景も絵になりますね。"},{"id":{"d":307,"n":7,"t":3},"speaker":"A","text":"はい。それを実際に見てみたいです。"},{"id":{"d":307,"n":8,"t":3},"speaker":"B","text":"素晴らしい体験ができそうですね。"}];
INSERT INTO sentence [{"id":{"d":308,"n":1,"t":3},"speaker":"A","text":"卒業旅行で大学の友人と大阪に行ってきました。"},{"id":{"d":308,"n":2,"t":3},"speaker":"B","text":"とても楽しそうですね。ホテルに泊まりましたか？それとも、旅館に泊まりましたか？"},{"id":{"d":308,"n":3,"t":3},"speaker":"A","text":"ホテルに泊まりました。学生なので少し安めのホテルにしたのですが、アメニティーが充実していてとても快適でした。"},{"id":{"d":308,"n":4,"t":3},"speaker":"B","text":"最近のホテルはとても充実していますよね。売店や温泉もとても充実しているホテルが多いと聞きます。大阪の方々の雰囲気はどうでしたか？"},{"id":{"d":308,"n":5,"t":3},"speaker":"A","text":"最初は大阪弁を少し怖く感じたのですが、慣れるにつれて人の温かみを感じました。"},{"id":{"d":308,"n":6,"t":3},"speaker":"B","text":"それはよかったですね。"},{"id":{"d":308,"n":7,"t":3},"speaker":"A","text":"方言に加えてユーモアのある方も多かったので、とても楽しい旅行でした。粉物もおいしかったです。"},{"id":{"d":308,"n":8,"t":3},"speaker":"B","text":"たこ焼きやお好み焼きはやはり大阪が一番おいしいですね。"},{"id":{"d":308,"n":9,"t":3},"speaker":"A","text":"はい。粉物を食べに、また大阪へ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":309,"n":1,"t":3},"speaker":"A","text":"昨年車で北海道横断をしました。運転には疲れましたが楽しかったです。"},{"id":{"d":309,"n":2,"t":3},"speaker":"B","text":"楽しそうですね。1人で行ったのですか？"},{"id":{"d":309,"n":3,"t":3},"speaker":"A","text":"はい。1,200キロメートルを2日間で走り切りましたよ。"},{"id":{"d":309,"n":4,"t":3},"speaker":"B","text":"それは疲れそうですね。夜はどこかホテルに泊まりましたか？"},{"id":{"d":309,"n":5,"t":3},"speaker":"A","text":"1日でどこまで行けるか分からなかったので、ホテルは予約せずに、車中泊をしました。"},{"id":{"d":309,"n":6,"t":3},"speaker":"B","text":"それもまた楽しそうですね。"},{"id":{"d":309,"n":7,"t":3},"speaker":"A","text":"ただ、さすがに長時間座っていたので腰が痛くなり、次の日は半日横になっていました。"},{"id":{"d":309,"n":8,"t":3},"speaker":"B","text":"代償はあったようですが、大学生で時間がある時に思い出に残る楽しいことができてよかったですね。"}];
INSERT INTO sentence [{"id":{"d":310,"n":1,"t":3},"speaker":"A","text":"居酒屋に行ったときに、世界一周旅行の旅という張り紙を見ました。"},{"id":{"d":310,"n":2,"t":3},"speaker":"B","text":"世界一周旅行ですか。いいですね、とても楽しそうです。"},{"id":{"d":310,"n":3,"t":3},"speaker":"A","text":"その旅行は船で回るもので、有名な観光地には1日程度宿泊できるそうですが、他の日は全て船で寝ると書いてありました。"},{"id":{"d":310,"n":4,"t":3},"speaker":"B","text":"船にまだ慣れていない旅の最初のころは、眠るのが大変そうですね。"},{"id":{"d":310,"n":5,"t":3},"speaker":"A","text":"そうですね。飲食はレストランのようなものがあるようなのですが、食費はツアー代に入っていないそうです。"},{"id":{"d":310,"n":6,"t":3},"speaker":"B","text":"毎回外食のようになるということですか、食費が高くつきそうですね。ちなみに費用はどれくらいですか？"},{"id":{"d":310,"n":7,"t":3},"speaker":"A","text":"ツアー代が99万円と書いてありました。ただこれは学生が行く場合の値段と書いてありましたので、社会人の場合はもっと高くなると思われます。"},{"id":{"d":310,"n":8,"t":3},"speaker":"B","text":"世界一周には憧れますが、その値段でしたら、自分が行きたい国に1週間ほど滞在した方がいいかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":311,"n":1,"t":3},"speaker":"A","text":"先日北海道に行ったのですが、どの居酒屋でも、料理の値段が安いことに驚きました。"},{"id":{"d":311,"n":2,"t":3},"speaker":"B","text":"どれくらい安いのですか？"},{"id":{"d":311,"n":3,"t":3},"speaker":"A","text":"東京の居酒屋の半分くらいの値段しかしませんでしたよ。"},{"id":{"d":311,"n":4,"t":3},"speaker":"B","text":"それは相当安いですね。料理の質や提供のスピードはどうですか？"},{"id":{"d":311,"n":5,"t":3},"speaker":"A","text":"料理の質に関してはさすが北海道という感じで、簡単なおつまみでもおいしかったです。提供スピードも変に早い遅いはありませんでした。"},{"id":{"d":311,"n":6,"t":3},"speaker":"B","text":"素晴らしいですね。外食のことを考えたら北海道に移住すると楽しそうですね。"}];
INSERT INTO sentence [{"id":{"d":312,"n":1,"t":3},"speaker":"A","text":"回転ずしにはよく行きますか？"},{"id":{"d":312,"n":2,"t":3},"speaker":"B","text":"最近はめったに行きませんが、幼い頃はご褒美として家族と行っていました。"},{"id":{"d":312,"n":3,"t":3},"speaker":"A","text":"回転ずしに行くと小さい子も喜びますからね。"},{"id":{"d":312,"n":4,"t":3},"speaker":"B","text":"あなたは学生時代を北海道で過ごしていたのですよね？北海道の回転ずしは、やはりおいしいのですか？"},{"id":{"d":312,"n":5,"t":3},"speaker":"A","text":"おいしかったですよ。魚介は新鮮ですし、北海道産のウニやカニなども安く食べられたので、友達とよく行っていました。"},{"id":{"d":312,"n":6,"t":3},"speaker":"B","text":"回らないすしを食べたことはありますか？"},{"id":{"d":312,"n":7,"t":3},"speaker":"A","text":"あります。北海道で何度か行きました。後輩がアルバイトしている店に行ったときは、安くしてもらったこともありました。"},{"id":{"d":312,"n":8,"t":3},"speaker":"B","text":"うらやましいです。本州だと、回らないすし屋には社会人でもなかなか行けませんよ。"}];
INSERT INTO sentence [{"id":{"d":313,"n":1,"t":3},"speaker":"A","text":"自動車を所有している人が減っているというニュースが、テレビで放送されていました。"},{"id":{"d":313,"n":2,"t":3},"speaker":"B","text":"若者の車離れとは最近よく言われますね。なぜなのでしょうか？"},{"id":{"d":313,"n":3,"t":3},"speaker":"A","text":"維持費が高いことが大きな理由として挙げられると思います。ガソリンも最近値上げされていることもあり、どんどん維持費は高くなっています。"},{"id":{"d":313,"n":4,"t":3},"speaker":"B","text":"維持費だけの問題なのでしょうか？免許取得率はあまり変わっていませんか？"},{"id":{"d":313,"n":5,"t":3},"speaker":"A","text":"免許取得率自体も下がってきているようです。また、免許証は持っていてもペーパードライバーの方も多くいるようです。"},{"id":{"d":313,"n":6,"t":3},"speaker":"B","text":"若者の車離れを食い止めるために、何か良い策はないのでしょうか？"},{"id":{"d":313,"n":7,"t":3},"speaker":"A","text":"最近は、カーシェアリングやレンタカーを利用する人が増えているようです。これらのサービスは、自分で車を所有していなくても時間単位や日単位で借りることができるというものです。これらを利用する若者の数を増やし、まずは、彼らが車に触れる機会を増やすことが大事だと思います。"},{"id":{"d":313,"n":8,"t":3},"speaker":"B","text":"そのような乗り方をして車の良さを知ってもらうしかありませんね。車の所有率を上げるのは、まだまだ難しそうです。"}];
INSERT INTO sentence [{"id":{"d":314,"n":1,"t":3},"speaker":"A","text":"最近は修学旅行に行くことができない中学生、高校生がいるようです。"},{"id":{"d":314,"n":2,"t":3},"speaker":"B","text":"感染症の影響で誰も責めることができない分かわいそうですね。"},{"id":{"d":314,"n":3,"t":3},"speaker":"A","text":"そのような中ある高校では、修学旅行気分だけでも味わってもらうために修学旅行で行く予定だった場所のビデオを見せているそうです。"},{"id":{"d":314,"n":4,"t":3},"speaker":"B","text":"あまり良い案とは思えません。もちろん歴史的建造物やその土地の特徴を感じることも修学旅行の目的ではありますが、それ以上に、友人と旅行に行きその時間を心から楽しむことが大切なのだと思います。"},{"id":{"d":314,"n":5,"t":3},"speaker":"A","text":"そうですね。学校でない非日常の中で、自分たちで決めたコースを歩きながら、友人とたわいもない話をすることが良い経験になるはずです。"},{"id":{"d":314,"n":6,"t":3},"speaker":"B","text":"私もそう思います。学校でビデオをおとなしく見るというのは、意味をなさないどころか行けなかったことを強調してしまうような気がしてしまいます。"},{"id":{"d":314,"n":7,"t":3},"speaker":"A","text":"その高校はどのような考えを持ってビデオを見せる選択をしたのでしょうか？とても気になりますね。"},{"id":{"d":314,"n":8,"t":3},"speaker":"B","text":"ええ。ぜひ聞いてみたいところですね。"}];
INSERT INTO sentence [{"id":{"d":315,"n":1,"t":3},"speaker":"A","text":"3月に北海道に行くのですが、その時期の北海道は飛行機もホテルもとても高いです。"},{"id":{"d":315,"n":2,"t":3},"speaker":"B","text":"雪まつりの時期でしょうか？"},{"id":{"d":315,"n":3,"t":3},"speaker":"A","text":"雪まつりは2月の上旬から下旬にかけて開催されるので、雪まつりの影響ではないと思います。"},{"id":{"d":315,"n":4,"t":3},"speaker":"B","text":"では、どうして3月も高いのでしょうか？"},{"id":{"d":315,"n":5,"t":3},"speaker":"A","text":"3月は春休みの時期なので、純粋に旅行シーズンということで価格が高騰しているのかもしれません。"},{"id":{"d":315,"n":6,"t":3},"speaker":"B","text":"確かに、卒業旅行などもその時期が多いですね。"}];
INSERT INTO sentence [{"id":{"d":316,"n":1,"t":3},"speaker":"A","text":"北海道札幌市では地下鉄が発達しているということを知っていましたか？"},{"id":{"d":316,"n":2,"t":3},"speaker":"B","text":"知りませんでした。JR北海道は冬にニュースになることが多いので存在は知っていましたが、地下鉄までは知りませんでした。"},{"id":{"d":316,"n":3,"t":3},"speaker":"A","text":"北海道の冬は雪がとても降るので、降雪の影響を受けづらい地下鉄が大活躍しています。"},{"id":{"d":316,"n":4,"t":3},"speaker":"B","text":"なるほど、そういう理由があるのですね。地下鉄でどこまで行くことができるのですか？"},{"id":{"d":316,"n":5,"t":3},"speaker":"A","text":"基本的に札幌市内であればどこへでも行けます。乗り換えも地上に出る必要がありません。"},{"id":{"d":316,"n":6,"t":3},"speaker":"B","text":"それはとても便利ですね。札幌で暮らす人々にとって、地下鉄は欠かせないのですね。"},{"id":{"d":316,"n":7,"t":3},"speaker":"A","text":"実際、利用者はとても多いですよ。特に、通勤時間の札幌駅は、毎日多くの人で混在しています。"},{"id":{"d":316,"n":8,"t":3},"speaker":"B","text":"電車の本数が少ないのですか？"},{"id":{"d":316,"n":9,"t":3},"speaker":"A","text":"いいえ、そのようなことはありません。早い時には約5分間隔で電車が来ます。それでも混雑するほどに利用者が多いのです。"}];
INSERT INTO sentence [{"id":{"d":317,"n":1,"t":3},"speaker":"A","text":"大阪弁や博多弁など、方言にはさまざまなものが存在しますね。"},{"id":{"d":317,"n":2,"t":3},"speaker":"B","text":"関東には方言がありませんので、方言のある地域に暮らす人をうらやましく感じるときがあります。"},{"id":{"d":317,"n":3,"t":3},"speaker":"A","text":"方言を話しているとそれだけで魅力のように感じますよね。博多弁を話しているとかわいいと言われたり、大阪弁は格好良いと言われたりします。"},{"id":{"d":317,"n":4,"t":3},"speaker":"B","text":"方言の中で、私が一番かわいいと思うのは北海道弁です。"},{"id":{"d":317,"n":5,"t":3},"speaker":"A","text":"北海道弁といえば、カーリング女子が「そだねー」と言っていたことが大きく取り上げられましたね。"},{"id":{"d":317,"n":6,"t":3},"speaker":"B","text":"はい。それ以外にも「なまら」や「しばれる」などは有名だと思います。しかし、このような有名な方言だけでなく、日常会話で使うような言葉もかわいいと思うのです。"},{"id":{"d":317,"n":7,"t":3},"speaker":"A","text":"どのような言葉ですか？"},{"id":{"d":317,"n":8,"t":3},"speaker":"B","text":"語尾に「なのさー」や「さー」をつけるところなどです。「走ってきた」ことを「走ってきたさー」と言うのですよ。ほんわかしていてかわいらしいと思いませんか？"},{"id":{"d":317,"n":9,"t":3},"speaker":"A","text":"柔らかな響きですてきですね。"}];
INSERT INTO sentence [{"id":{"d":318,"n":1,"t":3},"speaker":"A","text":"健康志向が強くなってきていることから、自転車の人気が上がっているようです。"},{"id":{"d":318,"n":2,"t":3},"speaker":"B","text":"確かに、最近はロードバイクに乗っている人をよく見ますね。"},{"id":{"d":318,"n":3,"t":3},"speaker":"A","text":"あのような自転車は、安いものだと10万円を切りますが、高いものは100万円も超えるようです。"},{"id":{"d":318,"n":4,"t":3},"speaker":"B","text":"とても高いですね。"},{"id":{"d":318,"n":5,"t":3},"speaker":"A","text":"私の知り合いには、フレームだけ購入してギアやタイヤなどは別に購入し、自分好みにカスタマイズしている人もいます。"},{"id":{"d":318,"n":6,"t":3},"speaker":"B","text":"車やバイクをカスタマイズしている人は見かけますが、自転車をカスタマイズしている人もいるのですね。驚きました。"},{"id":{"d":318,"n":7,"t":3},"speaker":"A","text":"パーツだけで10万円を超すものもあるそうですよ。彼らは、自分に合うパーツを選んで、それを組み合わせることで、自分のスタイルに適したオリジナルの自転車を作ることもできるそうです。"},{"id":{"d":318,"n":8,"t":3},"speaker":"B","text":"運転しやすいように自分に合わせてカスタマイズすることはとても楽しそうです。興味が湧きました。"}];
INSERT INTO sentence [{"id":{"d":319,"n":1,"t":3},"speaker":"A","text":"日本のパトカーは、時速180キロメートル以上出ている車を追いかけないということを知っていましたか？"},{"id":{"d":319,"n":2,"t":3},"speaker":"B","text":"知りませんでした。スピード違反をしていても、制限速度を大幅に超過している場合は捕まらないということですか？"},{"id":{"d":319,"n":3,"t":3},"speaker":"A","text":"恐らくナンバーは控えられるので罪には問われると思いますが、その場では捕まらないそうです。"},{"id":{"d":319,"n":4,"t":3},"speaker":"B","text":"なぜ時速180キロメートルを超えると追いかけられないのですか？"},{"id":{"d":319,"n":5,"t":3},"speaker":"A","text":"一般的なパトカーにはリミッターがついていて、時速180キロメートル以上出せないのです。もちろん、ポルシェやGT‐Rのようなスピードが出る車をパトカーにしている場合は、時速180キロメートルを超えていても追跡できます。"},{"id":{"d":319,"n":6,"t":3},"speaker":"B","text":"なるほど、そういうことでしたか。時速180キロメートル以上出ている車を追いかけないというよりも、追いかけることができないのですね。"}];
INSERT INTO sentence [{"id":{"d":320,"n":1,"t":3},"speaker":"A","text":"空港のラウンジに行ったことはありますか？"},{"id":{"d":320,"n":2,"t":3},"speaker":"B","text":"ありません。ANAやJALのラウンジがとても豪華だということは話に聞いたことがありますが、普段LCCを使うので行く機会がありません。"},{"id":{"d":320,"n":3,"t":3},"speaker":"A","text":"ANAやJALのラウンジはもちろん豪華で、飲み物やお菓子も充実しています。それに加えて、一部の空港にはカードラウンジという場所もあります。"},{"id":{"d":320,"n":4,"t":3},"speaker":"B","text":"初めて聞きました。カードラウンジには、どのような人が入ることができるのですか？"},{"id":{"d":320,"n":5,"t":3},"speaker":"A","text":"ある一定以上のランクのカードを持っているが入ることができます。なんとカードを提示するだけで、無料でラウンジを利用することができます。"},{"id":{"d":320,"n":6,"t":3},"speaker":"B","text":"良いことを聞きました。そのような仕組みがあることを知らなかったので、いつも搭乗時間にちょうど間に合うタイミングで空港に着いていました。"},{"id":{"d":320,"n":7,"t":3},"speaker":"A","text":"空港によりますが、カードラウンジでは飲み物の提供を行うほか、充電可能、Wi-Fi完備というところが多くあります。時間をつぶすにはちょうど良い場所ですよ。"},{"id":{"d":320,"n":8,"t":3},"speaker":"B","text":"それは便利ですね。今度空港を利用するときは、ぜひともカードラウンジを利用してみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":321,"n":1,"t":3},"speaker":"A","text":"セグウェイを知っていますか？"},{"id":{"d":321,"n":2,"t":3},"speaker":"B","text":"はい。体重移動で走ることができる機械ですよね。"},{"id":{"d":321,"n":3,"t":3},"speaker":"A","text":"そうです。実際に使用しているところを見たことや使用したことはありますか？"},{"id":{"d":321,"n":4,"t":3},"speaker":"B","text":"遊園地のような所で見たことはありますが、乗ったことはありません。"},{"id":{"d":321,"n":5,"t":3},"speaker":"A","text":"私も乗ったことはないのですが、この前、イヌの散歩にセグウェイを使用している人がいました。"},{"id":{"d":321,"n":6,"t":3},"speaker":"B","text":"最先端の散歩ですね。それほどイヌの動きが速かったのでしょうか？"},{"id":{"d":321,"n":7,"t":3},"speaker":"A","text":"イヌは普通の速さでしたし、セグウェイのスピードも徒歩と同じ程度でした。なぜセグウェイに乗っていたのか不思議です。"},{"id":{"d":321,"n":8,"t":3},"speaker":"B","text":"もしかしたら、セグウェイに乗る練習をしていたのかもしれませんね。私も乗ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":322,"n":1,"t":3},"speaker":"A","text":"コーヒーショップがついているガソリンスタンドが、最近増えてきましたね。"},{"id":{"d":322,"n":2,"t":3},"speaker":"B","text":"私の家の近くにもあります。そこでは、決まった日にコーヒーショップで一定金額購入するとガソリンが安くなるというキャンペーンをしています。"},{"id":{"d":322,"n":3,"t":3},"speaker":"A","text":"最近はガソリンが高いこともあるので、それはありがたいキャンペーンですね。"},{"id":{"d":322,"n":4,"t":3},"speaker":"B","text":"はい。1リットルで7円安くなるので相当安くなります。"},{"id":{"d":322,"n":5,"t":3},"speaker":"A","text":"どのくらいの頻度でガソリンを入れるのですか？"},{"id":{"d":322,"n":6,"t":3},"speaker":"B","text":"1カ月に1回はコーヒー豆を買いに行っているので、その時に毎回入れています。"},{"id":{"d":322,"n":7,"t":3},"speaker":"A","text":"そのガソリンスタンドに行くために必要なガソリン代を計算しても、そこへ行った方が安くなりますか？"},{"id":{"d":322,"n":8,"t":3},"speaker":"B","text":"はい、それを考慮しても安いです。近くに良いガソリンスタンドがあってよかったです。"}];
INSERT INTO sentence [{"id":{"d":323,"n":1,"t":3},"speaker":"A","text":"今一番学びたい言語は何ですか？"},{"id":{"d":323,"n":2,"t":3},"speaker":"B","text":"私はポルトガル語かドイツ語を勉強したいと思っています。"},{"id":{"d":323,"n":3,"t":3},"speaker":"A","text":"なぜその2つの言語を学びたいのですか？"},{"id":{"d":323,"n":4,"t":3},"speaker":"B","text":"サッカーで有名な国だからです。サッカーで有名なスペインではスペイン語が用いられていますが、ポルトガル語とかなり似ていると言います。ブラジルやアルゼンチンでもポルトガル語が用いられています。ドイツ語については、ドイツでもサッカーが人気だということに加えて、本場のビールを飲んでみたいという理由があります。"},{"id":{"d":323,"n":5,"t":3},"speaker":"A","text":"自分の好きなことを実際に体験するために語学を勉強するというのは、モチベーションも保てそうですね。"},{"id":{"d":323,"n":6,"t":3},"speaker":"B","text":"その通りです。さらに、自分の好きなことに関連するものは、とても早く覚えられます。現に漫画で少しだけ出てきたポルトガル語は、発音は自信がありませんが、すぐに覚えてしまいました。"},{"id":{"d":323,"n":7,"t":3},"speaker":"A","text":"その調子で覚えられたら、そのうち日常会話をすることができるようになり、旅行にも行けそうですね。"},{"id":{"d":323,"n":8,"t":3},"speaker":"B","text":"そうですね。発音も勉強して、近いうちにスペインかドイツに行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":324,"n":1,"t":3},"speaker":"A","text":"日本人は沖縄や北海道がとても好きですね。"},{"id":{"d":324,"n":2,"t":3},"speaker":"B","text":"言われてみれば、そうかもしれません。飛行機に乗らないと簡単には行くことができないということや、普段生活している場所とは異なる気候や文化にひかれるのでしょうか？"},{"id":{"d":324,"n":3,"t":3},"speaker":"A","text":"それはありそうですね。本州では高頻度で北海道物産展や沖縄物産展も開催されていますよね。"},{"id":{"d":324,"n":4,"t":3},"speaker":"B","text":"それだけ好評だということでしょうね。やはり、沖縄と北海道の人気がうかがえますね。"},{"id":{"d":324,"n":5,"t":3},"speaker":"A","text":"私は北海道に住んでいたことがあるのですが、物産展には、北海道に住んでいた時にも見たことのない商品も並んでいて、多くの人が興味を持てるような内容になっていると感じました。そのようなところも好評の要因でしょう。"},{"id":{"d":324,"n":6,"t":3},"speaker":"B","text":"そうなのですね。一方で、定番商品も多く販売されているので、懐かしい商品を見つけてつい買ってしまうという人もいそうです。"},{"id":{"d":324,"n":7,"t":3},"speaker":"A","text":"そうですね。実際、私もよくそのような買い方をしてしまいます。"}];
INSERT INTO sentence [{"id":{"d":325,"n":1,"t":3},"speaker":"A","text":"自動車産業では、今後、電気自動車が中心になりそうですね。"},{"id":{"d":325,"n":2,"t":3},"speaker":"B","text":"そうかもしれませんね。環境汚染が問題視され、ガソリン車は以前よりも注目されなくなっていますからね。"},{"id":{"d":325,"n":3,"t":3},"speaker":"A","text":"私は車が大好きなので、この風潮は少し複雑な思いがします。"},{"id":{"d":325,"n":4,"t":3},"speaker":"B","text":"電気自動車でも運転することはできますよ。電気自動車は自動運転とは異なります。"},{"id":{"d":325,"n":5,"t":3},"speaker":"A","text":"運転ができることはもちろん理解していますが、私は運転したい車があってマニュアル車の免許を取得したので、将来的に自動車産業が電気自動車だけになってしまうと私の夢がかないません。"},{"id":{"d":325,"n":6,"t":3},"speaker":"B","text":"なるほど。以前テレビのニュースで見ましたが、外見は昔の人気車のままエンジンなどの内面を電気自動車のものに変えた車が人気のようですよ。"},{"id":{"d":325,"n":7,"t":3},"speaker":"A","text":"そのニュースは私も見ましたが、外見がマニュアル車のものは、常にサードギアに入れた状態でオートマ車として運転できると紹介されていました。私は、マニュアル車を操っている感覚が好きなので、そのような車にはあまりひかれません。マニュアルの電気自動車で、エンジン音も付いてくれるといいのですが。"},{"id":{"d":325,"n":8,"t":3},"speaker":"B","text":"なるほど。マニュアルの電気自動車が発売されたら、車好きの人の間でも人気になるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":326,"n":1,"t":3},"speaker":"A","text":"最近はグーグルマップを見て旅行に行った気分を味わう人がいるようです。"},{"id":{"d":326,"n":2,"t":3},"speaker":"B","text":"実際にその場所に行かずとも、旅行をした気分になれるのでしょうか？"},{"id":{"d":326,"n":3,"t":3},"speaker":"A","text":"私が見た動画では、実際にその土地の名産物を取り寄せて、マップで移動しながら飲食をして楽しんでいました。"},{"id":{"d":326,"n":4,"t":3},"speaker":"B","text":"何やら不思議な感じもしますが、家にいながら旅をした気分になれるのはいいことですね。"},{"id":{"d":326,"n":5,"t":3},"speaker":"A","text":"ただ、やはり旅行の醍醐味は、その土地ならではの空気感を楽しむことにもあると思います。"},{"id":{"d":326,"n":6,"t":3},"speaker":"B","text":"ええ。方言に限らず、その土地特有の言葉を聞きながら、地元の人と話すというのも楽しいですよね。"},{"id":{"d":326,"n":7,"t":3},"speaker":"A","text":"その通りです。このご時世にそのようなことができないのは理解できますが、グーグルマップを見ていると、より旅に行きたくなってしまいそうだと思いました。"},{"id":{"d":326,"n":8,"t":3},"speaker":"B","text":"何の迷いもなく旅に出られるようになったら、グーグルマップで見た風景を実際に見に行くというのも楽しそうですね。"}];
INSERT INTO sentence [{"id":{"d":327,"n":1,"t":3},"speaker":"A","text":"一昨年のGoToトラベルキャンペーンは利用しましたか？旅行が好きな人にとっては、とても安く旅行に行くことができて、その上クーポンまでもらえる魅力的なキャンペーンでしたね。"},{"id":{"d":327,"n":2,"t":3},"speaker":"B","text":"使っていません。予約をしようと思ったら、既に埋まっていて予約できませんでした。白石さんは利用できましたか？"},{"id":{"d":327,"n":3,"t":3},"speaker":"A","text":"私は1回だけ使うことができました。飛行機や新幹線は利用せずにホテルの予約だけでしたが、宿泊費が半額以下になったのでとても良かったです。"},{"id":{"d":327,"n":4,"t":3},"speaker":"B","text":"クーポンも使いましたか？"},{"id":{"d":327,"n":5,"t":3},"speaker":"A","text":"使うことができませんでした。クーポンは旅行の前日までに申し込みをしないと利用できないものだったのですが、それを知らなかったのです。"},{"id":{"d":327,"n":6,"t":3},"speaker":"B","text":"それはとてももったいなかったですね。"}];
INSERT INTO sentence [{"id":{"d":328,"n":1,"t":3},"speaker":"A","text":"料理をテイクアウトしたことはありますか？"},{"id":{"d":328,"n":2,"t":3},"speaker":"B","text":"最近はさまざまな店でテイクアウトができるようになっていますよね。私は1回だけしたことがあります。"},{"id":{"d":328,"n":3,"t":3},"speaker":"A","text":"何をテイクアウトしたのですか？"},{"id":{"d":328,"n":4,"t":3},"speaker":"B","text":"晩ご飯をよく食べに行っていた店で、揚げ物をテイクアウトしました。"},{"id":{"d":328,"n":5,"t":3},"speaker":"A","text":"よく行く店のテイクアウトを利用したくなるという気持ちはとても分かります。このご時世は、応援の意味でも良いことですね。"},{"id":{"d":328,"n":6,"t":3},"speaker":"B","text":"はい。今度は、有名なファミリーレストランの料理をテイクアウトして、家でゆっくり食べてみたいです。"},{"id":{"d":328,"n":7,"t":3},"speaker":"A","text":"いいですね。家でもお店の味を食べることができるのは、魅力的ですね。"},{"id":{"d":328,"n":8,"t":3},"speaker":"B","text":"このご時世で飲食店は大変かと思いますが、応援の意味でもテイクアウトを利用し続けていきたいです。"}];
INSERT INTO sentence [{"id":{"d":329,"n":1,"t":3},"speaker":"A","text":"富士山に登ったことはありますか？"},{"id":{"d":329,"n":2,"t":3},"speaker":"B","text":"ありません。筑波山にしか登ったことがありません。"},{"id":{"d":329,"n":3,"t":3},"speaker":"A","text":"筑波山にはいつ登ったのですか？"},{"id":{"d":329,"n":4,"t":3},"speaker":"B","text":"小学生の校外学習で行きました。途中で少し飽きてしまいましたが、しっかり登り切ることができました。頂上から見た景色は今でも覚えています。"},{"id":{"d":329,"n":5,"t":3},"speaker":"A","text":"そうでしたか。筑波山の頂上からの景色が素晴らしかったのなら、ぜひ富士山にも登ってみてほしいです。感動すると思いますよ。"},{"id":{"d":329,"n":6,"t":3},"speaker":"B","text":"高速道路を運転している時に、富士山を見てきれいだと思うことはよくありますが、やはり頂上からの景色は別格ですか？"},{"id":{"d":329,"n":7,"t":3},"speaker":"A","text":"はい。頂上からの眺めは素晴らしいです。裾野の広さにも圧倒されると思うので、まずは近くに行ってみることをお勧めします。"},{"id":{"d":329,"n":8,"t":3},"speaker":"B","text":"それではまずは裾野に行ってみます。人生で一度は富士山に登ってみたいものですね。"}];
INSERT INTO sentence [{"id":{"d":330,"n":1,"t":3},"speaker":"A","text":"すみません、この近くで魚釣りができる所はありませんか？"},{"id":{"d":330,"n":2,"t":3},"speaker":"B","text":"もう少し海の近くに行った所で磯釣りができますよ。"},{"id":{"d":330,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。磯釣りですか、良いですね。"},{"id":{"d":330,"n":4,"t":3},"speaker":"B","text":"今日は波もちょうど良い具合なので、たくさん釣れると思いますよ。"},{"id":{"d":330,"n":5,"t":3},"speaker":"A","text":"そうですか。おいしい魚をたくさん釣って、家族へのお土産にしたいです。"},{"id":{"d":330,"n":6,"t":3},"speaker":"B","text":"それなら、もしよろしければ私の船で船釣りをしに行きませんか？"},{"id":{"d":330,"n":7,"t":3},"speaker":"A","text":"よろしいのですか？餌などはあまり準備していませんが大丈夫でしょうか？"},{"id":{"d":330,"n":8,"t":3},"speaker":"B","text":"餌ならたくさん持っていますので心配しないでください。では行きましょう。"}];
INSERT INTO sentence [{"id":{"d":331,"n":1,"t":3},"speaker":"A","text":"今月は観光客が多く来ると思われるので、料理の材料を多めに発注しておきましょう。"},{"id":{"d":331,"n":2,"t":3},"speaker":"B","text":"通常の発注と比較してどれくらい増やせばよいですか？"},{"id":{"d":331,"n":3,"t":3},"speaker":"A","text":"冷凍商品は倍量、冷蔵や常温商品は1.5倍量の発注をかけてください。"},{"id":{"d":331,"n":4,"t":3},"speaker":"B","text":"承知しました。初めのうちは、店長にも確認してもらってから発注してもよいでしょうか？"},{"id":{"d":331,"n":5,"t":3},"speaker":"A","text":"分かりました。慣れるまでは一緒に確認してから発注するようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":332,"n":1,"t":3},"speaker":"A","text":"運転を始めてからそろそろ2時間になりますね。休憩所に入りませんか？"},{"id":{"d":332,"n":2,"t":3},"speaker":"B","text":"もうそれほど運転しているのですね。それでは、次に休憩できそうな場所があったら入りましょう。"},{"id":{"d":332,"n":3,"t":3},"speaker":"A","text":"先ほどの看板には、あと1キロメートル先に休憩所があると書いてありました。"},{"id":{"d":332,"n":4,"t":3},"speaker":"B","text":"では、そこでいったん休憩しますね。休憩所に何かおいしいお店などがあったら、行きたいですね。"},{"id":{"d":332,"n":5,"t":3},"speaker":"A","text":"残念ながら、この先の休憩所にはご飯を食べられる場所はなさそうです。しかし、とてもきれいな滝を見ることができるようですよ。"},{"id":{"d":332,"n":6,"t":3},"speaker":"B","text":"そうですか。でも、きれいな風景を見ることができるのはいいですね。運転の疲れも軽減されそうです。"}];
INSERT INTO sentence [{"id":{"d":333,"n":1,"t":3},"speaker":"A","text":"お母さん、来月の第1週に北海道へ旅行に行ってきます。"},{"id":{"d":333,"n":2,"t":3},"speaker":"B","text":"くれぐれも体調には気を付けて行ってきてください。手洗い、うがいをしっかりして楽しんできてくださいね。"},{"id":{"d":333,"n":3,"t":3},"speaker":"A","text":"分かりました。金曜から2泊3日で行く予定です。"},{"id":{"d":333,"n":4,"t":3},"speaker":"B","text":"週末に行くのですか？平日に行く方が安いのではありませんか？"},{"id":{"d":333,"n":5,"t":3},"speaker":"A","text":"それが、調べてみたところ、移動手段もホテルもほとんど値段が変わりませんでした。"},{"id":{"d":333,"n":6,"t":3},"speaker":"B","text":"それなら、あちらであなたが会う相手は土日休みの方が多いでしょうから、土日を含めて行くというのは良い計画ですね。"},{"id":{"d":333,"n":7,"t":3},"speaker":"A","text":"はい。ところで、お土産のリクエストはありますか？"},{"id":{"d":333,"n":8,"t":3},"speaker":"B","text":"何でも構いませんよ。もし大変だったら私たちへのお土産はなくても大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":334,"n":1,"t":3},"speaker":"A","text":"来週のあなたの誕生日、一緒に沖縄旅行をしませんか？"},{"id":{"d":334,"n":2,"t":3},"speaker":"B","text":"私の誕生日に旅行へ行くのですか？随分と急な話ですが、飛行機やホテルはどうするのですか？"},{"id":{"d":334,"n":3,"t":3},"speaker":"A","text":"安心してください。もう予約してあります。"},{"id":{"d":334,"n":4,"t":3},"speaker":"B","text":"いつの間に予約したのですか？そのようなそぶりは全くなかったのに。"},{"id":{"d":334,"n":5,"t":3},"speaker":"A","text":"せっかくの誕生日なので思い出に残るものにしたいと思い、実はサプライズを計画していました。"},{"id":{"d":334,"n":6,"t":3},"speaker":"B","text":"あまりのサプライズに少し混乱していますが、もちろん行きたいです。"},{"id":{"d":334,"n":7,"t":3},"speaker":"A","text":"よかったです。オーシャンビューのホテルを予約しているので好きなだけ海に入ることができますよ。たくさん遊びましょう。"},{"id":{"d":334,"n":8,"t":3},"speaker":"B","text":"何から何までサプライズばかりでびっくりです。ありがとうございます。とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":335,"n":1,"t":3},"speaker":"A","text":"お客さま、本日はご利用いただきありがとうございます。少々よろしいでしょうか？"},{"id":{"d":335,"n":2,"t":3},"speaker":"B","text":"お世話になります。何かありましたか？"},{"id":{"d":335,"n":3,"t":3},"speaker":"A","text":"現在のお客さまの席は非常口座席ではない席なのですが、非常口座席に変更させていただいてもよろしいですか？"},{"id":{"d":335,"n":4,"t":3},"speaker":"B","text":"非常口座席を利用するにあたって、何か注意することはありますか？"},{"id":{"d":335,"n":5,"t":3},"speaker":"A","text":"非常口座席では、座席の下に荷物を置くことはできません。また、緊急着陸が必要な場合は、避難誘導の手助けをお願いすることがあります。"},{"id":{"d":335,"n":6,"t":3},"speaker":"B","text":"分かりました。問題ありませんので、変更していただいて結構です。"},{"id":{"d":335,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。お礼にもなりませんが、もしよろしければこちらから1つお菓子をお選びください。"},{"id":{"d":335,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。ではこのマドレーヌをいただきます。"}];
INSERT INTO sentence [{"id":{"d":336,"n":1,"t":3},"speaker":"A","text":"お父さん、今日は天気が良いので一緒にサイクリングに行きませんか？"},{"id":{"d":336,"n":2,"t":3},"speaker":"B","text":"いいですね、行きましょう。どこまで行きたいですか？"},{"id":{"d":336,"n":3,"t":3},"speaker":"A","text":"今日は丸1日時間があるので遠い所まで行きたいです。"},{"id":{"d":336,"n":4,"t":3},"speaker":"B","text":"それでは県をまたいで、おいしいご飯でも食べに行きましょうか？"},{"id":{"d":336,"n":5,"t":3},"speaker":"A","text":"他県まで行くのなら、忘れずにお土産も買ってきましょう。ところで、どの自転車で行きますか？"},{"id":{"d":336,"n":6,"t":3},"speaker":"B","text":"そういえば、先週サイクリングに行ったときにお気に入りの自転車の前輪がパンクしてしまったのでした。すぐに修理するので少し待ってください。"},{"id":{"d":336,"n":7,"t":3},"speaker":"A","text":"分かりました。どれくらいかかりそうですか？"},{"id":{"d":336,"n":8,"t":3},"speaker":"B","text":"修理してから準備をするので1時間後には出発できると思います。できるだけ急ぎますね。"}];
INSERT INTO sentence [{"id":{"d":337,"n":1,"t":3},"speaker":"A","text":"来週夜景を見に行きませんか？"},{"id":{"d":337,"n":2,"t":3},"speaker":"B","text":"ぜひ行きたいです。どこに行きますか？"},{"id":{"d":337,"n":3,"t":3},"speaker":"A","text":"来週は天気が良さそうなので、函館山に行くのはどうでしょうか？"},{"id":{"d":337,"n":4,"t":3},"speaker":"B","text":"賛成です。函館山からの夜景は100万ドルの夜景と称されますから、きれいに見られるといいですね。"},{"id":{"d":337,"n":5,"t":3},"speaker":"A","text":"そうですね。予報通り天気が良いことを願いましょう。"},{"id":{"d":337,"n":6,"t":3},"speaker":"B","text":"函館山まで車で行きますか？それとも歩いて行きますか？私は歩いて行ったこともあるので、どちらでも大丈夫です。"},{"id":{"d":337,"n":7,"t":3},"speaker":"A","text":"車で行くのもいいですが、函館市内を運転するのは少し不安なので、歩いて行きましょう。"},{"id":{"d":337,"n":8,"t":3},"speaker":"B","text":"そうしましょうか。そちらの方が道中で食べ歩きもできていいですね。"}];
INSERT INTO sentence [{"id":{"d":338,"n":1,"t":3},"speaker":"A","text":"電車に乗るとき、スマートフォンをかざすことで改札を通過できるというのは本当ですか？"},{"id":{"d":338,"n":2,"t":3},"speaker":"B","text":"本当です。体験してみますか？"},{"id":{"d":338,"n":3,"t":3},"speaker":"A","text":"少し怖いので、一度実際に通過しているところを見てから体験してみたいです。"},{"id":{"d":338,"n":4,"t":3},"speaker":"B","text":"分かりました。それでは私が最初に行ってみますね。"},{"id":{"d":338,"n":5,"t":3},"speaker":"A","text":"お願いします。"},{"id":{"d":338,"n":6,"t":3},"speaker":"B","text":"ほら、通過することができましたよ。"},{"id":{"d":338,"n":7,"t":3},"speaker":"A","text":"本当に通過できるのですね。ただスマートフォンをかざすだけでよいのですか？"},{"id":{"d":338,"n":8,"t":3},"speaker":"B","text":"乗車用のICカードを事前にスマートフォンに登録する必要があります。どのカードでもよいので登録してみてください。"},{"id":{"d":338,"n":9,"t":3},"speaker":"A","text":"このカードにします。登録ができたので、早速スマートフォンを使って改札に入ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":339,"n":1,"t":3},"speaker":"A","text":"何かお探しですか？"},{"id":{"d":339,"n":2,"t":3},"speaker":"B","text":"旅行バッグを探しています。お薦めはありますか？"},{"id":{"d":339,"n":3,"t":3},"speaker":"A","text":"旅行バッグでしたらこちらのキャリーバッグが特にお薦めです。とても多くの荷物を収容できますよ。"},{"id":{"d":339,"n":4,"t":3},"speaker":"B","text":"このキャリーバッグは、飛行機に乗るときに機内への持ち込みが可能な大きさですか？"},{"id":{"d":339,"n":5,"t":3},"speaker":"A","text":"はい、可能です。しかし、目いっぱい詰めてしまうと重量制限を超過してしまう可能性があります。"},{"id":{"d":339,"n":6,"t":3},"speaker":"B","text":"それではこちらのキャリーバッグは、重量の面で機内持ち込みが不可な場合があるということですね。"},{"id":{"d":339,"n":7,"t":3},"speaker":"A","text":"さようでございます。LCCでの移動では、お金が少し余分にかかってしまう可能性がございます。"},{"id":{"d":339,"n":8,"t":3},"speaker":"B","text":"なるほど。それでもやはり荷物を多く収容できるというのは魅力的ですので、こちらを購入しようと思います。"},{"id":{"d":339,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。お会計はこちらでお願いします。"}];
INSERT INTO sentence [{"id":{"d":340,"n":1,"t":3},"speaker":"A","text":"来月北海道に行くので、ホテルを探しています。"},{"id":{"d":340,"n":2,"t":3},"speaker":"B","text":"良さそうなホテルはありましたか？"},{"id":{"d":340,"n":3,"t":3},"speaker":"A","text":"はい。もちろん高いホテルもたくさんありますが、安いホテルがとても充実していることに驚きました。シェアハウスのようなホテルもあるようです。"},{"id":{"d":340,"n":4,"t":3},"speaker":"B","text":"シェアハウスのようなホテルとはどのようなものですか？明確に部屋ごとの空間が分けられていないということですか？"},{"id":{"d":340,"n":5,"t":3},"speaker":"A","text":"その通りです。寝る場所はほとんどカプセルホテルのようになっていて、他の人との仕切りも薄いカーテンのようなものだけのように見えました。"},{"id":{"d":340,"n":6,"t":3},"speaker":"B","text":"それは驚きですね。面白いですが、気は十分に休まるのでしょうか？"},{"id":{"d":340,"n":7,"t":3},"speaker":"A","text":"私もそこを不安に感じましたが、どうやら普通の一人部屋のような形式のシェアハウスもあるようです。そのような形式なら他人との距離も確保できますし、気も十分に休まりそうです。"}];
INSERT INTO sentence [{"id":{"d":341,"n":1,"t":3},"speaker":"A","text":"感染症のまん延に伴い、今までほどはエレベーターを利用しなくなりました。"},{"id":{"d":341,"n":2,"t":3},"speaker":"B","text":"乗ることができる人数も制限されるようになったので、利用する人は減っているような気がします。"},{"id":{"d":341,"n":3,"t":3},"speaker":"A","text":"感染症に気を使っている若い健康な人だと、密室のエレベーターにはあまり乗らないという人が多いようですね。"},{"id":{"d":341,"n":4,"t":3},"speaker":"B","text":"もちろんマンションなどのように利用者が限られていて他の人と一緒に乗る機会が少ない場合には乗るでしょうけれど、他の場合はあまり乗らないようですね。"},{"id":{"d":341,"n":5,"t":3},"speaker":"A","text":"そうですね。加えて健康を気にする人も増えていますので、そのような理由で階段を利用する人も増えているようです。"},{"id":{"d":341,"n":6,"t":3},"speaker":"B","text":"健康にも気を使っているとは素晴らしいことですね。"}];
INSERT INTO sentence [{"id":{"d":342,"n":1,"t":3},"speaker":"A","text":"北海道の車は寒冷地仕様になっているということを知っていますか？"},{"id":{"d":342,"n":2,"t":3},"speaker":"B","text":"知りませんでした。そもそも寒冷地仕様とはどのようなものを指すのですか？"},{"id":{"d":342,"n":3,"t":3},"speaker":"A","text":"寒冷地仕様車と普通仕様車の違いは、主にエンジン関係、電気関係、凍結や雪対策にあるそうです。"},{"id":{"d":342,"n":4,"t":3},"speaker":"B","text":"具体的にはそれらがどのように異なるのですか？"},{"id":{"d":342,"n":5,"t":3},"speaker":"A","text":"例えば、普通車とはエンジンオイルや冷却水などの油関係の濃度が異なります。北海道などの寒い地域では朝に車が凍ってしまって動かなくなることが多々あるので、それを防ぐためなのです。"},{"id":{"d":342,"n":6,"t":3},"speaker":"B","text":"そのような違いがあるのですね。それでは北海道の車は高いということでしょうか？"},{"id":{"d":342,"n":7,"t":3},"speaker":"A","text":"私の車はそうでもありませんでしたよ。北海道は全体的に物価が安いという特徴があるからなのか、車の価格は本州とほとんど変わりませんでした。"},{"id":{"d":342,"n":8,"t":3},"speaker":"B","text":"なるほど。寒冷地仕様車が本州の車と同じ値段ならば、北海道のような寒冷地では寒冷地仕様車を買った方が良さそうな気がしますね。"},{"id":{"d":342,"n":9,"t":3},"speaker":"A","text":"ただ、私の車は中古車だったので、新車の場合はどうなるか分かりません。"}];
INSERT INTO sentence [{"id":{"d":343,"n":1,"t":3},"speaker":"A","text":"バスの定期券を購入したいのですが、窓口はどこですか？"},{"id":{"d":343,"n":2,"t":3},"speaker":"B","text":"こちらの窓口でも承りますよ。"},{"id":{"d":343,"n":3,"t":3},"speaker":"A","text":"それではお願いします。今まで使っていた定期の区間はそのままで、期限の延長をお願いします。"},{"id":{"d":343,"n":4,"t":3},"speaker":"B","text":"期間はどれくらいにいたしますか？"},{"id":{"d":343,"n":5,"t":3},"speaker":"A","text":"半年でお願いします。"},{"id":{"d":343,"n":6,"t":3},"speaker":"B","text":"かしこまりました。それでは定期券半年分のお値段が3万円です。"},{"id":{"d":343,"n":7,"t":3},"speaker":"A","text":"クレジットカードでお願いします。"},{"id":{"d":343,"n":8,"t":3},"speaker":"B","text":"お預かりいたします。"}];
INSERT INTO sentence [{"id":{"d":344,"n":1,"t":3},"speaker":"A","text":"お父さん、船に乗ったことはありますか？"},{"id":{"d":344,"n":2,"t":3},"speaker":"B","text":"もちろんあります。若い時に何度も乗っていました。"},{"id":{"d":344,"n":3,"t":3},"speaker":"A","text":"うらやましいです。私も船に乗って魚を釣ってみたいです。"},{"id":{"d":344,"n":4,"t":3},"speaker":"B","text":"釣りに興味があるのなら、今度一緒に釣りに行きますか？"},{"id":{"d":344,"n":5,"t":3},"speaker":"A","text":"船にも乗れるのですか？"},{"id":{"d":344,"n":6,"t":3},"speaker":"B","text":"急なので船に乗ることはできませんが、釣りなら車で30分くらいの所でできますよ。"},{"id":{"d":344,"n":7,"t":3},"speaker":"A","text":"釣り自体にも興味はありますが、私は船に乗ってみたいです。"},{"id":{"d":344,"n":8,"t":3},"speaker":"B","text":"分かりました。では、知り合いの漁師に相談してみましょう。"},{"id":{"d":344,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":345,"n":1,"t":3},"speaker":"A","text":"あるテレビ番組で紹介されていたのですが、ノルウェーでは移動手段としてスキーを用いているそうです。"},{"id":{"d":345,"n":2,"t":3},"speaker":"B","text":"昔から日常的にスキーを用いているということですね。オリンピックで金メダルを多く獲得していることに合点がいきました。"},{"id":{"d":345,"n":3,"t":3},"speaker":"A","text":"ちなみにオランダでは、スケートを主に移動手段として用いているそうですよ。"},{"id":{"d":345,"n":4,"t":3},"speaker":"B","text":"スケート種目でオランダが強いことにも関係がありそうですね。"},{"id":{"d":345,"n":5,"t":3},"speaker":"A","text":"他にも、ドイツではソリは移動手段だそうです。"},{"id":{"d":345,"n":6,"t":3},"speaker":"B","text":"それは意外ですね。ソリが移動手段というのは、雪国の小さい子ども限定の話かと思っていました。"},{"id":{"d":345,"n":7,"t":3},"speaker":"A","text":"ソリは乗り物としての性能も大切なようで、あのBMWが協力しているそうですよ。"},{"id":{"d":345,"n":8,"t":3},"speaker":"B","text":"素晴らしいですね。日本では何が人気だと紹介されていましたか？"},{"id":{"d":345,"n":9,"t":3},"speaker":"A","text":"スノーボードが人気だと紹介されていました。"},{"id":{"d":345,"n":10,"t":3},"speaker":"B","text":"確かに、最近はスノーボードをしている若者が多いですね。これからはスノーボードで活躍する日本人選手が増えてくるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":346,"n":1,"t":3},"speaker":"A","text":"先ほど運転していたら、危うく事故になりそうな場面に遭遇しました。"},{"id":{"d":346,"n":2,"t":3},"speaker":"B","text":"危なかったですね。どのような運転をされたのですか？"},{"id":{"d":346,"n":3,"t":3},"speaker":"A","text":"少し前を走っていた車が、急にウインカーを出して近づいてきたのです。"},{"id":{"d":346,"n":4,"t":3},"speaker":"B","text":"車1台が入れるような間隔はなかったのですか？"},{"id":{"d":346,"n":5,"t":3},"speaker":"A","text":"ありませんでした。私の車の前輪は、その車の後輪くらいの場所にありました。"},{"id":{"d":346,"n":6,"t":3},"speaker":"B","text":"それは危ないですね。結局どうしたのですか？"},{"id":{"d":346,"n":7,"t":3},"speaker":"A","text":"幸いにも後続車がいなかったので私がブレーキをかけましたが、後続車がいたら事故になっていました。"},{"id":{"d":346,"n":8,"t":3},"speaker":"B","text":"そのような危険運転は本当に迷惑ですね。大事にならなくてよかったです。"}];
INSERT INTO sentence [{"id":{"d":347,"n":1,"t":3},"speaker":"A","text":"バイクは好きですか？"},{"id":{"d":347,"n":2,"t":3},"speaker":"B","text":"好きでも嫌いでもありません。しかし、夜中に近所を走るバイクの騒音が迷惑と感じているので、どちらかというと嫌いな方だと思います。"},{"id":{"d":347,"n":3,"t":3},"speaker":"A","text":"そうですか。私はバイクが好きなので、一部の人によってバイクの印象が悪くなってしまっているのは残念です。"},{"id":{"d":347,"n":4,"t":3},"speaker":"B","text":"あのような騒音さえなければバイクは良い乗り物だと思いますよ。あの騒音はわざとやっているのですか？"},{"id":{"d":347,"n":5,"t":3},"speaker":"A","text":"わざとの場合が多いと思います。時と場所は考えなければいけませんね。"}];
INSERT INTO sentence [{"id":{"d":348,"n":1,"t":3},"speaker":"A","text":"あるレンタカーの会社では、会員になると前日の夕方から車を借りられるということを知っていますか？"},{"id":{"d":348,"n":2,"t":3},"speaker":"B","text":"初めて聞きました。"},{"id":{"d":348,"n":3,"t":3},"speaker":"A","text":"その会社の会員になると、レンタカーの予約もしやすくなり、先ほども言いましたように前日の17時から同じ値段で借りることができるのです。"},{"id":{"d":348,"n":4,"t":3},"speaker":"B","text":"それにはどのような利点があるのですか？"},{"id":{"d":348,"n":5,"t":3},"speaker":"A","text":"同じ値段で早くから車を利用できるので、例えば、旅行前日の夜に近くの夜景がきれいな場所に行くことができますし、旅行当日には朝早くから出発することもできます。"},{"id":{"d":348,"n":6,"t":3},"speaker":"B","text":"では、早起きさえできれば、道がすいている早朝から移動を開始することもできるということですね。"},{"id":{"d":348,"n":7,"t":3},"speaker":"A","text":"その通りです。前日から移動を開始して車中泊をすることもできます。"}];
INSERT INTO sentence [{"id":{"d":349,"n":1,"t":3},"speaker":"A","text":"航空会社の飛行機チケットのガチャガチャというものを知っていますか？"},{"id":{"d":349,"n":2,"t":3},"speaker":"B","text":"初めて聞きました。よく分かりませんが、行き先は決められないのですか？"},{"id":{"d":349,"n":3,"t":3},"speaker":"A","text":"ガチャガチャですから、中身、つまり行き先を自分で選ぶことはできません。"},{"id":{"d":349,"n":4,"t":3},"speaker":"B","text":"それでは、旅行先は運任せということですか？"},{"id":{"d":349,"n":5,"t":3},"speaker":"A","text":"そうです。ちなみに、5,000円ほどで往復の航空券が手に入るようです。"},{"id":{"d":349,"n":6,"t":3},"speaker":"B","text":"それはとても楽しそうですね。いろいろな場所に安く行けるということなら、旅行好きの人はたくさん利用しそうです。"},{"id":{"d":349,"n":7,"t":3},"speaker":"A","text":"皆さん同じ考えのようで、かなり並ばないとそのガチャガチャを利用できないそうです。"},{"id":{"d":349,"n":8,"t":3},"speaker":"B","text":"並ばなければいけないとしても、興味をひかれる話ですね。"}];
INSERT INTO sentence [{"id":{"d":350,"n":1,"t":3},"speaker":"A","text":"観光バスに乗ったことはありますか？"},{"id":{"d":350,"n":2,"t":3},"speaker":"B","text":"ありません。ツアーなどに参加したこともありません。"},{"id":{"d":350,"n":3,"t":3},"speaker":"A","text":"私も観光バスに乗ったことがありませんが、とても興味があります。"},{"id":{"d":350,"n":4,"t":3},"speaker":"B","text":"私も興味があります。プロのガイドに案内してもらえるので、さまざまな観光スポットを隅々まで楽しめそうです。"},{"id":{"d":350,"n":5,"t":3},"speaker":"A","text":"あまり知られていない裏話なども教えてもらえそうですね。"},{"id":{"d":350,"n":6,"t":3},"speaker":"B","text":"はい。とても面白そうなのですが、一方で、自分のペースで観光したいという気持ちもあります。"},{"id":{"d":350,"n":7,"t":3},"speaker":"A","text":"ツアーでは予定や他の人に合わせる必要がありますから、興味があっても早く移動しなければいけない、反対に興味がなくても長居しないといけないという可能性がありますね。"},{"id":{"d":350,"n":8,"t":3},"speaker":"B","text":"その懸念がどうしてもあり、ツアーには参加したことがありません。"},{"id":{"d":350,"n":9,"t":3},"speaker":"A","text":"自分の興味のあることだけに集中して見る旅行の楽しいですが、たまには、ツアーに参加して視野を広げるのも魅力的かもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":351,"n":1,"t":3},"speaker":"A","text":"ここから練習場までは自転車で45分かかります。"},{"id":{"d":351,"n":2,"t":3},"speaker":"B","text":"自転車でないと行けませんか？バスや電車はありませんか？"},{"id":{"d":351,"n":3,"t":3},"speaker":"A","text":"バスと電車もありますが、1人暮らしの人は基本的に自転車で行っているようですよ。"},{"id":{"d":351,"n":4,"t":3},"speaker":"B","text":"私は長崎出身のため、自転車に乗ったことがほとんどありません。"},{"id":{"d":351,"n":5,"t":3},"speaker":"A","text":"なぜ、長崎出身が関係しているのですか？"},{"id":{"d":351,"n":6,"t":3},"speaker":"B","text":"長崎は坂が多かったので、自転車にほとんど乗りませんでした。その影響であまり自転車に乗れないのです。"},{"id":{"d":351,"n":7,"t":3},"speaker":"A","text":"なるほど。それでは練習開始の1時間半前にここに集合して、ゆっくり自転車で向かいましょうか？"},{"id":{"d":351,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。とてもゆっくりになってしまうかと思いますが、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":352,"n":1,"t":3},"speaker":"A","text":"カヌーを体験したことがありますか？"},{"id":{"d":352,"n":2,"t":3},"speaker":"B","text":"はい。小さい頃に近くの沼でよく乗っていました。"},{"id":{"d":352,"n":3,"t":3},"speaker":"A","text":"すてきな経験をしていますね。乗ってみてどうでしたか？"},{"id":{"d":352,"n":4,"t":3},"speaker":"B","text":"とても楽しくて気持ちよかった記憶があります。"},{"id":{"d":352,"n":5,"t":3},"speaker":"A","text":"それはいいですね。他にも、何か思い出に残っていることはありますか？"},{"id":{"d":352,"n":6,"t":3},"speaker":"B","text":"実はその沼にはカミツキガメが多く生息していたらしく、カヌーの底からコツコツと音がしていたことを覚えています。"},{"id":{"d":352,"n":7,"t":3},"speaker":"A","text":"カミツキガメは危険なのですよね。大丈夫でしたか？"},{"id":{"d":352,"n":8,"t":3},"speaker":"B","text":"大丈夫でした。上からコンコンとたたき返すと、逃げていきました。"}];
INSERT INTO sentence [{"id":{"d":353,"n":1,"t":3},"speaker":"A","text":"フェリーに乗ったことはありますか？"},{"id":{"d":353,"n":2,"t":3},"speaker":"B","text":"ありません。小さい頃から乗ってみたいと思っているのですが、いまだに乗ったことはありません。"},{"id":{"d":353,"n":3,"t":3},"speaker":"A","text":"そうでしたか。私もつい数年前まで乗ったことがなかったのですが、ついにこの間乗りました。"},{"id":{"d":353,"n":4,"t":3},"speaker":"B","text":"なぜ乗ることにしたのですか？"},{"id":{"d":353,"n":5,"t":3},"speaker":"A","text":"実家にバイクを一緒に持っていくために乗ったのです。"},{"id":{"d":353,"n":6,"t":3},"speaker":"B","text":"確かに、バイクや自動車を持っていくためには、フェリーに乗るのが楽ですね。"},{"id":{"d":353,"n":7,"t":3},"speaker":"A","text":"はい。おかげで貴重な経験ができました。しかし、思ったよりも旅費が高くなってしまいました。"},{"id":{"d":353,"n":8,"t":3},"speaker":"B","text":"学生にとっては旅費が高いと大変ですね。"}];
INSERT INTO sentence [{"id":{"d":354,"n":1,"t":3},"speaker":"A","text":"小さい頃は潮干狩りによく行っていました。"},{"id":{"d":354,"n":2,"t":3},"speaker":"B","text":"楽しそうですね。干潟が近くにあったのですか？"},{"id":{"d":354,"n":3,"t":3},"speaker":"A","text":"近くにはありませんでしたが、村の行事でバスを貸し切って行ったことがあります。"},{"id":{"d":354,"n":4,"t":3},"speaker":"B","text":"行事として行ったのですね。たくさん取れましたか？"},{"id":{"d":354,"n":5,"t":3},"speaker":"A","text":"たくさん取ることができた記憶があります。しかし、取ったアサリをどのようにして食べたのかあまり記憶がありません。"},{"id":{"d":354,"n":6,"t":3},"speaker":"B","text":"おみそ汁にして食べたのかもしれませんね。筋肉痛にはなりませんでしたか？"},{"id":{"d":354,"n":7,"t":3},"speaker":"A","text":"小さい頃だったので私はなりませんでしたが、母は筋肉痛になっていました。"}];
INSERT INTO sentence [{"id":{"d":355,"n":1,"t":3},"speaker":"A","text":"今週末イチゴ狩りに行きませんか？"},{"id":{"d":355,"n":2,"t":3},"speaker":"B","text":"いいですね。ちょうど今はイチゴが旬の季節なので、おいしいイチゴがたくさんあるはずです。"},{"id":{"d":355,"n":3,"t":3},"speaker":"A","text":"私もそのように思ったのでホームページを調べてみたのです。いつも行っているイチゴ農園では、先週末からイチゴ狩りを開催しているようでした。"},{"id":{"d":355,"n":4,"t":3},"speaker":"B","text":"そうですか。予約は必要でしょうか？"},{"id":{"d":355,"n":5,"t":3},"speaker":"A","text":"予約制ではなく先着順で、人数制限があるそうです。"},{"id":{"d":355,"n":6,"t":3},"speaker":"B","text":"農園の近くは団地ですから、当日は先に近所の人がたくさん行っているかもしれません。"},{"id":{"d":355,"n":7,"t":3},"speaker":"A","text":"そういえば、前回行った時もそうでしたね。"},{"id":{"d":355,"n":8,"t":3},"speaker":"B","text":"その反省を生かして、今回はかなり早く行くようにしましょう。"},{"id":{"d":355,"n":9,"t":3},"speaker":"A","text":"賛成です。とびきり早起きして農園へ向かいましょう。"}];
INSERT INTO sentence [{"id":{"d":356,"n":1,"t":3},"speaker":"A","text":"札幌まで新幹線が延びると言われてだいぶ時間がたちましたが、もう新幹線は通りましたか？"},{"id":{"d":356,"n":2,"t":3},"speaker":"B","text":"まだです。現在は北海道の函館までしか新幹線は通っていません。"},{"id":{"d":356,"n":3,"t":3},"speaker":"A","text":"時間がかかるのですね。完成はいつの予定でしたか？"},{"id":{"d":356,"n":4,"t":3},"speaker":"B","text":"元々は2035年の開通を予定していましたが、現在では2030年に開通予定といわれています。"},{"id":{"d":356,"n":5,"t":3},"speaker":"A","text":"5年も早くなりましたか。開通したら札幌まで新幹線で行こうと思いますか？"},{"id":{"d":356,"n":6,"t":3},"speaker":"B","text":"新幹線は時間もお金もかかるので、私はほとんど利用しないと思います。"},{"id":{"d":356,"n":7,"t":3},"speaker":"A","text":"今は格安航空会社も多いので、北海道への移動には飛行機が人気のようですね。"}];
INSERT INTO sentence [{"id":{"d":357,"n":1,"t":3},"speaker":"A","text":"ヘリコプターに興味はありますか？"},{"id":{"d":357,"n":2,"t":3},"speaker":"B","text":"あります。少しの怖さはありますが、人生で一度は乗ってみたいと思っています。"},{"id":{"d":357,"n":3,"t":3},"speaker":"A","text":"私も同じです。ところでヘリコプターのタクシーがあることをご存じですか？"},{"id":{"d":357,"n":4,"t":3},"speaker":"B","text":"知りませんでした。面白いですね。"},{"id":{"d":357,"n":5,"t":3},"speaker":"A","text":"私も初めて知ったときには驚いたものです。"},{"id":{"d":357,"n":6,"t":3},"speaker":"B","text":"利用料金はどのくらいなのでしょうか？"},{"id":{"d":357,"n":7,"t":3},"speaker":"A","text":"安いものでも、1時間で10万円前後だそうです。"},{"id":{"d":357,"n":8,"t":3},"speaker":"B","text":"やはりとても高いですね。1時間でどこまで移動できるのでしょうか？"},{"id":{"d":357,"n":9,"t":3},"speaker":"A","text":"東京から軽井沢や那須塩原まで行けるようですよ。"},{"id":{"d":357,"n":10,"t":3},"speaker":"B","text":"車か飛行機の方が何倍も安いですが、それはそれで面白そうですね。"}];
INSERT INTO sentence [{"id":{"d":358,"n":1,"t":3},"speaker":"A","text":"明日は、とても久しぶりに外出をします。"},{"id":{"d":358,"n":2,"t":3},"speaker":"B","text":"いいですね。どこに行くのですか？"},{"id":{"d":358,"n":3,"t":3},"speaker":"A","text":"品川まで行こうと考えています。"},{"id":{"d":358,"n":4,"t":3},"speaker":"B","text":"何をしに行くのですか？遊びに行くのですか？"},{"id":{"d":358,"n":5,"t":3},"speaker":"A","text":"いえ、就職先の見学に行ってきます。どのような場所にあるのか、どのような雰囲気なのかを見てきたいと思っています。"},{"id":{"d":358,"n":6,"t":3},"speaker":"B","text":"実際に働き始めるまであと1カ月でしたね。モチベーションを上げるためにも良いことだと思います。"},{"id":{"d":358,"n":7,"t":3},"speaker":"A","text":"はい。もちろん、今もモチベーションが低いというわけではありませんが、現地に足を運ぶことで、改めて気持ちを高められたらと思っています。"},{"id":{"d":358,"n":8,"t":3},"speaker":"B","text":"くれぐれも気を付けて行ってきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":359,"n":1,"t":3},"speaker":"A","text":"職場まで電車で何分かかりますか？"},{"id":{"d":359,"n":2,"t":3},"speaker":"B","text":"20分程度です。どうしてですか？"},{"id":{"d":359,"n":3,"t":3},"speaker":"A","text":"私の家は職場まで遠いため、他の人はどれくらいの距離に住んでいるのかと疑問に思ったのです。"},{"id":{"d":359,"n":4,"t":3},"speaker":"B","text":"私の家は職場から近い方だと思うので、あまり参考にならないかもしれません。"},{"id":{"d":359,"n":5,"t":3},"speaker":"A","text":"確かにとても近いですね。普通は電車でどれくらいかかるのでしょうか？"},{"id":{"d":359,"n":6,"t":3},"speaker":"B","text":"どうなのでしょうね。私の知人の中には、1人暮らしの家から職場まで1時間ほどかかるという人もいますよ。"},{"id":{"d":359,"n":7,"t":3},"speaker":"A","text":"1人暮らしでそれほど職場から遠くに住んでいるというのは珍しいですね。"},{"id":{"d":359,"n":8,"t":3},"speaker":"B","text":"私もそう思ったのですが、彼はどうしてもその建物に住みたかったのだそうです。"}];
INSERT INTO sentence [{"id":{"d":360,"n":1,"t":3},"speaker":"A","text":"この前、北海道でタクシーに乗ったのですが、そのときに怖い体験をしました。"},{"id":{"d":360,"n":2,"t":3},"speaker":"B","text":"何があったのですか？"},{"id":{"d":360,"n":3,"t":3},"speaker":"A","text":"つづら折りのいかにもスリップしそうな山道を、かなりのスピードで運転していたのです。"},{"id":{"d":360,"n":4,"t":3},"speaker":"B","text":"大変でしたね。スリップはしなかったのですか？"},{"id":{"d":360,"n":5,"t":3},"speaker":"A","text":"それが全くしませんでした。スピードは出ているように感じたのですが、速度超過もなく安全運転でした。"},{"id":{"d":360,"n":6,"t":3},"speaker":"B","text":"さすがプロですね。目が慣れていませんので、速く感じただけかもしれませんよ。"},{"id":{"d":360,"n":7,"t":3},"speaker":"A","text":"そうかもしれません、でも、乗っている時には心臓が止まりそうな気分でした。"}];
INSERT INTO sentence [{"id":{"d":361,"n":1,"t":3},"speaker":"A","text":"大型トラックに乗ったことはありますか？"},{"id":{"d":361,"n":2,"t":3},"speaker":"B","text":"ありません。もしかして、あなたはあるのですか？"},{"id":{"d":361,"n":3,"t":3},"speaker":"A","text":"実は私は乗ったことがあるのです。"},{"id":{"d":361,"n":4,"t":3},"speaker":"B","text":"なぜ乗ることになったのですか？普通の生活ではなかなか乗る機会はなさそうですが。"},{"id":{"d":361,"n":5,"t":3},"speaker":"A","text":"北海道に住んでいたとき、最終のバスを逃してしまって、バス停で凍えている時にたまたま通りかかった大型トラックに家まで送ってもらったことがあります。"},{"id":{"d":361,"n":6,"t":3},"speaker":"B","text":"優しい運転手さんですね。乗ってみてどうでしたか？"},{"id":{"d":361,"n":7,"t":3},"speaker":"A","text":"視点が高過ぎて怖かったことを覚えています。"},{"id":{"d":361,"n":8,"t":3},"speaker":"B","text":"視点が高い車の方が運転しやすいと言われますが、慣れない高さの目線はやはり怖く感じるのですね。"}];
INSERT INTO sentence [{"id":{"d":362,"n":1,"t":3},"speaker":"A","text":"いつもの5人で旅行に行きませんか？"},{"id":{"d":362,"n":2,"t":3},"speaker":"B","text":"ぜひ行きたいです。ずっと行きたいという話をしていましたが、行けなかったですからね。"},{"id":{"d":362,"n":3,"t":3},"speaker":"A","text":"みんなでスケジュールを合わせましょう。石川さんは来月の中頃はどうですか？"},{"id":{"d":362,"n":4,"t":3},"speaker":"B","text":"中旬は出張が入る予定なので、少しずらしてほしいです。"},{"id":{"d":362,"n":5,"t":3},"speaker":"A","text":"分かりました。ところで出張はどこに行く予定なのですか？"},{"id":{"d":362,"n":6,"t":3},"speaker":"B","text":"大阪に行く予定です。水曜日から金曜日まで行きます。"},{"id":{"d":362,"n":7,"t":3},"speaker":"A","text":"それでは、石川さんの出張後、金曜日から日曜日にかけての2泊3日で大阪に行くのはどうでしょうか？"},{"id":{"d":362,"n":8,"t":3},"speaker":"B","text":"それはいいですね。みんなで遊園地に行ったり、粉物を食べたりしたいです。"}];
INSERT INTO sentence [{"id":{"d":363,"n":1,"t":3},"speaker":"A","text":"プログラミングを勉強している中で英語の重要性を改めて感じました。"},{"id":{"d":363,"n":2,"t":3},"speaker":"B","text":"どうしてそう感じたのですか？"},{"id":{"d":363,"n":3,"t":3},"speaker":"A","text":"プログラミングは基本的に英語で書かないといけないのです。"},{"id":{"d":363,"n":4,"t":3},"speaker":"B","text":"なるほど。プログラムは世界共通で用いられるものですから、世界共通語を用いることは至極まっとうだと思います。"},{"id":{"d":363,"n":5,"t":3},"speaker":"A","text":"プログラム自体は比較的簡単な英単語が多いのですが、エラー文が難しいのです。"},{"id":{"d":363,"n":6,"t":3},"speaker":"B","text":"エラー文こそ単語ではなく英文なので分かりやすいのではありませんか？"},{"id":{"d":363,"n":7,"t":3},"speaker":"A","text":"普通の英文はもちろん読めるのですが、エラー文には専門用語が多く含まれるため難しいのです。毎回、分からない単語を検索しながら読んでいます。"},{"id":{"d":363,"n":8,"t":3},"speaker":"B","text":"確かにそれは大変そうですね。"}];
INSERT INTO sentence [{"id":{"d":364,"n":1,"t":3},"speaker":"A","text":"昨日、英語の発音の違いを紹介している動画を見ました。"},{"id":{"d":364,"n":2,"t":3},"speaker":"B","text":"どのような紹介がされていたのですか？"},{"id":{"d":364,"n":3,"t":3},"speaker":"A","text":"その動画では、アメリカの発音、イギリスの発音、オーストラリアの発音が紹介されていました。"},{"id":{"d":364,"n":4,"t":3},"speaker":"B","text":"アメリカとイギリスで発音が異なることは知っていますが、イギリスとオーストラリアも異なるのですか？"},{"id":{"d":364,"n":5,"t":3},"speaker":"A","text":"アメリカとイギリスの差に比べると、ほとんど同じだと感じました。少し違うように感じた部分もありましたが、私にとっては、発音する人が違うから少し違うというくらいの差でした。"},{"id":{"d":364,"n":6,"t":3},"speaker":"B","text":"それぞれの特徴は何かありましたか？"},{"id":{"d":364,"n":7,"t":3},"speaker":"A","text":"はい。まず、アメリカの英語は学校でも勉強している通りのものでした。聞き慣れた英語という印象です。"},{"id":{"d":364,"n":8,"t":3},"speaker":"B","text":"そうですね。日本ではアメリカ英語を学ぶことが多いですね。"},{"id":{"d":364,"n":9,"t":3},"speaker":"A","text":"イギリス英語はカタカナのような印象を受けました。日本人にとっては、もしかしたら、イギリス英語の方が発音しやすいかもしれません。"},{"id":{"d":364,"n":10,"t":3},"speaker":"B","text":"そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":365,"n":1,"t":3},"speaker":"A","text":"バイクでツーリングに行ってみたいです。"},{"id":{"d":365,"n":2,"t":3},"speaker":"B","text":"ツーリングは良いですよ。そういえば、今日仲間とバイクに乗っているときにとても格好良いバイクに乗ったおじさんたちとすれ違いました。"},{"id":{"d":365,"n":3,"t":3},"speaker":"A","text":"彼らはどのように格好良かったのですか？"},{"id":{"d":365,"n":4,"t":3},"speaker":"B","text":"仲間内でハンドサインを送り合っていたのですが、それがとてもスマートですてきだったのです。"},{"id":{"d":365,"n":5,"t":3},"speaker":"A","text":"バイクに乗っている人は、仲間を見かけるとハンドサインを送ることがありますよね。"},{"id":{"d":365,"n":6,"t":3},"speaker":"B","text":"はい。バイク乗りならではのすてきなコミュニケーションだなと思います。よくご存知ですね。"},{"id":{"d":365,"n":7,"t":3},"speaker":"A","text":"父がバイク乗りだったので、そのような話を聞いたことがありました。それもあって、幼い頃からバイクに憧れがあるのです。"}];
INSERT INTO sentence [{"id":{"d":366,"n":1,"t":3},"speaker":"A","text":"北海道大学の金葉祭に行きませんか？"},{"id":{"d":366,"n":2,"t":3},"speaker":"B","text":"いいですね。ちょうど今週末にはイチョウ並木のライトアップも始まるそうですよ。"},{"id":{"d":366,"n":3,"t":3},"speaker":"A","text":"ライトアップはとてもきれいだそうですね。ぜひ見に行きたいです。"},{"id":{"d":366,"n":4,"t":3},"speaker":"B","text":"この時期はとても寒いので、しっかり厚着をしていきましょう。"},{"id":{"d":366,"n":5,"t":3},"speaker":"A","text":"はい。見終わったら近くのよく行く居酒屋へも行きませんか？"},{"id":{"d":366,"n":6,"t":3},"speaker":"B","text":"とても良いプランですね。思い出に残りそうです。"},{"id":{"d":366,"n":7,"t":3},"speaker":"A","text":"お互い北海道にいられる最後の年ですからね。一緒にすてきな思い出が作れたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":367,"n":1,"t":3},"speaker":"A","text":"新婚旅行はどこに行きたいですか？"},{"id":{"d":367,"n":2,"t":3},"speaker":"B","text":"私はハワイやグアムなどの南の島に行きたいです。"},{"id":{"d":367,"n":3,"t":3},"speaker":"A","text":"ハワイやグアムは定番ですよね。私はオーストラリアやニュージーランドに行きたいです。"},{"id":{"d":367,"n":4,"t":3},"speaker":"B","text":"オーストラリアやニュージーランドも定番ですね。せっかくなので、定番ではなくても、私たちだからこそという場所にしませんか？"},{"id":{"d":367,"n":5,"t":3},"speaker":"A","text":"例えばどこですか？何か共通点の興味のある場所ということですか？"},{"id":{"d":367,"n":6,"t":3},"speaker":"B","text":"お互いサッカーが好きなので、ブラジルやヨーロッパはどうでしょうか？"},{"id":{"d":367,"n":7,"t":3},"speaker":"A","text":"いいですね。それでは新婚旅行としてヨーロッパにサッカーの試合を見に行くのはどうですか？"},{"id":{"d":367,"n":8,"t":3},"speaker":"B","text":"とても魅力的です。いくつかの国に滞在して、それぞれの国のリーグ戦を見に行くのはどうでしょうか？"},{"id":{"d":367,"n":9,"t":3},"speaker":"A","text":"それは面白そうですね。"}];
INSERT INTO sentence [{"id":{"d":368,"n":1,"t":3},"speaker":"A","text":"今度、ビール工房に行きませんか？"},{"id":{"d":368,"n":2,"t":3},"speaker":"B","text":"いいですね。どこのビール工房にするか迷いますね。"},{"id":{"d":368,"n":3,"t":3},"speaker":"A","text":"では、最初は工場見学にするのはどうでしょう。"},{"id":{"d":368,"n":4,"t":3},"speaker":"B","text":"工房を知る前にまずは量産体制の所を見に行くということですね。良い考えだと思います。"},{"id":{"d":368,"n":5,"t":3},"speaker":"A","text":"どこか行きたい所はありますか？"},{"id":{"d":368,"n":6,"t":3},"speaker":"B","text":"カップラーメンの工場のように、ビール工場にも、自分のオリジナルビールを作らせてもらえるところはあるのでしょうか？"},{"id":{"d":368,"n":7,"t":3},"speaker":"A","text":"調べたら見つかるかもしれませんね。"},{"id":{"d":368,"n":8,"t":3},"speaker":"B","text":"もしそのようなところがあったら、そこへ行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":369,"n":1,"t":3},"speaker":"A","text":"すみません、チェックインしたいのですが。"},{"id":{"d":369,"n":2,"t":3},"speaker":"B","text":"ご予約の名前をお伺いしてよろしいですか？"},{"id":{"d":369,"n":3,"t":3},"speaker":"A","text":"浜田と言います。"},{"id":{"d":369,"n":4,"t":3},"speaker":"B","text":"浜田さまですね。ただいま確認いたしますので少々お待ちください。"},{"id":{"d":369,"n":5,"t":3},"speaker":"A","text":"よろしくお願いします。"},{"id":{"d":369,"n":6,"t":3},"speaker":"B","text":"確認が取れました。1名さまでのご予約ですね。"},{"id":{"d":369,"n":7,"t":3},"speaker":"A","text":"はい、そうです。少し予約の時間より早いのですがよろしいですか？"},{"id":{"d":369,"n":8,"t":3},"speaker":"B","text":"問題ございません。お部屋の準備はできておりますのでご入室いただけます。"},{"id":{"d":369,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":370,"n":1,"t":3},"speaker":"A","text":"家の近くにとてもきれいなサクラの木があります。"},{"id":{"d":370,"n":2,"t":3},"speaker":"B","text":"それはいいですね。有名な木なのですか？"},{"id":{"d":370,"n":3,"t":3},"speaker":"A","text":"由緒ある木ではないようですが、ある音楽番組で、森山直太朗さんがそのサクラの木の前で歌っているのを見たことがあります。"},{"id":{"d":370,"n":4,"t":3},"speaker":"B","text":"まあ、それはきっと素晴らしいサクラなのでしょうね。"},{"id":{"d":370,"n":5,"t":3},"speaker":"A","text":"私はこれまで日中にしかそのサクラを見たことがなかったのですが、先日、その音楽番組で初めて夜桜を見て、あまりの美しさに感動しました。"},{"id":{"d":370,"n":6,"t":3},"speaker":"B","text":"近くにあるのに、初めて夜桜を見たのがテレビというのは、少し複雑ではありませんか？"},{"id":{"d":370,"n":7,"t":3},"speaker":"A","text":"いいえ、番組ではとてもきれいにライトアップされていたので、初めて見たのがテレビでむしろ良かったかもしれないと思っていますよ。"},{"id":{"d":370,"n":8,"t":3},"speaker":"B","text":"なるほど。確かに、撮影のために普段よりさらにきれいにしていたかもしれませんね。"},{"id":{"d":370,"n":9,"t":3},"speaker":"A","text":"またライトアップされるかは分かりませんが、来年以降もぜひ夜桜を見に行きたいと思います。"}];
INSERT INTO sentence [{"id":{"d":371,"n":1,"t":3},"speaker":"A","text":"外国に行ってみたいのですが、特にご飯がおいしい場所はどこでしょうか？"},{"id":{"d":371,"n":2,"t":3},"speaker":"B","text":"感性は人それぞれなので何ともいえませんが、韓国のご飯はおいしいと聞いたことがあります。"},{"id":{"d":371,"n":3,"t":3},"speaker":"A","text":"そうなのですね。あなたはたくさん海外へ行っていますが、どのような食べ物をおいしいと思いましたか？"},{"id":{"d":371,"n":4,"t":3},"speaker":"B","text":"私は、アメリカのハンバーガーやホットドッグがとてもおいしかったですね。"},{"id":{"d":371,"n":5,"t":3},"speaker":"A","text":"アメリカの食べ物はサイズが大きいと聞いたことがありますが、それは本当ですか？"},{"id":{"d":371,"n":6,"t":3},"speaker":"B","text":"本当です。大きくて味が濃いので、ハンバーガーもホットドッグも1個で十分に満足できるおいしさです。"},{"id":{"d":371,"n":7,"t":3},"speaker":"A","text":"そうなのですね。そのようなハンバーガーをぜひ一度は食べてみたいものです。"},{"id":{"d":371,"n":8,"t":3},"speaker":"B","text":"横須賀海軍基地の近くのハンバーガー屋さんでは、本場の味にとても近いハンバーガーを食べることができますよ。一緒に行きませんか？"},{"id":{"d":371,"n":9,"t":3},"speaker":"A","text":"とても気になります。ぜひ連れて行ってください。"}];
INSERT INTO sentence [{"id":{"d":372,"n":1,"t":3},"speaker":"A","text":"合宿が近づいてきましたね。"},{"id":{"d":372,"n":2,"t":3},"speaker":"B","text":"はい。今年は3泊4日常に天気が良いようなので楽しみです。"},{"id":{"d":372,"n":3,"t":3},"speaker":"A","text":"そうですね。ここ最近ではずっと天気が良いのは久しぶりかもしれません。"},{"id":{"d":372,"n":4,"t":3},"speaker":"B","text":"暑くなりそうなので、熱中症対策をしっかりしないといけませんね。"},{"id":{"d":372,"n":5,"t":3},"speaker":"A","text":"加えて、日焼け対策も必要不可欠ですね。"},{"id":{"d":372,"n":6,"t":3},"speaker":"B","text":"日焼け止めクリームは必須ですね。改めて、天気が良いことによって必要となるものを確認しましょう。"},{"id":{"d":372,"n":7,"t":3},"speaker":"A","text":"熱中症予防のために、十分な水分と汗を拭くためのタオルは必要ですね。塩分補給の飴もあった方がいいでしょう。"},{"id":{"d":372,"n":8,"t":3},"speaker":"B","text":"早速、今からドラッグストアに行ってこれらを買ってこようと思います。何か他に必要なものがあったら連絡してください。"}];
INSERT INTO sentence [{"id":{"d":373,"n":1,"t":3},"speaker":"A","text":"今年中であればいつでもよいので、水族館に行きたいです。"},{"id":{"d":373,"n":2,"t":3},"speaker":"B","text":"今年はあと10カ月もありますが、どの時期でもよいのですか？"},{"id":{"d":373,"n":3,"t":3},"speaker":"A","text":"はい。いつでもよいです。"},{"id":{"d":373,"n":4,"t":3},"speaker":"B","text":"行き先も水族館であればどこでもよいのですか？"},{"id":{"d":373,"n":5,"t":3},"speaker":"A","text":"シャチが跳んでいるところを見たいので、できれば鴨川シーワールドに行きたいです。"},{"id":{"d":373,"n":6,"t":3},"speaker":"B","text":"シャチですか。水がかかるかもしれないと聞きましたよ。"},{"id":{"d":373,"n":7,"t":3},"speaker":"A","text":"ぬれるかもしれませんが、近くで格好良い姿を見てみたいのです。"},{"id":{"d":373,"n":8,"t":3},"speaker":"B","text":"確かにそれは迫力がありそうですね。では、ぬれる可能性もありますから行くのは夏にしましょう。"},{"id":{"d":373,"n":9,"t":3},"speaker":"A","text":"分かりました。今年の夏が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":374,"n":1,"t":3},"speaker":"A","text":"すみません。このバスはどこまで行きますか？"},{"id":{"d":374,"n":2,"t":3},"speaker":"B","text":"こちらは品川に向かうバスですよ。"},{"id":{"d":374,"n":3,"t":3},"speaker":"A","text":"これ以外のバスで品川に行くことはできますか？"},{"id":{"d":374,"n":4,"t":3},"speaker":"B","text":"ええ、他のバスでも行けます。"},{"id":{"d":374,"n":5,"t":3},"speaker":"A","text":"一番安いバスはどのバスか分かりますか？"},{"id":{"d":374,"n":6,"t":3},"speaker":"B","text":"比較をしたことがありませんので分かりませんが、値段はほとんど同じだと思います。"},{"id":{"d":374,"n":7,"t":3},"speaker":"A","text":"そうですか。電車と比べると、どちらの方が安いでしょうか？"},{"id":{"d":374,"n":8,"t":3},"speaker":"B","text":"それはバスの方が安いですね。しかし電車の方が早く着きます。"},{"id":{"d":374,"n":9,"t":3},"speaker":"A","text":"なるほど、ありがとうございます。急いでいるわけではありませんのでこのバスに乗ろうと思います。"}];
INSERT INTO sentence [{"id":{"d":375,"n":1,"t":3},"speaker":"A","text":"かわいい子には旅をさせよと言いますが、小さい頃に一人旅を経験したことはありますか？"},{"id":{"d":375,"n":2,"t":3},"speaker":"B","text":"祖父母の家が岡山にあったので、小学生の時に1人で岡山まで行ったことがあります。"},{"id":{"d":375,"n":3,"t":3},"speaker":"A","text":"千葉から岡山への旅行とはなかなか距離がありますね。何を使って行ったのですか？"},{"id":{"d":375,"n":4,"t":3},"speaker":"B","text":"飛行機で行きました。飛行機に1人で乗るのは初めてだったのでとても緊張しましたが、客室乗務員の方が優しく対応してくれたため、落ち着いて乗ることができました。"},{"id":{"d":375,"n":5,"t":3},"speaker":"A","text":"とても良い経験ですね。他に行ったことのある所はありますか？"},{"id":{"d":375,"n":6,"t":3},"speaker":"B","text":"もっと小さい小学校低学年の時には、電車を乗り継いで神奈川の祖父母の家に行きました。"},{"id":{"d":375,"n":7,"t":3},"speaker":"A","text":"電車とはいえ、小学校低学年で一人旅をするとは素晴らしいですね。"},{"id":{"d":375,"n":8,"t":3},"speaker":"B","text":"われながらよく行ったと思います。その時は怖いもの知らずだったのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":376,"n":1,"t":3},"speaker":"A","text":"人間が乗ることのできるドローンの開発が行われているそうです。"},{"id":{"d":376,"n":2,"t":3},"speaker":"B","text":"私もテレビのニュースの特集で見たことがあります。航空会社と通信会社が共同で製作しているそうですね。"},{"id":{"d":376,"n":3,"t":3},"speaker":"A","text":"はい。とても面白い挑戦だと思いますが、想像していたものとは形が違って驚きました。"},{"id":{"d":376,"n":4,"t":3},"speaker":"B","text":"ええ。飛行機やヘリコプターのようなものではなく、まさにドローンという見た目でしたね。"},{"id":{"d":376,"n":5,"t":3},"speaker":"A","text":"あのままドローンを小さくしたら、普通のドローンになりそうです。"},{"id":{"d":376,"n":6,"t":3},"speaker":"B","text":"人間が乗ることができるドローンという紹介ですから、それで正しいのかもしれません。"},{"id":{"d":376,"n":7,"t":3},"speaker":"A","text":"なるほど、言われてみればそうですね。"}];
INSERT INTO sentence [{"id":{"d":377,"n":1,"t":3},"speaker":"A","text":"今日は久しぶりに外出をしたのですが、とても楽しかったです。"},{"id":{"d":377,"n":2,"t":3},"speaker":"B","text":"何日ぶりで、どこまで外出したのですか？"},{"id":{"d":377,"n":3,"t":3},"speaker":"A","text":"今年に入って初めて外出しました。行き先は上野です。"},{"id":{"d":377,"n":4,"t":3},"speaker":"B","text":"なぜ上野まで外出したのですか？"},{"id":{"d":377,"n":5,"t":3},"speaker":"A","text":"大学の友達から荷物を受け取りに行きました。それ以外には特に何もしていないため、少し寂しかったですが。"},{"id":{"d":377,"n":6,"t":3},"speaker":"B","text":"今年初めての外出は楽しかったですか？"},{"id":{"d":377,"n":7,"t":3},"speaker":"A","text":"はい。家族以外の人がいる場所はとても久しぶりでしたから、とても楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":378,"n":1,"t":3},"speaker":"A","text":"先週は毎日外で遊んでいましたから、今日は児童館へ行きませんか？"},{"id":{"d":378,"n":2,"t":3},"speaker":"B","text":"いいですね。児童館へ行くのは久しぶりです。"},{"id":{"d":378,"n":3,"t":3},"speaker":"A","text":"それでは、いつも遊んでいるみんなも誘ってみますね。"},{"id":{"d":378,"n":4,"t":3},"speaker":"B","text":"よろしくお願いします。児童館では何をして遊びましょうか？"},{"id":{"d":378,"n":5,"t":3},"speaker":"A","text":"私はドミノで遊びたいです。"},{"id":{"d":378,"n":6,"t":3},"speaker":"B","text":"前回は、周囲にたくさん人がいたこともあって、何度も崩してしまいましたね。どうやらドミノ専用の部屋ができたようですので、今日はそこでドミノをしましょう。"},{"id":{"d":378,"n":7,"t":3},"speaker":"A","text":"それはうれしいです。どこまで長くつなげることができるか楽しみです。"},{"id":{"d":378,"n":8,"t":3},"speaker":"B","text":"足の踏み場がないくらい、部屋いっぱいに作りたいですね。"}];
INSERT INTO sentence [{"id":{"d":379,"n":1,"t":3},"speaker":"A","text":"普段タクシーを利用しますか？"},{"id":{"d":379,"n":2,"t":3},"speaker":"B","text":"普段はあまり利用しませんが、どうしても電車や待ち合わせに間に合わないと判断した時は使っています。"},{"id":{"d":379,"n":3,"t":3},"speaker":"A","text":"それではあまり使用頻度は高くないということですね。"},{"id":{"d":379,"n":4,"t":3},"speaker":"B","text":"普通の人がどれくらい使っているのか分かりませんが、多くはないと思います。"},{"id":{"d":379,"n":5,"t":3},"speaker":"A","text":"それでしたらこの配車サービスアプリケーションがお勧めですよ。"},{"id":{"d":379,"n":6,"t":3},"speaker":"B","text":"どのようなものですか？"},{"id":{"d":379,"n":7,"t":3},"speaker":"A","text":"あまりタクシーを使わない人でも、最初から自分の目的地を伝えた上でタクシーを手配することができるというサービスです。"},{"id":{"d":379,"n":8,"t":3},"speaker":"B","text":"なるほど。それではタクシーの中でほとんど会話をしなくてもよいのでしょうか？"},{"id":{"d":379,"n":9,"t":3},"speaker":"A","text":"その通りです。会話が苦手な人にもいいかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":380,"n":1,"t":3},"speaker":"A","text":"試験の日に限って公共交通機関がよく止まるような気がします。"},{"id":{"d":380,"n":2,"t":3},"speaker":"B","text":"確かに試験の日は天気が悪くなったり、それによって公共交通機関が止まったりすることも多いですね。"},{"id":{"d":380,"n":3,"t":3},"speaker":"A","text":"公共交通機関であれば遅延証明書をもらえますが、遅延証明書があれば遅刻しても試験を受けられるのでしょうか？"},{"id":{"d":380,"n":4,"t":3},"speaker":"B","text":"私が最後にそのような試験を受けたのはもう7年も前ですが、どのような理由であれ遅刻は許されませんでした。"},{"id":{"d":380,"n":5,"t":3},"speaker":"A","text":"それでは電車が遅れた場合はどうしたのですか？"},{"id":{"d":380,"n":6,"t":3},"speaker":"B","text":"もし電車が遅れたとしても遅刻しないように、早めに家を出るしかありませんでした。他の対処法はありませんでした。"},{"id":{"d":380,"n":7,"t":3},"speaker":"A","text":"確かに、私も試験の日はとても早く移動していた記憶があります。"},{"id":{"d":380,"n":8,"t":3},"speaker":"B","text":"当時は万が一に備えてかなり早く家を出ていたので、電車がかなり遅延したにもかかわらず1時間前に試験場に到着したことがあります。"}];
INSERT INTO sentence [{"id":{"d":381,"n":1,"t":3},"speaker":"A","text":"明日は久しぶりにホテルに宿泊します。"},{"id":{"d":381,"n":2,"t":3},"speaker":"B","text":"それは楽しみですね。"},{"id":{"d":381,"n":3,"t":3},"speaker":"A","text":"はい。ただ、ホテルで寝るのは3年ぶりなので、気分が高揚し過ぎてなかなか寝つけないかもしれません。"},{"id":{"d":381,"n":4,"t":3},"speaker":"B","text":"それほど良いホテルに宿泊するのですか？"},{"id":{"d":381,"n":5,"t":3},"speaker":"A","text":"全く特別なホテルではありません。一般的なビジネスホテルです。"},{"id":{"d":381,"n":6,"t":3},"speaker":"B","text":"それでも久しぶりだと高揚するということですね。"},{"id":{"d":381,"n":7,"t":3},"speaker":"A","text":"はい。宿泊後には半年間会うことができていない友達と会う予定なので、それもとても楽しみです。"},{"id":{"d":381,"n":8,"t":3},"speaker":"B","text":"友達と楽しく過ごすためにも、ホテルでゆっくりできるといいですね。"}];
INSERT INTO sentence [{"id":{"d":382,"n":1,"t":3},"speaker":"A","text":"北海道に旅行に来ています。"},{"id":{"d":382,"n":2,"t":3},"speaker":"B","text":"とても楽しそうですね。やはり北海道は寒いですか？"},{"id":{"d":382,"n":3,"t":3},"speaker":"A","text":"寒いですが、本州とそれほど変わらないように感じます。"},{"id":{"d":382,"n":4,"t":3},"speaker":"B","text":"本当ですか？気温を見る限り北海道の方がとても寒いようですが。"},{"id":{"d":382,"n":5,"t":3},"speaker":"A","text":"恐らく本州にいるときよりも多く中に着込んでいるので、あまり寒く感じないのだと思います。"},{"id":{"d":382,"n":6,"t":3},"speaker":"B","text":"確かに本州でたくさん着込んでいたら暑くなってしまいますね。"},{"id":{"d":382,"n":7,"t":3},"speaker":"A","text":"北海道旅行では、本州で飛行機に乗る時と北海道に着いた時の服装を変えることが一番大変かもしれません。"},{"id":{"d":382,"n":8,"t":3},"speaker":"B","text":"なるほど。それさえ気を付ければ楽しい旅行になりますね。"}];
INSERT INTO sentence [{"id":{"d":383,"n":1,"t":3},"speaker":"A","text":"行きも帰りも飛行機で非常口座席に座りました。"},{"id":{"d":383,"n":2,"t":3},"speaker":"B","text":"珍しいですね。非常口座席の良い点、悪い点は何ですか？"},{"id":{"d":383,"n":3,"t":3},"speaker":"A","text":"良い点はとても足元が広い点です。私のように身長の高い人には特に良いと思います。"},{"id":{"d":383,"n":4,"t":3},"speaker":"B","text":"確かに飛行機の普通座席は足元が狭いので、長く座っていると足が疲れてしまいますね。"},{"id":{"d":383,"n":5,"t":3},"speaker":"A","text":"はい。悪い点は座席で作業をすることができない点です。"},{"id":{"d":383,"n":6,"t":3},"speaker":"B","text":"その作業とは具体的にどのようなものですか？"},{"id":{"d":383,"n":7,"t":3},"speaker":"A","text":"例えば、座席でテーブルパソコンを操作したり、書類を書いたりすることはできません。前の席が遠いので、テーブルを使うことができないのです。"},{"id":{"d":383,"n":8,"t":3},"speaker":"B","text":"それは社会人にはデメリットだと感じられるかもしれませんね。私だったら移動時間にもテーブルで作業をしたいと感じてしまいます。"}];
INSERT INTO sentence [{"id":{"d":384,"n":1,"t":3},"speaker":"A","text":"来週から卒業旅行をしてきます。行き先はどこだと思いますか？"},{"id":{"d":384,"n":2,"t":3},"speaker":"B","text":"よく聞く行き先は、北海道や沖縄などですね。普段はあまり行かない場所は人気がある印象です。"},{"id":{"d":384,"n":3,"t":3},"speaker":"A","text":"良い読みですね。"},{"id":{"d":384,"n":4,"t":3},"speaker":"B","text":"あなたは北海道の大学に通っていますから、旅行先は沖縄でしょうか？"},{"id":{"d":384,"n":5,"t":3},"speaker":"A","text":"いいえ、違います。私たちは北海道内を旅行する予定です。"},{"id":{"d":384,"n":6,"t":3},"speaker":"B","text":"道内は学生の間に十分巡ったのではありませんか？"},{"id":{"d":384,"n":7,"t":3},"speaker":"A","text":"確かに十分と言ってよいほど巡りましたが、昨今の状況を踏まえて道内旅行にしようという話になりました。"},{"id":{"d":384,"n":8,"t":3},"speaker":"B","text":"そうでしたか。就職したらあまり遊ぶこともできなくなりますから、今までよく行った所に最後に行くというのもいいかもしれませんね。"},{"id":{"d":384,"n":9,"t":3},"speaker":"A","text":"はい、そのつもりです。学生時代を振り返るという意味合いも込めて、楽しんでこようと思います。"}];
INSERT INTO sentence [{"id":{"d":385,"n":1,"t":3},"speaker":"A","text":"あなたは、旅行の予定は事前にしっかり立てておきたいと思いますか？"},{"id":{"d":385,"n":2,"t":3},"speaker":"B","text":"私はできるだけ予定を立てておきたいです。現地であまり無駄な時間を過ごしたくありません。"},{"id":{"d":385,"n":3,"t":3},"speaker":"A","text":"それではかなり厳しく時間管理をしてスケジュールを立てますか？"},{"id":{"d":385,"n":4,"t":3},"speaker":"B","text":"私が企画する旅行では基本的にスケジュールを全てしっかり決めます。石野さんはあまりしっかり決めませんか？"},{"id":{"d":385,"n":5,"t":3},"speaker":"A","text":"私は行きたい場所を大まかにリストアップしておいて、電車の時間や道の混雑状況などさまざまなものを考慮しながらその場でプランを組みます。"},{"id":{"d":385,"n":6,"t":3},"speaker":"B","text":"それでうまくいくのですか？"},{"id":{"d":385,"n":7,"t":3},"speaker":"A","text":"これまで大きな問題が起きたことはありません。旅行で行きたい所を巡ることはもちろん大事ですが、それ以上に一緒に行く人と楽しい時間を過ごすことも大事だと思っているので、私はこの方法が好きです。"},{"id":{"d":385,"n":8,"t":3},"speaker":"B","text":"時間を厳しく決めると楽しめませんか？"},{"id":{"d":385,"n":9,"t":3},"speaker":"A","text":"楽しめると思います。ただ、厳しく決めていると、少し予定と時間が狂ったときに楽しさよりもスケジュールを優先してしまいそうです。"},{"id":{"d":385,"n":10,"t":3},"speaker":"B","text":"確かに、そのようなことは多々ありますね。"}];
INSERT INTO sentence [{"id":{"d":386,"n":1,"t":3},"speaker":"A","text":"レンタカーを利用するという手もあります。武田さんはよく車を運転しますか？"},{"id":{"d":386,"n":2,"t":3},"speaker":"B","text":"はい。職業柄、全国どこでも運転します。"},{"id":{"d":386,"n":3,"t":3},"speaker":"A","text":"それでは北海道や新潟など雪が降る地域で、雪の中を運転したことはありますか？"},{"id":{"d":386,"n":4,"t":3},"speaker":"B","text":"札幌で少し運転したことがあります。雪の中の運転は正直なところ恐怖がありました。"},{"id":{"d":386,"n":5,"t":3},"speaker":"A","text":"今年は例年に比べてとても雪が降っていますから、レンタカーの運転はやめた方が賢明かもしれません。"},{"id":{"d":386,"n":6,"t":3},"speaker":"B","text":"今年は近年でもまれに見る大雪ですからね。では、今回は他の手段を検討しましょう。"}];
INSERT INTO sentence [{"id":{"d":387,"n":1,"t":3},"speaker":"A","text":"今度ワカサギ釣りに行きませんか？"},{"id":{"d":387,"n":2,"t":3},"speaker":"B","text":"ぜひ行きたいです。ワカサギ釣りには行ったことがありませんが、本当に釣れるのですか？"},{"id":{"d":387,"n":3,"t":3},"speaker":"A","text":"私も一度しか行ったことがありませんが、その時はたくさん釣ることができました。とても楽しかったですよ。"},{"id":{"d":387,"n":4,"t":3},"speaker":"B","text":"ワカサギは釣れたらその場で食べられますか？"},{"id":{"d":387,"n":5,"t":3},"speaker":"A","text":"はい。私はその場で天ぷらにして食べました。とても柔らかくおいしかったのでぜひ食べてみてほしいです。"},{"id":{"d":387,"n":6,"t":3},"speaker":"B","text":"それはおいしそうです。ぜひ行きましょう。何か注意点はありますか？"},{"id":{"d":387,"n":7,"t":3},"speaker":"A","text":"釣り場が氷の上ですから、もちろんとても寒いです。防寒対策はしっかりしてくださいね。"},{"id":{"d":387,"n":8,"t":3},"speaker":"B","text":"分かりました。しっかり防寒対策をしてワカサギ釣りを楽しみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":388,"n":1,"t":3},"speaker":"A","text":"日本には春夏秋冬が存在し、さまざまな季節の行事がありますね。どの季節が一番好きですか？"},{"id":{"d":388,"n":2,"t":3},"speaker":"B","text":"私は秋が一番好きです。"},{"id":{"d":388,"n":3,"t":3},"speaker":"A","text":"なぜ秋が一番好きなのですか？"},{"id":{"d":388,"n":4,"t":3},"speaker":"B","text":"秋はご飯がおいしいからです。紅葉狩りが楽しめるのもいいです。"},{"id":{"d":388,"n":5,"t":3},"speaker":"A","text":"確かに、秋はご飯がおいしく、美しい風景が楽しめる季節ですね。"},{"id":{"d":388,"n":6,"t":3},"speaker":"B","text":"他にも、秋は花粉が少なく、花粉症の私にとっては移動しやすい時期という理由もあります。"},{"id":{"d":388,"n":7,"t":3},"speaker":"A","text":"なるほど。花粉症の方にとって春の時期は外出しにくいですよね。"},{"id":{"d":388,"n":8,"t":3},"speaker":"B","text":"はい。そのような理由から、私にとって秋はとても魅力的な季節です。"}];
INSERT INTO sentence [{"id":{"d":389,"n":1,"t":3},"speaker":"A","text":"寝台特急に乗ったことはありますか？"},{"id":{"d":389,"n":2,"t":3},"speaker":"B","text":"一度もありません。乗ってみたいと思っていますが乗る機会がありません。"},{"id":{"d":389,"n":3,"t":3},"speaker":"A","text":"それでは今度乗ってみませんか？"},{"id":{"d":389,"n":4,"t":3},"speaker":"B","text":"ぜひ乗りたいです。どの寝台特急に乗るか決めていますか？"},{"id":{"d":389,"n":5,"t":3},"speaker":"A","text":"私は「ななつ星」という列車に乗ってみたいです。数年前にテレビで見てからずっと気になっています。"},{"id":{"d":389,"n":6,"t":3},"speaker":"B","text":"「ななつ星」ですか。少し調べてみたところ、まるでスイートルームのような内装ですね。"},{"id":{"d":389,"n":7,"t":3},"speaker":"A","text":"そうなのです。すてきでしょう？"},{"id":{"d":389,"n":8,"t":3},"speaker":"B","text":"ええ。とてもすきてです。"},{"id":{"d":389,"n":9,"t":3},"speaker":"A","text":"この列車には、大人しか乗車することができないそうです。落ち着いた旅行ができそうで憧れています。"},{"id":{"d":389,"n":10,"t":3},"speaker":"B","text":"私もぜひこの寝台特急に乗ってみたいです。これにしましょう。"}];
INSERT INTO sentence [{"id":{"d":390,"n":1,"t":3},"speaker":"A","text":"飛行機のツアーを調べていたら、飛行機代とホテル代を合わせた金額が、片道の飛行機代よりも安いツアーを見つけました。"},{"id":{"d":390,"n":2,"t":3},"speaker":"B","text":"本当ですか？何かの間違いではありませんか？"},{"id":{"d":390,"n":3,"t":3},"speaker":"A","text":"間違いではありません。ツアーだと安くなるのかもしれませんよ。"},{"id":{"d":390,"n":4,"t":3},"speaker":"B","text":"片道の飛行機代よりも往復の飛行機代とホテル代を足したものが安くなるということですよね？にわかには信じられません。"},{"id":{"d":390,"n":5,"t":3},"speaker":"A","text":"ツアーとはそのようなものなのではないのでしょうか？"},{"id":{"d":390,"n":6,"t":3},"speaker":"B","text":"確かに、ツアーは比較的安くなるものですが、そこまで安くなるものでしょうか？もしかして、ホテルがとても安いホテルということはありませんか？"},{"id":{"d":390,"n":7,"t":3},"speaker":"A","text":"安いホテルですが、私は泊まることさえできればホテルはそれほどこだわりません。"},{"id":{"d":390,"n":8,"t":3},"speaker":"B","text":"それもそうですね。寝る場所さえあれば、ご飯などは自分で買ってくればよいですからね。"}];
INSERT INTO sentence [{"id":{"d":391,"n":1,"t":3},"speaker":"A","text":"屋形船に乗ったことはありますか？"},{"id":{"d":391,"n":2,"t":3},"speaker":"B","text":"一度もありません。船に乗っている人が手を振っているのはジョギング中によく見かけますが。"},{"id":{"d":391,"n":3,"t":3},"speaker":"A","text":"私も乗ったことがありませんので、一度乗ってみたいと考えています。"},{"id":{"d":391,"n":4,"t":3},"speaker":"B","text":"屋形船にはどこで乗ることができるのですか？"},{"id":{"d":391,"n":5,"t":3},"speaker":"A","text":"ある程度大きな川では屋形船に乗ることができる印象です。少し調べたところ江戸川でも乗れるようです。"},{"id":{"d":391,"n":6,"t":3},"speaker":"B","text":"本当ですね。今は屋形船女子会というものもあるのですね。"},{"id":{"d":391,"n":7,"t":3},"speaker":"A","text":"一般の宴会コースもありますよ。"},{"id":{"d":391,"n":8,"t":3},"speaker":"B","text":"良さそうですね。この宴会コースに参加しましょう。"}];
INSERT INTO sentence [{"id":{"d":392,"n":1,"t":3},"speaker":"A","text":"軽自動車に乗ったことはありますか？"},{"id":{"d":392,"n":2,"t":3},"speaker":"B","text":"助手席に乗ったことはありますが、運転したことはありません。"},{"id":{"d":392,"n":3,"t":3},"speaker":"A","text":"軽自動車でどのような道を走りましたか？"},{"id":{"d":392,"n":4,"t":3},"speaker":"B","text":"一般道を走りました。橋本さんは軽自動車に乗ったことはありますか？"},{"id":{"d":392,"n":5,"t":3},"speaker":"A","text":"はい。私は運転をしたことがあります。"},{"id":{"d":392,"n":6,"t":3},"speaker":"B","text":"運転してみてどうでしたか？感想をぜひ教えてください。"},{"id":{"d":392,"n":7,"t":3},"speaker":"A","text":"レンタカーだったので使い込まれている車だったのだと思いますが、山道を上るためにアクセルを踏み込んでも時速40キロメートルしか出ませんでした。"},{"id":{"d":392,"n":8,"t":3},"speaker":"B","text":"それは馬力が弱過ぎますね。無事に上ることはできたのですか？"},{"id":{"d":392,"n":9,"t":3},"speaker":"A","text":"普通車よりも時間はかかりましたが一応上ることはできました。"}];
INSERT INTO sentence [{"id":{"d":393,"n":1,"t":3},"speaker":"A","text":"対向車が怖くて、一方通行ではない道路を運転することができません。"},{"id":{"d":393,"n":2,"t":3},"speaker":"B","text":"対向車の何が怖いのですか？"},{"id":{"d":393,"n":3,"t":3},"speaker":"A","text":"ミラーの部分がぶつかるのではないかと不安になってしまいます。"},{"id":{"d":393,"n":4,"t":3},"speaker":"B","text":"しっかり車道の範囲内で走っていたら何も問題はありませんよ。"},{"id":{"d":393,"n":5,"t":3},"speaker":"A","text":"しかし、もしお互いにセンターラインの近くを走っていたらぶつかりませんか？"},{"id":{"d":393,"n":6,"t":3},"speaker":"B","text":"大丈夫ですよ。もしそのことが怖いのであれば、センターライン側をできるだけ開けて運転するといいですよ。"},{"id":{"d":393,"n":7,"t":3},"speaker":"A","text":"それは思いつきませんでした。これからそのようにして運転してみます。"},{"id":{"d":393,"n":8,"t":3},"speaker":"B","text":"少し変な運転になると思いますが、不安は減ると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":394,"n":1,"t":3},"speaker":"A","text":"今日はどこに行きましょうか？"},{"id":{"d":394,"n":2,"t":3},"speaker":"B","text":"先週は水族館に行ったので動物園に行きたいです。"},{"id":{"d":394,"n":3,"t":3},"speaker":"A","text":"どこの動物園に行きたいですか？"},{"id":{"d":394,"n":4,"t":3},"speaker":"B","text":"上野動物園に行きたいです。私はパンダが大好きなので、パンダを見に行きたいです。"},{"id":{"d":394,"n":5,"t":3},"speaker":"A","text":"そういえば、先週、上野動物園の近くに行きましたが、休園しているようでしたよ。"},{"id":{"d":394,"n":6,"t":3},"speaker":"B","text":"なぜ休園しているのでしょうか？感染症の影響でしょうか？"},{"id":{"d":394,"n":7,"t":3},"speaker":"A","text":"感染症対策だと思います。他の動物園も休園しているかもしれませんね。"},{"id":{"d":394,"n":8,"t":3},"speaker":"B","text":"それは残念です。では、感染症の流行がもう少し収まってから行きましょう。"}];
INSERT INTO sentence [{"id":{"d":395,"n":1,"t":3},"speaker":"A","text":"飛行機ではどこの席を指定することが多いですか？"},{"id":{"d":395,"n":2,"t":3},"speaker":"B","text":"私は席を指定することはほとんどありませんが、指定する場合には窓際を選択することが多いです。"},{"id":{"d":395,"n":3,"t":3},"speaker":"A","text":"やはり景色を見られる方がいいですか？"},{"id":{"d":395,"n":4,"t":3},"speaker":"B","text":"景色を見ることももちろん大事ですが、通路側だと自分の足が客室乗務員の移動に邪魔になるのではないかと思ってしまい、くつろぐことができません。"},{"id":{"d":395,"n":5,"t":3},"speaker":"A","text":"確かにどこに足を置いてよいか分かりませんね。"},{"id":{"d":395,"n":6,"t":3},"speaker":"B","text":"はい、窓際はその心配がないので楽です。ただし、窓際は出発する前にしっかりとトイレを済ませておく必要があります。"},{"id":{"d":395,"n":7,"t":3},"speaker":"A","text":"そうですね。窓際からトイレに行くのは他の人に気を使わせてしまいますからね。"},{"id":{"d":395,"n":8,"t":3},"speaker":"B","text":"はい。人に気を使わせてしまうのはできるだけ避けたいものです。"}];
INSERT INTO sentence [{"id":{"d":396,"n":1,"t":3},"speaker":"A","text":"深夜バスに乗ったことはありますか？"},{"id":{"d":396,"n":2,"t":3},"speaker":"B","text":"1度だけあります。祖父母の家の近くから乗りました。"},{"id":{"d":396,"n":3,"t":3},"speaker":"A","text":"どれくらいの時間乗っていましたか？"},{"id":{"d":396,"n":4,"t":3},"speaker":"B","text":"小さい時に乗ったのであまり正確には覚えていませんが、確か6、7時間ほど乗っていた気がします。"},{"id":{"d":396,"n":5,"t":3},"speaker":"A","text":"バスではよく眠ることはできましたか？"},{"id":{"d":396,"n":6,"t":3},"speaker":"B","text":"あまり眠ることはできませんでした。乗り心地は悪くありませんでしたが、初めての体験に興奮してしまい、なかなか寝付けませんでした。"},{"id":{"d":396,"n":7,"t":3},"speaker":"A","text":"確かに、普段乗らないものに乗ると、楽しくて眠ることができませんね。"},{"id":{"d":396,"n":8,"t":3},"speaker":"B","text":"はい。朝までずっとゲームをしていた記憶があります。"}];
INSERT INTO sentence [{"id":{"d":397,"n":1,"t":3},"speaker":"A","text":"最近、韓国の食べ物や化粧品が日本で流行していますね。"},{"id":{"d":397,"n":2,"t":3},"speaker":"B","text":"そうですね。食べ物や化粧品以外にも、韓国のドラマやアーティストなども人気があるようですね。"},{"id":{"d":397,"n":3,"t":3},"speaker":"A","text":"はい。それらの影響もあり、私もいつか韓国に旅行に行きたいと思っています。"},{"id":{"d":397,"n":4,"t":3},"speaker":"B","text":"本場に行ってみたいということですか？"},{"id":{"d":397,"n":5,"t":3},"speaker":"A","text":"そうです。ただ、韓国語が全く分かりませんので、旅行先で困るのではないかと不安です。"},{"id":{"d":397,"n":6,"t":3},"speaker":"B","text":"ハングル文字も難しそうですね。せっかくですから、良い機会だと捉えて旅行に必要そうな表現を勉強してみてはどうですか？"},{"id":{"d":397,"n":7,"t":3},"speaker":"A","text":"私も、旅行の計画の前に韓国語の勉強を始めたいと思っているのですが、まだ実行できていません。"},{"id":{"d":397,"n":8,"t":3},"speaker":"B","text":"今はオンラインで外国語を勉強できるサイトもありますよ。"},{"id":{"d":397,"n":9,"t":3},"speaker":"A","text":"それなら気軽に始められそうです。旅行用の韓国語を学べるサイトがあるかどうか調べてみます。"}];
INSERT INTO sentence [{"id":{"d":398,"n":1,"t":3},"speaker":"A","text":"ロープウエー乗り場はどこですか？"},{"id":{"d":398,"n":2,"t":3},"speaker":"B","text":"ここからは歩いて行けませんから、バスに乗ってください。"},{"id":{"d":398,"n":3,"t":3},"speaker":"A","text":"バス乗り場はここから近いのでしょうか？"},{"id":{"d":398,"n":4,"t":3},"speaker":"B","text":"はい。この階段を下りて、3番のバス停です。"},{"id":{"d":398,"n":5,"t":3},"speaker":"A","text":"終点まで行けばよいのですか？"},{"id":{"d":398,"n":6,"t":3},"speaker":"B","text":"そうです。終点がロープウエーの駅です。"},{"id":{"d":398,"n":7,"t":3},"speaker":"A","text":"山の上は寒いでしょうか？"},{"id":{"d":398,"n":8,"t":3},"speaker":"B","text":"今日の天気なら、それほど寒くはないと思いますよ。"},{"id":{"d":398,"n":9,"t":3},"speaker":"A","text":"それはよかったです。どうもありがとうございました。"},{"id":{"d":398,"n":10,"t":3},"speaker":"B","text":"どういたしまして。お気を付けて。"}];
INSERT INTO sentence [{"id":{"d":399,"n":1,"t":3},"speaker":"A","text":"宇治に来たからには、お茶のお菓子を食べなくてはいけませんね。"},{"id":{"d":399,"n":2,"t":3},"speaker":"B","text":"ただのお茶ではなくて、お茶のお菓子なのですか？"},{"id":{"d":399,"n":3,"t":3},"speaker":"A","text":"お茶でもよいですが、私は甘いものの方が好きなのです。"},{"id":{"d":399,"n":4,"t":3},"speaker":"B","text":"そうですか。どうしても食べたいというものはありますか？"},{"id":{"d":399,"n":5,"t":3},"speaker":"A","text":"特にこれというものはありません。時間が許す限り片端から食べるつもりでした。"},{"id":{"d":399,"n":6,"t":3},"speaker":"B","text":"そうですか。私はそこまで甘いものが得意ではありませんので、別行動にしませんか？"},{"id":{"d":399,"n":7,"t":3},"speaker":"A","text":"そのようなことは言わずに、1軒くらい付き合ってくださいよ。"},{"id":{"d":399,"n":8,"t":3},"speaker":"B","text":"分かりました。1軒だけですよ。"}];
INSERT INTO sentence [{"id":{"d":400,"n":1,"t":3},"speaker":"A","text":"こちらが、旅行のお土産です。"},{"id":{"d":400,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。これは何ですか？"},{"id":{"d":400,"n":3,"t":3},"speaker":"A","text":"これは「さるぼぼ」と言って飛騨高山の民芸品ですよ。"},{"id":{"d":400,"n":4,"t":3},"speaker":"B","text":"「さる」はおサルさんのことだと思いますが、「ぼぼ」とは何のことですか？"},{"id":{"d":400,"n":5,"t":3},"speaker":"A","text":"「ぼぼ」は「赤ちゃん」ということらしいです。"},{"id":{"d":400,"n":6,"t":3},"speaker":"B","text":"では、「さるぼぼ」はおサルさんの赤ちゃんなのですね。"},{"id":{"d":400,"n":7,"t":3},"speaker":"A","text":"はい。初めは子どもの成長を祈って作られたのですが、今では人々の幸せの象徴になっているそうです。"},{"id":{"d":400,"n":8,"t":3},"speaker":"B","text":"幸せの象徴ですか。すてきですね。大切にします。"}];
INSERT INTO sentence [{"id":{"d":401,"n":1,"t":3},"speaker":"A","text":"飛騨高山には何があるのですか？"},{"id":{"d":401,"n":2,"t":3},"speaker":"B","text":"飛騨高山は古い城下町なので、町並みがきれいですよ。私も町並みを楽しんできました。"},{"id":{"d":401,"n":3,"t":3},"speaker":"A","text":"どれくらい古い城下町なのですか？"},{"id":{"d":401,"n":4,"t":3},"speaker":"B","text":"江戸時代の城下町だそうです。"},{"id":{"d":401,"n":5,"t":3},"speaker":"A","text":"そうですか。ところで、飛騨高山では何かおいしいものを食べてきましたか？"},{"id":{"d":401,"n":6,"t":3},"speaker":"B","text":"飛騨牛と高山ラーメンを食べてきました。"},{"id":{"d":401,"n":7,"t":3},"speaker":"A","text":"飛騨牛は私も聞いたことがあります。"},{"id":{"d":401,"n":8,"t":3},"speaker":"B","text":"飛騨地方で飼育されている高級黒毛和牛です。飛騨牛のステーキは最高でした。"}];
INSERT INTO sentence [{"id":{"d":402,"n":1,"t":3},"speaker":"A","text":"京都にお花見に行きたいのですが、今年の見頃はいつでしょうか？"},{"id":{"d":402,"n":2,"t":3},"speaker":"B","text":"例年なら4月の前半頃です。ただ、サクラの開花は毎年まちまちなので、今年はどうでしょうね。"},{"id":{"d":402,"n":3,"t":3},"speaker":"A","text":"今年も例年どおりなら、4月の初め頃に京都へ行くのはどうでしょうか？"},{"id":{"d":402,"n":4,"t":3},"speaker":"B","text":"今年は暖冬ですから、例年より少し開花が早いかもしれません。"},{"id":{"d":402,"n":5,"t":3},"speaker":"A","text":"では、3月の終わり頃でしょうか？"},{"id":{"d":402,"n":6,"t":3},"speaker":"B","text":"そのくらいがいいのではないかと思いますが、こればかりはサクラに聞いてみないと分かりませんね。"},{"id":{"d":402,"n":7,"t":3},"speaker":"A","text":"分かりました。サクラさん、今年の開花はいつですか？"},{"id":{"d":402,"n":8,"t":3},"speaker":"B","text":"まさか本当に聞いてみるとは思いませんでした。"}];
INSERT INTO sentence [{"id":{"d":403,"n":1,"t":3},"speaker":"A","text":"宿泊の予約をお願いします。"},{"id":{"d":403,"n":2,"t":3},"speaker":"B","text":"はい、承ります。ご宿泊のお日にちと、人数を教えていただけますか？"},{"id":{"d":403,"n":3,"t":3},"speaker":"A","text":"11月3日から1泊、大人3人でお願いします。"},{"id":{"d":403,"n":4,"t":3},"speaker":"B","text":"ご希望のお部屋タイプはございますか？"},{"id":{"d":403,"n":5,"t":3},"speaker":"A","text":"ファミリータイプは空いていますか？"},{"id":{"d":403,"n":6,"t":3},"speaker":"B","text":"はい、ご利用いただけます。"},{"id":{"d":403,"n":7,"t":3},"speaker":"A","text":"よかったです。ではファミリータイプでお願いします。"},{"id":{"d":403,"n":8,"t":3},"speaker":"B","text":"かしこまりました。お客さまのお名前とご連絡先をお伺いいたします。"}];
INSERT INTO sentence [{"id":{"d":404,"n":1,"t":3},"speaker":"A","text":"お母さん、あそこに魚がたくさんいます。"},{"id":{"d":404,"n":2,"t":3},"speaker":"B","text":"あれはイワシという魚ですよ。"},{"id":{"d":404,"n":3,"t":3},"speaker":"A","text":"昨日僕たちが食べたのもイワシではありませんでしたか？"},{"id":{"d":404,"n":4,"t":3},"speaker":"B","text":"はい、そうです。夕食はイワシのつみれ汁でしたね。"},{"id":{"d":404,"n":5,"t":3},"speaker":"A","text":"あの魚は夕食のものと違って丸くありませんね。"},{"id":{"d":404,"n":6,"t":3},"speaker":"B","text":"昨日のものは、つみれとして調理したので丸い形になっていたのです。調理する前は、まさにこのような姿でしたよ。"}];
INSERT INTO sentence [{"id":{"d":405,"n":1,"t":3},"speaker":"A","text":"今度の休みに、SLに乗りに行きませんか？"},{"id":{"d":405,"n":2,"t":3},"speaker":"B","text":"ぜひ行きたいです。鉄道博物館へ行くのですか？"},{"id":{"d":405,"n":3,"t":3},"speaker":"A","text":"いえ、鉄道博物館へ行くのではありませんよ。"},{"id":{"d":405,"n":4,"t":3},"speaker":"B","text":"もしかして、本物のSLに乗れるのですか？今もSLが走っている地域があるのですか？"},{"id":{"d":405,"n":5,"t":3},"speaker":"A","text":"はい。日本国内には数カ所だけですが、今もSLが走っている所があります。"},{"id":{"d":405,"n":6,"t":3},"speaker":"B","text":"そうだったのですね。その地域ではSLで通勤する人もいるのですか？"},{"id":{"d":405,"n":7,"t":3},"speaker":"A","text":"いいえ、SLは観光列車です。"}];
INSERT INTO sentence [{"id":{"d":406,"n":1,"t":3},"speaker":"A","text":"このお菓子は北海道の名産品ですか？"},{"id":{"d":406,"n":2,"t":3},"speaker":"B","text":"はい、そうです。人気のある商品です。"},{"id":{"d":406,"n":3,"t":3},"speaker":"A","text":"このハスカップというのは何ですか？"},{"id":{"d":406,"n":4,"t":3},"speaker":"B","text":"ハスカップは果物です。このお菓子にはジャムにしたものが入っています。"},{"id":{"d":406,"n":5,"t":3},"speaker":"A","text":"なるほど。生のハスカップも食べてみたいのですが、こちらのお店に置いてありますか？"},{"id":{"d":406,"n":6,"t":3},"speaker":"B","text":"当店では扱っておりません。生のハスカップはすぐに傷んでしまうので、扱っているお店はほとんどないと思います。"},{"id":{"d":406,"n":7,"t":3},"speaker":"A","text":"そうですか。それで、お菓子に加工しているのですね。"},{"id":{"d":406,"n":8,"t":3},"speaker":"B","text":"ハスカップのお菓子はどれもおいしいですよ。いかがですか？"}];
INSERT INTO sentence [{"id":{"d":407,"n":1,"t":3},"speaker":"A","text":"あそこから観光船が出ていますね。"},{"id":{"d":407,"n":2,"t":3},"speaker":"B","text":"乗りたいです。"},{"id":{"d":407,"n":3,"t":3},"speaker":"A","text":"乗りましょう。チケット売り場はどこでしょうか？"},{"id":{"d":407,"n":4,"t":3},"speaker":"B","text":"道路の向こうの雑貨屋さんで、チケットを購入できるようです。あそこに看板が出ています。"},{"id":{"d":407,"n":5,"t":3},"speaker":"A","text":"本当ですね。私が買ってきます。"},{"id":{"d":407,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。では、私は飲み物を買って、乗り場で待っていますね。"}];
INSERT INTO sentence [{"id":{"d":408,"n":1,"t":3},"speaker":"A","text":"お昼ご飯は讃岐うどんにしませんか？"},{"id":{"d":408,"n":2,"t":3},"speaker":"B","text":"いいですね。ちょうどあちらに讃岐うどんのお店がありますよ。"},{"id":{"d":408,"n":3,"t":3},"speaker":"A","text":"行ってみましょう。"},{"id":{"d":408,"n":4,"t":3},"speaker":"B","text":"お店の前の看板には、自分でつゆをかけられると書かれていました。面白そうですね。"},{"id":{"d":408,"n":5,"t":3},"speaker":"A","text":"そうですね。私もぜひやってみたいです。"},{"id":{"d":408,"n":6,"t":3},"speaker":"B","text":"でも、やり方がよく分かりません。"},{"id":{"d":408,"n":7,"t":3},"speaker":"A","text":"地元の人に聞いてみましょう。"}];
INSERT INTO sentence [{"id":{"d":409,"n":1,"t":3},"speaker":"A","text":"卒業旅行が楽しみですね。"},{"id":{"d":409,"n":2,"t":3},"speaker":"B","text":"はい。楽しみすぎて、旅のしおりを作ってきました。"},{"id":{"d":409,"n":3,"t":3},"speaker":"A","text":"まあ、すてきなしおりです。まるで修学旅行のようですね。"},{"id":{"d":409,"n":4,"t":3},"speaker":"B","text":"力作ですから、ぜひ見てください。"},{"id":{"d":409,"n":5,"t":3},"speaker":"A","text":"何ページあるのですか？"},{"id":{"d":409,"n":6,"t":3},"speaker":"B","text":"表紙と裏表紙も合わせて32ページあります。"},{"id":{"d":409,"n":7,"t":3},"speaker":"A","text":"表紙のイラストも手が込んでいますね。"},{"id":{"d":409,"n":8,"t":3},"speaker":"B","text":"描くのに2日かかりました。"}];
INSERT INTO sentence [{"id":{"d":410,"n":1,"t":3},"speaker":"A","text":"あなたが計画してくれた旅程には、少し問題があるようです。"},{"id":{"d":410,"n":2,"t":3},"speaker":"B","text":"どこが問題でしょうか？"},{"id":{"d":410,"n":3,"t":3},"speaker":"A","text":"2泊3日の旅行にこのスケジュールは詰め込みすぎだと思います。"},{"id":{"d":410,"n":4,"t":3},"speaker":"B","text":"そうでしょうか？"},{"id":{"d":410,"n":5,"t":3},"speaker":"A","text":"1日に観光スポットを5箇所も回るのは難しいのではないでしょうか？"},{"id":{"d":410,"n":6,"t":3},"speaker":"B","text":"そうですか？私は可能だと思いましたが。"},{"id":{"d":410,"n":7,"t":3},"speaker":"A","text":"ここに書かれているような分刻みのスケジュールで動けば可能かもしれませんが、私はキャリーバッグを引きずって駆け回るのは嫌です。"},{"id":{"d":410,"n":8,"t":3},"speaker":"B","text":"分かりました。では、もう一度考え直してみます。"}];
INSERT INTO sentence [{"id":{"d":411,"n":1,"t":3},"speaker":"A","text":"せっかくの卒業旅行なので、もう少しアクティビティーを入れませんか？"},{"id":{"d":411,"n":2,"t":3},"speaker":"B","text":"少しだけなら構いませんが、あまり疲れることはしたくありません。"},{"id":{"d":411,"n":3,"t":3},"speaker":"A","text":"旅館の近くにパラセーリングができる場所があるようですよ。これに挑戦しませんか？"},{"id":{"d":411,"n":4,"t":3},"speaker":"B","text":"パラセーリングは少しハード過ぎます。もう少しお手軽なものはありませんか？"},{"id":{"d":411,"n":5,"t":3},"speaker":"A","text":"パラセーリングも十分お手軽だと思います。そもそも、あなたはどうしてそれほど疲れているのですか？"},{"id":{"d":411,"n":6,"t":3},"speaker":"B","text":"何日も徹夜をして卒論を書き終えたばかりなのですよ。"},{"id":{"d":411,"n":7,"t":3},"speaker":"A","text":"それでぐったりしているのですね。では旅行1日目は旅館でゆっくりしましょう。"},{"id":{"d":411,"n":8,"t":3},"speaker":"B","text":"そうしてもらえるとありがたいです。温泉や館内の施設を楽しみましょう。"}];
INSERT INTO sentence [{"id":{"d":412,"n":1,"t":3},"speaker":"A","text":"私たちは、実は、旅の好みが合いませんよね。"},{"id":{"d":412,"n":2,"t":3},"speaker":"B","text":"あなたはホテルでゆっくりしたい派で、私は活発に動き回りたい派なので、確かに好みは真逆ですね。"},{"id":{"d":412,"n":3,"t":3},"speaker":"A","text":"はい。あなたと仲の良い友人であることが、時々、奇跡のように思えます。"},{"id":{"d":412,"n":4,"t":3},"speaker":"B","text":"本当にそうですね。さて、今回の旅はどうしますか？"},{"id":{"d":412,"n":5,"t":3},"speaker":"A","text":"初日はホテルでゆっくりしたいです。もし、アクティビティーをするのであれば、2日目以降にしませんか？"},{"id":{"d":412,"n":6,"t":3},"speaker":"B","text":"そうしましょう。アクティビティーの内容は私が決めて良いですか？"},{"id":{"d":412,"n":7,"t":3},"speaker":"A","text":"もちろんです。あなたが興味のあるアクティビティーを体験しましょう。"},{"id":{"d":412,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。最近気になっているものがいくつかあるので、何を選ぼうか迷います。"}];
INSERT INTO sentence [{"id":{"d":413,"n":1,"t":3},"speaker":"A","text":"明日の待ち合わせは、羽田空港でよいですか？"},{"id":{"d":413,"n":2,"t":3},"speaker":"B","text":"はい。時間に余裕を持って来てくださいね。"},{"id":{"d":413,"n":3,"t":3},"speaker":"A","text":"ちょうどいいバスがあるので、1時間前には着くはずです。"},{"id":{"d":413,"n":4,"t":3},"speaker":"B","text":"それなら安心です。私はモノレールで行きますね。"},{"id":{"d":413,"n":5,"t":3},"speaker":"A","text":"分かりました。待ち合わせ場所は、第一ターミナルの太陽の塔にしましょう。"},{"id":{"d":413,"n":6,"t":3},"speaker":"B","text":"太陽の塔ですね。"},{"id":{"d":413,"n":7,"t":3},"speaker":"A","text":"はい。月の塔と間違えないでくださいね。"},{"id":{"d":413,"n":8,"t":3},"speaker":"B","text":"気を付けます。"}];
INSERT INTO sentence [{"id":{"d":414,"n":1,"t":3},"speaker":"A","text":"来年の夏に向けて、日本の世界遺産を巡る旅を計画しています。"},{"id":{"d":414,"n":2,"t":3},"speaker":"B","text":"面白そうですが、全部を一度に回ることはできませんよね？"},{"id":{"d":414,"n":3,"t":3},"speaker":"A","text":"そうですね。長い休みも取れませんから、10年かけて少しずつ巡る計画です。"},{"id":{"d":414,"n":4,"t":3},"speaker":"B","text":"気が長い話ですね。まずは、どこから行くのですか？"},{"id":{"d":414,"n":5,"t":3},"speaker":"A","text":"迷っています。あ、もしあなたが興味のある場所があれば一緒に行きませんか？"},{"id":{"d":414,"n":6,"t":3},"speaker":"B","text":"私が興味のある場所でよいのですか？"},{"id":{"d":414,"n":7,"t":3},"speaker":"A","text":"はい。私一人で悩んでいてはいつまでも決まらなさそうなので、あなたが行きたい場所を旅の始点にしようかと思いまして。"},{"id":{"d":414,"n":8,"t":3},"speaker":"B","text":"それでは、私は白川郷に行きたいです。"}];
INSERT INTO sentence [{"id":{"d":415,"n":1,"t":3},"speaker":"A","text":"白川郷の建築は素晴らしいですね。"},{"id":{"d":415,"n":2,"t":3},"speaker":"B","text":"はい。これらの建築様式は、合掌造りと呼ばれるものです。"},{"id":{"d":415,"n":3,"t":3},"speaker":"A","text":"合唱コンクールの合唱ですか？"},{"id":{"d":415,"n":4,"t":3},"speaker":"B","text":"いいえ。手を合わせて拝む方の合掌です。"},{"id":{"d":415,"n":5,"t":3},"speaker":"A","text":"どうして合掌というのですか？"},{"id":{"d":415,"n":6,"t":3},"speaker":"B","text":"正面から見ると屋根の形が手を合わせているように見えるからです。"},{"id":{"d":415,"n":7,"t":3},"speaker":"A","text":"言われてみればそうですね。"},{"id":{"d":415,"n":8,"t":3},"speaker":"B","text":"建物の中が見学できる所もあるそうなので、行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":416,"n":1,"t":3},"speaker":"A","text":"小布施の栗おこわはおいしいですね。"},{"id":{"d":416,"n":2,"t":3},"speaker":"B","text":"はい、とてもおいしいです。デザートにはモンブランを食べましょう。"},{"id":{"d":416,"n":3,"t":3},"speaker":"A","text":"おなかがいっぱいですが、デザートは別腹に入りそうですね。"},{"id":{"d":416,"n":4,"t":3},"speaker":"B","text":"はい。"},{"id":{"d":416,"n":5,"t":3},"speaker":"A","text":"モンブランも捨て難いですが、こちらの栗のアイスクリームもおいしそうですよ。"},{"id":{"d":416,"n":6,"t":3},"speaker":"B","text":"モンブランとアイスクリームのどちらにするか迷います。"},{"id":{"d":416,"n":7,"t":3},"speaker":"A","text":"良いことを思いつきました。どちらも食べればよいのではありませんか？"},{"id":{"d":416,"n":8,"t":3},"speaker":"B","text":"なるほど、その手がありましたか。名案ですね。"}];
INSERT INTO sentence [{"id":{"d":417,"n":1,"t":3},"speaker":"A","text":"栗菓子のお土産を買いたいです。"},{"id":{"d":417,"n":2,"t":3},"speaker":"B","text":"隣の売店にいろいろあるようですよ。"},{"id":{"d":417,"n":3,"t":3},"speaker":"A","text":"たくさんありますね。では、栗ようかんと、栗かのこと、栗最中と、栗らくがんと…。"},{"id":{"d":417,"n":4,"t":3},"speaker":"B","text":"まだ買うのですか？"},{"id":{"d":417,"n":5,"t":3},"speaker":"A","text":"はい。全部おいしそうなので、お財布が許す限り買って帰ろうと思います。"},{"id":{"d":417,"n":6,"t":3},"speaker":"B","text":"それほどまでに栗が好きなのですね。"},{"id":{"d":417,"n":7,"t":3},"speaker":"A","text":"はい、大好きです。"},{"id":{"d":417,"n":8,"t":3},"speaker":"B","text":"私は店の外で待っていますので、ごゆっくりお買い物をどうぞ。"},{"id":{"d":417,"n":9,"t":3},"speaker":"A","text":"お待たせしました。お会計してきました。"},{"id":{"d":417,"n":10,"t":3},"speaker":"B","text":"お土産がとても重そうですね。半分持ちますよ。"}];
INSERT INTO sentence [{"id":{"d":418,"n":1,"t":3},"speaker":"A","text":"おいしいものを食べに北海道へ行ってきました。"},{"id":{"d":418,"n":2,"t":3},"speaker":"B","text":"この時期のおいしいものと言えば、カニやホタテでしょうか？"},{"id":{"d":418,"n":3,"t":3},"speaker":"A","text":"はい。海鮮ももちろん食べました。それから、札幌ラーメンとジンギスカン料理とチーズとソフトクリームと…"},{"id":{"d":418,"n":4,"t":3},"speaker":"B","text":"まだあるのですか？たくさん食べましたね。"},{"id":{"d":418,"n":5,"t":3},"speaker":"A","text":"はい。大満足です。そういえば、あなたにお土産を買ってきましたよ。"},{"id":{"d":418,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。何かおいしいものを買ってきてくださったのですか？"},{"id":{"d":418,"n":7,"t":3},"speaker":"A","text":"いいえ、お土産はマリモです。はい、どうぞ。"},{"id":{"d":418,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。でも、マリモは食べられないではありませんか。おいしいものを期待していたので少し残念です。"}];
INSERT INTO sentence [{"id":{"d":419,"n":1,"t":3},"speaker":"A","text":"温泉がとても気持ち良かったですね。"},{"id":{"d":419,"n":2,"t":3},"speaker":"B","text":"はい、とても良いお湯でした。お風呂の中でお酒が飲めるなんて知りませんでしたよ。"},{"id":{"d":419,"n":3,"t":3},"speaker":"A","text":"この旅館ならではのサービスです。すてきですよね。"},{"id":{"d":419,"n":4,"t":3},"speaker":"B","text":"温泉でゆっくり温まりながら日本酒をくいっと飲んで、幸せな時間でした。"},{"id":{"d":419,"n":5,"t":3},"speaker":"A","text":"そうですね。体も温まったことですし、この後は温泉街を歩きませんか？"},{"id":{"d":419,"n":6,"t":3},"speaker":"B","text":"いいですね。酔い覚ましに歩くのもいいかもしれません。"},{"id":{"d":419,"n":7,"t":3},"speaker":"A","text":"近くに良い酒蔵があるのです。そこへ行きましょう。"},{"id":{"d":419,"n":8,"t":3},"speaker":"B","text":"まさか、まだ飲むのですか？"},{"id":{"d":419,"n":9,"t":3},"speaker":"A","text":"ええ、もちろんそのつもりです。"}];
INSERT INTO sentence [{"id":{"d":420,"n":1,"t":3},"speaker":"A","text":"こんにちは。子ども用のSuicaを1枚ください。"},{"id":{"d":420,"n":2,"t":3},"speaker":"B","text":"こんにちは。偉いですね、自分で買いに来たのですか？"},{"id":{"d":420,"n":3,"t":3},"speaker":"A","text":"はい。でも、お母さんはあそこにいます。"},{"id":{"d":420,"n":4,"t":3},"speaker":"B","text":"そうですか。Suicaの発行には、健康保険証と2,000円が必要です。"},{"id":{"d":420,"n":5,"t":3},"speaker":"A","text":"はい、これです。"},{"id":{"d":420,"n":6,"t":3},"speaker":"B","text":"お預かりいたします。お待たせしました。こちらが子ども用のSuicaとなります。"},{"id":{"d":420,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":420,"n":8,"t":3},"speaker":"B","text":"小学生の間はずっと使えますから大事にしてくださいね。"},{"id":{"d":420,"n":9,"t":3},"speaker":"A","text":"はい、大事にします。"},{"id":{"d":420,"n":10,"t":3},"speaker":"B","text":"この説明書はお母さんに渡してください。"},{"id":{"d":420,"n":11,"t":3},"speaker":"A","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":421,"n":1,"t":3},"speaker":"A","text":"昨日はヘリコプターに乗って東京の空を遊覧してきました。"},{"id":{"d":421,"n":2,"t":3},"speaker":"B","text":"それは貴重な体験をしましたね。いかがでしたか？"},{"id":{"d":421,"n":3,"t":3},"speaker":"A","text":"とても楽しかったです。空から見る東京の夜景はとてもきれいでした。"},{"id":{"d":421,"n":4,"t":3},"speaker":"B","text":"すてきな景色が見られたのですね。"},{"id":{"d":421,"n":5,"t":3},"speaker":"A","text":"はい。景色に見とれていたら、自分の膝に小箱が置かれていました。"},{"id":{"d":421,"n":6,"t":3},"speaker":"B","text":"何の話ですか？"},{"id":{"d":421,"n":7,"t":3},"speaker":"A","text":"もう少しだけ、話の続きを聞いてください。"},{"id":{"d":421,"n":8,"t":3},"speaker":"B","text":"分かりました。"},{"id":{"d":421,"n":9,"t":3},"speaker":"A","text":"その小箱には婚約指輪が入っていたのですよ。"},{"id":{"d":421,"n":10,"t":3},"speaker":"B","text":"プロポーズされたということですか？おめでとうございます！"}];
INSERT INTO sentence [{"id":{"d":422,"n":1,"t":3},"speaker":"A","text":"せっかく栃木に来たのですから、イチゴ狩りをしませんか？"},{"id":{"d":422,"n":2,"t":3},"speaker":"B","text":"いいですね。"},{"id":{"d":422,"n":3,"t":3},"speaker":"A","text":"ガイドブックによると、この近くに30分食べ放題の所があるそうです。"},{"id":{"d":422,"n":4,"t":3},"speaker":"B","text":"ここからは歩いて行けそうですか？"},{"id":{"d":422,"n":5,"t":3},"speaker":"A","text":"はい、恐らく徒歩10分くらいで着くと思います。"},{"id":{"d":422,"n":6,"t":3},"speaker":"B","text":"近いですね。行きましょう。"},{"id":{"d":422,"n":7,"t":3},"speaker":"A","text":"入場料は2,500円だそうですが、大丈夫ですか？"},{"id":{"d":422,"n":8,"t":3},"speaker":"B","text":"はい、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":423,"n":1,"t":3},"speaker":"A","text":"久しぶりにイチゴ狩りをしましたが、とても楽しかったです。"},{"id":{"d":423,"n":2,"t":3},"speaker":"B","text":"おなかがいっぱいになりましたね。"},{"id":{"d":423,"n":3,"t":3},"speaker":"A","text":"イチゴだけでおなかがいっぱいになったのは初めてです。"},{"id":{"d":423,"n":4,"t":3},"speaker":"B","text":"ところで、ここではイチゴ大福づくりの体験もできるそうですよ。体験していきますか？"},{"id":{"d":423,"n":5,"t":3},"speaker":"A","text":"興味がありますが、今はもうこれ以上何も食べることができません。"},{"id":{"d":423,"n":6,"t":3},"speaker":"B","text":"持ち帰りもできるようですよ。"},{"id":{"d":423,"n":7,"t":3},"speaker":"A","text":"そうでしたか。ではぜひやりましょう。"},{"id":{"d":423,"n":8,"t":3},"speaker":"B","text":"イチゴ大福は良いお土産になりますね。"}];
INSERT INTO sentence [{"id":{"d":424,"n":1,"t":3},"speaker":"A","text":"先週はUFJに行ってきました。"},{"id":{"d":424,"n":2,"t":3},"speaker":"B","text":"三菱UFJ銀行ですか？"},{"id":{"d":424,"n":3,"t":3},"speaker":"A","text":"違いますよ。大阪にあるテーマパークです。"},{"id":{"d":424,"n":4,"t":3},"speaker":"B","text":"もしかして、USJのことですか？"},{"id":{"d":424,"n":5,"t":3},"speaker":"A","text":"そうです。あれ、私は先ほど何と言いましたか？"},{"id":{"d":424,"n":6,"t":3},"speaker":"B","text":"UFJと言っていました。"},{"id":{"d":424,"n":7,"t":3},"speaker":"A","text":"うっかり言い間違えてしまいました。すみません。"},{"id":{"d":424,"n":8,"t":3},"speaker":"B","text":"よく似ているので仕方ありませんよ。USJは楽しめましたか？"},{"id":{"d":424,"n":9,"t":3},"speaker":"A","text":"はい。久しぶりに行ったのですが、とても楽しかったです。これはあなたへのお土産です。"},{"id":{"d":424,"n":10,"t":3},"speaker":"B","text":"わざわざありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":425,"n":1,"t":3},"speaker":"A","text":"USJのお土産です。どうぞ。"},{"id":{"d":425,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。開けてよいですか？"},{"id":{"d":425,"n":3,"t":3},"speaker":"A","text":"もちろんです。"},{"id":{"d":425,"n":4,"t":3},"speaker":"B","text":"これは映画『ジョーズ』に登場するサメのマスクですね。マスクの表面にサメの口が印刷されていています。"},{"id":{"d":425,"n":5,"t":3},"speaker":"A","text":"なかなかすてきなマスクでしょう？"},{"id":{"d":425,"n":6,"t":3},"speaker":"B","text":"面白いですね。でも、これはどこに着けていけばよいのでしょうか？"},{"id":{"d":425,"n":7,"t":3},"speaker":"A","text":"笑いで場を和ませたいところに、ぜひ着けていってください。"}];
INSERT INTO sentence [{"id":{"d":426,"n":1,"t":3},"speaker":"A","text":"たまにはハイキングもいいですね。"},{"id":{"d":426,"n":2,"t":3},"speaker":"B","text":"はい。山の空気は気持ちが良いです。"},{"id":{"d":426,"n":3,"t":3},"speaker":"A","text":"新緑が目にまぶしいこの季節は、特に気持ちが良いです。"},{"id":{"d":426,"n":4,"t":3},"speaker":"B","text":"だいぶ歩きましたし、この先の見晴らし台で休憩しませんか？"},{"id":{"d":426,"n":5,"t":3},"speaker":"A","text":"そうしましょう。"},{"id":{"d":426,"n":6,"t":3},"speaker":"B","text":"休憩の時のために、コーヒーセットを持ってきました。"},{"id":{"d":426,"n":7,"t":3},"speaker":"A","text":"コーヒーを山で入れるのですか？"},{"id":{"d":426,"n":8,"t":3},"speaker":"B","text":"はい。ぜいたくでしょう？"}];
INSERT INTO sentence [{"id":{"d":427,"n":1,"t":3},"speaker":"A","text":"雨が降りそうですね。"},{"id":{"d":427,"n":2,"t":3},"speaker":"B","text":"ええ。山の天気は本当に変わりやすいですね。"},{"id":{"d":427,"n":3,"t":3},"speaker":"A","text":"はい。ヤッケを着てきて正解でした。"},{"id":{"d":427,"n":4,"t":3},"speaker":"B","text":"事前に教えてもらいましたので、私もワークマンでヤッケを買ってきました。"},{"id":{"d":427,"n":5,"t":3},"speaker":"A","text":"ヤッケは防寒性も優れているので快適ですね。"},{"id":{"d":427,"n":6,"t":3},"speaker":"B","text":"はい。空気は冷たいですが、おかげで少しも寒くありません。"},{"id":{"d":427,"n":7,"t":3},"speaker":"A","text":"あと少しでケーブルカー乗り場に着きますよ。急ぎましょう。"},{"id":{"d":427,"n":8,"t":3},"speaker":"B","text":"雨が降らないうちに着きたいですね。"}];
INSERT INTO sentence [{"id":{"d":428,"n":1,"t":3},"speaker":"A","text":"ギョーザを食べに宇都宮へ行きたいです。"},{"id":{"d":428,"n":2,"t":3},"speaker":"B","text":"確かにギョーザは宇都宮の名物ですが、ここから宇都宮までは少し遠いですよ。"},{"id":{"d":428,"n":3,"t":3},"speaker":"A","text":"スーパーマーケットで売っているものではなく、たまには本場のギョーザが食べたいのです。"},{"id":{"d":428,"n":4,"t":3},"speaker":"B","text":"では、ナンジャ餃子スタジアムに行きませんか？"},{"id":{"d":428,"n":5,"t":3},"speaker":"A","text":"それは何ですか？"},{"id":{"d":428,"n":6,"t":3},"speaker":"B","text":"全国のおいしいギョーザが食べられるフードテーマパークです。"},{"id":{"d":428,"n":7,"t":3},"speaker":"A","text":"初めて知りました。どこにあるのですか？"},{"id":{"d":428,"n":8,"t":3},"speaker":"B","text":"池袋です。"},{"id":{"d":428,"n":9,"t":3},"speaker":"A","text":"ここからも近くていいですね。では、そこへ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":429,"n":1,"t":3},"speaker":"A","text":"ナンジャ餃子スタジアムへは初めて行きましたが、とても楽しかったです。"},{"id":{"d":429,"n":2,"t":3},"speaker":"B","text":"それはよかったです。たくさんギョーザを食べましたね。"},{"id":{"d":429,"n":3,"t":3},"speaker":"A","text":"はい。羽根つきギョーザが特においしかったです。"},{"id":{"d":429,"n":4,"t":3},"speaker":"B","text":"羽根つきギョーザは自分ではなかなかきれいに焼けませんからね。"},{"id":{"d":429,"n":5,"t":3},"speaker":"A","text":"あまりにもおいしかったので、自分でも焼いてみようと思ってお土産のギョーザを買ってきました。"},{"id":{"d":429,"n":6,"t":3},"speaker":"B","text":"では、しばらく家でもギョーザ三昧ですね。"},{"id":{"d":429,"n":7,"t":3},"speaker":"A","text":"良い所を教えてくれてありがとうございます。とても楽しかったです。"},{"id":{"d":429,"n":8,"t":3},"speaker":"B","text":"そう言っていただけて良かったです。また行きましょうね。"}];
INSERT INTO sentence [{"id":{"d":430,"n":1,"t":3},"speaker":"A","text":"北海道に着いたらレンタカーでドライブをする予定でしたね。"},{"id":{"d":430,"n":2,"t":3},"speaker":"B","text":"はい。北海道は広いですから、交代しながら運転しましょう。"},{"id":{"d":430,"n":3,"t":3},"speaker":"A","text":"そうしましょう。楽しみです。"},{"id":{"d":430,"n":4,"t":3},"speaker":"B","text":"レンタカーのカウンターはあちらですね。"},{"id":{"d":430,"n":5,"t":3},"speaker":"A","text":"あれ、運転免許証がありません。"},{"id":{"d":430,"n":6,"t":3},"speaker":"B","text":"もしかして、運転免許証を忘れてきたのですか？"},{"id":{"d":430,"n":7,"t":3},"speaker":"A","text":"はい、そのようです。すみません。"},{"id":{"d":430,"n":8,"t":3},"speaker":"B","text":"私1人で運転するのですか？"}];
INSERT INTO sentence [{"id":{"d":431,"n":1,"t":3},"speaker":"A","text":"すみません、タクシーを1台お願いします。"},{"id":{"d":431,"n":2,"t":3},"speaker":"B","text":"お電話ありがとうございます。本日は少々混んでおりまして、到着まで30分くらいかかりますがよろしいですか？"},{"id":{"d":431,"n":3,"t":3},"speaker":"A","text":"はい。それほど急いではいませんから待っています。"},{"id":{"d":431,"n":4,"t":3},"speaker":"B","text":"それではお名前とご住所、お電話番号をお願いします。"},{"id":{"d":431,"n":5,"t":3},"speaker":"A","text":"吉田です。山田町123、電話番号は044-123-3456です。"},{"id":{"d":431,"n":6,"t":3},"speaker":"B","text":"どちらまでお送りいたしましょうか？"},{"id":{"d":431,"n":7,"t":3},"speaker":"A","text":"イオンモールまでお願いします。"},{"id":{"d":431,"n":8,"t":3},"speaker":"B","text":"かしこまりました。では、約30分後にお伺いします。"},{"id":{"d":431,"n":9,"t":3},"speaker":"A","text":"はい。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":432,"n":1,"t":3},"speaker":"A","text":"神戸の異人館巡りは昔からの夢でした。"},{"id":{"d":432,"n":2,"t":3},"speaker":"B","text":"私もそうです。"},{"id":{"d":432,"n":3,"t":3},"speaker":"A","text":"でも、これほどまでに疲れるものだとは想像していませんでした。"},{"id":{"d":432,"n":4,"t":3},"speaker":"B","text":"これほどまでに坂道だらけだとは思いませんでしたね。"},{"id":{"d":432,"n":5,"t":3},"speaker":"A","text":"はい。でも、行けてよかったです。"},{"id":{"d":432,"n":6,"t":3},"speaker":"B","text":"そうですね。どこが一番印象に残っていますか？"},{"id":{"d":432,"n":7,"t":3},"speaker":"A","text":"私は風見鶏の館が一番良かったです。"},{"id":{"d":432,"n":8,"t":3},"speaker":"B","text":"外観がすてきでしたね。"},{"id":{"d":432,"n":9,"t":3},"speaker":"A","text":"ええ。とてもすてきだったので、写真をたくさん撮りました。"},{"id":{"d":432,"n":10,"t":3},"speaker":"B","text":"おかげでカメラの充電池が切れそうになってしまいました。"}];
INSERT INTO sentence [{"id":{"d":433,"n":1,"t":3},"speaker":"A","text":"午前中に訪れた英国館は、とてもすてきでしたね。"},{"id":{"d":433,"n":2,"t":3},"speaker":"B","text":"英国館は、シャーロック・ホームズの部屋があった所ですね。"},{"id":{"d":433,"n":3,"t":3},"speaker":"A","text":"はい。私は小説を全部読んでいる上、実写ドラマのファンでもあるので感動しました。"},{"id":{"d":433,"n":4,"t":3},"speaker":"B","text":"楽しかったですね。ホームズの衣装を着て写真を撮ることもできるなんて知りませんでした。"},{"id":{"d":433,"n":5,"t":3},"speaker":"A","text":"すてきな写真を撮ることができて、うれしかったです。写真の私はどうですか？"},{"id":{"d":433,"n":6,"t":3},"speaker":"B","text":"良い笑顔ですね。それから、ホームズになりきっているからか、なんだか頭が良さそうに見えます。"}];
INSERT INTO sentence [{"id":{"d":434,"n":1,"t":3},"speaker":"A","text":"旧ムーア邸は喫茶店になっていたのですね。"},{"id":{"d":434,"n":2,"t":3},"speaker":"B","text":"そのようですね。疲れた体に甘いものが染み入ります。"},{"id":{"d":434,"n":3,"t":3},"speaker":"A","text":"はい、至福の時間ですね。イチゴのミルフィーユを食べたのは久しぶりです。"},{"id":{"d":434,"n":4,"t":3},"speaker":"B","text":"イチゴとパイが崩れてしまって、なかなかきれいに食べられません。"},{"id":{"d":434,"n":5,"t":3},"speaker":"A","text":"私も上手には食べられません。でも、とてもおいしいです。"},{"id":{"d":434,"n":6,"t":3},"speaker":"B","text":"清水さん、クリームが頬に付いてしまっていますよ。"},{"id":{"d":434,"n":7,"t":3},"speaker":"A","text":"あら、本当ですね。"}];
INSERT INTO sentence [{"id":{"d":435,"n":1,"t":3},"speaker":"A","text":"神戸のお土産は老祥記の豚まんです。"},{"id":{"d":435,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。異人館巡りに行ったのですか？"},{"id":{"d":435,"n":3,"t":3},"speaker":"A","text":"そうですよ。"},{"id":{"d":435,"n":4,"t":3},"speaker":"B","text":"中華の異人館へ行ったのですか？"},{"id":{"d":435,"n":5,"t":3},"speaker":"A","text":"いえ、私が行ったのは西洋の異人館です。"},{"id":{"d":435,"n":6,"t":3},"speaker":"B","text":"では、どうして豚まんがお土産なのですか？"},{"id":{"d":435,"n":7,"t":3},"speaker":"A","text":"現地で食べたものの中で、豚まんが一番おいしかったからです。"},{"id":{"d":435,"n":8,"t":3},"speaker":"B","text":"なるほど。そういうことでしたか。"}];
INSERT INTO sentence [{"id":{"d":436,"n":1,"t":3},"speaker":"A","text":"お花見に行きましょう。"},{"id":{"d":436,"n":2,"t":3},"speaker":"B","text":"ソメイヨシノの開花予報はまだ出ていませんよ。"},{"id":{"d":436,"n":3,"t":3},"speaker":"A","text":"見に行くのは、ソメイヨシノではなくてカワヅザクラです。"},{"id":{"d":436,"n":4,"t":3},"speaker":"B","text":"名前の由来は、静岡の河津ですか？"},{"id":{"d":436,"n":5,"t":3},"speaker":"A","text":"そうです。でも、カワヅザクラは東京でも咲いています。"},{"id":{"d":436,"n":6,"t":3},"speaker":"B","text":"でも、今は2月ですよ。サクラが咲くにはまだ早いのではありませんか？"},{"id":{"d":436,"n":7,"t":3},"speaker":"A","text":"カワヅザクラは早咲きですから今が見ごろです。"},{"id":{"d":436,"n":8,"t":3},"speaker":"B","text":"そうなのですね。では、寒そうですがお花見に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":437,"n":1,"t":3},"speaker":"A","text":"今度の日曜日に横浜中華街へ行きませんか？"},{"id":{"d":437,"n":2,"t":3},"speaker":"B","text":"いいですね。久しぶりに中華が食べたいと思っていたところでした。"},{"id":{"d":437,"n":3,"t":3},"speaker":"A","text":"一番何を食べたいですか？"},{"id":{"d":437,"n":4,"t":3},"speaker":"B","text":"本格的な飲茶を食べたいです。"},{"id":{"d":437,"n":5,"t":3},"speaker":"A","text":"それなら良いお店を知っていますよ。"},{"id":{"d":437,"n":6,"t":3},"speaker":"B","text":"ではそこに行きましょう。予約してもらえますか？"},{"id":{"d":437,"n":7,"t":3},"speaker":"A","text":"もちろんです。お任せください。"},{"id":{"d":437,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。日曜日が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":438,"n":1,"t":3},"speaker":"A","text":"スペイン旅行に行ってきました。これは旅行で撮ってきた写真です。"},{"id":{"d":438,"n":2,"t":3},"speaker":"B","text":"写真をたくさん撮ってきたのですね。"},{"id":{"d":438,"n":3,"t":3},"speaker":"A","text":"デジカメだとついついたくさん撮ってしまいます。"},{"id":{"d":438,"n":4,"t":3},"speaker":"B","text":"どれもすてきな写真ですね。これはサグラダファミリアですか？"},{"id":{"d":438,"n":5,"t":3},"speaker":"A","text":"はい、そうです。工事中だったのですが、それでも見学ができて驚きました。"},{"id":{"d":438,"n":6,"t":3},"speaker":"B","text":"サグラダファミリアへは、私も10年ほど前に行ったことがあります。"},{"id":{"d":438,"n":7,"t":3},"speaker":"A","text":"そうだったのですね。その時とは変わっていますか？"},{"id":{"d":438,"n":8,"t":3},"speaker":"B","text":"はい。塔の数が増えています。"}];
INSERT INTO sentence [{"id":{"d":439,"n":1,"t":3},"speaker":"A","text":"スペインのお土産です。"},{"id":{"d":439,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。FCバルセロナのレプリカユニフォームですね。"},{"id":{"d":439,"n":3,"t":3},"speaker":"A","text":"はい。子ども用ですよ。"},{"id":{"d":439,"n":4,"t":3},"speaker":"B","text":"うちの子がFCバルセロナのファンであることを覚えていてくださったのですね。"},{"id":{"d":439,"n":5,"t":3},"speaker":"A","text":"はい。これを着て選手になりきってくださいね。"},{"id":{"d":439,"n":6,"t":3},"speaker":"B","text":"きっと喜びます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":440,"n":1,"t":3},"speaker":"A","text":"この夏は沖縄旅行に行ってきました。"},{"id":{"d":440,"n":2,"t":3},"speaker":"B","text":"沖縄ですか。いいですね。"},{"id":{"d":440,"n":3,"t":3},"speaker":"A","text":"海はきれいでしたし、おいしいものもたくさん食べられて、とても良い旅でした。"},{"id":{"d":440,"n":4,"t":3},"speaker":"B","text":"沖縄の海はきれいですよね。私も初めて見たときは感動しました。"},{"id":{"d":440,"n":5,"t":3},"speaker":"A","text":"あのような青い色の海は初めて見ました。"},{"id":{"d":440,"n":6,"t":3},"speaker":"B","text":"おいしいものとは、何を食べてきたのですか？"},{"id":{"d":440,"n":7,"t":3},"speaker":"A","text":"有名な沖縄料理を一通り食べました。ステーキやハンバーガーもおいしかったです。"}];
INSERT INTO sentence [{"id":{"d":441,"n":1,"t":3},"speaker":"A","text":"すみません。水族館への道を教えていただけますか？"},{"id":{"d":441,"n":2,"t":3},"speaker":"B","text":"はい。まず、ここからまっすぐ行くと海に着きます。海まで行ったら、右の方に水族館の建物が見えますよ。"},{"id":{"d":441,"n":3,"t":3},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":441,"n":4,"t":3},"speaker":"B","text":"海のそばの道路は交通量が多いので、歩道橋を使ってくださいね。"},{"id":{"d":441,"n":5,"t":3},"speaker":"A","text":"はい。ご親切にありがとうございます。"},{"id":{"d":441,"n":6,"t":3},"speaker":"B","text":"どういたしまして。お気を付けて。"}];
INSERT INTO sentence [{"id":{"d":442,"n":1,"t":3},"speaker":"A","text":"ようやく水族館に着きましたね。"},{"id":{"d":442,"n":2,"t":3},"speaker":"B","text":"親切な人が道を教えてくれて助かりました。"},{"id":{"d":442,"n":3,"t":3},"speaker":"A","text":"そうですね。その人がお薦めしていたイルカショーがもうすぐ開演するようです。"},{"id":{"d":442,"n":4,"t":3},"speaker":"B","text":"ぜひ見に行きましょう。ショーの場所はここから近いのでしょうか？"},{"id":{"d":442,"n":5,"t":3},"speaker":"A","text":"いいえ、入り口からショーの場所まではかなり遠そうです。"},{"id":{"d":442,"n":6,"t":3},"speaker":"B","text":"そうですか。では、今から行っても間に合わないかもしれませんね。"},{"id":{"d":442,"n":7,"t":3},"speaker":"A","text":"イルカショーはこの後も何回かあるようです。次の回を見ませんか？"},{"id":{"d":442,"n":8,"t":3},"speaker":"B","text":"名案ですね。展示もゆっくりと見たいですし、カフェのソフトクリームも食べたいので、イルカショーは最終の回でもいいかもしれませんね。"},{"id":{"d":442,"n":9,"t":3},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":443,"n":1,"t":3},"speaker":"A","text":"土曜日のはとバスツアーに申し込みました。"},{"id":{"d":443,"n":2,"t":3},"speaker":"B","text":"先日、パンフレットを頂いてきたツアーですね。"},{"id":{"d":443,"n":3,"t":3},"speaker":"A","text":"はい。春の房総日帰りツアーです。"},{"id":{"d":443,"n":4,"t":3},"speaker":"B","text":"特にイチゴ狩りと浜焼き食べ放題が魅力的ですね。"},{"id":{"d":443,"n":5,"t":3},"speaker":"A","text":"ええ、とても楽しみです。人気のツアーだったようで、私が申込みをしたときにはもうほとんど満席だった聞きました。"},{"id":{"d":443,"n":6,"t":3},"speaker":"B","text":"無事に席が取れてよかったです。"},{"id":{"d":443,"n":7,"t":3},"speaker":"A","text":"はい。ところで、お土産はやはり海産物がいいのでしょうか？"},{"id":{"d":443,"n":8,"t":3},"speaker":"B","text":"どうでしょうか。房総は農産物もおいしいと聞いたことがありますよ。"}];
INSERT INTO sentence [{"id":{"d":444,"n":1,"t":3},"speaker":"A","text":"イチゴ狩りで、イチゴを食べ過ぎてしまいました。"},{"id":{"d":444,"n":2,"t":3},"speaker":"B","text":"この後は浜焼きの食べ放題に行く予定ですが、大丈夫ですか？"},{"id":{"d":444,"n":3,"t":3},"speaker":"A","text":"もうおなかに何も入らないような気がします。"},{"id":{"d":444,"n":4,"t":3},"speaker":"B","text":"後のことを考えて食べればよかったのですよ。"},{"id":{"d":444,"n":5,"t":3},"speaker":"A","text":"そうするつもりだったのですが、あまりにもイチゴがおいしくて、ついたくさん食べてしまいました。"},{"id":{"d":444,"n":6,"t":3},"speaker":"B","text":"確かにとてもおいしかったですが、私は浜焼きも楽しみたい我慢しました。"},{"id":{"d":444,"n":7,"t":3},"speaker":"A","text":"偉いです。浜焼きで何も食べないのは悔しいので、私も高級そうなものだけでも食べるつもりです。"},{"id":{"d":444,"n":8,"t":3},"speaker":"B","text":"それでしたらイセエビとサザエはぜひ食べてください。"}];
INSERT INTO sentence [{"id":{"d":445,"n":1,"t":3},"speaker":"A","text":"次のバスは何時ですか？"},{"id":{"d":445,"n":2,"t":3},"speaker":"B","text":"次にバスが来るのは14時15分です。"},{"id":{"d":445,"n":3,"t":3},"speaker":"A","text":"1時間後ですか？"},{"id":{"d":445,"n":4,"t":3},"speaker":"B","text":"はい。本数が少なくてご不便をおかけしております。"},{"id":{"d":445,"n":5,"t":3},"speaker":"A","text":"いえ、大丈夫です。近くに時間をつぶせる場所はありませんか？"},{"id":{"d":445,"n":6,"t":3},"speaker":"B","text":"少し歩いたところに、飲食店やカフェがございます。お食事はお済みでしょうか？"},{"id":{"d":445,"n":7,"t":3},"speaker":"A","text":"いいえ、まだです。"},{"id":{"d":445,"n":8,"t":3},"speaker":"B","text":"では、飲食店はいかがでしょうか？左は100メートルほど行った所に、おいしい定食屋さんがあります。"},{"id":{"d":445,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。その定食屋さんに行ってみます。"}];
INSERT INTO sentence [{"id":{"d":446,"n":1,"t":3},"speaker":"A","text":"大学生のとき、バイクで北海道を1周したことがあります。"},{"id":{"d":446,"n":2,"t":3},"speaker":"B","text":"それは良い経験をしましたね。どこを回ったのですか？"},{"id":{"d":446,"n":3,"t":3},"speaker":"A","text":"函館から出発して、苫小牧、釧路、網走、稚内、富良野を回り、札幌まで行きました。"},{"id":{"d":446,"n":4,"t":3},"speaker":"B","text":"私も一度富良野に行ったことがありますが、自然の大きさに圧倒されました。"},{"id":{"d":446,"n":5,"t":3},"speaker":"A","text":"あそこの風景は、まるで日本ではないかのようですよね。"},{"id":{"d":446,"n":6,"t":3},"speaker":"B","text":"はい。美瑛の丘には本当に感動しました。ヨーロッパにでもいるみたいでした。"},{"id":{"d":446,"n":7,"t":3},"speaker":"A","text":"素晴らしい景色でした。私も、美瑛の丘では夢中になってバイクを走らせてきました。"},{"id":{"d":446,"n":8,"t":3},"speaker":"B","text":"あそこは何度でも行きたいところですね。"},{"id":{"d":446,"n":9,"t":3},"speaker":"A","text":"私もまた行ってみたいです。年が年ですからもうバイクでというわけにはいきませんが。"}];
INSERT INTO sentence [{"id":{"d":447,"n":1,"t":3},"speaker":"A","text":"先日、あおり運転に遭遇してしまいました。"},{"id":{"d":447,"n":2,"t":3},"speaker":"B","text":"最近よく聞きますが、身近で遭遇したという話を聞くのは初めてのことです。どういう経緯だったのですか？"},{"id":{"d":447,"n":3,"t":3},"speaker":"A","text":"それがよく分からないのです。気が付いたらすぐ後ろに大きなトラックが迫っていて、そこからずっと近い距離で追ってきたのです。"},{"id":{"d":447,"n":4,"t":3},"speaker":"B","text":"それは怖いですね。高速道路ですか？"},{"id":{"d":447,"n":5,"t":3},"speaker":"A","text":"ええ、中央道の小仏トンネルを抜けた辺りです。"},{"id":{"d":447,"n":6,"t":3},"speaker":"B","text":"あの辺りは私も時々走ります。よく渋滞が起こる所ですね。"},{"id":{"d":447,"n":7,"t":3},"speaker":"A","text":"その時も大渋滞が起こって、もしかするとトラックの運転手もそれでいら立っていたのかもしれません。渋滞がやっと終わったと思ったところで、あおり運転に遭ったのです。"},{"id":{"d":447,"n":8,"t":3},"speaker":"B","text":"スティーブン・スピルバーグの『激突！』という映画を思い出しますね。あれは、最後はトラックが崖から墜落して終わりましたけれど。"},{"id":{"d":447,"n":9,"t":3},"speaker":"A","text":"私も運転しながら、その映画を思い出しましたよ。"},{"id":{"d":447,"n":10,"t":3},"speaker":"B","text":"それで、どうやって逃れたのですか？警察に連絡したのですか？"},{"id":{"d":447,"n":11,"t":3},"speaker":"A","text":"いえ、1人だったので、電話する余裕もなかったです。ひたすら逃げるだけでした。八王子で高速道路を下りて、やっと振り切ることができました。"}];
INSERT INTO sentence [{"id":{"d":448,"n":1,"t":3},"speaker":"A","text":"昨年末に、クルーズ船で地中海を回ってきました。"},{"id":{"d":448,"n":2,"t":3},"speaker":"B","text":"地中海クルーズですか。私も一度行ってみたいと思っているのです。どこを回ったのですか？"},{"id":{"d":448,"n":3,"t":3},"speaker":"A","text":"ギリシャのアテネから出発して、イタリア、フランスを回って、バルセロナに着くコースでした。"},{"id":{"d":448,"n":4,"t":3},"speaker":"B","text":"寄港地はどこですか？"},{"id":{"d":448,"n":5,"t":3},"speaker":"A","text":"ギリシャはアテネ、ミコノス、サントリーニ島、イタリアはパレルモとナポリ、フランスはモナコとマルセイユ、スペインはバルセロナです。"},{"id":{"d":448,"n":6,"t":3},"speaker":"B","text":"随分たくさんの港を回るのですね。何日間で回るのですか？"},{"id":{"d":448,"n":7,"t":3},"speaker":"A","text":"10日間です。アテネに2日間滞在して、それ以外の所はそれぞれ1日で観光をしました。"},{"id":{"d":448,"n":8,"t":3},"speaker":"B","text":"夜の間に航海するわけですね。食事や娯楽も豪華なのでしょうね。"},{"id":{"d":448,"n":9,"t":3},"speaker":"A","text":"ええ、ディナーは各地の名物料理が出るし、コンサートやディナーショーもあるしで、退屈する暇もありませんでした。"},{"id":{"d":448,"n":10,"t":3},"speaker":"B","text":"すてきですね。私も定年になったら、妻と2人で行くつもりです。"},{"id":{"d":448,"n":11,"t":3},"speaker":"A","text":"ぜひ行かれてみてください。奥さんもきっと喜びますよ。"}];
INSERT INTO sentence [{"id":{"d":449,"n":1,"t":3},"speaker":"A","text":"宮本さん、夏休みの計画はありますか？"},{"id":{"d":449,"n":2,"t":3},"speaker":"B","text":"毎年、夏休みは息子とサイクリングで旅行をすることにしているのです。"},{"id":{"d":449,"n":3,"t":3},"speaker":"A","text":"それは面白いですね。いつから続けているのですか？"},{"id":{"d":449,"n":4,"t":3},"speaker":"B","text":"息子が中学1年の時からです。今年で3年目になりますね。最初は箱根まで行ったのです。"},{"id":{"d":449,"n":5,"t":3},"speaker":"A","text":"箱根駅伝のあの急坂を上ったのですか？"},{"id":{"d":449,"n":6,"t":3},"speaker":"B","text":"そうです。さすがに息子も疲れ果てて「もうサイクリングなんてしない」と言っていましたが、結局、次の年もその次の年も、時期になると「今年もどこかへ行こう」と催促してくるのです。"},{"id":{"d":449,"n":7,"t":3},"speaker":"A","text":"お子さんもサイクリングの魅力に取りつかれたというわけですね。箱根のほかにはどこに行かれましたか？"},{"id":{"d":449,"n":8,"t":3},"speaker":"B","text":"去年は富士山麓でした。今年はどこにするか息子と相談しているところです。"}];
INSERT INTO sentence [{"id":{"d":450,"n":1,"t":3},"speaker":"A","text":"笠井さんは釣りが趣味だそうですね。"},{"id":{"d":450,"n":2,"t":3},"speaker":"B","text":"はい、休日はだいたい釣りに行きます。家族からはひんしゅくを買っていますが。"},{"id":{"d":450,"n":3,"t":3},"speaker":"A","text":"どこに行かれるのですか？海釣りですか？それとも渓流釣りですか？"},{"id":{"d":450,"n":4,"t":3},"speaker":"B","text":"私が行くのは主に川です。関東や首都圏の川はだいたい行ったことがあると思います。"},{"id":{"d":450,"n":5,"t":3},"speaker":"A","text":"実は、私も釣りを始めたいと思っているのですが、初心者にお薦めの渓流などを教えてもらえませんか？"},{"id":{"d":450,"n":6,"t":3},"speaker":"B","text":"初心者には利根川水系の湯檜曽川がお薦めです。駐車スペースもたくさんあって、渓流に入りやすいです。"},{"id":{"d":450,"n":7,"t":3},"speaker":"A","text":"湯檜曽川では、どのような魚が釣れますか？"},{"id":{"d":450,"n":8,"t":3},"speaker":"B","text":"水がきれいな川ですから、イワナやヤマメがよく釣れます。今度、一緒に行きましょうか？"},{"id":{"d":450,"n":9,"t":3},"speaker":"A","text":"はい、ぜひお願いします。なにしろ全く経験がありませんので、初歩から教えてください。"}];
INSERT INTO sentence [{"id":{"d":451,"n":1,"t":3},"speaker":"A","text":"逆走車がしばしば話題になっていますが、私もついに先日、経験してしまいました。"},{"id":{"d":451,"n":2,"t":3},"speaker":"B","text":"黒田さんが逆走したのですか？"},{"id":{"d":451,"n":3,"t":3},"speaker":"A","text":"いいえ、私が逆走したのではなく、他の車が逆走しているのに遭遇したのです。"},{"id":{"d":451,"n":4,"t":3},"speaker":"B","text":"それは危ないところでしたね。どのような状況だったのですか？"},{"id":{"d":451,"n":5,"t":3},"speaker":"A","text":"片側1車線の自動車道路を走っていたときです。一方通行のはずが、前方から車が走ってきたのです。"},{"id":{"d":451,"n":6,"t":3},"speaker":"B","text":"それはびっくりしたでしょうね。もしそのときに運転していたのが私だったら、慌てふためいてどうしたらよいか分からないでいるうちに、向かってきた車と衝突してしまったかもしれません。"},{"id":{"d":451,"n":7,"t":3},"speaker":"A","text":"本当に危ないところでした。たまたま路肩があったので、とっさにハンドルを切って、何とか事故に遭わずにすみました。"},{"id":{"d":451,"n":8,"t":3},"speaker":"B","text":"想像するだけでも冷や汗が出ます。私にそのようなとっさの判断ができるかどうか、自信がありませんね。"},{"id":{"d":451,"n":9,"t":3},"speaker":"A","text":"自分でもよくあのような対応ができたものだと思います。ほとんど考える間もなかったのですから。"},{"id":{"d":451,"n":10,"t":3},"speaker":"B","text":"無意識のうちに体が反応したのですね。あなたが無事でよかったです。"}];
INSERT INTO sentence [{"id":{"d":452,"n":1,"t":3},"speaker":"A","text":"息子が卒業旅行にアメリカに行きたいと言うのです。"},{"id":{"d":452,"n":2,"t":3},"speaker":"B","text":"いいことではありませんか。「かわいい子には旅をさせよ」とも言いますし。"},{"id":{"d":452,"n":3,"t":3},"speaker":"A","text":"最近アルバイトに精を出していたので、一体何に使うのかと思っていたら、旅行の費用だったようです。"},{"id":{"d":452,"n":4,"t":3},"speaker":"B","text":"旅行の費用を自分で稼ぐというのは素晴らしいことですよ。"},{"id":{"d":452,"n":5,"t":3},"speaker":"A","text":"それはそうですね。"},{"id":{"d":452,"n":6,"t":3},"speaker":"B","text":"親としては少し心配かもしれませんが、息子さんを信じて送り出してあげるといいと思いますよ。"},{"id":{"d":452,"n":7,"t":3},"speaker":"A","text":"そうですね。そうしようと思います。"}];
INSERT INTO sentence [{"id":{"d":453,"n":1,"t":3},"speaker":"A","text":"新婚旅行はどこに行きたいですか？"},{"id":{"d":453,"n":2,"t":3},"speaker":"B","text":"私はヨーロッパがいいです。あなたはどこがいいですか？"},{"id":{"d":453,"n":3,"t":3},"speaker":"A","text":"ヨーロッパもいいですが、私はアメリカに行ってみたいです。"},{"id":{"d":453,"n":4,"t":3},"speaker":"B","text":"アメリカですか。それはなぜですか？"},{"id":{"d":453,"n":5,"t":3},"speaker":"A","text":"アメリカの最先端の文化に触れてみたいのです。あなたがヨーロッパへ行きたいのはなぜですか？"},{"id":{"d":453,"n":6,"t":3},"speaker":"B","text":"私は中世の街並みが残るヨーロッパで古くからの伝統的な文化に触れたいと思っていました。"},{"id":{"d":453,"n":7,"t":3},"speaker":"A","text":"なるほど。まったく正反対の希望ですね。"},{"id":{"d":453,"n":8,"t":3},"speaker":"B","text":"そうですね。今回はどちらかを選ぶか相談して決めましょう。"},{"id":{"d":453,"n":9,"t":3},"speaker":"A","text":"日程を半々に分けて、ヨーロッパとアメリカの両方に行くのはどうですか？"},{"id":{"d":453,"n":10,"t":3},"speaker":"B","text":"それだと、かなり慌ただしい旅になりそうですよ。"},{"id":{"d":453,"n":11,"t":3},"speaker":"A","text":"ゆっくり楽しむのは次の機会に回して、今回は2人が満足できるような旅行にしませんか？"},{"id":{"d":453,"n":12,"t":3},"speaker":"B","text":"分かりました。それもいいですね。"}];
INSERT INTO sentence [{"id":{"d":454,"n":1,"t":3},"speaker":"A","text":"先日北海道旅行をした時に、初めて函館の夜景を見ました。"},{"id":{"d":454,"n":2,"t":3},"speaker":"B","text":"世界三大夜景と言われるくらいですから、さぞかしきれいだったでしょうね。"},{"id":{"d":454,"n":3,"t":3},"speaker":"A","text":"それは息をのむ程でした。真っ暗な海の中に函館の地形がくっきり浮かび上がっていました。"},{"id":{"d":454,"n":4,"t":3},"speaker":"B","text":"私もガイドブックの写真で見たことがあります。確か、函館山からの眺めでしたね。"},{"id":{"d":454,"n":5,"t":3},"speaker":"A","text":"そうです。函館山は岬の先端にあるので、町の全体が見えるのですね。"},{"id":{"d":454,"n":6,"t":3},"speaker":"B","text":"ロープウエーが通っているそうですが、それで昇ったのですか？"},{"id":{"d":454,"n":7,"t":3},"speaker":"A","text":"はい、そのロープウエーで昇りました。125人乗りの大きなロープウエーでした。"},{"id":{"d":454,"n":8,"t":3},"speaker":"B","text":"それほど大きいのですか？それなら山頂は観光客でいっぱいでしょうね。"},{"id":{"d":454,"n":9,"t":3},"speaker":"A","text":"はい。私が行ったのは夏の観光シーズンでしたから、山頂は人でごった返していました。"},{"id":{"d":454,"n":10,"t":3},"speaker":"B","text":"人が多いのは少し気が引けますが、それでも一度は函館の夜景を見てみたいものです。"},{"id":{"d":454,"n":11,"t":3},"speaker":"A","text":"とても素晴らしかったので、ぜひ言ってみてください。私もまた別の季節に行きたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":455,"n":1,"t":3},"speaker":"A","text":"リニア新幹線に試乗してきました。"},{"id":{"d":455,"n":2,"t":3},"speaker":"B","text":"山梨にある実験線ですね。やはり速かったですか？"},{"id":{"d":455,"n":3,"t":3},"speaker":"A","text":"さすがに速かったですね。時速500キロメートルで、地上を走る乗り物では最速だそうです。"},{"id":{"d":455,"n":4,"t":3},"speaker":"B","text":"今の新幹線が時速250キロメートルぐらいですから、その2倍ですね。乗り心地はいかがでしたか？"},{"id":{"d":455,"n":5,"t":3},"speaker":"A","text":"空中に浮かんでいますから静かで振動もなく、あれほど乗り心地の良い乗り物は初めてです。"},{"id":{"d":455,"n":6,"t":3},"speaker":"B","text":"私も一度乗ってみたいと思っていたのです。それを聞くと、ますます乗ってみたくなりました。"},{"id":{"d":455,"n":7,"t":3},"speaker":"A","text":"総じて素晴らしい体験でしたが、一つだけ残念だったのは、走行中に窓から見える景色です。"},{"id":{"d":455,"n":8,"t":3},"speaker":"B","text":"それはなぜですか？"},{"id":{"d":455,"n":9,"t":3},"speaker":"A","text":"走っているところの大部分はトンネルの中ですので、外の景色はあまり見られませんでした。"},{"id":{"d":455,"n":10,"t":3},"speaker":"B","text":"では、ずっと地下鉄に乗っているようなものですか？"},{"id":{"d":455,"n":11,"t":3},"speaker":"A","text":"その通りです。リニア新幹線は、旅行というよりはビジネスのための乗り物だと思います。"},{"id":{"d":455,"n":12,"t":3},"speaker":"B","text":"なるほど。それは少し味気ないですね。"}];
INSERT INTO sentence [{"id":{"d":456,"n":1,"t":3},"speaker":"A","text":"冬休みにどこかへ旅行しましょう。どこがいいですか？"},{"id":{"d":456,"n":2,"t":3},"speaker":"B","text":"僕は河口湖にあるPICAリゾートという所に行きたいです。"},{"id":{"d":456,"n":3,"t":3},"speaker":"A","text":"初めて聞く所です。そこには何があるのですか？"},{"id":{"d":456,"n":4,"t":3},"speaker":"B","text":"PICAリゾートはアウトドア体験ができる宿泊施設で、ドーム型のテントなどに泊まれるそうです。友達の黒田君が先日行ってきたそうで、とても面白かったと言っていました。"},{"id":{"d":456,"n":5,"t":3},"speaker":"A","text":"河口湖といえば、真冬は零下10度にもなる所です。冬のキャンプというのはあまり聞いたことがありませんが、今の時期に行っても大丈夫なのですか？"},{"id":{"d":456,"n":6,"t":3},"speaker":"B","text":"黒田君は、暖房設備がしっかり付いていて中はとても暖かかったと言っていました。"},{"id":{"d":456,"n":7,"t":3},"speaker":"A","text":"そうですか。それなら快適そうですね。"},{"id":{"d":456,"n":8,"t":3},"speaker":"B","text":"星空の下でバーベキューをするのも楽しそうですよ。"},{"id":{"d":456,"n":9,"t":3},"speaker":"A","text":"そうですね。では、冬休みはそこに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":457,"n":1,"t":3},"speaker":"A","text":"東京に行ったら神保町の古本屋巡りをしたいのですが、付き合ってもらえますか？"},{"id":{"d":457,"n":2,"t":3},"speaker":"B","text":"よいですよ。何か探しているものでもあるのですか？"},{"id":{"d":457,"n":3,"t":3},"speaker":"A","text":"はい。江戸時代の人々がしていた観光旅行について書かれている本を、探しています。"},{"id":{"d":457,"n":4,"t":3},"speaker":"B","text":"江戸時代の人も観光旅行をしていたのですか？"},{"id":{"d":457,"n":5,"t":3},"speaker":"A","text":"そのようです。お伊勢参りや善行寺詣などは、人気の観光地だったそうです。"},{"id":{"d":457,"n":6,"t":3},"speaker":"B","text":"それは観光旅行というよりは、宗教的な行事なのではありませんか？"},{"id":{"d":457,"n":7,"t":3},"speaker":"A","text":"宗教行事に名を借りた観光旅行だったそうですよ。最盛期には、半年で500万人近い人がお伊勢参りに行っていたそうです。"},{"id":{"d":457,"n":8,"t":3},"speaker":"B","text":"それほどまでに大勢行ったのですか。興味深いですね。"}];
INSERT INTO sentence [{"id":{"d":458,"n":1,"t":3},"speaker":"A","text":"富士観光のパンフレットに、イタリア旅行の企画が載っていましたよ。"},{"id":{"d":458,"n":2,"t":3},"speaker":"B","text":"イタリアには興味があります。どのような企画ですか？"},{"id":{"d":458,"n":3,"t":3},"speaker":"A","text":"1週間でミラノ、ベニス、フィレンツェ、ローマを回るそうです。"},{"id":{"d":458,"n":4,"t":3},"speaker":"B","text":"それはまた忙しい旅ですね。それで、費用はどのくらいかかるのですか？"},{"id":{"d":458,"n":5,"t":3},"speaker":"A","text":"17万円と書いてあります。随分安いと思いませんか？"},{"id":{"d":458,"n":6,"t":3},"speaker":"B","text":"そうですね。ホテルや食事がいささか心配になるくらいです。"},{"id":{"d":458,"n":7,"t":3},"speaker":"A","text":"出発地は河口湖駅だそうです。成田空港までバスで連れていってくれるそうですから、とても楽そうです。"},{"id":{"d":458,"n":8,"t":3},"speaker":"B","text":"悪くありませんね。"},{"id":{"d":458,"n":9,"t":3},"speaker":"A","text":"そうですよね。申し込んでみますか？"},{"id":{"d":458,"n":10,"t":3},"speaker":"B","text":"興味はあります。でも、少し考えさせてください。"},{"id":{"d":458,"n":11,"t":3},"speaker":"A","text":"ぜひ前向きに検討してみてください。このような機会はまたとないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":459,"n":1,"t":3},"speaker":"A","text":"先週、車椅子の母と一緒に旅行をしてきました。"},{"id":{"d":459,"n":2,"t":3},"speaker":"B","text":"それは親孝行をしましたね。どこに行ったのですか？"},{"id":{"d":459,"n":3,"t":3},"speaker":"A","text":"母が前から北海道に行きたいと言っていたので、思い切って行きました。"},{"id":{"d":459,"n":4,"t":3},"speaker":"B","text":"確か、遠藤さんは車の運転はなさいませんよね。鉄道とバスで行ったのですか？"},{"id":{"d":459,"n":5,"t":3},"speaker":"A","text":"はい。主に鉄道を使い、観光地ではバスで回りました。"},{"id":{"d":459,"n":6,"t":3},"speaker":"B","text":"車椅子だとなかなか大変だったのではありませんか？何かトラブルはありませんでしたか？"},{"id":{"d":459,"n":7,"t":3},"speaker":"A","text":"私もそれが気がかりだったのですが、駅の階段には昇降機がついていましたし、駅員さんも親切に手伝ってくれましたので驚くほど快適に過ごすことができました。"},{"id":{"d":459,"n":8,"t":3},"speaker":"B","text":"それはよかったですね。お母さんも喜ばれたでしょう。"},{"id":{"d":459,"n":9,"t":3},"speaker":"A","text":"はい、とても喜んでいました。また旅行をしたいと言っています。"}];
INSERT INTO sentence [{"id":{"d":460,"n":1,"t":3},"speaker":"A","text":"今度の土日にツーリングに行きませんか？"},{"id":{"d":460,"n":2,"t":3},"speaker":"B","text":"いいですね。"},{"id":{"d":460,"n":3,"t":3},"speaker":"A","text":"石田さんはどこか行きたい所がありますか？"},{"id":{"d":460,"n":4,"t":3},"speaker":"B","text":"いいえ、特にはありません。もし大塚さんに考えがあるなら、私はそれでよいです。"},{"id":{"d":460,"n":5,"t":3},"speaker":"A","text":"では八ヶ岳を1周するのはどうですか？"},{"id":{"d":460,"n":6,"t":3},"speaker":"B","text":"面白そうですね。オートバイで高原の風を受けて走るのは気持ちが良いでしょう。コースを教えてください。"},{"id":{"d":460,"n":7,"t":3},"speaker":"A","text":"小淵沢まで中央高速で行って、そこから八ヶ岳高原ラインで清里まで行きます。清泉寮に寄って、それから国道141号線で松原湖まで行き、八ヶ岳ビューロードとメルヘン街道、八ヶ岳エコーラインを通って小淵沢に戻ります。"},{"id":{"d":460,"n":8,"t":3},"speaker":"B","text":"1泊するならばどこか良い温泉宿に泊まりたいですね。どこかお薦めはありますか？"},{"id":{"d":460,"n":9,"t":3},"speaker":"A","text":"茅野に白樺荘という温泉宿があります。眺望の良い露天風呂があるそうですよ。"},{"id":{"d":460,"n":10,"t":3},"speaker":"B","text":"露天風呂ですか。風呂につかって、山を見ながらビールを一杯というのは最高ですね。"},{"id":{"d":460,"n":11,"t":3},"speaker":"A","text":"オートバイ仲間を誘って、一緒に行きましょう。"},{"id":{"d":460,"n":12,"t":3},"speaker":"B","text":"ぜひ行きましょう。私からも声を掛けておきますね。"}];
INSERT INTO sentence [{"id":{"d":461,"n":1,"t":3},"speaker":"A","text":"夏山シーズンですね。この時期になると登山をしたい欲がむくむくと湧いてきます。"},{"id":{"d":461,"n":2,"t":3},"speaker":"B","text":"私は先日一足先に行ってきましたよ。"},{"id":{"d":461,"n":3,"t":3},"speaker":"A","text":"早々と行きましたね。いつ、どこに行ったのですか？"},{"id":{"d":461,"n":4,"t":3},"speaker":"B","text":"6月5日に鉢ノ木岳慎太郎祭というものがあって、それに参加したのです。"},{"id":{"d":461,"n":5,"t":3},"speaker":"A","text":"近代登山の先駆者である百瀬慎太郎を記念して開かれるお祭りですね。"},{"id":{"d":461,"n":6,"t":3},"speaker":"B","text":"はい。神事のあとは、山頂まで登山する人と周辺を散策する人に分かれて、それぞれ夏山を楽しみました。"},{"id":{"d":461,"n":7,"t":3},"speaker":"A","text":"榎木さんはどうなさったのですか？"},{"id":{"d":461,"n":8,"t":3},"speaker":"B","text":"もちろん、私は山頂まで登りました。鳥の声やきれいな花々を堪能し心身ともにリフレッシュできました。"},{"id":{"d":461,"n":9,"t":3},"speaker":"A","text":"そのような話を聞くと、居ても立ってもいられなくなります。私も早く山へ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":462,"n":1,"t":3},"speaker":"A","text":"もう随分前になりますが、小型飛行機で伊豆大島に行ったことがあります。"},{"id":{"d":462,"n":2,"t":3},"speaker":"B","text":"プロペラのついたセスナ機ですか？"},{"id":{"d":462,"n":3,"t":3},"speaker":"A","text":"そうです。4人乗りで、会社の同僚と3人で行きました。"},{"id":{"d":462,"n":4,"t":3},"speaker":"B","text":"私は乗ったことがありませんが、結構揺れるのではありませんか？"},{"id":{"d":462,"n":5,"t":3},"speaker":"A","text":"穏やかな天気だったためあまり揺れませんでした。ただ、1度だけ大変強い風が吹いて、その時はさすがに大きく揺れました。"},{"id":{"d":462,"n":6,"t":3},"speaker":"B","text":"小型機ですから揺れも激しかったでしょうね。"},{"id":{"d":462,"n":7,"t":3},"speaker":"A","text":"ひやりとしましたが、パイロットがベテランの方でとても操縦が上手でしたので、すぐに落ち着きました。"},{"id":{"d":462,"n":8,"t":3},"speaker":"B","text":"私は高所恐怖症なので、想像しただけでもぞっとします。"}];
INSERT INTO sentence [{"id":{"d":463,"n":1,"t":3},"speaker":"A","text":"台風で旅行に行けない可能性が出てきましたね。ホテルをキャンセルしましょうか？"},{"id":{"d":463,"n":2,"t":3},"speaker":"B","text":"台風の進路にもよりますね。天気予報では何と言っていますか？"},{"id":{"d":463,"n":3,"t":3},"speaker":"A","text":"台風がジグザグに進んでいて、この先の進路がはっきりしないようです。"},{"id":{"d":463,"n":4,"t":3},"speaker":"B","text":"それでは、もう少し様子を見てみましょう。キャンセルの判断は少し待ってからにしましょう。"},{"id":{"d":463,"n":5,"t":3},"speaker":"A","text":"しかし、判断が遅くなるとキャンセル手数料がかかりますよ。"},{"id":{"d":463,"n":6,"t":3},"speaker":"B","text":"いくらですか？"},{"id":{"d":463,"n":7,"t":3},"speaker":"A","text":"当日だと宿泊料の100パーセント、前日で80パーセント、2日から3日前で50パーセント、4日から7日前で20パーセントと書いてあります。"},{"id":{"d":463,"n":8,"t":3},"speaker":"B","text":"それなら、8日前までにはどちらかに決めることにしましょう。"},{"id":{"d":463,"n":9,"t":3},"speaker":"A","text":"台風の進路がそれまでに分かるでしょうか？"},{"id":{"d":463,"n":10,"t":3},"speaker":"B","text":"きっとそれまでには分かりますよ。"}];
INSERT INTO sentence [{"id":{"d":464,"n":1,"t":3},"speaker":"A","text":"お母さんから絵葉書が届きました。"},{"id":{"d":464,"n":2,"t":3},"speaker":"B","text":"どこから送られたものですか？"},{"id":{"d":464,"n":3,"t":3},"speaker":"A","text":"マルセイユからです。1週間ほど前に着いていたそうですよ。"},{"id":{"d":464,"n":4,"t":3},"speaker":"B","text":"では、今頃は夫婦でマルセイユ観光を楽しんでいる頃でしょうね。"},{"id":{"d":464,"n":5,"t":3},"speaker":"A","text":"ところがお父さんは少し体調が悪いらしく、マルセイユ観光には参加しないそうです。"},{"id":{"d":464,"n":6,"t":3},"speaker":"B","text":"それは心配ですね。どこが悪いのか書いていますか？"},{"id":{"d":464,"n":7,"t":3},"speaker":"A","text":"熱が38度ほどあって、せきが止まらないそうです。"},{"id":{"d":464,"n":8,"t":3},"speaker":"B","text":"風邪でもひいたのでしょうか？それ以上悪くならないといいのですが。"},{"id":{"d":464,"n":9,"t":3},"speaker":"A","text":"しばらくは休んで、様子を見るそうです。お医者さんもいますから心配しないようにと書いてありました。"}];
INSERT INTO sentence [{"id":{"d":465,"n":1,"t":3},"speaker":"A","text":"こんにちは、お世話になります。お部屋の予約をお願いしたいのですが。"},{"id":{"d":465,"n":2,"t":3},"speaker":"B","text":"お電話ありがとうございます。かしこまりました。ご利用はいつのご予定ですか？"},{"id":{"d":465,"n":3,"t":3},"speaker":"A","text":"3月の20日と21日です。部屋は空いていますか？"},{"id":{"d":465,"n":4,"t":3},"speaker":"B","text":"何部屋のご予約でしょうか？"},{"id":{"d":465,"n":5,"t":3},"speaker":"A","text":"5部屋です。"},{"id":{"d":465,"n":6,"t":3},"speaker":"B","text":"少々お待ちください。20日と21日に5部屋をご用意できます。ご予約をお取りしてよろしいでしょうか？"},{"id":{"d":465,"n":7,"t":3},"speaker":"A","text":"はい、お願いします。それと、20日の夜に宴会をしたいのですが、宴会場は利用できますか？"},{"id":{"d":465,"n":8,"t":3},"speaker":"B","text":"はい、可能でございます。お食事はどういたしましょうか？"},{"id":{"d":465,"n":9,"t":3},"speaker":"A","text":"宴会用のメニューはありますか？"},{"id":{"d":465,"n":10,"t":3},"speaker":"B","text":"はい、松と竹と梅の3種類がございます。それぞれお1人さま2万円、1万5,000円、1万円となっております。"},{"id":{"d":465,"n":11,"t":3},"speaker":"A","text":"それでは竹でお願いします。"},{"id":{"d":465,"n":12,"t":3},"speaker":"B","text":"竹ですね。何名さま分をご用意いたしますか？"},{"id":{"d":465,"n":13,"t":3},"speaker":"A","text":"12名でお願いします。"},{"id":{"d":465,"n":14,"t":3},"speaker":"B","text":"かしこまりました。それでは当日、お待ちしております。"}];
INSERT INTO sentence [{"id":{"d":466,"n":1,"t":3},"speaker":"A","text":"外国を旅行をしたいのですが、残念なことに、あまり外国語ができないのです。"},{"id":{"d":466,"n":2,"t":3},"speaker":"B","text":"それなら、スマートフォンで翻訳アプリケーションを使うのはどうですか？"},{"id":{"d":466,"n":3,"t":3},"speaker":"A","text":"翻訳アプリケーションは前に試したことがありますが、あまりうまく使えませんでした。"},{"id":{"d":466,"n":4,"t":3},"speaker":"B","text":"それは随分前のことでしょう。今はとても進歩していますから、旅行で使うなら十分ですよ。"},{"id":{"d":466,"n":5,"t":3},"speaker":"A","text":"そうなのですか？当時は文字を入力するのも面倒で、使い勝手が良くなかった覚えがあるのですが。"},{"id":{"d":466,"n":6,"t":3},"speaker":"B","text":"今は、音声入力が主流です。スマートフォンに向かって発話すると、相手側の言語に翻訳して再生してくれます。"},{"id":{"d":466,"n":7,"t":3},"speaker":"A","text":"それは便利ですね。何カ国語ぐらい翻訳できるのですか？"},{"id":{"d":466,"n":8,"t":3},"speaker":"B","text":"30カ国語ほどです。欧米の主要な言語の他に、アフリカやアジアの言語も含まれているようです。"},{"id":{"d":466,"n":9,"t":3},"speaker":"A","text":"それなら大抵の国で不自由しませんね。今度、その翻訳アプリケーションを使ってみます。"}];
INSERT INTO sentence [{"id":{"d":467,"n":1,"t":3},"speaker":"A","text":"夏休みに釧路湿原をカヌーで回るというのはどうでしょう？"},{"id":{"d":467,"n":2,"t":3},"speaker":"B","text":"釧路湿原でカヌーですか。いいですね。大自然を満喫できそうです。"},{"id":{"d":467,"n":3,"t":3},"speaker":"A","text":"早朝のコースと昼のコース、夕方のコースがあるそうですが、どれがいいですか？"},{"id":{"d":467,"n":4,"t":3},"speaker":"B","text":"強いて言えば早朝のコースが良いです。朝もやの中を進むのは楽しそうです。"},{"id":{"d":467,"n":5,"t":3},"speaker":"A","text":"夕方のコースも良いですよ。沈みゆく夕日を見ながら進むようです。"},{"id":{"d":467,"n":6,"t":3},"speaker":"B","text":"昼時にのんびり進むというのも捨てがたいですね。"},{"id":{"d":467,"n":7,"t":3},"speaker":"A","text":"とても悩ましいですが、どれかに決めなくてはいけません。"},{"id":{"d":467,"n":8,"t":3},"speaker":"B","text":"どれも魅力的ですが、やはり早朝のコースにしませんか？"},{"id":{"d":467,"n":9,"t":3},"speaker":"A","text":"そうしましょうか。それでは、早朝のコースを予約しておきます。"},{"id":{"d":467,"n":10,"t":3},"speaker":"B","text":"お願いします。今から楽しみです。"}];
INSERT INTO sentence [{"id":{"d":468,"n":1,"t":3},"speaker":"A","text":"今年の忘年会は隅田川の屋形船で開催するのはどうでしょうか？"},{"id":{"d":468,"n":2,"t":3},"speaker":"B","text":"それはとても良い案ですね。毎年同じような形式の忘年会で、飽きていたところです。"},{"id":{"d":468,"n":3,"t":3},"speaker":"A","text":"船政という屋形船の会社があり、貸し切りもできるそうですよ。"},{"id":{"d":468,"n":4,"t":3},"speaker":"B","text":"料金はいくらですか？"},{"id":{"d":468,"n":5,"t":3},"speaker":"A","text":"1人当たり1万1,000円です。"},{"id":{"d":468,"n":6,"t":3},"speaker":"B","text":"例年通り会社から1人当たり5,000円補助が出ますから、実際に支払うのは1人当たり6,000円になりますね。料理はどうですか？"},{"id":{"d":468,"n":7,"t":3},"speaker":"A","text":"天ぷら、刺身の船盛り、鍋物がメインで、それに突き出し、前菜、デザートが付きます。"},{"id":{"d":468,"n":8,"t":3},"speaker":"B","text":"なるほど。船はどのようなコースを通るのですか？"},{"id":{"d":468,"n":9,"t":3},"speaker":"A","text":"品川から出発して、お台場、レインボーブリッジを通って、永代橋までの往復です。"},{"id":{"d":468,"n":10,"t":3},"speaker":"B","text":"きっと夜景がきれいでしょうね。社員の皆さんにも喜んでもらえそうです。"},{"id":{"d":468,"n":11,"t":3},"speaker":"A","text":"それでは、早速予約しておきます。"}];
INSERT INTO sentence [{"id":{"d":469,"n":1,"t":3},"speaker":"A","text":"島田さんは今年もどこか登山する予定ですか？"},{"id":{"d":469,"n":2,"t":3},"speaker":"B","text":"今年は八ヶ岳の縦走をするつもりです。"},{"id":{"d":469,"n":3,"t":3},"speaker":"A","text":"八ヶ岳なら私も学生時代に登ったことがあります。どのルートで行くのですか？"},{"id":{"d":469,"n":4,"t":3},"speaker":"B","text":"南八ヶ岳のルートです。"},{"id":{"d":469,"n":5,"t":3},"speaker":"A","text":"美濃戸山荘から出発して、硫黄岳、横岳、赤岳、阿弥陀岳を縦走するルートですね。"},{"id":{"d":469,"n":6,"t":3},"speaker":"B","text":"はい。このルートは前に1度登ったことがあるのですが、その時の印象が良かったので、また行きたいと思っていました。"},{"id":{"d":469,"n":7,"t":3},"speaker":"A","text":"そうでしたか。奥さんもご一緒に行かれるのですか？"},{"id":{"d":469,"n":8,"t":3},"speaker":"B","text":"はい、妻と一緒に登ります。"},{"id":{"d":469,"n":9,"t":3},"speaker":"A","text":"仲が良くてよろしいですね。事故に気を付けて、楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":470,"n":1,"t":3},"speaker":"A","text":"確か以前はこの先にコンビニエンスストアがありましたよね。"},{"id":{"d":470,"n":2,"t":3},"speaker":"B","text":"はい。この坂道を上りきった辺りにあるはずです。"},{"id":{"d":470,"n":3,"t":3},"speaker":"A","text":"しかし、見当たりませんね。2人とも記憶違いということはないと思いますが。"},{"id":{"d":470,"n":4,"t":3},"speaker":"B","text":"もしかしたら閉店したのかもしれません。コンビニエンスストアは開店するのも閉店するのも早いですから。"},{"id":{"d":470,"n":5,"t":3},"speaker":"A","text":"あ、ありましたよ。しかしこれはファミリーマートです。私の記憶ではセブン-イレブンでした。"},{"id":{"d":470,"n":6,"t":3},"speaker":"B","text":"セブン-イレブンが閉店して、その後にファミリーマートが開店したのでしょうか？"},{"id":{"d":470,"n":7,"t":3},"speaker":"A","text":"そうだと思います。ところで、私はカフェラテにしますが、松田さんは何がいいですか？"},{"id":{"d":470,"n":8,"t":3},"speaker":"B","text":"私はモカブレンドがいいです。砂糖なしでお願いします。"},{"id":{"d":470,"n":9,"t":3},"speaker":"A","text":"分かりました。一緒に買ってきますから、車の中で待っていてください。"},{"id":{"d":470,"n":10,"t":3},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":471,"n":1,"t":3},"speaker":"A","text":"カフェラテとモカブレンドをください。"},{"id":{"d":471,"n":2,"t":3},"speaker":"B","text":"こちらがカフェラテのコップで、こちらがモカブレンドのコップです。全部で300円です。"},{"id":{"d":471,"n":3,"t":3},"speaker":"A","text":"PayPayは使えますか？"},{"id":{"d":471,"n":4,"t":3},"speaker":"B","text":"はい、今月から使えるようになりました。ただ今、キャンペーン中で5パーセントのキャッシュバックを実施しております。"},{"id":{"d":471,"n":5,"t":3},"speaker":"A","text":"それはありがたいです。すみません、カフェラテはこちらのボタンでよいのですよね。"},{"id":{"d":471,"n":6,"t":3},"speaker":"B","text":"はい、そうです。"},{"id":{"d":471,"n":7,"t":3},"speaker":"A","text":"ボタンを押してもカフェラテが出ないようですが。"},{"id":{"d":471,"n":8,"t":3},"speaker":"B","text":"申し訳ありません、ミルクが切れていました。少々お待ちください。"},{"id":{"d":471,"n":9,"t":3},"speaker":"A","text":"そういえば、前はキャラメルシロップを置いていましたが、今はありませんね。"},{"id":{"d":471,"n":10,"t":3},"speaker":"B","text":"はい、今はシュガーだけです。"},{"id":{"d":471,"n":11,"t":3},"speaker":"A","text":"そうでしたか。"},{"id":{"d":471,"n":12,"t":3},"speaker":"B","text":"お待たせいたしました。ミルクの補充が終わりましたので、どうぞお使いください。"},{"id":{"d":471,"n":13,"t":3},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":472,"n":1,"t":3},"speaker":"A","text":"三朝庵が閉店したそうです。"},{"id":{"d":472,"n":2,"t":3},"speaker":"B","text":"三朝庵とは、早稲田にあるあのそば屋ですか？"},{"id":{"d":472,"n":3,"t":3},"speaker":"A","text":"そうです。学生時代によく行きましたね。"},{"id":{"d":472,"n":4,"t":3},"speaker":"B","text":"あの店のかつ丼やカレー南蛮が懐かしいですね。よく食べたものです。"},{"id":{"d":472,"n":5,"t":3},"speaker":"A","text":"そのかつ丼とカレー南蛮は、あの店が発祥だったことを知っていますか？"},{"id":{"d":472,"n":6,"t":3},"speaker":"B","text":"それは知りませんでした。道理でおいしいはずです。"},{"id":{"d":472,"n":7,"t":3},"speaker":"A","text":"番台に座っていた女将さんがお年を召して、体力が続かなくなり、泣く泣く閉店に踏み切ったのだそうです。"},{"id":{"d":472,"n":8,"t":3},"speaker":"B","text":"やさしい女将さんでしたね。学生時代はとても良くしてもらっていました。"},{"id":{"d":472,"n":9,"t":3},"speaker":"A","text":"はい。閉店は本当に残念です。"},{"id":{"d":472,"n":10,"t":3},"speaker":"B","text":"そうですね。閉店前にもう一度行きたかったです。"}];
INSERT INTO sentence [{"id":{"d":473,"n":1,"t":3},"speaker":"A","text":"旅先で病気になることほど心細いことはありませんね。"},{"id":{"d":473,"n":2,"t":3},"speaker":"B","text":"そうですね。現地の医療事情も分からない上、言葉もなかなか通じませんものね。"},{"id":{"d":473,"n":3,"t":3},"speaker":"A","text":"去年の冬、フランス旅行中に風邪をひいたのは実に困りました。"},{"id":{"d":473,"n":4,"t":3},"speaker":"B","text":"それはさぞ大変だったことでしょう。"},{"id":{"d":473,"n":5,"t":3},"speaker":"A","text":"はい。飛行機で隣に座っていた人がしきりにせきをしていたからか、パリに着いた翌日にのどが痛くなってしまったのです。"},{"id":{"d":473,"n":6,"t":3},"speaker":"B","text":"冬は特に風邪が流行する季節ですものね。医者には診てもらわなかったのですか？"},{"id":{"d":473,"n":7,"t":3},"speaker":"A","text":"はい、1週間ほど我慢して旅行を続けていました。"},{"id":{"d":473,"n":8,"t":3},"speaker":"B","text":"それでは旅行を楽しむどころではなかったでしょうね。"},{"id":{"d":473,"n":9,"t":3},"speaker":"A","text":"はい。なかなか治らないので、1週間目にようやく領事館に事情を話して、クリニックを紹介してもらいました。"},{"id":{"d":473,"n":10,"t":3},"speaker":"B","text":"言葉は通じましたか？病状を説明するのに苦労なさったことでしょう。"},{"id":{"d":473,"n":11,"t":3},"speaker":"A","text":"幸いなことに日本人のよく行くクリニックだったので、お医者さんも慣れていました。"},{"id":{"d":473,"n":12,"t":3},"speaker":"B","text":"それは幸運でしたね。"}];
INSERT INTO sentence [{"id":{"d":474,"n":1,"t":3},"speaker":"A","text":"パリにパサージュというものがあるのをご存じですか？"},{"id":{"d":474,"n":2,"t":3},"speaker":"B","text":"19世紀に盛んだったアーケードのある商店街ですね。私も以前訪れたことがあります。"},{"id":{"d":474,"n":3,"t":3},"speaker":"A","text":"はい。そのパサージュに面白い古本屋がありました。"},{"id":{"d":474,"n":4,"t":3},"speaker":"B","text":"もしかして、入り口の所にある古本屋でしょう？"},{"id":{"d":474,"n":5,"t":3},"speaker":"A","text":"そうです。その店の前には古い絵葉書が置いてありました。ある時、そばにいたフランス人が私に絵葉書を見せて、これは侍かと聞くのです。"},{"id":{"d":474,"n":6,"t":3},"speaker":"B","text":"侍の絵葉書が置いてあったのですか？"},{"id":{"d":474,"n":7,"t":3},"speaker":"A","text":"そうです。3人の若者の写真で、洋装なのですが、まげを結っていました。"},{"id":{"d":474,"n":8,"t":3},"speaker":"B","text":"幕末の頃の写真ですね。彼らは、この国に留学でもしていたのでしょうか？"},{"id":{"d":474,"n":9,"t":3},"speaker":"A","text":"恐らくそうだと思います。店頭にはそのような日本人が描かれた絵葉書がたくさん飾ってあり、思わずまじまじと見てしまいました。"}];
INSERT INTO sentence [{"id":{"d":475,"n":1,"t":3},"speaker":"A","text":"春休みに富士山の周りを自転車で1周するという旅に行ってきました。"},{"id":{"d":475,"n":2,"t":3},"speaker":"B","text":"面白いですね。泊まりがけですか？"},{"id":{"d":475,"n":3,"t":3},"speaker":"A","text":"はい。日帰りも可能だったのですが、途中で1泊してゆっくりと1周してきました。"},{"id":{"d":475,"n":4,"t":3},"speaker":"B","text":"1周するのにどのくらいくらいかかるのですか？"},{"id":{"d":475,"n":5,"t":3},"speaker":"A","text":"7時間くらいです。アップダウンが激しいので大変ですが、眺めも良く、爽快な気分が味わえました。"},{"id":{"d":475,"n":6,"t":3},"speaker":"B","text":"私もサイクリングに挑戦してみたいのですが、もう少し楽なコースはありませんか？"},{"id":{"d":475,"n":7,"t":3},"speaker":"A","text":"近場から始めてはいかがでしょう？まずは、津久井湖まで行ってみませんか？"},{"id":{"d":475,"n":8,"t":3},"speaker":"B","text":"津久井湖といえば、クロワッサンがおいしいと評判のカフェがあります。一度、行ってみたいと思っていたところです。"},{"id":{"d":475,"n":9,"t":3},"speaker":"A","text":"いいですね。気候が良いうちに、ぜひ一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":476,"n":1,"t":3},"speaker":"A","text":"今度出張で日本に行くのですが、週末に東京観光ができそうです。"},{"id":{"d":476,"n":2,"t":3},"speaker":"B","text":"日本へ来るのはいつ頃ですか？"},{"id":{"d":476,"n":3,"t":3},"speaker":"A","text":"4月の初めです。"},{"id":{"d":476,"n":4,"t":3},"speaker":"B","text":"ちょうどサクラが満開の頃ですね。お花見が楽しめそうです。"},{"id":{"d":476,"n":5,"t":3},"speaker":"A","text":"サクラの季節なのですか？それは楽しみです。お薦めの花見スポットはありますか？"},{"id":{"d":476,"n":6,"t":3},"speaker":"B","text":"お花見といえば特に上野公園が有名ですが、他にもいろいろあります。千鳥ヶ淵のサクラもきれいですよ。ボートをこぎながら、ゆっくりお花見ができます。"},{"id":{"d":476,"n":7,"t":3},"speaker":"A","text":"旅の疲れをいやすのにぴったりですね。お花見は夜でもできますか？"},{"id":{"d":476,"n":8,"t":3},"speaker":"B","text":"できますよ。千鳥ヶ淵もライトアップされていてきれいですが、屋形船に乗るのもお薦めです。日本料理を食べながら、隅田川沿いのサクラを眺めることができます。"},{"id":{"d":476,"n":9,"t":3},"speaker":"A","text":"すてきですね。早速、屋形船のツアーを探してみます。"}];
INSERT INTO sentence [{"id":{"d":477,"n":1,"t":3},"speaker":"A","text":"明日宿泊予定の山田ですが、早めにチェックインすることはできますか？"},{"id":{"d":477,"n":2,"t":3},"speaker":"B","text":"山田春夫さまですね。ご予約ありがとうございます。あいにくですが、お部屋にお入りいただけるのは午後3時以降となります。"},{"id":{"d":477,"n":3,"t":3},"speaker":"A","text":"そうですか。午後3時より前に荷物を預かっていただくことはできますか？"},{"id":{"d":477,"n":4,"t":3},"speaker":"B","text":"そちらは可能でございます。フロントでお預かりいたします。"},{"id":{"d":477,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。助かります。"},{"id":{"d":477,"n":6,"t":3},"speaker":"B","text":"ご来館は何時頃のご予定ですか？12時以降でしたら、大浴場もご利用いただけます。"},{"id":{"d":477,"n":7,"t":3},"speaker":"A","text":"それはありがたいです。では、12時に伺います。"},{"id":{"d":477,"n":8,"t":3},"speaker":"B","text":"かしこまりました。それでは明日12時にお待ちしております。どうぞお気を付けてお越しください。"}];
INSERT INTO sentence [{"id":{"d":478,"n":1,"t":3},"speaker":"A","text":"田中さん、卒業旅行は楽しめましたか？"},{"id":{"d":478,"n":2,"t":3},"speaker":"B","text":"はい、先生。高校時代からの友人3人と一緒に、ヨーロッパを周遊しました。"},{"id":{"d":478,"n":3,"t":3},"speaker":"A","text":"何カ国を回ったのですか？"},{"id":{"d":478,"n":4,"t":3},"speaker":"B","text":"4カ国です。ポルトガル、スペイン、フランス、イタリアを、主に鉄道で移動しました。"},{"id":{"d":478,"n":5,"t":3},"speaker":"A","text":"それは豪勢ですね。"},{"id":{"d":478,"n":6,"t":3},"speaker":"B","text":"はい。そのために、頑張ってアルバイトをしました。"},{"id":{"d":478,"n":7,"t":3},"speaker":"A","text":"良い思い出ができてよかったですね。それはそうと、そろそろ卒論の方も頑張ってくださいね。"},{"id":{"d":478,"n":8,"t":3},"speaker":"B","text":"はい。思い切り遊んだので、今度は勉強に集中します。"}];
INSERT INTO sentence [{"id":{"d":479,"n":1,"t":3},"speaker":"A","text":"出張でニューヨークとボストンに行ってきました。"},{"id":{"d":479,"n":2,"t":3},"speaker":"B","text":"ニューヨークからボストンへは飛行機で移動したのですか？"},{"id":{"d":479,"n":3,"t":3},"speaker":"A","text":"その予定で夕方に空港まで行ったのですが、ボストン行きの便が欠航になってしまったのです。"},{"id":{"d":479,"n":4,"t":3},"speaker":"B","text":"それは災難でしたね。それで、どうしたのですか？"},{"id":{"d":479,"n":5,"t":3},"speaker":"A","text":"仕方がないので、ホテルに戻って、翌日の朝早くにバスで移動しました。"},{"id":{"d":479,"n":6,"t":3},"speaker":"B","text":"大変でしたね。バスだと移動に何時間くらいかかるのですか？"},{"id":{"d":479,"n":7,"t":3},"speaker":"A","text":"4時間くらいかかりました。"},{"id":{"d":479,"n":8,"t":3},"speaker":"B","text":"飛行機だと1時間半くらいで着きますよね。しかし、飛行機に乗るためには早めに空港に行かなければいけませんし、到着してからも預けていた荷物が出てくるのを待つ必要がありますので、実際は飛行機とバスとでが、所要時間にそれほど大きな違いはないのかもしれませんね。"},{"id":{"d":479,"n":9,"t":3},"speaker":"A","text":"そうなのです。移動途中の景色も楽しめましたので、バスの旅も悪くないと思いました。"}];
INSERT INTO sentence [{"id":{"d":480,"n":1,"t":3},"speaker":"A","text":"お盆休みは福岡に帰省するのですか？"},{"id":{"d":480,"n":2,"t":3},"speaker":"B","text":"はい。帰省を兼ねて、九州をあちこち回ってこようと思います。"},{"id":{"d":480,"n":3,"t":3},"speaker":"A","text":"それはいいですね。九州までは車で行かれるのですか？"},{"id":{"d":480,"n":4,"t":3},"speaker":"B","text":"いいえ。ここから車で行くのは大変なので、向こうでレンタカーを借りようかと思います。"},{"id":{"d":480,"n":5,"t":3},"speaker":"A","text":"では、福岡まではどのようにして行かれるのですか？"},{"id":{"d":480,"n":6,"t":3},"speaker":"B","text":"息子が電車好きなので、福岡までは新幹線で行こうと思っています。"},{"id":{"d":480,"n":7,"t":3},"speaker":"A","text":"そうですか。お子さんたちも楽しみにしているでしょうね。"},{"id":{"d":480,"n":8,"t":3},"speaker":"B","text":"はい、久しぶりにおじいちゃんとおばあちゃんに会えるので大喜びです。"}];
INSERT INTO sentence [{"id":{"d":481,"n":1,"t":3},"speaker":"A","text":"来月、韓国へ行きたいのですが、飛行機のチケットを取ることができず困っています。"},{"id":{"d":481,"n":2,"t":3},"speaker":"B","text":"それなら、飛行機を諦めてフェリーで韓国へ行くのはどうでしょうか？"},{"id":{"d":481,"n":3,"t":3},"speaker":"A","text":"フェリーで韓国へ行けるのですか？"},{"id":{"d":481,"n":4,"t":3},"speaker":"B","text":"高速船なら、福岡から釜山までを3時間くらいで行けますよ。"},{"id":{"d":481,"n":5,"t":3},"speaker":"A","text":"山田さんは韓国への出張が多いので、よくご存じですね。韓国への移動にフェリーをよく利用するのですか？"},{"id":{"d":481,"n":6,"t":3},"speaker":"B","text":"大抵は飛行機で行きますが、去年、急な出張でチケットが取れないときには、新幹線とフェリーを乗り継いで行きました。"},{"id":{"d":481,"n":7,"t":3},"speaker":"A","text":"そうだったのですね。船旅も楽しそうですね。"},{"id":{"d":481,"n":8,"t":3},"speaker":"B","text":"なかなか楽しかったですよ。観光目的の旅行でしたら、大阪から釜山まで出ているクルーズ船を利用するのもお薦めです。"}];
INSERT INTO sentence [{"id":{"d":482,"n":1,"t":3},"speaker":"A","text":"春休みにハワイに行ってきました。"},{"id":{"d":482,"n":2,"t":3},"speaker":"B","text":"それはいいですね。家族旅行ですか？"},{"id":{"d":482,"n":3,"t":3},"speaker":"A","text":"はい。子どもが小さいので、あまり観光はせずに、ハワイの海をのんびり楽しみました。"},{"id":{"d":482,"n":4,"t":3},"speaker":"B","text":"私も子どもが小さい頃に沖縄に行ったことがありますが、やはり観光といえば水族館くらいでした。"},{"id":{"d":482,"n":5,"t":3},"speaker":"A","text":"私もワイキキ水族館に行きました。子どもが大喜びでした。"},{"id":{"d":482,"n":6,"t":3},"speaker":"B","text":"水族館は大人も子どもも楽しめますものね。"},{"id":{"d":482,"n":7,"t":3},"speaker":"A","text":"はい。エアコンが効いているのもありがたいです。"},{"id":{"d":482,"n":8,"t":3},"speaker":"B","text":"本当にそうですね。親も一休みできますものね。"}];
INSERT INTO sentence [{"id":{"d":483,"n":1,"t":3},"speaker":"A","text":"秋も深まってきました。紅葉狩りの季節ですね。"},{"id":{"d":483,"n":2,"t":3},"speaker":"B","text":"紅葉狩りですか？何を狩るのですか？"},{"id":{"d":483,"n":3,"t":3},"speaker":"A","text":"紅葉狩りというのは、紅葉を見て楽しむことです。春のお花見のようなものです。"},{"id":{"d":483,"n":4,"t":3},"speaker":"B","text":"すてきな言い方ですね。紅葉を取ってくるわけではないのですね。"},{"id":{"d":483,"n":5,"t":3},"speaker":"A","text":"ええ、そういうわけではありません。今度、紅葉狩りのドライブに行きませんか？"},{"id":{"d":483,"n":6,"t":3},"speaker":"B","text":"景色を見るだけですか？私は、秋といえば食欲の秋です。"},{"id":{"d":483,"n":7,"t":3},"speaker":"A","text":"それなら、キノコ狩りやブドウ狩りもしましょう。"},{"id":{"d":483,"n":8,"t":3},"speaker":"B","text":"紅葉狩りのようにキノコやブドウも見るだけですか？"},{"id":{"d":483,"n":9,"t":3},"speaker":"A","text":"いえ、キノコ農園やブドウ農園で収穫を体験して、現地で食べたり持ち帰ったりします。"},{"id":{"d":483,"n":10,"t":3},"speaker":"B","text":"楽しそうですね。それはぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":484,"n":1,"t":3},"speaker":"A","text":"春休みに車で少し遠出したいのですが、お薦めの場所はありますか？"},{"id":{"d":484,"n":2,"t":3},"speaker":"B","text":"春休みでしたら、房総半島はいかがでしょうか？道沿いにたくさんの花が咲いていてきれいですよ。"},{"id":{"d":484,"n":3,"t":3},"speaker":"A","text":"花と海を見ながらドライブですか？気持ちが良さそうですね。"},{"id":{"d":484,"n":4,"t":3},"speaker":"B","text":"花を摘む体験ができる花畑もいくつかありますよ。"},{"id":{"d":484,"n":5,"t":3},"speaker":"A","text":"それはいいですね。子どももきっと喜ぶと思います。"},{"id":{"d":484,"n":6,"t":3},"speaker":"B","text":"房総半島には、潮干狩りができる場所もあります。"},{"id":{"d":484,"n":7,"t":3},"speaker":"A","text":"潮干狩りのシーズンはいつですか？"},{"id":{"d":484,"n":8,"t":3},"speaker":"B","text":"3月から8月です。"},{"id":{"d":484,"n":9,"t":3},"speaker":"A","text":"房総半島といえば夏に海水浴へ行く所という印象が強かったのですが、春もいろいろ楽しめそうですね。"}];
INSERT INTO sentence [{"id":{"d":485,"n":1,"t":3},"speaker":"A","text":"山田さんは、魚が好きですか？"},{"id":{"d":485,"n":2,"t":3},"speaker":"B","text":"はい。肉よりも魚が好きです。"},{"id":{"d":485,"n":3,"t":3},"speaker":"A","text":"それでしたら、来月船釣りに行くのですが、一緒に行きませんか？"},{"id":{"d":485,"n":4,"t":3},"speaker":"B","text":"お誘いありがとうございます。魚を食べるのは大好きですが、あいにく魚釣りの道具は何も持っていません。"},{"id":{"d":485,"n":5,"t":3},"speaker":"A","text":"心配ありません。釣りに必要な道具は全部用意されていますので、手ぶらで参加できます。"},{"id":{"d":485,"n":6,"t":3},"speaker":"B","text":"そうですか。でも、釣った魚を自分で料理するのは大変そうです。"},{"id":{"d":485,"n":7,"t":3},"speaker":"A","text":"それについても心配無用です。釣った魚は、釣り船屋さんで料理してもらうことができます。その場で、食べることもできますよ。"},{"id":{"d":485,"n":8,"t":3},"speaker":"B","text":"つまり、釣りを楽しんだ後に、飲み会を開催できるということですか？"},{"id":{"d":485,"n":9,"t":3},"speaker":"A","text":"はい、そういうことです。"},{"id":{"d":485,"n":10,"t":3},"speaker":"B","text":"なるほど。それはすてきですね。そういうことならぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":486,"n":1,"t":3},"speaker":"A","text":"山田さんは、九州に行ったことはありますか？"},{"id":{"d":486,"n":2,"t":3},"speaker":"B","text":"私は本州から出たことがありません。一番遠くに行ったのは、関西です。"},{"id":{"d":486,"n":3,"t":3},"speaker":"A","text":"そうでしたか。関西というと、大阪ですか？"},{"id":{"d":486,"n":4,"t":3},"speaker":"B","text":"大阪にも行ったことはありますが、高校の修学旅行では京都に行きました。"},{"id":{"d":486,"n":5,"t":3},"speaker":"A","text":"京都は神社やお寺がたくさんあって、見どころがたくさんですね。"},{"id":{"d":486,"n":6,"t":3},"speaker":"B","text":"そうですね。実は、最近になって仏像に興味が湧いてきました。高校生の頃はあまりしっかりと見ていなかったので、もったいないことをしたと思っています。"},{"id":{"d":486,"n":7,"t":3},"speaker":"A","text":"それでは今度鎌倉に行きませんか？鎌倉にも素晴らしい仏像がありまよ。"},{"id":{"d":486,"n":8,"t":3},"speaker":"B","text":"鎌倉なら日帰りで行けますね。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":487,"n":1,"t":3},"speaker":"A","text":"先週は四国に行ってきました。"},{"id":{"d":487,"n":2,"t":3},"speaker":"B","text":"おいしいものをたくさん食べることができましたか？"},{"id":{"d":487,"n":3,"t":3},"speaker":"A","text":"はい。うどんタクシーに乗って、おいしいうどんをたくさん食べてきました。"},{"id":{"d":487,"n":4,"t":3},"speaker":"B","text":"うどんタクシーといえば、香川県ですね。聞いたことがあります。タクシーでうどん屋を回ってくれるのですよね。"},{"id":{"d":487,"n":5,"t":3},"speaker":"A","text":"そうです。2時間で3軒のうどん屋をはしごしました。"},{"id":{"d":487,"n":6,"t":3},"speaker":"B","text":"うどんを3杯も食べたのですか？それは驚きです。"},{"id":{"d":487,"n":7,"t":3},"speaker":"A","text":"はい。一口に讃岐うどんといっても、お店ごとにさまざまな特徴があり、どれも大変おいしかったです。うどんの素晴らしさを堪能できました。"},{"id":{"d":487,"n":8,"t":3},"speaker":"B","text":"それはよかったですね。話を聞いているうちに、私もうどんが食べたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":488,"n":1,"t":3},"speaker":"A","text":"一度、日本縦断の旅をしてみたいと思っています。"},{"id":{"d":488,"n":2,"t":3},"speaker":"B","text":"楽しそうですね。車で旅をするのですか？"},{"id":{"d":488,"n":3,"t":3},"speaker":"A","text":"車を走らせるのも楽しそうですが、鉄道の旅をしたいです。"},{"id":{"d":488,"n":4,"t":3},"speaker":"B","text":"鈍行列車の旅なら、私は、学生の頃にしたことがあります。本州の北の端から南の端まで、10日くらいかけて移動しました。"},{"id":{"d":488,"n":5,"t":3},"speaker":"A","text":"いいですね。さすがに鈍行列車だけで時間がかかり過ぎては大変なので、私は、新幹線や特急列車も使う予定です。"},{"id":{"d":488,"n":6,"t":3},"speaker":"B","text":"それなら、すいぶん効率よく日本縦断の旅ができそうですね。"},{"id":{"d":488,"n":7,"t":3},"speaker":"A","text":"はい。5日間くらいかけて、北海道から鹿児島まで旅行したいと思っています。"}];
INSERT INTO sentence [{"id":{"d":489,"n":1,"t":3},"speaker":"A","text":"この間、出張でドイツに行ったのですが、電車に乗ったときにとても驚いたことがありました。"},{"id":{"d":489,"n":2,"t":3},"speaker":"B","text":"何があったのですか？"},{"id":{"d":489,"n":3,"t":3},"speaker":"A","text":"自転車を押して、そのまま電車に乗ってくる人を見かけたのです。"},{"id":{"d":489,"n":4,"t":3},"speaker":"B","text":"ドイツでは、自転車の車輪を外さずに、そのまま電車に乗れるのですか？"},{"id":{"d":489,"n":5,"t":3},"speaker":"A","text":"はい、そういうことです。私も知りませんでした。"},{"id":{"d":489,"n":6,"t":3},"speaker":"B","text":"それはうらやましいですね。自転車をそのまま電車に乗せられると、通勤にも便利ですし、サイクリングなどへも気軽に行くことができそうです。"},{"id":{"d":489,"n":7,"t":3},"speaker":"A","text":"そうですね。日本では、電車に乗るときは、自転車の車輪を外して袋に入れないといけませんからね。"}];
INSERT INTO sentence [{"id":{"d":490,"n":1,"t":3},"speaker":"A","text":"今度の家族旅行では、初めて、旅館に泊まる予定です。"},{"id":{"d":490,"n":2,"t":3},"speaker":"B","text":"すてきです。家族水入らずで、ゆっくりできるといいですね。"},{"id":{"d":490,"n":3,"t":3},"speaker":"A","text":"はい。ところで、旅館とホテルは何が違うのですか？"},{"id":{"d":490,"n":4,"t":3},"speaker":"B","text":"ホテルの客室は洋室にベッドの場合が多いですが、旅館の客室は和室に布団の場合が多いです。"},{"id":{"d":490,"n":5,"t":3},"speaker":"A","text":"旅館では、布団は自分で敷くのですか？"},{"id":{"d":490,"n":6,"t":3},"speaker":"B","text":"いいえ、仲居さんが敷いてくれますよ。仲居さんというのは、客の身の回りのお世話をするスタッフのことです。仲居さんは、部屋に到着した時にお茶をいれてくれたり、部屋に食事を運んでくれたりもします。"},{"id":{"d":490,"n":7,"t":3},"speaker":"A","text":"至れり尽くせりですね。とてもくつろげそうです。"},{"id":{"d":490,"n":8,"t":3},"speaker":"B","text":"その通りです。私は、それこそが旅館の魅力の一つだと思っています。"}];
INSERT INTO sentence [{"id":{"d":491,"n":1,"t":3},"speaker":"A","text":"来週、子どもを連れて東京に行く予定です。"},{"id":{"d":491,"n":2,"t":3},"speaker":"B","text":"赤ちゃんを連れて東京へ行くのなら、スマートフォンにこのアプリケーションを入れておくと便利ですよ。"},{"id":{"d":491,"n":3,"t":3},"speaker":"A","text":"これは何ができるアプリケーションなのですか？"},{"id":{"d":491,"n":4,"t":3},"speaker":"B","text":"これは、地下鉄駅のバリアフリー情報が分かるアプリケーションです。"},{"id":{"d":491,"n":5,"t":3},"speaker":"A","text":"それはとてもありがたいです。実は、ベビーカーをひいた状態で、地下鉄の乗り換え問題なくできか心配していたのです。"},{"id":{"d":491,"n":6,"t":3},"speaker":"B","text":"このアプリケーションを使うと、車椅子やベビーカーで移動できる乗り換えルートが分かります。"},{"id":{"d":491,"n":7,"t":3},"speaker":"A","text":"エレベーターの場所も分かりますか？"},{"id":{"d":491,"n":8,"t":3},"speaker":"B","text":"もちろんです。おむつ替えができる場所も分かりますよ。"},{"id":{"d":491,"n":9,"t":3},"speaker":"A","text":"とても便利なアプリケーションを教えていただき、ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":492,"n":1,"t":3},"speaker":"A","text":"東京旅行は楽しかったですか？"},{"id":{"d":492,"n":2,"t":3},"speaker":"B","text":"はい。息子はパトカーに乗ることができて、大変喜んでいました。"},{"id":{"d":492,"n":3,"t":3},"speaker":"A","text":"息子さんが迷子になったのですか？それは大変だったのではありませんか？"},{"id":{"d":492,"n":4,"t":3},"speaker":"B","text":"紛らわしい言い方ですみません。息子が乗ったのは、本物のパトカーではないのでご安心ください。ポリスミュージアムで、パトカーの乗車体験をしてきたのです。"},{"id":{"d":492,"n":5,"t":3},"speaker":"A","text":"そういうことでしたか。ポリスミュージアムというのは、警視庁がやっている警察博物館のことですよね。"},{"id":{"d":492,"n":6,"t":3},"speaker":"B","text":"そうです。警察が大好きな息子が大喜びでした。子ども用の制服を着て、白バイにも乗りました。"},{"id":{"d":492,"n":7,"t":3},"speaker":"A","text":"それはよかったですね。ところで、その博物館にあるパトカーや白バイの機体は本物なのですか？"},{"id":{"d":492,"n":8,"t":3},"speaker":"B","text":"はい、実車です。"},{"id":{"d":492,"n":9,"t":3},"speaker":"A","text":"そうでしたか。パトカーや白バイの実車が間近で見られるのなら、ぜひ一度は行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":493,"n":1,"t":3},"speaker":"A","text":"この間、初めて大阪に行きました。"},{"id":{"d":493,"n":2,"t":3},"speaker":"B","text":"大阪弁はテンポが速いでしょう？コミュニケーションは大丈夫でしたか？"},{"id":{"d":493,"n":3,"t":3},"speaker":"A","text":"それは大丈夫でした。大阪出身の友人がたくさんいるので、大阪弁には慣れています。しかし、駅の乗り換えが大変でした。"},{"id":{"d":493,"n":4,"t":3},"speaker":"B","text":"ああ、大阪の人は歩くのが速いですからね。"},{"id":{"d":493,"n":5,"t":3},"speaker":"A","text":"そうなのです。それに、エスカレーターでも、立つ位置が東京と違うのでまごつきました。"},{"id":{"d":493,"n":6,"t":3},"speaker":"B","text":"大阪では右側に立ちますね。"},{"id":{"d":493,"n":7,"t":3},"speaker":"A","text":"はい。ところが、うっかり東京のように左側に立ってしまい、慌ててしまうことがありました。"},{"id":{"d":493,"n":8,"t":3},"speaker":"B","text":"都市によって文化や習慣が違うので、慣れないうちは戸惑いますよね。"},{"id":{"d":493,"n":9,"t":3},"speaker":"A","text":"はい。大阪へは、今後も何回か行く予定なので、徐々に慣れていきたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":494,"n":1,"t":3},"speaker":"A","text":"今度の3連休は、山へ行きませんか？"},{"id":{"d":494,"n":2,"t":3},"speaker":"B","text":"山登りですか？私は最近めっきり運動不足なので、山を登りきる自信がありません。"},{"id":{"d":494,"n":3,"t":3},"speaker":"A","text":"それほど本格的な山登りをするつもりはありません。山登りというほどではなく、山歩きです。"},{"id":{"d":494,"n":4,"t":3},"speaker":"B","text":"そのくらいなら、私にもできるかもしれません。外で体を動かすのは、気持ちが良さそうですね。"},{"id":{"d":494,"n":5,"t":3},"speaker":"A","text":"天気が良いと、より一層気持ちが良いですよ。では、今回は初心者向けのハイキングコースをのんびり歩くというのはどうでしょうか？"},{"id":{"d":494,"n":6,"t":3},"speaker":"B","text":"いいですね。そのようなコースは近場にありますか？"},{"id":{"d":494,"n":7,"t":3},"speaker":"A","text":"たくさんありますよ。日帰りで行ける所にも、そのようなコースはいくつかあります。"},{"id":{"d":494,"n":8,"t":3},"speaker":"B","text":"せっかく休日に出かけるなら、温泉にも行きたいです。近くに温泉がある所にしませんか？"},{"id":{"d":494,"n":9,"t":3},"speaker":"A","text":"そうしましょう。ハイキングの帰りには温泉に寄って、疲れた体をほぐしましょう。"}];
INSERT INTO sentence [{"id":{"d":495,"n":1,"t":3},"speaker":"A","text":"新幹線の指定席を取っていただけますか？"},{"id":{"d":495,"n":2,"t":3},"speaker":"B","text":"はい。乗車する駅はどこですか？"},{"id":{"d":495,"n":3,"t":3},"speaker":"A","text":"東京駅です。"},{"id":{"d":495,"n":4,"t":3},"speaker":"B","text":"降りる駅はどこですか？"},{"id":{"d":495,"n":5,"t":3},"speaker":"A","text":"新大阪駅です。"},{"id":{"d":495,"n":6,"t":3},"speaker":"B","text":"本日の乗車でよろしいですか？"},{"id":{"d":495,"n":7,"t":3},"speaker":"A","text":"はい。なるべく早い時間が良いです。"},{"id":{"d":495,"n":8,"t":3},"speaker":"B","text":"10時45分東京駅出発の新幹線はいかがですか？"},{"id":{"d":495,"n":9,"t":3},"speaker":"A","text":"それでお願いします。"}];
INSERT INTO sentence [{"id":{"d":496,"n":1,"t":3},"speaker":"A","text":"来月の三連休に旅行に行くのですが、旅館とホテルのどちらに泊まるか迷っています。"},{"id":{"d":496,"n":2,"t":3},"speaker":"B","text":"それぞれの良さがありますよね。"},{"id":{"d":496,"n":3,"t":3},"speaker":"A","text":"あなたはどちらが好きですか？"},{"id":{"d":496,"n":4,"t":3},"speaker":"B","text":"どちらかというと、私はホテルが好きです。"},{"id":{"d":496,"n":5,"t":3},"speaker":"A","text":"私も同じですが、有名な旅館があって、そこに泊まりたい気持ちもあります。"},{"id":{"d":496,"n":6,"t":3},"speaker":"B","text":"それは迷いますね。"},{"id":{"d":496,"n":7,"t":3},"speaker":"A","text":"ホテルだと食事が豪華で、旅館だと温泉があって魅力的なのです。"},{"id":{"d":496,"n":8,"t":3},"speaker":"B","text":"あれこれ迷うのも旅行の楽しみの一つですよ。"}];
INSERT INTO sentence [{"id":{"d":497,"n":1,"t":3},"speaker":"A","text":"レンタカーの予約をしたいのですが可能ですか？"},{"id":{"d":497,"n":2,"t":3},"speaker":"B","text":"かしこまりました。お日にちはいつですか？"},{"id":{"d":497,"n":3,"t":3},"speaker":"A","text":"7月16日から18日までです。"},{"id":{"d":497,"n":4,"t":3},"speaker":"B","text":"3日間のご利用ですね。車種のご希望はありますか？"},{"id":{"d":497,"n":5,"t":3},"speaker":"A","text":"私だけが利用するので軽自動車で良いです。"},{"id":{"d":497,"n":6,"t":3},"speaker":"B","text":"1名さまで、軽自動車のご利用ですね。ご用意できます。"},{"id":{"d":497,"n":7,"t":3},"speaker":"A","text":"18日は何時までに返却すれば良いですか？"},{"id":{"d":497,"n":8,"t":3},"speaker":"B","text":"午後9時までにご返却をお願いします。ご予約を確定してもよろしいですか？"},{"id":{"d":497,"n":9,"t":3},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":498,"n":1,"t":3},"speaker":"A","text":"ご実家へ帰省されたそうですね？"},{"id":{"d":498,"n":2,"t":3},"speaker":"B","text":"はい。2泊3日で夫の実家へ行ってきました。"},{"id":{"d":498,"n":3,"t":3},"speaker":"A","text":"ご実家の皆さんはお変わりなかったですか？"},{"id":{"d":498,"n":4,"t":3},"speaker":"B","text":"義母が膝を痛めてしまって、外出に車椅子が必要という状態になっていました。"},{"id":{"d":498,"n":5,"t":3},"speaker":"A","text":"それは大変ですね。"},{"id":{"d":498,"n":6,"t":3},"speaker":"B","text":"車への乗り降りにも補助が必要でしたし、いろいろと手助けが必要でした。"},{"id":{"d":498,"n":7,"t":3},"speaker":"A","text":"何かサポートを受けられると良いですね。"},{"id":{"d":498,"n":8,"t":3},"speaker":"B","text":"介護サービスを利用しているようです。心配なので、これからは頻繁に連絡を取ろうと思っています。"}];
INSERT INTO sentence [{"id":{"d":499,"n":1,"t":3},"speaker":"A","text":"一人旅をしたことはありますか？"},{"id":{"d":499,"n":2,"t":3},"speaker":"B","text":"はい。大学生の時に一度だけあります。"},{"id":{"d":499,"n":3,"t":3},"speaker":"A","text":"私は経験がないので、一人旅に行ってみたいです。"},{"id":{"d":499,"n":4,"t":3},"speaker":"B","text":"一人旅の計画をしてはどうですか？"},{"id":{"d":499,"n":5,"t":3},"speaker":"A","text":"そうなのですが、いざとなると勇気が出ません。"},{"id":{"d":499,"n":6,"t":3},"speaker":"B","text":"そのようなに心配する必要はないと思いますよ。"},{"id":{"d":499,"n":7,"t":3},"speaker":"A","text":"どのようにかなるものでしょうか？"},{"id":{"d":499,"n":8,"t":3},"speaker":"B","text":"はい。何か困ったら誰かに聞けばきっと大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":500,"n":1,"t":3},"speaker":"A","text":"何を見ているのですか？"},{"id":{"d":500,"n":2,"t":3},"speaker":"B","text":"豪華客船での旅行のパンフレットです。"},{"id":{"d":500,"n":3,"t":3},"speaker":"A","text":"豪華客船の旅に行きたいのですか？"},{"id":{"d":500,"n":4,"t":3},"speaker":"B","text":"もちろんです。このような旅行をすることが夢です。"},{"id":{"d":500,"n":5,"t":3},"speaker":"A","text":"私も憧れますが、船酔いしてしまうので諦めています。"},{"id":{"d":500,"n":6,"t":3},"speaker":"B","text":"そうでしたか。私は一度も船酔いしたことがありません。"},{"id":{"d":500,"n":7,"t":3},"speaker":"A","text":"うらやましいです。"},{"id":{"d":500,"n":8,"t":3},"speaker":"B","text":"酔い止め薬を飲んでも駄目ですか？"},{"id":{"d":500,"n":9,"t":3},"speaker":"A","text":"はい。酔い止め薬を一度だけ試しましたが、効果は感じられませんでした。"}];
INSERT INTO sentence [{"id":{"d":501,"n":1,"t":3},"speaker":"A","text":"お土産の温泉まんじゅうです。よかったら1つどうぞ。"},{"id":{"d":501,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。どちらへ行かれたのですか？"},{"id":{"d":501,"n":3,"t":3},"speaker":"A","text":"箱根へ行ってきました。"},{"id":{"d":501,"n":4,"t":3},"speaker":"B","text":"良いですね。温泉を楽しめましたか？"},{"id":{"d":501,"n":5,"t":3},"speaker":"A","text":"はい。ゆっくりお湯に漬かって、疲れを癒しました。"},{"id":{"d":501,"n":6,"t":3},"speaker":"B","text":"混んでいませんでしたか？"},{"id":{"d":501,"n":7,"t":3},"speaker":"A","text":"はい。意外とすいていて、大丈夫でした。"},{"id":{"d":501,"n":8,"t":3},"speaker":"B","text":"それは良かったですね。"}];
INSERT INTO sentence [{"id":{"d":502,"n":1,"t":3},"speaker":"A","text":"あさっては、北海道へ出張に行くことになりました。"},{"id":{"d":502,"n":2,"t":3},"speaker":"B","text":"随分と急な予定が入りましたね。"},{"id":{"d":502,"n":3,"t":3},"speaker":"A","text":"北海道への出張は、部長が行く予定だったのですが、他の予定が入ってしまって、代わりに私が行くことになりました。"},{"id":{"d":502,"n":4,"t":3},"speaker":"B","text":"何泊する予定ですか？"},{"id":{"d":502,"n":5,"t":3},"speaker":"A","text":"3泊です。"},{"id":{"d":502,"n":6,"t":3},"speaker":"B","text":"荷物の準備をしなければいけませんね。"},{"id":{"d":502,"n":7,"t":3},"speaker":"A","text":"北海道は寒いので、防寒着を持って行きます。"},{"id":{"d":502,"n":8,"t":3},"speaker":"B","text":"手袋やマフラーも用意した方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":503,"n":1,"t":3},"speaker":"A","text":"すみません。忘れ物の受付はこちらですか？"},{"id":{"d":503,"n":2,"t":3},"speaker":"B","text":"はい、承ります。"},{"id":{"d":503,"n":3,"t":3},"speaker":"A","text":"先ほど乗っていた電車に傘を忘れてしまいました。"},{"id":{"d":503,"n":4,"t":3},"speaker":"B","text":"降りた駅と、時間を教えてください。"},{"id":{"d":503,"n":5,"t":3},"speaker":"A","text":"品川駅に午後2時過ぎくらいに降りました。"},{"id":{"d":503,"n":6,"t":3},"speaker":"B","text":"山手線ですか？"},{"id":{"d":503,"n":7,"t":3},"speaker":"A","text":"京浜東北線の上り電車です。"},{"id":{"d":503,"n":8,"t":3},"speaker":"B","text":"どのような傘か、教えてください。"},{"id":{"d":503,"n":9,"t":3},"speaker":"A","text":"紺色で無地の傘です。"},{"id":{"d":503,"n":10,"t":3},"speaker":"B","text":"見つかり次第ご連絡しますので、こちらに連絡先をご記入ください。"}];
INSERT INTO sentence [{"id":{"d":504,"n":1,"t":3},"speaker":"A","text":"新婚旅行はどこに行きましたか？"},{"id":{"d":504,"n":2,"t":3},"speaker":"B","text":"オーストラリアです。"},{"id":{"d":504,"n":3,"t":3},"speaker":"A","text":"良いですね。どうしてオーストラリアにしたのですか？"},{"id":{"d":504,"n":4,"t":3},"speaker":"B","text":"コアラを抱いてみたかったのです。"},{"id":{"d":504,"n":5,"t":3},"speaker":"A","text":"実際に抱っこしてみて、どうでしたか？"},{"id":{"d":504,"n":6,"t":3},"speaker":"B","text":"少し緊張しましたが、とてもかわいかったです。"},{"id":{"d":504,"n":7,"t":3},"speaker":"A","text":"写真も撮れましたか？"},{"id":{"d":504,"n":8,"t":3},"speaker":"B","text":"はい。たくさん撮りました。"},{"id":{"d":504,"n":9,"t":3},"speaker":"A","text":"今度、ぜひ見せてください。"}];
INSERT INTO sentence [{"id":{"d":505,"n":1,"t":3},"speaker":"A","text":"すみません。部屋のシャワーからお湯が出ません。"},{"id":{"d":505,"n":2,"t":3},"speaker":"B","text":"それは失礼いたしました。水しか出ないということでしょうか？"},{"id":{"d":505,"n":3,"t":3},"speaker":"A","text":"お湯の蛇口を回すとぬるい水が出ますが、熱いお湯は出ません。"},{"id":{"d":505,"n":4,"t":3},"speaker":"B","text":"お部屋はどちらですか？"},{"id":{"d":505,"n":5,"t":3},"speaker":"A","text":"1125号室です。"},{"id":{"d":505,"n":6,"t":3},"speaker":"B","text":"担当の者がお部屋に伺いますので、少々お待ちください。"},{"id":{"d":505,"n":7,"t":3},"speaker":"A","text":"シャワーを浴びてから出掛けたいので、早目にお願いします。"},{"id":{"d":505,"n":8,"t":3},"speaker":"B","text":"承知しました。"}];
INSERT INTO sentence [{"id":{"d":506,"n":1,"t":3},"speaker":"A","text":"チェックインをお願いします。"},{"id":{"d":506,"n":2,"t":3},"speaker":"B","text":"ご予約のお名前をお教えいただけますか？"},{"id":{"d":506,"n":3,"t":3},"speaker":"A","text":"田辺洋平です。"},{"id":{"d":506,"n":4,"t":3},"speaker":"B","text":"1名さま、2泊でご予約をいただいております。"},{"id":{"d":506,"n":5,"t":3},"speaker":"A","text":"3泊に変更できますか？"},{"id":{"d":506,"n":6,"t":3},"speaker":"B","text":"大丈夫です。それでは本日から3泊のご利用ですね。"},{"id":{"d":506,"n":7,"t":3},"speaker":"A","text":"はい。部屋は何号室でしょうか？"},{"id":{"d":506,"n":8,"t":3},"speaker":"B","text":"8階の811号室です。こちらがお部屋の鍵です。"},{"id":{"d":506,"n":9,"t":3},"speaker":"A","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":507,"n":1,"t":3},"speaker":"A","text":"次の快速電車は何時に出発しますか？"},{"id":{"d":507,"n":2,"t":3},"speaker":"B","text":"どちら方面の快速電車ですか？"},{"id":{"d":507,"n":3,"t":3},"speaker":"A","text":"成田空港行きです。"},{"id":{"d":507,"n":4,"t":3},"speaker":"B","text":"13時20分の出発です。"},{"id":{"d":507,"n":5,"t":3},"speaker":"A","text":"出発まで随分時間がありますね。"},{"id":{"d":507,"n":6,"t":3},"speaker":"B","text":"先に出発する特急電車の方が、早く成田空港に着きますよ。"},{"id":{"d":507,"n":7,"t":3},"speaker":"A","text":"その特急電車は何時の出発ですか？"},{"id":{"d":507,"n":8,"t":3},"speaker":"B","text":"12時55分です。"},{"id":{"d":507,"n":9,"t":3},"speaker":"A","text":"それでは、特急電車に乗ることにします。"}];
INSERT INTO sentence [{"id":{"d":508,"n":1,"t":3},"speaker":"A","text":"京葉線に乗り換えたいのですが、どこから乗れますか？"},{"id":{"d":508,"n":2,"t":3},"speaker":"B","text":"このまま真っ直ぐ進むと下りのエスカレーターがありますので、それで地下に降りて、右に少し行くとホームがあるので、そこから乗れますよ。"},{"id":{"d":508,"n":3,"t":3},"speaker":"A","text":"どうもありがとうございます。"},{"id":{"d":508,"n":4,"t":3},"speaker":"B","text":"結構、距離があるので、時間がかかりますよ。乗りたい電車はいつ出発ですか？"},{"id":{"d":508,"n":5,"t":3},"speaker":"A","text":"3分後に出発です。間に合うでしょうか？"},{"id":{"d":508,"n":6,"t":3},"speaker":"B","text":"出発寸前の時間となりますが、恐らく間に合うと思います。"},{"id":{"d":508,"n":7,"t":3},"speaker":"A","text":"それでは急ぎ足で向かいますので、失礼します。"},{"id":{"d":508,"n":8,"t":3},"speaker":"B","text":"お気を付けて。"}];
INSERT INTO sentence [{"id":{"d":509,"n":1,"t":3},"speaker":"A","text":"新宿への行き方は分かりますか？"},{"id":{"d":509,"n":2,"t":3},"speaker":"B","text":"いいえ。分からないので、路線図を見ましょう。"},{"id":{"d":509,"n":3,"t":3},"speaker":"A","text":"たくさんの路線がありますね。"},{"id":{"d":509,"n":4,"t":3},"speaker":"B","text":"新宿駅は見つかりましたか？"},{"id":{"d":509,"n":5,"t":3},"speaker":"A","text":"はい。左上の方に書いてあります。"},{"id":{"d":509,"n":6,"t":3},"speaker":"B","text":"本当ですね。新宿駅にはいろいろな路線の電車やバスが止まるようですね。"},{"id":{"d":509,"n":7,"t":3},"speaker":"A","text":"ここからだと、乗り換えが2回必要です。"},{"id":{"d":509,"n":8,"t":3},"speaker":"B","text":"間違えないように気を付けて行きましょう。"}];
