INSERT INTO sentence [{"id":{"d":511,"n":1,"t":3},"speaker":"A","text":"お食事がお済みでしたら、お下げしてよろしいでしょうか？"},{"id":{"d":511,"n":2,"t":3},"speaker":"B","text":"はい、お願いします。"},{"id":{"d":511,"n":3,"t":3},"speaker":"A","text":"コーヒーか紅茶はいかがですか？"},{"id":{"d":511,"n":4,"t":3},"speaker":"B","text":"紅茶を下さい。"},{"id":{"d":511,"n":5,"t":3},"speaker":"A","text":"ミルクとお砂糖はお付けしますか？"},{"id":{"d":511,"n":6,"t":3},"speaker":"B","text":"ミルクだけでよいです。"},{"id":{"d":511,"n":7,"t":3},"speaker":"A","text":"承知しました。こちらに置きますね。"},{"id":{"d":511,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":512,"n":1,"t":3},"speaker":"A","text":"乗車券を拝見します。"},{"id":{"d":512,"n":2,"t":3},"speaker":"B","text":"はい、どうぞ。"},{"id":{"d":512,"n":3,"t":3},"speaker":"A","text":"座席をお間違えのようです。"},{"id":{"d":512,"n":4,"t":3},"speaker":"B","text":"本当ですか？"},{"id":{"d":512,"n":5,"t":3},"speaker":"A","text":"はい。乗車券には、5号車の10番Cのお席と記載されています。"},{"id":{"d":512,"n":6,"t":3},"speaker":"B","text":"ここは何号車ですか？"},{"id":{"d":512,"n":7,"t":3},"speaker":"A","text":"6号車です。"},{"id":{"d":512,"n":8,"t":3},"speaker":"B","text":"号車の番号を見間違えたようです。すみません。"}];
INSERT INTO sentence [{"id":{"d":513,"n":1,"t":3},"speaker":"A","text":"すみません。隣の席は空いていますか？"},{"id":{"d":513,"n":2,"t":3},"speaker":"B","text":"いいえ。友人が座っています。今、トイレに行っていますが、すぐに戻ります。"},{"id":{"d":513,"n":3,"t":3},"speaker":"A","text":"そうでしたか。他の席を探します。"},{"id":{"d":513,"n":4,"t":3},"speaker":"B","text":"今日は混んでいるから、空いている席が少ないですね。"},{"id":{"d":513,"n":5,"t":3},"speaker":"A","text":"はい。どの車両もほぼ満席のようです。"},{"id":{"d":513,"n":6,"t":3},"speaker":"B","text":"私と友人は2駅先で降りますから、その後でよろしければお座りください。"},{"id":{"d":513,"n":7,"t":3},"speaker":"A","text":"助かります。ありがとうございます。"},{"id":{"d":513,"n":8,"t":3},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":514,"n":1,"t":3},"speaker":"A","text":"そろそろ、降りる駅ですね。"},{"id":{"d":514,"n":2,"t":3},"speaker":"B","text":"ええ。降りる準備をしましょう。"},{"id":{"d":514,"n":3,"t":3},"speaker":"A","text":"そうですね。新幹線のチケットは手元にありますか？"},{"id":{"d":514,"n":4,"t":3},"speaker":"B","text":"ポケットに入れておいたのですが、見当たりません。"},{"id":{"d":514,"n":5,"t":3},"speaker":"A","text":"ジャケットのポケットですか？"},{"id":{"d":514,"n":6,"t":3},"speaker":"B","text":"いえ、ズボンのポケットです。"},{"id":{"d":514,"n":7,"t":3},"speaker":"A","text":"よく探してみてください。"},{"id":{"d":514,"n":8,"t":3},"speaker":"B","text":"後ろのポケットにありました。良かったです。"}];
INSERT INTO sentence [{"id":{"d":515,"n":1,"t":3},"speaker":"A","text":"スキー旅行に出掛けることになりました。"},{"id":{"d":515,"n":2,"t":3},"speaker":"B","text":"誰と行くのですか？"},{"id":{"d":515,"n":3,"t":3},"speaker":"A","text":"大学の友達です。"},{"id":{"d":515,"n":4,"t":3},"speaker":"B","text":"どこのスキー場ですか？"},{"id":{"d":515,"n":5,"t":3},"speaker":"A","text":"苗場スキー場です。"},{"id":{"d":515,"n":6,"t":3},"speaker":"B","text":"苗場スキー場なら近くていいですね。"},{"id":{"d":515,"n":7,"t":3},"speaker":"A","text":"はい。近いので、日帰りで行く予定です。"},{"id":{"d":515,"n":8,"t":3},"speaker":"B","text":"帰りに温泉に寄るのもいいですよ。"},{"id":{"d":515,"n":9,"t":3},"speaker":"A","text":"それは良いですね。友達に提案してみます。"}];
INSERT INTO sentence [{"id":{"d":516,"n":1,"t":3},"speaker":"A","text":"釣りに行ってみたいです。"},{"id":{"d":516,"n":2,"t":3},"speaker":"B","text":"釣りに行ったことはありませんか？"},{"id":{"d":516,"n":3,"t":3},"speaker":"A","text":"はい。一度もありません。あなたはありますか？"},{"id":{"d":516,"n":4,"t":3},"speaker":"B","text":"はい。父が何度か連れて行ってくれました。"},{"id":{"d":516,"n":5,"t":3},"speaker":"A","text":"今度、一緒に行ってくれませんか？"},{"id":{"d":516,"n":6,"t":3},"speaker":"B","text":"よいですよ。海釣りと川釣りのどちらが良いですか？"},{"id":{"d":516,"n":7,"t":3},"speaker":"A","text":"川釣りをしてみたいです。"},{"id":{"d":516,"n":8,"t":3},"speaker":"B","text":"それでは次の週末に計画しましょう。"}];
