INSERT INTO sentence [{"id":{"d":518,"n":1,"t":3},"speaker":"A","text":"どちらまで行きますか？"},{"id":{"d":518,"n":2,"t":3},"speaker":"B","text":"大阪城までお願いします。"},{"id":{"d":518,"n":3,"t":3},"speaker":"A","text":"かしこまりました。お客さまは、本日は大阪観光ですか？"},{"id":{"d":518,"n":4,"t":3},"speaker":"B","text":"そうです。大阪は初めて来ました。"},{"id":{"d":518,"n":5,"t":3},"speaker":"A","text":"大阪城の後はどちらへ行かれる予定ですか？"},{"id":{"d":518,"n":6,"t":3},"speaker":"B","text":"どこへ行こうか悩んでいるところです。"},{"id":{"d":518,"n":7,"t":3},"speaker":"A","text":"定番ですが、通天閣とアメリカ村は行かれた方がいいと思いますよ。"},{"id":{"d":518,"n":8,"t":3},"speaker":"B","text":"そうですよね。大阪城を訪ねた後で両方の場所へ行ってみます。"}];
INSERT INTO sentence [{"id":{"d":519,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。チケットをお求めですか？"},{"id":{"d":519,"n":2,"t":3},"speaker":"B","text":"はい。どのような種類のチケットがありますか？"},{"id":{"d":519,"n":3,"t":3},"speaker":"A","text":"フリーパスと回数券があります。"},{"id":{"d":519,"n":4,"t":3},"speaker":"B","text":"どちらが良いか迷います。"},{"id":{"d":519,"n":5,"t":3},"speaker":"A","text":"アトラクションにたくさん乗るようでしたら、フリーパスがお得ですよ。"},{"id":{"d":519,"n":6,"t":3},"speaker":"B","text":"アトラクションは全部で何種類ありますか？"},{"id":{"d":519,"n":7,"t":3},"speaker":"A","text":"18種類です。"},{"id":{"d":519,"n":8,"t":3},"speaker":"B","text":"一通り全部に乗ってみたいので、フリーパスにします。"},{"id":{"d":519,"n":9,"t":3},"speaker":"A","text":"お連れさまと合わせて、大人2名さま分でよろしいですか？"},{"id":{"d":519,"n":10,"t":3},"speaker":"B","text":"はい、それでお願いします。"}];
INSERT INTO sentence [{"id":{"d":520,"n":1,"t":3},"speaker":"A","text":"すみません。イルカのショーは何時から始まりますか？"},{"id":{"d":520,"n":2,"t":3},"speaker":"B","text":"11時半からです。"},{"id":{"d":520,"n":3,"t":3},"speaker":"A","text":"まだ、だいぶ時間がありますね。"},{"id":{"d":520,"n":4,"t":3},"speaker":"B","text":"11時からペンギンの餌やりができるイベントがありますよ。"},{"id":{"d":520,"n":5,"t":3},"speaker":"A","text":"いいですね。場所はどこですか？"},{"id":{"d":520,"n":6,"t":3},"speaker":"B","text":"中庭の多目的広場です。"},{"id":{"d":520,"n":7,"t":3},"speaker":"A","text":"予約は必要ありませんか？"},{"id":{"d":520,"n":8,"t":3},"speaker":"B","text":"はい。開始時間までに直接行っていただければ大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":521,"n":1,"t":3},"speaker":"A","text":"峰岸さん、体調が悪そうですね。"},{"id":{"d":521,"n":2,"t":3},"speaker":"B","text":"米国への出張から帰ったばかりで、まだ時差ぼけが治りません。"},{"id":{"d":521,"n":3,"t":3},"speaker":"A","text":"それはつらいですね。"},{"id":{"d":521,"n":4,"t":3},"speaker":"B","text":"はい。夜になっても目がさえてしまって全然眠れません。"},{"id":{"d":521,"n":5,"t":3},"speaker":"A","text":"今は、反対に眠いのですか？"},{"id":{"d":521,"n":6,"t":3},"speaker":"B","text":"そうなのです。仕事中に申し訳ありませんが、眠くて居眠りをしそうになってしまいます。"},{"id":{"d":521,"n":7,"t":3},"speaker":"A","text":"私も時差ぼけがひどい方なので、よく分かります。"},{"id":{"d":521,"n":8,"t":3},"speaker":"B","text":"段々と体が日本時間に慣れてくるのを待つより他はありませんね。"}];
INSERT INTO sentence [{"id":{"d":522,"n":1,"t":3},"speaker":"A","text":"この辺りはお土産屋さんが多いですね。"},{"id":{"d":522,"n":2,"t":3},"speaker":"B","text":"少し寄って行きましょうか？"},{"id":{"d":522,"n":3,"t":3},"speaker":"A","text":"そうですね。このお店に入りましょう。"},{"id":{"d":522,"n":4,"t":3},"speaker":"B","text":"たくさんの種類のお菓子がありますよ。"},{"id":{"d":522,"n":5,"t":3},"speaker":"A","text":"どれもおいしそうです。"},{"id":{"d":522,"n":6,"t":3},"speaker":"B","text":"両親に何か1つ買うことにします。"},{"id":{"d":522,"n":7,"t":3},"speaker":"A","text":"ご両親は甘いものがお好きですか？"},{"id":{"d":522,"n":8,"t":3},"speaker":"B","text":"はい。大好物です。"},{"id":{"d":522,"n":9,"t":3},"speaker":"A","text":"それなら、この温泉まんじゅうはいかがでしょう。"},{"id":{"d":522,"n":10,"t":3},"speaker":"B","text":"値段が手頃で良いですね。これを買うことにします。"}];
INSERT INTO sentence [{"id":{"d":523,"n":1,"t":3},"speaker":"A","text":"人力車に乗ったことはありますか？"},{"id":{"d":523,"n":2,"t":3},"speaker":"B","text":"はい。一度だけあります。"},{"id":{"d":523,"n":3,"t":3},"speaker":"A","text":"どこで乗ったのですか？"},{"id":{"d":523,"n":4,"t":3},"speaker":"B","text":"浅草です。"},{"id":{"d":523,"n":5,"t":3},"speaker":"A","text":"浅草は人力車をよく見かけますね。"},{"id":{"d":523,"n":6,"t":3},"speaker":"B","text":"結構スピードが出て、気持ち良かったです。"},{"id":{"d":523,"n":7,"t":3},"speaker":"A","text":"お1人で乗ったのですか？"},{"id":{"d":523,"n":8,"t":3},"speaker":"B","text":"いいえ。友達と一緒に乗りました。"},{"id":{"d":523,"n":9,"t":3},"speaker":"A","text":"私も乗ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":524,"n":1,"t":3},"speaker":"A","text":"すみません、このホテルの近くでまだ空いているレストランはありますか？"},{"id":{"d":524,"n":2,"t":3},"speaker":"B","text":"はい。この時間ですと、少し遠いレストランになってしまいますがよろしいですか？"},{"id":{"d":524,"n":3,"t":3},"speaker":"A","text":"はい。教えてください。"},{"id":{"d":524,"n":4,"t":3},"speaker":"B","text":"今、地図をお持ちしますね。"},{"id":{"d":524,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":524,"n":6,"t":3},"speaker":"B","text":"地図の、この辺りにイタリアンレストランがあるのですが、そちらはまだ空いております。"},{"id":{"d":524,"n":7,"t":3},"speaker":"A","text":"閉店時間は分かりますか？"},{"id":{"d":524,"n":8,"t":3},"speaker":"B","text":"はい。閉店は11時半で、ラストオーダーが11時です。"},{"id":{"d":524,"n":9,"t":3},"speaker":"A","text":"それではここに行くことにします。"},{"id":{"d":524,"n":10,"t":3},"speaker":"B","text":"お気を付けてお出掛けください。"}];
INSERT INTO sentence [{"id":{"d":525,"n":1,"t":3},"speaker":"A","text":"サービスエリアに着きましたよ。"},{"id":{"d":525,"n":2,"t":3},"speaker":"B","text":"何か軽く食べましょう。"},{"id":{"d":525,"n":3,"t":3},"speaker":"A","text":"お店がたくさんありますね。"},{"id":{"d":525,"n":4,"t":3},"speaker":"B","text":"目移りしてしまいます。"},{"id":{"d":525,"n":5,"t":3},"speaker":"A","text":"このサービスエリアは焼き鳥がおいしくて有名らしいですよ。"},{"id":{"d":525,"n":6,"t":3},"speaker":"B","text":"食べてみたいです。"},{"id":{"d":525,"n":7,"t":3},"speaker":"A","text":"焼き鳥のお店を探しましょうか？"},{"id":{"d":525,"n":8,"t":3},"speaker":"B","text":"はい。向こうに案内図がありましたよ。"},{"id":{"d":525,"n":9,"t":3},"speaker":"A","text":"見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":526,"n":1,"t":3},"speaker":"A","text":"渋滞していますね。"},{"id":{"d":526,"n":2,"t":3},"speaker":"B","text":"せっかくのドライブなのに、運が悪いですね。"},{"id":{"d":526,"n":3,"t":3},"speaker":"A","text":"本当ですね。"},{"id":{"d":526,"n":4,"t":3},"speaker":"B","text":"事故か工事が原因でしょうか？"},{"id":{"d":526,"n":5,"t":3},"speaker":"A","text":"交通情報で渋滞の原因を調べてみます。"},{"id":{"d":526,"n":6,"t":3},"speaker":"B","text":"ラジオの交通情報ですか？"},{"id":{"d":526,"n":7,"t":3},"speaker":"A","text":"いいえ。私はいつも、インターネットの交通情報サイトで調べます。"},{"id":{"d":526,"n":8,"t":3},"speaker":"B","text":"この渋滞の原因は分かりましたか？"},{"id":{"d":526,"n":9,"t":3},"speaker":"A","text":"はい。少し先で事故が発生したようです。"}];
INSERT INTO sentence [{"id":{"d":527,"n":1,"t":3},"speaker":"A","text":"サークルのメンバーで旅行に行きたいですね。"},{"id":{"d":527,"n":2,"t":3},"speaker":"B","text":"もし行くとしたら、いつ頃がいいでしょうか？"},{"id":{"d":527,"n":3,"t":3},"speaker":"A","text":"今は大学のテスト前で忙しいので、テストが終わってからがいいですよね。"},{"id":{"d":527,"n":4,"t":3},"speaker":"B","text":"テストの後は、サークルで大会に参加する予定が入っています。"},{"id":{"d":527,"n":5,"t":3},"speaker":"A","text":"それでは、大会の後がいいですね。"},{"id":{"d":527,"n":6,"t":3},"speaker":"B","text":"大会の1週間後に、三連休がありますよ。"},{"id":{"d":527,"n":7,"t":3},"speaker":"A","text":"その時なら、メンバーの都合が付きそうですね。"},{"id":{"d":527,"n":8,"t":3},"speaker":"B","text":"その三連休で、旅行を計画してみましょう。"}];
INSERT INTO sentence [{"id":{"d":528,"n":1,"t":3},"speaker":"A","text":"お姉さん、明日から林間学校ですね？"},{"id":{"d":528,"n":2,"t":3},"speaker":"B","text":"はい。とても興奮しています。"},{"id":{"d":528,"n":3,"t":3},"speaker":"A","text":"もう準備はできたのですか？"},{"id":{"d":528,"n":4,"t":3},"speaker":"B","text":"随分と前に準備して、荷物は玄関に置いてあります。"},{"id":{"d":528,"n":5,"t":3},"speaker":"A","text":"準備万端というわけですね。"},{"id":{"d":528,"n":6,"t":3},"speaker":"B","text":"あとは、天気が良いことを祈るだけです。"},{"id":{"d":528,"n":7,"t":3},"speaker":"A","text":"先ほど、テレビ番組の天気予報コーナーで気象予報士が「しばらく晴天が続く」と言っていましたよ。"},{"id":{"d":528,"n":8,"t":3},"speaker":"B","text":"それを聞いて安心しました。"}];
INSERT INTO sentence [{"id":{"d":529,"n":1,"t":3},"speaker":"A","text":"そろそろバスが出発しますが、全員そろっていますか？"},{"id":{"d":529,"n":2,"t":3},"speaker":"B","text":"先生、僕が人数を確認しましょうか？"},{"id":{"d":529,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。それでは、人数の確認をお願いします。"},{"id":{"d":529,"n":4,"t":3},"speaker":"B","text":"確認しましたが、1人足りないようです。"},{"id":{"d":529,"n":5,"t":3},"speaker":"A","text":"誰がいないのでしょうか？"},{"id":{"d":529,"n":6,"t":3},"speaker":"B","text":"相沢君です。"},{"id":{"d":529,"n":7,"t":3},"speaker":"A","text":"困りましたね。どこかで相沢君を見かけませんでしたか？"},{"id":{"d":529,"n":8,"t":3},"speaker":"B","text":"いいえ。他の人にも、相沢君のことを知らないか聞いてみます。"},{"id":{"d":529,"n":9,"t":3},"speaker":"A","text":"そうですね。先生は外に出て探します。"}];
INSERT INTO sentence [{"id":{"d":530,"n":1,"t":3},"speaker":"A","text":"ここからは外が見えますよ。"},{"id":{"d":530,"n":2,"t":3},"speaker":"B","text":"夜景がきれいですね。"},{"id":{"d":530,"n":3,"t":3},"speaker":"A","text":"そうですね。このタワーは高いので、周囲が一望できますね。"},{"id":{"d":530,"n":4,"t":3},"speaker":"B","text":"あれは東京タワーでしょうか？"},{"id":{"d":530,"n":5,"t":3},"speaker":"A","text":"はい、そうです。ライトアップされていて美しいです。"},{"id":{"d":530,"n":6,"t":3},"speaker":"B","text":"あそこにある橋もライトアップされています。"},{"id":{"d":530,"n":7,"t":3},"speaker":"A","text":"レインボーブリッジでしょうか？"},{"id":{"d":530,"n":8,"t":3},"speaker":"B","text":"そうだと思います。車がたくさん走っていますね。"}];
INSERT INTO sentence [{"id":{"d":531,"n":1,"t":3},"speaker":"A","text":"中国語の教室に通うことにしました。"},{"id":{"d":531,"n":2,"t":3},"speaker":"B","text":"どうして中国語を勉強しようと思ったのですか？"},{"id":{"d":531,"n":3,"t":3},"speaker":"A","text":"中国へ旅行をしたたいので、その前に習っておきたいのです。"},{"id":{"d":531,"n":4,"t":3},"speaker":"B","text":"旅行はいつ頃出掛けるのですか？"},{"id":{"d":531,"n":5,"t":3},"speaker":"A","text":"半年後です。"},{"id":{"d":531,"n":6,"t":3},"speaker":"B","text":"それなら、まだ勉強する時間はたくさんありますね。"},{"id":{"d":531,"n":7,"t":3},"speaker":"A","text":"ただ、レッスンは週に1回だけなのです。"},{"id":{"d":531,"n":8,"t":3},"speaker":"B","text":"教室に通うだけでなく、自宅でも勉強した方がいいですね。"},{"id":{"d":531,"n":9,"t":3},"speaker":"A","text":"はい。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":532,"n":1,"t":3},"speaker":"A","text":"この新幹線は車内販売が利用できるようですよ。"},{"id":{"d":532,"n":2,"t":3},"speaker":"B","text":"お弁当を買いそびれたので、ありがたいです。"},{"id":{"d":532,"n":3,"t":3},"speaker":"A","text":"私も軽食を買うつもりです。"},{"id":{"d":532,"n":4,"t":3},"speaker":"B","text":"お弁当は買いませんか？"},{"id":{"d":532,"n":5,"t":3},"speaker":"A","text":"今日はパンを食べたい気分なので、サンドイッチを買いたいです。"},{"id":{"d":532,"n":6,"t":3},"speaker":"B","text":"そうですか。飲み物も欲しいですね。"},{"id":{"d":532,"n":7,"t":3},"speaker":"A","text":"どのような飲み物を売っているか、販売員さんに聞いてから決めましょう。"},{"id":{"d":532,"n":8,"t":3},"speaker":"B","text":"そうですね。早く車内販売が来てほしいですね。"}];
INSERT INTO sentence [{"id":{"d":533,"n":1,"t":3},"speaker":"A","text":"給油をお願いします。"},{"id":{"d":533,"n":2,"t":3},"speaker":"B","text":"レギュラーガソリンですか？"},{"id":{"d":533,"n":3,"t":3},"speaker":"A","text":"はい。満タンにしてください。"},{"id":{"d":533,"n":4,"t":3},"speaker":"B","text":"はい、分かりました。窓を拭きましょうか？"},{"id":{"d":533,"n":5,"t":3},"speaker":"A","text":"お願いします。"},{"id":{"d":533,"n":6,"t":3},"speaker":"B","text":"給油が終わりました。料金は5,287円です。"},{"id":{"d":533,"n":7,"t":3},"speaker":"A","text":"支払いは、クレジットカードでお願いします。"},{"id":{"d":533,"n":8,"t":3},"speaker":"B","text":"クレジットカードをお預かりします。少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":534,"n":1,"t":3},"speaker":"A","text":"頂上にはまだ着かないのでしょうか？"},{"id":{"d":534,"n":2,"t":3},"speaker":"B","text":"はい。まだ半分も歩いていませんよ。"},{"id":{"d":534,"n":3,"t":3},"speaker":"A","text":"そうなのですか？先は長いですね。"},{"id":{"d":534,"n":4,"t":3},"speaker":"B","text":"つらいようなら、休憩しましょうか？"},{"id":{"d":534,"n":5,"t":3},"speaker":"A","text":"いえ、大丈夫です。もう少し頑張ります。"},{"id":{"d":534,"n":6,"t":3},"speaker":"B","text":"休憩したくなったら、いつでも声を掛けてくださいね。"},{"id":{"d":534,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":534,"n":8,"t":3},"speaker":"B","text":"それでは、先に進みましょう。"}];
INSERT INTO sentence [{"id":{"d":535,"n":1,"t":3},"speaker":"A","text":"顔色が良くないようですが、大丈夫ですか？"},{"id":{"d":535,"n":2,"t":3},"speaker":"B","text":"いいえ。気分が悪いです。"},{"id":{"d":535,"n":3,"t":3},"speaker":"A","text":"船酔いでしょうか？"},{"id":{"d":535,"n":4,"t":3},"speaker":"B","text":"そのようです。"},{"id":{"d":535,"n":5,"t":3},"speaker":"A","text":"水を飲みますか？"},{"id":{"d":535,"n":6,"t":3},"speaker":"B","text":"いえ、何も口にしたくありません。"},{"id":{"d":535,"n":7,"t":3},"speaker":"A","text":"それでは、ベンチに座って休みましょう。"},{"id":{"d":535,"n":8,"t":3},"speaker":"B","text":"はい。ご迷惑をお掛けしてすみません。"}];
INSERT INTO sentence [{"id":{"d":536,"n":1,"t":3},"speaker":"A","text":"昨日、ぶどう狩りに行きました。"},{"id":{"d":536,"n":2,"t":3},"speaker":"B","text":"ぶどうはおいしかったですか？"},{"id":{"d":536,"n":3,"t":3},"speaker":"A","text":"はい。今まで食べた中で一番おいしいぶどうでした。"},{"id":{"d":536,"n":4,"t":3},"speaker":"B","text":"自分で収穫すると、余計においしく感じますよね。"},{"id":{"d":536,"n":5,"t":3},"speaker":"A","text":"本当にそうですね。"},{"id":{"d":536,"n":6,"t":3},"speaker":"B","text":"たくさん食べられましたか？"},{"id":{"d":536,"n":7,"t":3},"speaker":"A","text":"はい。おなかがいっぱいになるまで食べました。"},{"id":{"d":536,"n":8,"t":3},"speaker":"B","text":"楽しい休日になって良かったですね。"}];
INSERT INTO sentence [{"id":{"d":537,"n":1,"t":3},"speaker":"A","text":"この公園に来たのは、初めてですね。"},{"id":{"d":537,"n":2,"t":3},"speaker":"B","text":"はい。広くて気持ちが良いですね。"},{"id":{"d":537,"n":3,"t":3},"speaker":"A","text":"フライングディスクを持ってきたので、一緒に投げ合いませんか？"},{"id":{"d":537,"n":4,"t":3},"speaker":"B","text":"いいですね。荷物はどうしましょうか？"},{"id":{"d":537,"n":5,"t":3},"speaker":"A","text":"あそこのベンチに置いておきましょう。"},{"id":{"d":537,"n":6,"t":3},"speaker":"B","text":"そうですね。それでは、フライングディスクを投げますよ。"},{"id":{"d":537,"n":7,"t":3},"speaker":"A","text":"どうぞ、投げてよいですよ。"},{"id":{"d":537,"n":8,"t":3},"speaker":"B","text":"しっかりキャッチしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":538,"n":1,"t":3},"speaker":"A","text":"今日の海は波が高いです。"},{"id":{"d":538,"n":2,"t":3},"speaker":"B","text":"そうですね。浮輪を使いますか？"},{"id":{"d":538,"n":3,"t":3},"speaker":"A","text":"はい。加えて、ゴーグルを付けたいのですが、どこにありますか？"},{"id":{"d":538,"n":4,"t":3},"speaker":"B","text":"ここにあります。どうぞ。"},{"id":{"d":538,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。私はもう一度海に入ってきますが、一緒に行きますか？"},{"id":{"d":538,"n":6,"t":3},"speaker":"B","text":"私は少し休憩します。"},{"id":{"d":538,"n":7,"t":3},"speaker":"A","text":"それでは、行ってきますね。"},{"id":{"d":538,"n":8,"t":3},"speaker":"B","text":"波に飲まれないように気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":539,"n":1,"t":3},"speaker":"A","text":"潮干狩りのシーズンですね。"},{"id":{"d":539,"n":2,"t":3},"speaker":"B","text":"潮干狩りにはシーズンがあるのですか？"},{"id":{"d":539,"n":3,"t":3},"speaker":"A","text":"はい。3月から6月くらいの期間です。"},{"id":{"d":539,"n":4,"t":3},"speaker":"B","text":"知りませんでした。"},{"id":{"d":539,"n":5,"t":3},"speaker":"A","text":"潮干狩りにはあまり行きませんか？"},{"id":{"d":539,"n":6,"t":3},"speaker":"B","text":"行きませんね。あなたはどうですか？"},{"id":{"d":539,"n":7,"t":3},"speaker":"A","text":"私はシーズン中に一度は行きますね。楽しいですよ。"},{"id":{"d":539,"n":8,"t":3},"speaker":"B","text":"私も興味が湧いてきました。"}];
INSERT INTO sentence [{"id":{"d":540,"n":1,"t":3},"speaker":"A","text":"今の交差点は左折して良かったのですか？"},{"id":{"d":540,"n":2,"t":3},"speaker":"B","text":"はい。直進すると思いましたか？"},{"id":{"d":540,"n":3,"t":3},"speaker":"A","text":"はい。カーナビゲーションシステムが直進するように指示していたので。"},{"id":{"d":540,"n":4,"t":3},"speaker":"B","text":"さっきの交差点を直進すると、渋滞に巻き込まれることが多いのですよ。"},{"id":{"d":540,"n":5,"t":3},"speaker":"A","text":"この道は抜け道ということですか？"},{"id":{"d":540,"n":6,"t":3},"speaker":"B","text":"はい。この道はいつもすいています。"},{"id":{"d":540,"n":7,"t":3},"speaker":"A","text":"詳しいですね。"},{"id":{"d":540,"n":8,"t":3},"speaker":"B","text":"この近くに住んでいたことがあるのですよ。"}];
INSERT INTO sentence [{"id":{"d":541,"n":1,"t":3},"speaker":"A","text":"ようやく頂上に着きました。"},{"id":{"d":541,"n":2,"t":3},"speaker":"B","text":"疲れましたね。"},{"id":{"d":541,"n":3,"t":3},"speaker":"A","text":"もう疲れ果てました。"},{"id":{"d":541,"n":4,"t":3},"speaker":"B","text":"それに、おなかがすきました。"},{"id":{"d":541,"n":5,"t":3},"speaker":"A","text":"早速、お弁当を食べましょう。"},{"id":{"d":541,"n":6,"t":3},"speaker":"B","text":"ゆっくり座れる所で食べたいですね。"},{"id":{"d":541,"n":7,"t":3},"speaker":"A","text":"向こうに小屋がありますね。"},{"id":{"d":541,"n":8,"t":3},"speaker":"B","text":"行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":542,"n":1,"t":3},"speaker":"A","text":"大きな船が停泊していますね。"},{"id":{"d":542,"n":2,"t":3},"speaker":"B","text":"近くで見ると圧倒されてしまいます。"},{"id":{"d":542,"n":3,"t":3},"speaker":"A","text":"このような船に乗ってみたいです。"},{"id":{"d":542,"n":4,"t":3},"speaker":"B","text":"私も憧れます。"},{"id":{"d":542,"n":5,"t":3},"speaker":"A","text":"もしこのような船に乗れたら、どこに行きたいですか？"},{"id":{"d":542,"n":6,"t":3},"speaker":"B","text":"ぜひ外国へ行ってみたいです。シンガポールとやフィリピンなど、挙げたら終わりがありません。"},{"id":{"d":542,"n":7,"t":3},"speaker":"A","text":"私は日本一周したいです。"},{"id":{"d":542,"n":8,"t":3},"speaker":"B","text":"それも楽しそうです。夢は広がりますね。"}];
INSERT INTO sentence [{"id":{"d":543,"n":1,"t":3},"speaker":"A","text":"この写真はどこで撮ったのですか？"},{"id":{"d":543,"n":2,"t":3},"speaker":"B","text":"メキシコです。大学の卒業旅行で訪問しました。ヒッチハイクして、自動車に乗せてもらった時に撮りました。"},{"id":{"d":543,"n":3,"t":3},"speaker":"A","text":"ヒッチハイクは、すぐにできましたか？"},{"id":{"d":543,"n":4,"t":3},"speaker":"B","text":"2時間くらいかかり、ようやく自動車に乗せてもらえました。"},{"id":{"d":543,"n":5,"t":3},"speaker":"A","text":"それは大変でしたね。"},{"id":{"d":543,"n":6,"t":3},"speaker":"B","text":"今となっては、良い思い出です。"}];
INSERT INTO sentence [{"id":{"d":544,"n":1,"t":3},"speaker":"A","text":"湖に着きましたよ。"},{"id":{"d":544,"n":2,"t":3},"speaker":"B","text":"きれいな湖ですね。"},{"id":{"d":544,"n":3,"t":3},"speaker":"A","text":"遊覧船が見えます。乗ってみますか？"},{"id":{"d":544,"n":4,"t":3},"speaker":"B","text":"あちらには手こぎのボートがありますよ。どちらに乗りましょうか？"},{"id":{"d":544,"n":5,"t":3},"speaker":"A","text":"手こぎボートに乗ってみたいですが、うまくこぐ自信がありません。"},{"id":{"d":544,"n":6,"t":3},"speaker":"B","text":"それなら、私がこぎますよ。"},{"id":{"d":544,"n":7,"t":3},"speaker":"A","text":"本当ですか？うれしいです。"},{"id":{"d":544,"n":8,"t":3},"speaker":"B","text":"それではボート乗り場に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":545,"n":1,"t":3},"speaker":"A","text":"お客さまのお部屋はこちらです。"},{"id":{"d":545,"n":2,"t":3},"speaker":"B","text":"広いですね。"},{"id":{"d":545,"n":3,"t":3},"speaker":"A","text":"奥に縁側もあります。"},{"id":{"d":545,"n":4,"t":3},"speaker":"B","text":"本当ですね。景色も良いです。"},{"id":{"d":545,"n":5,"t":3},"speaker":"A","text":"海が一望できるようになっております。"},{"id":{"d":545,"n":6,"t":3},"speaker":"B","text":"良いですね。お手洗いはどこですか？"},{"id":{"d":545,"n":7,"t":3},"speaker":"A","text":"入口の近くにございます。あちらの引き出しには浴衣が入っていますので、大浴場をご利用の際にお持ちください。"},{"id":{"d":545,"n":8,"t":3},"speaker":"B","text":"分かりました。浴衣もすてきですね。"}];
INSERT INTO sentence [{"id":{"d":546,"n":1,"t":3},"speaker":"A","text":"ガイドブックによると、この近くに神社があるようです。"},{"id":{"d":546,"n":2,"t":3},"speaker":"B","text":"寄って行きましょうか？"},{"id":{"d":546,"n":3,"t":3},"speaker":"A","text":"そうですね。今、私たちは、この地図上のどこにいるか分かりますか？"},{"id":{"d":546,"n":4,"t":3},"speaker":"B","text":"目印になるものが少なくて、分かりにくいですね。"},{"id":{"d":546,"n":5,"t":3},"speaker":"A","text":"先ほど休憩したのは、地図のこの辺だと思います。"},{"id":{"d":546,"n":6,"t":3},"speaker":"B","text":"そうすると、今は神社のすぐ近くにいることになりますね。"},{"id":{"d":546,"n":7,"t":3},"speaker":"A","text":"この交差点を右に曲がったら、神社が見えてきそうですね。"},{"id":{"d":546,"n":8,"t":3},"speaker":"B","text":"行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":547,"n":1,"t":3},"speaker":"A","text":"ドイツへの出張はどうでしたか？"},{"id":{"d":547,"n":2,"t":3},"speaker":"B","text":"毎日予定が詰まっていて、忙しかったです。"},{"id":{"d":547,"n":3,"t":3},"speaker":"A","text":"会議がたくさん行われたようですね。"},{"id":{"d":547,"n":4,"t":3},"speaker":"B","text":"はい。1日に3回行われたこともあります。"},{"id":{"d":547,"n":5,"t":3},"speaker":"A","text":"ドイツ語の会議に参加するだけで大変そうです。"},{"id":{"d":547,"n":6,"t":3},"speaker":"B","text":"専門用語が多くて苦労しました。"},{"id":{"d":547,"n":7,"t":3},"speaker":"A","text":"出張中に少しは観光もできましたか？"},{"id":{"d":547,"n":8,"t":3},"speaker":"B","text":"はい。最終日に数時間だけ市内観光をしました。"}];
INSERT INTO sentence [{"id":{"d":548,"n":1,"t":3},"speaker":"A","text":"すみませんが、写真を撮っていただけますか？"},{"id":{"d":548,"n":2,"t":3},"speaker":"B","text":"よいですよ。どこで撮りますか？"},{"id":{"d":548,"n":3,"t":3},"speaker":"A","text":"このタワーを背景にして撮りたいです。"},{"id":{"d":548,"n":4,"t":3},"speaker":"B","text":"分かりました。では、撮りますので、あちらに並んでください。"},{"id":{"d":548,"n":5,"t":3},"speaker":"A","text":"はい、お願いします。"},{"id":{"d":548,"n":6,"t":3},"speaker":"B","text":"その場所だと日陰になってしまうので、もう少し左へ動いてください。"},{"id":{"d":548,"n":7,"t":3},"speaker":"A","text":"分かりました。ここでどうですか？"},{"id":{"d":548,"n":8,"t":3},"speaker":"B","text":"大丈夫です。それでは撮ります。はい、チーズ。"}];
INSERT INTO sentence [{"id":{"d":549,"n":1,"t":3},"speaker":"A","text":"来月、遠足があります。"},{"id":{"d":549,"n":2,"t":3},"speaker":"B","text":"どこへ行くのですか？"},{"id":{"d":549,"n":3,"t":3},"speaker":"A","text":"大浜公園です。"},{"id":{"d":549,"n":4,"t":3},"speaker":"B","text":"結構遠いですね。バスに乗って行くのですか？"},{"id":{"d":549,"n":5,"t":3},"speaker":"A","text":"行きは歩きで、帰りはバスに乗ります。"},{"id":{"d":549,"n":6,"t":3},"speaker":"B","text":"片道だけでも、歩くのは大変そうです。"},{"id":{"d":549,"n":7,"t":3},"speaker":"A","text":"学校で、「歩きやすい靴を履いて来るように」と話がありました。"},{"id":{"d":549,"n":8,"t":3},"speaker":"B","text":"履きなれた靴で参加しましょう。"}];
INSERT INTO sentence [{"id":{"d":550,"n":1,"t":3},"speaker":"A","text":"新婚旅行はどうしましょうか？"},{"id":{"d":550,"n":2,"t":3},"speaker":"B","text":"行きたいのですが、しばらく仕事が忙しくて休みを取れません。"},{"id":{"d":550,"n":3,"t":3},"speaker":"A","text":"いつ頃なら、大丈夫ですか？"},{"id":{"d":550,"n":4,"t":3},"speaker":"B","text":"今のプロジェクトが9月に終わるので、10月以降なら休みが取れると思います。"},{"id":{"d":550,"n":5,"t":3},"speaker":"A","text":"10月なら、私の仕事も調整が付きます。"},{"id":{"d":550,"n":6,"t":3},"speaker":"B","text":"それでは、10月に新婚旅行をすることにしましょう。"},{"id":{"d":550,"n":7,"t":3},"speaker":"A","text":"行先はどこがいいですか？"},{"id":{"d":550,"n":8,"t":3},"speaker":"B","text":"時間があるので、ゆっくり考えましょう。"}];
INSERT INTO sentence [{"id":{"d":551,"n":1,"t":3},"speaker":"A","text":"はい、セントラルホテルです。"},{"id":{"d":551,"n":2,"t":3},"speaker":"B","text":"もしもし。少々お伺いしたいのですが。"},{"id":{"d":551,"n":3,"t":3},"speaker":"A","text":"どのようなことでしょうか？"},{"id":{"d":551,"n":4,"t":3},"speaker":"B","text":"そちらのホテルに宿泊したら、駐車場は利用できますか？"},{"id":{"d":551,"n":5,"t":3},"speaker":"A","text":"はい。1泊につき1,500円でご利用いただけます。"},{"id":{"d":551,"n":6,"t":3},"speaker":"B","text":"教えていただいてありがとうございます。"},{"id":{"d":551,"n":7,"t":3},"speaker":"A","text":"駐車できる台数に限りがありますので、事前のご予約をお勧めします。"},{"id":{"d":551,"n":8,"t":3},"speaker":"B","text":"分かりました。宿泊する日程が決まったら、駐車場も予約します。"}];
INSERT INTO sentence [{"id":{"d":552,"n":1,"t":3},"speaker":"A","text":"テレビで旅番組を放送していますよ。"},{"id":{"d":552,"n":2,"t":3},"speaker":"B","text":"どこを旅しているのですか？"},{"id":{"d":552,"n":3,"t":3},"speaker":"A","text":"沖縄です。"},{"id":{"d":552,"n":4,"t":3},"speaker":"B","text":"海がきれいですね。"},{"id":{"d":552,"n":5,"t":3},"speaker":"A","text":"もう泳いでいる人がいますね。"},{"id":{"d":552,"n":6,"t":3},"speaker":"B","text":"同じ日本でも、随分気候が違いますね。"},{"id":{"d":552,"n":7,"t":3},"speaker":"A","text":"このようにきれいな海で泳いでみたいです。"},{"id":{"d":552,"n":8,"t":3},"speaker":"B","text":"いつか、一緒に行きたいですね。"}];
INSERT INTO sentence [{"id":{"d":553,"n":1,"t":3},"speaker":"A","text":"新しいバイクを買いました。"},{"id":{"d":553,"n":2,"t":3},"speaker":"B","text":"どのようなバイクですか？"},{"id":{"d":553,"n":3,"t":3},"speaker":"A","text":"色は黒で、中型です。"},{"id":{"d":553,"n":4,"t":3},"speaker":"B","text":"乗り心地はどうですか？"},{"id":{"d":553,"n":5,"t":3},"speaker":"A","text":"初めて乗る時は遠出したいので、まだ乗っていません。"},{"id":{"d":553,"n":6,"t":3},"speaker":"B","text":"そうなのですか。楽しみは取っておくのですね。"},{"id":{"d":553,"n":7,"t":3},"speaker":"A","text":"はい。どこへ行くかを考えるだけで楽しいです。"},{"id":{"d":553,"n":8,"t":3},"speaker":"B","text":"実際に遠出したら、感想を聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":554,"n":1,"t":3},"speaker":"A","text":"1泊2日の旅行なのに、随分荷物が多いですね。"},{"id":{"d":554,"n":2,"t":3},"speaker":"B","text":"必要になりそうな物を詰めていたら、この量になっていました。"},{"id":{"d":554,"n":3,"t":3},"speaker":"A","text":"私は着替えと洗面用具くらいしか入っていません。"},{"id":{"d":554,"n":4,"t":3},"speaker":"B","text":"私は雨具や防寒具、予備の着替えやパジャマなども持って来ました。"},{"id":{"d":554,"n":5,"t":3},"speaker":"A","text":"そのように大きなバッグだと、重くありませんか？"},{"id":{"d":554,"n":6,"t":3},"speaker":"B","text":"そうですね。かなりの重量です。"},{"id":{"d":554,"n":7,"t":3},"speaker":"A","text":"帰りはお土産が増えて、さらに重くなるかもしれませんよ。"},{"id":{"d":554,"n":8,"t":3},"speaker":"B","text":"もしそうなったら、宅配便で送るつもりです。"}];
INSERT INTO sentence [{"id":{"d":555,"n":1,"t":3},"speaker":"A","text":"石野さんは関西の出身ですか？"},{"id":{"d":555,"n":2,"t":3},"speaker":"B","text":"はい。どうして分かったのですか？"},{"id":{"d":555,"n":3,"t":3},"speaker":"A","text":"言葉に関西弁のイントネーションがあるからです。"},{"id":{"d":555,"n":4,"t":3},"speaker":"B","text":"自分では完全な標準語を話しているつもりでした。"},{"id":{"d":555,"n":5,"t":3},"speaker":"A","text":"関西弁は使わないようにしているのですか？"},{"id":{"d":555,"n":6,"t":3},"speaker":"B","text":"はい。職場では控えた方がいいかと思いまして、標準語を話すようにしています。"},{"id":{"d":555,"n":7,"t":3},"speaker":"A","text":"気にせずに、関西弁で会話をしても大丈夫だと思いますよ。"},{"id":{"d":555,"n":8,"t":3},"speaker":"B","text":"慣れてしまったせいか、今となっては会社にいると自然と関西弁が出てきません。"}];
INSERT INTO sentence [{"id":{"d":556,"n":1,"t":3},"speaker":"A","text":"自転車の修理をお願いできますか？"},{"id":{"d":556,"n":2,"t":3},"speaker":"B","text":"はい。どうしたのですか？"},{"id":{"d":556,"n":3,"t":3},"speaker":"A","text":"サイクリングをしていたら、自転車の調子が悪くなってしまったのです。"},{"id":{"d":556,"n":4,"t":3},"speaker":"B","text":"それはお困りですね。自転車を見せてください。"},{"id":{"d":556,"n":5,"t":3},"speaker":"A","text":"これです。ペダルをこいでも、いつも通りに進みません。"},{"id":{"d":556,"n":6,"t":3},"speaker":"B","text":"これはタイヤがパンクしている上に、チェーンの具合も悪いですね。"},{"id":{"d":556,"n":7,"t":3},"speaker":"A","text":"修理をしたら直りますか？"},{"id":{"d":556,"n":8,"t":3},"speaker":"B","text":"はい。少し時間がかかりますが直りますよ。"}];
INSERT INTO sentence [{"id":{"d":557,"n":1,"t":3},"speaker":"A","text":"魚拓が飾ってありますね。"},{"id":{"d":557,"n":2,"t":3},"speaker":"B","text":"以前、私が釣った魚です。"},{"id":{"d":557,"n":3,"t":3},"speaker":"A","text":"釣りが趣味なのですね。"},{"id":{"d":557,"n":4,"t":3},"speaker":"B","text":"はい。休みの日は大抵釣りに出掛けています。"},{"id":{"d":557,"n":5,"t":3},"speaker":"A","text":"これはタイですか？"},{"id":{"d":557,"n":6,"t":3},"speaker":"B","text":"はい。イシダイという種類です。"},{"id":{"d":557,"n":7,"t":3},"speaker":"A","text":"大物ですね。"},{"id":{"d":557,"n":8,"t":3},"speaker":"B","text":"これ以上の大きさのイシダイは、いまだに釣ったことがありません。"}];
INSERT INTO sentence [{"id":{"d":558,"n":1,"t":3},"speaker":"A","text":"天気が良いので、デッキに出てみましょうか？"},{"id":{"d":558,"n":2,"t":3},"speaker":"B","text":"そうですね。行きましょう。"},{"id":{"d":558,"n":3,"t":3},"speaker":"A","text":"風が強いですね。"},{"id":{"d":558,"n":4,"t":3},"speaker":"B","text":"はい。風に帽子を飛ばされそうです。"},{"id":{"d":558,"n":5,"t":3},"speaker":"A","text":"遠くに船が見えます。"},{"id":{"d":558,"n":6,"t":3},"speaker":"B","text":"本当ですね。手を振ってみましょうか？"},{"id":{"d":558,"n":7,"t":3},"speaker":"A","text":"はい。あちらの船の乗客が気付くでしょうか？"},{"id":{"d":558,"n":8,"t":3},"speaker":"B","text":"気が付いたら、手を振り返してくれるかもしれませんよ。"}];
INSERT INTO sentence [{"id":{"d":559,"n":1,"t":3},"speaker":"A","text":"週末に東京へ遊びに行きます。"},{"id":{"d":559,"n":2,"t":3},"speaker":"B","text":"新幹線で行くのですか？"},{"id":{"d":559,"n":3,"t":3},"speaker":"A","text":"いいえ。交通費を安く済ませたいので、夜行バスで行きます。"},{"id":{"d":559,"n":4,"t":3},"speaker":"B","text":"東京には何時ごろ着くのですか？"},{"id":{"d":559,"n":5,"t":3},"speaker":"A","text":"朝の5時半です。"},{"id":{"d":559,"n":6,"t":3},"speaker":"B","text":"早朝ですね。"},{"id":{"d":559,"n":7,"t":3},"speaker":"A","text":"はい。観光するにもお店が開いていないので、しばらく時間を調整する必要があります。"},{"id":{"d":559,"n":8,"t":3},"speaker":"B","text":"マンガ喫茶や24時間営業のファミリーレストランなら、時間の調整のために使えそうですね。"}];
INSERT INTO sentence [{"id":{"d":560,"n":1,"t":3},"speaker":"A","text":"せっかくイタリアに来たので、何か名物を食べたいですね。"},{"id":{"d":560,"n":2,"t":3},"speaker":"B","text":"イタリアの名物というと、何でしょうか？"},{"id":{"d":560,"n":3,"t":3},"speaker":"A","text":"パスタかピザが思い浮かびます。"},{"id":{"d":560,"n":4,"t":3},"speaker":"B","text":"そうですね。どちらも食べたいです。"},{"id":{"d":560,"n":5,"t":3},"speaker":"A","text":"それでは1品ずつ頼んで、分け合いましょう。"},{"id":{"d":560,"n":6,"t":3},"speaker":"B","text":"パスタもピザもどちらも種類がたくさんあって、どれにするか迷ってしまいます。"},{"id":{"d":560,"n":7,"t":3},"speaker":"A","text":"本当ですね。どれにしましょうか？"},{"id":{"d":560,"n":8,"t":3},"speaker":"B","text":"日本では食べられないような種類にしましょう。"}];
INSERT INTO sentence [{"id":{"d":561,"n":1,"t":3},"speaker":"A","text":"もしもし、パパですか？今、シドニーから電話しています。"},{"id":{"d":561,"n":2,"t":3},"speaker":"B","text":"はい。真紀は、無事にシドニーに着いたのですね？"},{"id":{"d":561,"n":3,"t":3},"speaker":"A","text":"はい。今、ホテルに着いたところです。"},{"id":{"d":561,"n":4,"t":3},"speaker":"B","text":"そちらは夏ですよね。暑いですか？"},{"id":{"d":561,"n":5,"t":3},"speaker":"A","text":"そうですね、日差しが強くて暑いです。"},{"id":{"d":561,"n":6,"t":3},"speaker":"B","text":"日本と季節が逆なので、体調を崩さないように気を付けてくださいね。"},{"id":{"d":561,"n":7,"t":3},"speaker":"A","text":"はい。気を付けます。今日はこれから市内観光に行く予定です。"},{"id":{"d":561,"n":8,"t":3},"speaker":"B","text":"良いですね。写真を撮ったら、帰国後に見せてください。"}];
INSERT INTO sentence [{"id":{"d":562,"n":1,"t":3},"speaker":"A","text":"このホテルはアメニティーが豪華ですよ。"},{"id":{"d":562,"n":2,"t":3},"speaker":"B","text":"どのようなアメニティーですか？"},{"id":{"d":562,"n":3,"t":3},"speaker":"A","text":"化粧水やクリームなどが全て、有名ブランド品です。"},{"id":{"d":562,"n":4,"t":3},"speaker":"B","text":"本当ですね。それに、種類がたくさんあります。"},{"id":{"d":562,"n":5,"t":3},"speaker":"A","text":"浴室や洗面所も広々しています。"},{"id":{"d":562,"n":6,"t":3},"speaker":"B","text":"そうですね。おまけに浴槽にはジャグジーが付いています。"},{"id":{"d":562,"n":7,"t":3},"speaker":"A","text":"ぜいたくな気分になりますね。"},{"id":{"d":562,"n":8,"t":3},"speaker":"B","text":"いつもより高級なホテルに決めて良かったです。"}];
INSERT INTO sentence [{"id":{"d":563,"n":1,"t":3},"speaker":"A","text":"動物園の入り口が閉まっています。"},{"id":{"d":563,"n":2,"t":3},"speaker":"B","text":"開園時間は過ぎているのに、おかしいですね。"},{"id":{"d":563,"n":3,"t":3},"speaker":"A","text":"入口に何かお知らせが掲示されているようです。"},{"id":{"d":563,"n":4,"t":3},"speaker":"B","text":"何が書いてありますか？"},{"id":{"d":563,"n":5,"t":3},"speaker":"A","text":"「臨時休園」と書いてあります。"},{"id":{"d":563,"n":6,"t":3},"speaker":"B","text":"休みとは、驚きですね。"},{"id":{"d":563,"n":7,"t":3},"speaker":"A","text":"せっかく来たのに、残念です。"},{"id":{"d":563,"n":8,"t":3},"speaker":"B","text":"この近くで、どこか観光できる所を探しましょう。"}];
INSERT INTO sentence [{"id":{"d":564,"n":1,"t":3},"speaker":"A","text":"搭乗まで時間がありますね。"},{"id":{"d":564,"n":2,"t":3},"speaker":"B","text":"免税店で買い物をしましょうか？"},{"id":{"d":564,"n":3,"t":3},"speaker":"A","text":"はい。父へのお土産を買いたいです。"},{"id":{"d":564,"n":4,"t":3},"speaker":"B","text":"免税店ならネクタイやベルトなどが売っていますよ。"},{"id":{"d":564,"n":5,"t":3},"speaker":"A","text":"ちょうど父がネクタイを欲しがっていたので、それをお土産にします。"},{"id":{"d":564,"n":6,"t":3},"speaker":"B","text":"お父さまの好みに合うネクタイがあると良いですね。"},{"id":{"d":564,"n":7,"t":3},"speaker":"A","text":"はい。北さんは何を買いますか？"},{"id":{"d":564,"n":8,"t":3},"speaker":"B","text":"私は職場へのお土産を買います。"}];
INSERT INTO sentence [{"id":{"d":565,"n":1,"t":3},"speaker":"A","text":"手荷物検査にご協力お願いします。"},{"id":{"d":565,"n":2,"t":3},"speaker":"B","text":"ここにバッグを置けば良いですか？"},{"id":{"d":565,"n":3,"t":3},"speaker":"A","text":"はい。中身を確認させていただきます。バッグの中のポーチも開けてよいでしょうか？"},{"id":{"d":565,"n":4,"t":3},"speaker":"B","text":"どうぞ。"},{"id":{"d":565,"n":5,"t":3},"speaker":"A","text":"すみませんが、ポーチの中のハサミは持ち込みができませんので、預からせていただきます。"},{"id":{"d":565,"n":6,"t":3},"speaker":"B","text":"後で返してもらえますか？"},{"id":{"d":565,"n":7,"t":3},"speaker":"A","text":"はい。お帰りの際に受付にて返却させていただきます。"},{"id":{"d":565,"n":8,"t":3},"speaker":"B","text":"分かりました。"},{"id":{"d":565,"n":9,"t":3},"speaker":"A","text":"ご協力いただき、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":566,"n":1,"t":3},"speaker":"A","text":"この地方の名物をお土産に買いたいのですが、どのような物がありますか？"},{"id":{"d":566,"n":2,"t":3},"speaker":"B","text":"この辺りは日本酒の名産地なので、さまざまな種類の日本酒が取りそろえてあります。"},{"id":{"d":566,"n":3,"t":3},"speaker":"A","text":"飲みやすいのはどれでしょうか？"},{"id":{"d":566,"n":4,"t":3},"speaker":"B","text":"こちらの純米酒は、軽くて飲みやすいとご好評を頂いております。"},{"id":{"d":566,"n":5,"t":3},"speaker":"A","text":"冷やと熱かんと、どちらが合いますか？"},{"id":{"d":566,"n":6,"t":3},"speaker":"B","text":"どちらもおいしいですが、冷やでお飲みいただいた方が、口当たりが良いと思います。"},{"id":{"d":566,"n":7,"t":3},"speaker":"A","text":"こちらの品だと大きくて重いので、もう少し小さいサイズはありませんか？"},{"id":{"d":566,"n":8,"t":3},"speaker":"B","text":"はい。この半分のサイズの物があります。"},{"id":{"d":566,"n":9,"t":3},"speaker":"A","text":"では、その小さい方を1本下さい。"}];
INSERT INTO sentence [{"id":{"d":567,"n":1,"t":3},"speaker":"A","text":"ここの温泉はにごり湯ですね。"},{"id":{"d":567,"n":2,"t":3},"speaker":"B","text":"私は濁り湯が大好きなので、うれしいです。"},{"id":{"d":567,"n":3,"t":3},"speaker":"A","text":"温泉の効能は何でしょうね？"},{"id":{"d":567,"n":4,"t":3},"speaker":"B","text":"ここに書いてあります。こちらの温泉の効能には、疲労回復、冷え性の改善、動脈硬化の予防などがありますよ。"},{"id":{"d":567,"n":5,"t":3},"speaker":"A","text":"美肌効果もあると書かれています。"},{"id":{"d":567,"n":6,"t":3},"speaker":"B","text":"来たかいがありますね。"},{"id":{"d":567,"n":7,"t":3},"speaker":"A","text":"そうですね。ゆっくり漬かって、温泉を満喫しましょう。"},{"id":{"d":567,"n":8,"t":3},"speaker":"B","text":"混んでいるのではないかと心配でしたが、空いているのでくつろげます。"}];
INSERT INTO sentence [{"id":{"d":568,"n":1,"t":3},"speaker":"A","text":"雨が降ってきました。"},{"id":{"d":568,"n":2,"t":3},"speaker":"B","text":"天気予報では「一日中晴れ」とのことでしたが、外れてしまいましたね。"},{"id":{"d":568,"n":3,"t":3},"speaker":"A","text":"傘は持って来ましたか？"},{"id":{"d":568,"n":4,"t":3},"speaker":"B","text":"いいえ。荷物を少なくしたかったので、持って来ませんでした。"},{"id":{"d":568,"n":5,"t":3},"speaker":"A","text":"私は折り畳み傘を持って来ました。ひとまず、この傘を二人で使いましょう。"},{"id":{"d":568,"n":6,"t":3},"speaker":"B","text":"すみません。どこか、雨宿りできる所を探しましょうか？"},{"id":{"d":568,"n":7,"t":3},"speaker":"A","text":"そうですね。休憩ができる所があると良いですね。"},{"id":{"d":568,"n":8,"t":3},"speaker":"B","text":"地図を見てみます。"}];
INSERT INTO sentence [{"id":{"d":569,"n":1,"t":3},"speaker":"A","text":"私は旅行会社で働いています。"},{"id":{"d":569,"n":2,"t":3},"speaker":"B","text":"どのような仕事をしているのですか？"},{"id":{"d":569,"n":3,"t":3},"speaker":"A","text":"パックツアーの企画をしています。"},{"id":{"d":569,"n":4,"t":3},"speaker":"B","text":"新しい企画を作るのは、大変そうですね。"},{"id":{"d":569,"n":5,"t":3},"speaker":"A","text":"毎月かなりの数の企画が必要なので、アイデアが浮かばないと苦労します。"},{"id":{"d":569,"n":6,"t":3},"speaker":"B","text":"予算の制約もありますよね。"},{"id":{"d":569,"n":7,"t":3},"speaker":"A","text":"そうなのです。いかに低予算でいかに魅力的なツアーを企画するかが、大きな課題です。"},{"id":{"d":569,"n":8,"t":3},"speaker":"B","text":"でも、やりがいがありそうです。"},{"id":{"d":569,"n":9,"t":3},"speaker":"A","text":"はい。今の仕事にとても満足しています。"}];
