INSERT INTO sentence [{"id":{"d":875,"n":1,"t":3},"speaker":"A","text":"卒業旅行でヨーロッパを訪問しませんか？"},{"id":{"d":875,"n":2,"t":3},"speaker":"B","text":"私はパスポートが取れないので、無理かもしれません。"},{"id":{"d":875,"n":3,"t":3},"speaker":"A","text":"なぜパスポートが取れないのですか？"},{"id":{"d":875,"n":4,"t":3},"speaker":"B","text":"住民票が実家の住所のままなので、実家まで取りに帰らないといけません。でも、そのような暇はありません。"},{"id":{"d":875,"n":5,"t":3},"speaker":"A","text":"確か、学生は住んでいるところでもパスポートを取れると聞いたことがありますよ。"},{"id":{"d":875,"n":6,"t":3},"speaker":"B","text":"そうなのですか？初耳です。"},{"id":{"d":875,"n":7,"t":3},"speaker":"A","text":"私も少し聞いただけですから、パスポートセンターに問い合わせてみてはいかがでしょうか？"},{"id":{"d":875,"n":8,"t":3},"speaker":"B","text":"そうします。パスポートが取れるようでしたら、憧れのヨーロッパ旅行にぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":876,"n":1,"t":3},"speaker":"A","text":"藍染体験は楽しかったですね。"},{"id":{"d":876,"n":2,"t":3},"speaker":"B","text":"楽しかったですけれど、結構大変でした。"},{"id":{"d":876,"n":3,"t":3},"speaker":"A","text":"染めるものをストールにしたからでしょうね。私のようにハンカチであれば比較的楽だったと思いますよ。"},{"id":{"d":876,"n":4,"t":3},"speaker":"B","text":"旅の荷物に防寒具を入れるのを忘れたので、ストールが欲しかったのです。"},{"id":{"d":876,"n":5,"t":3},"speaker":"A","text":"すぐに使うためにストールにしたのですね。"},{"id":{"d":876,"n":6,"t":3},"speaker":"B","text":"東京と違って、この辺りは寒いですから。"},{"id":{"d":876,"n":7,"t":3},"speaker":"A","text":"今、使っているそのストールは温かいですか？"},{"id":{"d":876,"n":8,"t":3},"speaker":"B","text":"はい。大判のストールにして正解でした。"}];
INSERT INTO sentence [{"id":{"d":877,"n":1,"t":3},"speaker":"A","text":"足湯がありますよ。入っていきませんか？"},{"id":{"d":877,"n":2,"t":3},"speaker":"B","text":"いいえ。私は遠慮しておきます。"},{"id":{"d":877,"n":3,"t":3},"speaker":"A","text":"なぜですか？歩き疲れていませんか？"},{"id":{"d":877,"n":4,"t":3},"speaker":"B","text":"歩き疲れてはいますが、ストッキングを履いているので脱ぐことが難しいかと思いまして。靴下であったならば、すぐに脱いで入りたいところです。"},{"id":{"d":877,"n":5,"t":3},"speaker":"A","text":"ストッキングだったのですね。それでは足湯には入れませんね。"},{"id":{"d":877,"n":6,"t":3},"speaker":"B","text":"漬からないで隣に座りますから、あなたは遠慮せずに入って良いですよ。"},{"id":{"d":877,"n":7,"t":3},"speaker":"A","text":"一人だけ足湯に入るのは申し訳ないです。"},{"id":{"d":877,"n":8,"t":3},"speaker":"B","text":"どうぞ、私に遠慮しないでください。私の方は座れるだけで疲れが取れますから大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":878,"n":1,"t":3},"speaker":"A","text":"デパートで駅弁フェアを実施していたので、みんなの分も買ってきましたよ。"},{"id":{"d":878,"n":2,"t":3},"speaker":"B","text":"おいしそうですね。どれを取っても良いのですか？"},{"id":{"d":878,"n":3,"t":3},"speaker":"A","text":"はい。私はこの牛タン弁当を食べたいと思って買ってきました。それ以外の駅弁は早い者勝ちです。"},{"id":{"d":878,"n":4,"t":3},"speaker":"B","text":"では、私は鶏わっぱめしにします。"},{"id":{"d":878,"n":5,"t":3},"speaker":"A","text":"分かりました。はい、どうぞ。"},{"id":{"d":878,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。せっかくなので車窓の旅のDVDを鑑賞しながら食べませんか？"},{"id":{"d":878,"n":7,"t":3},"speaker":"A","text":"それは良いですね。電車に乗っている気分になれそうです。"},{"id":{"d":878,"n":8,"t":3},"speaker":"B","text":"みんなに駅弁が行き渡ったら、DVDを再生しましょう。"}];
INSERT INTO sentence [{"id":{"d":879,"n":1,"t":3},"speaker":"A","text":"棚田というのはきれいなものなのですね。"},{"id":{"d":879,"n":2,"t":3},"speaker":"B","text":"昔の人の努力の結晶ですからね。"},{"id":{"d":879,"n":3,"t":3},"speaker":"A","text":"このように人里離れた場所に旅行に来ても、見るところなどないと思っていましたよ。"},{"id":{"d":879,"n":4,"t":3},"speaker":"B","text":"でも見るところはありましたでしょう？先ほどから写真をたくさん撮っていますものね。"},{"id":{"d":879,"n":5,"t":3},"speaker":"A","text":"はい。昨夜泊まった旅館の温泉も素晴らしかったですし、食事もおいしかったです。"},{"id":{"d":879,"n":6,"t":3},"speaker":"B","text":"気に入ってくれて良かったです。"},{"id":{"d":879,"n":7,"t":3},"speaker":"A","text":"いつもの旅行はアクティビティにばかり目が行っていました。"},{"id":{"d":879,"n":8,"t":3},"speaker":"B","text":"たまには、このようなのんびりした旅行も良いでしょう？"}];
INSERT INTO sentence [{"id":{"d":880,"n":1,"t":3},"speaker":"A","text":"課長、佐久間さんからお電話がありました。"},{"id":{"d":880,"n":2,"t":3},"speaker":"B","text":"佐久間君は今、出張中で広島にいますよね。"},{"id":{"d":880,"n":3,"t":3},"speaker":"A","text":"はい。出張先でトラブルが起きたようで、明日は帰社の予定でしたが帰れなくなったとのことです。"},{"id":{"d":880,"n":4,"t":3},"speaker":"B","text":"それは大変ですね。1泊延ばせば、帰社できるのですか？"},{"id":{"d":880,"n":5,"t":3},"speaker":"A","text":"はい。先ほどの電話では1泊延ばせば帰社できそうということでした。"},{"id":{"d":880,"n":6,"t":3},"speaker":"B","text":"分かりました。新幹線の予約は無事変更できたのでしょうか？"},{"id":{"d":880,"n":7,"t":3},"speaker":"A","text":"いいえ。これから変更すると話していました。宿の延泊も手続きされるそうです。"},{"id":{"d":880,"n":8,"t":3},"speaker":"B","text":"そうですか。後で私からも連絡してみます。帰ってきてから追加で必要な社内手続きがあれば、サポートをお願いします。"},{"id":{"d":880,"n":9,"t":3},"speaker":"A","text":"承知しました。"}];
INSERT INTO sentence [{"id":{"d":881,"n":1,"t":3},"speaker":"A","text":"お盆期間の高速道路の渋滞を避けようと思いましたが、しっかり渋滞に巻き込まれてしまいましたね。"},{"id":{"d":881,"n":2,"t":3},"speaker":"B","text":"高速道路の渋滞を回避するため、朝早く出発しようとするのは、みんなが考えることでした。"},{"id":{"d":881,"n":3,"t":3},"speaker":"A","text":"明るくなってからしばらくたちますから、次のサービスエリアで朝食でも食べますか？"},{"id":{"d":881,"n":4,"t":3},"speaker":"B","text":"次のサービスエリアは、恐らく満車だと思いますよ。"},{"id":{"d":881,"n":5,"t":3},"speaker":"A","text":"そうでしょうか？"},{"id":{"d":881,"n":6,"t":3},"speaker":"B","text":"はい。ちょうど朝食の時間ですし、私たちと同じようにみんなが食事をしたいと思っていることでしょう。"},{"id":{"d":881,"n":7,"t":3},"speaker":"A","text":"なるほど。でも、そろそろ休憩を取りたいです。"},{"id":{"d":881,"n":8,"t":3},"speaker":"B","text":"では、混雑を承知で、サービスエリアに行ってみましょうか。"}];
INSERT INTO sentence [{"id":{"d":882,"n":1,"t":3},"speaker":"A","text":"神戸のお土産をありがとうございました。"},{"id":{"d":882,"n":2,"t":3},"speaker":"B","text":"神戸はついでに寄っただけなので、少しだけでごめんなさい。"},{"id":{"d":882,"n":3,"t":3},"speaker":"A","text":"とんでもありません。うれしいです。今回の旅行の目的は、神戸ではなかったのですか？"},{"id":{"d":882,"n":4,"t":3},"speaker":"B","text":"はい。今回の目的は宝塚歌劇を見ることでした。チケットが取れたので行ってきたのです。"},{"id":{"d":882,"n":5,"t":3},"speaker":"A","text":"入山さんが宝塚ファンだとは今、初めて知りました。"},{"id":{"d":882,"n":6,"t":3},"speaker":"B","text":"内緒にしているわけではありませんが、驚かれることが多いですね。"},{"id":{"d":882,"n":7,"t":3},"speaker":"A","text":"言われてみれば納得です。入山さんは格好良いですものね。"},{"id":{"d":882,"n":8,"t":3},"speaker":"B","text":"私は格好良くなんてありませんよ。宝塚歌劇に憧れてファッションをまねしているだけです。"}];
INSERT INTO sentence [{"id":{"d":883,"n":1,"t":3},"speaker":"A","text":"同好会の夏の旅行の目的地はどこにしますか？"},{"id":{"d":883,"n":2,"t":3},"speaker":"B","text":"今年は那須にします。"},{"id":{"d":883,"n":3,"t":3},"speaker":"A","text":"もう決定なのですね？"},{"id":{"d":883,"n":4,"t":3},"speaker":"B","text":"はい。那須にコスチューム・プレイを楽しめるお宿があるので、そこへ行きたいと思っています。"},{"id":{"d":883,"n":5,"t":3},"speaker":"A","text":"それは良い計画です。私たちアニメーション同好会にはちょうど良いですね。"},{"id":{"d":883,"n":6,"t":3},"speaker":"B","text":"撮影場所を案内してくれたり、館内でも小道具を貸してくれたりするらしいですよ。"},{"id":{"d":883,"n":7,"t":3},"speaker":"A","text":"参加希望者は全員だと思いますが、一応確認してみます。"},{"id":{"d":883,"n":8,"t":3},"speaker":"B","text":"よろしくお願いします。日取りを決めて、宿を予約しましょう。"}];
INSERT INTO sentence [{"id":{"d":884,"n":1,"t":3},"speaker":"A","text":"明日から乗船のお仕事でしたよね？"},{"id":{"d":884,"n":2,"t":3},"speaker":"B","text":"はい、今回は1カ月の乗船です。"},{"id":{"d":884,"n":3,"t":3},"speaker":"A","text":"随分と長旅になるのですね。"},{"id":{"d":884,"n":4,"t":3},"speaker":"B","text":"ええ、クルーズツアーですから。"},{"id":{"d":884,"n":5,"t":3},"speaker":"A","text":"添乗員さんも1カ月間も気を抜けずに、大変ですね。"},{"id":{"d":884,"n":6,"t":3},"speaker":"B","text":"船の中で問題が起こらなければ良いのですが、なかなかそのようなわけにもいきません。"},{"id":{"d":884,"n":7,"t":3},"speaker":"A","text":"船内ではどのような問題がよく起こるのですか？"},{"id":{"d":884,"n":8,"t":3},"speaker":"B","text":"具合が悪くなる人が多いのです。でも、私は看護師ではなく添乗員なので、医療の対応はできず心苦しいです。"}];
INSERT INTO sentence [{"id":{"d":885,"n":1,"t":3},"speaker":"A","text":"先週は、カニの食べ放題に行ってきました。"},{"id":{"d":885,"n":2,"t":3},"speaker":"B","text":"日帰り旅行ですか？"},{"id":{"d":885,"n":3,"t":3},"speaker":"A","text":"そうです。バスツアーで行ってきました。"},{"id":{"d":885,"n":4,"t":3},"speaker":"B","text":"カニはたくさん食べられましたか？"},{"id":{"d":885,"n":5,"t":3},"speaker":"A","text":"思ったよりは食べられませんでした。私が貧乏性なので、時間制限があるなかではそれほど食べられませんでした。"},{"id":{"d":885,"n":6,"t":3},"speaker":"B","text":"貧乏性とはどのようなことですか？"},{"id":{"d":885,"n":7,"t":3},"speaker":"A","text":"どうしてもカニの爪の先まで食べないともったいないと思ってしまうのです。細部まで残さずに食べていたら、あっという間に終了時間になってしまいました。"},{"id":{"d":885,"n":8,"t":3},"speaker":"B","text":"そういうことでしたか。大きな身だけ食べていれば、たくさん食べられたのに残念でしたね。"}];
