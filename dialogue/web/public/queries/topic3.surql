INSERT INTO sentence [{"id":{"d":887,"n":1,"t":3},"speaker":"A","text":"このメニューは何だと思いますか？"},{"id":{"d":887,"n":2,"t":3},"speaker":"B","text":"何でしょう？「鉄パイプカニ肉」と書いてありますね。"},{"id":{"d":887,"n":3,"t":3},"speaker":"A","text":"カニ肉は分かりますが、鉄パイプとは何のことでしょうね？"},{"id":{"d":887,"n":4,"t":3},"speaker":"B","text":"中国語を翻訳アプリケーションで翻訳してくれたのだと思いますが、この文字からはどのようなメニューか分かりませんね。"},{"id":{"d":887,"n":5,"t":3},"speaker":"A","text":"そうですね。文字だけでなく、メニューに写真もあればよかったのですが。"},{"id":{"d":887,"n":6,"t":3},"speaker":"B","text":"試しにこのメニューを頼んでみませんか？"},{"id":{"d":887,"n":7,"t":3},"speaker":"A","text":"何か分からないのに頼むとは、あなたは勇気がありますね。"},{"id":{"d":887,"n":8,"t":3},"speaker":"B","text":"カニ肉ですからきっとおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":888,"n":1,"t":3},"speaker":"A","text":"ブルーベリー狩りに行ってきたので、少しおすそ分けします。"},{"id":{"d":888,"n":2,"t":3},"speaker":"B","text":"大きくて立派なブルーベリーですね。"},{"id":{"d":888,"n":3,"t":3},"speaker":"A","text":"たくさん取ってジャムにしようと思っていたのですが、ジャムにするのはもったいない大きさですよね。"},{"id":{"d":888,"n":4,"t":3},"speaker":"B","text":"これほどに大きなブルーベリーは食べたことがありません。ありがたくいただきます。"},{"id":{"d":888,"n":5,"t":3},"speaker":"A","text":"実は、これらの一部は自分で取ったものではありません。"},{"id":{"d":888,"n":6,"t":3},"speaker":"B","text":"買ってきたのですか？"},{"id":{"d":888,"n":7,"t":3},"speaker":"A","text":"いいえ。ゲリラ雷雨でブルーベリー狩りが途中で終了となってしまったのですが、残り時間の分ということで農園の方が朝に摘んだものをくださったのです。"},{"id":{"d":888,"n":8,"t":3},"speaker":"B","text":"それはうれしいですね。でも、どれが農園の方が摘んだものか区別がつかないほど全部立派な大粒ですよ。"}];
INSERT INTO sentence [{"id":{"d":889,"n":1,"t":3},"speaker":"A","text":"スーツケースに荷物が入り切りません。"},{"id":{"d":889,"n":2,"t":3},"speaker":"B","text":"お土産が増えたからですか？"},{"id":{"d":889,"n":3,"t":3},"speaker":"A","text":"いいえ。お土産はこちらのエコバッグに入っています。"},{"id":{"d":889,"n":4,"t":3},"speaker":"B","text":"不思議ですね。来るときは、全部の荷物が入っていたのでしょう？"},{"id":{"d":889,"n":5,"t":3},"speaker":"A","text":"はい。しかし、どのようにしてこれらの荷物をスーツケースに入れてきたのか思い出せません。どうしたら良いと思いますか？"},{"id":{"d":889,"n":6,"t":3},"speaker":"B","text":"スーツケースの荷物の一部をエコバッグの方に入れてはどうでしょうか？"},{"id":{"d":889,"n":7,"t":3},"speaker":"A","text":"良い考えですが、あいにくエコバッグはお土産でいっぱいです。"},{"id":{"d":889,"n":8,"t":3},"speaker":"B","text":"仕方がありませんね。私のサブバッグを貸しますので、ここに入れてください。"}];
INSERT INTO sentence [{"id":{"d":890,"n":1,"t":3},"speaker":"A","text":"このサラダには変わった野菜が使われていますね。"},{"id":{"d":890,"n":2,"t":3},"speaker":"B","text":"当店のサラダは、鎌倉野菜を使用しております。"},{"id":{"d":890,"n":3,"t":3},"speaker":"A","text":"これらは鎌倉野菜なのですね。初めて見るものばかりです。"},{"id":{"d":890,"n":4,"t":3},"speaker":"B","text":"日本では珍しい品種も多いので、その見た目や色に驚かれるかもしれませんが、どれもおいしいですよ。"},{"id":{"d":890,"n":5,"t":3},"speaker":"A","text":"確かに、とても色鮮やかですね。これらはこのままいただいて良いのですか？"},{"id":{"d":890,"n":6,"t":3},"speaker":"B","text":"はい。そのままでも召し上がれますし、よければこちらのドレッシングもお試しください。"},{"id":{"d":890,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。ドレッシングもおいしいですね。"},{"id":{"d":890,"n":8,"t":3},"speaker":"B","text":"気に入っていただけて良かったです。ごゆっくりお食事をお楽しみください。"}];
INSERT INTO sentence [{"id":{"d":891,"n":1,"t":3},"speaker":"A","text":"すみません、新館に戻るにはどちらに行ったらよいのでしょうか？大浴場に行ったら、帰り道が分からなくなってしまいました。"},{"id":{"d":891,"n":2,"t":3},"speaker":"B","text":"分かりにくくて申し訳ありません。ご案内いたします。ここは1階ですので、エレベーターで2階に上がってください。左に進んで突き当たりを右に行きますと、新館への渡り廊下がございます。"},{"id":{"d":891,"n":3,"t":3},"speaker":"A","text":"なるほど。2階に行かないと、新館に戻れなかったのですね。"},{"id":{"d":891,"n":4,"t":3},"speaker":"B","text":"さようでございます。渡り廊下を渡った先は、新館の1階でございますのでご注意ください。"},{"id":{"d":891,"n":5,"t":3},"speaker":"A","text":"分かりました。ありがとうございました。"},{"id":{"d":891,"n":6,"t":3},"speaker":"B","text":"この時間は渡り廊下の窓から夕日がきれいに見えます。帰り道に、ぜひ景色もお楽しみください。"}];
INSERT INTO sentence [{"id":{"d":892,"n":1,"t":3},"speaker":"A","text":"加藤さま、こちらがお部屋の鍵でございます。何かご質問はございますか？"},{"id":{"d":892,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。こちらのホテルには、子ども用のプールもあるのですか？"},{"id":{"d":892,"n":3,"t":3},"speaker":"A","text":"はい、ございます。"},{"id":{"d":892,"n":4,"t":3},"speaker":"B","text":"利用料金はおいくらですか？"},{"id":{"d":892,"n":5,"t":3},"speaker":"A","text":"ご宿泊のお客さまは、無料でご利用いただけます。"},{"id":{"d":892,"n":6,"t":3},"speaker":"B","text":"では、水着を持っていけば、いつでも泳げるのですね？"},{"id":{"d":892,"n":7,"t":3},"speaker":"A","text":"はい。9時から21時の間でしたら、ご自由にお使いいただけます。"},{"id":{"d":892,"n":8,"t":3},"speaker":"B","text":"ありがとうございました。後で子どもを連れて行ってみます。"}];
INSERT INTO sentence [{"id":{"d":893,"n":1,"t":3},"speaker":"A","text":"今日はドライブに行きますよ。"},{"id":{"d":893,"n":2,"t":3},"speaker":"B","text":"朝から張り切っていますね。"},{"id":{"d":893,"n":3,"t":3},"speaker":"A","text":"昨日、テレビ番組でおいしそうなエビフライを見たのです。"},{"id":{"d":893,"n":4,"t":3},"speaker":"B","text":"エビフライを食べるためにドライブですか？"},{"id":{"d":893,"n":5,"t":3},"speaker":"A","text":"そうです。ここからなら2時間弱でエビフライが食べられるお店に着くはずです。"},{"id":{"d":893,"n":6,"t":3},"speaker":"B","text":"それでは、朝食は軽いものを食べてから行きますか？"},{"id":{"d":893,"n":7,"t":3},"speaker":"A","text":"はい。トースト1枚くらいで良いですよ。"},{"id":{"d":893,"n":8,"t":3},"speaker":"B","text":"分かりました。9時過ぎには出られるように支度します。"}];
INSERT INTO sentence [{"id":{"d":894,"n":1,"t":3},"speaker":"A","text":"今、姉が新婚旅行中でオーストラリアを訪問しています。"},{"id":{"d":894,"n":2,"t":3},"speaker":"B","text":"お土産が楽しみですね。"},{"id":{"d":894,"n":3,"t":3},"speaker":"A","text":"お土産はコアラチョコと既に分かっているのです。"},{"id":{"d":894,"n":4,"t":3},"speaker":"B","text":"コアラチョコをリクエストしたのですか？"},{"id":{"d":894,"n":5,"t":3},"speaker":"A","text":"いいえ。毎日のようにコアラチョコの写真が送られてくるので、分かってしまったのです。"},{"id":{"d":894,"n":6,"t":3},"speaker":"B","text":"毎日メールで写真が送られてくるのですか？"},{"id":{"d":894,"n":7,"t":3},"speaker":"A","text":"ええ。時差が1時間なのが救いです。ほら、また来ました。"},{"id":{"d":894,"n":8,"t":3},"speaker":"B","text":"確かにコアラチョコが写っています。今日はウルルに行かれているようですね。"}];
INSERT INTO sentence [{"id":{"d":895,"n":1,"t":3},"speaker":"A","text":"スワンボートに乗りたいのですが、子どもも一緒に乗れますか？"},{"id":{"d":895,"n":2,"t":3},"speaker":"B","text":"はい。大人の方とご一緒でしたら、大丈夫ですよ。"},{"id":{"d":895,"n":3,"t":3},"speaker":"A","text":"料金はおいくらですか？"},{"id":{"d":895,"n":4,"t":3},"speaker":"B","text":"施設のフリーパス券をお持ちかどうかで料金が異なります。フリーパス券はお持ちですか？"},{"id":{"d":895,"n":5,"t":3},"speaker":"A","text":"はい、持っています。"},{"id":{"d":895,"n":6,"t":3},"speaker":"B","text":"それでは、フリーパス券を利用し、無料でお乗りいただけます。"},{"id":{"d":895,"n":7,"t":3},"speaker":"A","text":"ボートでおやつを食べたりしても良いですか？"},{"id":{"d":895,"n":8,"t":3},"speaker":"B","text":"申し訳ありません。ボートでのご飲食はご遠慮ください。"}];
INSERT INTO sentence [{"id":{"d":896,"n":1,"t":3},"speaker":"A","text":"海鮮丼がおいしかったですね。食後に抹茶アイスを頼みましょう。"},{"id":{"d":896,"n":2,"t":3},"speaker":"B","text":"そうしましょう。"},{"id":{"d":896,"n":3,"t":3},"speaker":"A","text":"デザートを食べ終わったらこのホテルに向かおうと思うのですが、移動手段はバスとタクシーのどちらが良いでしょうか？"},{"id":{"d":896,"n":4,"t":3},"speaker":"B","text":"ホテルの送迎バスが利用できるのではありませんか？"},{"id":{"d":896,"n":5,"t":3},"speaker":"A","text":"送迎バスは予約が必要だったはずです。予約していないので利用できません。"},{"id":{"d":896,"n":6,"t":3},"speaker":"B","text":"そうですか。路線バスを利用すると、バス停から20分ほど歩くことになってしまいますよ。"},{"id":{"d":896,"n":7,"t":3},"speaker":"A","text":"それではタクシーが良いですね。"},{"id":{"d":896,"n":8,"t":3},"speaker":"B","text":"そうですね。では、30分後くらいにこちらにタクシーを呼んでおきましょうか？"},{"id":{"d":896,"n":9,"t":3},"speaker":"A","text":"はい。ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":897,"n":1,"t":3},"speaker":"A","text":"明石で食べた明石焼きが忘れられなくて、たこ焼き機を買いました。"},{"id":{"d":897,"n":2,"t":3},"speaker":"B","text":"それで、明石焼きを家で作ったのですか？"},{"id":{"d":897,"n":3,"t":3},"speaker":"A","text":"はい。挑戦してみました。"},{"id":{"d":897,"n":4,"t":3},"speaker":"B","text":"上手に焼けましたか？"},{"id":{"d":897,"n":5,"t":3},"speaker":"A","text":"いいえ。それが、私が焼くとただのたこ焼きになってしまうのです。"},{"id":{"d":897,"n":6,"t":3},"speaker":"B","text":"明石焼きは生地作りが難しいのでしょうね。"},{"id":{"d":897,"n":7,"t":3},"speaker":"A","text":"生地作りもですが、焼き方も難しいです。ふわふわの状態で丸くすることができません。"},{"id":{"d":897,"n":8,"t":3},"speaker":"B","text":"そうなのですね。生地に使うだしも自分で作るのですか？"},{"id":{"d":897,"n":9,"t":3},"speaker":"A","text":"はい。今回は家にあるだしを使いました。"}];
INSERT INTO sentence [{"id":{"d":898,"n":1,"t":3},"speaker":"A","text":"夜行バスでは眠れましたか？"},{"id":{"d":898,"n":2,"t":3},"speaker":"B","text":"はい。私は結構、眠れましたよ。"},{"id":{"d":898,"n":3,"t":3},"speaker":"A","text":"私は、全然眠れませんでした。"},{"id":{"d":898,"n":4,"t":3},"speaker":"B","text":"眠れるように、高級なシートのバスを予約したのに残念でしたね。"},{"id":{"d":898,"n":5,"t":3},"speaker":"A","text":"ええ。朝食を食べて、少し仮眠を取りたいです。"},{"id":{"d":898,"n":6,"t":3},"speaker":"B","text":"この近くにモーニングサービスをしているファミリーレストランがありますから、そこに行きますか？"},{"id":{"d":898,"n":7,"t":3},"speaker":"A","text":"良いですね。食べ放題ののぼりが出ていますね。あそこですか？"},{"id":{"d":898,"n":8,"t":3},"speaker":"B","text":"そうです。ファミリーレストランなら、朝食を食べた後、その場で少しは休めますよ。"}];
INSERT INTO sentence [{"id":{"d":899,"n":1,"t":3},"speaker":"A","text":"今日はシーサー作り体験に行きましょう。"},{"id":{"d":899,"n":2,"t":3},"speaker":"B","text":"はい。昨日から楽しみにしていました。"},{"id":{"d":899,"n":3,"t":3},"speaker":"A","text":"お土産屋さんで販売されているシーサーを買うのも良いですが、自分で作るのも楽しそうですよね。"},{"id":{"d":899,"n":4,"t":3},"speaker":"B","text":"そうですね。シーサー作り体験の所要時間は2時間くらいと書いてありましたが、終わったら何をしますか？"},{"id":{"d":899,"n":5,"t":3},"speaker":"A","text":"シーサー作り体験が終わったら、沖縄そばを食べに行きましょう。"},{"id":{"d":899,"n":6,"t":3},"speaker":"B","text":"良いですね。そちらも楽しみです。"},{"id":{"d":899,"n":7,"t":3},"speaker":"A","text":"ところで、シーサーを持ち帰ったら、どこに置く予定ですか？"},{"id":{"d":899,"n":8,"t":3},"speaker":"B","text":"実家の門柱の上に置こうと思っています。"}];
INSERT INTO sentence [{"id":{"d":900,"n":1,"t":3},"speaker":"A","text":"東京の地下鉄の路線は複雑に入り組んでいますね。"},{"id":{"d":900,"n":2,"t":3},"speaker":"B","text":"はい。乗る前によく調べておかないと、迷子になってしまいそうです。"},{"id":{"d":900,"n":3,"t":3},"speaker":"A","text":"乗り放題パスを持っていれば、一定料金で移動できるのは良いですけれどね。"},{"id":{"d":900,"n":4,"t":3},"speaker":"B","text":"ここは浅草ですが、渋谷にはどのようにして行ったら良いのですか？"},{"id":{"d":900,"n":5,"t":3},"speaker":"A","text":"調べてみましょう。路線図を見ると、浅草と渋谷の間にはいろいろな地下鉄が通っていますね。"},{"id":{"d":900,"n":6,"t":3},"speaker":"B","text":"これらを全部乗り換えるのですか？"},{"id":{"d":900,"n":7,"t":3},"speaker":"A","text":"いいえ。待ってください。地図上では真っ直ぐではありませんが、この黄色の線なら1本で行けるのではありませんか？"},{"id":{"d":900,"n":8,"t":3},"speaker":"B","text":"路線図の黄色の線は銀座線を示しているようですね。これに乗って行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":901,"n":1,"t":3},"speaker":"A","text":"君たちは学生ですか？"},{"id":{"d":901,"n":2,"t":3},"speaker":"B","text":"はい。大学1年生です。"},{"id":{"d":901,"n":3,"t":3},"speaker":"A","text":"九州へは何をしに行くのですか？"},{"id":{"d":901,"n":4,"t":3},"speaker":"B","text":"自転車で九州一周をしに行きます。"},{"id":{"d":901,"n":5,"t":3},"speaker":"A","text":"それは良いですね。"},{"id":{"d":901,"n":6,"t":3},"speaker":"B","text":"おじさんは旅行ですか？"},{"id":{"d":901,"n":7,"t":3},"speaker":"A","text":"はい。私は妻と一緒に里帰りです。"},{"id":{"d":901,"n":8,"t":3},"speaker":"B","text":"夫婦でフェリーの旅とは、憧れます。"}];
INSERT INTO sentence [{"id":{"d":902,"n":1,"t":3},"speaker":"A","text":"美里はどこに行ったのですか？"},{"id":{"d":902,"n":2,"t":3},"speaker":"B","text":"「海岸を散歩してくる」と言って、出ていきました。"},{"id":{"d":902,"n":3,"t":3},"speaker":"A","text":"美里は海がない県の出身ですから、海を見られることがうれしいのですね。"},{"id":{"d":902,"n":4,"t":3},"speaker":"B","text":"そのようです。もうじき出掛けてから1時間になるので、そろそろ帰ってくると思いますよ。"},{"id":{"d":902,"n":5,"t":3},"speaker":"A","text":"朝食のバイキング料理は7時からでしたよね？"},{"id":{"d":902,"n":6,"t":3},"speaker":"B","text":"ええ。もう始まっている時間です。"},{"id":{"d":902,"n":7,"t":3},"speaker":"A","text":"美里が帰ってきたら、すぐに行きましょう。"},{"id":{"d":902,"n":8,"t":3},"speaker":"B","text":"そうですね。おなかがすいてきました。"}];
INSERT INTO sentence [{"id":{"d":903,"n":1,"t":3},"speaker":"A","text":"すみません、この施設におむつ替えのできるトイレはありますか？"},{"id":{"d":903,"n":2,"t":3},"speaker":"B","text":"エレベーターで2階に上がって、右に行った通路の右手にございます。"},{"id":{"d":903,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":903,"n":4,"t":3},"speaker":"B","text":"よろしければ、そばに授乳ルームもございますのでお使いください。"},{"id":{"d":903,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。おむつを替えた後、子どもと少し休みたいと思っていたのですが、そのために授乳ルームを使っても良いですか？"},{"id":{"d":903,"n":6,"t":3},"speaker":"B","text":"はい。長時間でなければ大丈夫です。"},{"id":{"d":903,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。今から行ってみます。"},{"id":{"d":903,"n":8,"t":3},"speaker":"B","text":"お気を付けていってらっしゃいませ。"}];
INSERT INTO sentence [{"id":{"d":904,"n":1,"t":3},"speaker":"A","text":"この通りは、お土産屋さんがたくさんありますね。"},{"id":{"d":904,"n":2,"t":3},"speaker":"B","text":"皆それぞれに見たいものがあるでしょうし、ここからは自由行動にしませんか？"},{"id":{"d":904,"n":3,"t":3},"speaker":"A","text":"そうですね。好きなお店を見て、1時間後にこの隣のカフェに集合でどうですか？"},{"id":{"d":904,"n":4,"t":3},"speaker":"B","text":"良いですね。土産店巡りにあまり時間がかからない人も、カフェで休んでいられますしね。"},{"id":{"d":904,"n":5,"t":3},"speaker":"A","text":"私は結構時間がかかりそうです。お待たせするかもしれません。"},{"id":{"d":904,"n":6,"t":3},"speaker":"B","text":"あまりに遅れるようでしたら、電話してください。"},{"id":{"d":904,"n":7,"t":3},"speaker":"A","text":"分かりました。"},{"id":{"d":904,"n":8,"t":3},"speaker":"B","text":"では、解散です。"}];
INSERT INTO sentence [{"id":{"d":905,"n":1,"t":3},"speaker":"A","text":"家に行く前に、コンビニエンスストアに寄りますね。"},{"id":{"d":905,"n":2,"t":3},"speaker":"B","text":"見たことがないコンビニエンスストアです。北海道のコンビニエンスストアですか？"},{"id":{"d":905,"n":3,"t":3},"speaker":"A","text":"このコンビニエンスストアは関東にはありませんか？"},{"id":{"d":905,"n":4,"t":3},"speaker":"B","text":"はい。私の家の近所にはありません。"},{"id":{"d":905,"n":5,"t":3},"speaker":"A","text":"そうでしたか。ここへは、予約していたオードブルを受け取りに来たのです。"},{"id":{"d":905,"n":6,"t":3},"speaker":"B","text":"わざわざありがとうございます。とてもおいしそうです。飲み物は私が買いますね。"},{"id":{"d":905,"n":7,"t":3},"speaker":"A","text":"では、お言葉に甘えてよろしくお願いします。北海道へようこそ。"},{"id":{"d":905,"n":8,"t":3},"speaker":"B","text":"しばらくご厄介になります。"}];
INSERT INTO sentence [{"id":{"d":906,"n":1,"t":3},"speaker":"A","text":"善光寺参りを終えても、なかなか駅にたどり着きませんね。"},{"id":{"d":906,"n":2,"t":3},"speaker":"B","text":"食べたいものが多すぎて、つい立ち寄ってしまうのが原因ですね。"},{"id":{"d":906,"n":3,"t":3},"speaker":"A","text":"おそばもおやきもみそソフトクリームも食べましたけれど、まだ何か食べたいものがあるのですか？"},{"id":{"d":906,"n":4,"t":3},"speaker":"B","text":"はい。有名な七味唐辛子のお店がそこにありますよ。"},{"id":{"d":906,"n":5,"t":3},"speaker":"A","text":"唐辛子ですか？"},{"id":{"d":906,"n":6,"t":3},"speaker":"B","text":"はい。唐辛子を始めとしたスパイス入りのアイスクリームが食べられるのですよ。早く行きましょう。"},{"id":{"d":906,"n":7,"t":3},"speaker":"A","text":"まだおなかに入るのですか？"},{"id":{"d":906,"n":8,"t":3},"speaker":"B","text":"アイスクリームくらい楽勝です。"}];
INSERT INTO sentence [{"id":{"d":907,"n":1,"t":3},"speaker":"A","text":"ダムカードがやっと100枚になりました。"},{"id":{"d":907,"n":2,"t":3},"speaker":"B","text":"ダムカードとは日本全国のダムで配布しているカードのことですか？"},{"id":{"d":907,"n":3,"t":3},"speaker":"A","text":"そうです。休みの日に近郊のダム巡りをしてやっと100枚集まりました。"},{"id":{"d":907,"n":4,"t":3},"speaker":"B","text":"日本にはそれほどの数のダムがあるのですか？"},{"id":{"d":907,"n":5,"t":3},"speaker":"A","text":"はい。日本にはたくさんのダムがあります。カードもあと500枚は集められるのではないでしょうか。"},{"id":{"d":907,"n":6,"t":3},"speaker":"B","text":"さらに500枚もあるのですか？"},{"id":{"d":907,"n":7,"t":3},"speaker":"A","text":"はい。地方にはしょっちゅうは行けませんから、全部集めるのは難しそうです。"},{"id":{"d":907,"n":8,"t":3},"speaker":"B","text":"大変な趣味を持ってしまったのですね。"}];
INSERT INTO sentence [{"id":{"d":908,"n":1,"t":3},"speaker":"A","text":"今日、車を運転していたら、パトカーに追跡されたような気がして肝が冷えました。"},{"id":{"d":908,"n":2,"t":3},"speaker":"B","text":"何をしてしまったのですか？"},{"id":{"d":908,"n":3,"t":3},"speaker":"A","text":"何もしていませんよ。一時停止もきちんと止まっていました。"},{"id":{"d":908,"n":4,"t":3},"speaker":"B","text":"そうですよね。私が知る限り、君はいつも安全運転です。それで、結局は何があったのですか？"},{"id":{"d":908,"n":5,"t":3},"speaker":"A","text":"パトカーだと思った車をよく見たら、パトカーではなかったのですよ。"},{"id":{"d":908,"n":6,"t":3},"speaker":"B","text":"パトカーに似ている車だったということですか？"},{"id":{"d":908,"n":7,"t":3},"speaker":"A","text":"そうです。車種もパトカーと同じで、パトカーと同じように上が白、下が黒にペイントされていました。赤色灯がなかったので、パトカーとは違うと分かりました。"},{"id":{"d":908,"n":8,"t":3},"speaker":"B","text":"そういうことでしたか。パトカーファンの人の車だったのですかね。"}];
INSERT INTO sentence [{"id":{"d":909,"n":1,"t":3},"speaker":"A","text":"写真で見た青い丘に行ってみたいのですが、写真の撮影場所をご存知ですか？"},{"id":{"d":909,"n":2,"t":3},"speaker":"B","text":"有名なところでしたら、国営ひたち海浜公園のみはらしの丘ではありませんか？"},{"id":{"d":909,"n":3,"t":3},"speaker":"A","text":"そうかもしれません。この場所へ行くにはどうしたら良いですか？"},{"id":{"d":909,"n":4,"t":3},"speaker":"B","text":"東京から特急ひたちに乗って勝田駅でバスに乗り換えれば良いと思います。"},{"id":{"d":909,"n":5,"t":3},"speaker":"A","text":"ありがとうございます。週末に行ってみます。"},{"id":{"d":909,"n":6,"t":3},"speaker":"B","text":"待ってください。今はネモフィラの咲いている季節ではありませんよ。"},{"id":{"d":909,"n":7,"t":3},"speaker":"A","text":"一年中、咲いているのではありませんか？"},{"id":{"d":909,"n":8,"t":3},"speaker":"B","text":"ネモフィラは春の花です。今はコキアが紅葉していると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":910,"n":1,"t":3},"speaker":"A","text":"湘南のドライブも気持ちが良かったですが、フェリーも良いですね。"},{"id":{"d":910,"n":2,"t":3},"speaker":"B","text":"フェリーだと車を運転せずに目的地に着けますし、風が気持ち良いです。"},{"id":{"d":910,"n":3,"t":3},"speaker":"A","text":"あちらに着いたらまた運転しなくてはいけませんから、今はゆっくり休んでください。コーヒーをどうぞ。"},{"id":{"d":910,"n":4,"t":3},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":910,"n":5,"t":3},"speaker":"A","text":"フェリーを降りて、港からいちご狩り農園までの移動時間はどれくらいですか？"},{"id":{"d":910,"n":6,"t":3},"speaker":"B","text":"1時間くらいで着くと思いますよ。"},{"id":{"d":910,"n":7,"t":3},"speaker":"A","text":"以前行ったときは、あまりいちごを食べられなかったので楽しみです。"},{"id":{"d":910,"n":8,"t":3},"speaker":"B","text":"そうですね。今日はたくさん食べましょう。"}];
INSERT INTO sentence [{"id":{"d":911,"n":1,"t":3},"speaker":"A","text":"次の駅で降りて、レンタサイクルを借りますよ。"},{"id":{"d":911,"n":2,"t":3},"speaker":"B","text":"予約してあるのですか？"},{"id":{"d":911,"n":3,"t":3},"speaker":"A","text":"もちろんです。"},{"id":{"d":911,"n":4,"t":3},"speaker":"B","text":"手際が良いですね。"},{"id":{"d":911,"n":5,"t":3},"speaker":"A","text":"学生の頃からよく幹事を担当していたので、このような準備は慣れています。"},{"id":{"d":911,"n":6,"t":3},"speaker":"B","text":"サイクリングで行く先の予約も済んでいるのですか？"},{"id":{"d":911,"n":7,"t":3},"speaker":"A","text":"はい。レストランは12時に予約済みです。のんびりしていると遅れてしまいますよ。"},{"id":{"d":911,"n":8,"t":3},"speaker":"B","text":"ありがとうございます。でも、楽しいサイクリングなのに、トレーニングのような走り方は勘弁してくださいね。"}];
INSERT INTO sentence [{"id":{"d":912,"n":1,"t":3},"speaker":"A","text":"車を買うとき、さらに大きい車にしようとは思わなかったのですか？"},{"id":{"d":912,"n":2,"t":3},"speaker":"B","text":"はい、思いませんでした。家に大きい車を置ける場所がありませんので。"},{"id":{"d":912,"n":3,"t":3},"speaker":"A","text":"軽自動車なら置けるのですね。"},{"id":{"d":912,"n":4,"t":3},"speaker":"B","text":"それでもスペースの限界間近ですけれどね。"},{"id":{"d":912,"n":5,"t":3},"speaker":"A","text":"あ、この駐車場が空いていますよ。ここに入りましょう。"},{"id":{"d":912,"n":6,"t":3},"speaker":"B","text":"はい。軽自動車用の場所が空いていて良かったです。"},{"id":{"d":912,"n":7,"t":3},"speaker":"A","text":"駐車場が満車に近くても、軽自動車用の場所は空いていることが多いですね。"},{"id":{"d":912,"n":8,"t":3},"speaker":"B","text":"そうですね。軽自動車用の駐車場は建物の入り口に近い場所にあることも多いので、そういうときは得をした気分になります。"}];
INSERT INTO sentence [{"id":{"d":913,"n":1,"t":3},"speaker":"A","text":"図書館で新選組の小説を借りて読んでいるうちに、旅に出たくなりました。"},{"id":{"d":913,"n":2,"t":3},"speaker":"B","text":"新選組といえば、京都ですか？"},{"id":{"d":913,"n":3,"t":3},"speaker":"A","text":"はい。京都も行きたい場所の一つです。"},{"id":{"d":913,"n":4,"t":3},"speaker":"B","text":"京都以外ではどこですか？"},{"id":{"d":913,"n":5,"t":3},"speaker":"A","text":"会津若松や函館です。"},{"id":{"d":913,"n":6,"t":3},"speaker":"B","text":"あなたが読んだ小説は、土方歳三について描かれたものですね？"},{"id":{"d":913,"n":7,"t":3},"speaker":"A","text":"ご名答です。なぜ分かったのですか？"},{"id":{"d":913,"n":8,"t":3},"speaker":"B","text":"函館という地名を聞いて分かりました。"}];
INSERT INTO sentence [{"id":{"d":914,"n":1,"t":3},"speaker":"A","text":"お父さん、夏休みには屋久島に連れて行ってください。"},{"id":{"d":914,"n":2,"t":3},"speaker":"B","text":"屋久島は東京から遠いので、お父さんの夏休み中に行くのは難しそうです。"},{"id":{"d":914,"n":3,"t":3},"speaker":"A","text":"それほどに遠いのですか？"},{"id":{"d":914,"n":4,"t":3},"speaker":"B","text":"はい。お父さんの夏休みは3日間しかありません。3日間で屋久島へ行くのは難しいです。"},{"id":{"d":914,"n":5,"t":3},"speaker":"A","text":"残念です。もののけの森に行きたかったのです。"},{"id":{"d":914,"n":6,"t":3},"speaker":"B","text":"もののけの森なら長野にもありますよ。そちらなら行けます。"},{"id":{"d":914,"n":7,"t":3},"speaker":"A","text":"本当ですか？"},{"id":{"d":914,"n":8,"t":3},"speaker":"B","text":"はい。では、今年の夏休みは長野の白駒の池に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":915,"n":1,"t":3},"speaker":"A","text":"ここのエスカレーターは長いですね。"},{"id":{"d":915,"n":2,"t":3},"speaker":"B","text":"そうですね。エスカレーター終わりが見えず、少し怖いです。"},{"id":{"d":915,"n":3,"t":3},"speaker":"A","text":"皆左に寄って乗っています。右が空いているからといって歩いてはいけませんよね。"},{"id":{"d":915,"n":4,"t":3},"speaker":"B","text":"もちろんです。エスカレーターでは歩かない方がいいです。"},{"id":{"d":915,"n":5,"t":3},"speaker":"A","text":"なかなか着きませんね。もう1分くらい乗っていますよ。"},{"id":{"d":915,"n":6,"t":3},"speaker":"B","text":"何階分を上っているのでしょうか？"},{"id":{"d":915,"n":7,"t":3},"speaker":"A","text":"恐らく4階分くらいではないでしょうか？"},{"id":{"d":915,"n":8,"t":3},"speaker":"B","text":"そうですか。エスカレーターに乗っただけで、旅をしたように思えます。"}];
INSERT INTO sentence [{"id":{"d":916,"n":1,"t":3},"speaker":"A","text":"長崎に変わったバス停があるのを知っていますか？"},{"id":{"d":916,"n":2,"t":3},"speaker":"B","text":"いいえ、知りません。"},{"id":{"d":916,"n":3,"t":3},"speaker":"A","text":"路線のバス停がフルーツになっているのですよ。"},{"id":{"d":916,"n":4,"t":3},"speaker":"B","text":"時刻表が付いているバス停の標識が、フルーツの形になっているのですか？"},{"id":{"d":916,"n":5,"t":3},"speaker":"A","text":"いいえ。待合所の建物というか、あずまやのようなものがメロンやいちごの形なのです。"},{"id":{"d":916,"n":6,"t":3},"speaker":"B","text":"理由は分かりませんが、かわいらしく感じますね。"},{"id":{"d":916,"n":7,"t":3},"speaker":"A","text":"昨日、私はテレビ番組でそのバス停を見たのです。それを見るために長崎へ行きたくなりました。"},{"id":{"d":916,"n":8,"t":3},"speaker":"B","text":"バス停を見るために長崎に行くのですか？もし行ったら、その変わったバス停を写真撮影して、後ほど見せてくださいね。"}];
INSERT INTO sentence [{"id":{"d":917,"n":1,"t":3},"speaker":"A","text":"釣竿を買ってきたのですか？"},{"id":{"d":917,"n":2,"t":3},"speaker":"B","text":"はい。今度、近場で釣りに行こうと思って買ってきました。"},{"id":{"d":917,"n":3,"t":3},"speaker":"A","text":"釣りが趣味なのですか？"},{"id":{"d":917,"n":4,"t":3},"speaker":"B","text":"ええ。先日、沖縄に旅行した際に、体験ツアーに参加したのです。"},{"id":{"d":917,"n":5,"t":3},"speaker":"A","text":"「大物が釣れた」と言っていましたよね。"},{"id":{"d":917,"n":6,"t":3},"speaker":"B","text":"あの楽しさが忘れられなくて、また釣りをしたくなったのですよ。"},{"id":{"d":917,"n":7,"t":3},"speaker":"A","text":"そうでしたか。剛君も連れて行くのでしたら、わが家の瑞樹も連れて行ってくれないでしょうか？"},{"id":{"d":917,"n":8,"t":3},"speaker":"B","text":"よいですよ。妻も一緒ですし、お安いご用です。日曜日の5時に出発予定です。"}];
INSERT INTO sentence [{"id":{"d":918,"n":1,"t":3},"speaker":"A","text":"かず君は、大阪に行ったら海遊館に行くでしょう？"},{"id":{"d":918,"n":2,"t":3},"speaker":"B","text":"はい。お母さんがそう言っていました。"},{"id":{"d":918,"n":3,"t":3},"speaker":"A","text":"海遊館に行くときはニンテンドーDSを持って行くと良いですよ。"},{"id":{"d":918,"n":4,"t":3},"speaker":"B","text":"どうしてニンテンドーDSが必要なのですか？"},{"id":{"d":918,"n":5,"t":3},"speaker":"A","text":"海遊館では、ニンテンドーDSを使ってお魚の説明を聞くことができるのです。"},{"id":{"d":918,"n":6,"t":3},"speaker":"B","text":"こう君もニンテンドーDSを持って行くのですか？"},{"id":{"d":918,"n":7,"t":3},"speaker":"A","text":"はい、持って行きます。イヤホンも要るのだそうです。"},{"id":{"d":918,"n":8,"t":3},"speaker":"B","text":"僕も忘れないようにニンテンドーDSを持っていこうと思います。"}];
INSERT INTO sentence [{"id":{"d":919,"n":1,"t":3},"speaker":"A","text":"残業を30分ほど行ったので、会社を出るのが遅くなってしまいました。"},{"id":{"d":919,"n":2,"t":3},"speaker":"B","text":"夜景クルーズ船の出航時間には間に合いますから、大丈夫です。"},{"id":{"d":919,"n":3,"t":3},"speaker":"A","text":"乗船前にラーメンでも食べて行くつもりでしたが、その時間がなくなってしまいました。"},{"id":{"d":919,"n":4,"t":3},"speaker":"B","text":"では、軽く食べられるものを買ってから、船乗り場へ行きましょう。"},{"id":{"d":919,"n":5,"t":3},"speaker":"A","text":"船で飲食をしても良いのですか？"},{"id":{"d":919,"n":6,"t":3},"speaker":"B","text":"はい。問い合わせたら大丈夫とのことでした。"},{"id":{"d":919,"n":7,"t":3},"speaker":"A","text":"それなら良かったです。"},{"id":{"d":919,"n":8,"t":3},"speaker":"B","text":"今日は、工場の夜景クルーズを楽しみましょう。"}];
INSERT INTO sentence [{"id":{"d":920,"n":1,"t":3},"speaker":"A","text":"日帰りバスツアーの帰りが、これほど遅くなるとは思いませんでした。"},{"id":{"d":920,"n":2,"t":3},"speaker":"B","text":"渋滞がひどかったですからね。"},{"id":{"d":920,"n":3,"t":3},"speaker":"A","text":"ええ。夕飯も食べられず空腹です。"},{"id":{"d":920,"n":4,"t":3},"speaker":"B","text":"どこかで、何かを食べていきますか？"},{"id":{"d":920,"n":5,"t":3},"speaker":"A","text":"この時間に空いているところだと、居酒屋さんが多いですね。"},{"id":{"d":920,"n":6,"t":3},"speaker":"B","text":"そうですね。せっかくなので、お酒を飲んで帰りましょうよ。"},{"id":{"d":920,"n":7,"t":3},"speaker":"A","text":"そうですね。あそこの焼き鳥屋さんはどうですか？"},{"id":{"d":920,"n":8,"t":3},"speaker":"B","text":"良いですね。生ビールを早く飲みたいです。"}];
INSERT INTO sentence [{"id":{"d":921,"n":1,"t":3},"speaker":"A","text":"優子が羽田空港に着く時間は、午後2時でしたか？"},{"id":{"d":921,"n":2,"t":3},"speaker":"B","text":"大体そのくらいです。優子は、「1時50分に着く」と言っていました。"},{"id":{"d":921,"n":3,"t":3},"speaker":"A","text":"では、12時半に家を出れば、余裕を持って迎えに行けますね。"},{"id":{"d":921,"n":4,"t":3},"speaker":"B","text":"早めに昼食を取ってから家を出るのは気忙しいので、空港で昼食を食べませんか？"},{"id":{"d":921,"n":5,"t":3},"speaker":"A","text":"はい。そうするならば、何時に家を出れば良いですか？"},{"id":{"d":921,"n":6,"t":3},"speaker":"B","text":"10時過ぎくらいに家を出ることにしましょう。"},{"id":{"d":921,"n":7,"t":3},"speaker":"A","text":"優子は昼食を食べてくるのでしょうか？"},{"id":{"d":921,"n":8,"t":3},"speaker":"B","text":"分かりませんが、食べていないときのために、空港のおすし屋さんですし折を作ってもらいましょう。"}];
INSERT INTO sentence [{"id":{"d":922,"n":1,"t":3},"speaker":"A","text":"農協の団体旅行で東京を訪問する予定です。"},{"id":{"d":922,"n":2,"t":3},"speaker":"B","text":"良いですね。"},{"id":{"d":922,"n":3,"t":3},"speaker":"A","text":"初日の夕方、東京の大学に行っている孫が宿泊するホテルへ会いに来てくれるのです。"},{"id":{"d":922,"n":4,"t":3},"speaker":"B","text":"お孫さんはもう大学3年生でしたか？"},{"id":{"d":922,"n":5,"t":3},"speaker":"A","text":"そうです。ホテルで一緒にお酒を飲めるのを楽しみにしているのです。"},{"id":{"d":922,"n":6,"t":3},"speaker":"B","text":"お孫さんが20歳を過ぎてから初めて会うのですか？"},{"id":{"d":922,"n":7,"t":3},"speaker":"A","text":"はい、そうです。どちらかというと東京観光よりそちらの方が楽しみです。"},{"id":{"d":922,"n":8,"t":3},"speaker":"B","text":"それは、本当に楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":923,"n":1,"t":3},"speaker":"A","text":"おれんじ食堂に乗ってきました。"},{"id":{"d":923,"n":2,"t":3},"speaker":"B","text":"動詞が間違っていませんか？食堂は「行ってきた」ですよね？"},{"id":{"d":923,"n":3,"t":3},"speaker":"A","text":"いいえ。「おれんじ食堂」という名前の観光列車があるのです。"},{"id":{"d":923,"n":4,"t":3},"speaker":"B","text":"だから「乗ってきた」なのですね。聞き慣れない名前の列車ですけれど、どこを走っているのですか？"},{"id":{"d":923,"n":5,"t":3},"speaker":"A","text":"熊本と鹿児島の間です。"},{"id":{"d":923,"n":6,"t":3},"speaker":"B","text":"食堂というからには、おいしいものが食べられるのですか？"},{"id":{"d":923,"n":7,"t":3},"speaker":"A","text":"はい。素晴らしいフレンチのコースをいただいてきました。"},{"id":{"d":923,"n":8,"t":3},"speaker":"B","text":"良いですね。私もおれんじ食堂に乗ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":924,"n":1,"t":3},"speaker":"A","text":"いつか宇宙旅行をしてみたいです。"},{"id":{"d":924,"n":2,"t":3},"speaker":"B","text":"宇宙旅行は夢のまた夢ですよね。"},{"id":{"d":924,"n":3,"t":3},"speaker":"A","text":"今は、お金を払えば宇宙ステーションまでなら、行けますよね。"},{"id":{"d":924,"n":4,"t":3},"speaker":"B","text":"億万長者になれれば、宇宙ステーションまで行けるのでしょうね。"},{"id":{"d":924,"n":5,"t":3},"speaker":"A","text":"私たちはUFOを見つけて乗せてもらうくらいしか、宇宙へ行く方法はないでしょうか？"},{"id":{"d":924,"n":6,"t":3},"speaker":"B","text":"それもさらに夢のまた夢ですよ。"},{"id":{"d":924,"n":7,"t":3},"speaker":"A","text":"UFOがたくさん目撃されている場所になら、私たちでも行けますよ。"},{"id":{"d":924,"n":8,"t":3},"speaker":"B","text":"それなら少しだけ現実味がありますね。今度行ってみましょうか？"}];
INSERT INTO sentence [{"id":{"d":925,"n":1,"t":3},"speaker":"A","text":"随分歩きましたが、目的地には、まだ着きませんか？"},{"id":{"d":925,"n":2,"t":3},"speaker":"B","text":"はい。地図には看板が出ていると書いてありましたが、まだ看板が見えません。"},{"id":{"d":925,"n":3,"t":3},"speaker":"A","text":"道を間違えたのかもしれません。少し戻ってみますか？"},{"id":{"d":925,"n":4,"t":3},"speaker":"B","text":"いいえ。ここまでの道中にも、看板はなかったですよ。"},{"id":{"d":925,"n":5,"t":3},"speaker":"A","text":"そうですか。このようなことになるなら、タクシーに乗れば良かったですね。"},{"id":{"d":925,"n":6,"t":3},"speaker":"B","text":"そうかもしれませんね。"},{"id":{"d":925,"n":7,"t":3},"speaker":"A","text":"携帯電話で、タクシーを呼びますか？"},{"id":{"d":925,"n":8,"t":3},"speaker":"B","text":"待ってください。あのカーブの先にのぼりが見えませんか？目的地はあの場所かもしれません。"}];
INSERT INTO sentence [{"id":{"d":926,"n":1,"t":3},"speaker":"A","text":"この先の駐車場に車を止めて、お参りしましょう。"},{"id":{"d":926,"n":2,"t":3},"speaker":"B","text":"運転、お疲れさまです。"},{"id":{"d":926,"n":3,"t":3},"speaker":"A","text":"ありがとうございます。車を降りたら階段を上りましょう。"},{"id":{"d":926,"n":4,"t":3},"speaker":"B","text":"階段を上がるのは車を運転するより疲れそうですね。"},{"id":{"d":926,"n":5,"t":3},"speaker":"A","text":"そうですね。でも、この神社にはどうしても来たかったので、長い階段も頑張って上ります。"},{"id":{"d":926,"n":6,"t":3},"speaker":"B","text":"御朱印帳は忘れずに持ってきましたか？"},{"id":{"d":926,"n":7,"t":3},"speaker":"A","text":"はい。車の中のバッグに入れたままでした。階段を上がる前に教えてもらえて良かったです。"},{"id":{"d":926,"n":8,"t":3},"speaker":"B","text":"そのようなことだろうと思って、一応言ってみて良かったです。"}];
INSERT INTO sentence [{"id":{"d":927,"n":1,"t":3},"speaker":"A","text":"今日はどこかでお祭りがあるのですか？"},{"id":{"d":927,"n":2,"t":3},"speaker":"B","text":"どうでしょうか。私は知りません。"},{"id":{"d":927,"n":3,"t":3},"speaker":"A","text":"駅に浴衣を着た女の子がたくさんいましたよ。"},{"id":{"d":927,"n":4,"t":3},"speaker":"B","text":"そうなのですか？どこかで花火大会でもあるのかもしれませんね。"},{"id":{"d":927,"n":5,"t":3},"speaker":"A","text":"インターネット検索をして調べたら、海岸で納涼花火大会があるようですね。"},{"id":{"d":927,"n":6,"t":3},"speaker":"B","text":"その花火大会は、私も昔、デートで行きましたよ。"},{"id":{"d":927,"n":7,"t":3},"speaker":"A","text":"うらやましいデートをしていたのですね。"},{"id":{"d":927,"n":8,"t":3},"speaker":"B","text":"たこ焼きのソースが浴衣についてしまい、恥ずかしかったことを覚えています。"}];
INSERT INTO sentence [{"id":{"d":928,"n":1,"t":3},"speaker":"A","text":"新型コロナウイルス感染症の影響で、この2年ほど全く旅行をしていません。"},{"id":{"d":928,"n":2,"t":3},"speaker":"B","text":"最近、行動制限が解除されましたが、どこかへ行く計画がありますか？"},{"id":{"d":928,"n":3,"t":3},"speaker":"A","text":"はい。遠くへ行くのはまだ不安なので、伊豆辺りに行こうかと思っています。"},{"id":{"d":928,"n":4,"t":3},"speaker":"B","text":"テレビ番組を見ると、旅行先は熱海が人気のようですね。"},{"id":{"d":928,"n":5,"t":3},"speaker":"A","text":"熱海は人出が多そうなので、私は別のところにしようと思いまして。"},{"id":{"d":928,"n":6,"t":3},"speaker":"B","text":"それで伊豆ですね。伊豆高原や西伊豆などでしょうか？"},{"id":{"d":928,"n":7,"t":3},"speaker":"A","text":"はい。伊豆スカイラインをドライブして、伊豆高原に行くのはどうかと思っているのです。"},{"id":{"d":928,"n":8,"t":3},"speaker":"B","text":"それは爽快でしょうね。久しぶりの旅行を楽しんでください。"}];
INSERT INTO sentence [{"id":{"d":929,"n":1,"t":3},"speaker":"A","text":"先月、北陸で大雪が降ったでしょう。"},{"id":{"d":929,"n":2,"t":3},"speaker":"B","text":"はい、覚えています。数十年に一度の大雪だったとか。"},{"id":{"d":929,"n":3,"t":3},"speaker":"A","text":"あのとき、偶然にも新幹線で北陸を旅行していたのです。"},{"id":{"d":929,"n":4,"t":3},"speaker":"B","text":"そうでしたか。それで影響がありましたか？"},{"id":{"d":929,"n":5,"t":3},"speaker":"A","text":"はい。その新幹線が雪で動けなくなってしまったのです。"},{"id":{"d":929,"n":6,"t":3},"speaker":"B","text":"そういえば、テレビのニュース番組で報道していましたね。"},{"id":{"d":929,"n":7,"t":3},"speaker":"A","text":"何時間も新幹線の中に閉じ込められてしまいました。"},{"id":{"d":929,"n":8,"t":3},"speaker":"B","text":"それは大変でしたね。その間、どうしていたのですか？"},{"id":{"d":929,"n":9,"t":3},"speaker":"A","text":"眠ろうと思っても眠れず、本を読もうと思っても落ち着いて読めず、ただ待つしかありませんでした。"}];
INSERT INTO sentence [{"id":{"d":930,"n":1,"t":3},"speaker":"A","text":"そばは、お好きですか？"},{"id":{"d":930,"n":2,"t":3},"speaker":"B","text":"はい、大好きです。以前、そばを食べ歩いたことがあります。"},{"id":{"d":930,"n":3,"t":3},"speaker":"A","text":"東京には老舗のそば屋がたくさんありますね。"},{"id":{"d":930,"n":4,"t":3},"speaker":"B","text":"はい、神田の長生庵やかぐらそば、日本橋の更科屋、麻布の滴亭などいろいろ行きました。"},{"id":{"d":930,"n":5,"t":3},"speaker":"A","text":"どこのお店がお薦めですか？"},{"id":{"d":930,"n":6,"t":3},"speaker":"B","text":"それぞれに特色があって、一概にどこのお店が良いとも言えません。"},{"id":{"d":930,"n":7,"t":3},"speaker":"A","text":"例えば、どのような特色がありますか？"},{"id":{"d":930,"n":8,"t":3},"speaker":"B","text":"長生庵にはかきそばやまつたけそばがあります。かぐらそばはそばとつけ汁の相性が良いですし、更科屋は甘めのつけ汁がおいしいです。"},{"id":{"d":930,"n":9,"t":3},"speaker":"A","text":"私も麻布の滴亭で三色そばを食べたことがあります。ゆずやよもぎの香りがしておいしかったです。"},{"id":{"d":930,"n":10,"t":3},"speaker":"B","text":"そうですよね。季節ごとにメニューが変わるのも楽しみの一つです。"}];
INSERT INTO sentence [{"id":{"d":931,"n":1,"t":3},"speaker":"A","text":"沖縄に行ったことがありますか？"},{"id":{"d":931,"n":2,"t":3},"speaker":"B","text":"いいえ。いつか行きたいと思っているのですが、まだ行ったことがありません。"},{"id":{"d":931,"n":3,"t":3},"speaker":"A","text":"私は先週、家族で沖縄に行って来ました。"},{"id":{"d":931,"n":4,"t":3},"speaker":"B","text":"そうですか。どうでしたか？"},{"id":{"d":931,"n":5,"t":3},"speaker":"A","text":"良かったです。ずっと天気もよくて、十二分に楽しめました。"},{"id":{"d":931,"n":6,"t":3},"speaker":"B","text":"どこかお薦めの観光地はありますか？今度、沖縄に行くときの参考にしたいので教えてください。"},{"id":{"d":931,"n":7,"t":3},"speaker":"A","text":"水納島のクロワッサンアイランドが良かったですね。真っ白な砂浜が続く三日月型の美しい島で、その形からクロワッサンアイランドと呼ばれています。"},{"id":{"d":931,"n":8,"t":3},"speaker":"B","text":"私はダイビングが趣味なのですが、そこでもできるでしょうか？"},{"id":{"d":931,"n":9,"t":3},"speaker":"A","text":"もちろんです。シュノーケルやダイビングが盛んなところです。"},{"id":{"d":931,"n":10,"t":3},"speaker":"B","text":"沖縄に行く機会があれば、ぜひその島にも行ってみます。"}];
INSERT INTO sentence [{"id":{"d":932,"n":1,"t":3},"speaker":"A","text":"去年、団体旅行でイタリアを訪問しました。"},{"id":{"d":932,"n":2,"t":3},"speaker":"B","text":"イタリアのどこを回ったのですか？"},{"id":{"d":932,"n":3,"t":3},"speaker":"A","text":"ミラノ、ベニス、フィレンツェ、ローマを回りました。"},{"id":{"d":932,"n":4,"t":3},"speaker":"B","text":"代表的な観光地を網羅しているのですね。"},{"id":{"d":932,"n":5,"t":3},"speaker":"A","text":"それぞれ1泊ずつなので、少し慌ただしかったですがね。"},{"id":{"d":932,"n":6,"t":3},"speaker":"B","text":"団体旅行ではよくあることですよね。どこか印象に残ったところはありましたか？"},{"id":{"d":932,"n":7,"t":3},"speaker":"A","text":"ベニスの運河、フィレンツェのアルノ川沿いの街並み、ローマのフォロ・ロマーノなど、どこも美しくてとても感動しました。"},{"id":{"d":932,"n":8,"t":3},"speaker":"B","text":"それは良かったですね。機会があれば、私も行きたいと思います。"},{"id":{"d":932,"n":9,"t":3},"speaker":"A","text":"ぜひ、イタリアに行くことをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":933,"n":1,"t":3},"speaker":"A","text":"外国を旅行していて困るのは、急に病気になることですね。"},{"id":{"d":933,"n":2,"t":3},"speaker":"B","text":"そうですね。病院を探すのも難しいですし、言葉が通じないと医者に症状を上手に説明できませんよね。"},{"id":{"d":933,"n":3,"t":3},"speaker":"A","text":"去年フランスに行ったとき、風邪をひいて困りました。熱は出るし、せきは止まらないし、散々な目に会いました。"},{"id":{"d":933,"n":4,"t":3},"speaker":"B","text":"それは心細かったでしょうね。それでどうしたのですか？"},{"id":{"d":933,"n":5,"t":3},"speaker":"A","text":"偶然にも友人がいたので、その友人に助けてもらいました。"},{"id":{"d":933,"n":6,"t":3},"speaker":"B","text":"それは心強かったですね。病院に連れて行ってもらったのですか？"},{"id":{"d":933,"n":7,"t":3},"speaker":"A","text":"いえ、日曜日だったので病院はどこも休みでした。それで、友人の知り合いの医者に頼んで往診してもらったのです。"},{"id":{"d":933,"n":8,"t":3},"speaker":"B","text":"それは運が良かったですね。普通はそのように都合良くはいきませんよ。"}];
INSERT INTO sentence [{"id":{"d":934,"n":1,"t":3},"speaker":"A","text":"今度の夏休みに、どこに行くか決めましたか？"},{"id":{"d":934,"n":2,"t":3},"speaker":"B","text":"いえ、まだ決めていません。"},{"id":{"d":934,"n":3,"t":3},"speaker":"A","text":"釧路湿原でカヌーに乗るのはどうですか？"},{"id":{"d":934,"n":4,"t":3},"speaker":"B","text":"私はカヌーに乗ったことがありませんが、大丈夫でしょうか？"},{"id":{"d":934,"n":5,"t":3},"speaker":"A","text":"はい。ガイドさんが操船してくれるので心配ありませんよ。"},{"id":{"d":934,"n":6,"t":3},"speaker":"B","text":"それなら安心ですね。カヌーに乗って、雄大な景色の中を進むのは気持ちが良いでしょうね。"},{"id":{"d":934,"n":7,"t":3},"speaker":"A","text":"エゾシカやキタキツネ、オジロワシ、タンチョウヅルが見られるかもしれません。"},{"id":{"d":934,"n":8,"t":3},"speaker":"B","text":"それは楽しみです。誘ってくださってありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":935,"n":1,"t":3},"speaker":"A","text":"ヨーロッパを旅行していたとき、一人で自転車旅行をしていた16歳の少年に会ったことがあります。"},{"id":{"d":935,"n":2,"t":3},"speaker":"B","text":"それは勇気のある少年ですね。"},{"id":{"d":935,"n":3,"t":3},"speaker":"A","text":"幼い感じの小さな少年で、とてもそのようなことをするようには見えませんでした。"},{"id":{"d":935,"n":4,"t":3},"speaker":"B","text":"そのような年齢でそのようなことをするとは、何か考えることがあるのでしょうね。"},{"id":{"d":935,"n":5,"t":3},"speaker":"A","text":"本人の話によれば、学校が嫌でたまらなかったのだそうです。"},{"id":{"d":935,"n":6,"t":3},"speaker":"B","text":"両親がよく許しましたね。"},{"id":{"d":935,"n":7,"t":3},"speaker":"A","text":"もちろん両親は大反対だったそうですが、それを振り切って実行したそうです。"},{"id":{"d":935,"n":8,"t":3},"speaker":"B","text":"しっかりした少年ですね。将来が楽しみです。"},{"id":{"d":935,"n":9,"t":3},"speaker":"A","text":"その後、連絡がないので、今頃どうしているかは知りませんが、きっとたくましくなっていることと思います。"}];
INSERT INTO sentence [{"id":{"d":936,"n":1,"t":3},"speaker":"A","text":"北海道が好きで、よくドライブ旅行をします。"},{"id":{"d":936,"n":2,"t":3},"speaker":"B","text":"東京から北海道まで車で行くのですか？"},{"id":{"d":936,"n":3,"t":3},"speaker":"A","text":"北海道まではフェリーで行きます。"},{"id":{"d":936,"n":4,"t":3},"speaker":"B","text":"東京の港からフェリーが出ているのですか？"},{"id":{"d":936,"n":5,"t":3},"speaker":"A","text":"いえ、フェリーは茨城県の大洗港から出ます。苫小牧港まで1晩かかります。"},{"id":{"d":936,"n":6,"t":3},"speaker":"B","text":"北海道でドライブするのは気持ちが良いでしょうね。"},{"id":{"d":936,"n":7,"t":3},"speaker":"A","text":"はい、景色が雄大でまるで日本ではないようです。"},{"id":{"d":936,"n":8,"t":3},"speaker":"B","text":"どこに行かれましたか？"},{"id":{"d":936,"n":9,"t":3},"speaker":"A","text":"ニセコや富良野などに行きました。稚内まで行ったこともあります。"},{"id":{"d":936,"n":10,"t":3},"speaker":"B","text":"テレビの旅番組でよく見ますね。私も行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":937,"n":1,"t":3},"speaker":"A","text":"宿泊の予約をキャンセルしたいのですがよろしいでしょうか？"},{"id":{"d":937,"n":2,"t":3},"speaker":"B","text":"かしこまりました。お名前をお聞かせください。"},{"id":{"d":937,"n":3,"t":3},"speaker":"A","text":"影山と言います。影山洋介です。"},{"id":{"d":937,"n":4,"t":3},"speaker":"B","text":"少々お待ちください。6月25日から2泊3日のご予定ですね。"},{"id":{"d":937,"n":5,"t":3},"speaker":"A","text":"はい、そうです。少し都合が悪くなり、行けなくなりました。"},{"id":{"d":937,"n":6,"t":3},"speaker":"B","text":"承知いたしました。手数料を50パーセント頂きますが、よろしいでしょうか？"},{"id":{"d":937,"n":7,"t":3},"speaker":"A","text":"それで結構です。"},{"id":{"d":937,"n":8,"t":3},"speaker":"B","text":"それでは、影山洋介さま、6月25日から2泊3日、確かにキャンセルを承りました。"}];
INSERT INTO sentence [{"id":{"d":938,"n":1,"t":3},"speaker":"A","text":"明日から海外観光客の入国が許可されるそうです。"},{"id":{"d":938,"n":2,"t":3},"speaker":"B","text":"ようやく入国制限が解除されるのですね。"},{"id":{"d":938,"n":3,"t":3},"speaker":"A","text":"観光業の方もほっとされていることでしょう。"},{"id":{"d":938,"n":4,"t":3},"speaker":"B","text":"本当に、この2年間、苦しい時期が続きましたからね。"},{"id":{"d":938,"n":5,"t":3},"speaker":"A","text":"しかし、この入国制限の解除は団体客に限られているので、個人客を当てにしている業者は残念そうです。"},{"id":{"d":938,"n":6,"t":3},"speaker":"B","text":"先月の観光視察団で新型コロナウイルス感染者が1人出たので、政府も慎重になっているのでしょう。"},{"id":{"d":938,"n":7,"t":3},"speaker":"A","text":"そうですね。入国制限の解除を皮切りに、コロナ禍からの回復がこのまま順調にいけば良いのですが。"},{"id":{"d":938,"n":8,"t":3},"speaker":"B","text":"まだ油断はできませんが、成り行きとしてはコロナ禍が終息する方向に向かうでしょう。"},{"id":{"d":938,"n":9,"t":3},"speaker":"A","text":"そう願わずにはいられません。"}];
INSERT INTO sentence [{"id":{"d":939,"n":1,"t":3},"speaker":"A","text":"地方を旅行するときの楽しみの一つに、古本屋巡りがあります。"},{"id":{"d":939,"n":2,"t":3},"speaker":"B","text":"私は、地方を旅行するときの楽しみは、専ら食べ歩きですね。古本屋を巡ったことはありません。"},{"id":{"d":939,"n":3,"t":3},"speaker":"A","text":"楽しみは人それぞれですが、古本屋巡りもなかなか面白いものですよ。"},{"id":{"d":939,"n":4,"t":3},"speaker":"B","text":"地方の古本屋ならではの楽しみがあるのでしょうね。"},{"id":{"d":939,"n":5,"t":3},"speaker":"A","text":"私は歴史に興味があるのですが、その土地の歴史や風物を詳しく紹介した本は、東京ではなかなか手に入りません。"},{"id":{"d":939,"n":6,"t":3},"speaker":"B","text":"今までに何か掘り出し物はありましたか？"},{"id":{"d":939,"n":7,"t":3},"speaker":"A","text":"はい。古い城下町のある地方に行くと、興味深い本に巡り合います。"},{"id":{"d":939,"n":8,"t":3},"speaker":"B","text":"なるほど。次第に古本屋巡りの楽しみが想像できるようになりました。"}];
INSERT INTO sentence [{"id":{"d":940,"n":1,"t":3},"speaker":"A","text":"ドイツを旅行したいのですが、お薦めのツアーはありますか？"},{"id":{"d":940,"n":2,"t":3},"speaker":"B","text":"それならロマンチック街道を巡るツアーはいかがですか？"},{"id":{"d":940,"n":3,"t":3},"speaker":"A","text":"名前を聞くと気持ちが動きますね。どのようなツアーですか？"},{"id":{"d":940,"n":4,"t":3},"speaker":"B","text":"中世の面影を残す街並みや、アルプスの大自然の中にある古城などを巡るツアーです。"},{"id":{"d":940,"n":5,"t":3},"speaker":"A","text":"良さそうですね。もう少し詳しく説明してくれませんか？"},{"id":{"d":940,"n":6,"t":3},"speaker":"B","text":"例えば、ローテンブルクという町は、14世紀の美しい街並みがそのまま残るメルヘンの世界のような町です。"},{"id":{"d":940,"n":7,"t":3},"speaker":"A","text":"古城にはどのようなものがあるのですか？"},{"id":{"d":940,"n":8,"t":3},"speaker":"B","text":"ノイシュバンシュタイン城といって、ワーグナーのパトロンだったルートヴィッヒ2世が建てた城があります。"},{"id":{"d":940,"n":9,"t":3},"speaker":"A","text":"ぜひとも行ってみたいです。そのツアーに決めます。"}];
INSERT INTO sentence [{"id":{"d":941,"n":1,"t":3},"speaker":"A","text":"今日のスケジュールを教えてください。"},{"id":{"d":941,"n":2,"t":3},"speaker":"B","text":"午前7時から朝食です。朝食の後、8時に次の目的地に向けて出発します。"},{"id":{"d":941,"n":3,"t":3},"speaker":"A","text":"出発まであまり時間がありませんね。それでは、大急ぎで準備しなければなりませんね。"},{"id":{"d":941,"n":4,"t":3},"speaker":"B","text":"申し訳ありません。予定が詰まっておりますので。"},{"id":{"d":941,"n":5,"t":3},"speaker":"A","text":"目的地には何時ごろ到着しますか？"},{"id":{"d":941,"n":6,"t":3},"speaker":"B","text":"11時に到着します。途中、道の駅で30分ほど休憩いたします。"},{"id":{"d":941,"n":7,"t":3},"speaker":"A","text":"それは助かります。小まめにトイレ休憩が取れるのはありがたいです。"},{"id":{"d":941,"n":8,"t":3},"speaker":"B","text":"その他にご要望がありましたら、何でも仰せ付けください。"},{"id":{"d":941,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。どうぞよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":942,"n":1,"t":3},"speaker":"A","text":"銀婚旅行の行き先はどこにしましょうか？"},{"id":{"d":942,"n":2,"t":3},"speaker":"B","text":"私はどこでも良いです。あなたにお任せします。"},{"id":{"d":942,"n":3,"t":3},"speaker":"A","text":"何か気のない返事ですね。"},{"id":{"d":942,"n":4,"t":3},"speaker":"B","text":"すみません。会社の仕事が忙しくてそこまで頭が回っていません。何か案がありますか？"},{"id":{"d":942,"n":5,"t":3},"speaker":"A","text":"瀬戸内海周遊ツアーというのはどうでしょうか？"},{"id":{"d":942,"n":6,"t":3},"speaker":"B","text":"良さそうですね。私も瀬戸内海はいつか旅行してみたいと思っていました。"},{"id":{"d":942,"n":7,"t":3},"speaker":"A","text":"東京発で2泊3日の瀬戸内海周遊ツアーがあります。クルーズ船で瀬戸内海を巡ります。"},{"id":{"d":942,"n":8,"t":3},"speaker":"B","text":"おいしいものが食べられそうですね。"}];
INSERT INTO sentence [{"id":{"d":943,"n":1,"t":3},"speaker":"A","text":"大学時代に友人と、北海道でヒッチハイクをしたことがあります。"},{"id":{"d":943,"n":2,"t":3},"speaker":"B","text":"ヒッチハイクですか？車がなかなか止まってくれなかったのではありませんか？"},{"id":{"d":943,"n":3,"t":3},"speaker":"A","text":"そうなのですよ。手を上げているのになかなか車は止まってくれませんでした。"},{"id":{"d":943,"n":4,"t":3},"speaker":"B","text":"私がその状況で車を運転していたとしても、よほどのことでなければ車を止める気にはなりませんよ。"},{"id":{"d":943,"n":5,"t":3},"speaker":"A","text":"それでも何とかなるもので、トラックが止まってくれました。"},{"id":{"d":943,"n":6,"t":3},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":943,"n":7,"t":3},"speaker":"A","text":"いろいろと面白い話を聞かせてくれました。最後はドライブインに寄って、ラーメンをごちそうしてくれました。"},{"id":{"d":943,"n":8,"t":3},"speaker":"B","text":"親切な運転手さんがいたものですね。"},{"id":{"d":943,"n":9,"t":3},"speaker":"A","text":"はい。そのような人に出会うことができて、私は幸運でした。"}];
INSERT INTO sentence [{"id":{"d":944,"n":1,"t":3},"speaker":"A","text":"先週、家族でぶどう狩りに行って来ました。"},{"id":{"d":944,"n":2,"t":3},"speaker":"B","text":"山梨ですか？あちらの地域はぶどうの栽培が盛んですよね。"},{"id":{"d":944,"n":3,"t":3},"speaker":"A","text":"はい。山梨の勝沼にあるぶどう園です。土産物店から送迎バスが出ていました。"},{"id":{"d":944,"n":4,"t":3},"speaker":"B","text":"観光客で混雑していましたか？"},{"id":{"d":944,"n":5,"t":3},"speaker":"A","text":"いいえ。私たちがぶどう園へ行ったのは平日だったので、それほどでもありませんでした。"},{"id":{"d":944,"n":6,"t":3},"speaker":"B","text":"お子さんたちは喜んだでしょう？"},{"id":{"d":944,"n":7,"t":3},"speaker":"A","text":"はい、みんな大はしゃぎでした。巨峰やシャインマスカットなど、たくさん食べていました。"},{"id":{"d":944,"n":8,"t":3},"speaker":"B","text":"シャインマスカットもあるのですか？市販のものは高くて手が出ませんよね。"},{"id":{"d":944,"n":9,"t":3},"speaker":"A","text":"はい。スタッフの方においしいシャインマスカットの選び方を教えてもらいました。"},{"id":{"d":944,"n":10,"t":3},"speaker":"B","text":"家族で楽しめたのは良かったですね。"},{"id":{"d":944,"n":11,"t":3},"speaker":"A","text":"はい、みんな「また行きたい」と言っています。来年も行くつもりです。"}];
INSERT INTO sentence [{"id":{"d":945,"n":1,"t":3},"speaker":"A","text":"チェックアウトの時間は何時ですか？"},{"id":{"d":945,"n":2,"t":3},"speaker":"B","text":"11時までにお願いします。"},{"id":{"d":945,"n":3,"t":3},"speaker":"A","text":"遅くなるかもしれませんが、かまいませんか？"},{"id":{"d":945,"n":4,"t":3},"speaker":"B","text":"申しわけありませんが、その場合は延長料金を頂くことになっております。"},{"id":{"d":945,"n":5,"t":3},"speaker":"A","text":"料金はおいくらですか？"},{"id":{"d":945,"n":6,"t":3},"speaker":"B","text":"1時間につき1,000円を頂きます。"},{"id":{"d":945,"n":7,"t":3},"speaker":"A","text":"チェックアウトの後で荷物を預けることはできますか？"},{"id":{"d":945,"n":8,"t":3},"speaker":"B","text":"はい、それはかまいません。荷物の大きさや数をあらかじめお知らせください。"},{"id":{"d":945,"n":9,"t":3},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":946,"n":1,"t":3},"speaker":"A","text":"下山している途中で転んで、手を擦りむいてしまいました。"},{"id":{"d":946,"n":2,"t":3},"speaker":"B","text":"血が出ていますね。早く手当をした方が善いですよ。"},{"id":{"d":946,"n":3,"t":3},"speaker":"A","text":"あいにく包帯もばんそうこうも何も持っていません。"},{"id":{"d":946,"n":4,"t":3},"speaker":"B","text":"ハンカチを持っていますので、それで応急処置をしておきましょう。"},{"id":{"d":946,"n":5,"t":3},"speaker":"A","text":"すみません。このようなときのために、しっかり準備しておくべきでした。"},{"id":{"d":946,"n":6,"t":3},"speaker":"B","text":"町に着いたらドラッグストアを探しましょう。"},{"id":{"d":946,"n":7,"t":3},"speaker":"A","text":"何から何までありがとうございます。大変助かります。"},{"id":{"d":946,"n":8,"t":3},"speaker":"B","text":"困ったときはお互いさまです。"}];
INSERT INTO sentence [{"id":{"d":947,"n":1,"t":3},"speaker":"A","text":"来週、タイに出張してもらえますか？"},{"id":{"d":947,"n":2,"t":3},"speaker":"B","text":"それは構いませんが、随分と急な出張ですね。何か起きたのですか？"},{"id":{"d":947,"n":3,"t":3},"speaker":"A","text":"工場のラインがストップしたのです。"},{"id":{"d":947,"n":4,"t":3},"speaker":"B","text":"現地にも優秀な技術員がいるはずですが、対処できないのでしょうか？"},{"id":{"d":947,"n":5,"t":3},"speaker":"A","text":"はい。現地の従業員だけでは、原因を突き止めることができないそうです。"},{"id":{"d":947,"n":6,"t":3},"speaker":"B","text":"分かりました。早速、出張の準備をします。"},{"id":{"d":947,"n":7,"t":3},"speaker":"A","text":"飛行機の予約は済ませてあります。来週の月曜日、午後1時の出発です。"},{"id":{"d":947,"n":8,"t":3},"speaker":"B","text":"原因を突き止めるのにどのくらいの時間がかかるか予想がつきませんが、早急に解決できるよう努力します。"},{"id":{"d":947,"n":9,"t":3},"speaker":"A","text":"よろしくお願いします。困ったことが起きたら、連絡をしてください。"}];
INSERT INTO sentence [{"id":{"d":948,"n":1,"t":3},"speaker":"A","text":"自動車で米国を横断してきました。前から計画していたのです。"},{"id":{"d":948,"n":2,"t":3},"speaker":"B","text":"それは素晴らしいことですね。どのようなルートを通ったのですか？"},{"id":{"d":948,"n":3,"t":3},"speaker":"A","text":"ニューヨーク市を出発して、シカゴ、カンザスシティー、コロラドを経由して、ロサンゼルスまで旅行しました。"},{"id":{"d":948,"n":4,"t":3},"speaker":"B","text":"全部で何日かかりましたか？"},{"id":{"d":948,"n":5,"t":3},"speaker":"A","text":"30日かかりました。横断した距離は、約6,000キロメートルですから、一日に200キロメートルを走りました。"},{"id":{"d":948,"n":6,"t":3},"speaker":"B","text":"1日に200キロメートルを運転するのは、大変だったのではありませんか？"},{"id":{"d":948,"n":7,"t":3},"speaker":"A","text":"いいえ。運転していたのは1日に4時間ほどです。食事や休憩の時間も十分確保できたので、持ったよりも楽でした。"},{"id":{"d":948,"n":8,"t":3},"speaker":"B","text":"そうなのですね。費用はどのくらいかかったのですか？"},{"id":{"d":948,"n":9,"t":3},"speaker":"A","text":"ホテル代、レンタカー代、ガソリン代、食費、その他を含めて、2人で約100万円かかりました。"},{"id":{"d":948,"n":10,"t":3},"speaker":"B","text":"30日で100万円なら、それほど高くありませんね。私にも実現できるかもしれません。"}];
INSERT INTO sentence [{"id":{"d":949,"n":1,"t":3},"speaker":"A","text":"先日、渓流釣りに行っていて怖い目に遭いました。"},{"id":{"d":949,"n":2,"t":3},"speaker":"B","text":"どうしたのですか？増水でもしたのですか？"},{"id":{"d":949,"n":3,"t":3},"speaker":"A","text":"クマに遭遇したのです。釣りに夢中になっていたので、クマがいることに気が付きませんでした。"},{"id":{"d":949,"n":4,"t":3},"speaker":"B","text":"それは怖かったでしょうね。"},{"id":{"d":949,"n":5,"t":3},"speaker":"A","text":"気が付いたら、10メートルぐらい先の対岸に、クマがいたのです。"},{"id":{"d":949,"n":6,"t":3},"speaker":"B","text":"それで、あなたはどうしたのですか？"},{"id":{"d":949,"n":7,"t":3},"speaker":"A","text":"逃げたら襲われると思いましたから、少しずつ後退りして何とか助かりました。"},{"id":{"d":949,"n":8,"t":3},"speaker":"B","text":"それは賢明でしたね。それにしても生きた心地がしなかったでしょうね。"},{"id":{"d":949,"n":9,"t":3},"speaker":"A","text":"あれほど緊張したことはありませんでした。"}];
INSERT INTO sentence [{"id":{"d":950,"n":1,"t":3},"speaker":"A","text":"レギュラーガソリンを満タンでお願いします。"},{"id":{"d":950,"n":2,"t":3},"speaker":"B","text":"かしこまりました。お支払いは現金ですか？それともクレジットカードですか？"},{"id":{"d":950,"n":3,"t":3},"speaker":"A","text":"クレジットカードでお願いします。"},{"id":{"d":950,"n":4,"t":3},"speaker":"B","text":"ポイントカードをお持ちですか？"},{"id":{"d":950,"n":5,"t":3},"speaker":"A","text":"はい。Tポイントカードは使えますか？"},{"id":{"d":950,"n":6,"t":3},"speaker":"B","text":"はい、使えます。1リットル当たり5円お安くなります。"},{"id":{"d":950,"n":7,"t":3},"speaker":"A","text":"それはありがたいです。このところ、随分ガソリン価格が値上がりしていますから。"},{"id":{"d":950,"n":8,"t":3},"speaker":"B","text":"そうですね。石油の元売り価格が上がって、私どもも困っています。"},{"id":{"d":950,"n":9,"t":3},"speaker":"A","text":"早く元に戻ってほしいものです。"}];
INSERT INTO sentence [{"id":{"d":951,"n":1,"t":3},"speaker":"A","text":"仕事が忙しかったから、次の休みはどこかの温泉にでも漬かって休みたいですね。"},{"id":{"d":951,"n":2,"t":3},"speaker":"B","text":"どこが良いでしょうね。近場で良い温泉はありますか？"},{"id":{"d":951,"n":3,"t":3},"speaker":"A","text":"下部温泉などは、どうでしょうか。武田信玄の隠し湯が存在したところです。"},{"id":{"d":951,"n":4,"t":3},"speaker":"B","text":"下部温泉は、名湯百選に選ばれたこともあるそうですね。山間に良い温泉宿があるかもしれません。"},{"id":{"d":951,"n":5,"t":3},"speaker":"A","text":"じゃらんを調べてみましょう。"},{"id":{"d":951,"n":6,"t":3},"speaker":"B","text":"良い温泉はありましたか？"},{"id":{"d":951,"n":7,"t":3},"speaker":"A","text":"はい。「温泉宿　岩屋」というのがありましたよ。"},{"id":{"d":951,"n":8,"t":3},"speaker":"B","text":"どのようなところですか？"},{"id":{"d":951,"n":9,"t":3},"speaker":"A","text":"料理がおいしいことで有名だそうです。湯葉料理や、やまめの塩焼きが出るそうですよ。"},{"id":{"d":951,"n":10,"t":3},"speaker":"B","text":"良さそうですね。どのようにして行けばよいのですか？"},{"id":{"d":951,"n":11,"t":3},"speaker":"A","text":"JR身延線下部温泉駅から徒歩3分と書いてあります。"},{"id":{"d":951,"n":12,"t":3},"speaker":"B","text":"そこにしましょう。予約をお願いします。"}];
INSERT INTO sentence [{"id":{"d":952,"n":1,"t":3},"speaker":"A","text":"もうすぐ頂上ですから、頑張ってください。"},{"id":{"d":952,"n":2,"t":3},"speaker":"B","text":"やっと頂上に着きますか。あとどのくらいですか？"},{"id":{"d":952,"n":3,"t":3},"speaker":"A","text":"500メートルくらいです。あと20分も歩けば着きますよ。"},{"id":{"d":952,"n":4,"t":3},"speaker":"B","text":"まだそれほどの距離があるのですか？足が震えてもう一歩も歩けません。"},{"id":{"d":952,"n":5,"t":3},"speaker":"A","text":"そう言わずに頑張って歩いてください。手を引いてあげましょうか？"},{"id":{"d":952,"n":6,"t":3},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"},{"id":{"d":952,"n":7,"t":3},"speaker":"A","text":"頂上についたら絶景が待っていますよ。あと少し頑張りましょう。"},{"id":{"d":952,"n":8,"t":3},"speaker":"B","text":"そうですね。それを楽しみに、最後の力をふりしぼって歩きます。"}];
INSERT INTO sentence [{"id":{"d":953,"n":1,"t":3},"speaker":"A","text":"夏休みは、キャンピングカーに乗って旅行をしましょう。"},{"id":{"d":953,"n":2,"t":3},"speaker":"B","text":"賛成です。子どもたちも喜ぶでしょう。"},{"id":{"d":953,"n":3,"t":3},"speaker":"A","text":"インターネットでオートキャンプ場を探しておきました。"},{"id":{"d":953,"n":4,"t":3},"speaker":"B","text":"良さそうなところがありましたか？"},{"id":{"d":953,"n":5,"t":3},"speaker":"A","text":"はい。富士山麓の本栖湖や八ヶ岳山麓の清里に、キャンピングカーが乗り入れ可能なキャンプ場があります。"},{"id":{"d":953,"n":6,"t":3},"speaker":"B","text":"どちらも子どもたちは初めて行く場所ですね。"},{"id":{"d":953,"n":7,"t":3},"speaker":"A","text":"そうです。学校から子どもたちが帰ったら話しをしましょう。"},{"id":{"d":953,"n":8,"t":3},"speaker":"B","text":"子どもたちは大喜びするでしょう。"}];
INSERT INTO sentence [{"id":{"d":954,"n":1,"t":3},"speaker":"A","text":"宇宙旅行も可能な時代になりましたね。"},{"id":{"d":954,"n":2,"t":3},"speaker":"B","text":"そうですね。最近では、民間人だけで宇宙船を操縦して宇宙旅行に出掛けていましたね。"},{"id":{"d":954,"n":3,"t":3},"speaker":"A","text":"でも、宇宙旅行の費用は高いので、今はまだ大富豪しか行けませんね。"},{"id":{"d":954,"n":4,"t":3},"speaker":"B","text":"そのうち、われわれのような庶民でも宇宙へ行けるような時代になるでしょうか？"},{"id":{"d":954,"n":5,"t":3},"speaker":"A","text":"そうなってほしいと思っています。いずれは、月や火星に行く時代になる可能性もありますよ。"},{"id":{"d":954,"n":6,"t":3},"speaker":"B","text":"人類の移住もプログラムに入ってくるかもしれませんね。"},{"id":{"d":954,"n":7,"t":3},"speaker":"A","text":"夢物語のようですが、実現するのは意外に早いかもしれません。"},{"id":{"d":954,"n":8,"t":3},"speaker":"B","text":"はい。技術の進歩は驚くほど速いですからね。"}];
INSERT INTO sentence [{"id":{"d":955,"n":1,"t":3},"speaker":"A","text":"車椅子に乗っているので、旅行したくてもなかなか出掛けられません。"},{"id":{"d":955,"n":2,"t":3},"speaker":"B","text":"そのような人のためのツアーもございますよ。"},{"id":{"d":955,"n":3,"t":3},"speaker":"A","text":"そうなのですか？それはどのようなツアーですか？"},{"id":{"d":955,"n":4,"t":3},"speaker":"B","text":"専任の添乗員が同行して、いろいろな介助させていただきます。"},{"id":{"d":955,"n":5,"t":3},"speaker":"A","text":"それは助かります。何しろ車椅子だと不自由なことがたくさんありますから。"},{"id":{"d":955,"n":6,"t":3},"speaker":"B","text":"乗り物の乗降や、坂道での通行サポートなども、お手伝いします。"},{"id":{"d":955,"n":7,"t":3},"speaker":"A","text":"お手洗いが一人ではできませんが、それもサポートしてくれますか？"},{"id":{"d":955,"n":8,"t":3},"speaker":"B","text":"はい、もちろんです。"},{"id":{"d":955,"n":9,"t":3},"speaker":"A","text":"それはありがたいです。では、今度そのツアーを利用することにします。"}];
INSERT INTO sentence [{"id":{"d":956,"n":1,"t":3},"speaker":"A","text":"展望デッキは気持ちが良いですね。涼しい風が吹いています。"},{"id":{"d":956,"n":2,"t":3},"speaker":"B","text":"はい。天気が良くて、水平線まで見渡せます。"},{"id":{"d":956,"n":3,"t":3},"speaker":"A","text":"あそこを見てください。イルカが泳いでいますよ。"},{"id":{"d":956,"n":4,"t":3},"speaker":"B","text":"どこですか？私には見えません。"},{"id":{"d":956,"n":5,"t":3},"speaker":"A","text":"前方、100メートルぐらい先のところです。"},{"id":{"d":956,"n":6,"t":3},"speaker":"B","text":"見つけました。代わる代わる水面に現れますね。"},{"id":{"d":956,"n":7,"t":3},"speaker":"A","text":"初めてイルカを見ました。"},{"id":{"d":956,"n":8,"t":3},"speaker":"B","text":"私も初めてです。これは壮観ですね。"},{"id":{"d":956,"n":9,"t":3},"speaker":"A","text":"ええ。家族にもこの感動を共有したいので、スマートフォンで動画を撮っておきましょう。"}];
INSERT INTO sentence [{"id":{"d":957,"n":1,"t":3},"speaker":"A","text":"飛行機が乱気流に巻き込まれて、とても怖い思いをしました。"},{"id":{"d":957,"n":2,"t":3},"speaker":"B","text":"飛行機ではよくあることですね。"},{"id":{"d":957,"n":3,"t":3},"speaker":"A","text":"そうなのですか？それほど生易しいものではありませんでしたよ。"},{"id":{"d":957,"n":4,"t":3},"speaker":"B","text":"どのような具合だったのですか？"},{"id":{"d":957,"n":5,"t":3},"speaker":"A","text":"突然、機体が上下に揺れたかと思うと、大きく傾きました。"},{"id":{"d":957,"n":6,"t":3},"speaker":"B","text":"それは心配でしたね。乗客がパニックになったのではありませんか？"},{"id":{"d":957,"n":7,"t":3},"speaker":"A","text":"はい。私も生きた心地がしませんでした。あちらこちらから、悲鳴が上がりました。"},{"id":{"d":957,"n":8,"t":3},"speaker":"B","text":"大変でしたね。ご無事で何よりです。"},{"id":{"d":957,"n":9,"t":3},"speaker":"A","text":"二度とあのよう経験はしたくありません。"}];
INSERT INTO sentence [{"id":{"d":958,"n":1,"t":3},"speaker":"A","text":"もしもし、510号室に宿泊している佐藤です。近くにドラッグストアはありませんか？"},{"id":{"d":958,"n":2,"t":3},"speaker":"B","text":"ございますが、いかがなさいましたか？"},{"id":{"d":958,"n":3,"t":3},"speaker":"A","text":"カミソリで指を切ってしまったのです。"},{"id":{"d":958,"n":4,"t":3},"speaker":"B","text":"それは大変ですね。出血していますか？"},{"id":{"d":958,"n":5,"t":3},"speaker":"A","text":"はい。ティッシュペーパーで指の切れたところを押さえているのですが、出血が止まりません。"},{"id":{"d":958,"n":6,"t":3},"speaker":"B","text":"応急処置のためにホテルの係の者を差し向けます。"},{"id":{"d":958,"n":7,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":958,"n":8,"t":3},"speaker":"B","text":"そのまましばらくお待ちください。"}];
INSERT INTO sentence [{"id":{"d":959,"n":1,"t":3},"speaker":"A","text":"旅行の楽しみの一つは、その土地の居酒屋に寄ることです。"},{"id":{"d":959,"n":2,"t":3},"speaker":"B","text":"方々に旅行なさったのですか？"},{"id":{"d":959,"n":3,"t":3},"speaker":"A","text":"はい。職業柄、日本全国に行きました。"},{"id":{"d":959,"n":4,"t":3},"speaker":"B","text":"どのようなお仕事をしているのですか？"},{"id":{"d":959,"n":5,"t":3},"speaker":"A","text":"旅行のガイドブックを書いています。"},{"id":{"d":959,"n":6,"t":3},"speaker":"B","text":"それなら居酒屋に行くのは仕事の一部ですね。うらやましいです。"},{"id":{"d":959,"n":7,"t":3},"speaker":"A","text":"仕事の最中は飲みませんよ。終わってから仕事とは別に居酒屋に行くのです。"},{"id":{"d":959,"n":8,"t":3},"speaker":"B","text":"お気に入りの居酒屋はありますか？"},{"id":{"d":959,"n":9,"t":3},"speaker":"A","text":"はい、お酒も料理もおいしい居酒屋がどの地方にもあります。"},{"id":{"d":959,"n":10,"t":3},"speaker":"B","text":"今度、私が旅行するときに教えてください。"},{"id":{"d":959,"n":11,"t":3},"speaker":"A","text":"良いですよ。いつでもお教えします。"}];
INSERT INTO sentence [{"id":{"d":960,"n":1,"t":3},"speaker":"A","text":"ペルーのマチュピチュに行って来ました。"},{"id":{"d":960,"n":2,"t":3},"speaker":"B","text":"マチュピチュへは、私もいつか行きたいと思っています。いかがでしたか？"},{"id":{"d":960,"n":3,"t":3},"speaker":"A","text":"素晴らしかったです。あれほど感動したことはありません。"},{"id":{"d":960,"n":4,"t":3},"speaker":"B","text":"写真で見てさえ素晴らしいと思うのですから、実際に現地で目の当たりにすると感動するでしょうね。"},{"id":{"d":960,"n":5,"t":3},"speaker":"A","text":"遺跡の中を歩いていても、このような絶壁の上によくもこれほど壮大な都市を作ったものだと不思議でなりませんでした。"},{"id":{"d":960,"n":6,"t":3},"speaker":"B","text":"確かに、都市を作るにはふさわしくないところですものね。"},{"id":{"d":960,"n":7,"t":3},"speaker":"A","text":"それほど壮大な都市に、インカの人々はたった80年しか生活しなかったそうです。それも不思議です。"},{"id":{"d":960,"n":8,"t":3},"speaker":"B","text":"そうですね。それから600年近く、誰の目にも触れずに残っていたのも不思議です。"},{"id":{"d":960,"n":9,"t":3},"speaker":"A","text":"はい。マチュピチュには今もなお多くの謎が残されており、それもまた魅力の一つです。"}];
INSERT INTO sentence [{"id":{"d":961,"n":1,"t":3},"speaker":"A","text":"船酔いが激しくなってきました。"},{"id":{"d":961,"n":2,"t":3},"speaker":"B","text":"大丈夫ですか？今日は船の揺れが激しいですからね。"},{"id":{"d":961,"n":3,"t":3},"speaker":"A","text":"吐き気と頭痛が次第にひどくなっています。"},{"id":{"d":961,"n":4,"t":3},"speaker":"B","text":"うつむくとよくありませんよ。吐きやすくなります。"},{"id":{"d":961,"n":5,"t":3},"speaker":"A","text":"気分が悪いので、ついうつむいてしまいます。"},{"id":{"d":961,"n":6,"t":3},"speaker":"B","text":"なるべく遠くを見るようにした方がいいですよ。"},{"id":{"d":961,"n":7,"t":3},"speaker":"A","text":"なるほど、少し楽になるような気がします。"},{"id":{"d":961,"n":8,"t":3},"speaker":"B","text":"もし、症状がひどくなるようでしたら、薬を持ち合わせていますから声を掛けてください。"},{"id":{"d":961,"n":9,"t":3},"speaker":"A","text":"ご親切にありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":962,"n":1,"t":3},"speaker":"A","text":"タイに旅行したいと思っているのですが、可能ですか？"},{"id":{"d":962,"n":2,"t":3},"speaker":"B","text":"はい。現在は、ビジネス目的の方、観光目的の方、共に入国ができます。"},{"id":{"d":962,"n":3,"t":3},"speaker":"A","text":"入国の際の条件はありますか？"},{"id":{"d":962,"n":4,"t":3},"speaker":"B","text":"新型コロナワクチンを接種している方はCOVID-19ワクチン接種の証明書が必要です。"},{"id":{"d":962,"n":5,"t":3},"speaker":"A","text":"私はまだ新型コロナワクチンを接種していませんが、その場合はどうしたらよろしいでしょうか？"},{"id":{"d":962,"n":6,"t":3},"speaker":"B","text":"それでしたら、出発前72時間以内のRT-PCR検査、または専門家実施のATK検査の陰性証明書が必要です。"},{"id":{"d":962,"n":7,"t":3},"speaker":"A","text":"分かりました。それでは7月20日から3泊4日の予定で予約をしてください。"},{"id":{"d":962,"n":8,"t":3},"speaker":"B","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":963,"n":1,"t":3},"speaker":"A","text":"今日のスケジュールを教えてください。"},{"id":{"d":963,"n":2,"t":3},"speaker":"B","text":"午前中は、フォロ・ロマーノを中心に、コロッセオや真実の口などを回ります。"},{"id":{"d":963,"n":3,"t":3},"speaker":"A","text":"真実の口というのは、映画の『ローマの休日』に出てきたところですか？"},{"id":{"d":963,"n":4,"t":3},"speaker":"B","text":"そうです。グレゴリー・ペックが腕を入れて、オードリー・ヘップバーンを驚かせたところです。"},{"id":{"d":963,"n":5,"t":3},"speaker":"A","text":"それは楽しみですね。私も腕を入れてみましょう。"},{"id":{"d":963,"n":6,"t":3},"speaker":"B","text":"昼食は、真実の口の近くのレストランで取りましょう。"},{"id":{"d":963,"n":7,"t":3},"speaker":"A","text":"午後はどのようなところを回るのですか？"},{"id":{"d":963,"n":8,"t":3},"speaker":"B","text":"午後はパンテオンからトレビの泉、スペイン広場などを巡ります。スペイン広場も『ローマの休日』に出てきましたね。"},{"id":{"d":963,"n":9,"t":3},"speaker":"A","text":"オードリー・ヘップバーンがジェラートを食べたところですね。"},{"id":{"d":963,"n":10,"t":3},"speaker":"B","text":"はい。でも、今はあいにく、ジェラートなどを食べてはいけないことになっています。"},{"id":{"d":963,"n":11,"t":3},"speaker":"A","text":"それは残念です。スペイン広場でジェラートを食べたかったです。"}];
INSERT INTO sentence [{"id":{"d":964,"n":1,"t":3},"speaker":"A","text":"旅行でニューヨーク市に滞在していたときに、大停電に遭いました。"},{"id":{"d":964,"n":2,"t":3},"speaker":"B","text":"先月の大停電ですね。日本でも大きく報道されました。"},{"id":{"d":964,"n":3,"t":3},"speaker":"A","text":"私は、そのときちょうどホテルのエレベーターに乗っていたのです。"},{"id":{"d":964,"n":4,"t":3},"speaker":"B","text":"それは大変でしたね。ずっとエレベーターに閉じ込められていたのですか？"},{"id":{"d":964,"n":5,"t":3},"speaker":"A","text":"はい。2時間ほど暗闇のエレベーターの中に閉じ込められました。他に乗客が2人いたので、互いに励まし合いました。"},{"id":{"d":964,"n":6,"t":3},"speaker":"B","text":"停電は何時間も続いたと思いますが、どのようにして助かったのですか？"},{"id":{"d":964,"n":7,"t":3},"speaker":"A","text":"ホテルがレスキュー隊を手配してくれて、彼らに救助してもらいました。"},{"id":{"d":964,"n":8,"t":3},"speaker":"B","text":"それまでの間、さぞ心細かったでしょうね。"},{"id":{"d":964,"n":9,"t":3},"speaker":"A","text":"はい、それだけに救出されたときの喜びは大きかったです。"}];
INSERT INTO sentence [{"id":{"d":965,"n":1,"t":3},"speaker":"A","text":"四国のお遍路に行って来ました。"},{"id":{"d":965,"n":2,"t":3},"speaker":"B","text":"確か、霊場は全部で88カ所でしたね。全部回ったのですか？"},{"id":{"d":965,"n":3,"t":3},"speaker":"A","text":"いえ、一度に全部は回りきれません。今回は、そのうち徳島の23カ所だけ回りました。"},{"id":{"d":965,"n":4,"t":3},"speaker":"B","text":"歩いてお遍路をしたのですか？それともバスかタクシーを利用したのですか？"},{"id":{"d":965,"n":5,"t":3},"speaker":"A","text":"私は歩いて霊場を回りました。1週間かかりました。"},{"id":{"d":965,"n":6,"t":3},"speaker":"B","text":"1週間というと、1日に何キロメートルぐらい歩いたのですか？"},{"id":{"d":965,"n":7,"t":3},"speaker":"A","text":"平地と山間部では違いますが、平均すると1日に20キロメートルぐらい歩いたと思います。"},{"id":{"d":965,"n":8,"t":3},"speaker":"B","text":"脚が丈夫でないとお遍路ができませんね。"},{"id":{"d":965,"n":9,"t":3},"speaker":"A","text":"そうですね。土地の人が親切で、いろいろとお世話になりながらの旅でした。"}];
INSERT INTO sentence [{"id":{"d":966,"n":1,"t":3},"speaker":"A","text":"以前、ローマのレストランに入ったことがありました。"},{"id":{"d":966,"n":2,"t":3},"speaker":"B","text":"ローマですか。私も行ってみたいです。"},{"id":{"d":966,"n":3,"t":3},"speaker":"A","text":"そのレストランで面白い光景を見ました。"},{"id":{"d":966,"n":4,"t":3},"speaker":"B","text":"面白い光景というのは何ですか？"},{"id":{"d":966,"n":5,"t":3},"speaker":"A","text":"隣のテーブルに男女のカップルがいたのですが、そのカップルがずっとスマートフォンで話していたのです。"},{"id":{"d":966,"n":6,"t":3},"speaker":"B","text":"まさか、二人がスマートフォンを使って話をしていたのですか？"},{"id":{"d":966,"n":7,"t":3},"speaker":"A","text":"いいえ。そうではなくて、お互いは一言も会話を交わさずに二人がそれぞれ別の人と話していたのです。"},{"id":{"d":966,"n":8,"t":3},"speaker":"B","text":"いかにも現代風な光景ですね。"}];
INSERT INTO sentence [{"id":{"d":967,"n":1,"t":3},"speaker":"A","text":"ベニスに行ったのですが、ちょうど高潮の時期にぶつかってしまいました。"},{"id":{"d":967,"n":2,"t":3},"speaker":"B","text":"街が浸水していたのですね。毎年、テレビの報道番組で、そのニュースを見ます。"},{"id":{"d":967,"n":3,"t":3},"speaker":"A","text":"冬の時期はいつもそうなってしまうようです。"},{"id":{"d":967,"n":4,"t":3},"speaker":"B","text":"どのくらい浸水しましたか？"},{"id":{"d":967,"n":5,"t":3},"speaker":"A","text":"今年は特にひどかったようで、私の膝の上まで水が来ました。"},{"id":{"d":967,"n":6,"t":3},"speaker":"B","text":"観光どころではありませんね。冬にベニスに行くのは避けた方がいいかもしれません。"},{"id":{"d":967,"n":7,"t":3},"speaker":"A","text":"冬は夏に比べて旅費が安いのでつい行く気になったのですが、それが間違いでした。"},{"id":{"d":967,"n":8,"t":3},"speaker":"B","text":"浸水もまたベニスの観光名物だと思うしかありませんね。"},{"id":{"d":967,"n":9,"t":3},"speaker":"A","text":"確かに、忘れられない思い出になるでしょう。"}];
INSERT INTO sentence [{"id":{"d":968,"n":1,"t":3},"speaker":"A","text":"新車を買ったので、ドライブに行きましょう。"},{"id":{"d":968,"n":2,"t":3},"speaker":"B","text":"良いですね。今日は天気も良いし風も爽やかで、ドライブ日和です。"},{"id":{"d":968,"n":3,"t":3},"speaker":"A","text":"どこか行きたいところはありますか？"},{"id":{"d":968,"n":4,"t":3},"speaker":"B","text":"それはあなたにお任せしますよ。"},{"id":{"d":968,"n":5,"t":3},"speaker":"A","text":"それでは、伊豆のスカイラインを走って、伊豆高原までというのはどうですか？"},{"id":{"d":968,"n":6,"t":3},"speaker":"B","text":"良いですね。ちょうど新緑の季節だから、とても気持ちが良いですよ。"},{"id":{"d":968,"n":7,"t":3},"speaker":"A","text":"それでは、車を移動させてきます。少し待ってください。"},{"id":{"d":968,"n":8,"t":3},"speaker":"B","text":"ほろが付いているスポーツカーですね。"},{"id":{"d":968,"n":9,"t":3},"speaker":"A","text":"ほろを上げて走りましょう。"},{"id":{"d":968,"n":10,"t":3},"speaker":"B","text":"良いですね。風を受けて走るのはきっと気持ちが良いことでしょう。"}];
INSERT INTO sentence [{"id":{"d":969,"n":1,"t":3},"speaker":"A","text":"最近、グランピングという言葉をよく聞きますね。"},{"id":{"d":969,"n":2,"t":3},"speaker":"B","text":"アウトドアの用語ですね。新しい体験型旅行の一種です。"},{"id":{"d":969,"n":3,"t":3},"speaker":"A","text":"グランピングとは、どのような意味ですか？"},{"id":{"d":969,"n":4,"t":3},"speaker":"B","text":"魅力的なという意味のグラマラスと、キャンピングを合わせた造語です。山や野に設置された豪華なテントやロッジなどに泊まり、ホテルのようなサービスを受けながら、自然との触れ合いを楽しむという意味があります。"},{"id":{"d":969,"n":5,"t":3},"speaker":"A","text":"これまでのキャンプとは違うのですか？"},{"id":{"d":969,"n":6,"t":3},"speaker":"B","text":"はい。テントやドームがあらかじめ準備されていて、旅行者はテントやドームの設営に煩わされることがありません。"},{"id":{"d":969,"n":7,"t":3},"speaker":"A","text":"なるほど、気軽にキャンプを楽しむことができるわけですね。"},{"id":{"d":969,"n":8,"t":3},"speaker":"B","text":"はい。もともとヨーロッパから始まったのですが、最近は日本でも盛んになりました。"},{"id":{"d":969,"n":9,"t":3},"speaker":"A","text":"一度体験してみたいです。"}];
INSERT INTO sentence [{"id":{"d":970,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。"},{"id":{"d":970,"n":2,"t":3},"speaker":"B","text":"予約をしている阪本というものです。"},{"id":{"d":970,"n":3,"t":3},"speaker":"A","text":"阪本さまでございますね。少々お待ちください。本日から2泊のご予定ですね。"},{"id":{"d":970,"n":4,"t":3},"speaker":"B","text":"はい、そうです。チェックインが遅れてすみません。電車が遅れたものですから。"},{"id":{"d":970,"n":5,"t":3},"speaker":"A","text":"どうぞご心配なく。それでは、宿泊カードにお名前とご住所、電話番号をご記入ください。"},{"id":{"d":970,"n":6,"t":3},"speaker":"B","text":"これでよろしいですか？"},{"id":{"d":970,"n":7,"t":3},"speaker":"A","text":"はい、結構でございます。302号室でございます。これがルームキーでございます。ごゆっくりお過ごしください。"},{"id":{"d":970,"n":8,"t":3},"speaker":"B","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":971,"n":1,"t":3},"speaker":"A","text":"ピラミッドを見てきたそうですね。"},{"id":{"d":971,"n":2,"t":3},"speaker":"B","text":"はい、先週エジプトに行ってピラミッドを見ました。"},{"id":{"d":971,"n":3,"t":3},"speaker":"A","text":"どうでしたか？良かったですか？"},{"id":{"d":971,"n":4,"t":3},"speaker":"B","text":"はい。何千年も前によくあのようものを作ったものだと、感動しました。"},{"id":{"d":971,"n":5,"t":3},"speaker":"A","text":"誰しもそう思いますよね。しかも、なぜ作ったのかも、どのようにして作ったのかも分からないので、一層神秘的です。"},{"id":{"d":971,"n":6,"t":3},"speaker":"B","text":"そうですね。ピラミッドを下から見上げると、その高さに圧倒されました。上から見るとどのような感じだろうと想像をかき立られました。"},{"id":{"d":971,"n":7,"t":3},"speaker":"A","text":"ピラミッドに登ることはできないのですか？"},{"id":{"d":971,"n":8,"t":3},"speaker":"B","text":"はい。登るのは禁止されていますから。先日も、ピラミッドを登った若者が警察に捕まったそうです。"},{"id":{"d":971,"n":9,"t":3},"speaker":"A","text":"それは困りますね。ピラミッドのような人類の宝はみんなで守らなくてはいけません。"}];
INSERT INTO sentence [{"id":{"d":972,"n":1,"t":3},"speaker":"A","text":"新幹線に乗るのは初めてです。"},{"id":{"d":972,"n":2,"t":3},"speaker":"B","text":"そうですか。あなたの国にはこのような鉄道はありませんか？"},{"id":{"d":972,"n":3,"t":3},"speaker":"A","text":"はい、私の出身国には、これほど速い鉄道はありません。"},{"id":{"d":972,"n":4,"t":3},"speaker":"B","text":"鉄道を建設する計画はありませんか？"},{"id":{"d":972,"n":5,"t":3},"speaker":"A","text":"今、計画を練っているところです。日本の援助で建設することになっています。"},{"id":{"d":972,"n":6,"t":3},"speaker":"B","text":"早く完成するといいですね。"},{"id":{"d":972,"n":7,"t":3},"speaker":"A","text":"はい、完成が楽しみです。あ、あの山が富士山ですか？"},{"id":{"d":972,"n":8,"t":3},"speaker":"B","text":"そうです。今日は天気が良くて頂上まで、くっきりと見えます。"},{"id":{"d":972,"n":9,"t":3},"speaker":"A","text":"崇高な山ですね。感動しました。"}];
INSERT INTO sentence [{"id":{"d":973,"n":1,"t":3},"speaker":"A","text":"新婚旅行はどこにしましょうか？"},{"id":{"d":973,"n":2,"t":3},"speaker":"B","text":"あなたはどこに行きたいですか？"},{"id":{"d":973,"n":3,"t":3},"speaker":"A","text":"私はパリに行きたいです。あなたはどこへ行きたいですか？"},{"id":{"d":973,"n":4,"t":3},"speaker":"B","text":"私はギリシャに行きたいと思っていました。"},{"id":{"d":973,"n":5,"t":3},"speaker":"A","text":"1週間の予定ですから、両方というわけにはいきませんね。"},{"id":{"d":973,"n":6,"t":3},"speaker":"B","text":"そうですね。今回はあなたの希望を優先して、パリに行くことにしましょう。"},{"id":{"d":973,"n":7,"t":3},"speaker":"A","text":"それではあなたに申し訳ありません。ギリシャにしましょうか？"},{"id":{"d":973,"n":8,"t":3},"speaker":"B","text":"いいえ、ギリシャはまた別の機会に行くことができますから、今回はパリにしましょう。"},{"id":{"d":973,"n":9,"t":3},"speaker":"A","text":"あなたは優しいですね。それではパリに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":974,"n":1,"t":3},"speaker":"A","text":"コンテナ船で旅行できることを知っていますか？"},{"id":{"d":974,"n":2,"t":3},"speaker":"B","text":"知りませんでした。そのようなことができるのですか？"},{"id":{"d":974,"n":3,"t":3},"speaker":"A","text":"はい。コンテナ船を利用した旅行を手配する旅行代理店もあるのですよ。"},{"id":{"d":974,"n":4,"t":3},"speaker":"B","text":"想像がつきませんが、きちんとした客室があるのですか？"},{"id":{"d":974,"n":5,"t":3},"speaker":"A","text":"はい。豪華客船のような設備はありませんが、寝室とリビングルームとバスルームが付いています。"},{"id":{"d":974,"n":6,"t":3},"speaker":"B","text":"それなら十分ですね。食事はどうなっているのですか？"},{"id":{"d":974,"n":7,"t":3},"speaker":"A","text":"もちろん、食事も出ます。乗組員と同じ場所で取ります。"},{"id":{"d":974,"n":8,"t":3},"speaker":"B","text":"すると、乗組員との交流もできるわけですね。"},{"id":{"d":974,"n":9,"t":3},"speaker":"A","text":"そうです。普通の客船とは一味違った旅行を楽しむことができます。"},{"id":{"d":974,"n":10,"t":3},"speaker":"B","text":"機会があれば、私もコンテナ船を使った旅行をしてみたいです。"}];
INSERT INTO sentence [{"id":{"d":975,"n":1,"t":3},"speaker":"A","text":"仙台に来たら牛タンを食べるのを楽しみにしていたのに、目当ての牛タン屋が臨時休業とは残念です。"},{"id":{"d":975,"n":2,"t":3},"speaker":"B","text":"私もひいきの牛タン屋に案内したいと思っていたのに、残念です。"},{"id":{"d":975,"n":3,"t":3},"speaker":"A","text":"今度仙台に来られるのは、何年先になるのか分かりません。"},{"id":{"d":975,"n":4,"t":3},"speaker":"B","text":"そのようなことを言わずに、来年また来てください。"},{"id":{"d":975,"n":5,"t":3},"speaker":"A","text":"その時までに、新型コロナウイルス感染症の流行が終息していれば良いのですが。"},{"id":{"d":975,"n":6,"t":3},"speaker":"B","text":"第6波がピークを過ぎたときは、新型コロナウイルス感染症の流行もこれで終わりかと思ったのですがね。"},{"id":{"d":975,"n":7,"t":3},"speaker":"A","text":"そうですね。実際は、次々と新種のウイルスが現われて、なかなか流行が終わりません。"},{"id":{"d":975,"n":8,"t":3},"speaker":"B","text":"来年には収束することを願います。"}];
INSERT INTO sentence [{"id":{"d":976,"n":1,"t":3},"speaker":"A","text":"京都に行ってきました。こちらは少しばかりですがお土産です。どうぞ、お召し上がりください。"},{"id":{"d":976,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。京都にはお仕事で行かれたのですか？"},{"id":{"d":976,"n":3,"t":3},"speaker":"A","text":"いいえ。京都に住んでいる学生時代の友人に会いに行ってきました。"},{"id":{"d":976,"n":4,"t":3},"speaker":"B","text":"京都のどちらへ行かれたのですか？"},{"id":{"d":976,"n":5,"t":3},"speaker":"A","text":"京都市内です。初日は友人と京都市街を散策しました。翌日はホテルを早めにチェックアウトして嵯峨野に行ってきました。早い時間だったので観光客はまばらで、有名な竹林の小径も人とすれ違うことなく散策することができました。"},{"id":{"d":976,"n":6,"t":3},"speaker":"B","text":"それは良かったですね。私も京都に行きたいと思っていますが、混雑しているとよく聞くのでなかなか気乗りがしません。"},{"id":{"d":976,"n":7,"t":3},"speaker":"A","text":"そうですね。人混みの中ではゆっくりと観光ができませんよね。午前中の早めの時間帯ならば、比較的すいているのでお勧めです。"},{"id":{"d":976,"n":8,"t":3},"speaker":"B","text":"そうですか。それでは私も京都旅行を計画したいと思います。お薦めの観光地や食べ物屋さんがありましたら教えてください。"}];
INSERT INTO sentence [{"id":{"d":977,"n":1,"t":3},"speaker":"A","text":"木村さんは神奈川県の中学校に通われていましたよね。修学旅行はどちらへ行きましたか？"},{"id":{"d":977,"n":2,"t":3},"speaker":"B","text":"ご多分に漏れず、修学旅行先は京都と奈良です。歴史的建造物よりも、修学旅行生だらけだったことを覚えています。落合さんも神奈川県のご出身ですから、修学旅行先は京都と奈良ですよね？"},{"id":{"d":977,"n":3,"t":3},"speaker":"A","text":"いいえ、私の中学時代の修学旅行の行き先は長野と岐阜でした。"},{"id":{"d":977,"n":4,"t":3},"speaker":"B","text":"珍しいですね。どちらへ観光に行ったのですか？"},{"id":{"d":977,"n":5,"t":3},"speaker":"A","text":"黒部ダム、馬籠宿、松本城などに行きました。黒部ダムでは、ダムの壮大さと素晴らしい景色に圧倒されたことを記憶しています。"},{"id":{"d":977,"n":6,"t":3},"speaker":"B","text":"馬籠宿に宿泊したのですか？"},{"id":{"d":977,"n":7,"t":3},"speaker":"A","text":"はい、そうです。クラスごとに宿を借り切って宿泊しました。いろり端でお宿のご主人から馬籠の暮らしについてのお話を伺いました。"},{"id":{"d":977,"n":8,"t":3},"speaker":"B","text":"それは思い出に残る修学旅行でしたね。私は往復の新幹線の中のことは覚えていますが、旅行中のことはあまり記憶にありません。家に帰ったら卒業アルバムを開いて修学旅行のことを思い出してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":978,"n":1,"t":3},"speaker":"A","text":"明子ちゃん、あさっては何か用事がありますか。特に予定がないのであれば、一緒に東京に行きませんか？"},{"id":{"d":978,"n":2,"t":3},"speaker":"B","text":"良いですね、行きましょう。お母さんと一緒に出掛けるのは久しぶりです。何かお目当てがあるのですか？"},{"id":{"d":978,"n":3,"t":3},"speaker":"A","text":"絵本作家の島田ゆかさんの作品展があるので行きたいと思っています。明子ちゃんも彼女の作品が好きだったでしょう。"},{"id":{"d":978,"n":4,"t":3},"speaker":"B","text":"ええ、大好きです。ぜひ、見に行きたいと思います。会場はどちらですか？"},{"id":{"d":978,"n":5,"t":3},"speaker":"A","text":"会場は銀座のデパートです。作品を見た後は、銀座でウインドーショッピングを楽しみましょう。それから、洋菓子屋さんにも行きましょう。"},{"id":{"d":978,"n":6,"t":3},"speaker":"B","text":"お母さんのお気に入りの、あの洋菓子屋さんですね。ぜひ、行きましょう。喫茶室で何を食べるか今から胸が躍ります。銀座に行くのなら、書店の教文館にも行きたいと思います。良いでしょうか？"},{"id":{"d":978,"n":7,"t":3},"speaker":"A","text":"もちろんですよ。久しぶりの銀座ですからいろいろ回りましょう。少し早起きをして、早めに家を出ることにしましょうか。"},{"id":{"d":978,"n":8,"t":3},"speaker":"B","text":"そうしましょう。車で行くよりも電車の方がいいですよね。私が電車の時間を調べておきますね。"}];
INSERT INTO sentence [{"id":{"d":979,"n":1,"t":3},"speaker":"A","text":"明日は朝から中央図書館に行って勉強をしたいと思っています。車で送ってもらうことはできますか？"},{"id":{"d":979,"n":2,"t":3},"speaker":"B","text":"はい。明日は予定がないので構いませんよ。迎えも必要ですよね。何時まで図書館に居る予定ですか？"},{"id":{"d":979,"n":3,"t":3},"speaker":"A","text":"閉館時間の夕方5時まで居たいと思っています。"},{"id":{"d":979,"n":4,"t":3},"speaker":"B","text":"5時に迎えに行くのは難しいと思います。5時半なら大丈夫なのですが。どこかで時間をつぶすことはできますか？"},{"id":{"d":979,"n":5,"t":3},"speaker":"A","text":"はい、大丈夫です。図書館の隣にスターカフェがあるので、そこで待つのでお迎えが5時半より遅くても構いません。"},{"id":{"d":979,"n":6,"t":3},"speaker":"B","text":"分かりました。スターカフェの駐車場に着いたらメールで連絡しますね。朝は何時に出ましょうか？"},{"id":{"d":979,"n":7,"t":3},"speaker":"A","text":"図書館の開館が9時なので、9時以降に着くのであれば何時でも差し支えありません。お母さんの都合に合わせます。"},{"id":{"d":979,"n":8,"t":3},"speaker":"B","text":"では、8時40分の出発を予定しておきましょう。昼食はどうしますか？"},{"id":{"d":979,"n":9,"t":3},"speaker":"A","text":"自習室は飲み物を持ち込むことはできますが、食べ物の持ち込みは禁止されているので、近くの定食屋さんにでも行きます。"}];
INSERT INTO sentence [{"id":{"d":980,"n":1,"t":3},"speaker":"A","text":"木内さん、僕は半年後に米国に留学する予定なのですが、留学先の学校の担当者から「指導教授の推薦状を提出するように」と言われました。教授に取り次いでいただけませんか？"},{"id":{"d":980,"n":2,"t":3},"speaker":"B","text":"はい。承知しました。松下さんが留学することを浅野教授はご存知でしょうか？"},{"id":{"d":980,"n":3,"t":3},"speaker":"A","text":"はい、もちろんです。留学先は浅野先生とご相談して決めました。"},{"id":{"d":980,"n":4,"t":3},"speaker":"B","text":"それでしたら、話は早いですね。推薦状に記載しなくてはならない事項をお知らせください。"},{"id":{"d":980,"n":5,"t":3},"speaker":"A","text":"口頭でお伝えするより、メールの方が確実ですよね。木内さんにメールでお送りすれば良いですか？"},{"id":{"d":980,"n":6,"t":3},"speaker":"B","text":"はい、そうしてください。推薦状の件は私から浅野教授にお伝えしますが、松下さんからもお願しておいてくださいね。"},{"id":{"d":980,"n":7,"t":3},"speaker":"A","text":"はい、分かりました。お手数をおかけしますがよろしくお願いします。"},{"id":{"d":980,"n":8,"t":3},"speaker":"B","text":"お任せください。推薦状が出来上がり次第、ご連絡しますね。"}];
INSERT INTO sentence [{"id":{"d":981,"n":1,"t":3},"speaker":"A","text":"すみません。奈良までの特急券を買いたいのですが。"},{"id":{"d":981,"n":2,"t":3},"speaker":"B","text":"お1人分ですか？"},{"id":{"d":981,"n":3,"t":3},"speaker":"A","text":"はい、大人1人です。"},{"id":{"d":981,"n":4,"t":3},"speaker":"B","text":"15分後に出発する列車があります。窓側、通路側、どちらも空いています。どちらになさいますか？"},{"id":{"d":981,"n":5,"t":3},"speaker":"A","text":"では、窓側をお願いします。支払いにクレジットカードは使えますか？"},{"id":{"d":981,"n":6,"t":3},"speaker":"B","text":"はい。お使いいただけます。乗車券も併せてお求めになりますか？"},{"id":{"d":981,"n":7,"t":3},"speaker":"A","text":"はい、乗車券と特急券を一緒に購入します。"},{"id":{"d":981,"n":8,"t":3},"speaker":"B","text":"乗車券は640円、特急券は520円ですので、合計で1,160円を頂きます。では、カードをそちらの挿し込み口に入れてください。メッセージが出ましたら、暗証番号を入力し、緑色のボタンを押してください。"},{"id":{"d":981,"n":9,"t":3},"speaker":"A","text":"はい、押しました。"},{"id":{"d":981,"n":10,"t":3},"speaker":"B","text":"カードをお取りください。こちらが乗車券と特急券、それからクレジットカードの利用控えです。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":982,"n":1,"t":3},"speaker":"A","text":"6月中旬に九州に行く予定を立てていたのですが、仕事が入ってしまいました。九州旅行は諦めました。"},{"id":{"d":982,"n":2,"t":3},"speaker":"B","text":"航空機やホテルは、まだ手配していなかったのですか？"},{"id":{"d":982,"n":3,"t":3},"speaker":"A","text":"いいえ。航空機もホテルも既に予約済みでした。ホテルはキャンセルできたのですが、航空機はキャンセルができませんでした。"},{"id":{"d":982,"n":4,"t":3},"speaker":"B","text":"航空機のキャンセルができないことがあるのですか？"},{"id":{"d":982,"n":5,"t":3},"speaker":"A","text":"はい。格安航空券の中にも、キャンセルができるものと、できないものがあるようです。価格の安さで選んだ私の航空券は、キャンセルができないタイプでした。"},{"id":{"d":982,"n":6,"t":3},"speaker":"B","text":"仕事が落ち着いたら、休暇を取って旅行ができると良いですね。"},{"id":{"d":982,"n":7,"t":3},"speaker":"A","text":"はい。秋にはまとまった休暇が取得できそうなので、もう一度、九州旅行の計画を立てたいと思います。"},{"id":{"d":982,"n":8,"t":3},"speaker":"B","text":"秋の九州は、関サバと関アジが最高です。ぜひとも海鮮グルメの旅を計画してください。"}];
INSERT INTO sentence [{"id":{"d":983,"n":1,"t":3},"speaker":"A","text":"今年は長い夏休みが取得できそうです。家族で北海道に行きませんか？"},{"id":{"d":983,"n":2,"t":3},"speaker":"B","text":"良いですね。時間がたくさんあるのなら、いつもと違う旅にしましょう。カーフェリーで車を運んで行けばわが家のワンボックスカーで北海道を回れますね。"},{"id":{"d":983,"n":3,"t":3},"speaker":"A","text":"それは良いですね。賛成です。茨城県の大洗港から苫小牧港までのフェリーが存在したように思います。"},{"id":{"d":983,"n":4,"t":3},"speaker":"B","text":"苫小牧港からは道東に行きましょう。知床の自然に触れたいと思います。"},{"id":{"d":983,"n":5,"t":3},"speaker":"A","text":"高速道路を使ったとしても苫小牧港から一気に道東に行くのは難しいと思います。釧路辺りで1泊しましょう。釧路に温泉があれば、そこに泊まるのはどうでしょう？"},{"id":{"d":983,"n":6,"t":3},"speaker":"B","text":"インターネットで調べたところ、釧路には温泉宿がいくつかありそうです。釧路で1泊して、それから知床に行きましょう。知床ではお湯が流れてくるという川に行きたいと思います。"},{"id":{"d":983,"n":7,"t":3},"speaker":"A","text":"カムイワッカ湯の滝ですね。僕も行ってみたいと思っていましたが、渓流を登って行かなくてはならないと聞いたことがあります。子どもたちには難しいのではないでしょうか？"},{"id":{"d":983,"n":8,"t":3},"speaker":"B","text":"ガイドブックや観光案内所のホームページを調べてみます。危険そうであればやめますね。この旅行で、子どもたちに日頃体験できないさまざまなことを経験させてやりたいと思います。"}];
INSERT INTO sentence [{"id":{"d":984,"n":1,"t":3},"speaker":"A","text":"白井さんは、クリはお好きですか？"},{"id":{"d":984,"n":2,"t":3},"speaker":"B","text":"はい。秋の味覚は何でも好きですが、特にクリは大好きです。"},{"id":{"d":984,"n":3,"t":3},"speaker":"A","text":"では、今週末に家族で北関東にドライブに行く予定なのですが、茨城県にとてもおいしいクリを売っているお店があるので、買ってきてごちそうしますね。"},{"id":{"d":984,"n":4,"t":3},"speaker":"B","text":"うれしいです。クリといえば、丹波くりの兵庫県などが名産地だと思っていました。茨城県にもおいしいクリがあるのですね。"},{"id":{"d":984,"n":5,"t":3},"speaker":"A","text":"そうなのです。驚くほど甘いクリです。甘い品種なのだとは思いますが、収穫後に熟成させる技術にも秘密があるそうです。以前、観光農園にぶどう狩りに行った時に、そのクリ屋さんのことを教えてもらいました。"},{"id":{"d":984,"n":6,"t":3},"speaker":"B","text":"それは楽しみです。茨城といえばサツマイモも有名ですよね。最近の焼き芋ブームで茨城県が脚光を浴びているように思います。"},{"id":{"d":984,"n":7,"t":3},"speaker":"A","text":"サツマイモも白井さんの好きな秋の味覚ですね。焼き芋のおいしいお店をご存じでしたら教えてください。"},{"id":{"d":984,"n":8,"t":3},"speaker":"B","text":"かすみがうら市にある焼き芋販売店が有名です。国道沿いの店舗で、広い駐車場が存在したように思います。後でお店の名前を調べてお知らせしますね。"}];
INSERT INTO sentence [{"id":{"d":985,"n":1,"t":3},"speaker":"A","text":"木村さんは、新婚旅行はどちらに行かれたのですか？"},{"id":{"d":985,"n":2,"t":3},"speaker":"B","text":"オーストラリアに行きました。その当時、シドニーオリンピックの開催が決まったタイミングでしたので、オーストラリアは人気の新婚旅行先でした。"},{"id":{"d":985,"n":3,"t":3},"speaker":"A","text":"確かに、私の友人達も新婚旅行にオーストラリアを選んだ人が多かったように思います。オーストラリアのどちらに行かれたのですか？"},{"id":{"d":985,"n":4,"t":3},"speaker":"B","text":"シドニー、グレートバリアリーフ、エアーズロックです。"},{"id":{"d":985,"n":5,"t":3},"speaker":"A","text":"どこも良いところですね。当時はエアーズロックに登ることができたのですか？今は登山が禁止されていると聞きました。"},{"id":{"d":985,"n":6,"t":3},"speaker":"B","text":"はい、当時は登山用の鎖も設置されていて、簡単な装備があればガイドさんの案内の下で観光客も登ることができました。原住民の方の聖地であることを感じさせる特別な場所でした。良い経験をさせてもらったと思っています。"},{"id":{"d":985,"n":7,"t":3},"speaker":"A","text":"その旅行中に、他に思い出に残っていることはありますか？"},{"id":{"d":985,"n":8,"t":3},"speaker":"B","text":"シドニーのレストランで現地在住の日本人のご夫妻と意気投合し、シドニーでの生活についていろいろと教えていただきました。それ以来、リタイアしたらオーストラリアに移住するという夢を夫婦で持ち続けています。"}];
INSERT INTO sentence [{"id":{"d":986,"n":1,"t":3},"speaker":"A","text":"仕事柄日本全国津々浦々に行くのですが、いまだに飛行機と新幹線に乗ると胸が躍ります。"},{"id":{"d":986,"n":2,"t":3},"speaker":"B","text":"私は滅多に関東地方から出ることがないので、飛行機や新幹線に乗るのは私にとって特別なことです。毎度、かなりテンションが上がります。谷沢さんのように頻繁に出張する方でもそうなのですね？"},{"id":{"d":986,"n":3,"t":3},"speaker":"A","text":"はい。飛行機でも新幹線でも可能な限り窓際の席を取り、外の景色を楽しみます。機内サービス、車内販売も堪能します。"},{"id":{"d":986,"n":4,"t":3},"speaker":"B","text":"分かります。私も新幹線に乗ると必ず車内販売のアイスクリームを買って食べます。日常の買い物でしたらちゅうちょする値段なのですが、新幹線の中では迷わず買ってしまいます。"},{"id":{"d":986,"n":5,"t":3},"speaker":"A","text":"機内誌や車内誌も必ず手に入れて、隅から隅まで目を通します。エッセイや地域の情報など、相当に読み応えがあります。"},{"id":{"d":986,"n":6,"t":3},"speaker":"B","text":"機内誌や車内誌は面白いですよね。私はショッピングのページが特に好きです。その場で衝動買いして、旅の目的地に着いてもいないのに荷物が増えてしまうことが多々あります。"},{"id":{"d":986,"n":7,"t":3},"speaker":"A","text":"他の乗り物ではこのような特別な感情は湧きませんが、飛行機と新幹線は特別ですね。"},{"id":{"d":986,"n":8,"t":3},"speaker":"B","text":"はい。単なる移動のための乗り物ではありません。私にとっては、いつまでも憧れの空間です。"}];
INSERT INTO sentence [{"id":{"d":987,"n":1,"t":3},"speaker":"A","text":"中沢さん、この連休はどちらかに行かれたのですか？"},{"id":{"d":987,"n":2,"t":3},"speaker":"B","text":"長野に行ってきました。当初はバイクの後ろに妻を乗せてツーリングの予定だったのですが、天気が怪しかったので車で行ってきました。"},{"id":{"d":987,"n":3,"t":3},"speaker":"A","text":"長野のどちらに行かれたのですか？"},{"id":{"d":987,"n":4,"t":3},"speaker":"B","text":"上高地と松本です。上高地は非常に涼しく、日頃の猛暑がうそのような気温でした。梓川の流れの美しさに心が洗われる思いでした。"},{"id":{"d":987,"n":5,"t":3},"speaker":"A","text":"お天気は大丈夫だったということですか？"},{"id":{"d":987,"n":6,"t":3},"speaker":"B","text":"はい。心配していた雨に降られることもなく、旅を楽しむことができました。道すがらに立ち寄った店での食事もおいしく、良い旅でした。"},{"id":{"d":987,"n":7,"t":3},"speaker":"A","text":"それは、良かったですね。奥さまも喜ばれたことでしょう。"},{"id":{"d":987,"n":8,"t":3},"speaker":"B","text":"はい、とても喜んでくれました。日頃の感謝の気持ちを伝えられたと思います。"}];
INSERT INTO sentence [{"id":{"d":988,"n":1,"t":3},"speaker":"A","text":"RV車は若者の乗る車のイメージがありましたが、今はそうではありませんね。"},{"id":{"d":988,"n":2,"t":3},"speaker":"B","text":"はい。運転している人を見ると、私よりも上の世代の方であることもしばしばあります。"},{"id":{"d":988,"n":3,"t":3},"speaker":"A","text":"運転席の位置が高くて遠くまで見えるので、運転しやすいのかもしれませんね。"},{"id":{"d":988,"n":4,"t":3},"speaker":"B","text":"私は専らセダン派なのですが、大雨の日や雪の積もりそうな日はRV車も欲しくなります。"},{"id":{"d":988,"n":5,"t":3},"speaker":"A","text":"永井さんは、どうしてセダンが好きなのですか？"},{"id":{"d":988,"n":6,"t":3},"speaker":"B","text":"運転席が低いことでコーナリングの安定感がありますし、大人の落ち着いた雰囲気があるので、セダンが好きです。大山さんは車にこだわりはありますか？"},{"id":{"d":988,"n":7,"t":3},"speaker":"A","text":"いいえ、こだわりはありません。独身時代は値段で選んで軽自動車、結婚して最初の車はステーションワゴン、家族が増えたらワンボックス、今はセダンです。思い返すとライフスタイルに合わせて、車のタイプを変えていますね。"},{"id":{"d":988,"n":8,"t":3},"speaker":"B","text":"いろいろな車に乗ってこられたのですね。私も今の車に長く乗っているので、そろそろ次の車の購入を検討したいと思っています。"}];
INSERT INTO sentence [{"id":{"d":989,"n":1,"t":3},"speaker":"A","text":"秋になったら、旅行会社のパックツアーで果物狩りに行きませんか？"},{"id":{"d":989,"n":2,"t":3},"speaker":"B","text":"秋は旅行シーズンですから、さまざまなツアーが企画されそうですね。私はパックツアーを利用したことはありませんが、相当お得だと聞くので興味があります。"},{"id":{"d":989,"n":3,"t":3},"speaker":"A","text":"はい、パックツアーはとてもお得です。バス代、昼食代、果物狩り代、それからお土産まで付いて、1万円以下が主流です。"},{"id":{"d":989,"n":4,"t":3},"speaker":"B","text":"それはとても安いですね。秋の果物狩りというとぶどうでしょうか？ぶどうは好物なので、ぜひ行きたいです。"},{"id":{"d":989,"n":5,"t":3},"speaker":"A","text":"出発は早朝だと思いますが、大丈夫ですか？"},{"id":{"d":989,"n":6,"t":3},"speaker":"B","text":"私は早起きが得意ですから問題ありません。行くと決まったら、早めに予約を入れた方が良さそうでしょうか？"},{"id":{"d":989,"n":7,"t":3},"speaker":"A","text":"そうですね。駅前に旅行会社が2店舗は存在したと思いますから、チラシを集めて具体的なツアーを検討しましょう。"},{"id":{"d":989,"n":8,"t":3},"speaker":"B","text":"思い立ったが吉日ですから、この足で駅前の旅行会社に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":990,"n":1,"t":3},"speaker":"A","text":"部長、来月の出張の飛行機とホテルの手配をしてもよろしいでしょうか？"},{"id":{"d":990,"n":2,"t":3},"speaker":"B","text":"お願いします。相手先との約束の時間が午前なので、朝一番の便を予約してください。"},{"id":{"d":990,"n":3,"t":3},"speaker":"A","text":"承知いたしました。成田国際空港発の方が、羽田空港発よりも早い時間の便がありますが、いかがいたしましょう？"},{"id":{"d":990,"n":4,"t":3},"speaker":"B","text":"成田国際空港発の便にしてください。成田国際空港の近辺のホテルで前泊することにします。"},{"id":{"d":990,"n":5,"t":3},"speaker":"A","text":"では、宿泊は7月20日の成田、21日の宮崎でよろしいですか？"},{"id":{"d":990,"n":6,"t":3},"speaker":"B","text":"はい、それでお願いします。20日は素泊まりで良いですが、21日は朝食付きにしてください。"},{"id":{"d":990,"n":7,"t":3},"speaker":"A","text":"承知いたしました。成田は空港至近のホテルが取れました。宮崎は22日の訪問先の近くの宿にいたします。帰りは15時過ぎに宮崎空港を出発して、羽田空港に戻る便でよろしいでしょうか？"},{"id":{"d":990,"n":8,"t":3},"speaker":"B","text":"はい、結構です。手配をお願いします。"}];
INSERT INTO sentence [{"id":{"d":991,"n":1,"t":3},"speaker":"A","text":"路線バスに乗るのは久しぶりです。緊張しつつも、胸が躍ります。"},{"id":{"d":991,"n":2,"t":3},"speaker":"B","text":"最後に路線バスに乗ったのはいつ頃ですか？"},{"id":{"d":991,"n":3,"t":3},"speaker":"A","text":"15年ほど前です。普段はどこへ行くにも自家用車なので、路線バスを使うことはありません。"},{"id":{"d":991,"n":4,"t":3},"speaker":"B","text":"15年前からですと料金の払い方が変わっているかもしれませんね。今はICカードでも料金を支払うことができるようになりました。"},{"id":{"d":991,"n":5,"t":3},"speaker":"A","text":"乗り口で番号の記載されている整理券を取る必要はありませんか？"},{"id":{"d":991,"n":6,"t":3},"speaker":"B","text":"現金で支払う場合は整理券を取る必要がありますが、ICカードを使う場合は整理券を取る必要がありません。"},{"id":{"d":991,"n":7,"t":3},"speaker":"A","text":"ICカードで支払えるのは便利ですね。昔は、お財布に小銭がないと、揺れるバスの中、両替機まで行って両替をしなくてはなりませんでした。運賃箱にバス代を入れる時も後ろの人を待たせてはいけないと思って、気を遣ったものです。"},{"id":{"d":991,"n":8,"t":3},"speaker":"B","text":"懐かしいです。今はほとんどの乗客がICカードを使いますので、バスの乗り降りはとてもスムーズですよ。"},{"id":{"d":991,"n":9,"t":3},"speaker":"A","text":"目的のバス停名が放送されたら降車ボタンを押すということは、変わりありませんか？"},{"id":{"d":991,"n":10,"t":3},"speaker":"B","text":"はい、それは今も同じです。"}];
INSERT INTO sentence [{"id":{"d":992,"n":1,"t":3},"speaker":"A","text":"あさってはお天気が良いようです。ピクニックに行きましょう。"},{"id":{"d":992,"n":2,"t":3},"speaker":"B","text":"良いですね。どこに行きましょうか？景色の良い場所でレジャーシートを広げてお弁当を食べたいです。"},{"id":{"d":992,"n":3,"t":3},"speaker":"A","text":"おいしくお弁当を食べるためにはおなかをすかせた方がいいですね。軽い登山ができるようなところがあると良いのですが。"},{"id":{"d":992,"n":4,"t":3},"speaker":"B","text":"鎌倉山はどうでしょうか？少し標高もありますし、良い運動になると思います。"},{"id":{"d":992,"n":5,"t":3},"speaker":"A","text":"そうですね。久しぶりに鎌倉山へ行きましょう。お弁当は何にしましょうか？サンドイッチだとおなかが満たされないかもしれませんね。"},{"id":{"d":992,"n":6,"t":3},"speaker":"B","text":"サンドイッチでよいですよ。山頂でお弁当を食べましょう。下山した後は、小町通りのケーキ屋さんでケーキを食べて、疲れを癒やしましょう。"},{"id":{"d":992,"n":7,"t":3},"speaker":"A","text":"それは良い計画ですね。今から楽しみです。鎌倉は混みますから、あさっては早めに出発しましょう。"},{"id":{"d":992,"n":8,"t":3},"speaker":"B","text":"それでは、明日の夜のうちにお弁当の下ごしらえを済ませておくことにしますね。明日は早く寝ることにしましょう。"}];
INSERT INTO sentence [{"id":{"d":993,"n":1,"t":3},"speaker":"A","text":"こんにちは。無事に到着して良かったです。"},{"id":{"d":993,"n":2,"t":3},"speaker":"B","text":"こんにちは。京都から奈良まで約3時間かかりましたが、良いツーリングコースでしたよ。"},{"id":{"d":993,"n":3,"t":3},"speaker":"A","text":"途中の山道も難なく通過できましたか？"},{"id":{"d":993,"n":4,"t":3},"speaker":"B","text":"ええ。上り坂は自転車が全く進まないような感覚で少し大変でしたが、無事通過できました。"},{"id":{"d":993,"n":5,"t":3},"speaker":"A","text":"そうでしたか。車との接触もなかったようで、安心しました。"},{"id":{"d":993,"n":6,"t":3},"speaker":"B","text":"2度ほど少し危険を感じる場面がありましたが、事なきを得ました。"},{"id":{"d":993,"n":7,"t":3},"speaker":"A","text":"危ない状況になりかけた、という事ですか？"},{"id":{"d":993,"n":8,"t":3},"speaker":"B","text":"はい。でも、そのスリルも快感なのです。"},{"id":{"d":993,"n":9,"t":3},"speaker":"A","text":"くれぐれも気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":994,"n":1,"t":3},"speaker":"A","text":"すみません。いちご狩りの会場はこちらで間違いありませんか？"},{"id":{"d":994,"n":2,"t":3},"speaker":"B","text":"はい。そうですよ。"},{"id":{"d":994,"n":3,"t":3},"speaker":"A","text":"15時からいちご狩りの予約をしている大木です。受付をお願いします。"},{"id":{"d":994,"n":4,"t":3},"speaker":"B","text":"すみませんが、私たちもいちご狩りをしにきた客なのです。"},{"id":{"d":994,"n":5,"t":3},"speaker":"A","text":"これは失礼しました。"},{"id":{"d":994,"n":6,"t":3},"speaker":"B","text":"受付は、隣のビニールハウスで行っていますよ。"},{"id":{"d":994,"n":7,"t":3},"speaker":"A","text":"ご丁寧にありがとうございます。"},{"id":{"d":994,"n":8,"t":3},"speaker":"B","text":"いえいえ。お互い楽しみましょうね。"}];
INSERT INTO sentence [{"id":{"d":995,"n":1,"t":3},"speaker":"A","text":"あなたの趣味を教えてもらえますか？"},{"id":{"d":995,"n":2,"t":3},"speaker":"B","text":"私の趣味は、ソロキャンプです。"},{"id":{"d":995,"n":3,"t":3},"speaker":"A","text":"ソロキャンプにはどのような魅力がありますか？"},{"id":{"d":995,"n":4,"t":3},"speaker":"B","text":"やはり、日常生活から離れられるところでしょうか。日々の騒がしさから逃れるには、ソロキャンプが打って付けです。"},{"id":{"d":995,"n":5,"t":3},"speaker":"A","text":"非日常の体験ですか。すてきですね。"},{"id":{"d":995,"n":6,"t":3},"speaker":"B","text":"しかし、困ったことに、最近のめり込み過ぎてしまっているのです。"},{"id":{"d":995,"n":7,"t":3},"speaker":"A","text":"どのような弊害が起きているのですか？"},{"id":{"d":995,"n":8,"t":3},"speaker":"B","text":"貯金のほとんどをキャンプ用品に充ててしまっていて、手持ちのお金が尽き始めています。"},{"id":{"d":995,"n":9,"t":3},"speaker":"A","text":"それは大問題ですね。お財布と相談しながら楽しんでください。"}];
INSERT INTO sentence [{"id":{"d":996,"n":1,"t":3},"speaker":"A","text":"最近は家の近くで修学旅行生をよく見かけます。"},{"id":{"d":996,"n":2,"t":3},"speaker":"B","text":"さすが、観光地に住んでいるだけのことはありますね。"},{"id":{"d":996,"n":3,"t":3},"speaker":"A","text":"ええ。このところ観光客も減少していたので、賑わいを取り戻して安心しました。"},{"id":{"d":996,"n":4,"t":3},"speaker":"B","text":"そうですね。ところで、太田さんは高校時代の修学旅行の目的地は、どちらへ行きましたか？"},{"id":{"d":996,"n":5,"t":3},"speaker":"A","text":"私は広島に行きました。原爆ドームの見学をしましたよ。"},{"id":{"d":996,"n":6,"t":3},"speaker":"B","text":"広島ですか。私立高校出身の方は、海外へ行っているのかと思っていました。"},{"id":{"d":996,"n":7,"t":3},"speaker":"A","text":"私もそう思っていたのですが、なぜか私たちの学年だけ国内だったのですよ。高校の修学旅行で海外へ行くチャンスを逃してしまい、いまだに海外旅行の経験がありません。"},{"id":{"d":996,"n":8,"t":3},"speaker":"B","text":"それは残念でしたね。有給休暇を取って、ぜひ海外旅行へ行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":997,"n":1,"t":3},"speaker":"A","text":"来月の5日は、私たちが付き合って1年の記念日ですね。"},{"id":{"d":997,"n":2,"t":3},"speaker":"B","text":"そうですね。長かったような、あっという間だったような不思議な気持ちです。"},{"id":{"d":997,"n":3,"t":3},"speaker":"A","text":"私はあっという間に感じましたよ。たくさんお出掛けもできましたし、とても充実していました。"},{"id":{"d":997,"n":4,"t":3},"speaker":"B","text":"それは良かったです。僕と一緒に行った場所の中で、一番思い出に残っているのはどこですか？"},{"id":{"d":997,"n":5,"t":3},"speaker":"A","text":"一番は福岡です。夜中まで食べ歩きができる最高の街でした。"},{"id":{"d":997,"n":6,"t":3},"speaker":"B","text":"そうでしたね。口に入れるもの全てがおいしかったです。"},{"id":{"d":997,"n":7,"t":3},"speaker":"A","text":"2人合わせて5キログラムも太って帰って来たのも、良い思い出です。"},{"id":{"d":997,"n":8,"t":3},"speaker":"B","text":"食いしん坊な僕らにとって、旅先の食事のクオリティーは旅の最重要項目の一つであると、あの旅行で気付かされましたね。"}];
INSERT INTO sentence [{"id":{"d":998,"n":1,"t":3},"speaker":"A","text":"来月の10日は、結婚記念日ですね。"},{"id":{"d":998,"n":2,"t":3},"speaker":"B","text":"そうですね。せっかくですから、どこかへお出掛けしましょうか？"},{"id":{"d":998,"n":3,"t":3},"speaker":"A","text":"賛成です。行きたいところはありますか？"},{"id":{"d":998,"n":4,"t":3},"speaker":"B","text":"京都水族館へ行きたいです。"},{"id":{"d":998,"n":5,"t":3},"speaker":"A","text":"京都水族館は一昨年にも行きましたね。また同じところで良いのですか？別の水族館が良いのではありませんか？"},{"id":{"d":998,"n":6,"t":3},"speaker":"B","text":"いえ、京都水族館が良いのです。ちょうど今、すてきなイベントが開催されているのですよ。"},{"id":{"d":998,"n":7,"t":3},"speaker":"A","text":"それならそちらに行きましょう。ちなみに、どのようなイベントですか？"},{"id":{"d":998,"n":8,"t":3},"speaker":"B","text":"夜限定でライトアップなどの特別な演出がされるようなのです。"},{"id":{"d":998,"n":9,"t":3},"speaker":"A","text":"想像しただけでもロマンチックですね。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":999,"n":1,"t":3},"speaker":"A","text":"今年の夏は、暑すぎて出掛ける気になれません。"},{"id":{"d":999,"n":2,"t":3},"speaker":"B","text":"そうですよね。私も大阪旅行を計画していましたが、8月の予想平均最高気温が35度と聞いて、行く気が失せています。"},{"id":{"d":999,"n":3,"t":3},"speaker":"A","text":"大阪ですか、良いですね。涼しく過ごせる旅行プランはないでしょうか？"},{"id":{"d":999,"n":4,"t":3},"speaker":"B","text":"大阪では難しいかもしれませんね。京都でしたら納涼の旅ができそうですよ。"},{"id":{"d":999,"n":5,"t":3},"speaker":"A","text":"京都の納涼の旅とは、どのようなことをするのですか？"},{"id":{"d":999,"n":6,"t":3},"speaker":"B","text":"貴船神社へ行った後、川床で食事をするというのはどうでしょうか？"},{"id":{"d":999,"n":7,"t":3},"speaker":"A","text":"川床という案はすてきですね。川沿いは視覚的にも涼しげですし、実際に気温も少し低いでしょうし。"},{"id":{"d":999,"n":8,"t":3},"speaker":"B","text":"本格的に計画を練ってみましょうか？"},{"id":{"d":999,"n":9,"t":3},"speaker":"A","text":"ええ。私も考えます。"}];
INSERT INTO sentence [{"id":{"d":1000,"n":1,"t":3},"speaker":"A","text":"久しぶりに、東京ディズニーランドへ行きたいです。"},{"id":{"d":1000,"n":2,"t":3},"speaker":"B","text":"私も最近同じことを思っています。東京ディズニーシーへはよく行くのですが、東京ディズニーランドへはもう8年も行っていません。"},{"id":{"d":1000,"n":3,"t":3},"speaker":"A","text":"8年ということは、中学生の頃以来行っていないということですね？"},{"id":{"d":1000,"n":4,"t":3},"speaker":"B","text":"そうです。東京ディズニーシーの方が写真映えするスポットが多いので、SNSで知り合った友人たちと遊びに行くときは、そちらを選んでしまいがちです。"},{"id":{"d":1000,"n":5,"t":3},"speaker":"A","text":"SNSが関わってくると、出先の選択にはフォトスポットの有無は重要ですよね。"},{"id":{"d":1000,"n":6,"t":3},"speaker":"B","text":"はい。どうしても気にしてしまいます。"},{"id":{"d":1000,"n":7,"t":3},"speaker":"A","text":"写真を撮ることも楽しいですが、たまには体験そのものを堪能するのも良いなと思っています。"},{"id":{"d":1000,"n":8,"t":3},"speaker":"B","text":"私もそう思います。今度、ぜひ一緒に東京ディズニーランドへ行きませんか？2人で「映え」を気にしない旅をしましょう。"}];
INSERT INTO sentence [{"id":{"d":1001,"n":1,"t":3},"speaker":"A","text":"お姉さん、相談したいことがあります。"},{"id":{"d":1001,"n":2,"t":3},"speaker":"B","text":"どうしましたか？何でも話を聞きますよ。"},{"id":{"d":1001,"n":3,"t":3},"speaker":"A","text":"実は、好きな子とデートに行くことになりました。初めてのデートにお薦めの場所を教えてください。"},{"id":{"d":1001,"n":4,"t":3},"speaker":"B","text":"それは楽しみですね。行き先は、お台場が良いと思いますよ。"},{"id":{"d":1001,"n":5,"t":3},"speaker":"A","text":"それはどうしてでしょうか？"},{"id":{"d":1001,"n":6,"t":3},"speaker":"B","text":"いろいろな施設があるので、女の子を飽きさせることがありません。レストランやカフェも多いので、食事や休憩をする場所にも困りません。"},{"id":{"d":1001,"n":7,"t":3},"speaker":"A","text":"なるほど、それは確かに魅力的ですね。参考になります。"},{"id":{"d":1001,"n":8,"t":3},"speaker":"B","text":"うまく行くと良いですね。"},{"id":{"d":1001,"n":9,"t":3},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":1001,"n":10,"t":3},"speaker":"B","text":"帰ってきたらどのようなデートになったか教えてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1002,"n":1,"t":3},"speaker":"A","text":"すみません、道をお尋ねしたいのですが。"},{"id":{"d":1002,"n":2,"t":3},"speaker":"B","text":"はい。行き先はどちらですか？"},{"id":{"d":1002,"n":3,"t":3},"speaker":"A","text":"四条河原町駅です。もう1時間近くこの付近をさまよっているのですが、いっこうにたどり着けません。"},{"id":{"d":1002,"n":4,"t":3},"speaker":"B","text":"四条河原町駅は地下鉄ですので、確かに分かりづらいですよね。ここから北上して、交番が見えたらその横の階段から地下へ降りてください。"},{"id":{"d":1002,"n":5,"t":3},"speaker":"A","text":"なるほど、地下に降りなければならないのですね。とても助かりました。ありがとうございました。"},{"id":{"d":1002,"n":6,"t":3},"speaker":"B","text":"どういたしまして。どちらからいらしたのですか？"},{"id":{"d":1002,"n":7,"t":3},"speaker":"A","text":"平和田村から来ました。"},{"id":{"d":1002,"n":8,"t":3},"speaker":"B","text":"平和田村ですか。道中長いかと思いますがお気を付けて。"}];
INSERT INTO sentence [{"id":{"d":1003,"n":1,"t":3},"speaker":"A","text":"ゆきちゃん、たかし君との初デートはどこに行きましたか？"},{"id":{"d":1003,"n":2,"t":3},"speaker":"B","text":"「星のブランコ」という紅葉の名所に行きました。"},{"id":{"d":1003,"n":3,"t":3},"speaker":"A","text":"奈良と大阪の県境にある観光地ですね。とても大きくて高いつり橋があると聞いたことがあります。"},{"id":{"d":1003,"n":4,"t":3},"speaker":"B","text":"そうです。つり橋はとても怖かったですが、たかし君と一緒だったのでなんとか渡りきれました。"},{"id":{"d":1003,"n":5,"t":3},"speaker":"A","text":"それは良かったですね。あのつり橋は電車だけでは行けないような場所にありますが、そこまではどのようにして行ったのですか？"},{"id":{"d":1003,"n":6,"t":3},"speaker":"B","text":"彼のバイクで行きました。初めてバイクの後ろに乗りましたが、なかなか良いものですね。"},{"id":{"d":1003,"n":7,"t":3},"speaker":"A","text":"私もバイクの免許を持っているので、バイクに乗ることの気持ち良さにはとても共感できます。風を切る感覚がたまりません。"},{"id":{"d":1003,"n":8,"t":3},"speaker":"B","text":"あなたもバイクに乗るのですね。私も近々二輪免許証を取りに行くつもりなので、いつか3人でツーリングしましょう。"}];
INSERT INTO sentence [{"id":{"d":1004,"n":1,"t":3},"speaker":"A","text":"あなたは、海外に行った経験がありますか？"},{"id":{"d":1004,"n":2,"t":3},"speaker":"B","text":"はい。高校の頃、語学研修で2週間ほど滞在しました。"},{"id":{"d":1004,"n":3,"t":3},"speaker":"A","text":"どちらへ行ったのですか？"},{"id":{"d":1004,"n":4,"t":3},"speaker":"B","text":"米国のロサンゼルスです。空気がカラッとしていて、最高でした。"},{"id":{"d":1004,"n":5,"t":3},"speaker":"A","text":"確かに、ロサンゼルスは年中晴れているイメージがあります。食事はおいしかったですか？"},{"id":{"d":1004,"n":6,"t":3},"speaker":"B","text":"はい。とてもおいしかったです。もし海外に住むことになったら、ロサンゼルスに住みたいと思っているほどですよ。"},{"id":{"d":1004,"n":7,"t":3},"speaker":"A","text":"そうなのですか。その時は、ぜひ私も遊びに行かせてください。"},{"id":{"d":1004,"n":8,"t":3},"speaker":"B","text":"もちろん。あそび一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1005,"n":1,"t":3},"speaker":"A","text":"あなたは、米国へ行ったことがありますか？"},{"id":{"d":1005,"n":2,"t":3},"speaker":"B","text":"はい。仕事の関係で3年ほど住んでいました。"},{"id":{"d":1005,"n":3,"t":3},"speaker":"A","text":"長期間の赴任だったのですね。米国に住んでみた感想を教えてください。"},{"id":{"d":1005,"n":4,"t":3},"speaker":"B","text":"とてもすてきなところでした。気候も爽やかで、食事も日本人の口に合うものが多い印象でした。"},{"id":{"d":1005,"n":5,"t":3},"speaker":"A","text":"それはとても良いですね。逆に、合わなかった点はありますか？"},{"id":{"d":1005,"n":6,"t":3},"speaker":"B","text":"唯一、シャワー文化であることが不満でした。湯船に漬かることができる機会がとても少なかったです。"},{"id":{"d":1005,"n":7,"t":3},"speaker":"A","text":"それはお風呂好きにはつらいですね。"},{"id":{"d":1005,"n":8,"t":3},"speaker":"B","text":"はい。温泉が大好きなので、本当につらかったです。"}];
INSERT INTO sentence [{"id":{"d":1006,"n":1,"t":3},"speaker":"A","text":"フロントの田中でございます。どうされましたか？"},{"id":{"d":1006,"n":2,"t":3},"speaker":"B","text":"501号室の者です。確認したいことがあるのですが。"},{"id":{"d":1006,"n":3,"t":3},"speaker":"A","text":"はい、お伺いいたします。"},{"id":{"d":1006,"n":4,"t":3},"speaker":"B","text":"チェックアウト時間の確認をさせてください。チェックアウト時間が11時のプランで予約しましたが、先ほど頂いた案内には10時と書かれていました。"},{"id":{"d":1006,"n":5,"t":3},"speaker":"A","text":"ただいま確認しましたところ、こちらの手違いで10時と記載してしまいました。申し訳ございません。"},{"id":{"d":1006,"n":6,"t":3},"speaker":"B","text":"では、予定通り11時にチェックアウトしてよろしいでしょうか？"},{"id":{"d":1006,"n":7,"t":3},"speaker":"A","text":"こちらの不手際でご迷惑をおかけしましたので、12時チェックアウトプランに変更いたします。"},{"id":{"d":1006,"n":8,"t":3},"speaker":"B","text":"追加料金はかかりませんか？"},{"id":{"d":1006,"n":9,"t":3},"speaker":"A","text":"はい。もちろんサービスです。ゆっくりご滞在をお楽しみくださいませ。"}];
INSERT INTO sentence [{"id":{"d":1007,"n":1,"t":3},"speaker":"A","text":"京都交通の上原です。ご予約ですか？"},{"id":{"d":1007,"n":2,"t":3},"speaker":"B","text":"はい、タクシーの予約をしたいのですが。"},{"id":{"d":1007,"n":3,"t":3},"speaker":"A","text":"承知いたしました。ご予約の日時と乗車場所をお伺いいたします。"},{"id":{"d":1007,"n":4,"t":3},"speaker":"B","text":"明日の10時に、京都駅から乗車したいと考えています。"},{"id":{"d":1007,"n":5,"t":3},"speaker":"A","text":"1台のみのご予約で間違いありませんか？"},{"id":{"d":1007,"n":6,"t":3},"speaker":"B","text":"微妙なところです。1台に何人まで乗れますか？"},{"id":{"d":1007,"n":7,"t":3},"speaker":"A","text":"1台にご乗車可能な人数は、5名までです。"},{"id":{"d":1007,"n":8,"t":3},"speaker":"B","text":"それでしたら1台で結構です。上限の5人で乗らせていただきます。"},{"id":{"d":1007,"n":9,"t":3},"speaker":"A","text":"承知いたしました。それでは明日、7月5日の午前10時に京都駅のA4乗り場でお待ちしております。"},{"id":{"d":1007,"n":10,"t":3},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":1008,"n":1,"t":3},"speaker":"A","text":"JR東日本の上野が承ります。"},{"id":{"d":1008,"n":2,"t":3},"speaker":"B","text":"新幹線の予約を変更したいのですが、可能でしょうか？"},{"id":{"d":1008,"n":3,"t":3},"speaker":"A","text":"はい。予約番号とお名前をお伺いいたします。"},{"id":{"d":1008,"n":4,"t":3},"speaker":"B","text":"予約番号54329876の大橋学です。"},{"id":{"d":1008,"n":5,"t":3},"speaker":"A","text":"確認ができました。本日20時10分発の博多行きに新大阪駅よりご乗車ですね。"},{"id":{"d":1008,"n":6,"t":3},"speaker":"B","text":"はい。その列車の3本後の列車に変更をお願いします。"},{"id":{"d":1008,"n":7,"t":3},"speaker":"A","text":"20時45分発ののぞみですね。指定席の場所はご希望ありますでしょうか？"},{"id":{"d":1008,"n":8,"t":3},"speaker":"B","text":"窓側でしたらどこでも構いません。"},{"id":{"d":1008,"n":9,"t":3},"speaker":"A","text":"承知いたしました。変更いたしましたので、オンラインチケットのご確認をお願いします。"},{"id":{"d":1008,"n":10,"t":3},"speaker":"B","text":"分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":1009,"n":1,"t":3},"speaker":"A","text":"いらっしゃいませ。ご注文をお伺いいたします。"},{"id":{"d":1009,"n":2,"t":3},"speaker":"B","text":"旅行でこちらに来ているので、ぜひ、博多名物を食べたいのですが、何がお薦めですか？"},{"id":{"d":1009,"n":3,"t":3},"speaker":"A","text":"王道はもつ鍋です。特に、明太子トッピングのもつ鍋は博多名物の盛り合わせです。"},{"id":{"d":1009,"n":4,"t":3},"speaker":"B","text":"良いですね。そちらを頂きます。"},{"id":{"d":1009,"n":5,"t":3},"speaker":"A","text":"承知いたしました。お飲み物はどうされますか？"},{"id":{"d":1009,"n":6,"t":3},"speaker":"B","text":"こちらには、博多の地酒などは置いていますか？"},{"id":{"d":1009,"n":7,"t":3},"speaker":"A","text":"地酒ではありませんが、こちらの焼酎が近くの酒屋さんのオリジナルのものです。普段、焼酎はお飲みになりますか？"},{"id":{"d":1009,"n":8,"t":3},"speaker":"B","text":"はい。私は、焼酎しか飲まないほど焼酎好きです。ぜひ、そちらを頂きます。"},{"id":{"d":1009,"n":9,"t":3},"speaker":"A","text":"はい、すぐにご用意いたします。"}];
INSERT INTO sentence [{"id":{"d":1010,"n":1,"t":3},"speaker":"A","text":"はい、やまとカフェでございます。"},{"id":{"d":1010,"n":2,"t":3},"speaker":"B","text":"こんにちは。席の予約をしたいのですがよろしいですか？"},{"id":{"d":1010,"n":3,"t":3},"speaker":"A","text":"かしこまりました。ご予約の日時とご来店人数をお伺いいたします。"},{"id":{"d":1010,"n":4,"t":3},"speaker":"B","text":"4月3日の18時に3人でお願いしたいです。"},{"id":{"d":1010,"n":5,"t":3},"speaker":"A","text":"申し訳ございません。そちらの日時は貸し切りの予約が入っておりまして、ご予約をお取りできません。"},{"id":{"d":1010,"n":6,"t":3},"speaker":"B","text":"そうですか。残念です。"},{"id":{"d":1010,"n":7,"t":3},"speaker":"A","text":"通りを挟んだところに、系列店がございますがいかがでしょうか？当店よりも少しカジュアルな印象ですが、お料理の様子は似ています。"},{"id":{"d":1010,"n":8,"t":3},"speaker":"B","text":"系列店があるのですね。教えていただきありがとうございます。調べてみます。"}];
INSERT INTO sentence [{"id":{"d":1011,"n":1,"t":3},"speaker":"A","text":"すみません、お尋ねしたいことがあります。"},{"id":{"d":1011,"n":2,"t":3},"speaker":"B","text":"はい、お伺いいたします。どうなさいましたか？"},{"id":{"d":1011,"n":3,"t":3},"speaker":"A","text":"こちらの時刻表にある、12時45分発のバスに乗る予定だったのですが、本日は運休でしょうか？到着時刻前から待っていますが、まだバスが来る気配がありません。"},{"id":{"d":1011,"n":4,"t":3},"speaker":"B","text":"こちらの時刻表は、平日ダイヤです。今日は土曜日なので、裏面の休日ダイヤをご覧ください。"},{"id":{"d":1011,"n":5,"t":3},"speaker":"A","text":"そうでしたか。きちんと隅まで見ていませんでした。すみません。"},{"id":{"d":1011,"n":6,"t":3},"speaker":"B","text":"こちらこそ申し訳ありません。さらに大きな文字で記載すべきでした。"},{"id":{"d":1011,"n":7,"t":3},"speaker":"A","text":"とんでもありません。次のバスは、13時に到着するようですね。"},{"id":{"d":1011,"n":8,"t":3},"speaker":"B","text":"はい。あと5分ほどお待ちください。"}];
INSERT INTO sentence [{"id":{"d":1012,"n":1,"t":3},"speaker":"A","text":"当選おめでとうございます。日本一周クルージングの旅券を2名さま分プレゼントいたします。"},{"id":{"d":1012,"n":2,"t":3},"speaker":"B","text":"ありがとうございます。旅行券が当たるのは人生で初めてです。"},{"id":{"d":1012,"n":3,"t":3},"speaker":"A","text":"そうでしたか。どうぞ、最高のクルージングを楽しんできてください。"},{"id":{"d":1012,"n":4,"t":3},"speaker":"B","text":"こちらのクルージングは、いつからいつまでの航海でしょうか？"},{"id":{"d":1012,"n":5,"t":3},"speaker":"A","text":"来年の7月8日から7月19日の12日間です。"},{"id":{"d":1012,"n":6,"t":3},"speaker":"B","text":"来年なら、もろもろの日程の調整が可能だと思います。では、ありがたく頂きます。"},{"id":{"d":1012,"n":7,"t":3},"speaker":"A","text":"チケットは2名さま分ですが、どなたと参加されますか？"},{"id":{"d":1012,"n":8,"t":3},"speaker":"B","text":"家内を誘うつもりです。彼女にリフレッシュしてもらおうと思います。"},{"id":{"d":1012,"n":9,"t":3},"speaker":"A","text":"すてきですね。お二人にとって良い旅になりますように。"}];
INSERT INTO sentence [{"id":{"d":1013,"n":1,"t":3},"speaker":"A","text":"来月はももちゃんの誕生日ですね。サプライズで旅行を計画しませんか？"},{"id":{"d":1013,"n":2,"t":3},"speaker":"B","text":"名案ですね。行き先はどうしましょうか？"},{"id":{"d":1013,"n":3,"t":3},"speaker":"A","text":"ももちゃんは、旅館の宿泊レポートをよく読んでいる印象があります。その中の彼女が気に入った旅館に連れて行きたいです。"},{"id":{"d":1013,"n":4,"t":3},"speaker":"B","text":"いいですね。彼女のお気に入りの旅館を、私たちはどのようにして知ることができるでしょうか？"},{"id":{"d":1013,"n":5,"t":3},"speaker":"A","text":"ももちゃんのTwitterの「いいね」欄を見てみませんか？もしかしたら手掛かりがあるかもしれません。"},{"id":{"d":1013,"n":6,"t":3},"speaker":"B","text":"なるほど、それは良い案です。"},{"id":{"d":1013,"n":7,"t":3},"speaker":"A","text":"見てください。ももちゃんが京都の旅館に「いいね」していますよ。"},{"id":{"d":1013,"n":8,"t":3},"speaker":"B","text":"すてきなところですね。京都駅前にあるようです。"},{"id":{"d":1013,"n":9,"t":3},"speaker":"A","text":"朝食付きで、1泊1人1万円からだそうです。"},{"id":{"d":1013,"n":10,"t":3},"speaker":"B","text":"それは平日の値段ですね。ももちゃんの誕生日は祝日なので、祝日料金になるのではないでしょうか？"},{"id":{"d":1013,"n":11,"t":3},"speaker":"A","text":"そうでした。平日より少し高くなりますね。"},{"id":{"d":1013,"n":12,"t":3},"speaker":"B","text":"でも、このくらいの値段なら十分プレゼントできます。旅館と合わせて、日中のプランも組み始めましょう。"}];
INSERT INTO sentence [{"id":{"d":1014,"n":1,"t":3},"speaker":"A","text":"おなかがすいてきました。たくや君はどうですか？"},{"id":{"d":1014,"n":2,"t":3},"speaker":"B","text":"僕も空腹です。そろそろ昼食にしましょう。どこか良いお店を知っていますか？"},{"id":{"d":1014,"n":3,"t":3},"speaker":"A","text":"私が学生時代によく行っていた定食屋さんがこの近くにあったはずです。そこへ行きましょう。"},{"id":{"d":1014,"n":4,"t":3},"speaker":"B","text":"良いですね。車のカーナビゲーションシステムでその定食屋さんを目的地に設定してくれますか？"},{"id":{"d":1014,"n":5,"t":3},"speaker":"A","text":"はい。あれ、おかしいですね。"},{"id":{"d":1014,"n":6,"t":3},"speaker":"B","text":"どうかしましたか？"},{"id":{"d":1014,"n":7,"t":3},"speaker":"A","text":"「ごはんの空」というお店なのですが、検索してもヒットしません。"},{"id":{"d":1014,"n":8,"t":3},"speaker":"B","text":"スマートフォンのマップで検索してみてはどうですか？"},{"id":{"d":1014,"n":9,"t":3},"speaker":"A","text":"はい、検索してみます。"},{"id":{"d":1014,"n":10,"t":3},"speaker":"B","text":"どうでしょうか？お店までの行き方は分かりそうですか？"},{"id":{"d":1014,"n":11,"t":3},"speaker":"A","text":"なんと、とても残念なことに、そのお店は去年閉店してしまったそうです。"},{"id":{"d":1014,"n":12,"t":3},"speaker":"B","text":"それは残念ですね。他に思い出のご飯屋さんはありませんか？"},{"id":{"d":1014,"n":13,"t":3},"speaker":"A","text":"今すぐ思い当たるところはありません。思い付くまで、ドライブを続けてもらえますか？"}];
INSERT INTO sentence [{"id":{"d":1015,"n":1,"t":3},"speaker":"A","text":"あなたの趣味は何ですか？"},{"id":{"d":1015,"n":2,"t":3},"speaker":"B","text":"ドライブです。何も考えずにひたすら真っ直ぐ車を走らせるのが快感で、高速道路をドライブするのが特に好きです。"},{"id":{"d":1015,"n":3,"t":3},"speaker":"A","text":"高速道路を走るだけで、着いた先の観光はされませんか？"},{"id":{"d":1015,"n":4,"t":3},"speaker":"B","text":"はい。観光はしませんが、サービスエリアを巡るのは好きです。"},{"id":{"d":1015,"n":5,"t":3},"speaker":"A","text":"面白いですね。お薦めのサービスエリアはありますか？"},{"id":{"d":1015,"n":6,"t":3},"speaker":"B","text":"海老名サービスエリアです。"},{"id":{"d":1015,"n":7,"t":3},"speaker":"A","text":"なぜ海老名サービスエリアがお薦めなのですか？"},{"id":{"d":1015,"n":8,"t":3},"speaker":"B","text":"そこで販売されている人形焼が、カスタードクリームが入っていて絶品なのです。"},{"id":{"d":1015,"n":9,"t":3},"speaker":"A","text":"海老名サービスエリアで人形焼が売られているとは知りませんでした。"},{"id":{"d":1015,"n":10,"t":3},"speaker":"B","text":"海老名サービスエリアに行ったことはありますか？"},{"id":{"d":1015,"n":11,"t":3},"speaker":"A","text":"はい。何度か立ち寄ったことはあります。海老名サービスエリアは駐車可能な台数がとても多く、駐車できずに困ることがほとんどないのが良いですよね。"},{"id":{"d":1015,"n":12,"t":3},"speaker":"B","text":"はい。それも海老名サービスエリアの魅力の一つです。"}];
INSERT INTO sentence [{"id":{"d":1016,"n":1,"t":3},"speaker":"A","text":"車内販売です。アイスクリームやコーヒー、マガジンはいかがでしょうか？"},{"id":{"d":1016,"n":2,"t":3},"speaker":"B","text":"すみません、アイスクリームを1個頂けますか？"},{"id":{"d":1016,"n":3,"t":3},"speaker":"A","text":"はい、300円でございます。コーヒーとセットになさいますと50円引きされますが、いかがでしょうか？"},{"id":{"d":1016,"n":4,"t":3},"speaker":"B","text":"では、コーヒーも頂きます。"},{"id":{"d":1016,"n":5,"t":3},"speaker":"A","text":"合計400円でございます。"},{"id":{"d":1016,"n":6,"t":3},"speaker":"B","text":"はい、400円お支払いします。"},{"id":{"d":1016,"n":7,"t":3},"speaker":"A","text":"ちょうどの金額をお預かりいたします。レシートはご利用ですか？"},{"id":{"d":1016,"n":8,"t":3},"speaker":"B","text":"いえ、必要ありません。"},{"id":{"d":1016,"n":9,"t":3},"speaker":"A","text":"かしこまりました。では、商品をお渡しいたしましね。"},{"id":{"d":1016,"n":10,"t":3},"speaker":"B","text":"はい。"},{"id":{"d":1016,"n":11,"t":3},"speaker":"A","text":"まずは、アイスクリームのお渡しです。"},{"id":{"d":1016,"n":12,"t":3},"speaker":"B","text":"ありがとうございます。袋も1枚頂けますか？"},{"id":{"d":1016,"n":13,"t":3},"speaker":"A","text":"かしこまりました。こちらが袋とコーヒーです。コーヒーは、大変熱いのでお気を付けください。"},{"id":{"d":1016,"n":14,"t":3},"speaker":"B","text":"ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":1017,"n":1,"t":3},"speaker":"A","text":"向井君が、今一番行きたいところはどこですか？"},{"id":{"d":1017,"n":2,"t":3},"speaker":"B","text":"月へ行きたいです。宇宙旅行は、物心が付いた頃からの夢なのです。"},{"id":{"d":1017,"n":3,"t":3},"speaker":"A","text":"最近、宇宙旅行が現実のものになりつつありますから、不可能ではないと思います。月に行けたら、何をしたいですか？"},{"id":{"d":1017,"n":4,"t":3},"speaker":"B","text":"僕は一眼レフカメラで旅行先の風景の写真を撮るのが好きなので、月でも撮影会をしたいです。"},{"id":{"d":1017,"n":5,"t":3},"speaker":"A","text":"とてもきれいな写真が撮れそうですね。これまでもカメラを持って旅行されていたとのことですが、旅行先で撮った写真を見せていただけますか？"},{"id":{"d":1017,"n":6,"t":3},"speaker":"B","text":"もちろんです。今一番お気に入りの写真は、琵琶湖の畔で撮った白鳥の写真です。"},{"id":{"d":1017,"n":7,"t":3},"speaker":"A","text":"白鳥のつがいが仲良く泳いでいますね。琵琶湖ならではの、のどかな空気が感じられます。"},{"id":{"d":1017,"n":8,"t":3},"speaker":"B","text":"それが写真の魅力です。その場の雰囲気も閉じ込めることができますから。ぜひ、宇宙の空気感も落とし込んだ写真を撮影したいです。"}];
INSERT INTO sentence [{"id":{"d":1018,"n":1,"t":3},"speaker":"A","text":"中村さんは、いろいろな国へ旅行に行っていますよね。とてもうらやましいです。"},{"id":{"d":1018,"n":2,"t":3},"speaker":"B","text":"はい。さまざまな文化に触れることは、自分自身の世界を広げることに繋がると考えているので、積極的に外国を訪れるようにしています。"},{"id":{"d":1018,"n":3,"t":3},"speaker":"A","text":"私もその考えに賛成します。ただ、言語の壁が立ちはだかるので、英語圏外には行く勇気が出ません。"},{"id":{"d":1018,"n":4,"t":3},"speaker":"B","text":"今は、言語の壁はあってないようなものですよ。"},{"id":{"d":1018,"n":5,"t":3},"speaker":"A","text":"それはどうしてでしょうか？"},{"id":{"d":1018,"n":6,"t":3},"speaker":"B","text":"言語翻訳機があるからです。言語翻訳機があれば、どこの国に行っても怖くありません。とても正確に翻訳してくれるため、この機械があれば自分の意思が伝わらなかったことがありません。"},{"id":{"d":1018,"n":7,"t":3},"speaker":"A","text":"そうなのですか。その機械は他言語から日本語へ和訳することも可能ですか？"},{"id":{"d":1018,"n":8,"t":3},"speaker":"B","text":"もちろんです。ぜひ次の旅行のお供にしてください。"}];
INSERT INTO sentence [{"id":{"d":1019,"n":1,"t":3},"speaker":"A","text":"先週末は何をして過ごしましたか？"},{"id":{"d":1019,"n":2,"t":3},"speaker":"B","text":"大阪まで一人旅行をしてきました。良いリフレッシュになりました。"},{"id":{"d":1019,"n":3,"t":3},"speaker":"A","text":"それは何よりです。実は、私は今まで一人旅行をしたことがありません。一人旅行の魅力を教えていただけますか？"},{"id":{"d":1019,"n":4,"t":3},"speaker":"B","text":"一番の魅力は、気になったお店に気軽に入れることです。同行者がいないため、気の向くままに旅行先を堪能できます。"},{"id":{"d":1019,"n":5,"t":3},"speaker":"A","text":"それは良いですね。しかし、道に迷った時に相談相手がいないと不安になることはありませんか？"},{"id":{"d":1019,"n":6,"t":3},"speaker":"B","text":"そういった時は、ぜひ現地の方に尋ねてみてください。その土地のことは何でもご存じですし、ガイドマップに載っていない裏情報を聞けることがあります。"},{"id":{"d":1019,"n":7,"t":3},"speaker":"A","text":"なるほど、参考になります。早速、次の3連休に初めての一人旅行をしてみようかしら。"},{"id":{"d":1019,"n":8,"t":3},"speaker":"B","text":"良いですね。ぜひ楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":1020,"n":1,"t":3},"speaker":"A","text":"新幹線の乗車券をなくしてしまいました。"},{"id":{"d":1020,"n":2,"t":3},"speaker":"B","text":"新幹線の乗車券の購入時に発行される領収書はお持ちですか？"},{"id":{"d":1020,"n":3,"t":3},"speaker":"A","text":"いえ、領収書と乗車券を入れたビニール袋をお弁当の容器と一緒に捨ててしまったため、領収書もありません。"},{"id":{"d":1020,"n":4,"t":3},"speaker":"B","text":"そうしましたら、恐れ入りますが、再度乗車券を購入していただく必要があります。"},{"id":{"d":1020,"n":5,"t":3},"speaker":"A","text":"本当に購入したのに、二重で払わなければなりませんか？納得いきません。"},{"id":{"d":1020,"n":6,"t":3},"speaker":"B","text":"近年、無賃乗車が増えてきています。そのため、購入の証拠となるものをお持ちでない場合は、乗車券の再発行は禁止されています。"},{"id":{"d":1020,"n":7,"t":3},"speaker":"A","text":"分かりました。もう一度購入してきます。"},{"id":{"d":1020,"n":8,"t":3},"speaker":"B","text":"次回ご利用時は、乗車券の紛失にお気を付けください。"}];
