INSERT INTO sentence [{"id":{"d":1,"n":1,"t":2},"speaker":"A","text":"英語の小テストの勉強はしましたか？"},{"id":{"d":1,"n":2,"t":2},"speaker":"B","text":"今日は小テストがある日ですか？知りませんでした。テスト範囲は、どのページか教えてくれますか？"},{"id":{"d":1,"n":3,"t":2},"speaker":"A","text":"分かりました。今日はテキストの16ページから20ページが範囲です。"},{"id":{"d":1,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。今からそのページを確認してみます。"},{"id":{"d":1,"n":5,"t":2},"speaker":"A","text":"はい、ぜひしっかり確認してください。"}];
INSERT INTO sentence [{"id":{"d":2,"n":1,"t":2},"speaker":"A","text":"遠足の持ち物について、質問がある人はいますか？"},{"id":{"d":2,"n":2,"t":2},"speaker":"B","text":"はい、バナナはおやつに入るのかどうか教えてください。"},{"id":{"d":2,"n":3,"t":2},"speaker":"A","text":"バナナは、おやつには入りません。"},{"id":{"d":2,"n":4,"t":2},"speaker":"B","text":"バナナを入れずに500円以内のおやつを買うということですね。"},{"id":{"d":2,"n":5,"t":2},"speaker":"A","text":"はい、そうです。でも、食べられる分だけにしてくださいね。"},{"id":{"d":2,"n":6,"t":2},"speaker":"B","text":"もちろんです。分かりました。"}];
INSERT INTO sentence [{"id":{"d":3,"n":1,"t":2},"speaker":"A","text":"佐藤さん、今日の放課後は何をして過ごしますか？"},{"id":{"d":3,"n":2,"t":2},"speaker":"B","text":"今日は映画館に、映画を見に行く予定です。"},{"id":{"d":3,"n":3,"t":2},"speaker":"A","text":"映画ですか、それは良いですね。ただ今度テストがあるので、その勉強を一緒にしませんか？"},{"id":{"d":3,"n":4,"t":2},"speaker":"B","text":"確かに、もうすぐテストがありますね。分かりました。一緒に勉強しましょう。"},{"id":{"d":3,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。そうしましたら、どこで勉強しましょうか？"},{"id":{"d":3,"n":6,"t":2},"speaker":"B","text":"私の自宅はどうでしょうか？家族も歓迎してくれますよ。"},{"id":{"d":3,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。では放課後、佐藤さんのお家で一緒にお勉強しましょう。"}];
INSERT INTO sentence [{"id":{"d":4,"n":1,"t":2},"speaker":"A","text":"ここは非常階段です。"},{"id":{"d":4,"n":2,"t":2},"speaker":"B","text":"ここの階段は、あまり人が通りませんね。"},{"id":{"d":4,"n":3,"t":2},"speaker":"A","text":"はい、ここは教室からは遠いので、あまり人が通りません。だから人気の告白スポットになっています。"},{"id":{"d":4,"n":4,"t":2},"speaker":"B","text":"それは良いですね。みんなに見られずに告白ができそうです。"}];
INSERT INTO sentence [{"id":{"d":5,"n":1,"t":2},"speaker":"A","text":"今日の給食はカレーです。カレーは好きなのですが、ご飯にグリーンピースが入っているのが残念です。"},{"id":{"d":5,"n":2,"t":2},"speaker":"B","text":"中村さんはグリーンピースが苦手なのですか？"},{"id":{"d":5,"n":3,"t":2},"speaker":"A","text":"はい。においが苦手で、あまり好きではありません。"},{"id":{"d":5,"n":4,"t":2},"speaker":"B","text":"そうなのですね。グリーンピースは栄養が豊富ですよ。部活動を頑張っている中村さんの力になると思います。"},{"id":{"d":5,"n":5,"t":2},"speaker":"A","text":"栄養豊富なのですか。それは知りませんでした。今日は頑張って食べてみます。"},{"id":{"d":5,"n":6,"t":2},"speaker":"B","text":"はい、ぜひ食べてみてください。"}];
INSERT INTO sentence [{"id":{"d":6,"n":1,"t":2},"speaker":"A","text":"今日からこの学校に通う転校生を紹介します。林さん、自己紹介をお願いします。"},{"id":{"d":6,"n":2,"t":2},"speaker":"B","text":"初めまして。東京都から引っ越してきました林です。よろしくお願いします。"},{"id":{"d":6,"n":3,"t":2},"speaker":"A","text":"これから、よろしくお願いします。林さんの好きな事は何でしょうか？"},{"id":{"d":6,"n":4,"t":2},"speaker":"B","text":"私はスポーツと絵を描く事が好きです。"},{"id":{"d":6,"n":5,"t":2},"speaker":"A","text":"それはすてきですね。この学校にはさまざまな部活動があるので、ぜひ見学してみてくださいね。"},{"id":{"d":6,"n":6,"t":2},"speaker":"B","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":7,"n":1,"t":2},"speaker":"A","text":"佐藤さん、おなかを押さえていますが、どうしましたか？"},{"id":{"d":7,"n":2,"t":2},"speaker":"B","text":"少し前からおなかが痛いのです。"},{"id":{"d":7,"n":3,"t":2},"speaker":"A","text":"大丈夫ですか？保健室に行きますか？"},{"id":{"d":7,"n":4,"t":2},"speaker":"B","text":"いえ、このまま安静にしていれば、いずれ治ると思うので、もう少しここで休もうと思います。"},{"id":{"d":7,"n":5,"t":2},"speaker":"A","text":"そうですか。もし痛みが治まらない場合は、無理せず保健室に行った方がいいと思います。何かあれば声を掛けてくださいね。"},{"id":{"d":7,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":8,"n":1,"t":2},"speaker":"A","text":"阿久津さんおはようございます。この学校には、慣れましたか？"},{"id":{"d":8,"n":2,"t":2},"speaker":"B","text":"おはようございます。はい、だいぶ慣れてきました。"},{"id":{"d":8,"n":3,"t":2},"speaker":"A","text":"それは何よりです。阿久津さんはどのようなスポーツがお好きなのですか？"},{"id":{"d":8,"n":4,"t":2},"speaker":"B","text":"ソフトテニスです。前にいた学校では、テニス部に所属していました。"},{"id":{"d":8,"n":5,"t":2},"speaker":"A","text":"そうなのですね。実は私は、ソフトテニス部なのです。よろしければ今日見学に来ませんか？"},{"id":{"d":8,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。ぜひ行ってみたいです。"},{"id":{"d":8,"n":7,"t":2},"speaker":"A","text":"では授業が終わったら、一緒にテニスコートへ行きましょう。"},{"id":{"d":8,"n":8,"t":2},"speaker":"B","text":"はい、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":9,"n":1,"t":2},"speaker":"A","text":"小林さん、ここがテニスコートです。"},{"id":{"d":9,"n":2,"t":2},"speaker":"B","text":"案内してくださり、ありがとうございます。広いですね。"},{"id":{"d":9,"n":3,"t":2},"speaker":"A","text":"テニスコートの他に野球部が使うグラウンドや、サッカーグラウンドもありますよ。"},{"id":{"d":9,"n":4,"t":2},"speaker":"B","text":"この学校には、スポーツをしている人が多いのですか？"},{"id":{"d":9,"n":5,"t":2},"speaker":"A","text":"そうですね。ただ文化部で活動している人もたくさんいますよ。"},{"id":{"d":9,"n":6,"t":2},"speaker":"B","text":"文化部にはどのようなものがありますか？"},{"id":{"d":9,"n":7,"t":2},"speaker":"A","text":"華道部や、書道部、美術部や、手芸部などがあります。"},{"id":{"d":9,"n":8,"t":2},"speaker":"B","text":"それはすてきですね。明日は、美術部に見学へ行ってみようと思います。教えてくださりありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":10,"n":1,"t":2},"speaker":"A","text":"吉田さん、何をしているのですか？"},{"id":{"d":10,"n":2,"t":2},"speaker":"B","text":"ザリガニを見ています。"},{"id":{"d":10,"n":3,"t":2},"speaker":"A","text":"お、このザリガニにはおなかにたくさん卵が付いていますね。"},{"id":{"d":10,"n":4,"t":2},"speaker":"B","text":"これが卵なのですか？"},{"id":{"d":10,"n":5,"t":2},"speaker":"A","text":"そうですよ。ザリガニは卵を産むとおなかに卵を付けて育てるのです。"},{"id":{"d":10,"n":6,"t":2},"speaker":"B","text":"知りませんでした。生まれてくるのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":11,"n":1,"t":2},"speaker":"A","text":"佐藤さんは数学が得意ですね。"},{"id":{"d":11,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。川田さんは英語が得意ですよね。"},{"id":{"d":11,"n":3,"t":2},"speaker":"A","text":"英語だけは昔から得意なのです。"},{"id":{"d":11,"n":4,"t":2},"speaker":"B","text":"すごいです。今度教えてください。"},{"id":{"d":11,"n":5,"t":2},"speaker":"A","text":"分かりました。では私には、数学を教えてくださいね。"},{"id":{"d":11,"n":6,"t":2},"speaker":"B","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":12,"n":1,"t":2},"speaker":"A","text":"私たちの班は、この町の歴史について調べることにしましょう。"},{"id":{"d":12,"n":2,"t":2},"speaker":"B","text":"そうしましょう。何から始めましょうか？"},{"id":{"d":12,"n":3,"t":2},"speaker":"A","text":"班のみんなで、それぞれが知りたいことを書き出していくのはどうでしょうか？"},{"id":{"d":12,"n":4,"t":2},"speaker":"B","text":"それは良い案ですね。その後、何から調べていくか相談して決めましょう。"}];
INSERT INTO sentence [{"id":{"d":13,"n":1,"t":2},"speaker":"A","text":"こんにちは、加藤さん。今日は町の歴史についてインタビューさせていただきます。よろしくお願いします。"},{"id":{"d":13,"n":2,"t":2},"speaker":"B","text":"どうぞ、よろしくお願いします。"},{"id":{"d":13,"n":3,"t":2},"speaker":"A","text":"加藤さんは、いつからこの町に住んでいるのですか？"},{"id":{"d":13,"n":4,"t":2},"speaker":"B","text":"生まれた時からずっと住んでいます。"},{"id":{"d":13,"n":5,"t":2},"speaker":"A","text":"それは長いですね。加藤さんのご年齢をお聞きしてもよいでしょうか？"},{"id":{"d":13,"n":6,"t":2},"speaker":"B","text":"85歳です。"},{"id":{"d":13,"n":7,"t":2},"speaker":"A","text":"では、85年もこの町に住んでいるのですね。"},{"id":{"d":13,"n":8,"t":2},"speaker":"B","text":"そうです。ですので、この町のことは何でも聞いてくださいね。"}];
INSERT INTO sentence [{"id":{"d":14,"n":1,"t":2},"speaker":"A","text":"新入生の川田さんですよね？初めまして、木田です。お姉さんの一恵先輩には部活でお世話になりました。"},{"id":{"d":14,"n":2,"t":2},"speaker":"B","text":"初めまして、一恵の妹の有子です。"},{"id":{"d":14,"n":3,"t":2},"speaker":"A","text":"有子さんは、何の部活動に入るかもう決めましたか？"},{"id":{"d":14,"n":4,"t":2},"speaker":"B","text":"いえ、まだ決めていません。"},{"id":{"d":14,"n":5,"t":2},"speaker":"A","text":"もし、よろしければ映画研究会はいかがですか？"},{"id":{"d":14,"n":6,"t":2},"speaker":"B","text":"興味はあります。姉がとても楽しそうだったので。"},{"id":{"d":14,"n":7,"t":2},"speaker":"A","text":"ではぜひ、一度見学にいらしてください。今日の放課後のご予定はいかがですか？"},{"id":{"d":14,"n":8,"t":2},"speaker":"B","text":"今日は予定があるので明日見学させてもらってもよいですか？"},{"id":{"d":14,"n":9,"t":2},"speaker":"A","text":"もちろんです。では明日の放課後、部室でお待ちしていますね。"}];
INSERT INTO sentence [{"id":{"d":15,"n":1,"t":2},"speaker":"A","text":"林さん、おはようございます。"},{"id":{"d":15,"n":2,"t":2},"speaker":"B","text":"おはようございます。"},{"id":{"d":15,"n":3,"t":2},"speaker":"A","text":"今日は、遅刻していましたね。"},{"id":{"d":15,"n":4,"t":2},"speaker":"B","text":"遅くまでテレビを見ていたので、寝坊して遅刻してしまいました。"},{"id":{"d":15,"n":5,"t":2},"speaker":"A","text":"そうでしたか。明日は遅刻しないように気を付けてくださいね。"},{"id":{"d":15,"n":6,"t":2},"speaker":"B","text":"はい、気を付けます。今日は早く寝ることにします。"}];
INSERT INTO sentence [{"id":{"d":16,"n":1,"t":2},"speaker":"A","text":"鈴木先生、定型文について質問です。"},{"id":{"d":16,"n":2,"t":2},"speaker":"B","text":"はい、小林さん。何でしょうか？"},{"id":{"d":16,"n":3,"t":2},"speaker":"A","text":"英語の定型文がなかなか思えられなくて困っています。先生はどのようにして定型文を覚えていますか？"},{"id":{"d":16,"n":4,"t":2},"speaker":"B","text":"良い質問ですね。先生は英語の本を読んで覚えています。"},{"id":{"d":16,"n":5,"t":2},"speaker":"A","text":"なるほど、本ですか。私も読んでみようと思いますが、お薦めがあったら教えてください。"},{"id":{"d":16,"n":6,"t":2},"speaker":"B","text":"分かりました。職員室に私のお薦めの本があるので放課後取りに来てください。"},{"id":{"d":16,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":17,"n":1,"t":2},"speaker":"A","text":"中村さん、部活は何をしていますか？"},{"id":{"d":17,"n":2,"t":2},"speaker":"B","text":"空手部です。"},{"id":{"d":17,"n":3,"t":2},"speaker":"A","text":"そうでしたか。段はお持ちですか？"},{"id":{"d":17,"n":4,"t":2},"speaker":"B","text":"ちょうど先日、3段を取りました。"},{"id":{"d":17,"n":5,"t":2},"speaker":"A","text":"それはすごいです。中村さんは強いのですね。"},{"id":{"d":17,"n":6,"t":2},"speaker":"B","text":"まだまだですよ。もっと強くなれるように毎日練習しています。"},{"id":{"d":17,"n":7,"t":2},"speaker":"A","text":"素晴らしいですね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":18,"n":1,"t":2},"speaker":"A","text":"今日はとても良いお天気です。"},{"id":{"d":18,"n":2,"t":2},"speaker":"B","text":"そうですね。このように良い天気の日は、屋上でのんびりと昼食を食べるのが良いかも知れませんね。"},{"id":{"d":18,"n":3,"t":2},"speaker":"A","text":"良いアイデアですね。今から一緒にどうですか？"},{"id":{"d":18,"n":4,"t":2},"speaker":"B","text":"はい、そうしましょう。お弁当を買ってくるので5分後にエレベーター前で待ち合わせましょう。"},{"id":{"d":18,"n":5,"t":2},"speaker":"A","text":"分かりました。私もお弁当を持ってきます。"},{"id":{"d":18,"n":6,"t":2},"speaker":"B","text":"では、また5分後にお会いしましょう。"},{"id":{"d":18,"n":7,"t":2},"speaker":"A","text":"はい、また後で。"}];
INSERT INTO sentence [{"id":{"d":19,"n":1,"t":2},"speaker":"A","text":"中林さん、阿久津先生があなたを探していましたよ。"},{"id":{"d":19,"n":2,"t":2},"speaker":"B","text":"あら、何でしょう。特に心当たりはないのですが。"},{"id":{"d":19,"n":3,"t":2},"speaker":"A","text":"「至急職員室へ来るように」とのことです。"},{"id":{"d":19,"n":4,"t":2},"speaker":"B","text":"分かりました、すぐに行きます。"}];
INSERT INTO sentence [{"id":{"d":20,"n":1,"t":2},"speaker":"A","text":"今日は早めに帰ってきてくださいね。"},{"id":{"d":20,"n":2,"t":2},"speaker":"B","text":"分かりましたが、今日は何かあるのですか？"},{"id":{"d":20,"n":3,"t":2},"speaker":"A","text":"今日はお父さんのお誕生日なので、夕食を豪華にする予定です。"},{"id":{"d":20,"n":4,"t":2},"speaker":"B","text":"忘れていました。そうでしたね。早めに帰って来るようにします。"},{"id":{"d":20,"n":5,"t":2},"speaker":"A","text":"帰って来たら、買い物をお願いしたいです。"},{"id":{"d":20,"n":6,"t":2},"speaker":"B","text":"分かりました。今日は午後に体育の授業があるので、疲れすぎないようにしたいと思います。では、学校へ行ってきます。"},{"id":{"d":20,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。気を付けていってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":21,"n":1,"t":2},"speaker":"A","text":"今日の帰りに本屋へ寄って行ってもよいですか？"},{"id":{"d":21,"n":2,"t":2},"speaker":"B","text":"よいですよ。何か欲しい本がありましたか？"},{"id":{"d":21,"n":3,"t":2},"speaker":"A","text":"今日は、私の好きな作家さんの新刊を発売する日です。"},{"id":{"d":21,"n":4,"t":2},"speaker":"B","text":"それはとても楽しみですね。短編ですか？続き物ですか？"},{"id":{"d":21,"n":5,"t":2},"speaker":"A","text":"短編です。この作家さんの作品は本当に面白いので、あなたにもぜひ読んでほしいです。"},{"id":{"d":21,"n":6,"t":2},"speaker":"B","text":"そこまで言うのなら私も読んでみようかしら。どの作品から読むべきですか？"},{"id":{"d":21,"n":7,"t":2},"speaker":"A","text":"明日お薦めの1冊を持ってきますので、まずはそれを読んでみてください。"},{"id":{"d":21,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":22,"n":1,"t":2},"speaker":"A","text":"おはようございます。"},{"id":{"d":22,"n":2,"t":2},"speaker":"B","text":"おはようございます。とても良い表情をしていますね。何か良いことがありましたか？"},{"id":{"d":22,"n":3,"t":2},"speaker":"A","text":"昨日は、好きなアニメーションを見てとても良い気分になりました。"},{"id":{"d":22,"n":4,"t":2},"speaker":"B","text":"それは良かったですね。ところで今日は小テストがありますが、その勉強はしましたか？"},{"id":{"d":22,"n":5,"t":2},"speaker":"A","text":"え？今日でしたっけ。アニメーションに気を取られていて、全くしていませんでした。どうしましょう。"},{"id":{"d":22,"n":6,"t":2},"speaker":"B","text":"急いで学校に行って勉強をしましょう。私も手伝いますよ。"}];
INSERT INTO sentence [{"id":{"d":23,"n":1,"t":2},"speaker":"A","text":"加藤先生、大島君の風邪の具合はいかがですか？"},{"id":{"d":23,"n":2,"t":2},"speaker":"B","text":"お母さんからの連絡ではもうだいぶ良くなったそうですよ。"},{"id":{"d":23,"n":3,"t":2},"speaker":"A","text":"それは良かったです。もうすぐテストなので心配していました。"},{"id":{"d":23,"n":4,"t":2},"speaker":"B","text":"そうですね。来週には学校に来られるそうですよ。"}];
INSERT INTO sentence [{"id":{"d":24,"n":1,"t":2},"speaker":"A","text":"馬術は楽しいです。"},{"id":{"d":24,"n":2,"t":2},"speaker":"B","text":"何がそれほど楽しいのですか？"},{"id":{"d":24,"n":3,"t":2},"speaker":"A","text":"馬と一体になって風を切るのがとても気持ち良いのです。"},{"id":{"d":24,"n":4,"t":2},"speaker":"B","text":"それは気持ちよさそうです。そういえば馬頭さんは馬術部のエースでしたね。"},{"id":{"d":24,"n":5,"t":2},"speaker":"A","text":"エースの名に恥じないよう、これからも頑張ります。"},{"id":{"d":24,"n":6,"t":2},"speaker":"B","text":"毎日お疲れさまです。これからも頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":25,"n":1,"t":2},"speaker":"A","text":"昨日、初めて革細工に挑戦しました。想像していた以上に簡単で驚きました。"},{"id":{"d":25,"n":2,"t":2},"speaker":"B","text":"それほどに簡単だったのですか？"},{"id":{"d":25,"n":3,"t":2},"speaker":"A","text":"はい、私の想像以上に難しい工程が少なく、あっという間に良い物ができたと思います。"},{"id":{"d":25,"n":4,"t":2},"speaker":"B","text":"「案ずるより産むが易し」とはこのことですね。"},{"id":{"d":25,"n":5,"t":2},"speaker":"A","text":"ことわざですか？どのような意味ですか？"},{"id":{"d":25,"n":6,"t":2},"speaker":"B","text":"「あれこれ考えるよりも、実際に行動に移してみると考えていたより簡単だった」という意味です。"},{"id":{"d":25,"n":7,"t":2},"speaker":"A","text":"なるほど、確かにそうですね。この調子で革細工の物をたくさん作りたいと思います。"}];
INSERT INTO sentence [{"id":{"d":26,"n":1,"t":2},"speaker":"A","text":"中林先輩。相談したいことがあるのですが、今日この後にお時間はありますか？"},{"id":{"d":26,"n":2,"t":2},"speaker":"B","text":"はい、放課後なら空いていますよ。部活のことですか？"},{"id":{"d":26,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。部活と勉強の、両立がうまくいかず悩んでいます。"},{"id":{"d":26,"n":4,"t":2},"speaker":"B","text":"それは重要なことですから、悩みますよね。私がお力になれると良いのですが。"},{"id":{"d":26,"n":5,"t":2},"speaker":"A","text":"勉強もできて、部活でも大活躍している先輩にいろいろ教えていただきたいのです。それでは放課後に、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":27,"n":1,"t":2},"speaker":"A","text":"上田さん、カボチャは好きですか？"},{"id":{"d":27,"n":2,"t":2},"speaker":"B","text":"嫌いではありませんよ。なぜですか？"},{"id":{"d":27,"n":3,"t":2},"speaker":"A","text":"今度のハロウィーンパーティーでパンプキンパイを作ろうと思っているので、嫌いな人がいないか確認していました。"},{"id":{"d":27,"n":4,"t":2},"speaker":"B","text":"とてもおいしそうですね。私も一緒に作りたいです。"},{"id":{"d":27,"n":5,"t":2},"speaker":"A","text":"もちろんです。一緒に作りましょう。"},{"id":{"d":27,"n":6,"t":2},"speaker":"B","text":"そういえば、私の弟も「ハロウィーンパーティーに参加したい」と言っていました。連れていっても良いでしょうか？"},{"id":{"d":27,"n":7,"t":2},"speaker":"A","text":"問題ありません。私の姉も「友達を呼ぶ」と言っていましたし、みんなで楽しみましょう。"},{"id":{"d":27,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。弟もきっと喜ぶと思います。"}];
INSERT INTO sentence [{"id":{"d":28,"n":1,"t":2},"speaker":"A","text":"加藤さん、今少しお時間ありますか？"},{"id":{"d":28,"n":2,"t":2},"speaker":"B","text":"5分くらいなら大丈夫ですよ。どうかなさいましたか？"},{"id":{"d":28,"n":3,"t":2},"speaker":"A","text":"先週の文化祭についての話し合いでは一部の係の振り分けがまだ決まっていませんでしたよね。残っている分を今決めませんか？"},{"id":{"d":28,"n":4,"t":2},"speaker":"B","text":"おお、そうでした。決めなければならない係は司会と受付でしたね。"},{"id":{"d":28,"n":5,"t":2},"speaker":"A","text":"はい。司会と受付はそれぞれ2人ずつ必要とのことです。"},{"id":{"d":28,"n":6,"t":2},"speaker":"B","text":"なるほど。では、まだどの係にも振り分けられていない、この6人の中から決めましょう。"}];
INSERT INTO sentence [{"id":{"d":29,"n":1,"t":2},"speaker":"A","text":"城田さん、もうすぐクリスマスですね。"},{"id":{"d":29,"n":2,"t":2},"speaker":"B","text":"はい、楽しみです。今年は自宅でパーティーを開く予定なのですが、木崎さんもいらっしゃいませんか？"},{"id":{"d":29,"n":3,"t":2},"speaker":"A","text":"どうもありがとうございます。ぜひ参加したいです。"},{"id":{"d":29,"n":4,"t":2},"speaker":"B","text":"ではこちらの招待状をお渡ししますので、当日お持ちください。"},{"id":{"d":29,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。ちなみに、会費はおいくらですか？"},{"id":{"d":29,"n":6,"t":2},"speaker":"B","text":"無料ですよ。"},{"id":{"d":29,"n":7,"t":2},"speaker":"A","text":"分かりました。当日を楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":30,"n":1,"t":2},"speaker":"A","text":"姉さん、明日の朝ごはんを作る当番を変わってほしいです。"},{"id":{"d":30,"n":2,"t":2},"speaker":"B","text":"よいですよ。でも、なぜですか？"},{"id":{"d":30,"n":3,"t":2},"speaker":"A","text":"明日、急きょ部活の朝練が入ってしまいました。7時には家を出なくてはいけないため、朝食を作る時間がなさそうです。"},{"id":{"d":30,"n":4,"t":2},"speaker":"B","text":"分かりました。では明日は私が担当します。"},{"id":{"d":30,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。その代わり、今日と明日の夕食は僕が担当しますね。"},{"id":{"d":30,"n":6,"t":2},"speaker":"B","text":"うれしいです。では、よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":31,"n":1,"t":2},"speaker":"A","text":"土曜日の交流試合の会場は、西中学校になりました。"},{"id":{"d":31,"n":2,"t":2},"speaker":"B","text":"分かりました。会場まではどのように行きますか？"},{"id":{"d":31,"n":3,"t":2},"speaker":"A","text":"東京駅に集合し、みんなで会場まで歩いて移動しましょう。"},{"id":{"d":31,"n":4,"t":2},"speaker":"B","text":"集合時間はどうしますか？"},{"id":{"d":31,"n":5,"t":2},"speaker":"A","text":"東京駅に朝7時にします。遅れないようにしてください。"},{"id":{"d":31,"n":6,"t":2},"speaker":"B","text":"分かりました。試合相手が強豪校なので勝てるか心配です。"},{"id":{"d":31,"n":7,"t":2},"speaker":"A","text":"勝つことも大事ですが、まずは全力で良いプレーをする事を意識してくださいね。"}];
INSERT INTO sentence [{"id":{"d":32,"n":1,"t":2},"speaker":"A","text":"これを見てください。"},{"id":{"d":32,"n":2,"t":2},"speaker":"B","text":"スマートフォンですね。どうしたのですか？"},{"id":{"d":32,"n":3,"t":2},"speaker":"A","text":"昨日両親に買ってもらいました。今日からスマートフォンデビューです。"},{"id":{"d":32,"n":4,"t":2},"speaker":"B","text":"おめでとうございます。使い心地はいかがですか？"},{"id":{"d":32,"n":5,"t":2},"speaker":"A","text":"フィーチャーフォンと使い方がまるで違うので、なかなか思うように使えません。"},{"id":{"d":32,"n":6,"t":2},"speaker":"B","text":"使っているうちに慣れますよ。スマートフォンの練習も兼ねて、私とメールアドレスを交換しましょう。"},{"id":{"d":32,"n":7,"t":2},"speaker":"A","text":"はい、お願いします。友達とメールアドレス交換をしたのはこれが初めてです。"},{"id":{"d":32,"n":8,"t":2},"speaker":"B","text":"他の友達とも、メールアドレス交換ができるといいですね。"}];
INSERT INTO sentence [{"id":{"d":33,"n":1,"t":2},"speaker":"A","text":"加藤さんはどの部活に入るか決めましたか？"},{"id":{"d":33,"n":2,"t":2},"speaker":"B","text":"まだ決まっていません。"},{"id":{"d":33,"n":3,"t":2},"speaker":"A","text":"それでは一緒に部活動の見学に行きませんか？"},{"id":{"d":33,"n":4,"t":2},"speaker":"B","text":"それは良いアイデアですね。どこの部活から見学に行きますか？"},{"id":{"d":33,"n":5,"t":2},"speaker":"A","text":"私は文化部に入りたいと思っているので、美術部や、吹奏楽部から見学したいです。"},{"id":{"d":33,"n":6,"t":2},"speaker":"B","text":"私も吹奏楽部に興味があります。では、まずは音楽室へ行きましょう。"},{"id":{"d":33,"n":7,"t":2},"speaker":"A","text":"そうしましょう。優しい先輩がたくさんいると良いですね。"}];
INSERT INTO sentence [{"id":{"d":34,"n":1,"t":2},"speaker":"A","text":"すみません、今の時刻を教えてください。"},{"id":{"d":34,"n":2,"t":2},"speaker":"B","text":"今は16時22分です。"},{"id":{"d":34,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。時計を忘れてしまい、困っていたので助かりました。"},{"id":{"d":34,"n":4,"t":2},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":35,"n":1,"t":2},"speaker":"A","text":"佐藤さん、今よいでしょうか？手伝ってほしいことがあります。"},{"id":{"d":35,"n":2,"t":2},"speaker":"B","text":"田中先生、こんにちは。はい、大丈夫です。何をすればよいですか？"},{"id":{"d":35,"n":3,"t":2},"speaker":"A","text":"次の授業で配るプリントを化学室まで運びたいのですが、量が多くて一人では運べません。運ぶのを手伝ってもらえないでしょうか？"},{"id":{"d":35,"n":4,"t":2},"speaker":"B","text":"お安い御用です。これを運べばよいのですね。"},{"id":{"d":35,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。助かりました。"}];
INSERT INTO sentence [{"id":{"d":36,"n":1,"t":2},"speaker":"A","text":"掃除の時間です。加藤さんは玄関の掃き掃除をお願いします。"},{"id":{"d":36,"n":2,"t":2},"speaker":"B","text":"はい。すみません、ほうきのある場所を教えてください。"},{"id":{"d":36,"n":3,"t":2},"speaker":"A","text":"昇降口を出て左の倉庫にあります。倉庫には鍵がかかっているので、職員室で鍵を預かってください。"},{"id":{"d":36,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。鍵を返却するのは掃除が終わってからでも大丈夫ですか？"},{"id":{"d":36,"n":5,"t":2},"speaker":"A","text":"はい、それで大丈夫です。"},{"id":{"d":36,"n":6,"t":2},"speaker":"B","text":"分かりました。では職員室へ行ってきます。"}];
INSERT INTO sentence [{"id":{"d":37,"n":1,"t":2},"speaker":"A","text":"先生、加藤さんは具合が悪くて保健室で休むそうです。"},{"id":{"d":37,"n":2,"t":2},"speaker":"B","text":"連絡をありがとうございます。最近寒くなってきたので、体調を崩してしまったのでしょうか。木村さんも体調には気を付けてくださいね。"},{"id":{"d":37,"n":3,"t":2},"speaker":"A","text":"はい、体調管理には気を付けたいと思います。"},{"id":{"d":37,"n":4,"t":2},"speaker":"B","text":"そうしてください。では、私は加藤さんの様子を見に保健室へ寄ってから教室へ行きますね。"},{"id":{"d":37,"n":5,"t":2},"speaker":"A","text":"分かりました。私は先に教室に戻っています。"}];
INSERT INTO sentence [{"id":{"d":38,"n":1,"t":2},"speaker":"A","text":"菊池さん、今週末は何か予定はありますか？"},{"id":{"d":38,"n":2,"t":2},"speaker":"B","text":"特にありません。どうしましたか？"},{"id":{"d":38,"n":3,"t":2},"speaker":"A","text":"良かったら、一緒に映画に行きませんか？"},{"id":{"d":38,"n":4,"t":2},"speaker":"B","text":"どのような映画ですか？"},{"id":{"d":38,"n":5,"t":2},"speaker":"A","text":"ラブコメディーにするのはどうでしょうか？"},{"id":{"d":38,"n":6,"t":2},"speaker":"B","text":"コメディーは大好きです。ぜひご一緒させてください。"},{"id":{"d":38,"n":7,"t":2},"speaker":"A","text":"良かったです。では10時に駅前で待っていますね。"},{"id":{"d":38,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":39,"n":1,"t":2},"speaker":"A","text":"今日から新学期ですね。緊張していますか？"},{"id":{"d":39,"n":2,"t":2},"speaker":"B","text":"少し緊張しています。新しいクラスにも知り合いがいると良いのですが。"},{"id":{"d":39,"n":3,"t":2},"speaker":"A","text":"そうですね。もし知り合いがいなくても、気の合う人はきっといるはずですよ。いろいろな人と交流してみるのも楽しそうです。"},{"id":{"d":39,"n":4,"t":2},"speaker":"B","text":"そうですね。せっかく新しい環境になったのだから、新しい友達も作ってみようと思います。では、そろそろ行ってきます。"},{"id":{"d":39,"n":5,"t":2},"speaker":"A","text":"はい、行ってらっしゃい。お気を付けて。"}];
INSERT INTO sentence [{"id":{"d":40,"n":1,"t":2},"speaker":"A","text":"中村さん、昨日の数学の宿題は終わりましたか？"},{"id":{"d":40,"n":2,"t":2},"speaker":"B","text":"はい、終わりました。次の授業で提出ですよね？"},{"id":{"d":40,"n":3,"t":2},"speaker":"A","text":"そうです。実は私、分からないところがあって、まだ終わっていないのです。解き方を教えてくれませんか？"},{"id":{"d":40,"n":4,"t":2},"speaker":"B","text":"よいですよ。どの問題でつまずいたのですか？"},{"id":{"d":40,"n":5,"t":2},"speaker":"A","text":"最後の問題です。どの公式を使えば良いのか分からないのです。"},{"id":{"d":40,"n":6,"t":2},"speaker":"B","text":"最後の問題は、僕も悩みました。でもこれは昨日習った公式を当てはめれば簡単に解けますよ。"},{"id":{"d":40,"n":7,"t":2},"speaker":"A","text":"そうなのですね。ありがとうございます。これから解いてみます。"},{"id":{"d":40,"n":8,"t":2},"speaker":"B","text":"頑張ってください。分からなくなったら、また聞いてください。"}];
INSERT INTO sentence [{"id":{"d":41,"n":1,"t":2},"speaker":"A","text":"中林さんも、今日はお弁当を持参されていますか？"},{"id":{"d":41,"n":2,"t":2},"speaker":"B","text":"はい、今日のおかずは大好物のハンバーグと唐揚げです。"},{"id":{"d":41,"n":3,"t":2},"speaker":"A","text":"とてもおいしそうです。私の卵焼きと交換しませんか？"},{"id":{"d":41,"n":4,"t":2},"speaker":"B","text":"良いですよ。あなたの卵焼きもおいしそうです。唐揚げを交換しましょう。"},{"id":{"d":41,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":42,"n":1,"t":2},"speaker":"A","text":"今日の放課後、買い物に付き合ってくれませんか？"},{"id":{"d":42,"n":2,"t":2},"speaker":"B","text":"ごめんなさい、今日は用事があります。明日なら空いています。"},{"id":{"d":42,"n":3,"t":2},"speaker":"A","text":"では、明日の放課後にお願いします。"},{"id":{"d":42,"n":4,"t":2},"speaker":"B","text":"分かりました。どこに行きますか？"},{"id":{"d":42,"n":5,"t":2},"speaker":"A","text":"駅前の靴屋に行きたいです。新しいローファーが欲しいです。"}];
INSERT INTO sentence [{"id":{"d":43,"n":1,"t":2},"speaker":"A","text":"兄弟はいますか？"},{"id":{"d":43,"n":2,"t":2},"speaker":"B","text":"はい、弟が2人います。"},{"id":{"d":43,"n":3,"t":2},"speaker":"A","text":"3人兄弟ですか。楽しそうです。僕は一人っ子なので兄弟に憧れがあります。"},{"id":{"d":43,"n":4,"t":2},"speaker":"B","text":"そうなのですね。一緒にゲームをしたり買い物に行ったりするのは、確かに楽しいですが、けんかも度々ですし、たまに煩わしく感じるときもありますよ。"},{"id":{"d":43,"n":5,"t":2},"speaker":"A","text":"そのようなものなのですか。弟さんとは年は離れているのですか？"},{"id":{"d":43,"n":6,"t":2},"speaker":"B","text":"すぐ下の弟とは3歳、一番下の弟とは5歳離れています。"}];
INSERT INTO sentence [{"id":{"d":44,"n":1,"t":2},"speaker":"A","text":"突然ですが、明日の給食は出せません。各家庭でお弁当を用意してもらうよう保護者の方にお伝えください。"},{"id":{"d":44,"n":2,"t":2},"speaker":"B","text":"教頭先生、突然どうしたのですか？本当なら明日は給食の予定でしたよね？"},{"id":{"d":44,"n":3,"t":2},"speaker":"A","text":"今日の昼頃から給食室の水道の調子が悪く、明日の午前中に急きょ業者の方に見てもらうことになりました。明日の昼まで給食室が使えないので、給食の準備ができません。"},{"id":{"d":44,"n":4,"t":2},"speaker":"B","text":"生徒たちは明日のカレーライスを楽しみにしていましたが、そういうことでしたら仕方がありませんね。保護者の方へも急ぎ連絡します。"}];
INSERT INTO sentence [{"id":{"d":45,"n":1,"t":2},"speaker":"A","text":"林さんは、野球部でしたか？"},{"id":{"d":45,"n":2,"t":2},"speaker":"B","text":"そうです。まだ補欠ですが、ピッチャーをしています。"},{"id":{"d":45,"n":3,"t":2},"speaker":"A","text":"明日の試合には出るのですか？"},{"id":{"d":45,"n":4,"t":2},"speaker":"B","text":"はい、2番手で出場する予定です。明日は練習試合なので、控えの選手もたくさん試合に出られます。"},{"id":{"d":45,"n":5,"t":2},"speaker":"A","text":"それは楽しみです。明日は応援に行くので、ぜひ頑張ってくださいね。"},{"id":{"d":45,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。明日の試合で活躍して、早くレギュラーになれるように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":46,"n":1,"t":2},"speaker":"A","text":"今度の文化祭が、楽しみですね。"},{"id":{"d":46,"n":2,"t":2},"speaker":"B","text":"そうですね。特に吹奏楽部の演奏が楽しみです。"},{"id":{"d":46,"n":3,"t":2},"speaker":"A","text":"この学校の吹奏楽部は強豪らしいですね。この前の県大会でも金賞を取ったらしいですよ。"},{"id":{"d":46,"n":4,"t":2},"speaker":"B","text":"そのようですね。演奏を聞いたことは、まだないので文化祭が本当に楽しみです。"},{"id":{"d":46,"n":5,"t":2},"speaker":"A","text":"文化祭では何の曲を演奏するのでしょうか？"},{"id":{"d":46,"n":6,"t":2},"speaker":"B","text":"演奏する曲目は、公表されていませんね。当日のお楽しみのようです。"}];
INSERT INTO sentence [{"id":{"d":47,"n":1,"t":2},"speaker":"A","text":"おはようございます。もうすっかり冬ですね。"},{"id":{"d":47,"n":2,"t":2},"speaker":"B","text":"おはようございます。でも、今年は例年より暖かい気がします。"},{"id":{"d":47,"n":3,"t":2},"speaker":"A","text":"そうかもしれません。今年は暖冬でしょうか？"},{"id":{"d":47,"n":4,"t":2},"speaker":"B","text":"そうだと良いですね。私は寒いのが苦手なので暖冬ならうれしいです。"}];
INSERT INTO sentence [{"id":{"d":48,"n":1,"t":2},"speaker":"A","text":"あなたの趣味は、何ですか？"},{"id":{"d":48,"n":2,"t":2},"speaker":"B","text":"スマートフォンゲームに夢中になっています。"},{"id":{"d":48,"n":3,"t":2},"speaker":"A","text":"どのようなゲームがお好きなのですか？"},{"id":{"d":48,"n":4,"t":2},"speaker":"B","text":"今はロボットを操作して、戦うゲームをよくしています。"},{"id":{"d":48,"n":5,"t":2},"speaker":"A","text":"そのゲームを知っています。私も以前していました。最近は同じ作者のパズルゲームに熱中しています。"},{"id":{"d":48,"n":6,"t":2},"speaker":"B","text":"面白いですか？今度ぜひ紹介してください。"},{"id":{"d":48,"n":7,"t":2},"speaker":"A","text":"とても面白いです。もちろん、いつでも紹介しますよ。"}];
INSERT INTO sentence [{"id":{"d":49,"n":1,"t":2},"speaker":"A","text":"部活は何に入るか決めましたか？"},{"id":{"d":49,"n":2,"t":2},"speaker":"B","text":"いいえ。まだ決めていません。今日の部活動紹介を見てから決めようと思います。近藤さんは決めたのですか？"},{"id":{"d":49,"n":3,"t":2},"speaker":"A","text":"実は、美術部に入ろうかと思っています。"},{"id":{"d":49,"n":4,"t":2},"speaker":"B","text":"ああ、絵がお上手ですものね。"},{"id":{"d":49,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。もっと絵がうまくなりたくて、部活で先生に指導してもらいたいのです。"},{"id":{"d":49,"n":6,"t":2},"speaker":"B","text":"とても良い考えですね。私も得意なことがあれば良かったのですが。"},{"id":{"d":49,"n":7,"t":2},"speaker":"A","text":"得意なことではなくても、佐々木さんにとって楽しい部活が見つかると良いですね。"},{"id":{"d":49,"n":8,"t":2},"speaker":"B","text":"そうですね。楽しく取り組める部活を探します。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":50,"n":1,"t":2},"speaker":"A","text":"もうすぐ、期末テストですね。"},{"id":{"d":50,"n":2,"t":2},"speaker":"B","text":"テストが終わったら、冬休みです。私は、それが楽しみです。"},{"id":{"d":50,"n":3,"t":2},"speaker":"A","text":"そうですね。冬休みの宿題がなければもっと楽しいのですが。"},{"id":{"d":50,"n":4,"t":2},"speaker":"B","text":"その通りですね。"}];
INSERT INTO sentence [{"id":{"d":51,"n":1,"t":2},"speaker":"A","text":"菊池先生は、今年で定年退職です。"},{"id":{"d":51,"n":2,"t":2},"speaker":"B","text":"もう定年なのですね。寂しいです。"},{"id":{"d":51,"n":3,"t":2},"speaker":"A","text":"はい。そこで、みんなで色紙に寄せ書きをしませんか？"},{"id":{"d":51,"n":4,"t":2},"speaker":"B","text":"それは良い案ですね。それでは菊池先生に分からないようクラスのみんなに伝えます。"},{"id":{"d":51,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。私は色紙を用意します。"},{"id":{"d":51,"n":6,"t":2},"speaker":"B","text":"人数が多いので、大きな色紙にするか複数枚用意した方がいいかもしれませんね。"},{"id":{"d":51,"n":7,"t":2},"speaker":"A","text":"そうですね。まずは大きな色紙を探してみます。なければ何枚か買ってきます。"},{"id":{"d":51,"n":8,"t":2},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":52,"n":1,"t":2},"speaker":"A","text":"中林さん、スマートフォンは持っていますか？"},{"id":{"d":52,"n":2,"t":2},"speaker":"B","text":"はい、もちろん持っていますよ。"},{"id":{"d":52,"n":3,"t":2},"speaker":"A","text":"LINEの連絡先を交換しませんか？"},{"id":{"d":52,"n":4,"t":2},"speaker":"B","text":"LINEはしていません。Twitterのアカウントならありますよ。"},{"id":{"d":52,"n":5,"t":2},"speaker":"A","text":"それは残念です。私はTwitterに登録をしていません。"},{"id":{"d":52,"n":6,"t":2},"speaker":"B","text":"そうでしたか。では、メールアドレスを交換しましょう。"}];
INSERT INTO sentence [{"id":{"d":53,"n":1,"t":2},"speaker":"A","text":"この前のテストの結果は、どうでしたか？"},{"id":{"d":53,"n":2,"t":2},"speaker":"B","text":"全然できませんでした。"},{"id":{"d":53,"n":3,"t":2},"speaker":"A","text":"私も同じくです。特に英語ができませんでした。"},{"id":{"d":53,"n":4,"t":2},"speaker":"B","text":"英語は、平均点がかなり低かったらしいですね。"},{"id":{"d":53,"n":5,"t":2},"speaker":"A","text":"先生が、復習プリントをたくさん用意しているみたいです。"},{"id":{"d":53,"n":6,"t":2},"speaker":"B","text":"記述文が多い問題は嫌です。"},{"id":{"d":53,"n":7,"t":2},"speaker":"A","text":"私も、文章を考えることが苦手です。"},{"id":{"d":53,"n":8,"t":2},"speaker":"B","text":"頑張るしかありませんね。"}];
INSERT INTO sentence [{"id":{"d":54,"n":1,"t":2},"speaker":"A","text":"次の体育の内容は持久走だそうです。"},{"id":{"d":54,"n":2,"t":2},"speaker":"B","text":"持久走ですか？苦手です。"},{"id":{"d":54,"n":3,"t":2},"speaker":"A","text":"私も走ることが苦手です。"},{"id":{"d":54,"n":4,"t":2},"speaker":"B","text":"山田さんは、吹奏楽部でしたよね？吹奏楽部は部活でいつも走っているので、得意かと思っていました。"},{"id":{"d":54,"n":5,"t":2},"speaker":"A","text":"体力を付けるために部活でランニングをしていますが、それも得意ではありません。"},{"id":{"d":54,"n":6,"t":2},"speaker":"B","text":"そうなのですね。今日の体育では、私たちは無理せず頑張りましょう。"},{"id":{"d":54,"n":7,"t":2},"speaker":"A","text":"はい。体力を使い過ぎて、午後に倒れないようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":55,"n":1,"t":2},"speaker":"A","text":"佐藤さんは、今年も無遅刻無欠席ですね。"},{"id":{"d":55,"n":2,"t":2},"speaker":"B","text":"はい。これで5年連続です。"},{"id":{"d":55,"n":3,"t":2},"speaker":"A","text":"すごいですね。"},{"id":{"d":55,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":55,"n":5,"t":2},"speaker":"A","text":"無遅刻無欠席だと、学校から表彰状がもらえるのですね。"},{"id":{"d":55,"n":6,"t":2},"speaker":"B","text":"そうです。家にもたくさんの表彰状がありますよ。"}];
INSERT INTO sentence [{"id":{"d":56,"n":1,"t":2},"speaker":"A","text":"林先生、運動会について確認したいことがあります。"},{"id":{"d":56,"n":2,"t":2},"speaker":"B","text":"はい、何でしょうか？"},{"id":{"d":56,"n":3,"t":2},"speaker":"A","text":"運動会当日が雨天の場合、開催を1週間後に延期するということで良かったでしょうか？"},{"id":{"d":56,"n":4,"t":2},"speaker":"B","text":"ええ、予定が組めたのは1週間後しかなかったので、そうしてください。"},{"id":{"d":56,"n":5,"t":2},"speaker":"A","text":"分かりました。当日、雨が降らないといいですね。"},{"id":{"d":56,"n":6,"t":2},"speaker":"B","text":"そうですね。生徒たちも楽しみにしているので晴れてほしいです。"}];
INSERT INTO sentence [{"id":{"d":57,"n":1,"t":2},"speaker":"A","text":"鈴木さん、受験校は決まりましたか？"},{"id":{"d":57,"n":2,"t":2},"speaker":"B","text":"はい、第一志望校は東京大学です。父も兄も東京大学を卒業しているので、私も目指すことにしました。"},{"id":{"d":57,"n":3,"t":2},"speaker":"A","text":"鈴木さんなら、きっと大丈夫です。応援しています。"},{"id":{"d":57,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。そうだと良いのですが。"}];
INSERT INTO sentence [{"id":{"d":58,"n":1,"t":2},"speaker":"A","text":"修学旅行は、東京に行くことになりました。"},{"id":{"d":58,"n":2,"t":2},"speaker":"B","text":"修学旅行ですか。懐かしいですね。"},{"id":{"d":58,"n":3,"t":2},"speaker":"A","text":"佐藤先輩は、修学旅行は、どこに行きましたか？"},{"id":{"d":58,"n":4,"t":2},"speaker":"B","text":"私は、京都に行きました。"},{"id":{"d":58,"n":5,"t":2},"speaker":"A","text":"京都は良いですね。清水寺には、一度行ってみたいと思っています。"},{"id":{"d":58,"n":6,"t":2},"speaker":"B","text":"清水寺といえば、今年は改修工事をしているそうですよ。清水寺を訪れるなら、工事が終わってからにするのが良さそうですね。"}];
INSERT INTO sentence [{"id":{"d":59,"n":1,"t":2},"speaker":"A","text":"あさってから修学旅行ですね。"},{"id":{"d":59,"n":2,"t":2},"speaker":"B","text":"はい、あさってが待ち遠しいです。佐藤さんたちの班は、自由行動でどこへ行く予定ですか？"},{"id":{"d":59,"n":3,"t":2},"speaker":"A","text":"私たちの班は通天閣と道頓堀へ行く予定です。時田さんの班はどこへ行きますか？"},{"id":{"d":59,"n":4,"t":2},"speaker":"B","text":"私たちは大阪城へ行きます。道頓堀というのは、あのグリコの大きな看板があるところですか？"},{"id":{"d":59,"n":5,"t":2},"speaker":"A","text":"はい、そうです。看板の前で、みんなで写真を撮ってこようと思います。"},{"id":{"d":59,"n":6,"t":2},"speaker":"B","text":"良いですね。撮った写真を後でぜひ見せてください。"}];
INSERT INTO sentence [{"id":{"d":60,"n":1,"t":2},"speaker":"A","text":"今朝は、とても寒いですね。"},{"id":{"d":60,"n":2,"t":2},"speaker":"B","text":"ええ。昨夜は少しですが、雪が降っていましたよ。"},{"id":{"d":60,"n":3,"t":2},"speaker":"A","text":"それは寒いはずです。まだ11月なのに雪が降るとは驚きました。"},{"id":{"d":60,"n":4,"t":2},"speaker":"B","text":"そうですね。今年の冬は、例年よりも寒くなるかもしれません。"}];
INSERT INTO sentence [{"id":{"d":61,"n":1,"t":2},"speaker":"A","text":"今度の野外活動では、登山をします。"},{"id":{"d":61,"n":2,"t":2},"speaker":"B","text":"先生、私は体力的に登りきれるか心配です。"},{"id":{"d":61,"n":3,"t":2},"speaker":"A","text":"大丈夫ですよ。頂上を目指すコースと、ハイキングコースの2つに分かれるので、自分に合った方のコースを選ぶことができます。"},{"id":{"d":61,"n":4,"t":2},"speaker":"B","text":"なるほど。では私はハイキングコースにしたいと思います。"},{"id":{"d":61,"n":5,"t":2},"speaker":"A","text":"当日は、防寒対策をしっかりして楽しみましょうね。"}];
INSERT INTO sentence [{"id":{"d":62,"n":1,"t":2},"speaker":"A","text":"加藤さん、今日は日直ですよ。黒板を掃除してください。"},{"id":{"d":62,"n":2,"t":2},"speaker":"B","text":"すみません、忘れていました。"},{"id":{"d":62,"n":3,"t":2},"speaker":"A","text":"早くしないと、もうすぐ朝礼が始まります。"},{"id":{"d":62,"n":4,"t":2},"speaker":"B","text":"急いで掃除します。"}];
INSERT INTO sentence [{"id":{"d":63,"n":1,"t":2},"speaker":"A","text":"中学校の運動着を譲ってもらえませんか？"},{"id":{"d":63,"n":2,"t":2},"speaker":"B","text":"良いですが、何に使うのですか？"},{"id":{"d":63,"n":3,"t":2},"speaker":"A","text":"近所の女の子に差し上げようと思います。今度、中学校に入学するのです。"},{"id":{"d":63,"n":4,"t":2},"speaker":"B","text":"そうなのですね。もう着ないので好きに使ってよいですよ。"},{"id":{"d":63,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。きっと喜びます。"}];
INSERT INTO sentence [{"id":{"d":64,"n":1,"t":2},"speaker":"A","text":"加藤さん、今度、制服をお借りしてもよいですか？"},{"id":{"d":64,"n":2,"t":2},"speaker":"B","text":"急にどうしたのですか？"},{"id":{"d":64,"n":3,"t":2},"speaker":"A","text":"今度の文化祭で劇をするのですが、私は学生役をすることになったため、制服が必要なのです。"},{"id":{"d":64,"n":4,"t":2},"speaker":"B","text":"なるほど、あなたの学校には制服がありませんものね。私の物で良ければ使ってください。"},{"id":{"d":64,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。助かります。"},{"id":{"d":64,"n":6,"t":2},"speaker":"B","text":"その劇を見たいのですが、文化祭に行ってもよいですか？"},{"id":{"d":64,"n":7,"t":2},"speaker":"A","text":"恥ずかしいですが、きっと素晴らしい劇になるのでぜひ見に来てください。"},{"id":{"d":64,"n":8,"t":2},"speaker":"B","text":"友達と一緒に見に行きます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":65,"n":1,"t":2},"speaker":"A","text":"失礼します。ブラウン先生はいらっしゃいますか？"},{"id":{"d":65,"n":2,"t":2},"speaker":"B","text":"はい。どうしましたか？"},{"id":{"d":65,"n":3,"t":2},"speaker":"A","text":"私は2年1組の田中といいます。テニス部に興味があって来ました。"},{"id":{"d":65,"n":4,"t":2},"speaker":"B","text":"テニス部の部員募集のチラシを見て来たのですね。テニスをしたことはありますか？"},{"id":{"d":65,"n":5,"t":2},"speaker":"A","text":"あります。前の中学校でテニス部に所属していました。部活を見学してもよいですか？"},{"id":{"d":65,"n":6,"t":2},"speaker":"B","text":"ぜひ見学に来てください。テニス部は放課後16時から活動しています。"},{"id":{"d":65,"n":7,"t":2},"speaker":"A","text":"分かりました。明日見学に行きます。"},{"id":{"d":65,"n":8,"t":2},"speaker":"B","text":"明日ですね。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":66,"n":1,"t":2},"speaker":"A","text":"太郎さんは、何の教科が好きですか？"},{"id":{"d":66,"n":2,"t":2},"speaker":"B","text":"私は社会が好きです。自分の国の歴史を知ることができます。"},{"id":{"d":66,"n":3,"t":2},"speaker":"A","text":"そうですか。私は体育が好きです。運動をすることは健康に良いです。"},{"id":{"d":66,"n":4,"t":2},"speaker":"B","text":"私は体育が苦手です。どうすれば運動ができるようになりますか？"},{"id":{"d":66,"n":5,"t":2},"speaker":"A","text":"毎朝、ランニングをすれば、体力が付きますよ。私は、毎朝家の近くを走っています。"},{"id":{"d":66,"n":6,"t":2},"speaker":"B","text":"そうですか。私も明日から走ります。"},{"id":{"d":66,"n":7,"t":2},"speaker":"A","text":"少しずつ、頑張ってみてください。"}];
INSERT INTO sentence [{"id":{"d":67,"n":1,"t":2},"speaker":"A","text":"修学旅行は北海道と沖縄のどちらが良いですか？"},{"id":{"d":67,"n":2,"t":2},"speaker":"B","text":"私は北海道の方がいいです。"},{"id":{"d":67,"n":3,"t":2},"speaker":"A","text":"どうしてですか？"},{"id":{"d":67,"n":4,"t":2},"speaker":"B","text":"カニが好きだからです。"},{"id":{"d":67,"n":5,"t":2},"speaker":"A","text":"北海道は、海産物がおいしいですよね。"},{"id":{"d":67,"n":6,"t":2},"speaker":"B","text":"そうですね。それと景色がきれいだからです。"},{"id":{"d":67,"n":7,"t":2},"speaker":"A","text":"それは沖縄も同じではありませんか？"},{"id":{"d":67,"n":8,"t":2},"speaker":"B","text":"函館の夜景を、一度見てみたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":68,"n":1,"t":2},"speaker":"A","text":"先生、具合が悪いので保健室に行ってもよいですか？"},{"id":{"d":68,"n":2,"t":2},"speaker":"B","text":"もちろんです。一人で行けますか？"},{"id":{"d":68,"n":3,"t":2},"speaker":"A","text":"少しふらつきがあるので、誰か付き添いをお願いします。"},{"id":{"d":68,"n":4,"t":2},"speaker":"B","text":"それは危ないですね。保健委員さん、田中さんを保健室に連れて行ってください。"},{"id":{"d":68,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":69,"n":1,"t":2},"speaker":"A","text":"先生、黒板の文字が見えないので、席を変えてください。"},{"id":{"d":69,"n":2,"t":2},"speaker":"B","text":"分かりました。では、前から3番目の空いている席を使ってください。"},{"id":{"d":69,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。今日は眼鏡を忘れてきてしまったので助かりました。"},{"id":{"d":69,"n":4,"t":2},"speaker":"B","text":"そうだったのですね。その席からは、黒板の文字が見えますか？"},{"id":{"d":69,"n":5,"t":2},"speaker":"A","text":"はい、大丈夫です。とてもよく見えます。"}];
INSERT INTO sentence [{"id":{"d":70,"n":1,"t":2},"speaker":"A","text":"今日、提出する数学の宿題は、終わりましたか？"},{"id":{"d":70,"n":2,"t":2},"speaker":"B","text":"もちろん、終わりました。今回は教科書を見ないで解く事ができました。"},{"id":{"d":70,"n":3,"t":2},"speaker":"A","text":"それは、素晴らしいですね。田中さんは塾に通っているのですか？"},{"id":{"d":70,"n":4,"t":2},"speaker":"B","text":"はい。月曜日と水曜日に塾に行っています。来月からは土曜日も行くことになりました。"},{"id":{"d":70,"n":5,"t":2},"speaker":"A","text":"週3日も塾で勉強とは偉いですね。"},{"id":{"d":70,"n":6,"t":2},"speaker":"B","text":"今勉強を頑張って、将来は医者になりたいと思っています。医者になって困っている人を助けてあげたいです。"},{"id":{"d":70,"n":7,"t":2},"speaker":"A","text":"将来の夢がかなうように、勉強を頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":71,"n":1,"t":2},"speaker":"A","text":"最近、寒くなってきましたね。"},{"id":{"d":71,"n":2,"t":2},"speaker":"B","text":"10月になって一気に寒くなりましたね。そろそろコートが必要となりそうです。"},{"id":{"d":71,"n":3,"t":2},"speaker":"A","text":"そうですね。昨年、父に買ってもらったお気に入りのコートを今年も出そうと思います。"},{"id":{"d":71,"n":4,"t":2},"speaker":"B","text":"私は今年、新しいコートを買おうと思います。フリーマーケットで買えば安く済むので、そこで気に入るものを探すつもりです。川口さんも2枚目のコートを探しに一緒に行きませんか？"},{"id":{"d":71,"n":5,"t":2},"speaker":"A","text":"それはいいですね。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":72,"n":1,"t":2},"speaker":"A","text":"足を引きずって、どうしたのですか？"},{"id":{"d":72,"n":2,"t":2},"speaker":"B","text":"先ほど、体育の授業で転んだ時に痛めました。"},{"id":{"d":72,"n":3,"t":2},"speaker":"A","text":"保健室で手当てをしてもらいましょう。"},{"id":{"d":72,"n":4,"t":2},"speaker":"B","text":"はい。次の授業に遅れるかもしれないので、先生に伝えてくれませんか？"},{"id":{"d":72,"n":5,"t":2},"speaker":"A","text":"分かりました。お大事にしてください。"}];
INSERT INTO sentence [{"id":{"d":73,"n":1,"t":2},"speaker":"A","text":"今日は、宿題がたくさんありますね。"},{"id":{"d":73,"n":2,"t":2},"speaker":"B","text":"明日から、連休だからですね。連休中は、何か予定はありますか？"},{"id":{"d":73,"n":3,"t":2},"speaker":"A","text":"部活の練習と宿題で忙しくなりそうです。"},{"id":{"d":73,"n":4,"t":2},"speaker":"B","text":"そうなのですね。時間があれば、カラオケに誘いたかったのですが、残念です。"},{"id":{"d":73,"n":5,"t":2},"speaker":"A","text":"すみません。私も残念ですが、他の人を誘ってください。"}];
INSERT INTO sentence [{"id":{"d":74,"n":1,"t":2},"speaker":"A","text":"今日の昼休みは縄跳びをしませんか？"},{"id":{"d":74,"n":2,"t":2},"speaker":"B","text":"良いですね。私は最近、親に新しい縄跳びを買ってもらいました。今回はピンク色にしました。"},{"id":{"d":74,"n":3,"t":2},"speaker":"A","text":"ピンク色はかわいいですね。私もそろそろ新しい縄跳びが欲しいと思っていて、今度の日曜日に買いに行く予定です。今度は何色にしようか今から考えています。"},{"id":{"d":74,"n":4,"t":2},"speaker":"B","text":"私が買ったお店には、赤・黄・オレンジ・青・緑・黒・七色とたくさんの種類の縄跳びがありました。"},{"id":{"d":74,"n":5,"t":2},"speaker":"A","text":"それほどたくさんの色の縄跳びがあるのですね。今から楽しみです。"},{"id":{"d":74,"n":6,"t":2},"speaker":"B","text":"買ったら見せてくださいね。"},{"id":{"d":74,"n":7,"t":2},"speaker":"A","text":"もちろんです。さあ、校庭で縄跳びをしましょう。"}];
INSERT INTO sentence [{"id":{"d":75,"n":1,"t":2},"speaker":"A","text":"今週最後の授業が、ようやく終わりました。明日は、待ちに待った土曜日です。"},{"id":{"d":75,"n":2,"t":2},"speaker":"B","text":"今週も大変でしたね。中丸さんは何が一番大変でしたか？"},{"id":{"d":75,"n":3,"t":2},"speaker":"A","text":"私はおとといの体育の授業で行った長距離走がとても大変でした。安藤さんは、いかがでしたか？"},{"id":{"d":75,"n":4,"t":2},"speaker":"B","text":"私は、今週はそこまで大変ではありませんでした。代わりに来週の小テストが怖いです。ところで、この後、何か食べに行きませんか？"},{"id":{"d":75,"n":5,"t":2},"speaker":"A","text":"行きましょう。最近おいしいスイーツのお店を知ったので、そのお店に行ってみましょう。"},{"id":{"d":75,"n":6,"t":2},"speaker":"B","text":"良いですね。とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":76,"n":1,"t":2},"speaker":"A","text":"私は物を作る事が好きです。"},{"id":{"d":76,"n":2,"t":2},"speaker":"B","text":"どのような物を作る事が好きなのですか？"},{"id":{"d":76,"n":3,"t":2},"speaker":"A","text":"今、熱中しているのは模型のプラモデルを作る事です。"},{"id":{"d":76,"n":4,"t":2},"speaker":"B","text":"戦車やロボットでしょうか？"},{"id":{"d":76,"n":5,"t":2},"speaker":"A","text":"はい、細かい作りのものをするのがとても楽しいです。"},{"id":{"d":76,"n":6,"t":2},"speaker":"B","text":"趣味があるのはいいことです。私も何か作ろうと思います。"},{"id":{"d":76,"n":7,"t":2},"speaker":"A","text":"ぜひ。お薦めのプラモデルの紹介もしますよ。"},{"id":{"d":76,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":77,"n":1,"t":2},"speaker":"A","text":"最近、乗馬を始めたのですが、馬に乗るのはとても楽しいです。"},{"id":{"d":77,"n":2,"t":2},"speaker":"B","text":"楽しいのですか？馬は体が大きくて怖く見えてしまいます。"},{"id":{"d":77,"n":3,"t":2},"speaker":"A","text":"確かに、見た目は少し怖いです。しかし、実はかなり賢く、臆病な生き物なのです。"},{"id":{"d":77,"n":4,"t":2},"speaker":"B","text":"そうなのですか？"},{"id":{"d":77,"n":5,"t":2},"speaker":"A","text":"そうなのです。馬のすごいところは人馬一体となった時の風景や速さがとても素晴らしいところです。"},{"id":{"d":77,"n":6,"t":2},"speaker":"B","text":"話を聞いていたら、私も馬に乗ってみたくなりました。乗馬体験のできるところはありますか？"},{"id":{"d":77,"n":7,"t":2},"speaker":"A","text":"私が通っているクラブでも乗馬体験をしていますよ。ぜひ、いらしてみてください。"}];
INSERT INTO sentence [{"id":{"d":78,"n":1,"t":2},"speaker":"A","text":"体育館の天井に挟まっているバレーボールの横に、いつの間にか縄跳びが引っ掛かっています。"},{"id":{"d":78,"n":2,"t":2},"speaker":"B","text":"本当ですね、よく気が付きましたね。"},{"id":{"d":78,"n":3,"t":2},"speaker":"A","text":"私はバレーボール部なので、練習の時に天井を見る機会が多いのです。"},{"id":{"d":78,"n":4,"t":2},"speaker":"B","text":"そうだったのですね。"},{"id":{"d":78,"n":5,"t":2},"speaker":"A","text":"川口さんは何部ですか？"},{"id":{"d":78,"n":6,"t":2},"speaker":"B","text":"私は美術部です。"},{"id":{"d":78,"n":7,"t":2},"speaker":"A","text":"そうなのですね。美術といえば、川口さんは絵画に興味はありますか？県立美術館でちょうど今開催している絵画展に行きたいと思っていたのですが、一緒に行きませんか？"},{"id":{"d":78,"n":8,"t":2},"speaker":"B","text":"もちろんです。私も行きたいと思っていたところでした。"},{"id":{"d":78,"n":9,"t":2},"speaker":"A","text":"うれしいです。今度の日曜日に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":79,"n":1,"t":2},"speaker":"A","text":"あなたはいつも同じものを飲んでいますね。"},{"id":{"d":79,"n":2,"t":2},"speaker":"B","text":"このカフェオレの事ですか？"},{"id":{"d":79,"n":3,"t":2},"speaker":"A","text":"はい。"},{"id":{"d":79,"n":4,"t":2},"speaker":"B","text":"このカフェオレは、とてもおいしくて全く飽きが来ません。ぜひ、飲んでみてほしいです。"},{"id":{"d":79,"n":5,"t":2},"speaker":"A","text":"興味はあるのですが、私は苦いものが苦手なので遠慮しておきます。"}];
INSERT INTO sentence [{"id":{"d":80,"n":1,"t":2},"speaker":"A","text":"先生は、なぜ教師になろうと思ったのですか？"},{"id":{"d":80,"n":2,"t":2},"speaker":"B","text":"私は、勉強を教えるのが好きなのと、子どもが好きだからです。"},{"id":{"d":80,"n":3,"t":2},"speaker":"A","text":"そうなのですね。でも子どもに勉強を教えるのは難しくありませんか？"},{"id":{"d":80,"n":4,"t":2},"speaker":"B","text":"難しいですが、とても楽しいですし、やりがいもあります。何事もできるようになるには、時間がかかるものなので、焦らずじっくり教えるよう心掛けています。"},{"id":{"d":80,"n":5,"t":2},"speaker":"A","text":"だから、先生は何度も繰り返し教えてくださるのですね。いつも優しく教えてくださりありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":81,"n":1,"t":2},"speaker":"A","text":"部活は文化部と運動部がありますが、どちらが人気ですか？"},{"id":{"d":81,"n":2,"t":2},"speaker":"B","text":"所属している人数が多いのは運動部です。野球部やサッカー部が特に多いです。"},{"id":{"d":81,"n":3,"t":2},"speaker":"A","text":"そうですか。文化部で人気があるのはどのような部活ですか？"},{"id":{"d":81,"n":4,"t":2},"speaker":"B","text":"吹奏楽部は圧倒的に、部員数が多いです。あとは文芸部や漫画部も最近、部員数が増えています。"},{"id":{"d":81,"n":5,"t":2},"speaker":"A","text":"そうなのですね。私は宮沢賢治の作品が好きなので、文芸部に少し興味があります。"},{"id":{"d":81,"n":6,"t":2},"speaker":"B","text":"文学作品がお好きなら、部活ではありませんが、図書委員会に所属するのも良いかもしれませんね。委員になれば好きな本を入れる事もできますよ。"},{"id":{"d":81,"n":7,"t":2},"speaker":"A","text":"なるほど、それは良い案かもしれません。"}];
INSERT INTO sentence [{"id":{"d":82,"n":1,"t":2},"speaker":"A","text":"自動販売機のところまで買い物に行ってきます。何か買ってくる物はありませんか？"},{"id":{"d":82,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。私はコーラが欲しいです。"},{"id":{"d":82,"n":3,"t":2},"speaker":"A","text":"分かりました。では、行ってきます。"},{"id":{"d":82,"n":4,"t":2},"speaker":"B","text":"あ、福田さん。お財布を机の上に忘れていますよ。"},{"id":{"d":82,"n":5,"t":2},"speaker":"A","text":"うっかりしていました。教えてくださり、ありがとうございます。今度こそ行ってきます。"}];
INSERT INTO sentence [{"id":{"d":83,"n":1,"t":2},"speaker":"A","text":"今日は、とても暑い日ですね。"},{"id":{"d":83,"n":2,"t":2},"speaker":"B","text":"そうですね。この日差しは、すっかり夏ですね。"},{"id":{"d":83,"n":3,"t":2},"speaker":"A","text":"この炎天下のなか、今日の体育の授業は校庭でランニングですよ。"},{"id":{"d":83,"n":4,"t":2},"speaker":"B","text":"そうでした。このような日に、校庭で体育の授業をするのはとても嫌です。"},{"id":{"d":83,"n":5,"t":2},"speaker":"A","text":"みんなで先生にお願いして、体育館で体育の授業をしてもらいませんか？"},{"id":{"d":83,"n":6,"t":2},"speaker":"B","text":"そうですね。賛成です。"}];
INSERT INTO sentence [{"id":{"d":84,"n":1,"t":2},"speaker":"A","text":"私は、リコーダーの演奏があまり得意ではありません。"},{"id":{"d":84,"n":2,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":84,"n":3,"t":2},"speaker":"A","text":"指をうまく動かせなくて、低いドの音を出すのが難しいからです。"},{"id":{"d":84,"n":4,"t":2},"speaker":"B","text":"全部の指を使いますものね。発表会では違うパートになれるといいですね。"},{"id":{"d":84,"n":5,"t":2},"speaker":"A","text":"はい、大太鼓を狙っています。"}];
INSERT INTO sentence [{"id":{"d":85,"n":1,"t":2},"speaker":"A","text":"今日も、とても良い天気ですね。"},{"id":{"d":85,"n":2,"t":2},"speaker":"B","text":"そうですね、本当に良い天気です。天気予報によると、今週いっぱいは青空が続くようです。"},{"id":{"d":85,"n":3,"t":2},"speaker":"A","text":"来週は天気が悪くなるのでしょうか？"},{"id":{"d":85,"n":4,"t":2},"speaker":"B","text":"テレビ番組の天気予報によると「来週の頭に雨が降る」と言っていました。その後も曇りや雨の日が多くなるそうです。"},{"id":{"d":85,"n":5,"t":2},"speaker":"A","text":"そうですか。そろそろ、梅雨に入るのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":86,"n":1,"t":2},"speaker":"A","text":"この学校は、昼休みに屋上を開放しているそうです。"},{"id":{"d":86,"n":2,"t":2},"speaker":"B","text":"そうなのですね。昼食を食べるときに屋上へ行ったら気持ち良さそうです。"},{"id":{"d":86,"n":3,"t":2},"speaker":"A","text":"明日の昼食は屋上にお弁当を持参しませんか？"},{"id":{"d":86,"n":4,"t":2},"speaker":"B","text":"良いですね、明日の天気はどうでしょうか？"},{"id":{"d":86,"n":5,"t":2},"speaker":"A","text":"晴れるそうですよ。絶好のお弁当日和ですね。"},{"id":{"d":86,"n":6,"t":2},"speaker":"B","text":"早起きして腕によりをかけてお弁当を作ってきます。明日の昼食が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":87,"n":1,"t":2},"speaker":"A","text":"先生、今日の美術の授業では、何をするのですか？"},{"id":{"d":87,"n":2,"t":2},"speaker":"B","text":"皆さんには自画像を描いてもらいます。"},{"id":{"d":87,"n":3,"t":2},"speaker":"A","text":"分かりました。でも、自分の顔を描くのは何だか恥ずかしいです。"},{"id":{"d":87,"n":4,"t":2},"speaker":"B","text":"恥ずかしいことはありません。よく細部まで見てその通りに描けばよいのですよ。"}];
INSERT INTO sentence [{"id":{"d":88,"n":1,"t":2},"speaker":"A","text":"午後の授業は、私が苦手な理系科目ばかりです。"},{"id":{"d":88,"n":2,"t":2},"speaker":"B","text":"そうですね、数学や化学がそろっています。"},{"id":{"d":88,"n":3,"t":2},"speaker":"A","text":"早く放課後になってほしいです。部活を思い切り楽しみたいです。"},{"id":{"d":88,"n":4,"t":2},"speaker":"B","text":"サッカー部でしたね。サッカーは好きですか？"},{"id":{"d":88,"n":5,"t":2},"speaker":"A","text":"はい、「部活動のために学校に来ている」と言っても過言ではないくらいに大好きです。"},{"id":{"d":88,"n":6,"t":2},"speaker":"B","text":"それはいいですね。打ち込めることがあるのは良いと思います。"}];
INSERT INTO sentence [{"id":{"d":89,"n":1,"t":2},"speaker":"A","text":"鍋料理の中身が焦げてしまいました。どうしてこうなってしまったのでしょうか？"},{"id":{"d":89,"n":2,"t":2},"speaker":"B","text":"分かりません。レシピ通りに作ったはずなのですが。"},{"id":{"d":89,"n":3,"t":2},"speaker":"A","text":"火加減を間違えてしまったのでしょうか？"},{"id":{"d":89,"n":4,"t":2},"speaker":"B","text":"いえ、火加減は間違っていないはずです。砂糖を入れ過ぎたせいでしょうか？"},{"id":{"d":89,"n":5,"t":2},"speaker":"A","text":"あるいはその両方かもしれません。いずれにせよ、今日の昼食がなくなってしまいました。"},{"id":{"d":89,"n":6,"t":2},"speaker":"B","text":"それは残念です。近くのファミリーマートへコンビニ弁当を買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":90,"n":1,"t":2},"speaker":"A","text":"蛯名先生、少しよろしいでしょうか？"},{"id":{"d":90,"n":2,"t":2},"speaker":"B","text":"どうしましたか、渡辺先生？"},{"id":{"d":90,"n":3,"t":2},"speaker":"A","text":"明日の体育は校庭で行なう予定でしたが、雨が降るという天気予報のため体育館に変更しようとしたところ、あいにく体育館は既に埋まっていました。明日の体育の授業ができそうになくて困っています。"},{"id":{"d":90,"n":4,"t":2},"speaker":"B","text":"なるほど。明日の私の英語の授業と交換したい、という事ですね？"},{"id":{"d":90,"n":5,"t":2},"speaker":"A","text":"はい、勝手を申しますが、いかがでしょう？"},{"id":{"d":90,"n":6,"t":2},"speaker":"B","text":"分かりました。大丈夫ですよ。生徒には「今日は英語の授業が2回続く」とお伝えください。"},{"id":{"d":90,"n":7,"t":2},"speaker":"A","text":"蛯名先生、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":91,"n":1,"t":2},"speaker":"A","text":"朝礼前に、読書タイムを設けることになりました。皆さん、自分の好きな本を毎日10分読みましょう。"},{"id":{"d":91,"n":2,"t":2},"speaker":"B","text":"先生、質問です。読むのはどのような本でもよいのですか？"},{"id":{"d":91,"n":3,"t":2},"speaker":"A","text":"はい、佐藤君の好きな本で大丈夫ですよ。"},{"id":{"d":91,"n":4,"t":2},"speaker":"B","text":"分かりました。それでは、僕は動物が好きだから、動物図鑑を読む事にします。"},{"id":{"d":91,"n":5,"t":2},"speaker":"A","text":"佐藤君は動物が好きなのですね。先生も動物が好きです。特にアフリカの動物たちが好きです。"},{"id":{"d":91,"n":6,"t":2},"speaker":"B","text":"アフリカの動物だと、僕はライオンが一番好きです。強くて格好良いです。先生は何が好きですか？"},{"id":{"d":91,"n":7,"t":2},"speaker":"A","text":"先生はカバが好きです。見ていてとても癒されます。"}];
INSERT INTO sentence [{"id":{"d":92,"n":1,"t":2},"speaker":"A","text":"以上で、この問題の解説は終わります。何か質問のある人はいますか？"},{"id":{"d":92,"n":2,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":92,"n":3,"t":2},"speaker":"A","text":"尾田君、どうぞ。"},{"id":{"d":92,"n":4,"t":2},"speaker":"B","text":"英語のことわざが、なかなか覚えられないのですが、何か覚えるコツはありますか？"},{"id":{"d":92,"n":5,"t":2},"speaker":"A","text":"ことわざは、その土地の文化や価値観が、反映されていることも多いです。そのため、英語圏の歴史や思想なども勉強すると、ことわざを覚えやすくなるかもしれません。"},{"id":{"d":92,"n":6,"t":2},"speaker":"B","text":"なるほど。ありがとうございます。試してみます。"},{"id":{"d":92,"n":7,"t":2},"speaker":"A","text":"他に質問のある人はいますか？いなければ先へ進みます。"}];
INSERT INTO sentence [{"id":{"d":93,"n":1,"t":2},"speaker":"A","text":"これは、一体何をしているのですか？"},{"id":{"d":93,"n":2,"t":2},"speaker":"B","text":"花壇を整備して花を植えようとしています。"},{"id":{"d":93,"n":3,"t":2},"speaker":"A","text":"花ですか。良いですね。何の花を植えるのですか？"},{"id":{"d":93,"n":4,"t":2},"speaker":"B","text":"ヒマワリを植える予定です。夏にはヒマワリが満開になりますよ。"}];
INSERT INTO sentence [{"id":{"d":94,"n":1,"t":2},"speaker":"A","text":"今日は、とても寒いですね。ストーブが動いていないように見えます。"},{"id":{"d":94,"n":2,"t":2},"speaker":"B","text":"昨日の放課後に、突然動かなくなってしまいました。もうすぐ先生が来て、対処してくれるとの事です。"},{"id":{"d":94,"n":3,"t":2},"speaker":"A","text":"その間は、この寒さを自力で解消するしかないのですね。"},{"id":{"d":94,"n":4,"t":2},"speaker":"B","text":"そうですね。私は見ての通りジャケットを着たままです。"},{"id":{"d":94,"n":5,"t":2},"speaker":"A","text":"私もまだ、このジャケットが手放せませんね。"},{"id":{"d":94,"n":6,"t":2},"speaker":"B","text":"これから冬本番なので、もっと暖かいジャケットを買いたいと思っています。"},{"id":{"d":94,"n":7,"t":2},"speaker":"A","text":"一緒に買いに行きますか？放課後で良ければお店に行けますよ。"},{"id":{"d":94,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。では、一緒に買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":95,"n":1,"t":2},"speaker":"A","text":"今日の部活動は、突然ですが中止になりました。"},{"id":{"d":95,"n":2,"t":2},"speaker":"B","text":"なぜ、中止になったのですか？"},{"id":{"d":95,"n":3,"t":2},"speaker":"A","text":"この後、台風が来るそうなので、部活動をすると危険と判断したからです。"},{"id":{"d":95,"n":4,"t":2},"speaker":"B","text":"そうすると、午後の授業も切り上げて帰れるのですか？"},{"id":{"d":95,"n":5,"t":2},"speaker":"A","text":"はい、今日の授業はここまでにして、帰りの準備をしてください。"},{"id":{"d":95,"n":6,"t":2},"speaker":"B","text":"学校の授業が途中で終わるほどの、強い台風ですか。早く帰りたいですね。"}];
INSERT INTO sentence [{"id":{"d":96,"n":1,"t":2},"speaker":"A","text":"疲れた顔をしていますが、どうしましたか？"},{"id":{"d":96,"n":2,"t":2},"speaker":"B","text":"昨日、部屋の片付けを夜遅くまでしていたから、寝不足です。"},{"id":{"d":96,"n":3,"t":2},"speaker":"A","text":"今日の体育の授業は、持久走と聞いていますが、大丈夫ですか？"},{"id":{"d":96,"n":4,"t":2},"speaker":"B","text":"ゆっくり走れば、大丈夫だと思います。"},{"id":{"d":96,"n":5,"t":2},"speaker":"A","text":"そうですか、無理はしないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":97,"n":1,"t":2},"speaker":"A","text":"先生は、音楽はお好きですか？"},{"id":{"d":97,"n":2,"t":2},"speaker":"B","text":"好きですよ。趣味は音楽鑑賞で、最近の流行の曲から昔の曲まで幅広く聴きますよ。"},{"id":{"d":97,"n":3,"t":2},"speaker":"A","text":"どのようなジャンルの音楽が、お好きなのですか？"},{"id":{"d":97,"n":4,"t":2},"speaker":"B","text":"Jポップは好きでよく聴きます。最近は昭和の歌謡曲に熱中しています。"},{"id":{"d":97,"n":5,"t":2},"speaker":"A","text":"どのような時に音楽を聴いているのですか？"},{"id":{"d":97,"n":6,"t":2},"speaker":"B","text":"通勤中と寝る前は必ず聴いています。休日は、一日中音楽を聴いていることもありますよ。"}];
INSERT INTO sentence [{"id":{"d":98,"n":1,"t":2},"speaker":"A","text":"こんばんは。"},{"id":{"d":98,"n":2,"t":2},"speaker":"B","text":"こんばんは。遅くまでお疲れさまです。今日は部活ですか？"},{"id":{"d":98,"n":3,"t":2},"speaker":"A","text":"いいえ、今日は数学の補習を受けていました。"},{"id":{"d":98,"n":4,"t":2},"speaker":"B","text":"そうでしたか。この通り、もう外はだいぶ暗いので、帰り道には気を付けてくださいね。"},{"id":{"d":98,"n":5,"t":2},"speaker":"A","text":"分かりました。少し寒くなってきたので、警備員さんも暖かくしてくださいね。"},{"id":{"d":98,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。では、また明日。"},{"id":{"d":98,"n":7,"t":2},"speaker":"A","text":"はい、さようなら。"}];
INSERT INTO sentence [{"id":{"d":99,"n":1,"t":2},"speaker":"A","text":"先生、その計算は間違っていませんか？"},{"id":{"d":99,"n":2,"t":2},"speaker":"B","text":"どこのことでしょうか？"},{"id":{"d":99,"n":3,"t":2},"speaker":"A","text":"問3の最後の足し算です。正しい答えは42になると思います。"},{"id":{"d":99,"n":4,"t":2},"speaker":"B","text":"その通りですね。ご指摘いただき、ありがとうございます。"},{"id":{"d":99,"n":5,"t":2},"speaker":"A","text":"先生も間違う事があるのですね。"},{"id":{"d":99,"n":6,"t":2},"speaker":"B","text":"もちろんです。先生も人間なので、間違う事はありますよ。"}];
INSERT INTO sentence [{"id":{"d":100,"n":1,"t":2},"speaker":"A","text":"昨日の部活で頑張りすぎて、脚が痛いです。"},{"id":{"d":100,"n":2,"t":2},"speaker":"B","text":"陸上部の練習中に痛めたのですか？"},{"id":{"d":100,"n":3,"t":2},"speaker":"A","text":"そうです。うっかり転んで、けがをしてしまいました。"},{"id":{"d":100,"n":4,"t":2},"speaker":"B","text":"とても痛そうです。湿布を貼りに保健室へ行きますか？"},{"id":{"d":100,"n":5,"t":2},"speaker":"A","text":"そうします。付いてきてくれますか？"},{"id":{"d":100,"n":6,"t":2},"speaker":"B","text":"構いません。それでは行きましょう。"}];
INSERT INTO sentence [{"id":{"d":101,"n":1,"t":2},"speaker":"A","text":"お薦めの音楽があったら教えてください。"},{"id":{"d":101,"n":2,"t":2},"speaker":"B","text":"田中さんがそのようなことを言うことは、珍しいですね。何かありましたか？"},{"id":{"d":101,"n":3,"t":2},"speaker":"A","text":"最近音楽に興味が出てきたのですが、何から手を付けて良いかさっぱり分からず悩んでいました。"},{"id":{"d":101,"n":4,"t":2},"speaker":"B","text":"なるほど、それでお薦めを聞いたのですね。それならマイケル・ジャクソンなどはどうでしょう？"},{"id":{"d":101,"n":5,"t":2},"speaker":"A","text":"名前はもちろん知っていますが、曲を聴いたことはまだありません。"},{"id":{"d":101,"n":6,"t":2},"speaker":"B","text":"ぜひ、一度聞いてみてください。特に『THRILLER』はとてもテンポもよくて素晴らしい曲です。"},{"id":{"d":101,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。今日の放課後、早速CDショップに行って探してみます。"}];
INSERT INTO sentence [{"id":{"d":102,"n":1,"t":2},"speaker":"A","text":"歴史の授業は、覚えなくてはならないものがたくさんあって、大変です。"},{"id":{"d":102,"n":2,"t":2},"speaker":"B","text":"そうですね。1つの場面ごとに、たくさんの名前や用語が出てきますものね。"},{"id":{"d":102,"n":3,"t":2},"speaker":"A","text":"何か、良い覚え方はないでしょうか？"},{"id":{"d":102,"n":4,"t":2},"speaker":"B","text":"私は、とにかくまず人の名前を覚えて、それらの関連性を覚えるようにしています。"},{"id":{"d":102,"n":5,"t":2},"speaker":"A","text":"なるほど、まずは人名の暗記から始めるのですね。"},{"id":{"d":102,"n":6,"t":2},"speaker":"B","text":"はい。ぜひ試してみてください。"}];
INSERT INTO sentence [{"id":{"d":103,"n":1,"t":2},"speaker":"A","text":"新しく始まる倫理の授業では、どのようなことを学ぶのでしょうか？"},{"id":{"d":103,"n":2,"t":2},"speaker":"B","text":"人類の思想や信仰の歴史を、哲学や心理学、宗教学、倫理学的な観点から学んでいくそうです。高橋さんは倫理に興味があるのですか？"},{"id":{"d":103,"n":3,"t":2},"speaker":"A","text":"はい、とても興味があります。授業が始まるのが楽しみです。"},{"id":{"d":103,"n":4,"t":2},"speaker":"B","text":"そうなのですね。私は、倫理の授業は難しそうでつい敬遠してしまいます。"},{"id":{"d":103,"n":5,"t":2},"speaker":"A","text":"何でも、初めてのものは難しく感じるものですよ。授業を受けるうちに、だんだんと好きになり難しいと思わなくなるかもしれません。"},{"id":{"d":103,"n":6,"t":2},"speaker":"B","text":"確かに、そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":104,"n":1,"t":2},"speaker":"A","text":"コーヒーに砂糖を入れるのは、普通のことです。"},{"id":{"d":104,"n":2,"t":2},"speaker":"B","text":"そうですね。"},{"id":{"d":104,"n":3,"t":2},"speaker":"A","text":"紅茶に砂糖を入れることも、ごく普通のことです。"},{"id":{"d":104,"n":4,"t":2},"speaker":"B","text":"そうですね。"},{"id":{"d":104,"n":5,"t":2},"speaker":"A","text":"なぜ、お茶に砂糖を入れるのはおかしいのでしょうか？"},{"id":{"d":104,"n":6,"t":2},"speaker":"B","text":"習慣として、砂糖を入れないからだと思います。海外では、お茶に砂糖を入れるところもあると聞きます。"},{"id":{"d":104,"n":7,"t":2},"speaker":"A","text":"なるほど、習慣からでしたか。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":105,"n":1,"t":2},"speaker":"A","text":"今日の放課後は珍しく部活動がありません。"},{"id":{"d":105,"n":2,"t":2},"speaker":"B","text":"珍しいですね。なぜなのですか？"},{"id":{"d":105,"n":3,"t":2},"speaker":"A","text":"担当の先生が、急な会議に出席して、私たちの監督をする人がいなくなってしまったからです。"},{"id":{"d":105,"n":4,"t":2},"speaker":"B","text":"では、今日の放課後は空いていますか？"},{"id":{"d":105,"n":5,"t":2},"speaker":"A","text":"はい、空いていますがどうしましたか？"},{"id":{"d":105,"n":6,"t":2},"speaker":"B","text":"駅前に、おいしいケーキのお店ができたそうです。学校帰りに寄って行きませんか？"},{"id":{"d":105,"n":7,"t":2},"speaker":"A","text":"ぜひ。ケーキが大好きなので、とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":106,"n":1,"t":2},"speaker":"A","text":"スマートフォンは相変わらず使いにくいです。"},{"id":{"d":106,"n":2,"t":2},"speaker":"B","text":"なかなか慣れませんか？"},{"id":{"d":106,"n":3,"t":2},"speaker":"A","text":"はい。使いこなすコツなどは、ありますか？"},{"id":{"d":106,"n":4,"t":2},"speaker":"B","text":"そうですね、慣れないと思うところを一つずつ解消していくしかないと思います。"},{"id":{"d":106,"n":5,"t":2},"speaker":"A","text":"一番は、ディスプレイ操作というのが難しいです。"},{"id":{"d":106,"n":6,"t":2},"speaker":"B","text":"分からないことがあれば、いつでも連絡してくださって大丈夫ですよ。一緒に慣れていきましょう。"}];
INSERT INTO sentence [{"id":{"d":107,"n":1,"t":2},"speaker":"A","text":"習慣というものは、恐ろしいものです。"},{"id":{"d":107,"n":2,"t":2},"speaker":"B","text":"何かありましたか？"},{"id":{"d":107,"n":3,"t":2},"speaker":"A","text":"今日は朝練がないのに、朝早起きをしてしまいました。"},{"id":{"d":107,"n":4,"t":2},"speaker":"B","text":"生活習慣が突然変わっても、体のリズムをすぐに変えるのは難しいです。"},{"id":{"d":107,"n":5,"t":2},"speaker":"A","text":"今日はゆっくりと、眠っていたかったです。"},{"id":{"d":107,"n":6,"t":2},"speaker":"B","text":"つらいようなら先生に伝えて、保健室で仮眠をとりますか？"},{"id":{"d":107,"n":7,"t":2},"speaker":"A","text":"そこまでではないので、頑張りたいと思います。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":108,"n":1,"t":2},"speaker":"A","text":"失礼します。田中さんはいますか？"},{"id":{"d":108,"n":2,"t":2},"speaker":"B","text":"はい、どうしましたか？"},{"id":{"d":108,"n":3,"t":2},"speaker":"A","text":"国語の教科書を忘れてしまったので、貸してほしいです。"},{"id":{"d":108,"n":4,"t":2},"speaker":"B","text":"分かりました。国語辞典はなくても大丈夫ですか？"},{"id":{"d":108,"n":5,"t":2},"speaker":"A","text":"そういえば、先生が国語辞典を使うと言っていましたね。一緒に貸してください。"},{"id":{"d":108,"n":6,"t":2},"speaker":"B","text":"はい。授業が終わったら、教科書と辞典の両方を返しにきてくださいね。"},{"id":{"d":108,"n":7,"t":2},"speaker":"A","text":"恩に着ます。後日、お礼をさせてください。"},{"id":{"d":108,"n":8,"t":2},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":109,"n":1,"t":2},"speaker":"A","text":"初めまして、新しく赴任してきた佐藤です。このクラスの担任です。1年間よろしくお願いします。"},{"id":{"d":109,"n":2,"t":2},"speaker":"B","text":"佐藤先生、よろしくお願いします。"},{"id":{"d":109,"n":3,"t":2},"speaker":"A","text":"一緒に勉強を頑張りましょうね。楽しい学校生活にしましょう。"},{"id":{"d":109,"n":4,"t":2},"speaker":"B","text":"はい、先生。"}];
INSERT INTO sentence [{"id":{"d":110,"n":1,"t":2},"speaker":"A","text":"加賀さんにとって、学校生活は楽しいですか？"},{"id":{"d":110,"n":2,"t":2},"speaker":"B","text":"私は学校のお昼の給食が楽しみです。赤城さんは違うのですか？"},{"id":{"d":110,"n":3,"t":2},"speaker":"A","text":"そのようなことは、ありません。私も給食は大好きです。授業は楽しいですか？"},{"id":{"d":110,"n":4,"t":2},"speaker":"B","text":"授業もいろいろな知識を知ることができるので、楽しいですよ。"},{"id":{"d":110,"n":5,"t":2},"speaker":"A","text":"すごいですね。私は覚えることが苦手で四苦八苦しております。"},{"id":{"d":110,"n":6,"t":2},"speaker":"B","text":"では、今日の放課後に勉強会をしませんか？"},{"id":{"d":110,"n":7,"t":2},"speaker":"A","text":"良いですね、分からないところを教えてください。"},{"id":{"d":110,"n":8,"t":2},"speaker":"B","text":"ええ、喜んで。"}];
INSERT INTO sentence [{"id":{"d":111,"n":1,"t":2},"speaker":"A","text":"今日の算数の授業は九九をしますね。"},{"id":{"d":111,"n":2,"t":2},"speaker":"B","text":"先生、九九とは何ですか？"},{"id":{"d":111,"n":3,"t":2},"speaker":"A","text":"掛け算の事をいいますよ。まずは九九を覚えてみましょう。暗記表があるので、繰り返し音読して覚えていきましょう。"},{"id":{"d":111,"n":4,"t":2},"speaker":"B","text":"はい。全部覚えられるように、頑張ります。"}];
INSERT INTO sentence [{"id":{"d":112,"n":1,"t":2},"speaker":"A","text":"五十鈴さんは、毎朝の起床時間は早いのですか？"},{"id":{"d":112,"n":2,"t":2},"speaker":"B","text":"はい、両親が牧場を経営しているので、毎日朝5時に起きています。"},{"id":{"d":112,"n":3,"t":2},"speaker":"A","text":"そうなのですね。そのように早起きだと眠くなりませんか？"},{"id":{"d":112,"n":4,"t":2},"speaker":"B","text":"夜は早めに寝ているので、大丈夫です。"},{"id":{"d":112,"n":5,"t":2},"speaker":"A","text":"私は、昨日の夜遅くまで起きていたのでとても眠いです。"},{"id":{"d":112,"n":6,"t":2},"speaker":"B","text":"眠すぎてつらいようでしたら、先生に伝えましょうか？"},{"id":{"d":112,"n":7,"t":2},"speaker":"A","text":"そうですね、これ以上ひどくなったらお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":113,"n":1,"t":2},"speaker":"A","text":"飼育委員さん、教室の後ろの水槽の中のザリガニの様子がおかしいようです。"},{"id":{"d":113,"n":2,"t":2},"speaker":"B","text":"気が付きませんでした。教えてくださりありがとうございます。少し見てみます。"},{"id":{"d":113,"n":3,"t":2},"speaker":"A","text":"何か手伝えることはありますか？"},{"id":{"d":113,"n":4,"t":2},"speaker":"B","text":"多分、ポンプの調子が悪いせいだと思います。これを洗ってきてもらえますか？"},{"id":{"d":113,"n":5,"t":2},"speaker":"A","text":"分かりました。お安い御用です。"}];
INSERT INTO sentence [{"id":{"d":114,"n":1,"t":2},"speaker":"A","text":"今回のテストの結果は、どうだったのでしょうか？"},{"id":{"d":114,"n":2,"t":2},"speaker":"B","text":"学年トップをキープできました。南海さんはどうでしたか？"},{"id":{"d":114,"n":3,"t":2},"speaker":"A","text":"風野さんと一緒に勉強したおかげで、赤点を免れました。"},{"id":{"d":114,"n":4,"t":2},"speaker":"B","text":"それは良かったです。お祝いに、何か食べに行きませんか？"},{"id":{"d":114,"n":5,"t":2},"speaker":"A","text":"良いですね。おいしいスイーツのお店が良いです。"},{"id":{"d":114,"n":6,"t":2},"speaker":"B","text":"賛成です。甘い物をたくさん食べましょう。探してみます。"},{"id":{"d":114,"n":7,"t":2},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":115,"n":1,"t":2},"speaker":"A","text":"今日の天気は雨ですね。園芸係の私たちは、今日はお花の世話をしなくて済みそうです。"},{"id":{"d":115,"n":2,"t":2},"speaker":"B","text":"雨が降るので、じょうろを使って水をあげる必要がないということですね。"},{"id":{"d":115,"n":3,"t":2},"speaker":"A","text":"その通りです。雨が私たちの仕事の代わりになってくれます。"},{"id":{"d":115,"n":4,"t":2},"speaker":"B","text":"明日も雨の予報なので、明日も私たちの仕事はありませんね。"},{"id":{"d":115,"n":5,"t":2},"speaker":"A","text":"私は花に水をあげるのが好きなので少しだけ寂しいです。"}];
INSERT INTO sentence [{"id":{"d":116,"n":1,"t":2},"speaker":"A","text":"クリスマスにパーティーをしたいと思います。"},{"id":{"d":116,"n":2,"t":2},"speaker":"B","text":"どこでパーティーをしたいのですか？"},{"id":{"d":116,"n":3,"t":2},"speaker":"A","text":"私の家でしたいと思っています。須田さんもぜひいらしてください。"},{"id":{"d":116,"n":4,"t":2},"speaker":"B","text":"ぜひ、行きたいです。おいしいケーキを持って行きますね。"},{"id":{"d":116,"n":5,"t":2},"speaker":"A","text":"いえ、ケーキはこちらで用意しますので手ぶらで来ても大丈夫です。"},{"id":{"d":116,"n":6,"t":2},"speaker":"B","text":"分かりました。では、みんなで楽しめるようなお菓子を差し入れに持っていきますね。"},{"id":{"d":116,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。クリスマス当日が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":117,"n":1,"t":2},"speaker":"A","text":"今日の授業はここまでにします。残りの問題は次週で構いません。"},{"id":{"d":117,"n":2,"t":2},"speaker":"B","text":"鈴木先生、分からないところがあるのですが質問してもよろしいでしょうか？"},{"id":{"d":117,"n":3,"t":2},"speaker":"A","text":"もちろんよいですよ。どこが分からないのですか？"},{"id":{"d":117,"n":4,"t":2},"speaker":"B","text":"この英語の文章の覚え方が、分かりません。"},{"id":{"d":117,"n":5,"t":2},"speaker":"A","text":"そうですね、長い文章は、英単語よりも文法を意識して覚えると良いと思います。"},{"id":{"d":117,"n":6,"t":2},"speaker":"B","text":"なるほど、分かりました。試してみます。"}];
INSERT INTO sentence [{"id":{"d":118,"n":1,"t":2},"speaker":"A","text":"なぜ明日の授業は体育から算数になったのですか？"},{"id":{"d":118,"n":2,"t":2},"speaker":"B","text":"体育の授業を担当している森先生が、けがをして入院をされたからです。"},{"id":{"d":118,"n":3,"t":2},"speaker":"A","text":"入院をされたのですか。きっと大けがをされたのですね？"},{"id":{"d":118,"n":4,"t":2},"speaker":"B","text":"はい、そうみたいです。隣のクラスではお見舞いに行く予定があるそうです。"},{"id":{"d":118,"n":5,"t":2},"speaker":"A","text":"私たちのクラスも、お見舞いに行きませんか？"},{"id":{"d":118,"n":6,"t":2},"speaker":"B","text":"それは良い案ですね。今日の終礼でみんなの意見も聞いてみましょう。"},{"id":{"d":118,"n":7,"t":2},"speaker":"A","text":"それがいいですね。学級委員長の佐藤さんに早速相談してみましょう。"}];
INSERT INTO sentence [{"id":{"d":119,"n":1,"t":2},"speaker":"A","text":"飲み物だと何がお好きですか？"},{"id":{"d":119,"n":2,"t":2},"speaker":"B","text":"私はハーブティーが好きです。"},{"id":{"d":119,"n":3,"t":2},"speaker":"A","text":"コーラやサイダーは、お好きではありませんか？"},{"id":{"d":119,"n":4,"t":2},"speaker":"B","text":"炭酸飲料は苦手です。"},{"id":{"d":119,"n":5,"t":2},"speaker":"A","text":"なるほど、では今度一緒に紅茶のお店にでも行きましょう。"}];
INSERT INTO sentence [{"id":{"d":120,"n":1,"t":2},"speaker":"A","text":"校長先生は、怖くて苦手です。"},{"id":{"d":120,"n":2,"t":2},"speaker":"B","text":"そうですか？私は運動会の時に、校長先生が笑っている顔を見てから怖くなくなりました。"},{"id":{"d":120,"n":3,"t":2},"speaker":"A","text":"運動会の時に笑っていたのですね。私もその場面を見ていたら怖い印象が消えていたかもしれません。"},{"id":{"d":120,"n":4,"t":2},"speaker":"B","text":"正直に言うと、私も運動会の前は怖くて苦手でした。"},{"id":{"d":120,"n":5,"t":2},"speaker":"A","text":"そうですか。生徒の中にも校長先生を怖いと思っている人が結構いるようです。"}];
INSERT INTO sentence [{"id":{"d":121,"n":1,"t":2},"speaker":"A","text":"今日は、学芸会がありますね。"},{"id":{"d":121,"n":2,"t":2},"speaker":"B","text":"そうですね、あなたのクラスはどのような出し物をするのですか？"},{"id":{"d":121,"n":3,"t":2},"speaker":"A","text":"私たちのクラスは影絵で劇をします。"},{"id":{"d":121,"n":4,"t":2},"speaker":"B","text":"面白いですね。とても楽しみです。私のクラスは、歌を歌う事になりました。"},{"id":{"d":121,"n":5,"t":2},"speaker":"A","text":"すてきです。お互いに良い発表ができるといいですね。"}];
INSERT INTO sentence [{"id":{"d":122,"n":1,"t":2},"speaker":"A","text":"今日の放課後、一緒に公園で遊びませんか？"},{"id":{"d":122,"n":2,"t":2},"speaker":"B","text":"良いですよ。ひまわり公園に集合で良いですか？"},{"id":{"d":122,"n":3,"t":2},"speaker":"A","text":"今日はブランコに乗りたいので、日吉公園に行きませんか？"},{"id":{"d":122,"n":4,"t":2},"speaker":"B","text":"良いですよ。日吉公園は少し遠いので、自転車で行きますね。"}];
INSERT INTO sentence [{"id":{"d":123,"n":1,"t":2},"speaker":"A","text":"田中さん、昨日の昼食は何を食べましたか？"},{"id":{"d":123,"n":2,"t":2},"speaker":"B","text":"昨日は近くのラーメンショップでみそラーメンを食べました。私はラーメンに目がないのです。"},{"id":{"d":123,"n":3,"t":2},"speaker":"A","text":"ラーメンが好きなのですね。味はみそ味が好きですか？"},{"id":{"d":123,"n":4,"t":2},"speaker":"B","text":"はい。いろいろなお店のみそラーメンを食べ比べしています。"},{"id":{"d":123,"n":5,"t":2},"speaker":"A","text":"みそ味と一口に言っても、お店によって味が違いますからね。"},{"id":{"d":123,"n":6,"t":2},"speaker":"B","text":"そうなのですよ。東京駅の近くにお薦めのラーメン屋さんがあるので、今度、一緒に行きませんか？"},{"id":{"d":123,"n":7,"t":2},"speaker":"A","text":"ええ、ぜひ一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":124,"n":1,"t":2},"speaker":"A","text":"昨日は、台風で運動会は中止になりましたが、今日は晴天なので運動会を開催します。"},{"id":{"d":124,"n":2,"t":2},"speaker":"B","text":"待ちに待った運動会が、開催されるのですね。"},{"id":{"d":124,"n":3,"t":2},"speaker":"A","text":"あまりはしゃぎすぎて、けがをしないようにしましょう。"},{"id":{"d":124,"n":4,"t":2},"speaker":"B","text":"はい。準備運動をきちんとして本番に備えます。"},{"id":{"d":124,"n":5,"t":2},"speaker":"A","text":"それはいいことですね。"}];
INSERT INTO sentence [{"id":{"d":125,"n":1,"t":2},"speaker":"A","text":"用務員さんは、いつも学校の美化活動をしてくださっていますね。"},{"id":{"d":125,"n":2,"t":2},"speaker":"B","text":"そうですね。とてもありがたいことです。"},{"id":{"d":125,"n":3,"t":2},"speaker":"A","text":"あの用務員さんは、私の親戚ですよ。"},{"id":{"d":125,"n":4,"t":2},"speaker":"B","text":"そうなのですか。それは知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":126,"n":1,"t":2},"speaker":"A","text":"教室のストーブがついているのに、あまり暖かくならなりませんね。"},{"id":{"d":126,"n":2,"t":2},"speaker":"B","text":"確かに、全然暖かくありません。なぜでしょうか？"},{"id":{"d":126,"n":3,"t":2},"speaker":"A","text":"ストーブが壊れているのかもしれませんね。"},{"id":{"d":126,"n":4,"t":2},"speaker":"B","text":"それは困りました。どうしたらよいですか？"},{"id":{"d":126,"n":5,"t":2},"speaker":"A","text":"用務員さんが直せると思うので、今日中に用務員さんにこの件を伝えましょう。"},{"id":{"d":126,"n":6,"t":2},"speaker":"B","text":"分かりました。今から用務員室へ行ってきます。"}];
INSERT INTO sentence [{"id":{"d":127,"n":1,"t":2},"speaker":"A","text":"伊藤君、下駄箱に何か入っていますが何ですか？"},{"id":{"d":127,"n":2,"t":2},"speaker":"B","text":"何でしょうか。紙のようです。"},{"id":{"d":127,"n":3,"t":2},"speaker":"A","text":"この形は手紙のようですね。もしかしてラブレターですか？"},{"id":{"d":127,"n":4,"t":2},"speaker":"B","text":"分かりません。が、以前ラブレターが下駄箱に入っていた事はありました。"},{"id":{"d":127,"n":5,"t":2},"speaker":"A","text":"うらやましいことです。"}];
INSERT INTO sentence [{"id":{"d":128,"n":1,"t":2},"speaker":"A","text":"昨日の放課後、用務員さんがひとりで校庭の掃除をしていましたよ。"},{"id":{"d":128,"n":2,"t":2},"speaker":"B","text":"落ち葉を掃いてくださっていたのでしょうか。校庭は広いのでひとりでするのは大変そうです。"},{"id":{"d":128,"n":3,"t":2},"speaker":"A","text":"そうですね。今日は私たちも手伝いに行きましょう。"},{"id":{"d":128,"n":4,"t":2},"speaker":"B","text":"それは良いアイデアですね。友達にも声を掛けてみます。"}];
INSERT INTO sentence [{"id":{"d":129,"n":1,"t":2},"speaker":"A","text":"校庭のところにいる、あの女の人は高橋君のお母さんですか？"},{"id":{"d":129,"n":2,"t":2},"speaker":"B","text":"そうです。給食袋を忘れてしまったので、さっき届けてもらったのです。"},{"id":{"d":129,"n":3,"t":2},"speaker":"A","text":"優しいお母さんですね。"},{"id":{"d":129,"n":4,"t":2},"speaker":"B","text":"はい、とてもありがたいです。家に帰ったらお手伝いをしようと思います。"}];
INSERT INTO sentence [{"id":{"d":130,"n":1,"t":2},"speaker":"A","text":"もう夏ですね。"},{"id":{"d":130,"n":2,"t":2},"speaker":"B","text":"そうですね。暑い夏は苦手です。"},{"id":{"d":130,"n":3,"t":2},"speaker":"A","text":"私も暑いのは苦手ですが、夏は、水泳の授業があるので良いです。"},{"id":{"d":130,"n":4,"t":2},"speaker":"B","text":"そうですか。あさってから、水泳の授業が始まりますよ。"},{"id":{"d":130,"n":5,"t":2},"speaker":"A","text":"はい、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":131,"n":1,"t":2},"speaker":"A","text":"すみません。この問題の解き方が分からないので、教えてもらえますか？"},{"id":{"d":131,"n":2,"t":2},"speaker":"B","text":"この問題は、今日習った公式を素直に使えば簡単に解けますよ。"},{"id":{"d":131,"n":3,"t":2},"speaker":"A","text":"そうなのですか？"},{"id":{"d":131,"n":4,"t":2},"speaker":"B","text":"教科書の76ページの例題がとても参考になります。"},{"id":{"d":131,"n":5,"t":2},"speaker":"A","text":"分かりました、例題を見ながら解いてみます。"}];
INSERT INTO sentence [{"id":{"d":132,"n":1,"t":2},"speaker":"A","text":"今日の放課後は、仙台駅に行きませんか？"},{"id":{"d":132,"n":2,"t":2},"speaker":"B","text":"構いませんが、何かあるのですか？"},{"id":{"d":132,"n":3,"t":2},"speaker":"A","text":"仙台駅の近くにある書店に行きたいと思っています。新しく本が出るのでそれが欲しいのです。"},{"id":{"d":132,"n":4,"t":2},"speaker":"B","text":"分かりました。私も、ちょうど参考書を買いに行きたいと思っていたところでした。"},{"id":{"d":132,"n":5,"t":2},"speaker":"A","text":"ではまた、放課後に。授業が終わったら連絡します。"}];
INSERT INTO sentence [{"id":{"d":133,"n":1,"t":2},"speaker":"A","text":"今日は、朝から頭痛がひどいのですが、頑張って学校に来ました。"},{"id":{"d":133,"n":2,"t":2},"speaker":"B","text":"そこまで無理をしなくても、良いのではないでしょうか？"},{"id":{"d":133,"n":3,"t":2},"speaker":"A","text":"皆勤賞が、懸かっているので休めません。"},{"id":{"d":133,"n":4,"t":2},"speaker":"B","text":"そうでしたか。でも本当につらくなったら保健室に行きましょうね。"},{"id":{"d":133,"n":5,"t":2},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":134,"n":1,"t":2},"speaker":"A","text":"あさっては、先生と補習があります。"},{"id":{"d":134,"n":2,"t":2},"speaker":"B","text":"何の補習ですか？"},{"id":{"d":134,"n":3,"t":2},"speaker":"A","text":"国語の漢字の書き取りの補習です。先週の漢字の小テストで落第点を取ってしまいました。"},{"id":{"d":134,"n":4,"t":2},"speaker":"B","text":"私も、先週に先生とその補習をしましたよ。"},{"id":{"d":134,"n":5,"t":2},"speaker":"A","text":"そうなのですね。これからは補習にならないように、勉強を頑張ります。"},{"id":{"d":134,"n":6,"t":2},"speaker":"B","text":"そうですね。お互い頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":135,"n":1,"t":2},"speaker":"A","text":"来週はテストに向けて、今日の昼休み一緒に勉強しませんか？"},{"id":{"d":135,"n":2,"t":2},"speaker":"B","text":"良いですね。図書室で良いですか？"},{"id":{"d":135,"n":3,"t":2},"speaker":"A","text":"そうしましょう。他にも誰か誘いますか？"},{"id":{"d":135,"n":4,"t":2},"speaker":"B","text":"勉強が得意な友人も誘ってみようと思います。授業が終わり次第図書室に向かいますね。"},{"id":{"d":135,"n":5,"t":2},"speaker":"A","text":"分かりました。先に図書館に行って席をとっておきます。"}];
INSERT INTO sentence [{"id":{"d":136,"n":1,"t":2},"speaker":"A","text":"私たちの担任の先生はいつも優しいですね。"},{"id":{"d":136,"n":2,"t":2},"speaker":"B","text":"そうですね。でも昨日、廊下を走っていた他のクラスの生徒に注意をしていました。"},{"id":{"d":136,"n":3,"t":2},"speaker":"A","text":"生徒が悪いことをしていたら、先生が注意するのは当然のことだと思いますよ。"},{"id":{"d":136,"n":4,"t":2},"speaker":"B","text":"確かにそうですね。私たちも悪いことをして怒られないように、学校生活を過ごしましょう。"},{"id":{"d":136,"n":5,"t":2},"speaker":"A","text":"お互いに、気を付けていきましょう。"}];
INSERT INTO sentence [{"id":{"d":137,"n":1,"t":2},"speaker":"A","text":"今日の昼食も学生食堂で食べましょう。メニューは何にしましょう。"},{"id":{"d":137,"n":2,"t":2},"speaker":"B","text":"あちらにお薦めがありますね。今日はサバのみそ煮がお薦めみたいです。"},{"id":{"d":137,"n":3,"t":2},"speaker":"A","text":"なるほど、おいしそうです。石井さんは、どうされますか？"},{"id":{"d":137,"n":4,"t":2},"speaker":"B","text":"私はラーメンを食べたいです。昨日の夜、雑誌でラーメン特集の記事を見て、今日はラーメンと決めていました。"},{"id":{"d":137,"n":5,"t":2},"speaker":"A","text":"では、決まりですね。席を取っておいてください、注文してきます。"},{"id":{"d":137,"n":6,"t":2},"speaker":"B","text":"分かりました。お願いします。"}];
INSERT INTO sentence [{"id":{"d":138,"n":1,"t":2},"speaker":"A","text":"ついに、犬を飼う事になりました。"},{"id":{"d":138,"n":2,"t":2},"speaker":"B","text":"それは良かったですね。名前は、もう決めましたか？"},{"id":{"d":138,"n":3,"t":2},"speaker":"A","text":"茶色い犬なので、「ブラウン」という名前にしました。"},{"id":{"d":138,"n":4,"t":2},"speaker":"B","text":"すてきな名前ですね。かわいいですか？"},{"id":{"d":138,"n":5,"t":2},"speaker":"A","text":"とてもかわいいです。今度、ぜひ見に来てください。"},{"id":{"d":138,"n":6,"t":2},"speaker":"B","text":"ぜひ、見てみたいです。今週の土曜日に、川崎さんのお宅にお伺いしてもよろしいですか？"},{"id":{"d":138,"n":7,"t":2},"speaker":"A","text":"ぜひどうぞ。楽しみが増えてうれしいです。"}];
INSERT INTO sentence [{"id":{"d":139,"n":1,"t":2},"speaker":"A","text":"合唱コンクールのピアノ演奏を、加藤さんにお願いしても良いでしょうか？"},{"id":{"d":139,"n":2,"t":2},"speaker":"B","text":"はい、喜んでお引き受けします。一度、合唱コンクールでピアノを弾いてみたいと思っていました。"},{"id":{"d":139,"n":3,"t":2},"speaker":"A","text":"それは良かったです。では、よろしくお願いします。加藤さんはピアノを習っているのですか？"},{"id":{"d":139,"n":4,"t":2},"speaker":"B","text":"はい、3歳から習っていました。習い始めた頃はレッスンが嫌いでしたが、好きな曲が弾けるようになってからは楽しくなってきました。"},{"id":{"d":139,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。加藤さんのピアノの演奏を楽しみにしています。"},{"id":{"d":139,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。練習を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":140,"n":1,"t":2},"speaker":"A","text":"この漫画は、とても面白いです。"},{"id":{"d":140,"n":2,"t":2},"speaker":"B","text":"何という漫画ですか？"},{"id":{"d":140,"n":3,"t":2},"speaker":"A","text":"『SLAM DUNK』というバスケットボールの漫画です。"},{"id":{"d":140,"n":4,"t":2},"speaker":"B","text":"スポーツ漫画ですね。スポーツ漫画を読んでいると、体を動かしたくなりませんか？"},{"id":{"d":140,"n":5,"t":2},"speaker":"A","text":"とてもよく分かります。お昼休みに、体育館でバスケットボールをしませんか？"},{"id":{"d":140,"n":6,"t":2},"speaker":"B","text":"良いですね。他の友達にも声を掛けてみましょう。"}];
INSERT INTO sentence [{"id":{"d":141,"n":1,"t":2},"speaker":"A","text":"昨日は夜通しゲームをしていて、徹夜をしてしまいました。"},{"id":{"d":141,"n":2,"t":2},"speaker":"B","text":"少しも寝ていないのですか？"},{"id":{"d":141,"n":3,"t":2},"speaker":"A","text":"はい。そのため今すごく眠いです。"},{"id":{"d":141,"n":4,"t":2},"speaker":"B","text":"それは困りましたね。授業で寝てしまわないように、気を付けてください。"},{"id":{"d":141,"n":5,"t":2},"speaker":"A","text":"はい、頑張ります。"}];
INSERT INTO sentence [{"id":{"d":142,"n":1,"t":2},"speaker":"A","text":"カードゲームのUNOをしませんか？"},{"id":{"d":142,"n":2,"t":2},"speaker":"B","text":"しましょう。他にも何人か集めてきましょうか？"},{"id":{"d":142,"n":3,"t":2},"speaker":"A","text":"そうですね、このゲームは人数が多い方が楽しいです。"},{"id":{"d":142,"n":4,"t":2},"speaker":"B","text":"では、隣のクラスの友人を呼んできます。"},{"id":{"d":142,"n":5,"t":2},"speaker":"A","text":"分かりました。僕も、何人かに声を掛けてみます。"}];
INSERT INTO sentence [{"id":{"d":143,"n":1,"t":2},"speaker":"A","text":"将棋で、勝負をしましょう。"},{"id":{"d":143,"n":2,"t":2},"speaker":"B","text":"受けて立ちます。私は結構強いですよ。"},{"id":{"d":143,"n":3,"t":2},"speaker":"A","text":"お手柔らかにお願いします。でも、私も負けません。"},{"id":{"d":143,"n":4,"t":2},"speaker":"B","text":"それは、楽しみです。先手は、金山さんからでよいですか？"},{"id":{"d":143,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。では、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":144,"n":1,"t":2},"speaker":"A","text":"今日は、校庭でサッカーをしましょう。"},{"id":{"d":144,"n":2,"t":2},"speaker":"B","text":"雨が降りそうですが、外でサッカーをするのですか？"},{"id":{"d":144,"n":3,"t":2},"speaker":"A","text":"雨が降ったら、体育館に移動しましょう。それまでは、外でサッカーをしようと思います。"},{"id":{"d":144,"n":4,"t":2},"speaker":"B","text":"分かりました。では、サッカーボールを持ってきます。"},{"id":{"d":144,"n":5,"t":2},"speaker":"A","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":145,"n":1,"t":2},"speaker":"A","text":"恋人ができました。"},{"id":{"d":145,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。それはうれしいですね。"},{"id":{"d":145,"n":3,"t":2},"speaker":"A","text":"とてもうれしいです。彼女と一緒にいると、とても幸せな気持ちになります。"},{"id":{"d":145,"n":4,"t":2},"speaker":"B","text":"それは良かったです。池田さんが幸せそうで、私もうれしくなります。"}];
INSERT INTO sentence [{"id":{"d":146,"n":1,"t":2},"speaker":"A","text":"最近のプラモデルは、精密で仕上がりがとても素晴らしいです。"},{"id":{"d":146,"n":2,"t":2},"speaker":"B","text":"そうなのですか？"},{"id":{"d":146,"n":3,"t":2},"speaker":"A","text":"はい、10年前に比べて、格段に技術が上がっていると思います。説明書通りに組み立てるだけでも、とても満足できるものになっています。"},{"id":{"d":146,"n":4,"t":2},"speaker":"B","text":"色を塗ったりしなくても良いのですか？"},{"id":{"d":146,"n":5,"t":2},"speaker":"A","text":"はい。元からしっかり着色されているものが多く、色を塗らなくとも十分きれいです。"},{"id":{"d":146,"n":6,"t":2},"speaker":"B","text":"それはすごいですね。お薦めのものがあるのなら、ぜひ作ってみたいです。"},{"id":{"d":146,"n":7,"t":2},"speaker":"A","text":"では、今日の放課後は、模型店に一緒に行きませんか？"},{"id":{"d":146,"n":8,"t":2},"speaker":"B","text":"はい、ぜひ。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":147,"n":1,"t":2},"speaker":"A","text":"山田さんは、何部ですか？"},{"id":{"d":147,"n":2,"t":2},"speaker":"B","text":"僕は、帰宅部です。持病があって、部活動ができないのです。"},{"id":{"d":147,"n":3,"t":2},"speaker":"A","text":"そうだったのですね。何の病気ですか？"},{"id":{"d":147,"n":4,"t":2},"speaker":"B","text":"ぜんそくです。激しい運動ができないのです。"},{"id":{"d":147,"n":5,"t":2},"speaker":"A","text":"それは大変ですね。私の姉もぜんそくを患っていて運動ができません。"},{"id":{"d":147,"n":6,"t":2},"speaker":"B","text":"でも、最近は昔よりも症状が良くなってきているのです。"},{"id":{"d":147,"n":7,"t":2},"speaker":"A","text":"それは良かったですね。もし、ぜんそくが良くなったら卓球部に入りませんか？"},{"id":{"d":147,"n":8,"t":2},"speaker":"B","text":"よいですよ。卓球を見るのは好きなので、プレーすることにも興味がありました。"},{"id":{"d":147,"n":9,"t":2},"speaker":"A","text":"ありがとうございます、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":148,"n":1,"t":2},"speaker":"A","text":"ほれぼれする投球フォームですね。"},{"id":{"d":148,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。中学まで野球をしていました。"},{"id":{"d":148,"n":3,"t":2},"speaker":"A","text":"道理で。今はもう野球をしていないのですか？"},{"id":{"d":148,"n":4,"t":2},"speaker":"B","text":"はい、今は帰宅部です。"},{"id":{"d":148,"n":5,"t":2},"speaker":"A","text":"それは、なぜですか？"},{"id":{"d":148,"n":6,"t":2},"speaker":"B","text":"一度肩を痛めてしまい、続けて行くのが難しくなったからです。今はのんびりと、帰宅部を満喫しています。"},{"id":{"d":148,"n":7,"t":2},"speaker":"A","text":"それだけのセンスがあるのにもったいないですね。"},{"id":{"d":148,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。今でもたまに、父とキャッチボールをして楽しんでいますよ。"}];
INSERT INTO sentence [{"id":{"d":149,"n":1,"t":2},"speaker":"A","text":"今日は道路に、落ち葉がたくさんありますね。"},{"id":{"d":149,"n":2,"t":2},"speaker":"B","text":"そうですね。校庭のイチョウの木から、葉っぱが落ちているのでしょう。"},{"id":{"d":149,"n":3,"t":2},"speaker":"A","text":"校庭のイチョウの木が紅葉すると、とてもきれいです、落ち葉の掃除は、毎年なかなか骨が折れます。"},{"id":{"d":149,"n":4,"t":2},"speaker":"B","text":"そうですね。今年も、そろそろ落ち葉掃除をする季節です。"},{"id":{"d":149,"n":5,"t":2},"speaker":"A","text":"秋が来たことを実感します。今年も、頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":150,"n":1,"t":2},"speaker":"A","text":"この部屋には、衣装がたくさんありますね。"},{"id":{"d":150,"n":2,"t":2},"speaker":"B","text":"演劇部の衣装室です。興味のある服はありますか？"},{"id":{"d":150,"n":3,"t":2},"speaker":"A","text":"あちらの王子様の衣装が、とても気になります。"},{"id":{"d":150,"n":4,"t":2},"speaker":"B","text":"あれは『シンデレラ』の劇で使った王子様の衣装です。"},{"id":{"d":150,"n":5,"t":2},"speaker":"A","text":"とてもすてきです。一度着てみたいのですが良いですか？"},{"id":{"d":150,"n":6,"t":2},"speaker":"B","text":"先生の許可を取ってこないといけません。もうすぐ顧問の先生がいらっしゃるので、聞いてみますね。"},{"id":{"d":150,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。他にも素晴らしい衣装がたくさんありますね。"},{"id":{"d":150,"n":8,"t":2},"speaker":"B","text":"この衣装は全て先輩や私たちの手作りです。「素晴らしい」と言っていただけて、とてもうれしいです。"}];
INSERT INTO sentence [{"id":{"d":151,"n":1,"t":2},"speaker":"A","text":"モーツァルトの『魔笛』と言う曲が好きです。"},{"id":{"d":151,"n":2,"t":2},"speaker":"B","text":"そうなのですか？軽音楽部では、ロックを演奏しているイメージがあったので意外です。"},{"id":{"d":151,"n":3,"t":2},"speaker":"A","text":"もともと、クラシック音楽やオペラ音楽は大好きですよ。"},{"id":{"d":151,"n":4,"t":2},"speaker":"B","text":"そうだったのですね。お薦めのクラシック音楽はありますか？"},{"id":{"d":151,"n":5,"t":2},"speaker":"A","text":"王道でも良ければ、ヴィヴァルディの『四季』やシューマンの『トロイメライ』、ワーグナーなどがお薦めです。"},{"id":{"d":151,"n":6,"t":2},"speaker":"B","text":"知識が豊富で、すごいですね。ぜひ聴いてみます。"},{"id":{"d":151,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。番場さんは、流行のJポップに詳しいですよね。"},{"id":{"d":151,"n":8,"t":2},"speaker":"B","text":"ラジオが好きなので、よく耳にするのです。Jポップならお薦めの曲をいくらでも紹介しますよ。"},{"id":{"d":151,"n":9,"t":2},"speaker":"A","text":"それはうれしいです。ぜひ教えてください。"}];
INSERT INTO sentence [{"id":{"d":152,"n":1,"t":2},"speaker":"A","text":"来月の遠足は、富士山に行きます。"},{"id":{"d":152,"n":2,"t":2},"speaker":"B","text":"富士山には登った事がないので、とても楽しみです。"},{"id":{"d":152,"n":3,"t":2},"speaker":"A","text":"でも、富士山は冬に登れるのでしょうか？"},{"id":{"d":152,"n":4,"t":2},"speaker":"B","text":"富士山は、冬には登れないので、5合目までしか行けません。"},{"id":{"d":152,"n":5,"t":2},"speaker":"A","text":"冬の遠足はやめて、夏の遠足を富士山にしませんか？"},{"id":{"d":152,"n":6,"t":2},"speaker":"B","text":"それも良いですね。校長先生に話してみます。それから決めましょう。"},{"id":{"d":152,"n":7,"t":2},"speaker":"A","text":"お願いします。夏に富士山に行けると良いですね。"}];
INSERT INTO sentence [{"id":{"d":153,"n":1,"t":2},"speaker":"A","text":"風邪をひきました。高熱が出ていて、とてもつらいです。"},{"id":{"d":153,"n":2,"t":2},"speaker":"B","text":"電話越しでも、つらさがうかがえます。"},{"id":{"d":153,"n":3,"t":2},"speaker":"A","text":"「今日は学校をお休みする」と先生に伝えたと思っているのですが、記憶が曖昧で、よく覚えておりません。"},{"id":{"d":153,"n":4,"t":2},"speaker":"B","text":"後で先生に、連絡が来ているか聞きますか？"},{"id":{"d":153,"n":5,"t":2},"speaker":"A","text":"助かります。お願いします。"},{"id":{"d":153,"n":6,"t":2},"speaker":"B","text":"了解です。では後は、ゆっくりと休んでください。早く良くなるといいですね。"},{"id":{"d":153,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。早く良くなりたいです。"}];
INSERT INTO sentence [{"id":{"d":154,"n":1,"t":2},"speaker":"A","text":"山田さんのお見舞いに行こうと思います。"},{"id":{"d":154,"n":2,"t":2},"speaker":"B","text":"今日で3日お休みしているので、それも良いでしょう。"},{"id":{"d":154,"n":3,"t":2},"speaker":"A","text":"何を持って行けばよいのか、悩んでいます。"},{"id":{"d":154,"n":4,"t":2},"speaker":"B","text":"学校の配布プリントを持っていきましょう。あとは、飲み物などはどうでしょう？"},{"id":{"d":154,"n":5,"t":2},"speaker":"A","text":"良いですね。それでは、ミネラルウォーターを途中で買って持って行ってみます。"},{"id":{"d":154,"n":6,"t":2},"speaker":"B","text":"それは良いですね。遠藤君も風邪には気を付けてください。"},{"id":{"d":154,"n":7,"t":2},"speaker":"A","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":155,"n":1,"t":2},"speaker":"A","text":"初めまして、川田です。長野から転校してきました。"},{"id":{"d":155,"n":2,"t":2},"speaker":"B","text":"私は佐藤です。席が隣なので分からない事があったら、何でも聞いてください。"},{"id":{"d":155,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。早速なのですが、下校時間は何時ですか？"},{"id":{"d":155,"n":4,"t":2},"speaker":"B","text":"下校時間は、16時になります。もし良かったら、今日一緒に帰りませんか？"},{"id":{"d":155,"n":5,"t":2},"speaker":"A","text":"うれしいです。これからよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":156,"n":1,"t":2},"speaker":"A","text":"この前、友達の前で、先生のことを「お母さん」と間違えて言ってしまいました。"},{"id":{"d":156,"n":2,"t":2},"speaker":"B","text":"それは、恥ずかしいですね。私も間違えてしまったことがあり、とても恥ずかしい思いをしました。"},{"id":{"d":156,"n":3,"t":2},"speaker":"A","text":"あの時は、今までで一番恥ずかしくて、顔が真っ赤になってしまいました。"},{"id":{"d":156,"n":4,"t":2},"speaker":"B","text":"とても、よく分かります。"}];
INSERT INTO sentence [{"id":{"d":157,"n":1,"t":2},"speaker":"A","text":"失礼します。けがの治療をお願いします。"},{"id":{"d":157,"n":2,"t":2},"speaker":"B","text":"はい、ここに掛けてください。擦り傷ですね。これはどうしましたか？"},{"id":{"d":157,"n":3,"t":2},"speaker":"A","text":"登校中に、電信柱にぶつかって転んでしまいました。"},{"id":{"d":157,"n":4,"t":2},"speaker":"B","text":"あら、それは大変でしたね。"},{"id":{"d":157,"n":5,"t":2},"speaker":"A","text":"今日の文化祭の出し物の準備に夢中になって、夜更かしをしてしまいました。"},{"id":{"d":157,"n":6,"t":2},"speaker":"B","text":"睡眠不足のときは、いつも以上に気を付けて行動してくださいね。"},{"id":{"d":157,"n":7,"t":2},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":158,"n":1,"t":2},"speaker":"A","text":"9時から校庭で、校長先生のお話がありますよ。"},{"id":{"d":158,"n":2,"t":2},"speaker":"B","text":"あと10分ですね。そろそろ校庭に移動しましょう。"},{"id":{"d":158,"n":3,"t":2},"speaker":"A","text":"はい。校長先生の話は少し退屈です。"},{"id":{"d":158,"n":4,"t":2},"speaker":"B","text":"退屈などと言ってはいけませんよ。校長先生は大切な話をしてくれるので、しっかり聞きましょう。"},{"id":{"d":158,"n":5,"t":2},"speaker":"A","text":"そうですね。今日も「夏休みの生活で注意することについて話す」と言っていました。"}];
INSERT INTO sentence [{"id":{"d":159,"n":1,"t":2},"speaker":"A","text":"明日の英語の授業は、歴史の授業に変更になりました。"},{"id":{"d":159,"n":2,"t":2},"speaker":"B","text":"それは、なぜですか？"},{"id":{"d":159,"n":3,"t":2},"speaker":"A","text":"英語担当の高橋先生が、急きょ研修に参加することになったからです。"},{"id":{"d":159,"n":4,"t":2},"speaker":"B","text":"そうなのですね。英語の振り替え授業はありますか？"},{"id":{"d":159,"n":5,"t":2},"speaker":"A","text":"来週に振り替え授業をする予定です。"},{"id":{"d":159,"n":6,"t":2},"speaker":"B","text":"分かりました。では明日は、歴史の教科書を持ってきます。"}];
INSERT INTO sentence [{"id":{"d":160,"n":1,"t":2},"speaker":"A","text":"今年の運動会も、6年生の出し物は毎年恒例の『ソーラン節』です。"},{"id":{"d":160,"n":2,"t":2},"speaker":"B","text":"ついに『ソーラン節』を踊れるのですね。楽しみにしていたので、うれしいです。"},{"id":{"d":160,"n":3,"t":2},"speaker":"A","text":"練習は、来週から始めたいと思います。"},{"id":{"d":160,"n":4,"t":2},"speaker":"B","text":"何か用意するものはありますか？"},{"id":{"d":160,"n":5,"t":2},"speaker":"A","text":"豆絞りの手拭いを、1枚持ってきてください。"},{"id":{"d":160,"n":6,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":161,"n":1,"t":2},"speaker":"A","text":"なぜあなたは、昨日休んだのですか？"},{"id":{"d":161,"n":2,"t":2},"speaker":"B","text":"母が倒れてしまったからです。"},{"id":{"d":161,"n":3,"t":2},"speaker":"A","text":"それは大変でしたね。お母さまの具合はいかがですか？"},{"id":{"d":161,"n":4,"t":2},"speaker":"B","text":"貧血のようでした。"},{"id":{"d":161,"n":5,"t":2},"speaker":"A","text":"私も貧血によくなるので、大変なのは分かります。"},{"id":{"d":161,"n":6,"t":2},"speaker":"B","text":"そうですか。学年集会でよく貧血になって倒れる人がいますよね。私も一度、貧血で倒れてしまった事がありました。"},{"id":{"d":161,"n":7,"t":2},"speaker":"A","text":"それは、あなたも大変な経験をしましたね。"}];
INSERT INTO sentence [{"id":{"d":162,"n":1,"t":2},"speaker":"A","text":"若林先生、体育倉庫の鍵を知りませんか？"},{"id":{"d":162,"n":2,"t":2},"speaker":"B","text":"泉先生、鍵置き場にはありませんでしたか？"},{"id":{"d":162,"n":3,"t":2},"speaker":"A","text":"先ほど見てきましたが、ありませんでした。"},{"id":{"d":162,"n":4,"t":2},"speaker":"B","text":"そうですか。あれ、もしかしたら私のジャージのポケットに入れたままにしていたかもしれません。"},{"id":{"d":162,"n":5,"t":2},"speaker":"A","text":"確認してもらってもよいですか？"},{"id":{"d":162,"n":6,"t":2},"speaker":"B","text":"はい。ああ、ありました。先生のお時間を取ってしまい申し訳ありません。"},{"id":{"d":162,"n":7,"t":2},"speaker":"A","text":"すぐに見つかって、良かったです。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":163,"n":1,"t":2},"speaker":"A","text":"来年は、どのような年にしたいですか？"},{"id":{"d":163,"n":2,"t":2},"speaker":"B","text":"勉強を頑張って、学年で1位になりたいです。"},{"id":{"d":163,"n":3,"t":2},"speaker":"A","text":"目標が明確で、良いですね。"},{"id":{"d":163,"n":4,"t":2},"speaker":"B","text":"目標を明確にしないと、達成できないとの母の教えです。"},{"id":{"d":163,"n":5,"t":2},"speaker":"A","text":"素晴らしいお母さまです。目標を達成できるように、頑張ってくださいね。"},{"id":{"d":163,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。加藤先生にも、いろいろ勉強について相談させていただくと思いますが、よろしくお願いします。"},{"id":{"d":163,"n":7,"t":2},"speaker":"A","text":"もちろんです。一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":164,"n":1,"t":2},"speaker":"A","text":"なぜ、あなたはいつも寝坊や遅刻をするのですか？"},{"id":{"d":164,"n":2,"t":2},"speaker":"B","text":"私は、朝がとても弱いのです。社会人になる前に、直したいとは思っているのですが。"},{"id":{"d":164,"n":3,"t":2},"speaker":"A","text":"朝起きられないのであれば、まずは、早めに寝ることから始めてみるのはどうですか？"},{"id":{"d":164,"n":4,"t":2},"speaker":"B","text":"なるほど。試してみします。今日から20時には寝るようにしてみます。"}];
INSERT INTO sentence [{"id":{"d":165,"n":1,"t":2},"speaker":"A","text":"加藤さん、すこし良いですか？"},{"id":{"d":165,"n":2,"t":2},"speaker":"B","text":"どうしました、佐藤さん？"},{"id":{"d":165,"n":3,"t":2},"speaker":"A","text":"今日はご両親の帰りが遅いと聞きました。それなら、私の家に夕食を食べに来ませんか？"},{"id":{"d":165,"n":4,"t":2},"speaker":"B","text":"お気遣いいただきありがとうございます。でも、佐藤さんに申し訳ありません。"},{"id":{"d":165,"n":5,"t":2},"speaker":"A","text":"いえいえ。家族での付き合いも長いですし、加藤さんも1人で食べるのは味気ないでしょう。私の弟や妹も加藤さんと食事するのを、とても楽しみにしていますので、ぜひいらしてください。"},{"id":{"d":165,"n":6,"t":2},"speaker":"B","text":"本当に良いのですか？では、お言葉に甘えてお願いします。"},{"id":{"d":165,"n":7,"t":2},"speaker":"A","text":"はい、もちろんですよ。早速私の母に伝えます。"}];
INSERT INTO sentence [{"id":{"d":166,"n":1,"t":2},"speaker":"A","text":"小さい頃はどのような絵本を読んでいましたか？"},{"id":{"d":166,"n":2,"t":2},"speaker":"B","text":"日本昔話や世界の名作を読んでいました。『浦島太郎』や『長靴をはいた猫』が大好きでした。"},{"id":{"d":166,"n":3,"t":2},"speaker":"A","text":"私は昔話だと『竹取物語』が、世界の名作では『シンデレラ』が好きです。"},{"id":{"d":166,"n":4,"t":2},"speaker":"B","text":"『シンデレラ』も良いですね。いつかドレスを着てみたいです。"},{"id":{"d":166,"n":5,"t":2},"speaker":"A","text":"そういえば、演劇部には『シンデレラ』のドレスがあるそうですよ。顧問の先生にお願いして、試着させてもらいませんか？"},{"id":{"d":166,"n":6,"t":2},"speaker":"B","text":"良いですね。見るだけでもよいのでお願いしてみましょう。顧問の先生はどなたでしたか？"},{"id":{"d":166,"n":7,"t":2},"speaker":"A","text":"山百合先生です。一緒にお願いに行きましょう。"},{"id":{"d":166,"n":8,"t":2},"speaker":"B","text":"はい。良い返事が来ると良いですね。"}];
INSERT INTO sentence [{"id":{"d":167,"n":1,"t":2},"speaker":"A","text":"今日は調理実習があります。カレーを作ってもらいます。"},{"id":{"d":167,"n":2,"t":2},"speaker":"B","text":"カレーですね。分かりました。おいしく作れるように頑張ります。"},{"id":{"d":167,"n":3,"t":2},"speaker":"A","text":"家で、カレーを作ったことはありますか？"},{"id":{"d":167,"n":4,"t":2},"speaker":"B","text":"カレーは私の大好物なので、家でもよく食べますが、作ったことはありません。"},{"id":{"d":167,"n":5,"t":2},"speaker":"A","text":"では、今日は家でカレーを作れるようになることを目指して頑張ってみてください。"},{"id":{"d":167,"n":6,"t":2},"speaker":"B","text":"はい、頑張ります。"}];
INSERT INTO sentence [{"id":{"d":168,"n":1,"t":2},"speaker":"A","text":"今日は佐藤さんの家に家庭訪問へ行く日なので、よろしくお願いします。"},{"id":{"d":168,"n":2,"t":2},"speaker":"B","text":"先生が家に来ると思うと、とても緊張します。"},{"id":{"d":168,"n":3,"t":2},"speaker":"A","text":"普段通りの佐藤さんの様子が見たいので緊張しなくて大丈夫ですよ。"},{"id":{"d":168,"n":4,"t":2},"speaker":"B","text":"分かりました。ところで、先生はコーヒーと紅茶どちらがお好きですか？"},{"id":{"d":168,"n":5,"t":2},"speaker":"A","text":"私は、紅茶が好きですが、何も用意いただかなくて大丈夫ですよ。お母さまにも伝えておいてください。"},{"id":{"d":168,"n":6,"t":2},"speaker":"B","text":"そうなのですね。分かりました。"}];
INSERT INTO sentence [{"id":{"d":169,"n":1,"t":2},"speaker":"A","text":"今週から、グラウンドの改修工事が始まります。事前に伝えた通り、その間はグラウンドでの活動はできなくなります。"},{"id":{"d":169,"n":2,"t":2},"speaker":"B","text":"夏休みの間に行うのは良いですが、野球やサッカーといったグラウンド競技の練習ができませんね。"},{"id":{"d":169,"n":3,"t":2},"speaker":"A","text":"校内での基礎トレーニングで、我慢するように伝えてください。"},{"id":{"d":169,"n":4,"t":2},"speaker":"B","text":"了解しました。そういえば、工事という事で思い出したのですが、教室の扉に、少々建付けが悪くなっている模様です。"},{"id":{"d":169,"n":5,"t":2},"speaker":"A","text":"どこの教室ですか？"},{"id":{"d":169,"n":6,"t":2},"speaker":"B","text":"2年2組の前扉です。"},{"id":{"d":169,"n":7,"t":2},"speaker":"A","text":"今度、用務員さんにお願いしてみましょう。駄目だったらそこも工事を検討します。"},{"id":{"d":169,"n":8,"t":2},"speaker":"B","text":"分かりました。では用務員さんへの連絡は、私がしておきます。"},{"id":{"d":169,"n":9,"t":2},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":170,"n":1,"t":2},"speaker":"A","text":"転校生の佐藤と言います。皆さん、よろしくお願いします。"},{"id":{"d":170,"n":2,"t":2},"speaker":"B","text":"佐藤さん、よろしくお願いします。私は近藤と言います。"},{"id":{"d":170,"n":3,"t":2},"speaker":"A","text":"近藤さん、よろしくお願いします。私は東京から来ました。近藤さんはずっと宮城ですか？"},{"id":{"d":170,"n":4,"t":2},"speaker":"B","text":"はい、私は生まれてからずっと宮城にいます。"},{"id":{"d":170,"n":5,"t":2},"speaker":"A","text":"まだ越してきて、少ししかたっていませんが、宮城県はすてきな所ですね。これからの生活が楽しみです。"},{"id":{"d":170,"n":6,"t":2},"speaker":"B","text":"それは良かったです。もし良かったら、宮城県の名所を案内しますよ。"},{"id":{"d":170,"n":7,"t":2},"speaker":"A","text":"お誘いいただきありがとうございます。ぜひ、お願いします。"},{"id":{"d":170,"n":8,"t":2},"speaker":"B","text":"近いうちに、一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":171,"n":1,"t":2},"speaker":"A","text":"倉田さん、何を書いているのですか？"},{"id":{"d":171,"n":2,"t":2},"speaker":"B","text":"授業で先生に教えていただいた事を、ノートにまとめています。"},{"id":{"d":171,"n":3,"t":2},"speaker":"A","text":"いつもこのように、ノートにまとめているのですか？"},{"id":{"d":171,"n":4,"t":2},"speaker":"B","text":"はい。まとめておくと、復習する時に楽になりますので。"},{"id":{"d":171,"n":5,"t":2},"speaker":"A","text":"確かにそうですね。まとめるコツはありますか？"},{"id":{"d":171,"n":6,"t":2},"speaker":"B","text":"色の付いたペンや、付箋を使って工夫しています。"},{"id":{"d":171,"n":7,"t":2},"speaker":"A","text":"それは良いですね。私も見習わせていただきます。"}];
INSERT INTO sentence [{"id":{"d":172,"n":1,"t":2},"speaker":"A","text":"夏休み中も、学校のプールに入りに来ても良いですか？"},{"id":{"d":172,"n":2,"t":2},"speaker":"B","text":"はい。プールの開放日があるので、その日に来てください。"},{"id":{"d":172,"n":3,"t":2},"speaker":"A","text":"分かりました。浮輪を持ってきても良いですか？"},{"id":{"d":172,"n":4,"t":2},"speaker":"B","text":"浮輪やビーチボールは、学校にあるものを使ってください。"},{"id":{"d":172,"n":5,"t":2},"speaker":"A","text":"分かりました。家から持ってくるのはいけないのですね？"},{"id":{"d":172,"n":6,"t":2},"speaker":"B","text":"はい。学校のプールは授業の場として開放しているので、私物の浮輪やビーチボールは持ち込めません。"},{"id":{"d":172,"n":7,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":173,"n":1,"t":2},"speaker":"A","text":"今日も雨ですね。"},{"id":{"d":173,"n":2,"t":2},"speaker":"B","text":"しかも、今日は気温も低いので、外に出るのがとてもおっくうです。"},{"id":{"d":173,"n":3,"t":2},"speaker":"A","text":"こういう天気の日は一歩も外に出たくありませんね。いつになったら晴れるのでしょうか？"},{"id":{"d":173,"n":4,"t":2},"speaker":"B","text":"向こう何日かは、雨が続くそうですよ。外での体育の授業や屋外プールでの水泳も、しばらくできなさそうで残念です。"}];
INSERT INTO sentence [{"id":{"d":174,"n":1,"t":2},"speaker":"A","text":"みかんのおいしい季節になりました。"},{"id":{"d":174,"n":2,"t":2},"speaker":"B","text":"そうですね。こたつに入りながら食べるみかんは最高です。"},{"id":{"d":174,"n":3,"t":2},"speaker":"A","text":"こたつで食べる食事は、なぜか一層おいしく感じます。こたつに入りながら食べるおそばやお餅、雑炊などもとてもおいしいです。"},{"id":{"d":174,"n":4,"t":2},"speaker":"B","text":"それは家族で集まって、のんびりと食べるからかもしれませんね。"},{"id":{"d":174,"n":5,"t":2},"speaker":"A","text":"そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":175,"n":1,"t":2},"speaker":"A","text":"先生、遠足のバスは前の方に乗りたいです。"},{"id":{"d":175,"n":2,"t":2},"speaker":"B","text":"はい、分かりました。でも、どうしてですか？"},{"id":{"d":175,"n":3,"t":2},"speaker":"A","text":"乗り物に酔いやすいからです。"},{"id":{"d":175,"n":4,"t":2},"speaker":"B","text":"それは大変ですね。もし心配なら、先生の隣に座っても良いですよ。"},{"id":{"d":175,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。先生の隣に座ります。"}];
INSERT INTO sentence [{"id":{"d":176,"n":1,"t":2},"speaker":"A","text":"今から、新しい教科書を配ります。学級委員長は、こちらをクラス全員に配ってください。"},{"id":{"d":176,"n":2,"t":2},"speaker":"B","text":"はい、分かりました。"},{"id":{"d":176,"n":3,"t":2},"speaker":"A","text":"最初は、国語の教科書を配ります。次は、社会の教科書です。"},{"id":{"d":176,"n":4,"t":2},"speaker":"B","text":"先生、国語の教科書が1冊足りないようです。"},{"id":{"d":176,"n":5,"t":2},"speaker":"A","text":"おお、失礼しました。国語の教科書が足りない分はこちらの箱から取ってください。"}];
INSERT INTO sentence [{"id":{"d":177,"n":1,"t":2},"speaker":"A","text":"今日の昼休みは、図書室で過ごしませんか？"},{"id":{"d":177,"n":2,"t":2},"speaker":"B","text":"良いですよ。ちょうど読みたい本があったので図書室で読書をしようと思います。"},{"id":{"d":177,"n":3,"t":2},"speaker":"A","text":"私も久しぶりに、読書をしようと思います。普段、図書室では勉強をしていることが多いので。"},{"id":{"d":177,"n":4,"t":2},"speaker":"B","text":"図書館は静かなので勉強するのに向いていそうです。私も今度、勉強をしてみます。"},{"id":{"d":177,"n":5,"t":2},"speaker":"A","text":"とても集中できるので、お勧めですよ。"}];
INSERT INTO sentence [{"id":{"d":178,"n":1,"t":2},"speaker":"A","text":"おはようございます。毎日ここで会いますね。あなたはいつも、この道を通るのですか？"},{"id":{"d":178,"n":2,"t":2},"speaker":"B","text":"はい。この道は私の通学路です。"},{"id":{"d":178,"n":3,"t":2},"speaker":"A","text":"そうでしたか。私も、この道が通学路なのですよ。"},{"id":{"d":178,"n":4,"t":2},"speaker":"B","text":"そうなのですね。良かったら明日から一緒に通学しませんか？"},{"id":{"d":178,"n":5,"t":2},"speaker":"A","text":"ええ、ぜひ。では、7時30分に公園前で、待ち合わせにしましょう。"},{"id":{"d":178,"n":6,"t":2},"speaker":"B","text":"分かりました。明日から、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":179,"n":1,"t":2},"speaker":"A","text":"あなたの好きな武将は誰ですか？"},{"id":{"d":179,"n":2,"t":2},"speaker":"B","text":"私の好きな武将は、徳川家康です。"},{"id":{"d":179,"n":3,"t":2},"speaker":"A","text":"なるほど。それは、なぜですか？"},{"id":{"d":179,"n":4,"t":2},"speaker":"B","text":"戦乱の世でも、我慢強く、どっしりと構えていたという話が好きです。勉強家で倹約家だったという点も魅力的です。"},{"id":{"d":179,"n":5,"t":2},"speaker":"A","text":"倹約家という一面があったのは知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":180,"n":1,"t":2},"speaker":"A","text":"今日は晴れていて、気持ちの良い朝ですね。"},{"id":{"d":180,"n":2,"t":2},"speaker":"B","text":"そうですね。昨日は雨でしたから、晴れると気持ちが良いですね。"},{"id":{"d":180,"n":3,"t":2},"speaker":"A","text":"でも、私は、本当は雨の日が好きです。"},{"id":{"d":180,"n":4,"t":2},"speaker":"B","text":"雨の日が好きなのですか？"},{"id":{"d":180,"n":5,"t":2},"speaker":"A","text":"はい。私は雨の日が好きです。雨が降ると空気がきれいになりますからね。"},{"id":{"d":180,"n":6,"t":2},"speaker":"B","text":"確かに。雨が降った後の晴れはどこかすがすがしいですね。"}];
INSERT INTO sentence [{"id":{"d":181,"n":1,"t":2},"speaker":"A","text":"さっき、教室に入ろうとして、焦って隣のクラスに入ってしまいました。"},{"id":{"d":181,"n":2,"t":2},"speaker":"B","text":"だから、いなかったのですね。"},{"id":{"d":181,"n":3,"t":2},"speaker":"A","text":"入った瞬間、いつもと雰囲気が違う気がして、気が付きました。"},{"id":{"d":181,"n":4,"t":2},"speaker":"B","text":"注目されて、「君は隣のクラスだよ」と言われて、恥ずかしい気持ちになりますよね。"},{"id":{"d":181,"n":5,"t":2},"speaker":"A","text":"はい。とても恥ずかしかったです。"}];
INSERT INTO sentence [{"id":{"d":182,"n":1,"t":2},"speaker":"A","text":"今日は体育館で、身体測定があります。"},{"id":{"d":182,"n":2,"t":2},"speaker":"B","text":"何時からですか？"},{"id":{"d":182,"n":3,"t":2},"speaker":"A","text":"11時からです。体操着に着替えて、遅れないように来てください。"},{"id":{"d":182,"n":4,"t":2},"speaker":"B","text":"分かりました。何か、持っていくものはありますか？"},{"id":{"d":182,"n":5,"t":2},"speaker":"A","text":"筆記用具を持参してください。"}];
INSERT INTO sentence [{"id":{"d":183,"n":1,"t":2},"speaker":"A","text":"今から体育館で、バスケットボールの試合をしませんか？"},{"id":{"d":183,"n":2,"t":2},"speaker":"B","text":"よいですよ。一対一の勝負ですか？"},{"id":{"d":183,"n":3,"t":2},"speaker":"A","text":"もちろんです。"},{"id":{"d":183,"n":4,"t":2},"speaker":"B","text":"僕は、チームで勝負したいです。"},{"id":{"d":183,"n":5,"t":2},"speaker":"A","text":"では、最初にチームで勝負しましょう。その次に、一対一で勝負しましょう。"},{"id":{"d":183,"n":6,"t":2},"speaker":"B","text":"分かりました。他の人を誘ってきます。"}];
INSERT INTO sentence [{"id":{"d":184,"n":1,"t":2},"speaker":"A","text":"明日の放課後の、予定を聞いても良いですか？"},{"id":{"d":184,"n":2,"t":2},"speaker":"B","text":"明日は部活がないので、授業が終わったらそのまま帰るつもりです。"},{"id":{"d":184,"n":3,"t":2},"speaker":"A","text":"駅前にあるユニクロへ、服を見に行きませんか？"},{"id":{"d":184,"n":4,"t":2},"speaker":"B","text":"それなら、今日の放課後に行きませんか？"},{"id":{"d":184,"n":5,"t":2},"speaker":"A","text":"今日の放課後は、先生に呼び出しを受けているのです。"},{"id":{"d":184,"n":6,"t":2},"speaker":"B","text":"そうでしたか。時間がかかりますか？"},{"id":{"d":184,"n":7,"t":2},"speaker":"A","text":"分かりません。ただ、そこまで長くはないと思います。"},{"id":{"d":184,"n":8,"t":2},"speaker":"B","text":"では教室で待っていますので、今日は服を見に行きましょう。"},{"id":{"d":184,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。楽しみですね。"},{"id":{"d":184,"n":10,"t":2},"speaker":"B","text":"はい、とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":185,"n":1,"t":2},"speaker":"A","text":"海は好きですか？"},{"id":{"d":185,"n":2,"t":2},"speaker":"B","text":"はい。海は好きです。悩み事があると海に行って、ゆっくりとした時間を過ごしています。"},{"id":{"d":185,"n":3,"t":2},"speaker":"A","text":"ゆっくり、何をしているのですか？"},{"id":{"d":185,"n":4,"t":2},"speaker":"B","text":"特に何もしません。広い海を、何も考えずに眺めているだけで、心が整理されていく気がします。"},{"id":{"d":185,"n":5,"t":2},"speaker":"A","text":"なるほど、それは良いですね。"},{"id":{"d":185,"n":6,"t":2},"speaker":"B","text":"海の音、香り、風を感じていると心を無にすることができます。そうしているうちに自分の悩みが小さく感じられて、また頑張ろうという気持ちになります。"}];
INSERT INTO sentence [{"id":{"d":186,"n":1,"t":2},"speaker":"A","text":"この前、なくしたと思っていた大切なハンカチが、落とし物箱に入っていました。"},{"id":{"d":186,"n":2,"t":2},"speaker":"B","text":"そうでしたか。落とし物箱を確認して良かったですね。"},{"id":{"d":186,"n":3,"t":2},"speaker":"A","text":"はい。この花柄のハンカチは去年の誕生日に有子さんにもらった物だったので、なくしてしまいとても落ち込んでいました。"},{"id":{"d":186,"n":4,"t":2},"speaker":"B","text":"大事な物だったのですね。見つかって良かったです。"},{"id":{"d":186,"n":5,"t":2},"speaker":"A","text":"はい。届けてくれた方に、お礼を言いたいです。"}];
INSERT INTO sentence [{"id":{"d":187,"n":1,"t":2},"speaker":"A","text":"ああ、うっかりしていました。"},{"id":{"d":187,"n":2,"t":2},"speaker":"B","text":"どうしましたか？"},{"id":{"d":187,"n":3,"t":2},"speaker":"A","text":"今日が提出日の課題を、自宅から持ってくるのを忘れてしまいました。"},{"id":{"d":187,"n":4,"t":2},"speaker":"B","text":"それは大変です。今から家に取りに帰ったら辛うじて間に合うのではないでしょうか？"},{"id":{"d":187,"n":5,"t":2},"speaker":"A","text":"確かに、急げば間に合うかもしれません。"},{"id":{"d":187,"n":6,"t":2},"speaker":"B","text":"荷物は、私が学校に持って行きます。急いで、自宅へ取りに戻ってください。"},{"id":{"d":187,"n":7,"t":2},"speaker":"A","text":"すみません、ありがとうございます。では学校で。"}];
INSERT INTO sentence [{"id":{"d":188,"n":1,"t":2},"speaker":"A","text":"とても、たくさんの雪が降っていますね。"},{"id":{"d":188,"n":2,"t":2},"speaker":"B","text":"そうですね、テレビ番組に出ていた気象予報士はここまで降るとは言っていなかったのですが。"},{"id":{"d":188,"n":3,"t":2},"speaker":"A","text":"これだけ降ると、雪合戦がしたくなりますね。かまくらも作れそうです。"},{"id":{"d":188,"n":4,"t":2},"speaker":"B","text":"そうですね。雪遊びがたくさんできそうです。"},{"id":{"d":188,"n":5,"t":2},"speaker":"A","text":"早速、今から雪遊びをしましょう。"},{"id":{"d":188,"n":6,"t":2},"speaker":"B","text":"はい。着替えてきます。"}];
INSERT INTO sentence [{"id":{"d":189,"n":1,"t":2},"speaker":"A","text":"加藤さん、宿題のプリントを配ってくれますか？"},{"id":{"d":189,"n":2,"t":2},"speaker":"B","text":"はい。1枚ずつで良いでしょうか？"},{"id":{"d":189,"n":3,"t":2},"speaker":"A","text":"2種類あるので、1枚ずつ配ってください。"},{"id":{"d":189,"n":4,"t":2},"speaker":"B","text":"分かりました。それでは帰りのホームルームの時にみんなに配りますね。"},{"id":{"d":189,"n":5,"t":2},"speaker":"A","text":"よろしくお願いします。宿題の期限は明日まで。という事も伝えてくださいね。"},{"id":{"d":189,"n":6,"t":2},"speaker":"B","text":"はい。分かりました。"}];
INSERT INTO sentence [{"id":{"d":190,"n":1,"t":2},"speaker":"A","text":"今日の給食は、クリスマス献立です。"},{"id":{"d":190,"n":2,"t":2},"speaker":"B","text":"クリスマス献立のメニューは何ですか？"},{"id":{"d":190,"n":3,"t":2},"speaker":"A","text":"献立には、鶏の唐揚げとショートケーキが毎年出ますが、今年はチョコレートケーキと聞きました。"},{"id":{"d":190,"n":4,"t":2},"speaker":"B","text":"本当ですか？私はショートケーキよりもチョコレートケーキの方が好きなので、うれしいです。"},{"id":{"d":190,"n":5,"t":2},"speaker":"A","text":"給食の時間が、待ち遠しいですね。"}];
INSERT INTO sentence [{"id":{"d":191,"n":1,"t":2},"speaker":"A","text":"いつも陰から支えてくださっている用務員さんに、感謝を伝えたいです。"},{"id":{"d":191,"n":2,"t":2},"speaker":"B","text":"良いアイデアですね。どのような感謝を伝えましょうか？"},{"id":{"d":191,"n":3,"t":2},"speaker":"A","text":"美化委員の人と話がついていて、裏の花壇の花を少し頂けることになりました。"},{"id":{"d":191,"n":4,"t":2},"speaker":"B","text":"それはうれしいですね。いつ渡しますか？"},{"id":{"d":191,"n":5,"t":2},"speaker":"A","text":"今日の放課後にでも、感謝の言葉とともにお渡ししようと思っています。"}];
INSERT INTO sentence [{"id":{"d":192,"n":1,"t":2},"speaker":"A","text":"今回の期末試験の自己採点をしました。全体的には、ある程度の点数が取れているようで一安心です。"},{"id":{"d":192,"n":2,"t":2},"speaker":"B","text":"おお、すごいですね。私も平均点は取れていると思います。"},{"id":{"d":192,"n":3,"t":2},"speaker":"A","text":"私が特に良かったのは数学で、国語はいまひとつです。"},{"id":{"d":192,"n":4,"t":2},"speaker":"B","text":"私は歴史がよくできたのですが、あなたと同じく、国語があまり良くありません。赤点は免れていると思いますが、自己採点をしたところ、あまり良い点数ではありませんでした。"},{"id":{"d":192,"n":5,"t":2},"speaker":"A","text":"国語は難しいですよね。「登場人物の気持ちになって答えよ」という問いが、私は苦手です。"},{"id":{"d":192,"n":6,"t":2},"speaker":"B","text":"私も同じく苦手です。"},{"id":{"d":192,"n":7,"t":2},"speaker":"A","text":"次のテストに向けて、集中的に勉強する問題が見つかりましたね。"},{"id":{"d":192,"n":8,"t":2},"speaker":"B","text":"そうですね。それとは別に、今回のテストで良い点が取れていたらいいなと思います。"}];
INSERT INTO sentence [{"id":{"d":193,"n":1,"t":2},"speaker":"A","text":"図書館で本を借りるには、どうしたらよいでしょうか？"},{"id":{"d":193,"n":2,"t":2},"speaker":"B","text":"昼休みは図書係の人がカウンターにいるので、その人のところへ借りたい本を持っていけばよいです。"},{"id":{"d":193,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。1度に何冊まで貸していただけるのでしょうか？"},{"id":{"d":193,"n":4,"t":2},"speaker":"B","text":"1度に5冊までで、貸出期間は2週間です。"},{"id":{"d":193,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。早速、今日の昼休みに図書室に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":194,"n":1,"t":2},"speaker":"A","text":"校長先生、体育の授業で使うバスケットボールが足りないので、購入していただけないでしょうか？"},{"id":{"d":194,"n":2,"t":2},"speaker":"B","text":"分かりました。あとどのくらい必要ですか？"},{"id":{"d":194,"n":3,"t":2},"speaker":"A","text":"生徒20人分のボールを用意したいので、最低でもあと5つは必要です。"},{"id":{"d":194,"n":4,"t":2},"speaker":"B","text":"分かりました。ちょうど明日、フタバスポーツの川田さんが来校する予定なので注文しておきます。"},{"id":{"d":194,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。スポーツの備品はフタバスポーツさんに注文しているのですか？"},{"id":{"d":194,"n":6,"t":2},"speaker":"B","text":"そうですよ。いつもフタバスポーツさんに注文しています。"},{"id":{"d":194,"n":7,"t":2},"speaker":"A","text":"そうなのですね。では、よろしくお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":195,"n":1,"t":2},"speaker":"A","text":"先生、教室のヒーターが、止まってしまいました。"},{"id":{"d":195,"n":2,"t":2},"speaker":"B","text":"それは困りましたね。予備のヒーターを手配しますので、しばらく教室で待っていてください。"},{"id":{"d":195,"n":3,"t":2},"speaker":"A","text":"はい。"},{"id":{"d":195,"n":4,"t":2},"speaker":"B","text":"もう少ししたら、教頭先生が予備のヒーターを教室まで持ってきてくださいます。このまま授業を続けます。"},{"id":{"d":195,"n":5,"t":2},"speaker":"A","text":"分かりました。早く暖まりたいです。"}];
INSERT INTO sentence [{"id":{"d":196,"n":1,"t":2},"speaker":"A","text":"学校付近で、不審者の目撃情報がありました。"},{"id":{"d":196,"n":2,"t":2},"speaker":"B","text":"どの辺で、不審者が目撃されたのですか？"},{"id":{"d":196,"n":3,"t":2},"speaker":"A","text":"駅近くのコンビニエンスストアです。"},{"id":{"d":196,"n":4,"t":2},"speaker":"B","text":"警察には、連絡をしましたか？"},{"id":{"d":196,"n":5,"t":2},"speaker":"A","text":"連絡はしてあります。登下校の際、お子さまが少人数または1人きりにならないよう、保護者の方もご協力ください。"},{"id":{"d":196,"n":6,"t":2},"speaker":"B","text":"分かりました。登下校の時間に合わせて、保護者がパトロールをするようにします。"},{"id":{"d":196,"n":7,"t":2},"speaker":"A","text":"ご協力いただきありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":197,"n":1,"t":2},"speaker":"A","text":"来週からは体育の授業で縄跳びをします。皆さん、各自縄跳びの縄の準備をお願いします。"},{"id":{"d":197,"n":2,"t":2},"speaker":"B","text":"分かりました。縄の色の指定はありますか？"},{"id":{"d":197,"n":3,"t":2},"speaker":"A","text":"特にありません。縄の長さも、あらかじめ調整しておいていただけると助かります。"},{"id":{"d":197,"n":4,"t":2},"speaker":"B","text":"ちなみに、来週の何曜日から縄跳びの縄を使いますか？"},{"id":{"d":197,"n":5,"t":2},"speaker":"A","text":"水曜日です。準備が出来次第、早めに幼稚園に持ってきていただいて構いませんよ。"},{"id":{"d":197,"n":6,"t":2},"speaker":"B","text":"分かりました。準備しておきます。"}];
INSERT INTO sentence [{"id":{"d":198,"n":1,"t":2},"speaker":"A","text":"明日、席替えをします。"},{"id":{"d":198,"n":2,"t":2},"speaker":"B","text":"先生が、席順を決めるのですか？"},{"id":{"d":198,"n":3,"t":2},"speaker":"A","text":"これまではそうしていましたが、今回は違います。"},{"id":{"d":198,"n":4,"t":2},"speaker":"B","text":"では、どのように席順を決めるのですか？"},{"id":{"d":198,"n":5,"t":2},"speaker":"A","text":"くじ引きにしようと思います。"},{"id":{"d":198,"n":6,"t":2},"speaker":"B","text":"公平でいいですね。くじを作るのは、誰ですか？"},{"id":{"d":198,"n":7,"t":2},"speaker":"A","text":"学級委員の2人にお願いしたいです。"}];
INSERT INTO sentence [{"id":{"d":199,"n":1,"t":2},"speaker":"A","text":"これからプリントを配るので、1枚ずつ取って後ろの人に渡してください。"},{"id":{"d":199,"n":2,"t":2},"speaker":"B","text":"先生、1枚余りました。"},{"id":{"d":199,"n":3,"t":2},"speaker":"A","text":"人数分しかないはずです。おかしいですね。"},{"id":{"d":199,"n":4,"t":2},"speaker":"B","text":"でも、もらっていない人はいないようです。"},{"id":{"d":199,"n":5,"t":2},"speaker":"A","text":"今日は、全員出席していますか？"},{"id":{"d":199,"n":6,"t":2},"speaker":"B","text":"渡辺さんが、お休みです。"},{"id":{"d":199,"n":7,"t":2},"speaker":"A","text":"では、その余った1枚は渡辺さんの分ですね。"},{"id":{"d":199,"n":8,"t":2},"speaker":"B","text":"僕がこのまま預かって、明日渡辺さんに渡します。"},{"id":{"d":199,"n":9,"t":2},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":200,"n":1,"t":2},"speaker":"A","text":"この班の班長を決めましょう。"},{"id":{"d":200,"n":2,"t":2},"speaker":"B","text":"まずは、立候補を募りますか？"},{"id":{"d":200,"n":3,"t":2},"speaker":"A","text":"そうですね。班長に立候補する人はいますか？"},{"id":{"d":200,"n":4,"t":2},"speaker":"B","text":"いないようです。"},{"id":{"d":200,"n":5,"t":2},"speaker":"A","text":"それでは、どのように班長を決めましょうか？"},{"id":{"d":200,"n":6,"t":2},"speaker":"B","text":"班長にふさわしいと思う人を、紙に書いて投票するという方法はどうでしょう？"},{"id":{"d":200,"n":7,"t":2},"speaker":"A","text":"一番多く票を集めた人が、班長になるということですね。"},{"id":{"d":200,"n":8,"t":2},"speaker":"B","text":"はい。みんなが賛成なら、この方法がいいと思います。"},{"id":{"d":200,"n":9,"t":2},"speaker":"A","text":"みんなの意見を聞いてみましょう。"}];
INSERT INTO sentence [{"id":{"d":201,"n":1,"t":2},"speaker":"A","text":"あなたの息子さんは、どの幼稚園に通っていますか？"},{"id":{"d":201,"n":2,"t":2},"speaker":"B","text":"けやき幼稚園です。"},{"id":{"d":201,"n":3,"t":2},"speaker":"A","text":"私の息子を4月から幼稚園に通わせようと思っているので、けやき幼稚園のことを教えてもらえますか？"},{"id":{"d":201,"n":4,"t":2},"speaker":"B","text":"もちろんよいですよ。けやき幼稚園は、外遊びの時間が多いです。"},{"id":{"d":201,"n":5,"t":2},"speaker":"A","text":"園庭で遊ぶのですか？"},{"id":{"d":201,"n":6,"t":2},"speaker":"B","text":"園庭はもちろん、近くの公園に行って遊ぶことも多いです。"},{"id":{"d":201,"n":7,"t":2},"speaker":"A","text":"うちの息子は活発なので、そういう幼稚園が合っていそうです。"},{"id":{"d":201,"n":8,"t":2},"speaker":"B","text":"先生も一緒になって、鬼ごっこやかくれんぼをして走り回っているようです。"},{"id":{"d":201,"n":9,"t":2},"speaker":"A","text":"聞いているだけでも楽しそうです。息子が喜ぶ姿が想像できます。"},{"id":{"d":201,"n":10,"t":2},"speaker":"B","text":"来月、見学会があるので参加してはどうですか？"},{"id":{"d":201,"n":11,"t":2},"speaker":"A","text":"ぜひ、参加したいです。"}];
INSERT INTO sentence [{"id":{"d":202,"n":1,"t":2},"speaker":"A","text":"今度、大学時代のサークル仲間で集まることになりました。"},{"id":{"d":202,"n":2,"t":2},"speaker":"B","text":"どのようなサークルに入っていたのですか？"},{"id":{"d":202,"n":3,"t":2},"speaker":"A","text":"テニスサークルです。"},{"id":{"d":202,"n":4,"t":2},"speaker":"B","text":"それでテニスが上手なのですね。"},{"id":{"d":202,"n":5,"t":2},"speaker":"A","text":"練習が厳しいサークルだったので大変でしたが、おかげでテニスは上達しました。"},{"id":{"d":202,"n":6,"t":2},"speaker":"B","text":"当時の仲間で集まって、テニスをするのですか？"},{"id":{"d":202,"n":7,"t":2},"speaker":"A","text":"はい。社会人になるとなかなかテニスをする機会がないので、ラケットを握ることさえ久しぶりです。"},{"id":{"d":202,"n":8,"t":2},"speaker":"B","text":"楽しんできてくださいね。"},{"id":{"d":202,"n":9,"t":2},"speaker":"A","text":"腕が落ちていないか心配ですが、みんなに会えるだけでもうれしいです。"}];
INSERT INTO sentence [{"id":{"d":203,"n":1,"t":2},"speaker":"A","text":"絵里さん、一緒に帰りませんか？"},{"id":{"d":203,"n":2,"t":2},"speaker":"B","text":"あれ、小春さん。今日は、陸上部の練習があるのではありませんか？"},{"id":{"d":203,"n":3,"t":2},"speaker":"A","text":"雨が降ってきたので、中止になったのです。"},{"id":{"d":203,"n":4,"t":2},"speaker":"B","text":"そうですか。では、一緒に帰りましょう。"},{"id":{"d":203,"n":5,"t":2},"speaker":"A","text":"傘はありますか？"},{"id":{"d":203,"n":6,"t":2},"speaker":"B","text":"ロッカーに折り畳み傘が置いてあります。取って来るので、待っていてもらえますか？"},{"id":{"d":203,"n":7,"t":2},"speaker":"A","text":"昇降口で待っています。"},{"id":{"d":203,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。すぐに戻ります。"}];
INSERT INTO sentence [{"id":{"d":204,"n":1,"t":2},"speaker":"A","text":"今日は転入生を紹介します。南山中学から、転校してきた宮川俊介君です。"},{"id":{"d":204,"n":2,"t":2},"speaker":"B","text":"宮川俊介です。よろしくお願いします。"},{"id":{"d":204,"n":3,"t":2},"speaker":"A","text":"では、宮川君は廊下側の列の一番後ろの席に座ってください。"},{"id":{"d":204,"n":4,"t":2},"speaker":"B","text":"はい。荷物はロッカーに入れてよいですか？"},{"id":{"d":204,"n":5,"t":2},"speaker":"A","text":"そうですね。一番右端のロッカーが空いているので、そこを使ってください。"},{"id":{"d":204,"n":6,"t":2},"speaker":"B","text":"分かりました。"},{"id":{"d":204,"n":7,"t":2},"speaker":"A","text":"それでは授業を始めます。そういえば、宮川君は、まだ教科書が届いていなかったですね。"},{"id":{"d":204,"n":8,"t":2},"speaker":"B","text":"はい。まだ受け取っていません。"},{"id":{"d":204,"n":9,"t":2},"speaker":"A","text":"明日届く予定なので、今日は隣の席の磯野さんに教科書を見せてもらってください。"},{"id":{"d":204,"n":10,"t":2},"speaker":"B","text":"はい。磯野さん、すみませんが、お願いします。"}];
INSERT INTO sentence [{"id":{"d":205,"n":1,"t":2},"speaker":"A","text":"今日は、体育があるので気が重いです。"},{"id":{"d":205,"n":2,"t":2},"speaker":"B","text":"いつも体育があると喜んでいるのに、どうしたのですか？"},{"id":{"d":205,"n":3,"t":2},"speaker":"A","text":"体育は基本的に大好きなのですが、唯一、跳び箱だけは大の苦手で好きになれないのです。"},{"id":{"d":205,"n":4,"t":2},"speaker":"B","text":"今日から跳び箱でしたね。それで気が重いのですか？"},{"id":{"d":205,"n":5,"t":2},"speaker":"A","text":"跳び箱を見ると怖くなってしまって、どうしても跳び越せないのです。"},{"id":{"d":205,"n":6,"t":2},"speaker":"B","text":"怖くなってしまう、きっかけがあったのですか？"},{"id":{"d":205,"n":7,"t":2},"speaker":"A","text":"小学校低学年の頃、跳び箱を跳ぼうとして、腕を痛めたことがあるのですが、それがきっかけかもしれません。"},{"id":{"d":205,"n":8,"t":2},"speaker":"B","text":"それは怖くなってしまいますね。体育の先生に、相談してみてはどうでしょう？"},{"id":{"d":205,"n":9,"t":2},"speaker":"A","text":"先生から、何かアドバイスをいただけるかもしれませんね。後で話してみます。"}];
INSERT INTO sentence [{"id":{"d":206,"n":1,"t":2},"speaker":"A","text":"先生、美術の授業で今描いている絵の提出期限は、いつでしょうか？"},{"id":{"d":206,"n":2,"t":2},"speaker":"B","text":"来週の美術の授業が終わったら、提出してもらおうと考えています。"},{"id":{"d":206,"n":3,"t":2},"speaker":"A","text":"つまり今週と来週の授業で、絵を仕上げなければいけないということになりますね？"},{"id":{"d":206,"n":4,"t":2},"speaker":"B","text":"そうなりますね。どうかしましたか？"},{"id":{"d":206,"n":5,"t":2},"speaker":"A","text":"私はまだ色塗りを始めたばかりで、とても仕上げられそうにないのです。"},{"id":{"d":206,"n":6,"t":2},"speaker":"B","text":"下絵に時間がかかってしまったのですね？"},{"id":{"d":206,"n":7,"t":2},"speaker":"A","text":"はい。放課後に残って、絵を描いてもよいでしょうか？"},{"id":{"d":206,"n":8,"t":2},"speaker":"B","text":"美術室は美術部が使うと思うので、美術部の顧問の先生と相談してみます。"}];
INSERT INTO sentence [{"id":{"d":207,"n":1,"t":2},"speaker":"A","text":"顔色が良くありませんが、大丈夫ですか？"},{"id":{"d":207,"n":2,"t":2},"speaker":"B","text":"少し気分が悪いです。"},{"id":{"d":207,"n":3,"t":2},"speaker":"A","text":"朝から体調が悪かったのですか？"},{"id":{"d":207,"n":4,"t":2},"speaker":"B","text":"いいえ、朝は普段通りでしたが、昼過ぎから急に気分が悪くなってきました。"},{"id":{"d":207,"n":5,"t":2},"speaker":"A","text":"保健室で休んだ方が良さそうですね？"},{"id":{"d":207,"n":6,"t":2},"speaker":"B","text":"はい。少し横になりたいです。"},{"id":{"d":207,"n":7,"t":2},"speaker":"A","text":"保健室で様子を見て、回復しないようなら無理しないで早退しましょう。"},{"id":{"d":207,"n":8,"t":2},"speaker":"B","text":"分かりました。取りあえず保健室で休ませてもらいます。"}];
INSERT INTO sentence [{"id":{"d":208,"n":1,"t":2},"speaker":"A","text":"いよいよ、イギリス留学への出発が近づいてきましたね。"},{"id":{"d":208,"n":2,"t":2},"speaker":"B","text":"はい。私は、緊張と不安が混ざったような気分です。"},{"id":{"d":208,"n":3,"t":2},"speaker":"A","text":"野中さんならきっとすぐに慣れて、友達もたくさんできると思いますよ。"},{"id":{"d":208,"n":4,"t":2},"speaker":"B","text":"そうだと良いのですが。まずは勉強についていけるかどうかも心配です。"},{"id":{"d":208,"n":5,"t":2},"speaker":"A","text":"留学生のためのフォロー体制が整っていると、先生もおっしゃっていたので、きっと大丈夫ですよ。"},{"id":{"d":208,"n":6,"t":2},"speaker":"B","text":"留学したらあなたとも、1年間会えなくなりますね。"},{"id":{"d":208,"n":7,"t":2},"speaker":"A","text":"寂しいので時々メールしますね。野中さんも近況を教えてください。"},{"id":{"d":208,"n":8,"t":2},"speaker":"B","text":"もちろんです。現地の写真も送りますね。"},{"id":{"d":208,"n":9,"t":2},"speaker":"A","text":"それは楽しみです。出発当日は、空港まで見送りに行きます。"},{"id":{"d":208,"n":10,"t":2},"speaker":"B","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":209,"n":1,"t":2},"speaker":"A","text":"今日は、宿題がたくさん出ましたね。"},{"id":{"d":209,"n":2,"t":2},"speaker":"B","text":"僕は、塾の宿題もたくさん抱えているので、全部できるか心配です。"},{"id":{"d":209,"n":3,"t":2},"speaker":"A","text":"塾の宿題は、毎回出されるのですか？"},{"id":{"d":209,"n":4,"t":2},"speaker":"B","text":"はい。それも毎回かなりの量が出されるので大変です。"},{"id":{"d":209,"n":5,"t":2},"speaker":"A","text":"塾へは、週に何回通っているのですか？"},{"id":{"d":209,"n":6,"t":2},"speaker":"B","text":"週に2回、火曜日と金曜日です。"},{"id":{"d":209,"n":7,"t":2},"speaker":"A","text":"ということは、前回はおとといですね。その時の宿題は終わりましたか？"},{"id":{"d":209,"n":8,"t":2},"speaker":"B","text":"それが、部活動が忙しくてまだ手付かずなのです。"},{"id":{"d":209,"n":9,"t":2},"speaker":"A","text":"今日は、夜遅くまで宿題をすることになりそうですね。"},{"id":{"d":209,"n":10,"t":2},"speaker":"B","text":"はい。でも、寝不足にならないよう、集中してなるべく短時間で終わらせたいです。"},{"id":{"d":209,"n":11,"t":2},"speaker":"A","text":"それがいいですね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":210,"n":1,"t":2},"speaker":"A","text":"2組は、合唱コンクールのピアノ伴奏者が決まりましたか？"},{"id":{"d":210,"n":2,"t":2},"speaker":"B","text":"はい。立候補した生徒がいて、すぐに決まりました。"},{"id":{"d":210,"n":3,"t":2},"speaker":"A","text":"早いですね。私のクラスは、まだ決まっていません。"},{"id":{"d":210,"n":4,"t":2},"speaker":"B","text":"ピアノを弾ける生徒がいないのですか？"},{"id":{"d":210,"n":5,"t":2},"speaker":"A","text":"弾ける人は何人かいるのですが、弾きたい人がいないのです。"},{"id":{"d":210,"n":6,"t":2},"speaker":"B","text":"そうですか。なぜ彼らは弾きたくないのでしょうか？"},{"id":{"d":210,"n":7,"t":2},"speaker":"A","text":"何人かは、楽譜を見て「難しそう」と言っていました。「弾くよりも歌いたい」と言っていた人もいました。"},{"id":{"d":210,"n":8,"t":2},"speaker":"B","text":"なるほど。では、もっと伴奏が簡単な曲に変更するのはどうでしょうか？"},{"id":{"d":210,"n":9,"t":2},"speaker":"A","text":"そうですね。クラスで相談してみます。"}];
INSERT INTO sentence [{"id":{"d":211,"n":1,"t":2},"speaker":"A","text":"次の調理実習の内容は、肉じゃがですね。"},{"id":{"d":211,"n":2,"t":2},"speaker":"B","text":"上手に作れるかどうか心配です。"},{"id":{"d":211,"n":3,"t":2},"speaker":"A","text":"家で料理をしませんか？"},{"id":{"d":211,"n":4,"t":2},"speaker":"B","text":"全くしません。母が料理好きで、任せきりになっています。"},{"id":{"d":211,"n":5,"t":2},"speaker":"A","text":"班のメンバーで協力して調理するので、大丈夫ですよ。"},{"id":{"d":211,"n":6,"t":2},"speaker":"B","text":"他のメンバーの、足手まといになってしまうかもしれません。"},{"id":{"d":211,"n":7,"t":2},"speaker":"A","text":"一度、家で練習しておくと良いかもしれませんね。"},{"id":{"d":211,"n":8,"t":2},"speaker":"B","text":"そうですね。母と一緒に作ってみます。"}];
INSERT INTO sentence [{"id":{"d":212,"n":1,"t":2},"speaker":"A","text":"今日の放課後、運動会の打ち合わせをしたいのですが、ご都合はいかがですか？"},{"id":{"d":212,"n":2,"t":2},"speaker":"B","text":"今日は保健委員の集まりがあるので、明日の放課後でもよいですか？"},{"id":{"d":212,"n":3,"t":2},"speaker":"A","text":"分かりました。それでは明日の放課後、職員室で打ち合わせをしましょう。"},{"id":{"d":212,"n":4,"t":2},"speaker":"B","text":"どのようなことを話し合うのですか？"},{"id":{"d":212,"n":5,"t":2},"speaker":"A","text":"学年ごとの種目を決めるので、どのようなものがいいか、考えておいてください。"},{"id":{"d":212,"n":6,"t":2},"speaker":"B","text":"分かりました。毎年、いろいろな種目を実施していますよね。"},{"id":{"d":212,"n":7,"t":2},"speaker":"A","text":"そうですね。過去数年間の種目をこの資料にまとめたので、良かったら参考にしてください。"},{"id":{"d":212,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":213,"n":1,"t":2},"speaker":"A","text":"もうすぐ、先輩たちが部活動を引退してしまいます。"},{"id":{"d":213,"n":2,"t":2},"speaker":"B","text":"先輩がいない部活動などは、想像できません。"},{"id":{"d":213,"n":3,"t":2},"speaker":"A","text":"ずっと一緒に練習してきたので寂しいですね？"},{"id":{"d":213,"n":4,"t":2},"speaker":"B","text":"はい。お世話になったお礼の気持ちを込めて、先輩に何かプレゼントしたいです。"},{"id":{"d":213,"n":5,"t":2},"speaker":"A","text":"みんなで色紙に寄せ書きをして、先輩に渡しましょうか？"},{"id":{"d":213,"n":6,"t":2},"speaker":"B","text":"いいですね。それに、全員の写真を添えたいです。"},{"id":{"d":213,"n":7,"t":2},"speaker":"A","text":"それでは、今度の練習の後に、全員で写真を撮りましょう。"},{"id":{"d":213,"n":8,"t":2},"speaker":"B","text":"私は、色紙を用意します。"},{"id":{"d":213,"n":9,"t":2},"speaker":"A","text":"先輩が、喜んでくれるといいですね。"}];
INSERT INTO sentence [{"id":{"d":214,"n":1,"t":2},"speaker":"A","text":"和馬君、ブロックで何を作っているのですか？"},{"id":{"d":214,"n":2,"t":2},"speaker":"B","text":"恐竜です。"},{"id":{"d":214,"n":3,"t":2},"speaker":"A","text":"上手にできましたね。"},{"id":{"d":214,"n":4,"t":2},"speaker":"B","text":"小さい恐竜も作りました。"},{"id":{"d":214,"n":5,"t":2},"speaker":"A","text":"恐竜の親子のようですね。"},{"id":{"d":214,"n":6,"t":2},"speaker":"B","text":"はい。恐竜のお父さんと赤ちゃんです。これから恐竜のお母さんも作ります。"},{"id":{"d":214,"n":7,"t":2},"speaker":"A","text":"ブロックが足りなさそうですね。あちらの箱のブロックも使ってよいですよ。"},{"id":{"d":214,"n":8,"t":2},"speaker":"B","text":"はい。できあがったら見てください。"},{"id":{"d":214,"n":9,"t":2},"speaker":"A","text":"もちろんです。楽しみにしていますよ。"}];
INSERT INTO sentence [{"id":{"d":215,"n":1,"t":2},"speaker":"A","text":"今日の昼食は、お弁当ですか？学生食堂を利用しますか？"},{"id":{"d":215,"n":2,"t":2},"speaker":"B","text":"今日は学生食堂で食事をします。あなたはどうしますか？"},{"id":{"d":215,"n":3,"t":2},"speaker":"A","text":"私も学生食堂で食事をします。"},{"id":{"d":215,"n":4,"t":2},"speaker":"B","text":"いつもお弁当なのに、珍しいですね。"},{"id":{"d":215,"n":5,"t":2},"speaker":"A","text":"母が出張で留守なので、今日はお弁当を持参していません。"},{"id":{"d":215,"n":6,"t":2},"speaker":"B","text":"そうですか。では、一緒に学生食堂に行きましょう。"},{"id":{"d":215,"n":7,"t":2},"speaker":"A","text":"学生食堂が混んでいないといいですね。"},{"id":{"d":215,"n":8,"t":2},"speaker":"B","text":"遅く行くと、席がないかもしれませんね。少し急ぎましょう。"}];
INSERT INTO sentence [{"id":{"d":216,"n":1,"t":2},"speaker":"A","text":"来月5名の学生が、教育実習に来ることになりました。"},{"id":{"d":216,"n":2,"t":2},"speaker":"B","text":"去年は2名でしたよね。今年は多いですね？"},{"id":{"d":216,"n":3,"t":2},"speaker":"A","text":"はい。そこで、高橋先生にも協力していただきたいです。"},{"id":{"d":216,"n":4,"t":2},"speaker":"B","text":"どのようなことでしょうか？"},{"id":{"d":216,"n":5,"t":2},"speaker":"A","text":"教育実習生の授業計画にアドバイスをしたり、実際の授業を見学して気付いたことを伝えてあげたりしてください。"},{"id":{"d":216,"n":6,"t":2},"speaker":"B","text":"5人全員に対してですか？"},{"id":{"d":216,"n":7,"t":2},"speaker":"A","text":"いえ、5人それぞれ別の先生にお願いする予定です。"},{"id":{"d":216,"n":8,"t":2},"speaker":"B","text":"分かりました。お引き受けします。"},{"id":{"d":216,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。それでは、担当していただく教育実習生が決まり次第お伝えしますね。"}];
INSERT INTO sentence [{"id":{"d":217,"n":1,"t":2},"speaker":"A","text":"近藤さんですか？お久しぶりです。"},{"id":{"d":217,"n":2,"t":2},"speaker":"B","text":"まあ、山口さん。本当にお久しぶりです。卒業式以来ですね。"},{"id":{"d":217,"n":3,"t":2},"speaker":"A","text":"ええ。卒業後はどうしていましたか？"},{"id":{"d":217,"n":4,"t":2},"speaker":"B","text":"卒業後、すぐに今の会社に就職して働いています。山口さんは、どうされていましたか？"},{"id":{"d":217,"n":5,"t":2},"speaker":"A","text":"私は、卒業後1年間アメリカに留学して、帰国後に就職しました。"},{"id":{"d":217,"n":6,"t":2},"speaker":"B","text":"そうだったのですね。それにしても、本当に懐かしいです。"},{"id":{"d":217,"n":7,"t":2},"speaker":"A","text":"本当ですね。そろそろ同窓会も行いたいです。"},{"id":{"d":217,"n":8,"t":2},"speaker":"B","text":"幹事に連絡を取ってみましょうか？"},{"id":{"d":217,"n":9,"t":2},"speaker":"A","text":"私は幹事の連絡先が分からないので、お願いしてもよいですか？"},{"id":{"d":217,"n":10,"t":2},"speaker":"B","text":"よいですよ。同窓会が実現するといいですね。"}];
INSERT INTO sentence [{"id":{"d":218,"n":1,"t":2},"speaker":"A","text":"今日は、雨が降りそうですか？"},{"id":{"d":218,"n":2,"t":2},"speaker":"B","text":"天気予報では、降水確率が50パーセントでした。"},{"id":{"d":218,"n":3,"t":2},"speaker":"A","text":"それでは、雨が降るかどうか分かりませんね。"},{"id":{"d":218,"n":4,"t":2},"speaker":"B","text":"雨だと、何か影響があるのですか？"},{"id":{"d":218,"n":5,"t":2},"speaker":"A","text":"体育の先生から、「晴れなら水泳の道具を、雨なら体操服を持ってくるように」と言われたのです。"},{"id":{"d":218,"n":6,"t":2},"speaker":"B","text":"なるほど、それは判断が難しいですね。"},{"id":{"d":218,"n":7,"t":2},"speaker":"A","text":"仕方がないので、両方持っていきます。"},{"id":{"d":218,"n":8,"t":2},"speaker":"B","text":"荷物が多くなって大変ですね。"}];
INSERT INTO sentence [{"id":{"d":219,"n":1,"t":2},"speaker":"A","text":"今日の数学のテストはどうでしたか？"},{"id":{"d":219,"n":2,"t":2},"speaker":"B","text":"時間が足りなくて、焦ってしまいました。"},{"id":{"d":219,"n":3,"t":2},"speaker":"A","text":"最後の問題まで解けましたか？"},{"id":{"d":219,"n":4,"t":2},"speaker":"B","text":"一応解けましたが、見直しをする時間はありませんでした。"},{"id":{"d":219,"n":5,"t":2},"speaker":"A","text":"何か難しい問題があったのですか？"},{"id":{"d":219,"n":6,"t":2},"speaker":"B","text":"図形の問題が、とても難しくて時間を取られてしまいました。"},{"id":{"d":219,"n":7,"t":2},"speaker":"A","text":"どのような問題でしょうか？"},{"id":{"d":219,"n":8,"t":2},"speaker":"B","text":"問題用紙を回収されてしまったので、思い出せないのですが、返却されたら復習します。"}];
INSERT INTO sentence [{"id":{"d":220,"n":1,"t":2},"speaker":"A","text":"今日学校に、テレビ番組の取材の人が来ました。"},{"id":{"d":220,"n":2,"t":2},"speaker":"B","text":"学校で何かあったのですか？"},{"id":{"d":220,"n":3,"t":2},"speaker":"A","text":"オリンピックに出場した、石橋選手がいらしたのです。"},{"id":{"d":220,"n":4,"t":2},"speaker":"B","text":"確か石、橋選手はあなたの学校の卒業生ですよね。"},{"id":{"d":220,"n":5,"t":2},"speaker":"A","text":"はい。自慢の先輩です。"},{"id":{"d":220,"n":6,"t":2},"speaker":"B","text":"母校にいらしたというわけですね。"},{"id":{"d":220,"n":7,"t":2},"speaker":"A","text":"オリンピックで銅メダルを獲得したので、その報告にいらしたようです。"},{"id":{"d":220,"n":8,"t":2},"speaker":"B","text":"すごいですね。テレビ番組で放送されるのはいつですか？"},{"id":{"d":220,"n":9,"t":2},"speaker":"A","text":"今夜9時からです。ニュース番組のスポーツコーナーで特集されるそうです。"},{"id":{"d":220,"n":10,"t":2},"speaker":"B","text":"その番組を録画しておきましょう。"}];
INSERT INTO sentence [{"id":{"d":221,"n":1,"t":2},"speaker":"A","text":"高校時代は何か部活動に入っていましたか？"},{"id":{"d":221,"n":2,"t":2},"speaker":"B","text":"私は、サッカー部でした。岡村さんは、何部でしたか？"},{"id":{"d":221,"n":3,"t":2},"speaker":"A","text":"私は野球部でしたが、2年生の夏に退部しました。"},{"id":{"d":221,"n":4,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":221,"n":5,"t":2},"speaker":"A","text":"ピッチャーだったのですが、肘をけがしてしまったのです。"},{"id":{"d":221,"n":6,"t":2},"speaker":"B","text":"それは悔しいですね。"},{"id":{"d":221,"n":7,"t":2},"speaker":"A","text":"部活動が続けられなかった悔しさを、勉強を頑張ることで紛らわしていました。"},{"id":{"d":221,"n":8,"t":2},"speaker":"B","text":"なるほど。私は引退まで続けましたが、勉強との両立が難しかったです。"}];
INSERT INTO sentence [{"id":{"d":222,"n":1,"t":2},"speaker":"A","text":"今日は、学校で「新1年生を迎える会」がありました。"},{"id":{"d":222,"n":2,"t":2},"speaker":"B","text":"会では、どのようなことをしたのですか？"},{"id":{"d":222,"n":3,"t":2},"speaker":"A","text":"校歌を歌ったり、学校紹介をしたりしました。"},{"id":{"d":222,"n":4,"t":2},"speaker":"B","text":"あなたが1年生だった時のことを、思い出したのではありませんか？"},{"id":{"d":222,"n":5,"t":2},"speaker":"A","text":"そうですね。そのような会を先輩方に実施してもらったことはぼんやりと覚えています。"},{"id":{"d":222,"n":6,"t":2},"speaker":"B","text":"今度は迎える立場なので、頼りになる存在になりたいですね。"},{"id":{"d":222,"n":7,"t":2},"speaker":"A","text":"はい。上級生としての実感が湧きました。"}];
INSERT INTO sentence [{"id":{"d":223,"n":1,"t":2},"speaker":"A","text":"中学校は、徒歩通学でしたか？"},{"id":{"d":223,"n":2,"t":2},"speaker":"B","text":"いえ、自転車通学でした。"},{"id":{"d":223,"n":3,"t":2},"speaker":"A","text":"そうなのですか。私の中学校では、全員が徒歩通学でした。"},{"id":{"d":223,"n":4,"t":2},"speaker":"B","text":"学区がとても広いので、遠くから通学する生徒は、自転車で通っていました。"},{"id":{"d":223,"n":5,"t":2},"speaker":"A","text":"雨の日も自転車通学ですか？"},{"id":{"d":223,"n":6,"t":2},"speaker":"B","text":"はい。レインコートを着ましたが、制服がぬれてしまうことも多かったです。"},{"id":{"d":223,"n":7,"t":2},"speaker":"A","text":"冬の寒さもつらそうです。"},{"id":{"d":223,"n":8,"t":2},"speaker":"B","text":"厚いコートにマフラー、手袋とできる限り防寒していました。"},{"id":{"d":223,"n":9,"t":2},"speaker":"A","text":"よく頑張りましたね。"},{"id":{"d":223,"n":10,"t":2},"speaker":"B","text":"若かったので、意外と大丈夫でしたよ。今となっては、懐かしい思い出です。"}];
INSERT INTO sentence [{"id":{"d":224,"n":1,"t":2},"speaker":"A","text":"これは、昇君のジャージですよね。"},{"id":{"d":224,"n":2,"t":2},"speaker":"B","text":"そうです。どうして健太君が持っているのですか？"},{"id":{"d":224,"n":3,"t":2},"speaker":"A","text":"昨日、部室で着替えをした時に、間違えて昇君のジャージを着てしまったようです。"},{"id":{"d":224,"n":4,"t":2},"speaker":"B","text":"それでは、私は健太君のジャージを着てしまったのですね。"},{"id":{"d":224,"n":5,"t":2},"speaker":"A","text":"多分そうだと思います。部活動の後はジャージを着たまま帰り、帰宅後ジャージを脱いだ時にラベルに昇君の名前が書いてあることに気が付きました。"},{"id":{"d":224,"n":6,"t":2},"speaker":"B","text":"私は、全然気が付きませんでした。"},{"id":{"d":224,"n":7,"t":2},"speaker":"A","text":"同じサイズだし仕方ありませんよ。このジャージは、洗濯したので返しますね。"},{"id":{"d":224,"n":8,"t":2},"speaker":"B","text":"ありがとう。明日、忘れずに健太君のジャージを持ってきます。"}];
INSERT INTO sentence [{"id":{"d":225,"n":1,"t":2},"speaker":"A","text":"後期は、どの委員会に所属を希望するか決めましたか？"},{"id":{"d":225,"n":2,"t":2},"speaker":"B","text":"図書委員会と放送委員会で、どちらにするか迷っています。"},{"id":{"d":225,"n":3,"t":2},"speaker":"A","text":"私は前期に放送委員に所属していましたが、お薦めですよ。"},{"id":{"d":225,"n":4,"t":2},"speaker":"B","text":"放送委員会は人気があるので、所属希望者が多いと思うのです。"},{"id":{"d":225,"n":5,"t":2},"speaker":"A","text":"確かにそうですね。前期の委員会の所属を決める時も、一番希望者が多かったです。"},{"id":{"d":225,"n":6,"t":2},"speaker":"B","text":"公平に、じゃんけんで決めようということになっていましたよね。"},{"id":{"d":225,"n":7,"t":2},"speaker":"A","text":"はい。私は、運良くじゃんけんに勝ちました。"},{"id":{"d":225,"n":8,"t":2},"speaker":"B","text":"私はじゃんけんに勝つ自信がないので、図書委員会への所属を希望しようと思います。"}];
INSERT INTO sentence [{"id":{"d":226,"n":1,"t":2},"speaker":"A","text":"お母さん、明日は授業参観ですが、来てくれますよね？"},{"id":{"d":226,"n":2,"t":2},"speaker":"B","text":"はい。会社には有給休暇を申請しました。"},{"id":{"d":226,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。見に来てもらうのは、英語の授業です。"},{"id":{"d":226,"n":4,"t":2},"speaker":"B","text":"あなたの得意科目ですね。教室はどこですか？"},{"id":{"d":226,"n":5,"t":2},"speaker":"A","text":"南棟の3階、音楽室の隣です。"},{"id":{"d":226,"n":6,"t":2},"speaker":"B","text":"昇降口は本館にあるから、渡り廊下で南棟へ行けばよいのですね？"},{"id":{"d":226,"n":7,"t":2},"speaker":"A","text":"そうです。渡り廊下は本館の2階にあります。"},{"id":{"d":226,"n":8,"t":2},"speaker":"B","text":"あなたの席は、教室のどのあたりですか？"},{"id":{"d":226,"n":9,"t":2},"speaker":"A","text":"窓際の列の前から2番目です。"},{"id":{"d":226,"n":10,"t":2},"speaker":"B","text":"それでは、窓際に立って参観しますね。"}];
INSERT INTO sentence [{"id":{"d":227,"n":1,"t":2},"speaker":"A","text":"日直の人、来てください。"},{"id":{"d":227,"n":2,"t":2},"speaker":"B","text":"何でしょうか？"},{"id":{"d":227,"n":3,"t":2},"speaker":"A","text":"次の授業でプロジェクターを使うので、教室へ運んでおいてくれますか？"},{"id":{"d":227,"n":4,"t":2},"speaker":"B","text":"分かりました。プロジェクターは理科室にありますよね？"},{"id":{"d":227,"n":5,"t":2},"speaker":"A","text":"そうです。先に職員室へ行って、理科室の鍵を取りに行ってください。"},{"id":{"d":227,"n":6,"t":2},"speaker":"B","text":"理科室の鍵は、職員室のどこにありますか？"},{"id":{"d":227,"n":7,"t":2},"speaker":"A","text":"大山先生が鍵の管理をしているので、用件を伝えれば鍵を渡してくれます。"},{"id":{"d":227,"n":8,"t":2},"speaker":"B","text":"それでは職員室へ行ってきます。"},{"id":{"d":227,"n":9,"t":2},"speaker":"A","text":"プロジェクターは、黒板の前まで運んでください。"},{"id":{"d":227,"n":10,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":228,"n":1,"t":2},"speaker":"A","text":"そろそろ受験シーズンですね。"},{"id":{"d":228,"n":2,"t":2},"speaker":"B","text":"そうですね。自分が受験した時のことを思い出します。"},{"id":{"d":228,"n":3,"t":2},"speaker":"A","text":"高校受験ですか？"},{"id":{"d":228,"n":4,"t":2},"speaker":"B","text":"いえ、私は私立の中学校を受験したのです。"},{"id":{"d":228,"n":5,"t":2},"speaker":"A","text":"そうでしたか。小学生で受験となると、とてつもない緊張や不安があったのではありませんか？"},{"id":{"d":228,"n":6,"t":2},"speaker":"B","text":"はい。受験当日は、朝ご飯が喉を通りませんでした。"},{"id":{"d":228,"n":7,"t":2},"speaker":"A","text":"お察しします。"},{"id":{"d":228,"n":8,"t":2},"speaker":"B","text":"母が受験会場まで付き添ってくれたのですが、途中でおなかが痛くなって心配をかけてしまいました。"},{"id":{"d":228,"n":9,"t":2},"speaker":"A","text":"それは大変でしたね。試験は大丈夫でしたか？"},{"id":{"d":228,"n":10,"t":2},"speaker":"B","text":"はい、かろうじてこなすことができました。無事、第一希望の中学校に合格できました。"}];
INSERT INTO sentence [{"id":{"d":229,"n":1,"t":2},"speaker":"A","text":"次の大会に向けて、部活動の練習メニューを見直しましょう。"},{"id":{"d":229,"n":2,"t":2},"speaker":"B","text":"賛成です。実戦形式の練習を増やした方がいいと思います。"},{"id":{"d":229,"n":3,"t":2},"speaker":"A","text":"私もそう思います。"},{"id":{"d":229,"n":4,"t":2},"speaker":"B","text":"他校と練習試合をするのはどうですか？"},{"id":{"d":229,"n":5,"t":2},"speaker":"A","text":"いいですね。顧問の先生に相談してみましょう。"},{"id":{"d":229,"n":6,"t":2},"speaker":"B","text":"あとは、筋力アップも必要ですね。"},{"id":{"d":229,"n":7,"t":2},"speaker":"A","text":"どういうトレーニングが、筋力アップに効果的なのでしょうか？"},{"id":{"d":229,"n":8,"t":2},"speaker":"B","text":"それについても、顧問の先生にアドバイスをもらいましょう。"}];
INSERT INTO sentence [{"id":{"d":230,"n":1,"t":2},"speaker":"A","text":"先生は、太平大学の出身ですよね？"},{"id":{"d":230,"n":2,"t":2},"speaker":"B","text":"そうですよ。"},{"id":{"d":230,"n":3,"t":2},"speaker":"A","text":"太平大学に興味があるので、いろいろと教えていただけますか？"},{"id":{"d":230,"n":4,"t":2},"speaker":"B","text":"構いませんよ。志望している学部は、どこですか？"},{"id":{"d":230,"n":5,"t":2},"speaker":"A","text":"社会学部です。"},{"id":{"d":230,"n":6,"t":2},"speaker":"B","text":"私は教育学部だったので、社会学部のことはあまり分かりませんが、大学全体のことなら答えられると思います。"},{"id":{"d":230,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。キャンパスの雰囲気などを教えてほしいです。"},{"id":{"d":230,"n":8,"t":2},"speaker":"B","text":"キャンパスはとても広くて、敷地内にテニスコートや野球場もあります。"},{"id":{"d":230,"n":9,"t":2},"speaker":"A","text":"サークル活動は盛んですか？"},{"id":{"d":230,"n":10,"t":2},"speaker":"B","text":"とても盛んです。他の大学と合同のサークルもありますよ。"},{"id":{"d":230,"n":11,"t":2},"speaker":"A","text":"それは魅力的ですね。"}];
INSERT INTO sentence [{"id":{"d":231,"n":1,"t":2},"speaker":"A","text":"明日の遠足について、何か質問はありますか？"},{"id":{"d":231,"n":2,"t":2},"speaker":"B","text":"先生、バスの席順はどうなりますか？"},{"id":{"d":231,"n":3,"t":2},"speaker":"A","text":"前から順に、班ごとに座ることにしましょう。"},{"id":{"d":231,"n":4,"t":2},"speaker":"B","text":"乗り物酔いする人は、前の方に座った方がいいと思います。"},{"id":{"d":231,"n":5,"t":2},"speaker":"A","text":"良い意見ですね。乗り物酔いが心配な人はいますか？"},{"id":{"d":231,"n":6,"t":2},"speaker":"B","text":"今日、休んでいる加藤君は、乗り物酔いすると聞いたことがあります。"},{"id":{"d":231,"n":7,"t":2},"speaker":"A","text":"そうですか。では、加藤君に前に座りたいか聞いてみましょう。"}];
INSERT INTO sentence [{"id":{"d":232,"n":1,"t":2},"speaker":"A","text":"修学旅行が楽しみです。"},{"id":{"d":232,"n":2,"t":2},"speaker":"B","text":"そうですね。班行動の時間はどこへ行くか決まりましたか？"},{"id":{"d":232,"n":3,"t":2},"speaker":"A","text":"班のメンバーで話し合っているのですが、なかなかまとまりません。"},{"id":{"d":232,"n":4,"t":2},"speaker":"B","text":"それぞれに希望があるから、まとめるのは難しいですよね。"},{"id":{"d":232,"n":5,"t":2},"speaker":"A","text":"私は、班長なので、どのようにして意見を調整するか悩んでいます。"},{"id":{"d":232,"n":6,"t":2},"speaker":"B","text":"どのような意見が、出ているのですか？"},{"id":{"d":232,"n":7,"t":2},"speaker":"A","text":"寺院を巡りたいという意見と、景色の良い場所を散策したいという意見です。"},{"id":{"d":232,"n":8,"t":2},"speaker":"B","text":"両方に行くことは、できないのですか？"},{"id":{"d":232,"n":9,"t":2},"speaker":"A","text":"時間が限られているので、両方行くのは無理なのです。"},{"id":{"d":232,"n":10,"t":2},"speaker":"B","text":"どうしても決まらないようなら、多数決で決めるのも良いかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":233,"n":1,"t":2},"speaker":"A","text":"先月から、自動車教習所に通っています。"},{"id":{"d":233,"n":2,"t":2},"speaker":"B","text":"教習は、順調に進んでいますか？"},{"id":{"d":233,"n":3,"t":2},"speaker":"A","text":"まだ、教習所内のコースを走っているだけですが、運転に慣れません。"},{"id":{"d":233,"n":4,"t":2},"speaker":"B","text":"最初は、緊張しますよね。"},{"id":{"d":233,"n":5,"t":2},"speaker":"A","text":"教官がいろいろと指導してくれるのですが、思うように運転できません。"},{"id":{"d":233,"n":6,"t":2},"speaker":"B","text":"学科は、どうですか？"},{"id":{"d":233,"n":7,"t":2},"speaker":"A","text":"覚えることがたくさんあって、驚いています。"},{"id":{"d":233,"n":8,"t":2},"speaker":"B","text":"交通ルールや道路標識などの数が多いですよね。"},{"id":{"d":233,"n":9,"t":2},"speaker":"A","text":"テキストを持ち歩いて、時間を見つけて覚えるようにしています。"},{"id":{"d":233,"n":10,"t":2},"speaker":"B","text":"頑張ってくださいね。運転免許を取ったら、一緒にドライブに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":234,"n":1,"t":2},"speaker":"A","text":"掃除の時間ですね。"},{"id":{"d":234,"n":2,"t":2},"speaker":"B","text":"手分けして、机の上を拭きましょう。"},{"id":{"d":234,"n":3,"t":2},"speaker":"A","text":"私は廊下側の机から拭いていくので、窓側の机をお願いできますか？"},{"id":{"d":234,"n":4,"t":2},"speaker":"B","text":"分かりました。"},{"id":{"d":234,"n":5,"t":2},"speaker":"A","text":"雑巾が、いつもの場所にありませんね。"},{"id":{"d":234,"n":6,"t":2},"speaker":"B","text":"昨日の掃除当番が、別の場所に置いてしまったのかもしれません。"},{"id":{"d":234,"n":7,"t":2},"speaker":"A","text":"昨日の掃除当番は、3班の人でしたね。"},{"id":{"d":234,"n":8,"t":2},"speaker":"B","text":"あちらに3班の人がいるので、雑巾のことを聞いてみます。"},{"id":{"d":234,"n":9,"t":2},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":235,"n":1,"t":2},"speaker":"A","text":"先生、質問があります。"},{"id":{"d":235,"n":2,"t":2},"speaker":"B","text":"はい、何の御用ですか？"},{"id":{"d":235,"n":3,"t":2},"speaker":"A","text":"この数学の問題の考え方が、分かりません。"},{"id":{"d":235,"n":4,"t":2},"speaker":"B","text":"少し見せてください。ああ、これは二次関数の問題ですね。"},{"id":{"d":235,"n":5,"t":2},"speaker":"A","text":"はい。途中までは分かったのですが、正解にたどり着けません。"},{"id":{"d":235,"n":6,"t":2},"speaker":"B","text":"分かったところまで、書いたものはありますか？"},{"id":{"d":235,"n":7,"t":2},"speaker":"A","text":"ノートのここに書いてあります。"},{"id":{"d":235,"n":8,"t":2},"speaker":"B","text":"これは惜しいですね。一緒に、考えを整理していきましょう。"}];
INSERT INTO sentence [{"id":{"d":236,"n":1,"t":2},"speaker":"A","text":"文系コースにするか、理系コースにするか決めましたか？"},{"id":{"d":236,"n":2,"t":2},"speaker":"B","text":"悩みましたが、理系コースにします。"},{"id":{"d":236,"n":3,"t":2},"speaker":"A","text":"何か、不安な点があるのでしょうか？"},{"id":{"d":236,"n":4,"t":2},"speaker":"B","text":"数学は得意なのですが、化学や、生物はあまり成績が良くないのです。"},{"id":{"d":236,"n":5,"t":2},"speaker":"A","text":"物理は、どうですか？"},{"id":{"d":236,"n":6,"t":2},"speaker":"B","text":"化学や、生物よりは苦手意識が少ないです。"},{"id":{"d":236,"n":7,"t":2},"speaker":"A","text":"では、物理の勉強に力を入れて、得意科目になるといいですね。"},{"id":{"d":236,"n":8,"t":2},"speaker":"B","text":"この先、文系に変更することは不可能ですか？"},{"id":{"d":236,"n":9,"t":2},"speaker":"A","text":"11月までなら、コース変更ができます。"}];
INSERT INTO sentence [{"id":{"d":237,"n":1,"t":2},"speaker":"A","text":"お母さん、空き缶や、空のペットボトルはありますか？"},{"id":{"d":237,"n":2,"t":2},"speaker":"B","text":"キッチンにありますよ。"},{"id":{"d":237,"n":3,"t":2},"speaker":"A","text":"見つけました。これらを学校へ持って行ってよいですか？"},{"id":{"d":237,"n":4,"t":2},"speaker":"B","text":"よいですよ。何に使うのですか？"},{"id":{"d":237,"n":5,"t":2},"speaker":"A","text":"授業でリサイクルマークについて調べるので、マークの付いた物を持ち寄ることになったのです。"},{"id":{"d":237,"n":6,"t":2},"speaker":"B","text":"そうですか。環境について学ぶ良い機会ですね。"},{"id":{"d":237,"n":7,"t":2},"speaker":"A","text":"はい。調べた結果を発表する会があるので、頑張ります。"},{"id":{"d":237,"n":8,"t":2},"speaker":"B","text":"張り切っていますね。"},{"id":{"d":237,"n":9,"t":2},"speaker":"A","text":"優れた発表には優秀賞が与えられるので、これを受賞したいのです。"},{"id":{"d":237,"n":10,"t":2},"speaker":"B","text":"応援しています。"}];
INSERT INTO sentence [{"id":{"d":238,"n":1,"t":2},"speaker":"A","text":"今日の数学の宿題はしましたか？"},{"id":{"d":238,"n":2,"t":2},"speaker":"B","text":"一応しましたが、すごく難しかったので、あまり当たっている気はしません。"},{"id":{"d":238,"n":3,"t":2},"speaker":"A","text":"そうですよね。特に最後の問題が難しくて、答えが合っているか自信がありません。"},{"id":{"d":238,"n":4,"t":2},"speaker":"B","text":"先生は復習だと仰っていたけれど、あのような問題は授業で習った記憶がありません。"},{"id":{"d":238,"n":5,"t":2},"speaker":"A","text":"私も今まで見た記憶がありません。"},{"id":{"d":238,"n":6,"t":2},"speaker":"B","text":"教科書を見直したのですが、似た問題も見当たりませんでした。"},{"id":{"d":238,"n":7,"t":2},"speaker":"A","text":"もしかしたら、かなり前に学習したのでしょうか？"},{"id":{"d":238,"n":8,"t":2},"speaker":"B","text":"そうかもしれませんね。どうしても気になりますから、数学の先生に質問してみましょう。"},{"id":{"d":238,"n":9,"t":2},"speaker":"A","text":"3時間目が数学なので、その前に質問したいですね。"},{"id":{"d":238,"n":10,"t":2},"speaker":"B","text":"2時間目の後の休み時間は少し長いので、その時に職員室へ行きましょう。"},{"id":{"d":238,"n":11,"t":2},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":239,"n":1,"t":2},"speaker":"A","text":"次は音楽の授業だから、音楽室へ移動ですね。一緒に音楽室へ行きましょう。"},{"id":{"d":239,"n":2,"t":2},"speaker":"B","text":"ごめんなさい、先に行っていただけますか？"},{"id":{"d":239,"n":3,"t":2},"speaker":"A","text":"どうしてですか？"},{"id":{"d":239,"n":4,"t":2},"speaker":"B","text":"私は今日の日直なので、いろいろとすべきことがあるのです。"},{"id":{"d":239,"n":5,"t":2},"speaker":"A","text":"そうでしたね。それに、もう1人の日直の鈴木君は早退してしまったのですよね。"},{"id":{"d":239,"n":6,"t":2},"speaker":"B","text":"そうなのです。だから、日直の仕事を1人で行わなければならいのですが、なかなか終わらないのです。"},{"id":{"d":239,"n":7,"t":2},"speaker":"A","text":"それは大変ですね。手伝いますよ。"},{"id":{"d":239,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。助かります。"},{"id":{"d":239,"n":9,"t":2},"speaker":"A","text":"私が黒板を消すので、他のことをしていただいて構いませんよ。"},{"id":{"d":239,"n":10,"t":2},"speaker":"B","text":"では、日誌を書こうと思います。"},{"id":{"d":239,"n":11,"t":2},"speaker":"A","text":"音楽の授業に遅れないように、急ぎましょう。"},{"id":{"d":239,"n":12,"t":2},"speaker":"B","text":"はい。音楽室は遠くて移動に時間がかかるので、早めに終えられるよう頑張ります。"}];
INSERT INTO sentence [{"id":{"d":240,"n":1,"t":2},"speaker":"A","text":"君のクラスは、文化祭で何をするのですか？"},{"id":{"d":240,"n":2,"t":2},"speaker":"B","text":"お化け屋敷をするみたいですよ。"},{"id":{"d":240,"n":3,"t":2},"speaker":"A","text":"君は、参加しないのですか？"},{"id":{"d":240,"n":4,"t":2},"speaker":"B","text":"はい、僕はクラスではなく、部での出し物に参加することになっています。"},{"id":{"d":240,"n":5,"t":2},"speaker":"A","text":"そうなのですね。何部なのですか？"},{"id":{"d":240,"n":6,"t":2},"speaker":"B","text":"天文部です。"},{"id":{"d":240,"n":7,"t":2},"speaker":"A","text":"天文部ですか。普段はどのような活動をしているのですか？"},{"id":{"d":240,"n":8,"t":2},"speaker":"B","text":"放課後は天体の勉强することが多いです。課外活動として夜に撮影会をすることもあります。"},{"id":{"d":240,"n":9,"t":2},"speaker":"A","text":"面白そうですね。文化祭では、どのような出し物をするのですか？"},{"id":{"d":240,"n":10,"t":2},"speaker":"B","text":"プラネタリウムを展示します。とても本格的なので、ぜひ見に来てください。"}];
INSERT INTO sentence [{"id":{"d":241,"n":1,"t":2},"speaker":"A","text":"さっき、先生に呼ばれていましたね。"},{"id":{"d":241,"n":2,"t":2},"speaker":"B","text":"はい、明日は天気が悪いので、自転車通学をしないように言われました。"},{"id":{"d":241,"n":3,"t":2},"speaker":"A","text":"ああ、確か天気予報では明日は大雪でしたね。"},{"id":{"d":241,"n":4,"t":2},"speaker":"B","text":"そうらしいです。僕はいつも自転車で通学しているので困りました。"},{"id":{"d":241,"n":5,"t":2},"speaker":"A","text":"自転車通学をしている生徒は多いのですか？"},{"id":{"d":241,"n":6,"t":2},"speaker":"B","text":"20人くらいです。徒歩で40分以上かかる人は自転車通学が許されています。"},{"id":{"d":241,"n":7,"t":2},"speaker":"A","text":"なるほど。他の人たちも明日の通学はどうするのでしょうか？"},{"id":{"d":241,"n":8,"t":2},"speaker":"B","text":"山田さんは、「家族に車で送迎してもらう」と言っていました。"},{"id":{"d":241,"n":9,"t":2},"speaker":"A","text":"それは安心ですね。あなたはどうするのですか？"},{"id":{"d":241,"n":10,"t":2},"speaker":"B","text":"お父さんは仕事で留守にしていて、お母さんは雪の日には運転ができないので、僕は、明日は徒歩で登校しようと思っています。"}];
INSERT INTO sentence [{"id":{"d":242,"n":1,"t":2},"speaker":"A","text":"教授、今日提出したレポートは返却されるのでしょうか？"},{"id":{"d":242,"n":2,"t":2},"speaker":"B","text":"いえ、返却しません。"},{"id":{"d":242,"n":3,"t":2},"speaker":"A","text":"レポートを返却してもらうことはできますか？パソコンがクラッシュしてしまい、レポートに記載した情報が取り出せなくなってしまったのです。"},{"id":{"d":242,"n":4,"t":2},"speaker":"B","text":"なるほど。では採点が終わったら返却しますよ。USBを用意しておいてください。"},{"id":{"d":242,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。卒業論文の結論部分に必要なので、助かりました。"}];
INSERT INTO sentence [{"id":{"d":243,"n":1,"t":2},"speaker":"A","text":"あなたは、何部に入るつもりですか？"},{"id":{"d":243,"n":2,"t":2},"speaker":"B","text":"まだ決めていませんが、運動部にするつもりです。"},{"id":{"d":243,"n":3,"t":2},"speaker":"A","text":"中学の時に何か運動をしていたのですか？"},{"id":{"d":243,"n":4,"t":2},"speaker":"B","text":"中学は陸上部で短距離をしていました。"},{"id":{"d":243,"n":5,"t":2},"speaker":"A","text":"陸上は続けないのですか？"},{"id":{"d":243,"n":6,"t":2},"speaker":"B","text":"陸上もいいですが、チームでプレーするようなスポーツをしてみたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":244,"n":1,"t":2},"speaker":"A","text":"こちらの問題集は、この学校の教科書に準拠したものになっています。"},{"id":{"d":244,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。3年間のまとめの問題集もありますか？"},{"id":{"d":244,"n":3,"t":2},"speaker":"A","text":"見本はまだ送られてきていないので、カタログになりますが、こちらの3種があります。"},{"id":{"d":244,"n":4,"t":2},"speaker":"B","text":"解答が別冊になっているものは、どれですか？"},{"id":{"d":244,"n":5,"t":2},"speaker":"A","text":"どれも解答は別冊です。繰り返し学習できるように、解答欄が別冊になっているものもあります。"},{"id":{"d":244,"n":6,"t":2},"speaker":"B","text":"最近はそのようなものも多いようですね。最後のまとめにある、入試用の問題を見てみたいのですが。"},{"id":{"d":244,"n":7,"t":2},"speaker":"A","text":"去年の問題集なら車にあります。今、持ってきましょうか？"},{"id":{"d":244,"n":8,"t":2},"speaker":"B","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":245,"n":1,"t":2},"speaker":"A","text":"学生証をなくしてしまったので、再発行をお願いします。"},{"id":{"d":245,"n":2,"t":2},"speaker":"B","text":"いつ紛失しましたか。見つかることはありませんか？"},{"id":{"d":245,"n":3,"t":2},"speaker":"A","text":"どこでなくしたのかも分からないので、見つかる可能性は低いと思います。なくしたことに気付いたのは2日前です。"},{"id":{"d":245,"n":4,"t":2},"speaker":"B","text":"そうですか。悪用されるかもしれないので、一応警察に届け出た方がいいでしょう。"},{"id":{"d":245,"n":5,"t":2},"speaker":"A","text":"はい、そうします。"},{"id":{"d":245,"n":6,"t":2},"speaker":"B","text":"では、手続きを進めますので、学生証に登録する写真と合わせて、この書類を提出してください。"},{"id":{"d":245,"n":7,"t":2},"speaker":"A","text":"分かりました。再発行には、どのくらいかかりますか？"},{"id":{"d":245,"n":8,"t":2},"speaker":"B","text":"新しい学生証が手元に届くには、2週間ほどかかります。その前に学生証が必要な時には、こちらで在学証明書を発行して代用してください。"}];
INSERT INTO sentence [{"id":{"d":246,"n":1,"t":2},"speaker":"A","text":"佐々木先生、生徒に漢字の宿題を出していますか？"},{"id":{"d":246,"n":2,"t":2},"speaker":"B","text":"はい、出しています。ノートに漢字を10回ずつ書いて、それを提出させることにしています。"},{"id":{"d":246,"n":3,"t":2},"speaker":"A","text":"みんなちゃんと書いてきますか？"},{"id":{"d":246,"n":4,"t":2},"speaker":"B","text":"やはり性格は出ます。きちんと書く子もいれば、殴り書きの子もいます。"},{"id":{"d":246,"n":5,"t":2},"speaker":"A","text":"書いている漢字が、そもそも間違っていることはありませんか？"},{"id":{"d":246,"n":6,"t":2},"speaker":"B","text":"時折、そういう子もいますよ。そのため、間違った漢字を練習してしまわないよう、必ずテスト前に一度はノートを確認するようにしています。"},{"id":{"d":246,"n":7,"t":2},"speaker":"A","text":"テストの作成もあるのに、生徒全員のノートも確認するのですか？"},{"id":{"d":246,"n":8,"t":2},"speaker":"B","text":"はい。漢字のテスト範囲が広いと、生徒も私も大変になってしまいますが、仕方がありません。"},{"id":{"d":246,"n":9,"t":2},"speaker":"A","text":"素晴らしいですね。私も見習います。"}];
INSERT INTO sentence [{"id":{"d":247,"n":1,"t":2},"speaker":"A","text":"先輩、パート練習の場所はいつものところですか？"},{"id":{"d":247,"n":2,"t":2},"speaker":"B","text":"そうです。田中さんはクラリネットの担当ですから、体育館裏です。"},{"id":{"d":247,"n":3,"t":2},"speaker":"A","text":"雨が降ってきたら、視聴覚室に戻ってくるのですか？"},{"id":{"d":247,"n":4,"t":2},"speaker":"B","text":"そうです。パート練習の後は、5時から視聴覚室に集合して、全体練習です。"},{"id":{"d":247,"n":5,"t":2},"speaker":"A","text":"今日の全体練習に、井出先生はいらっしゃいますか？"},{"id":{"d":247,"n":6,"t":2},"speaker":"B","text":"先生は会議で出られないので、指揮は私がすることになっています。"},{"id":{"d":247,"n":7,"t":2},"speaker":"A","text":"分かりました。では、パート練習へいってきます。"}];
INSERT INTO sentence [{"id":{"d":248,"n":1,"t":2},"speaker":"A","text":"先月の模擬試験の結果はどうでしたか？"},{"id":{"d":248,"n":2,"t":2},"speaker":"B","text":"全く駄目でした。第2志望のレベルを下げることになりそうです。"},{"id":{"d":248,"n":3,"t":2},"speaker":"A","text":"僕は国立大学志望でしたが、このままだと私立大学に志望校を変えることになりそうです。"},{"id":{"d":248,"n":4,"t":2},"speaker":"B","text":"こちらは、私立文系3科目受験ですが、それでも勉強が追い付きません。5教科で受験する君が信じられません。"},{"id":{"d":248,"n":5,"t":2},"speaker":"A","text":"両親に負担をかけないよう、学費の安い国立大学を目指していたのですが、この模擬試験の結果はショックです。"},{"id":{"d":248,"n":6,"t":2},"speaker":"B","text":"本番までは、まだ時間があるので、お互いもっと頑張りましょう。"},{"id":{"d":248,"n":7,"t":2},"speaker":"A","text":"はい。諦めるには、まだ早いです。"}];
INSERT INTO sentence [{"id":{"d":249,"n":1,"t":2},"speaker":"A","text":"先生、音楽室のピアノを借りて合唱コンクールの練習をしたいのですが、よろしいでしょうか？"},{"id":{"d":249,"n":2,"t":2},"speaker":"B","text":"クラス全員で、練習するのですか？それなら、担任の先生の許可が必要です。"},{"id":{"d":249,"n":3,"t":2},"speaker":"A","text":"私と指揮者の久保さんとともに、ピアノを使って練習したいのです。久保さんが、みんなの前で指揮をする前に、まず私に見てほしいのだそうです。"},{"id":{"d":249,"n":4,"t":2},"speaker":"B","text":"それなら、担任の先生の許可は必要ありません。あなたの伴奏の練習ではなく、久保さんの練習なのですね。"},{"id":{"d":249,"n":5,"t":2},"speaker":"A","text":"はい。彼女はみんなが歌いやすく、説得力のある指揮をするために一生懸命です。"},{"id":{"d":249,"n":6,"t":2},"speaker":"B","text":"素晴らしいですね。はい、これが音楽室の鍵です。外が暗くなる前には、帰宅するようにしてください。"},{"id":{"d":249,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":250,"n":1,"t":2},"speaker":"A","text":"体育祭担当の長友先生から、「体育祭時の保健体制について聞いてくるように」と言われました。"},{"id":{"d":250,"n":2,"t":2},"speaker":"B","text":"当日は保健室を閉めて、救護所を設けることになっています。"},{"id":{"d":250,"n":3,"t":2},"speaker":"A","text":"救護所の場所はどこですか？"},{"id":{"d":250,"n":4,"t":2},"speaker":"B","text":"放送ブースの隣の予定です。"},{"id":{"d":250,"n":5,"t":2},"speaker":"A","text":"職員室近くのテントですね。クラスの保健係は、何かするのですか？"},{"id":{"d":250,"n":6,"t":2},"speaker":"B","text":"体育祭前の委員会で打ち合わせますが、けが人が出た場合は、保健係が救護所まで連れてくることになっています。"},{"id":{"d":250,"n":7,"t":2},"speaker":"A","text":"大きなけがの場合どうなるのですか？"},{"id":{"d":250,"n":8,"t":2},"speaker":"B","text":"その時は、保健室を開けて一時収容後、車で病院ということになります。"},{"id":{"d":250,"n":9,"t":2},"speaker":"A","text":"今まで、そのようなことはあったのですか？"},{"id":{"d":250,"n":10,"t":2},"speaker":"B","text":"ないと思います。今年もそうあってほしいものです。"}];
INSERT INTO sentence [{"id":{"d":251,"n":1,"t":2},"speaker":"A","text":"当校を志望する理由は、何ですか？"},{"id":{"d":251,"n":2,"t":2},"speaker":"B","text":"この学校の卒業生である私の父からいろいろと話を聞いて、私に合っていると思い決めました。"},{"id":{"d":251,"n":3,"t":2},"speaker":"A","text":"どういうところが、合っていると思いますか？"},{"id":{"d":251,"n":4,"t":2},"speaker":"B","text":"まずは、進学校というところです。勉強に集中できる環境があるのは魅力的です。また、文化系の部活動が盛んであるところにも魅力的を感じます。気の合う友達がたくさんできそうです。"},{"id":{"d":251,"n":5,"t":2},"speaker":"A","text":"なるほど、よく分かりました。気になる部活動は、ありますか？"},{"id":{"d":251,"n":6,"t":2},"speaker":"B","text":"鉄道部です。実は父も鉄道部でした。"},{"id":{"d":251,"n":7,"t":2},"speaker":"A","text":"そうなのですね。以上で面接を終了します。お疲れさまでした。"},{"id":{"d":251,"n":8,"t":2},"speaker":"B","text":"ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":252,"n":1,"t":2},"speaker":"A","text":"すみません。このクラスは教育児童心理の講義ですか？"},{"id":{"d":252,"n":2,"t":2},"speaker":"B","text":"はい、そうです。"},{"id":{"d":252,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。教室が分からず、授業開始時刻に少し遅れてしまいました。"},{"id":{"d":252,"n":4,"t":2},"speaker":"B","text":"まだ、教授が来ていないので遅刻にはなりませんよ。私は教育学部2年の奈良坂と申します。あなたは何学部ですか？"},{"id":{"d":252,"n":5,"t":2},"speaker":"A","text":"それは幸運でした。私は理学部の米屋です。2年生です。"},{"id":{"d":252,"n":6,"t":2},"speaker":"B","text":"米屋さんですね、よろしくお願いします。この建物は少し分かりづらいので、普段から使っている教育学部以外の人は迷ってしまうかもしれませんね。"},{"id":{"d":252,"n":7,"t":2},"speaker":"A","text":"こちらこそよろしくお願いします。はい、今日初めて入ったのですが、どこから2階に上がれるのかが分からず、しばらく歩き回っていました。廊下の奥に階段があることが分かったので、来週からは大丈夫だと思います。"},{"id":{"d":252,"n":8,"t":2},"speaker":"B","text":"それは良かったです。あ、向こうから教授が来ました。そろそろ講義が始まりますね。"}];
INSERT INTO sentence [{"id":{"d":253,"n":1,"t":2},"speaker":"A","text":"小学生に算数を教えるコツはありますか？"},{"id":{"d":253,"n":2,"t":2},"speaker":"B","text":"小学何年生に教えるのですか？"},{"id":{"d":253,"n":3,"t":2},"speaker":"A","text":"小学5年の男の子です。分数が苦手なようです。"},{"id":{"d":253,"n":4,"t":2},"speaker":"B","text":"なるほど。分数はそれが表す数の大きさを具体的にイメージできるようになると、苦手意識が減りそうです。"},{"id":{"d":253,"n":5,"t":2},"speaker":"A","text":"イメージですか？"},{"id":{"d":253,"n":6,"t":2},"speaker":"B","text":"はい。例えば、分数を少数に変換してみる、分数が表す値を数直線上にプロットしてみる、などです。"},{"id":{"d":253,"n":7,"t":2},"speaker":"A","text":"なるほど。それは良さそうです。"},{"id":{"d":253,"n":8,"t":2},"speaker":"B","text":"分数が表す数の大きさがイメージできると、分数の計算なども理解しやすくなるはずです。"},{"id":{"d":253,"n":9,"t":2},"speaker":"A","text":"確かにそうですね。教えていただいた方法を今度試してみます。"}];
INSERT INTO sentence [{"id":{"d":254,"n":1,"t":2},"speaker":"A","text":"先生、高校2年になったばかりなのに、大学の志望校調査は早すぎないでしょうか？"},{"id":{"d":254,"n":2,"t":2},"speaker":"B","text":"そうでもありませんよ。早いうちから、進路を具体的に考えておくことは大事です。"},{"id":{"d":254,"n":3,"t":2},"speaker":"A","text":"まだ私は志望校どころか、行きたい学部も考えられていません。"},{"id":{"d":254,"n":4,"t":2},"speaker":"B","text":"夏には多くの大学でオープンキャンパスが開催されるので、それを見ていろいろ考えるのが良いと思います。"},{"id":{"d":254,"n":5,"t":2},"speaker":"A","text":"3年でなくともオープンキャンパスに参加してよいのですか？"},{"id":{"d":254,"n":6,"t":2},"speaker":"B","text":"もちろんです。3年生の夏から秋には受験勉強が本格化します。そのため、2年生うちにたくさん大学を見学して、進路をじっくり考えておくことが重要です。"},{"id":{"d":254,"n":7,"t":2},"speaker":"A","text":"分かりました。今年の夏は友達を誘っていろいろな大学のオープンキャンパスに参加してみます。"}];
INSERT INTO sentence [{"id":{"d":255,"n":1,"t":2},"speaker":"A","text":"同窓会の出席者も年々減ってきました。"},{"id":{"d":255,"n":2,"t":2},"speaker":"B","text":"残念ですが仕方がありませんね。もうこのあたりに住んでいる人は、数えるほどしかいませんから。"},{"id":{"d":255,"n":3,"t":2},"speaker":"A","text":"遠隔地からわざわざ来る人はいないのでしょうか。近所にいる者たちで同窓会をすると、町内会の会合と変わらなくなってしまいます。"},{"id":{"d":255,"n":4,"t":2},"speaker":"B","text":"実際、近所には同窓生があまり住んでいないようですね。前回の同窓会も、地元の住んでいる数人しか出席しませんでした。"},{"id":{"d":255,"n":5,"t":2},"speaker":"A","text":"同窓会を、クラス単位から、学年単位にしてみたらどうでしょうか？町内会の会合のようなところは変わらないかもしれませんが、人数が増えて、にぎやかにはなると思います。"},{"id":{"d":255,"n":6,"t":2},"speaker":"B","text":"それは良い案かもしれません。他のクラスの幹事にも、相談してみましょう。"}];
INSERT INTO sentence [{"id":{"d":256,"n":1,"t":2},"speaker":"A","text":"今も小学校で、そろばんを教えていますか？"},{"id":{"d":256,"n":2,"t":2},"speaker":"B","text":"ないと思いますよ。私の孫は、そろばんを見たことさえありませんでした。"},{"id":{"d":256,"n":3,"t":2},"speaker":"A","text":"それは、驚きました。では、鍵盤ハーモニカは授業で使っていますか。持ち歩いている子を、最近は見かけません。"},{"id":{"d":256,"n":4,"t":2},"speaker":"B","text":"リコーダーは使うようですが、ハーモニカは使わない学校も多いようです。"},{"id":{"d":256,"n":5,"t":2},"speaker":"A","text":"それでは、最近は何を教えているのですか。新しいカリキュラムが、行われているのでしょうか？"},{"id":{"d":256,"n":6,"t":2},"speaker":"B","text":"孫から聞いたところ、英語とダンスの授業があるそうです。"},{"id":{"d":256,"n":7,"t":2},"speaker":"A","text":"英語の授業のことはニュースで聞いたような気がしますが、ダンスは初耳です。"},{"id":{"d":256,"n":8,"t":2},"speaker":"B","text":"学校も大変だと聞いています。ダンスのための先生も必要だということです。"},{"id":{"d":256,"n":9,"t":2},"speaker":"A","text":"駅前にダンス教室ができたのは、そのような背景を考えてのことかも知れませんね。"}];
INSERT INTO sentence [{"id":{"d":257,"n":1,"t":2},"speaker":"A","text":"息子さんはもっと上の高校を目指せると思いますが、どこか希望する高校はありますか？"},{"id":{"d":257,"n":2,"t":2},"speaker":"B","text":"公立で上の高校となると男子校になります。息子はそれを嫌がっています。"},{"id":{"d":257,"n":3,"t":2},"speaker":"A","text":"なるほど。私立の高校はどうですか？"},{"id":{"d":257,"n":4,"t":2},"speaker":"B","text":"経済的な問題でまだ思案中です。"},{"id":{"d":257,"n":5,"t":2},"speaker":"A","text":"私立高校には特待生制度があるところもあります。特待生になれば授業料が免除されるので、公立に行くのと同じかそれ以下の費用で進学できます。"},{"id":{"d":257,"n":6,"t":2},"speaker":"B","text":"知りませんでした。帰ったら息子にも話してみますが、先生からも詳しく説明してもらえますか？"},{"id":{"d":257,"n":7,"t":2},"speaker":"A","text":"分かりました。明日本人と話してみましょう。"}];
INSERT INTO sentence [{"id":{"d":258,"n":1,"t":2},"speaker":"A","text":"先生、あさっての追試験はどこで行われますか？"},{"id":{"d":258,"n":2,"t":2},"speaker":"B","text":"放課後に視聴覚室で行います。さっき掲示板に、そのお知らせを貼りました。"},{"id":{"d":258,"n":3,"t":2},"speaker":"A","text":"視聴覚室ですか？"},{"id":{"d":258,"n":4,"t":2},"speaker":"B","text":"今回は大会出場などで、試験を受けられなかった運動部の学生も参加するので人数が多く、普通の教室では席が足りないのです。"},{"id":{"d":258,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。ちなみに、追試験で合格できなかった場合はどうなるのですか？"},{"id":{"d":258,"n":6,"t":2},"speaker":"B","text":"その時は、再度試験を受けてもらいます。1回で合格できるよう、しっかり勉強してくださいね。"},{"id":{"d":258,"n":7,"t":2},"speaker":"A","text":"はい、頑張ります。"}];
INSERT INTO sentence [{"id":{"d":259,"n":1,"t":2},"speaker":"A","text":"大学でも、体育の授業があるとは思いませんでした。"},{"id":{"d":259,"n":2,"t":2},"speaker":"B","text":"私も知らなかったので驚きました。いろいろな種目の中から選べるので楽しいです。"},{"id":{"d":259,"n":3,"t":2},"speaker":"A","text":"乗馬や登山まであるのは驚きです。スキー合宿は、大人気だそうです。"},{"id":{"d":259,"n":4,"t":2},"speaker":"B","text":"半年間のカリキュラムを、5日間で済ませられるのですから人気があるのも納得です。"},{"id":{"d":259,"n":5,"t":2},"speaker":"A","text":"旅行気分で単位を取得できるのだから、言うことがありません。君はどの種目しましたか？"},{"id":{"d":259,"n":6,"t":2},"speaker":"B","text":"私は本校の体育館で行われる球技にしました。なるべく、お金のかからないものを選びました。"},{"id":{"d":259,"n":7,"t":2},"speaker":"A","text":"体育の授業だけで遠出はしたくないので、講義の合間の体育でよいということですか？"},{"id":{"d":259,"n":8,"t":2},"speaker":"B","text":"そうです。君は何にしましたか？"},{"id":{"d":259,"n":9,"t":2},"speaker":"A","text":"水泳にしました。シャワーを使えるので、そのままアルバイトやデートに行けると思ったのです。"},{"id":{"d":259,"n":10,"t":2},"speaker":"B","text":"お互いに実利的な選択ですね。"}];
INSERT INTO sentence [{"id":{"d":260,"n":1,"t":2},"speaker":"A","text":"今まで転校したことはありますか？"},{"id":{"d":260,"n":2,"t":2},"speaker":"B","text":"父親が転勤族だったので、中学までは何度か転校をしています。"},{"id":{"d":260,"n":3,"t":2},"speaker":"A","text":"どういうところに行きましたか？"},{"id":{"d":260,"n":4,"t":2},"speaker":"B","text":"秋田、大阪、フィリピンのマニラにも2年くらいいました。"},{"id":{"d":260,"n":5,"t":2},"speaker":"A","text":"海外にも転校したのですか。環境が変わるのは大変だったのではありませんか？"},{"id":{"d":260,"n":6,"t":2},"speaker":"B","text":"そうですね。新しいコミュニティーに入るときは毎度緊張しますが、どこも温かく迎え入れてくれたので、そこまで大変だとは思いませんでした。"},{"id":{"d":260,"n":7,"t":2},"speaker":"A","text":"それは、良かったですね。"}];
INSERT INTO sentence [{"id":{"d":261,"n":1,"t":2},"speaker":"A","text":"社会科目で好きな教科は何ですか？日本史、地理、政治・経済、世界史などいろいろな教科がありますが。"},{"id":{"d":261,"n":2,"t":2},"speaker":"B","text":"もちろん日本史です。私は新選組が好きなので、特に幕末周辺の歴史はとても面白いです。"},{"id":{"d":261,"n":3,"t":2},"speaker":"A","text":"新選組は実在した組織だったのですか？私はアニメーションで新選組を知ったので、架空の組織かと思っていました。"},{"id":{"d":261,"n":4,"t":2},"speaker":"B","text":"実在していましたよ。ほら、歴史の教科書にも小さくですが載っています。"},{"id":{"d":261,"n":5,"t":2},"speaker":"A","text":"本当ですね。私は日本史が本当に苦手なので、それすら知りませんでした。"},{"id":{"d":261,"n":6,"t":2},"speaker":"B","text":"日本史は苦手な人が多い印象があります。あなたの好きな科目は何ですか？"},{"id":{"d":261,"n":7,"t":2},"speaker":"A","text":"地理です。地理の授業で世界中の都市や自然を見ることができ、それがとても楽しいです。"},{"id":{"d":261,"n":8,"t":2},"speaker":"B","text":"確かに、地理の教科書はまるで写真集のようですね。きれいな建物や景色がたくさん載っています。"}];
INSERT INTO sentence [{"id":{"d":262,"n":1,"t":2},"speaker":"A","text":"中学では、美術という科目が新しく始まるようです。今日、教科書が配布されました。"},{"id":{"d":262,"n":2,"t":2},"speaker":"B","text":"そうですね。楽しみですか？"},{"id":{"d":262,"n":3,"t":2},"speaker":"A","text":"はい。図工が好きだったので、きっと美術も楽しめると思います。"},{"id":{"d":262,"n":4,"t":2},"speaker":"B","text":"そうかもしれませんね。確かに、美術の授業では絵を描いたり、工作をしたりもします。"},{"id":{"d":262,"n":5,"t":2},"speaker":"A","text":"他には、どのようなことをするのですか？"},{"id":{"d":262,"n":6,"t":2},"speaker":"B","text":"あとは、美術史を学んだり美術品を鑑賞したりもします。実技と座学の両方で学びます。"},{"id":{"d":262,"n":7,"t":2},"speaker":"A","text":"それで美術の教科書には、いろいろな作品が載っていたのですね。授業が始まるのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":263,"n":1,"t":2},"speaker":"A","text":"それは、何のテキストですか？"},{"id":{"d":263,"n":2,"t":2},"speaker":"B","text":"これは、普通免許取得のためのテキストです。2カ月ほど前から、自動車教習所に通いはじめました。"},{"id":{"d":263,"n":3,"t":2},"speaker":"A","text":"そうだったのですね。免許取得のための勉強は順調ですか？"},{"id":{"d":263,"n":4,"t":2},"speaker":"B","text":"はい。自動車教習所がとても親切に教えてくれるので、困っていることは特にありません。"},{"id":{"d":263,"n":5,"t":2},"speaker":"A","text":"それは良いですね。車の運転はもうしましたか？"},{"id":{"d":263,"n":6,"t":2},"speaker":"B","text":"1度だけしました。自動車教習所の敷地内を少し運転しただけですが、とても緊張しました。"}];
INSERT INTO sentence [{"id":{"d":264,"n":1,"t":2},"speaker":"A","text":"英語を話すことには自信が付いてきましたが、英文を書くことはまだ苦手です。克服するための何か良い方法はありませんか？"},{"id":{"d":264,"n":2,"t":2},"speaker":"B","text":"単語のスペルをしっかり覚えることと、基本的な文法を理解することは重要だと思います。"},{"id":{"d":264,"n":3,"t":2},"speaker":"A","text":"確かに、英語のテストでもスペルミスで減点されたことが何度もあります。中島さんは、どのようにして単語のスペルを覚えていますか？"},{"id":{"d":264,"n":4,"t":2},"speaker":"B","text":"私はひたすら書いて覚えます。小学生の頃、漢字を覚えるために何度も書いて勉強していたように、英単語も覚えるまで何度も紙に書いています。"},{"id":{"d":264,"n":5,"t":2},"speaker":"A","text":"なるほど、試してみます。"},{"id":{"d":264,"n":6,"t":2},"speaker":"B","text":"それから、「フォニックス」と呼ばれる英語の発音ルールを覚えるのも有効だと思います。これを覚えると、単語の発音が分かっていれば、そこからスペルを逆算できます。スピーキングが得意な菊池さんは、「フォニックス」を覚えるだけでスペルミスが一気に減りそうですね。"},{"id":{"d":264,"n":7,"t":2},"speaker":"A","text":"「フォニックス」という言葉は初めて聞きました。ぜひ、詳しく教えてください。"},{"id":{"d":264,"n":8,"t":2},"speaker":"B","text":"はい。今、資料を持ってきますね。"}];
INSERT INTO sentence [{"id":{"d":265,"n":1,"t":2},"speaker":"A","text":"レナード、君から見てオーストラリアと日本の学校で違うところはありますか？"},{"id":{"d":265,"n":2,"t":2},"speaker":"B","text":"いくつかありますが、まず校舎に入るときに、上履きという履物に変えることに驚きました。"},{"id":{"d":265,"n":3,"t":2},"speaker":"A","text":"外国では、家に入るときも靴を履いているのでしたっけ？"},{"id":{"d":265,"n":4,"t":2},"speaker":"B","text":"最近では外国でも、自宅では靴を脱ぐ人も増えてきています。私が不思議なのは、学校に上履きという特殊な履物が存在することです。"},{"id":{"d":265,"n":5,"t":2},"speaker":"A","text":"なるほど、確かに特殊な履物かもしれませんね。私も、学校以外では使ったことがありません。"},{"id":{"d":265,"n":6,"t":2},"speaker":"B","text":"日本でもデパートや市役所や公民館では屋内へもそのまま靴で入れます。なぜ、学校だけは駄目なのでしょうか？"},{"id":{"d":265,"n":7,"t":2},"speaker":"A","text":"言われてみればそうですね。掃除をするときに、楽だからではないでしょうか？"},{"id":{"d":265,"n":8,"t":2},"speaker":"B","text":"そこも不思議なところです。なぜ、みんなで掃除をする習慣があるのですか？"},{"id":{"d":265,"n":9,"t":2},"speaker":"A","text":"自分の教室を、自分で掃除すること、は当然のことではないでしょうか？"},{"id":{"d":265,"n":10,"t":2},"speaker":"B","text":"そういえば、そこも違うところです。オーストラリアの学校は、授業ごとに移動するので、自分の教室といった考えはありません。"}];
INSERT INTO sentence [{"id":{"d":266,"n":1,"t":2},"speaker":"A","text":"先生、理科の選択科目で迷っています。"},{"id":{"d":266,"n":2,"t":2},"speaker":"B","text":"君は、大学のどこの学部を受験するか決めていますか？"},{"id":{"d":266,"n":3,"t":2},"speaker":"A","text":"少し高望みかも知れませんが、医学部を受験するつもりです。"},{"id":{"d":266,"n":4,"t":2},"speaker":"B","text":"そのつもりならば、1つは化学で決まりです。"},{"id":{"d":266,"n":5,"t":2},"speaker":"A","text":"はい。もう1つを物理か生物にするかで迷っています。医学部なら生物の方がいいのでしょうか？"},{"id":{"d":266,"n":6,"t":2},"speaker":"B","text":"試験科目自体に優遇措置はありませんから、そこは試験で高得点を取れる科目を選ぶべきです。"},{"id":{"d":266,"n":7,"t":2},"speaker":"A","text":"物理と生物で試験の難易度や出題傾向は異なりますか？"},{"id":{"d":266,"n":8,"t":2},"speaker":"B","text":"難易度は変わらないでしょう。ただ問題を解くときに必要なスキルは少し異なるかもしれません。暗記が得意なら生物が、数学的な計算が得意なら物理が向いているかもしれません。"},{"id":{"d":266,"n":9,"t":2},"speaker":"A","text":"計算問題はあまり得意ではないので、生物の方がいいかもしれません。"},{"id":{"d":266,"n":10,"t":2},"speaker":"B","text":"どちらにしても、決める前に過去の試験問題を確認しておいた方がいいです。"}];
INSERT INTO sentence [{"id":{"d":267,"n":1,"t":2},"speaker":"A","text":"今日の体力テストは、とても疲れました。"},{"id":{"d":267,"n":2,"t":2},"speaker":"B","text":"お疲れさまです。しかしこの好成績はすごいですね。"},{"id":{"d":267,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。サッカー部部長の名にかけて、一生懸命頑張りました。"},{"id":{"d":267,"n":4,"t":2},"speaker":"B","text":"さすがです。それは疲れるわけですね。"},{"id":{"d":267,"n":5,"t":2},"speaker":"A","text":"今日は部活動が休みだったので、良かったです。この疲れでは、サッカーどころではありません。"},{"id":{"d":267,"n":6,"t":2},"speaker":"B","text":"そうですね。今日はゆっくりと休んで回復に努めてください。"},{"id":{"d":267,"n":7,"t":2},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":268,"n":1,"t":2},"speaker":"A","text":"学校行事で一番好きだったものは何ですか？"},{"id":{"d":268,"n":2,"t":2},"speaker":"B","text":"運動会です。スポーツが得意だったので、1日中スポーツをしていられる運動会を毎年楽しみにしていました。"},{"id":{"d":268,"n":3,"t":2},"speaker":"A","text":"私も運動会は、好きな行事の1つでした。岩田さんはスポーツ万能ですし、大活躍されていそうですね。"},{"id":{"d":268,"n":4,"t":2},"speaker":"B","text":"クラス対抗リレーは、毎年アンカーを走っていました。緊張はしますが、みんなの応援を受けて走るのは気持ち良かったです。中川さんはどの行事が一番好きでしたか？"},{"id":{"d":268,"n":5,"t":2},"speaker":"A","text":"少し珍しいかもしれませんが、私は授業参観が一番好きでした。"},{"id":{"d":268,"n":6,"t":2},"speaker":"B","text":"授業参観ですか？それは珍しいですね。なぜですか？"},{"id":{"d":268,"n":7,"t":2},"speaker":"A","text":"私は勉強が得意だったので、得意なことを一生懸命頑張っているところを、家族に見てもらえるのがうれしかったのです。"}];
INSERT INTO sentence [{"id":{"d":269,"n":1,"t":2},"speaker":"A","text":"山田君が、大学に合格したと聞きました。おめでとうございます。"},{"id":{"d":269,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":269,"n":3,"t":2},"speaker":"A","text":"第1志望の大学に合格できて良かったですね。"},{"id":{"d":269,"n":4,"t":2},"speaker":"B","text":"はい。先生にもたくさんサポートしていただいたおかげです。"},{"id":{"d":269,"n":5,"t":2},"speaker":"A","text":"山田君が、頑張った成果ですよ。"},{"id":{"d":269,"n":6,"t":2},"speaker":"B","text":"もう塾に通わないと思うと寂しいです。"},{"id":{"d":269,"n":7,"t":2},"speaker":"A","text":"私も同じ気持ちです。これからもたまには塾へ遊びに来てくださいね。"},{"id":{"d":269,"n":8,"t":2},"speaker":"B","text":"はい、そうします。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":270,"n":1,"t":2},"speaker":"A","text":"中間テストの結果はどうでしたか？"},{"id":{"d":270,"n":2,"t":2},"speaker":"B","text":"数学と物理のテストが散々でした。"},{"id":{"d":270,"n":3,"t":2},"speaker":"A","text":"珍しいですね。何があったのですか？"},{"id":{"d":270,"n":4,"t":2},"speaker":"B","text":"公式が思い出せず、問題が解けませんでした。多分追試験になると思います。"},{"id":{"d":270,"n":5,"t":2},"speaker":"A","text":"それは大変ですね。文系科目はどうでしたか？"},{"id":{"d":270,"n":6,"t":2},"speaker":"B","text":"そちらはおおむね良かったです。地理は満点の自信があります。"},{"id":{"d":270,"n":7,"t":2},"speaker":"A","text":"それは良かったです。結果が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":271,"n":1,"t":2},"speaker":"A","text":"ようやく給食の時間になりました。おなかがぺこぺこです。"},{"id":{"d":271,"n":2,"t":2},"speaker":"B","text":"佐々木君は、給食の時間をいつも楽しみにしていますね。"},{"id":{"d":271,"n":3,"t":2},"speaker":"A","text":"はい。毎日給食を楽しみに勉強を頑張っています。"},{"id":{"d":271,"n":4,"t":2},"speaker":"B","text":"今日のメニューは、何だか知っていますか？"},{"id":{"d":271,"n":5,"t":2},"speaker":"A","text":"いいえ、知りません。鈴木さんは知っているのですか？"},{"id":{"d":271,"n":6,"t":2},"speaker":"B","text":"はい。給食室の前を通ったときに、匂いで分かりました。今日は、カレーライスです。"},{"id":{"d":271,"n":7,"t":2},"speaker":"A","text":"私の一番好きなメニューです。おかわりをして、たくさん食べようと思います。"},{"id":{"d":271,"n":8,"t":2},"speaker":"B","text":"給食の後は体育ですよ。あまり食べ過ぎないようにしてくださいね。"},{"id":{"d":271,"n":9,"t":2},"speaker":"A","text":"そうでした。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":272,"n":1,"t":2},"speaker":"A","text":"今日は節分ですね。"},{"id":{"d":272,"n":2,"t":2},"speaker":"B","text":"そうですね。私は保育園児の頃は、節分が嫌いでした。"},{"id":{"d":272,"n":3,"t":2},"speaker":"A","text":"どうしてですか？"},{"id":{"d":272,"n":4,"t":2},"speaker":"B","text":"豆まきをするときに、園長先生が鬼の格好をしてクラスに来るのですが、それがとても怖かったのです。"},{"id":{"d":272,"n":5,"t":2},"speaker":"A","text":"それは怖かったでしょうね。"},{"id":{"d":272,"n":6,"t":2},"speaker":"B","text":"はい。ずっと先生の後ろに隠れて、泣いていた記憶があります。"},{"id":{"d":272,"n":7,"t":2},"speaker":"A","text":"私の通っていた保育園では、豆まきはしませんでした。"},{"id":{"d":272,"n":8,"t":2},"speaker":"B","text":"何か他のことをしたのですか？"},{"id":{"d":272,"n":9,"t":2},"speaker":"A","text":"お昼にみんなで、大豆と恵方巻を食べていました。"},{"id":{"d":272,"n":10,"t":2},"speaker":"B","text":"それはいいですね。"}];
INSERT INTO sentence [{"id":{"d":273,"n":1,"t":2},"speaker":"A","text":"先生、お久しぶりです。"},{"id":{"d":273,"n":2,"t":2},"speaker":"B","text":"まあ、遠藤さん。中学校の卒業式以来ですか？"},{"id":{"d":273,"n":3,"t":2},"speaker":"A","text":"そうですね。もう大学3年生になりました。"},{"id":{"d":273,"n":4,"t":2},"speaker":"B","text":"時の流れは早いですね。クラスの友達とは今も会っていますか？"},{"id":{"d":273,"n":5,"t":2},"speaker":"A","text":"年に数回は、会っています。先月も会いました。"},{"id":{"d":273,"n":6,"t":2},"speaker":"B","text":"交流が続いているのですね。"},{"id":{"d":273,"n":7,"t":2},"speaker":"A","text":"はい。この前は、クラスのみんなで「同窓会をしよう」という話をしました。先生も良かったら参加していただけませんか？"},{"id":{"d":273,"n":8,"t":2},"speaker":"B","text":"私も参加してよいのですか？"},{"id":{"d":273,"n":9,"t":2},"speaker":"A","text":"ぜひ参加してください。みんな大喜びです。"},{"id":{"d":273,"n":10,"t":2},"speaker":"B","text":"ありがとうございます。それでは、参加させてください。"},{"id":{"d":273,"n":11,"t":2},"speaker":"A","text":"うれしいです。計画を立てて、また後日ご連絡しますね。"}];
INSERT INTO sentence [{"id":{"d":274,"n":1,"t":2},"speaker":"A","text":"真由さん、受験する大学は決まりましたか？"},{"id":{"d":274,"n":2,"t":2},"speaker":"B","text":"まだ決まっていません。恵美さんはどうですか？"},{"id":{"d":274,"n":3,"t":2},"speaker":"A","text":"私も決まっていません。良かったら今度の日曜日にオープンキャンパスに行きませんか？"},{"id":{"d":274,"n":4,"t":2},"speaker":"B","text":"行きたいです。どこの大学ですか？"},{"id":{"d":274,"n":5,"t":2},"speaker":"A","text":"部活動の先輩が通っている大学です。先輩が学校案内をしてくださるそうです。"},{"id":{"d":274,"n":6,"t":2},"speaker":"B","text":"そうなのですね。私も知っている先輩ですか？"},{"id":{"d":274,"n":7,"t":2},"speaker":"A","text":"知っていると思いますよ。去年の体育祭で、応援団長をしていた先輩です。"},{"id":{"d":274,"n":8,"t":2},"speaker":"B","text":"よく知っています。一緒に写真を撮ってもらいました。またお会いできるとはうれしいです。日曜日が来ることを、楽しみにしていますね。"}];
INSERT INTO sentence [{"id":{"d":275,"n":1,"t":2},"speaker":"A","text":"もうすぐ文化祭ですね。瞳さんのクラスは、何が出し物をしますか？"},{"id":{"d":275,"n":2,"t":2},"speaker":"B","text":"私のクラスは、たこ焼き屋をやります。"},{"id":{"d":275,"n":3,"t":2},"speaker":"A","text":"いいですね。当日は食べに行きます。"},{"id":{"d":275,"n":4,"t":2},"speaker":"B","text":"お待ちしています。優香さんのクラスは何をしますか？"},{"id":{"d":275,"n":5,"t":2},"speaker":"A","text":"私たちは劇を披露します。"},{"id":{"d":275,"n":6,"t":2},"speaker":"B","text":"何の劇ですか？"},{"id":{"d":275,"n":7,"t":2},"speaker":"A","text":"『シンデレラ』です。私は継母役で出演します。"},{"id":{"d":275,"n":8,"t":2},"speaker":"B","text":"それは重要な役ですね。必ず見に行きます。"},{"id":{"d":275,"n":9,"t":2},"speaker":"A","text":"今から緊張していますが、頑張ります。"}];
INSERT INTO sentence [{"id":{"d":276,"n":1,"t":2},"speaker":"A","text":"バレーボール部が、全国大会に出場するそうです。"},{"id":{"d":276,"n":2,"t":2},"speaker":"B","text":"私も、今朝になって知りました。同じ高校に通う生徒として、誇らしい気持ちです。"},{"id":{"d":276,"n":3,"t":2},"speaker":"A","text":"私も同じ思いです。バレーボール部は、毎日体育館で練習していますよね。"},{"id":{"d":276,"n":4,"t":2},"speaker":"B","text":"はい。顧問の先生や、部員の声が聞こえてきます。"},{"id":{"d":276,"n":5,"t":2},"speaker":"A","text":"今度、壮行会があるようですよ。廊下の掲示板に、張り紙がありました。"},{"id":{"d":276,"n":6,"t":2},"speaker":"B","text":"そうなのですか。それはぜひ参加したいですね。"},{"id":{"d":276,"n":7,"t":2},"speaker":"A","text":"はい。全国大会の日も、応援に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":277,"n":1,"t":2},"speaker":"A","text":"廊下に飾られている、佐藤さんの絵を見ました。とても上手ですね。"},{"id":{"d":277,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。小さい頃からお絵描きが好きで、中学でも高校でも美術部です。"},{"id":{"d":277,"n":3,"t":2},"speaker":"A","text":"そうでしたか。大学生になっても絵を続けるのですか？"},{"id":{"d":277,"n":4,"t":2},"speaker":"B","text":"はい。大学は、美術系の専門学部を志望するつもりです。"},{"id":{"d":277,"n":5,"t":2},"speaker":"A","text":"ぴったりだと思います。私は美術のセンスがまるっきりありません。"}];
INSERT INTO sentence [{"id":{"d":278,"n":1,"t":2},"speaker":"A","text":"いよいよ明日は、体育祭ですね。"},{"id":{"d":278,"n":2,"t":2},"speaker":"B","text":"はい。明日が待ち遠しいです。"},{"id":{"d":278,"n":3,"t":2},"speaker":"A","text":"あなたは、体育祭のリレーの選手に選ばれたそうですね。"},{"id":{"d":278,"n":4,"t":2},"speaker":"B","text":"そうです。体力テストで行った50メートル走の記録が良かったので、リレーの選手に選ばれました。"},{"id":{"d":278,"n":5,"t":2},"speaker":"A","text":"クラスで何人が選ばれるのですか？"},{"id":{"d":278,"n":6,"t":2},"speaker":"B","text":"男女4人ずつです。みんなで優勝を狙っています。"},{"id":{"d":278,"n":7,"t":2},"speaker":"A","text":"それは楽しみです。けんじさんは、何番目に走りますか？"},{"id":{"d":278,"n":8,"t":2},"speaker":"B","text":"アンカーです。ゴールテープ目掛けて走り抜けます。"},{"id":{"d":278,"n":9,"t":2},"speaker":"A","text":"それは楽しみです。応援にも熱が入ります。"}];
INSERT INTO sentence [{"id":{"d":279,"n":1,"t":2},"speaker":"A","text":"多田さんの娘さんは、中学受験をしますか？"},{"id":{"d":279,"n":2,"t":2},"speaker":"B","text":"いいえ、公立の中学校に進学します。内田さんの息子さんはどうですか？"},{"id":{"d":279,"n":3,"t":2},"speaker":"A","text":"私の息子も、公立の中学校です。"},{"id":{"d":279,"n":4,"t":2},"speaker":"B","text":"それでは、同じ中学校ですね。最近は、中学受験をする子も多いと聞きます。"},{"id":{"d":279,"n":5,"t":2},"speaker":"A","text":"はい。息子の友達も、中学受験をする子が多いようです。"},{"id":{"d":279,"n":6,"t":2},"speaker":"B","text":"娘の友達も、塾に通っている子が多いそうです。"},{"id":{"d":279,"n":7,"t":2},"speaker":"A","text":"内田さんの息子さんと、同じ学校に通えると心強いです。これからも、よろしくお願いします。"},{"id":{"d":279,"n":8,"t":2},"speaker":"B","text":"こちらこそ、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":280,"n":1,"t":2},"speaker":"A","text":"先ほど、宅配便の荷物が届きませんでしたか？"},{"id":{"d":280,"n":2,"t":2},"speaker":"B","text":"はい。受け取りました。お母さんの荷物ですか？"},{"id":{"d":280,"n":3,"t":2},"speaker":"A","text":"いいえ、これは、あなたの中学校の制服ですよ。"},{"id":{"d":280,"n":4,"t":2},"speaker":"B","text":"私の制服ですか。試着してもよいですか？"},{"id":{"d":280,"n":5,"t":2},"speaker":"A","text":"よいですよ。身長がまだ伸びているので、少し大きめの制服を買いました。"},{"id":{"d":280,"n":6,"t":2},"speaker":"B","text":"着てみました。どうですか？"},{"id":{"d":280,"n":7,"t":2},"speaker":"A","text":"よく似合っています。"},{"id":{"d":280,"n":8,"t":2},"speaker":"B","text":"早くこれを着て、中学校に通いたいです。"}];
INSERT INTO sentence [{"id":{"d":281,"n":1,"t":2},"speaker":"A","text":"どこに出掛けるのですか？"},{"id":{"d":281,"n":2,"t":2},"speaker":"B","text":"友達の家に遊びに行ってきます。"},{"id":{"d":281,"n":3,"t":2},"speaker":"A","text":"宿題は終わったのですか？"},{"id":{"d":281,"n":4,"t":2},"speaker":"B","text":"まだです。遊びから帰ってきてから宿題をします。"},{"id":{"d":281,"n":5,"t":2},"speaker":"A","text":"今日は、水泳の習い事がある日ですよ。"},{"id":{"d":281,"n":6,"t":2},"speaker":"B","text":"そうでした。では、やっぱり宿題を終えてから遊びに行きます。"},{"id":{"d":281,"n":7,"t":2},"speaker":"A","text":"そうしてください。水泳の道具は準備してありますか？"},{"id":{"d":281,"n":8,"t":2},"speaker":"B","text":"それも、まだできていません。宿題と水泳の準備を終えてから、遊びに行きます。"}];
INSERT INTO sentence [{"id":{"d":282,"n":1,"t":2},"speaker":"A","text":"放課後になりました。掃除の時間ですね。"},{"id":{"d":282,"n":2,"t":2},"speaker":"B","text":"はい。掃除をしましょう。"},{"id":{"d":282,"n":3,"t":2},"speaker":"A","text":"今日の掃除場所は、どこですか？"},{"id":{"d":282,"n":4,"t":2},"speaker":"B","text":"私たちの担当は教室です。"},{"id":{"d":282,"n":5,"t":2},"speaker":"A","text":"分かりました。ではまず、机と椅子を運びましょう。"},{"id":{"d":282,"n":6,"t":2},"speaker":"B","text":"はい。廊下に出しますか？"},{"id":{"d":282,"n":7,"t":2},"speaker":"A","text":"廊下は別の子が掃除をしているので、机と椅子は教室の後ろに集める方がいいですね。"},{"id":{"d":282,"n":8,"t":2},"speaker":"B","text":"そうですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":283,"n":1,"t":2},"speaker":"A","text":"期末テストが終わりましたね。"},{"id":{"d":283,"n":2,"t":2},"speaker":"B","text":"はい。テストの手応えはありますか？"},{"id":{"d":283,"n":3,"t":2},"speaker":"A","text":"ありません。特に、数学は分からない問題が多かったです。"},{"id":{"d":283,"n":4,"t":2},"speaker":"B","text":"私も同じく、解けない問題が多かったです。答案用紙を埋められませんでした。"},{"id":{"d":283,"n":5,"t":2},"speaker":"A","text":"このテストの成績が悪かったら、放課後の補習授業を1週間受けるのでしたよね。"},{"id":{"d":283,"n":6,"t":2},"speaker":"B","text":"はい。その間は、部活動にも参加できません。"},{"id":{"d":283,"n":7,"t":2},"speaker":"A","text":"私は、補習授業を受けることになる気がします。"},{"id":{"d":283,"n":8,"t":2},"speaker":"B","text":"同じく、私もそうなりそうです。その時は、一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":284,"n":1,"t":2},"speaker":"A","text":"去年の合唱コンクールの伴奏、とてもすてきでした。"},{"id":{"d":284,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":284,"n":3,"t":2},"speaker":"A","text":"由美さんは、今年の合唱コンクールでも伴奏を担当しますか？"},{"id":{"d":284,"n":4,"t":2},"speaker":"B","text":"はい。今も練習をしているところです。"},{"id":{"d":284,"n":5,"t":2},"speaker":"A","text":"今年も由美さんの演奏が聞けるとは、うれしいです。由美さんは、ピアノを習っているのですか？"},{"id":{"d":284,"n":6,"t":2},"speaker":"B","text":"はい。4歳のときから10年間は習っています。"},{"id":{"d":284,"n":7,"t":2},"speaker":"A","text":"それほどに長く習っているのですね。"},{"id":{"d":284,"n":8,"t":2},"speaker":"B","text":"はい。ピアノは、趣味の1つになっています。"}];
INSERT INTO sentence [{"id":{"d":285,"n":1,"t":2},"speaker":"A","text":"そろそろ、就職活動を始める時期ですね。"},{"id":{"d":285,"n":2,"t":2},"speaker":"B","text":"そうですね。ですが、私はまだ就職活動を始めません。"},{"id":{"d":285,"n":3,"t":2},"speaker":"A","text":"そうなのですか。大学院に進学するのですか？"},{"id":{"d":285,"n":4,"t":2},"speaker":"B","text":"いいえ。休学するのです。"},{"id":{"d":285,"n":5,"t":2},"speaker":"A","text":"休学ですか。その間には何をするのですか？"},{"id":{"d":285,"n":6,"t":2},"speaker":"B","text":"アメリカに留学します。"},{"id":{"d":285,"n":7,"t":2},"speaker":"A","text":"すてきです。どのくらいの期間ですか？"},{"id":{"d":285,"n":8,"t":2},"speaker":"B","text":"1年間の予定です。アメリカで語学を学んでから、社会人になりたいと思います。"}];
INSERT INTO sentence [{"id":{"d":286,"n":1,"t":2},"speaker":"A","text":"加藤さん、どうしたのですか？"},{"id":{"d":286,"n":2,"t":2},"speaker":"B","text":"大学の課題が進まないのです。"},{"id":{"d":286,"n":3,"t":2},"speaker":"A","text":"どの課題ですか？"},{"id":{"d":286,"n":4,"t":2},"speaker":"B","text":"経済学のレポートです。何から手を付けてよいのか分かりません。"},{"id":{"d":286,"n":5,"t":2},"speaker":"A","text":"ああ、これなら、以前教授が紹介されていた本を参考にすると良いと思います。教授が紹介されていた本は読みましたか？"},{"id":{"d":286,"n":6,"t":2},"speaker":"B","text":"いいえ、読んでいません。"},{"id":{"d":286,"n":7,"t":2},"speaker":"A","text":"まずは、その本を読むのが良いです。書くべきことのコツが分かりやすく説明されていましたよ。"},{"id":{"d":286,"n":8,"t":2},"speaker":"B","text":"なるほど、読んでみます。大学の図書館にありますか？"},{"id":{"d":286,"n":9,"t":2},"speaker":"A","text":"はい。私は図書館で借りて読みました。"},{"id":{"d":286,"n":10,"t":2},"speaker":"B","text":"ありがとうございます。今から図書館に行ってきます。"}];
INSERT INTO sentence [{"id":{"d":287,"n":1,"t":2},"speaker":"A","text":"中学校の頃、一番好きだった科目は何ですか？"},{"id":{"d":287,"n":2,"t":2},"speaker":"B","text":"私は音楽です。歌うことが好きなので、音楽のある日はいつも楽しみにしていました。"},{"id":{"d":287,"n":3,"t":2},"speaker":"A","text":"私は1人で歌うことは苦手でしたが、合唱は好きでした。"},{"id":{"d":287,"n":4,"t":2},"speaker":"B","text":"悠太さんの一番好きだった科目は何ですか？"},{"id":{"d":287,"n":5,"t":2},"speaker":"A","text":"理科です。理科室に行って実験をすることが好きでした。"},{"id":{"d":287,"n":6,"t":2},"speaker":"B","text":"なるほど。理科は、今でも好きですか？"},{"id":{"d":287,"n":7,"t":2},"speaker":"A","text":"はい。特に、化学が好きです。大学の学科も、化学科を選びました。"},{"id":{"d":287,"n":8,"t":2},"speaker":"B","text":"理科に対する興味関心を持ち続けているのですね。"}];
INSERT INTO sentence [{"id":{"d":288,"n":1,"t":2},"speaker":"A","text":"午前中の授業が全て終わりましたね。"},{"id":{"d":288,"n":2,"t":2},"speaker":"B","text":"はい。学生食堂へ昼食を食べに行きませんか？"},{"id":{"d":288,"n":3,"t":2},"speaker":"A","text":"すみません。今日は昼食を食べない予定なのです。"},{"id":{"d":288,"n":4,"t":2},"speaker":"B","text":"体調が悪いのですか？"},{"id":{"d":288,"n":5,"t":2},"speaker":"A","text":"いえ、元気です。昼休みの後が家庭科の授業で、調理実習があるのです。"},{"id":{"d":288,"n":6,"t":2},"speaker":"B","text":"そういうことでしたか。それはお昼を食べずにいた方がいいですね。"},{"id":{"d":288,"n":7,"t":2},"speaker":"A","text":"はい。明日は、一緒に学生食堂に行きましょう。"},{"id":{"d":288,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。調理実習頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":289,"n":1,"t":2},"speaker":"A","text":"高校の文化祭には、行ったことがありますか？"},{"id":{"d":289,"n":2,"t":2},"speaker":"B","text":"いいえ、一度もありません。"},{"id":{"d":289,"n":3,"t":2},"speaker":"A","text":"良かったら今度の日曜日に一緒に行きませんか？"},{"id":{"d":289,"n":4,"t":2},"speaker":"B","text":"どこの高校ですか？"},{"id":{"d":289,"n":5,"t":2},"speaker":"A","text":"山上高校です。兄が通っています。"},{"id":{"d":289,"n":6,"t":2},"speaker":"B","text":"お兄さんの通っていらっしゃる高校ですか。行ってみたいです。お兄さんには会えるのですか？"},{"id":{"d":289,"n":7,"t":2},"speaker":"A","text":"会えると思います。兄は「クラスの出し物の担当をしている」と言っていました。"},{"id":{"d":289,"n":8,"t":2},"speaker":"B","text":"ではお兄さんのクラスに、行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":290,"n":1,"t":2},"speaker":"A","text":"次の授業は何ですか？"},{"id":{"d":290,"n":2,"t":2},"speaker":"B","text":"理科です。今日は実験をする日ですね。"},{"id":{"d":290,"n":3,"t":2},"speaker":"A","text":"それでは、理科室に移動しなければなりませんね。"},{"id":{"d":290,"n":4,"t":2},"speaker":"B","text":"はい。行きましょう。"},{"id":{"d":290,"n":5,"t":2},"speaker":"A","text":"持ち物は、白衣と保護眼鏡だけですよね？"},{"id":{"d":290,"n":6,"t":2},"speaker":"B","text":"そうです。忘れ物はありませんか？"},{"id":{"d":290,"n":7,"t":2},"speaker":"A","text":"はい。前回の授業の時に、持ち物を忘れてしまったので、今回は、昨日のうちにかばんに入れました。"},{"id":{"d":290,"n":8,"t":2},"speaker":"B","text":"準備万全ですね。"}];
INSERT INTO sentence [{"id":{"d":291,"n":1,"t":2},"speaker":"A","text":"高校の受験日が近づいてきましたね。"},{"id":{"d":291,"n":2,"t":2},"speaker":"B","text":"はい。あと1カ月です。"},{"id":{"d":291,"n":3,"t":2},"speaker":"A","text":"緊張していますか？"},{"id":{"d":291,"n":4,"t":2},"speaker":"B","text":"はい。毎日緊張感が増していく気がしています。"},{"id":{"d":291,"n":5,"t":2},"speaker":"A","text":"分かります。私も同じです。"},{"id":{"d":291,"n":6,"t":2},"speaker":"B","text":"近藤さんは、神社に合格祈願をしに行きましたか？"},{"id":{"d":291,"n":7,"t":2},"speaker":"A","text":"いえ、まだ行っていません。芦田さんは、行きましたか？"},{"id":{"d":291,"n":8,"t":2},"speaker":"B","text":"私もまだ参拝していません。良かったら、今日の放課後に、一緒に行きませんか？"},{"id":{"d":291,"n":9,"t":2},"speaker":"A","text":"いいですね。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":292,"n":1,"t":2},"speaker":"A","text":"今日から、新学期が始まりますね。"},{"id":{"d":292,"n":2,"t":2},"speaker":"B","text":"はい。クラス替えの発表があります。"},{"id":{"d":292,"n":3,"t":2},"speaker":"A","text":"緊張しますね。神田さんと同じクラスになりたいです。"},{"id":{"d":292,"n":4,"t":2},"speaker":"B","text":"私も、加藤さんと同じクラスになりたいです。"},{"id":{"d":292,"n":5,"t":2},"speaker":"A","text":"私たちは、同じクラスになったことがありませんね。"},{"id":{"d":292,"n":6,"t":2},"speaker":"B","text":"はい。小学校でも中学校でも一度もありません。"},{"id":{"d":292,"n":7,"t":2},"speaker":"A","text":"今年は3年生なので、最後の機会です。"},{"id":{"d":292,"n":8,"t":2},"speaker":"B","text":"そうですね。今年こそ同じクラスになれますように。"}];
INSERT INTO sentence [{"id":{"d":293,"n":1,"t":2},"speaker":"A","text":"遠藤さんは運転免許を持っていますか？"},{"id":{"d":293,"n":2,"t":2},"speaker":"B","text":"いいえ、まだ取得していません。今は、自動車教習所に通っています。"},{"id":{"d":293,"n":3,"t":2},"speaker":"A","text":"そうなのですね。いつから、通っているのですか？"},{"id":{"d":293,"n":4,"t":2},"speaker":"B","text":"高校を卒業して、すぐに通い始めました。"},{"id":{"d":293,"n":5,"t":2},"speaker":"A","text":"今は5月なので、もうすぐ卒業ですか？"},{"id":{"d":293,"n":6,"t":2},"speaker":"B","text":"はい。あと、学科の試験と運転のテストに合格すれば、免許を取得できます。"},{"id":{"d":293,"n":7,"t":2},"speaker":"A","text":"あと少しですね。私も先週から、自動車教習所に通い始めたところです。"},{"id":{"d":293,"n":8,"t":2},"speaker":"B","text":"そうだったのですね。お互い自動車の免許を取得したら、ぜひ、ドライブに行きましょう。"},{"id":{"d":293,"n":9,"t":2},"speaker":"A","text":"ぜひ、行きましょう。"}];
INSERT INTO sentence [{"id":{"d":294,"n":1,"t":2},"speaker":"A","text":"今日は雨の予報が出ていましたよね。"},{"id":{"d":294,"n":2,"t":2},"speaker":"B","text":"はい。でもまだ、雨は降っていませんね。"},{"id":{"d":294,"n":3,"t":2},"speaker":"A","text":"このまま、雨が降らないでほしいです。"},{"id":{"d":294,"n":4,"t":2},"speaker":"B","text":"何か予定があるのですか？"},{"id":{"d":294,"n":5,"t":2},"speaker":"A","text":"午後に体育の授業があり、グラウンドでサッカーをする予定なのです。"},{"id":{"d":294,"n":6,"t":2},"speaker":"B","text":"そういうことでしたか。雨の場合はどうなるのですか？"},{"id":{"d":294,"n":7,"t":2},"speaker":"A","text":"サッカーは中止になり、教室で自習をする時間になります。"},{"id":{"d":294,"n":8,"t":2},"speaker":"B","text":"体育館は、使えないのですか？"},{"id":{"d":294,"n":9,"t":2},"speaker":"A","text":"体育館は、他のクラスが使うようなので、私たちは使用できません。"},{"id":{"d":294,"n":10,"t":2},"speaker":"B","text":"なるほど。それでは、雨が降らないことを願うしかありませんね。"}];
INSERT INTO sentence [{"id":{"d":295,"n":1,"t":2},"speaker":"A","text":"夏休みが終わるまで、残り1週間しかありません。"},{"id":{"d":295,"n":2,"t":2},"speaker":"B","text":"夏休みの宿題は、どのくらい終わりましたか？"},{"id":{"d":295,"n":3,"t":2},"speaker":"A","text":"半分も終わっていません。あなたはどうですか？"},{"id":{"d":295,"n":4,"t":2},"speaker":"B","text":"私はもう終わりました。"},{"id":{"d":295,"n":5,"t":2},"speaker":"A","text":"素晴らしいです。計画的に宿題をしたのですか？"},{"id":{"d":295,"n":6,"t":2},"speaker":"B","text":"はい。去年、宿題に全く手を付けずに夏休みを終えて、後悔したので、今年は計画的に宿題をしました。"},{"id":{"d":295,"n":7,"t":2},"speaker":"A","text":"そうだったのですね。私は、まさに今、後悔をしています。"},{"id":{"d":295,"n":8,"t":2},"speaker":"B","text":"まだあと、1週間ありますよ。来年は、一緒に計画的に頑張りましょう。"},{"id":{"d":295,"n":9,"t":2},"speaker":"A","text":"はい。まずは、今年の宿題を成し遂げます。"}];
INSERT INTO sentence [{"id":{"d":296,"n":1,"t":2},"speaker":"A","text":"これは、模擬試験の成績表ですか？"},{"id":{"d":296,"n":2,"t":2},"speaker":"B","text":"はい。昨日返ってきました。まだお母さんに成績表を見せていませんでしたね。"},{"id":{"d":296,"n":3,"t":2},"speaker":"A","text":"見ていません。見てもよいですか？"},{"id":{"d":296,"n":4,"t":2},"speaker":"B","text":"よいですよ。"},{"id":{"d":296,"n":5,"t":2},"speaker":"A","text":"得意な科目と、苦手な科目が、はっきりしていますね。"},{"id":{"d":296,"n":6,"t":2},"speaker":"B","text":"はい。数学や理科などの理系の科目が得意です。"},{"id":{"d":296,"n":7,"t":2},"speaker":"A","text":"大学も理系の学部を選びますか？"},{"id":{"d":296,"n":8,"t":2},"speaker":"B","text":"そのつもりでいます。"}];
INSERT INTO sentence [{"id":{"d":297,"n":1,"t":2},"speaker":"A","text":"体育の先生から、今年は水泳の授業がないと聞きました。"},{"id":{"d":297,"n":2,"t":2},"speaker":"B","text":"そうなのですか。知らなかったです。なぜですか？"},{"id":{"d":297,"n":3,"t":2},"speaker":"A","text":"プールの改修工事をするため、使用中止になるそうです。"},{"id":{"d":297,"n":4,"t":2},"speaker":"B","text":"なるほど。それは仕方ありませんね。"},{"id":{"d":297,"n":5,"t":2},"speaker":"A","text":"はい。私は水泳が得意なので、少し残念です。"},{"id":{"d":297,"n":6,"t":2},"speaker":"B","text":"丸山さんは、泳ぎがとても速いですよね。水泳の習い事をしているのですか？"},{"id":{"d":297,"n":7,"t":2},"speaker":"A","text":"はい。幼稚園の年長の頃から水泳教室に通っています。"},{"id":{"d":297,"n":8,"t":2},"speaker":"B","text":"道理で泳ぎが速いわけですね。"}];
INSERT INTO sentence [{"id":{"d":298,"n":1,"t":2},"speaker":"A","text":"沢田さんは、転校の経験はありますか？"},{"id":{"d":298,"n":2,"t":2},"speaker":"B","text":"はい。小学生のときに2回、中学生のときに1回、転校しました。"},{"id":{"d":298,"n":3,"t":2},"speaker":"A","text":"それほどたくさん転校をしているのですか？"},{"id":{"d":298,"n":4,"t":2},"speaker":"B","text":"はい。親の転勤が多かったので、転校も多かったです。"},{"id":{"d":298,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。転校が多いと、大変ではありませんか？"},{"id":{"d":298,"n":6,"t":2},"speaker":"B","text":"もちろん大変なこともありますが、良いことも多いですよ。"},{"id":{"d":298,"n":7,"t":2},"speaker":"A","text":"例えば、どのようなことですか？"},{"id":{"d":298,"n":8,"t":2},"speaker":"B","text":"初対面の人と会話をすることに、抵抗がなくなります。そのおかげで、すぐに友達を作ることができます。"}];
INSERT INTO sentence [{"id":{"d":299,"n":1,"t":2},"speaker":"A","text":"あと1週間後には、期末テストが行われますね。"},{"id":{"d":299,"n":2,"t":2},"speaker":"B","text":"はい。数学が特に不安です。"},{"id":{"d":299,"n":3,"t":2},"speaker":"A","text":"今回は、試験範囲がかなり広いですよね。"},{"id":{"d":299,"n":4,"t":2},"speaker":"B","text":"そうなのです。試験範囲が変更になるといいのですが。"},{"id":{"d":299,"n":5,"t":2},"speaker":"A","text":"私もそう思ったのですが、先生が昨日の授業で、試験範囲は予定通りだとおっしゃっていました。"},{"id":{"d":299,"n":6,"t":2},"speaker":"B","text":"そうですか。それでは、勉強するしかありませんね。"},{"id":{"d":299,"n":7,"t":2},"speaker":"A","text":"頑張りましょう。今日の放課後、私の家で一緒に勉強しませんか？"},{"id":{"d":299,"n":8,"t":2},"speaker":"B","text":"いいですね。一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":300,"n":1,"t":2},"speaker":"A","text":"おはようございます。"},{"id":{"d":300,"n":2,"t":2},"speaker":"B","text":"おはようございます。体調は良くなりましたか？"},{"id":{"d":300,"n":3,"t":2},"speaker":"A","text":"はい。ただの風邪だったので、1日で治りました。"},{"id":{"d":300,"n":4,"t":2},"speaker":"B","text":"それは、良かったです。"},{"id":{"d":300,"n":5,"t":2},"speaker":"A","text":"昨日は、物理の授業がある日でしたよね。昨日の物理の授業中に作成したノートの内容を見せてもらえませんか？"},{"id":{"d":300,"n":6,"t":2},"speaker":"B","text":"もちろんです。どうぞ。"},{"id":{"d":300,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。助かります。"},{"id":{"d":300,"n":8,"t":2},"speaker":"B","text":"他の授業のノートも、必要だったらいつでもお声かけください。"}];
INSERT INTO sentence [{"id":{"d":301,"n":1,"t":2},"speaker":"A","text":"先生、すみません。"},{"id":{"d":301,"n":2,"t":2},"speaker":"B","text":"島田さん、どうされましたか？"},{"id":{"d":301,"n":3,"t":2},"speaker":"A","text":"上履きを忘れてしまいました。"},{"id":{"d":301,"n":4,"t":2},"speaker":"B","text":"そうでしたか。職員室には行きましたか？"},{"id":{"d":301,"n":5,"t":2},"speaker":"A","text":"いえ、まだです。"},{"id":{"d":301,"n":6,"t":2},"speaker":"B","text":"それでは、行ってみてください。貸出用の上履きがあります。"},{"id":{"d":301,"n":7,"t":2},"speaker":"A","text":"分かりました。ありがとうございます。"},{"id":{"d":301,"n":8,"t":2},"speaker":"B","text":"次からは、忘れ物をしないよう気を付けてくださいね。"},{"id":{"d":301,"n":9,"t":2},"speaker":"A","text":"はい。気を付けます。"}];
INSERT INTO sentence [{"id":{"d":302,"n":1,"t":2},"speaker":"A","text":"お母さん、私の体力テストの結果を見ましたか？"},{"id":{"d":302,"n":2,"t":2},"speaker":"B","text":"はい。とても良い成績でしたね。"},{"id":{"d":302,"n":3,"t":2},"speaker":"A","text":"初めてA評価をもらうことができました。"},{"id":{"d":302,"n":4,"t":2},"speaker":"B","text":"去年の評価は何でしたか？"},{"id":{"d":302,"n":5,"t":2},"speaker":"A","text":"C評価です。"},{"id":{"d":302,"n":6,"t":2},"speaker":"B","text":"1年で2段階も成績が上がったのですね。頑張りましたね。"},{"id":{"d":302,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。テニス部に入ったことで、運動量が増えました。"},{"id":{"d":302,"n":8,"t":2},"speaker":"B","text":"そうですね。毎日、欠かさず運動をしていますよね。"},{"id":{"d":302,"n":9,"t":2},"speaker":"A","text":"はい。これからも継続して、来年の体力テストも頑張ります。"}];
INSERT INTO sentence [{"id":{"d":303,"n":1,"t":2},"speaker":"A","text":"野田さんは、模擬試験で満点を取ったことはありますか？"},{"id":{"d":303,"n":2,"t":2},"speaker":"B","text":"1回だけあります。"},{"id":{"d":303,"n":3,"t":2},"speaker":"A","text":"すごいですね。何の教科ですか？"},{"id":{"d":303,"n":4,"t":2},"speaker":"B","text":"英語です。"},{"id":{"d":303,"n":5,"t":2},"speaker":"A","text":"英語は得意なのですか？"},{"id":{"d":303,"n":6,"t":2},"speaker":"B","text":"そうですね。英語の勉強が好きなので、模擬試験も英語が一番得意になりました。"},{"id":{"d":303,"n":7,"t":2},"speaker":"A","text":"普段どのように、英語を勉強しているのですか？"},{"id":{"d":303,"n":8,"t":2},"speaker":"B","text":"洋楽を聴いたり、海外ドラマを見たり、楽しみながら英語を学べるようにしています。"},{"id":{"d":303,"n":9,"t":2},"speaker":"A","text":"それは良い勉強法ですね。"}];
INSERT INTO sentence [{"id":{"d":304,"n":1,"t":2},"speaker":"A","text":"今週の日曜日は体育祭ですよね。"},{"id":{"d":304,"n":2,"t":2},"speaker":"B","text":"はい。そうです。"},{"id":{"d":304,"n":3,"t":2},"speaker":"A","text":"保護者も観覧できるのですか？"},{"id":{"d":304,"n":4,"t":2},"speaker":"B","text":"はい。できますよ。"},{"id":{"d":304,"n":5,"t":2},"speaker":"A","text":"それでは見に行きますね。種目は何に出るのですか？"},{"id":{"d":304,"n":6,"t":2},"speaker":"B","text":"100メートル走と、大縄跳びと、綱引きです。"},{"id":{"d":304,"n":7,"t":2},"speaker":"A","text":"なるほど。観覧にお薦めの種目はどれですか？"},{"id":{"d":304,"n":8,"t":2},"speaker":"B","text":"綱引きです。毎年恒例の目玉種目です。"},{"id":{"d":304,"n":9,"t":2},"speaker":"A","text":"それは楽しみです。"}];
INSERT INTO sentence [{"id":{"d":305,"n":1,"t":2},"speaker":"A","text":"いよいよ、大学生になりましたね。"},{"id":{"d":305,"n":2,"t":2},"speaker":"B","text":"大学の授業では、理数科目の授業がとても楽しみです。"},{"id":{"d":305,"n":3,"t":2},"speaker":"A","text":"具体的には、どの分野に興味がありますか？"},{"id":{"d":305,"n":4,"t":2},"speaker":"B","text":"バイオサイエンスです。高校よりも実験設備が充実しているので楽しみにしています。"},{"id":{"d":305,"n":5,"t":2},"speaker":"A","text":"とても、勉強熱心なのですね。"},{"id":{"d":305,"n":6,"t":2},"speaker":"B","text":"あなたは何が楽しみですか？"},{"id":{"d":305,"n":7,"t":2},"speaker":"A","text":"勉強はもちろんですが、学生食堂で食事をすることが楽しみです。"},{"id":{"d":305,"n":8,"t":2},"speaker":"B","text":"高校には、食堂はなかったのですか？"},{"id":{"d":305,"n":9,"t":2},"speaker":"A","text":"ありませんでした。いつも弁当を作るかコンビニエンスストアで食べ物を買うか、だったので学生食堂に憧れがあります。"},{"id":{"d":305,"n":10,"t":2},"speaker":"B","text":"それは楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":306,"n":1,"t":2},"speaker":"A","text":"日野さんは、今は学生ですか？"},{"id":{"d":306,"n":2,"t":2},"speaker":"B","text":"はい。専修学校に通っています。"},{"id":{"d":306,"n":3,"t":2},"speaker":"A","text":"何について学ぶ学校ですか？"},{"id":{"d":306,"n":4,"t":2},"speaker":"B","text":"美容師になるための学校です。"},{"id":{"d":306,"n":5,"t":2},"speaker":"A","text":"そうなのですね。将来は、美容師になるのですか？"},{"id":{"d":306,"n":6,"t":2},"speaker":"B","text":"はい。目指しています。今はまだ入ったばかりですが、卒業後は美容師になりたいです。"},{"id":{"d":306,"n":7,"t":2},"speaker":"A","text":"夢があって、すてきですね。ぜひ、日野さんに髪を切ってもらいたいです。"},{"id":{"d":306,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。夢を実現できるように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":307,"n":1,"t":2},"speaker":"A","text":"昨日の夜は、大きな落雷がありましたね。"},{"id":{"d":307,"n":2,"t":2},"speaker":"B","text":"はい。私は、なかなか寝付けませんでした。"},{"id":{"d":307,"n":3,"t":2},"speaker":"A","text":"落雷の影響で、電車が一部運休しているそうですよ。"},{"id":{"d":307,"n":4,"t":2},"speaker":"B","text":"そうなのですか。通学で使う電車は大丈夫でしょうか？"},{"id":{"d":307,"n":5,"t":2},"speaker":"A","text":"調べた方がいいですね。運休ではなくとも遅れは出ているかもしれません。"},{"id":{"d":307,"n":6,"t":2},"speaker":"B","text":"そうですね。調べてみます。"},{"id":{"d":307,"n":7,"t":2},"speaker":"A","text":"いつもより早く家を出られるよう、支度を急いだ方がいいかもしれません。"},{"id":{"d":307,"n":8,"t":2},"speaker":"B","text":"分かりました。急ぎます。"}];
INSERT INTO sentence [{"id":{"d":308,"n":1,"t":2},"speaker":"A","text":"高校での一番の思い出は、何ですか？"},{"id":{"d":308,"n":2,"t":2},"speaker":"B","text":"たくさんありますが、一番の思い出は、部活動ですかね。"},{"id":{"d":308,"n":3,"t":2},"speaker":"A","text":"明さんは、バドミントン部でしたね。"},{"id":{"d":308,"n":4,"t":2},"speaker":"B","text":"はい。最後の大会で優勝できたことも良い思い出ですし、大切な友達が多くできました。"},{"id":{"d":308,"n":5,"t":2},"speaker":"A","text":"すてきですね。"},{"id":{"d":308,"n":6,"t":2},"speaker":"B","text":"絵里さんの一番の思い出は何ですか？"},{"id":{"d":308,"n":7,"t":2},"speaker":"A","text":"私は高校3年生のときの体育祭です。"},{"id":{"d":308,"n":8,"t":2},"speaker":"B","text":"確か、絵里さんのクラスが優勝しましたよね。"},{"id":{"d":308,"n":9,"t":2},"speaker":"A","text":"はい。体育祭の後のクラス会も盛り上がりました。"},{"id":{"d":308,"n":10,"t":2},"speaker":"B","text":"高校時代の思い出話は、楽しいですね。近々、同窓会をしたいです。"},{"id":{"d":308,"n":11,"t":2},"speaker":"A","text":"いいですね。ぜひ、企画しましょう。"}];
INSERT INTO sentence [{"id":{"d":309,"n":1,"t":2},"speaker":"A","text":"先日、私が通っていた中学校の生徒を見かけて、懐かしくなりました。"},{"id":{"d":309,"n":2,"t":2},"speaker":"B","text":"母校の生徒を見ると、とても親近感が湧きますよね。"},{"id":{"d":309,"n":3,"t":2},"speaker":"A","text":"ただ、私が生徒の時といろいろと変わっていたので、少し違和感がありました。"},{"id":{"d":309,"n":4,"t":2},"speaker":"B","text":"時代は変わりますよね。ちなみに、どのようなところが変わっていましたか？"},{"id":{"d":309,"n":5,"t":2},"speaker":"A","text":"私が中学生の頃は、外靴は白で色が付いている靴は部活動の時の専用の靴のみ許されていました。しかし、今は靴の色は何色でもよいようです。黒の靴で登校している生徒を見ました。"},{"id":{"d":309,"n":6,"t":2},"speaker":"B","text":"及川君が通っていた時より、落ち着いた雰囲気の中学校になったのかもしれませんね。"},{"id":{"d":309,"n":7,"t":2},"speaker":"A","text":"そうですね。体育の授業をしているところが、たまに見えたりするのですが、みんな大人しくサッカーをしていて、私たちの頃のようなけがをしそうな体育ではありませんでした。"},{"id":{"d":309,"n":8,"t":2},"speaker":"B","text":"中学生は反抗期が多いのに、今の中学生は押さえつけられているのかもしれませんね。もし、そうならかわいそうですね。"}];
INSERT INTO sentence [{"id":{"d":310,"n":1,"t":2},"speaker":"A","text":"北海道大学の2016年入学者に占める北海道内の高校出身者は、3割程度になったそうです。"},{"id":{"d":310,"n":2,"t":2},"speaker":"B","text":"国立大学ということもあり、全国から学生が集まってきていることがうかがえますね。"},{"id":{"d":310,"n":3,"t":2},"speaker":"A","text":"北海道大学は、比較的第2次試験の難易度が低いこともあり、現役受験生にとって受験しやすい大学と考えられるようです。"},{"id":{"d":310,"n":4,"t":2},"speaker":"B","text":"一般選抜の前期日程合格者は1年生の時の成績によって、移行する学部が決定するそうですね。"},{"id":{"d":310,"n":5,"t":2},"speaker":"A","text":"高校の時には分からない、先輩の声を聞いたり、実際に研究室訪問をして雰囲気を知ったりして、移行することができるので、とても良いと思います。"},{"id":{"d":310,"n":6,"t":2},"speaker":"B","text":"入学後に学部移行ということは、大学の講義内容についていけない場合は、希望していない学部へ移行することもあということですか？"},{"id":{"d":310,"n":7,"t":2},"speaker":"A","text":"その通りです。大学入試で終わりではなく、入学後も競い合わないといけないということですね。"},{"id":{"d":310,"n":8,"t":2},"speaker":"B","text":"サークル活動と、勉強の両立を頑張らなければいけませんね。"}];
INSERT INTO sentence [{"id":{"d":311,"n":1,"t":2},"speaker":"A","text":"先週は自動車教習所の卒業検定でした。"},{"id":{"d":311,"n":2,"t":2},"speaker":"B","text":"お疲れさまでした。一発でしっかり合格できましたか？"},{"id":{"d":311,"n":3,"t":2},"speaker":"A","text":"はい。エンジンストールをすることなく試験官にも「問題は何もないから、今後もしっかり安全に運転するように」と告げられました。"},{"id":{"d":311,"n":4,"t":2},"speaker":"B","text":"試験管から、絶賛されたですね。素晴らしいです。私は一度、卒業検定に落ちました。"},{"id":{"d":311,"n":5,"t":2},"speaker":"A","text":"そうなのですね。オートマチック車の免許の卒業検定で不合格になるとは、何があったのですか？"},{"id":{"d":311,"n":6,"t":2},"speaker":"B","text":"路上で前に、大きなトラックがいたため信号を見ることができませんでした。その結果、赤信号で交差点に入ってしまいました。"},{"id":{"d":311,"n":7,"t":2},"speaker":"A","text":"それは不運ですね。試験官には、どのようなコメントをもらいましたか？"},{"id":{"d":311,"n":8,"t":2},"speaker":"B","text":"試験官も「これは不運だ」とおっしゃっていました。その後、2回目の試験で無事卒業することができました。"}];
INSERT INTO sentence [{"id":{"d":312,"n":1,"t":2},"speaker":"A","text":"先日、高校で体力テストがありました。"},{"id":{"d":312,"n":2,"t":2},"speaker":"B","text":"それは大変でしたね。高校の体力テストはどうでしたか？"},{"id":{"d":312,"n":3,"t":2},"speaker":"A","text":"同学年が400人近くいる中、上位15位を獲得しました。"},{"id":{"d":312,"n":4,"t":2},"speaker":"B","text":"素晴らしいですね。中学生の頃から運動が得意でしたか？"},{"id":{"d":312,"n":5,"t":2},"speaker":"A","text":"運動することは好きでしたが、特別良い成績だったわけではありません。"},{"id":{"d":312,"n":6,"t":2},"speaker":"B","text":"そうなのですね。"},{"id":{"d":312,"n":7,"t":2},"speaker":"A","text":"はい。しかし、高校では、文化部に所属しているにもかかわらず運動部の顧問の先生に「部活動の練習に体験として来てくれないか」と言われるほど、比較的、運動ができる方のようです。"},{"id":{"d":312,"n":8,"t":2},"speaker":"B","text":"顧問の先生に誘われるとは、相当運動ができるのですね。すごいですね。"}];
INSERT INTO sentence [{"id":{"d":313,"n":1,"t":2},"speaker":"A","text":"知り合いの保育士が、「仕事が大変だ」と言っていました。"},{"id":{"d":313,"n":2,"t":2},"speaker":"B","text":"テレビのニュース番組でも、保育士不足が多く取り上げられていますが、やはり大変なことが多いのですね。"},{"id":{"d":313,"n":3,"t":2},"speaker":"A","text":"そのようです。「毎日、日誌を書いて、次の日のお遊戯で使う小道具を作成して、寝るのは2時や3時」と言っていました。"},{"id":{"d":313,"n":4,"t":2},"speaker":"B","text":"大変ですね。そのような大変さがありながらも、給料が低いということが問題ですね。"},{"id":{"d":313,"n":5,"t":2},"speaker":"A","text":"家に帰って仕事をしているそうなので、残業としてではなく、より良いものをという考えでやっているものと、見なされているそうです。"},{"id":{"d":313,"n":6,"t":2},"speaker":"B","text":"それは、大変ですね。"}];
INSERT INTO sentence [{"id":{"d":314,"n":1,"t":2},"speaker":"A","text":"日本以外の先進国でも、理系離れが起きているようです。"},{"id":{"d":314,"n":2,"t":2},"speaker":"B","text":"理系科目が苦手という人は、算数から数学に変化した時に苦手になる人が多い気がします。"},{"id":{"d":314,"n":3,"t":2},"speaker":"A","text":"中学の時の数学は、ただ、式を覚えるようなイメージがどうしてもあるので、面白みを感じないのでしょう。"},{"id":{"d":314,"n":4,"t":2},"speaker":"B","text":"どのような教育をしたら、理系離れを食い止めることができると思いますか？"},{"id":{"d":314,"n":5,"t":2},"speaker":"A","text":"ある人が、雑談として話していたことが面白かったので紹介します。「中学の最初の数学の講義で理系科目ができると、将来お金持ちになれる可能性が高い」という話をすると言っていました。"},{"id":{"d":314,"n":6,"t":2},"speaker":"B","text":"確かに、理系の人は就職活動の際も重宝されることがありますね。"},{"id":{"d":314,"n":7,"t":2},"speaker":"A","text":"そうですね。その上で「欲しいものを買うためには、お金が必要である」ということ、「お金の計算は、数学が分からなければできない」ということを教えるそうです。"},{"id":{"d":314,"n":8,"t":2},"speaker":"B","text":"なるほど。数学ができた場合、どのような利点があり、生活が豊かになるか簡単に伝えるということですね。"}];
INSERT INTO sentence [{"id":{"d":315,"n":1,"t":2},"speaker":"A","text":"自分たちで、部活動を立ち上げたことはありますか？"},{"id":{"d":315,"n":2,"t":2},"speaker":"B","text":"今まで在籍した学校には、自分が入りたい部活動はあったので、作ろうと思ったことがありません。作ったことがあるのですか？"},{"id":{"d":315,"n":3,"t":2},"speaker":"A","text":"私が進学する予定だった中学にサッカー部がありませんでした。そこでサッカー部を立ち上げたことがあります。"},{"id":{"d":315,"n":4,"t":2},"speaker":"B","text":"サッカー部がない中学校があるとは、信じられませんが立ち上げたことはすごいですね。"},{"id":{"d":315,"n":5,"t":2},"speaker":"A","text":"最初からサッカー部として立ち上げることができたわけではなく、最初はサッカー同好会として立ち上げました。"},{"id":{"d":315,"n":6,"t":2},"speaker":"B","text":"同好会では、中学は活動を行うための予算がつかないので道具とかはどうしていたのですか？"},{"id":{"d":315,"n":7,"t":2},"speaker":"A","text":"近くの大学から、古くなって使わないボールなどをもらって練習をしていました。最初は練習場所もなかったので、グラウンドの隅っこで毎日基本的な練習しかしていませんでした。"},{"id":{"d":315,"n":8,"t":2},"speaker":"B","text":"1つのことを始める大変さを知ることができた、良い経験でしたね。中学生という若い時にそのような経験をすることは大切だと思います。"}];
INSERT INTO sentence [{"id":{"d":316,"n":1,"t":2},"speaker":"A","text":"芸術は勉強することも可能ですが、才能によるところもありますね。"},{"id":{"d":316,"n":2,"t":2},"speaker":"B","text":"小さい頃絵が上手な子どもは、才能があるということでしょうか？"},{"id":{"d":316,"n":3,"t":2},"speaker":"A","text":"そのような気がします。私は妹がいるのですが、妹はとても絵がうまく、小学生の頃に、全国的な大きな賞に入賞したことがあるほどです。"},{"id":{"d":316,"n":4,"t":2},"speaker":"B","text":"それはすごいですね。"},{"id":{"d":316,"n":5,"t":2},"speaker":"A","text":"絵が上手な妹に対し、私は全く絵心がなく、どうにか褒めようとした他の方に、「味がある絵」と言ってもらったことが何度もあります。"},{"id":{"d":316,"n":6,"t":2},"speaker":"B","text":"それは小さい頃からですか？それともだんだん苦手になってしまったのですか？"},{"id":{"d":316,"n":7,"t":2},"speaker":"A","text":"小さい頃から苦手でした。輪郭が確定せず、色も光の当たり方によって変わるので、まず下描きすらできません。同じ環境で育ったのに不思議です。"}];
INSERT INTO sentence [{"id":{"d":317,"n":1,"t":2},"speaker":"A","text":"小学校の近くを歩いていた時、小学生がグラウンドで遊んでいるところを見かけました。"},{"id":{"d":317,"n":2,"t":2},"speaker":"B","text":"私が小学生の頃は、毎日休み時間は外でサッカーをしていましたが、今の小学生は何をしていましたか？"},{"id":{"d":317,"n":3,"t":2},"speaker":"A","text":"サッカーはしていませんでした。それどころかサッカーのゴールすら、もう置いていませんでした。"},{"id":{"d":317,"n":4,"t":2},"speaker":"B","text":"そのようなことになっているのですね。私が小学生だった頃は、休み時間にサッカー以外をしている人は、ドッジボールをしていたので、今の小学生もドッジボールをしているのでしょうか？"},{"id":{"d":317,"n":5,"t":2},"speaker":"A","text":"それが、ドッジボールをしている人もいませんでした。"},{"id":{"d":317,"n":6,"t":2},"speaker":"B","text":"サッカーでも、ドッジボールでもない、休み時間に遊ぶものが考えもつきません。何がありますか？"},{"id":{"d":317,"n":7,"t":2},"speaker":"A","text":"縄跳びをしている子がいました。他の子は、追いかけっこや、ダンスをしているようでした。"},{"id":{"d":317,"n":8,"t":2},"speaker":"B","text":"なるほど、ダンスですか。最近は体育の授業にもダンスがあるそうですし、今の小学生にとっては一般的な遊びなのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":318,"n":1,"t":2},"speaker":"A","text":"業間休みを知っていますか？"},{"id":{"d":318,"n":2,"t":2},"speaker":"B","text":"知りません。業間休みとは何ですか？"},{"id":{"d":318,"n":3,"t":2},"speaker":"A","text":"小学校の時間割の、2限目と3限目の授業の間にある、20分間の休み時間のことです。"},{"id":{"d":318,"n":4,"t":2},"speaker":"B","text":"「業間休み」という言葉は一部の地域に特有のようです。他の地域では「中休み」というところもあるようです。"},{"id":{"d":318,"n":5,"t":2},"speaker":"A","text":"一部の地域だけで使われている言葉だとは知りませんでした。小学生の時は、全員が知っている言葉だと思っていました。"},{"id":{"d":318,"n":6,"t":2},"speaker":"B","text":"小学生の時は、転校生も同じ県の人が多かったりするので、そこまで違和感がなかったのかもしれませんね。"},{"id":{"d":318,"n":7,"t":2},"speaker":"A","text":"そうですね。昼休みはもちろん好きでしたが、給食後に動きすぎるとおなかが痛くなっていたので、業間休みの方が楽しかった覚えがあります。"},{"id":{"d":318,"n":8,"t":2},"speaker":"B","text":"業間休みも、昼休みも、外で遊ぶとは、小学生は、やはり体力があり余っているようですね。"}];
INSERT INTO sentence [{"id":{"d":319,"n":1,"t":2},"speaker":"A","text":"大学院の修士論文は提出したのですが、単位の計算が合っているのか不安で手放しに喜べません。"},{"id":{"d":319,"n":2,"t":2},"speaker":"B","text":"大学は単位によって卒業が決まることもあり、完全自己責任なので、取得単位数は怖い部分ですね。"},{"id":{"d":319,"n":3,"t":2},"speaker":"A","text":"そうなのです。学部生の時に単位数を間違えていて卒業できない可能性があったためより不安になっています。"},{"id":{"d":319,"n":4,"t":2},"speaker":"B","text":"そのようなことがあったのですね。今回は1人で確認したのですか？"},{"id":{"d":319,"n":5,"t":2},"speaker":"A","text":"2人の友達に確認してもらいました。その上で大丈夫だよと言ってもらったのですが、どうしても不安が残ります。"},{"id":{"d":319,"n":6,"t":2},"speaker":"B","text":"やはり卒業できるかどうかは、どれだけ確認しても不安ですね。"},{"id":{"d":319,"n":7,"t":2},"speaker":"A","text":"加えて、就職先も第1志望に内定を頂いたため、卒業できないことにより、その内定が取消しになることへの不安もあります。"},{"id":{"d":319,"n":8,"t":2},"speaker":"B","text":"どうしても、ネガティブな考えはついて回ることなので今から何も変えることはできませんし、就職後のことを今は考えましょう。"}];
INSERT INTO sentence [{"id":{"d":320,"n":1,"t":2},"speaker":"A","text":"中学生の時の制服は詰め襟の男子用学生服でしたか？"},{"id":{"d":320,"n":2,"t":2},"speaker":"B","text":"私の中学校は、詰め襟の男子用学生服でした。詰め襟の男子用学生服の方が格好良いので私は好きです。"},{"id":{"d":320,"n":3,"t":2},"speaker":"A","text":"私も格好良いと思うので好きです。ただ、中学生の時は詰め襟の男子用学生服で少し大変な思いをしました。"},{"id":{"d":320,"n":4,"t":2},"speaker":"B","text":"詰め襟の男子用学生服で大変な思いとは、どのようなことがあったのですか？"},{"id":{"d":320,"n":5,"t":2},"speaker":"A","text":"中学生の時に成長期が来て、25センチメートルほど身長が伸びました。その影響で学生服が小さく感じるようになってしまったのです。"},{"id":{"d":320,"n":6,"t":2},"speaker":"B","text":"身長が25センチメートルも伸びることを予想して制服は買っていないと思うので、大変ですね。どのように対処したのですか？"},{"id":{"d":320,"n":7,"t":2},"speaker":"A","text":"父が高校の時に着ていた詰め襟の男子用学生服が祖父母の家に保管してあったため、それを着ました。"},{"id":{"d":320,"n":8,"t":2},"speaker":"B","text":"おじいさまと、おばあさまがしっかり保管してくださっていたとのこと、良かったですね。無事着ることができたなら安心しました。"}];
INSERT INTO sentence [{"id":{"d":321,"n":1,"t":2},"speaker":"A","text":"今年の大学入試センター試験は、「難化した」と言われていますね。"},{"id":{"d":321,"n":2,"t":2},"speaker":"B","text":"そのようですね。特に数学が難しかったそうです。"},{"id":{"d":321,"n":3,"t":2},"speaker":"A","text":"「大学入試センターの方針が見えた」と専門家は言っていました。"},{"id":{"d":321,"n":4,"t":2},"speaker":"B","text":"やはり大切になるのは、課題を論理的に考える力ということでしょう。文系の人へ最低限必要な論理的思考としての基準を示したのだと思います。"},{"id":{"d":321,"n":5,"t":2},"speaker":"A","text":"国公立大学を志望している文系生徒は、必ず数学の大学入試センター試験を受けないといけないので、必要最低限の数学の学力を示しているという点では、意図がとても分かりやすかったですね。"},{"id":{"d":321,"n":6,"t":2},"speaker":"B","text":"暗記で数学を済ませることは意味がないということを分かりやすく提示したものだという印象を受けました。"},{"id":{"d":321,"n":7,"t":2},"speaker":"A","text":"ただこれによって数学などの理系科目離れが進みそうで、少し危機感を感じました。"},{"id":{"d":321,"n":8,"t":2},"speaker":"B","text":"難しいならやらないという選択肢がある分、そのような考えの生徒は増えそうですね。理系への関心を増やすのは難しそうです。"}];
INSERT INTO sentence [{"id":{"d":322,"n":1,"t":2},"speaker":"A","text":"今までの学生生活で恩師と呼べる先生はいますか？"},{"id":{"d":322,"n":2,"t":2},"speaker":"B","text":"中学の時の部活動の顧問の先生と、高校の時の担任の先生が恩師です。"},{"id":{"d":322,"n":3,"t":2},"speaker":"A","text":"2人もいるのですね。それぞれどのような思い出がありますか？"},{"id":{"d":322,"n":4,"t":2},"speaker":"B","text":"中学の時の顧問の先生はとにかく厳しく、とても怖かったことを覚えています。部活動の内容より物を大切にしないことや、人への感謝の気持ちを持つという人として大切なことを教えていただきました。"},{"id":{"d":322,"n":5,"t":2},"speaker":"A","text":"中学生はまだ小学生の延長でもあるので、中学のタイミングでそのようなことを学んだのは良い経験になりましたね。"},{"id":{"d":322,"n":6,"t":2},"speaker":"B","text":"はい。高校の先生は3年間同じクラスだったこともあり、私のことをとても理解してくれました。飽きやすく、楽しいことしかやろうとしない私の性格を矯正しようとはせず、勉強以外のさまざまなことを提案してくれました。"},{"id":{"d":322,"n":7,"t":2},"speaker":"A","text":"教師なのに勉強以外のことを提案してくれるのは、あなたのことを理解し、信頼してくださっている証拠ですね。"},{"id":{"d":322,"n":8,"t":2},"speaker":"B","text":"そうです。その時も良い先生だと思っていましたが、大学生になった今、より一層感謝の気持ちが大きくなっています。"}];
INSERT INTO sentence [{"id":{"d":323,"n":1,"t":2},"speaker":"A","text":"昨日は三者面談がありました。"},{"id":{"d":323,"n":2,"t":2},"speaker":"B","text":"どのようなことが話題になりましたか？"},{"id":{"d":323,"n":3,"t":2},"speaker":"A","text":"卒業後の進路について、いろいろと話し合いました。"},{"id":{"d":323,"n":4,"t":2},"speaker":"B","text":"もう進路を考える時期なのですね。私はまだ卒業後のことは何も決めていません。"},{"id":{"d":323,"n":5,"t":2},"speaker":"A","text":"私は大学に進学したいのですが、受験する大学が具体的に決まらなくて悩んでいます。"},{"id":{"d":323,"n":6,"t":2},"speaker":"B","text":"志望校を決めるのは難しいですよね。先生から何か、アドバイスはもらえましたか？"},{"id":{"d":323,"n":7,"t":2},"speaker":"A","text":"興味のある大学のオープンキャンパスに参加してみてはどうかと提案されました。それぞれの大学の雰囲気を知るために参加してみるつもりです。"},{"id":{"d":323,"n":8,"t":2},"speaker":"B","text":"それはいいですね。私もあさってに三者面談があるので、自分の進路についてよく考えてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":324,"n":1,"t":2},"speaker":"A","text":"私は理系ですが、高校の時、社会科目は世界史、日本史、政治経済の3科目しかありませんでした。"},{"id":{"d":324,"n":2,"t":2},"speaker":"B","text":"地理などは、なかったのですね。"},{"id":{"d":324,"n":3,"t":2},"speaker":"A","text":"そうです。ところが自分が行きたかった大学の理学部は、世界史、日本史、地理、倫理政経の4つから2科目を選択して受験しなければならなかったのです。"},{"id":{"d":324,"n":4,"t":2},"speaker":"B","text":"世界史や日本史を選択するということでは、ないのですか？"},{"id":{"d":324,"n":5,"t":2},"speaker":"A","text":"世界史や日本史は覚えることが多く、理系科目の第2次試験の勉強をしないといけない理系学生には少し大変でした。"},{"id":{"d":324,"n":6,"t":2},"speaker":"B","text":"それでは、社会は何を受験したのですか？"},{"id":{"d":324,"n":7,"t":2},"speaker":"A","text":"私は独学で地理を勉強して受験しました。多くの理系学生が、地理を選択していた記憶があります。"},{"id":{"d":324,"n":8,"t":2},"speaker":"B","text":"そうだったのですね。確かに、私の周りでも理系の人は地理選択が多かった印象です。"}];
INSERT INTO sentence [{"id":{"d":325,"n":1,"t":2},"speaker":"A","text":"生徒会に入っていたことは、ありますか？"},{"id":{"d":325,"n":2,"t":2},"speaker":"B","text":"ありません。生徒会に入ると変に目立ちそうで嫌でした。生徒会役員でしたか？"},{"id":{"d":325,"n":3,"t":2},"speaker":"A","text":"私は生徒会副会長をしました。いろいろ経験することができたので楽しかったです。"},{"id":{"d":325,"n":4,"t":2},"speaker":"B","text":"副会長はすごいですね。なぜしたのですか？"},{"id":{"d":325,"n":5,"t":2},"speaker":"A","text":"私の所属していた部活動は、私が1年生の時に作った部活動で、最初は同好会だったため普通は予算を付けることができませんでした。しかし、お金が必要だと思い、予算を付けるために副会長をしました。"},{"id":{"d":325,"n":6,"t":2},"speaker":"B","text":"普通の人の立候補の理由と、全然違う気がしますができて良かったですね。予算を付けることはできたのですか？"},{"id":{"d":325,"n":7,"t":2},"speaker":"A","text":"もちろん付けました。同好会の時から、学内の部活動の中で4番目に高い予算を付けました。次の年に部活動になりましたが、その時には2番目の予算を付けています。"},{"id":{"d":325,"n":8,"t":2},"speaker":"B","text":"しっかり実行できていますね。決めたことを実行できることは良いことですね。"}];
INSERT INTO sentence [{"id":{"d":326,"n":1,"t":2},"speaker":"A","text":"今までの運動会や体育祭で、何か思い出はありますか？"},{"id":{"d":326,"n":2,"t":2},"speaker":"B","text":"小学校の時の運動会が一番思い出に残っています。"},{"id":{"d":326,"n":3,"t":2},"speaker":"A","text":"何か特別なことがあったのですか？"},{"id":{"d":326,"n":4,"t":2},"speaker":"B","text":"小学校の時の運動会で応援団長をしました。また、途中で大雨が降ってしまい、運動会が2日に分けられたことも思い出に残っています。"},{"id":{"d":326,"n":5,"t":2},"speaker":"A","text":"応援団長は思い出に残りますね。スローガンや応援を考えたり、みんなに応援を教えたり、忙しいけど楽しい役ですね。"},{"id":{"d":326,"n":6,"t":2},"speaker":"B","text":"はい。加えて、私のチームは当時の仲が良かった子が多く、とても楽しかった思い出があります。"},{"id":{"d":326,"n":7,"t":2},"speaker":"A","text":"仲が良い人が近くにいるとそれだけで全て楽しくなりますね。結果は勝ちましたか？"},{"id":{"d":326,"n":8,"t":2},"speaker":"B","text":"もちろん勝ちました。リレーも勝ったので完全優勝をしました。うれしすぎて受け取った後に朝礼台から飛び降りて先生に注意されたことも覚えています。"}];
INSERT INTO sentence [{"id":{"d":327,"n":1,"t":2},"speaker":"A","text":"小学生の時に、授業参観はありましたか？"},{"id":{"d":327,"n":2,"t":2},"speaker":"B","text":"ありました。とても緊張したし、何か恥ずかしかったことを覚えています。"},{"id":{"d":327,"n":3,"t":2},"speaker":"A","text":"私も同じです。両親だけでなく、友達のお父さん、お母さんにも見られていたので、落ち着かない気分になりました。"},{"id":{"d":327,"n":4,"t":2},"speaker":"B","text":"思い出に残っている授業はありますか？"},{"id":{"d":327,"n":5,"t":2},"speaker":"A","text":"小学校2年生のときの算数の授業です。当時、唯一苦手だった九九の七の段が当たってしまいとても焦りました。"},{"id":{"d":327,"n":6,"t":2},"speaker":"B","text":"それは災難ですね。無事、答えられたのですか？"},{"id":{"d":327,"n":7,"t":2},"speaker":"A","text":"それが覚えていないのです。きっと、パニックになり記憶が飛んでしまったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":328,"n":1,"t":2},"speaker":"A","text":"私の小学校は、教室を分ける壁や扉がありませんでした。"},{"id":{"d":328,"n":2,"t":2},"speaker":"B","text":"珍しいですね。授業中に他のクラスの声が聞こえることはありませんでしたか？"},{"id":{"d":328,"n":3,"t":2},"speaker":"A","text":"とてもよく聞こえていました。楽しそうな授業しているなと感じたことが、何回もあります。"},{"id":{"d":328,"n":4,"t":2},"speaker":"B","text":"授業に集中できないので、壁や扉がない教室には、あまり利点がないように感じます。"},{"id":{"d":328,"n":5,"t":2},"speaker":"A","text":"教室に壁や扉がないことの利点は、教室が扉で仕切られていないので、クラス以外の友達とも簡単に関わることができたことです。この点は良かったです。"},{"id":{"d":328,"n":6,"t":2},"speaker":"B","text":"確かに、休み時間に教室にいても他のクラスの人が遊びに来やすく、友達がたくさんできそうですね。"},{"id":{"d":328,"n":7,"t":2},"speaker":"A","text":"分け隔てなく、全員と仲良くなることができたのはこの壁がなかった教室の作りのおかげかなと思っています。"},{"id":{"d":328,"n":8,"t":2},"speaker":"B","text":"小学生の時に全員が仲良く生活しているのはとても良いことですね。良い学校です。"}];
INSERT INTO sentence [{"id":{"d":329,"n":1,"t":2},"speaker":"A","text":"学校のチャイムは、どのようなものでしたか？"},{"id":{"d":329,"n":2,"t":2},"speaker":"B","text":"ごく普通のものでした。何か異なるものだったのですか？"},{"id":{"d":329,"n":3,"t":2},"speaker":"A","text":"私の通っていた小学校には、チャイムがありませんでした。"},{"id":{"d":329,"n":4,"t":2},"speaker":"B","text":"チャイムがない中で、授業に遅れることはなかったのですか？"},{"id":{"d":329,"n":5,"t":2},"speaker":"A","text":"休み時間に遊んでいたサッカーが盛り上がってしまい、6年間で数回は、授業に間に合わなかったことはありますが、それ以外は遅れたことはありません。"},{"id":{"d":329,"n":6,"t":2},"speaker":"B","text":"遊び盛りの小学生の時に、遅刻があまりなかったのはすごいですね。しっかり時計を見て生活していたのですね。"},{"id":{"d":329,"n":7,"t":2},"speaker":"A","text":"小学生の時には全く気付きませんでしたが、常に5分前行動をしていたため、今でも習慣付いていて、とてもありがたいことだと思っています。"},{"id":{"d":329,"n":8,"t":2},"speaker":"B","text":"小学生の時に習慣となっていたものは、無意識に今でも習慣になっているものが多いので、良い習慣が身に付いて良かったですね。"}];
INSERT INTO sentence [{"id":{"d":330,"n":1,"t":2},"speaker":"A","text":"今日小学校の近くを通った時に、小学生が縄跳びをしていたのを見ました。"},{"id":{"d":330,"n":2,"t":2},"speaker":"B","text":"懐かしいですね。同学年の中で一番二重跳びがうまかった自信があります。"},{"id":{"d":330,"n":3,"t":2},"speaker":"A","text":"それはすごいですね。何か勝負でもしたのですか？"},{"id":{"d":330,"n":4,"t":2},"speaker":"B","text":"はい。体育館で学年全員参加の縄跳び大会がありました。前跳びや後ろ跳び、二重跳びなど、さまざまな種目を競いました。"},{"id":{"d":330,"n":5,"t":2},"speaker":"A","text":"前跳びや後ろ跳びはどのように競っていたのですか？"},{"id":{"d":330,"n":6,"t":2},"speaker":"B","text":"時間で競いました。その時は前飛び、後ろ跳びともに2位でしたが、二重跳びは97回も跳べたので学年1位になりました。"},{"id":{"d":330,"n":7,"t":2},"speaker":"A","text":"97回はすごいですね。それだけできたらもう十分です。"},{"id":{"d":330,"n":8,"t":2},"speaker":"B","text":"ただ今は何回できるか分からないので、今の実力を知る意味でも挑戦してみたいです。"}];
INSERT INTO sentence [{"id":{"d":331,"n":1,"t":2},"speaker":"A","text":"私の大学ではオンライン講義が、主に行われています。"},{"id":{"d":331,"n":2,"t":2},"speaker":"B","text":"最近は、ほとんどの大学がオンライン講義をしていますね。"},{"id":{"d":331,"n":3,"t":2},"speaker":"A","text":"今年の3月に卒業予定なので結局大学院の講義は、全てオンラインで終わってしまいました。"},{"id":{"d":331,"n":4,"t":2},"speaker":"B","text":"オンライン講義はどうでしたか？学部の4年間は対面だったと思いますが、どちらの方が良かったですか？"},{"id":{"d":331,"n":5,"t":2},"speaker":"A","text":"緊張感があったのは、もちろん対面でした。しかし、単位を取り切った上で興味のあることをテストなどは関係なしに学ぶのであれば、オンラインの方がいいと思いました。"},{"id":{"d":331,"n":6,"t":2},"speaker":"B","text":"それぞれの良さがあるということですね。両方経験できたのは、良いことでしたか？"},{"id":{"d":331,"n":7,"t":2},"speaker":"A","text":"正直、大学院の講義で対面を1回は経験したかったと思いました。せっかく大学院まで進んだので、しっかり大学院の雰囲気を経験したかったです。"},{"id":{"d":331,"n":8,"t":2},"speaker":"B","text":"こればかりは誰も悪くないことなので、仕方ありませんね。大学院2年間、お疲れさまでした。"}];
INSERT INTO sentence [{"id":{"d":332,"n":1,"t":2},"speaker":"A","text":"ニュースの特集で放送していましたが、交換留学が現在、実施できていないようです。"},{"id":{"d":332,"n":2,"t":2},"speaker":"B","text":"語学系の大学は留学をしたい人はたくさんいるはずなのに、留学の方法の一つとして存在している交換留学ができないことはかわいそうですね。"},{"id":{"d":332,"n":3,"t":2},"speaker":"A","text":"実際に使うことができる語学は、やはり留学で身に付くと思いますので、せっかく語学系の大学に進学したのにかわいそうですね。"},{"id":{"d":332,"n":4,"t":2},"speaker":"B","text":"しかし、なぜ交換留学生ができないのですか？"},{"id":{"d":332,"n":5,"t":2},"speaker":"A","text":"日本政府が、外国人の入国を制限しているということが理由としてあります。"},{"id":{"d":332,"n":6,"t":2},"speaker":"B","text":"つまり日本の生徒は外国に行くことができるけれど、外国の生徒が入国できないため、交換留学ができないということですか？"},{"id":{"d":332,"n":7,"t":2},"speaker":"A","text":"そのようです。このご時世なので仕方ありませんが、運が悪いという他ありませんね。"}];
INSERT INTO sentence [{"id":{"d":333,"n":1,"t":2},"speaker":"A","text":"高校の時、学生食堂はありましたか？"},{"id":{"d":333,"n":2,"t":2},"speaker":"B","text":"学生食堂はありましたが、メニューも少なく利用者は少なかったです。あなたの高校にはありましたか？"},{"id":{"d":333,"n":3,"t":2},"speaker":"A","text":"私の高校にはありました。毎日日替わり定食があるくらい充実したものでした。"},{"id":{"d":333,"n":4,"t":2},"speaker":"B","text":"高校で日替わりがあるのは良いですね。"},{"id":{"d":333,"n":5,"t":2},"speaker":"A","text":"名前も学生食堂ではなく、カフェテリアという名前でした。カフェというだけあってフライドポテトなどをみんなでシェアできるようなものもありました。"},{"id":{"d":333,"n":6,"t":2},"speaker":"B","text":"それは、うらやましいです。みんなでシェアできると学生食堂で友達と食べるという習慣がありそうですね。"},{"id":{"d":333,"n":7,"t":2},"speaker":"A","text":"部活動の友達と食べていました。また、部活動が終わるくらいの時間にその日残ったメニューがとても安く大盛りで食べられたので、部活動が終わった後にみんなで行っていました。"},{"id":{"d":333,"n":8,"t":2},"speaker":"B","text":"とても楽しそうな学校生活ですね。"}];
INSERT INTO sentence [{"id":{"d":334,"n":1,"t":2},"speaker":"A","text":"大学の学園祭は、楽しかったですか？"},{"id":{"d":334,"n":2,"t":2},"speaker":"B","text":"とても楽しかったです。留学生が、自国の料理を出店として出していました。"},{"id":{"d":334,"n":3,"t":2},"speaker":"A","text":"それでは、本場に似た味を大学の学園祭で食べることができたのですね。とても、良い経験ですね。"},{"id":{"d":334,"n":4,"t":2},"speaker":"B","text":"出店している人は、「母国の方々が手伝いに来ていて、材料なども本国から持ってきている」と言っていました。"},{"id":{"d":334,"n":5,"t":2},"speaker":"A","text":"本物の味ということですね。それはおいしかったですか？本場の味はおいしいものとそうでもないものが分かれると聞いた事があります。"},{"id":{"d":334,"n":6,"t":2},"speaker":"B","text":"本場の調味料を使いながら、日本人好みの味にしていたので、とてもおいしく食べることができました。"},{"id":{"d":334,"n":7,"t":2},"speaker":"A","text":"それでは、値段はとても高かったのではありませんか？"},{"id":{"d":334,"n":8,"t":2},"speaker":"B","text":"それがとても安く食べることができ、店員さんと仲良くなるとさらに量を増やしてくれたり、他の商品を付けてくれたりしました。とても良い経験でした。"}];
INSERT INTO sentence [{"id":{"d":335,"n":1,"t":2},"speaker":"A","text":"体育の座学を、受けたことはありますか？"},{"id":{"d":335,"n":2,"t":2},"speaker":"B","text":"保健ではなく体育の座学ですか？"},{"id":{"d":335,"n":3,"t":2},"speaker":"A","text":"そうです。体育には実技と座学があります。大学の講義で、座学の体育がありました。"},{"id":{"d":335,"n":4,"t":2},"speaker":"B","text":"どのようなことを学ぶのですか？"},{"id":{"d":335,"n":5,"t":2},"speaker":"A","text":"人間の体の使い方を学びました。どのような使い方をすると、効率よく運動することができるのかということが主な講義の内容でした。"},{"id":{"d":335,"n":6,"t":2},"speaker":"B","text":"なぜ座学の体育を選択したのですか？実技の体育の方が楽しそうに思います。"},{"id":{"d":335,"n":7,"t":2},"speaker":"A","text":"私の大学は学生数が多いため、講義は選択しても抽選になることがあります。私はその抽選で外れたため座学の体育を選択しました。"},{"id":{"d":335,"n":8,"t":2},"speaker":"B","text":"座学を取りたくて取った訳ではないのですね。実技では学べないことを、座学で学ぶことができたのならば良かったです。"}];
INSERT INTO sentence [{"id":{"d":336,"n":1,"t":2},"speaker":"A","text":"大学では、企業との共同研究をする場合もあるようですね。"},{"id":{"d":336,"n":2,"t":2},"speaker":"B","text":"工学の分野ではよくありますね。私たちは数学専攻ですが、どこか共同研究をしたい企業がありますか？"},{"id":{"d":336,"n":3,"t":2},"speaker":"A","text":"私の研究内容は、工学にも関係するものなので企業ではどのような用いられ方をしているのか、数学の必要とされる知識は何なのかを学びたいと考えています。"},{"id":{"d":336,"n":4,"t":2},"speaker":"B","text":"私は今まで共同研究は、他の大学の同じ研究をしている先生としていたので、どのように企業との共同研究をするのか分かりませんが、検討してみましょう。"},{"id":{"d":336,"n":5,"t":2},"speaker":"A","text":"企業の例としてはコンピューターのシステムを作っているような企業が良いです。"},{"id":{"d":336,"n":6,"t":2},"speaker":"B","text":"自分で交渉することが可能ならば、そちらの方が企業へ自分の要望を伝えることができると思います。"},{"id":{"d":336,"n":7,"t":2},"speaker":"A","text":"その際には教授の名前を伝えた上で、共同研究を申し込んでもよろしいですか？"},{"id":{"d":336,"n":8,"t":2},"speaker":"B","text":"もちろんです。もし、何か私の名前を書く必要のあるのならば必要書類を研究室に置いておいてください。"}];
INSERT INTO sentence [{"id":{"d":337,"n":1,"t":2},"speaker":"A","text":"来月末に、卒業式がありますが出席しますか？"},{"id":{"d":337,"n":2,"t":2},"speaker":"B","text":"学部生の時は卒業式がなく、少し寂しかったので大学院の卒業式は出席しようと思います。"},{"id":{"d":337,"n":3,"t":2},"speaker":"A","text":"大塩さんが出席するなら、私も出席しようと思います。最後ですから、一緒に写真を撮りましょう。"},{"id":{"d":337,"n":4,"t":2},"speaker":"B","text":"式は何時から始まるか、ご存知ですか？"},{"id":{"d":337,"n":5,"t":2},"speaker":"A","text":"大学院は全ての専攻が、10:00から始まるようです。場所は第一体育館です。"},{"id":{"d":337,"n":6,"t":2},"speaker":"B","text":"全ての専攻が参加するということは、修了書は違う場所で渡されるということですか？"},{"id":{"d":337,"n":7,"t":2},"speaker":"A","text":"修了書は午後にそれぞれの専攻の建物内で授与すると書いてありました。つまり、昼食を食べないといけませんね。"},{"id":{"d":337,"n":8,"t":2},"speaker":"B","text":"それでは、卒業式の日は大学を訪れる最後の機会として、学生食堂で昼食を食べましょう。"}];
INSERT INTO sentence [{"id":{"d":338,"n":1,"t":2},"speaker":"A","text":"本校を受験した理由を教えてください。"},{"id":{"d":338,"n":2,"t":2},"speaker":"B","text":"はい。きれいな教室や図書館で勉強をしたいと思ったことが理由です。加えてグラウンドが6面もあり、施設も充実しているので、幸せな高校生活を送れると考えています。"},{"id":{"d":338,"n":3,"t":2},"speaker":"A","text":"グラウンド以外に魅力に感じた本校の施設は、何かありますか？"},{"id":{"d":338,"n":4,"t":2},"speaker":"B","text":"体育館も2つあり、ジムもあることが魅力だと思っています。"},{"id":{"d":338,"n":5,"t":2},"speaker":"A","text":"何か、運動をされているのですか？"},{"id":{"d":338,"n":6,"t":2},"speaker":"B","text":"私はサッカーをしています。ただ、高校では他のスポーツをしてみたいとも考えています。"},{"id":{"d":338,"n":7,"t":2},"speaker":"A","text":"本校には運動部がたくさんあるので、合格したら見学に行って興味のある部活動を見つけてください。"},{"id":{"d":338,"n":8,"t":2},"speaker":"B","text":"分かりました。ぜひ、合格してさまざまな部活動を体験してみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":339,"n":1,"t":2},"speaker":"A","text":"小林さんの得意科目は国語と言っていましたね。何か理由があるのですか？"},{"id":{"d":339,"n":2,"t":2},"speaker":"B","text":"中学時代の国語の先生のおかげだと思います。部活動のない日の放課後に、図書室に希望者を集めて長文読解の特別講義を開いてくださっていました。"},{"id":{"d":339,"n":3,"t":2},"speaker":"A","text":"必修ではない特別講義に参加するとは、小林さんは優等生だったのですね？"},{"id":{"d":339,"n":4,"t":2},"speaker":"B","text":"そのようなことはありません。人気のあるすてきな先生だったので、特別講義には大勢の生徒が参加していました。"},{"id":{"d":339,"n":5,"t":2},"speaker":"A","text":"どのような内容の講義だったのですか？"},{"id":{"d":339,"n":6,"t":2},"speaker":"B","text":"難関大学の国語の入試問題を解いて、長文読解を学ぶというものでした。読解力が身に付きました。"},{"id":{"d":339,"n":7,"t":2},"speaker":"A","text":"大学の試験問題ですか。それは中学生にはかなりハイレベルですね。"},{"id":{"d":339,"n":8,"t":2},"speaker":"B","text":"当然、私たち中学生には難問でした。そのような中で、普段は成績の振るわない同級生が度々正解を出していて、彼は一躍ヒーローになっていました。"}];
INSERT INTO sentence [{"id":{"d":340,"n":1,"t":2},"speaker":"A","text":"本校では芸術科目は選択形式となっています。3年間同じ科目を履修することになりますのでしっかりと考えて選択してください。"},{"id":{"d":340,"n":2,"t":2},"speaker":"B","text":"どのような科目があり、何科目選択するのですか？"},{"id":{"d":340,"n":3,"t":2},"speaker":"A","text":"音楽、書道、美術の中から1科目選択してください。"},{"id":{"d":340,"n":4,"t":2},"speaker":"B","text":"選択した場合、何か道具が必要になるものはありますか？"},{"id":{"d":340,"n":5,"t":2},"speaker":"A","text":"音楽は五線譜ノートを購入してください。学校の購買で購入できます。"},{"id":{"d":340,"n":6,"t":2},"speaker":"B","text":"リコーダーなど楽器は何もいらないのですか？"},{"id":{"d":340,"n":7,"t":2},"speaker":"A","text":"音楽は作曲を行いますので必要ありません。書道、美術はそれぞれセットがありますので、そちらの購入申込書をこの後、お渡しします。"},{"id":{"d":340,"n":8,"t":2},"speaker":"B","text":"一番手軽に始めることが可能な講義は、音楽ということですね。"}];
INSERT INTO sentence [{"id":{"d":341,"n":1,"t":2},"speaker":"A","text":"今まで学生生活をしてきて、一番憧れている先輩は誰ですか？"},{"id":{"d":341,"n":2,"t":2},"speaker":"B","text":"私は中学の時の先輩に憧れています。"},{"id":{"d":341,"n":3,"t":2},"speaker":"A","text":"中学の時に、憧れることができる先輩に出会えているのはいいことですね。どのような先輩だったのですか？"},{"id":{"d":341,"n":4,"t":2},"speaker":"B","text":"その先輩とは小学校の時から仲良く、あだ名で呼んでいるくらい仲が良かったです。中学校入学直前にも中学の校門の前で会ったら、先輩は友達といたのに友達と一緒に私を家まで送ってくれるくらいかわいがってくださっていました。"},{"id":{"d":341,"n":5,"t":2},"speaker":"A","text":"とても良い先輩ですね。まるでお兄さんのような印象を受けます。"},{"id":{"d":341,"n":6,"t":2},"speaker":"B","text":"その通りです。とても面倒見がよく、クラスでも部活動でもとても慕われていました。誰に対しても丁寧で、優しく、困っている人がいれば必ず助けに入るような人でした。それでいて、勉強も部活動もとても成績がよく、まさに完全無欠の人でした。"},{"id":{"d":341,"n":7,"t":2},"speaker":"A","text":"とてもすてきな人ですね。憧れるのもよく分かります。"}];
INSERT INTO sentence [{"id":{"d":342,"n":1,"t":2},"speaker":"A","text":"それでは今日の宿題の内容の小テストをします。机の上をきれいにしてください。"},{"id":{"d":342,"n":2,"t":2},"speaker":"B","text":"先生、宿題の小テストを行うとは聞いていません。"},{"id":{"d":342,"n":3,"t":2},"speaker":"A","text":"言っていませんが、宿題でやってきているはずなのでもちろん解けるはずです。"},{"id":{"d":342,"n":4,"t":2},"speaker":"B","text":"先週、先生は「難しい問題だから解けなくても仕方ない」と言っていた記憶がありますが、その問題の小テストですか？"},{"id":{"d":342,"n":5,"t":2},"speaker":"A","text":"そうです。解き方は先週教えたはずなので、あとはその流れに合わせるだけです。できませんか？"},{"id":{"d":342,"n":6,"t":2},"speaker":"B","text":"他の科目の宿題もあり、どのような問題だったか覚えていないので少し復習の時間をいただけませんか？"},{"id":{"d":342,"n":7,"t":2},"speaker":"A","text":"分かりました。5分間復習の時間を与えます。その後に小テストをしますので、しっかりとこの時間で確認してください。"},{"id":{"d":342,"n":8,"t":2},"speaker":"B","text":"一生懸命に復習しますが、難しい宿題の小テストなので、合格点は少し低めにしてほしいです。"}];
INSERT INTO sentence [{"id":{"d":343,"n":1,"t":2},"speaker":"A","text":"中学生、高校生の時に、勉強はいつもどこでしていましたか？"},{"id":{"d":343,"n":2,"t":2},"speaker":"B","text":"塾や予備校の自習室で勉強をしていました。家では集中できなかったので、周りが集中している環境に身を置いて、空気を感じながら勉強をしていました。"},{"id":{"d":343,"n":3,"t":2},"speaker":"A","text":"学校の図書館や教室ではやらなかったのですか？"},{"id":{"d":343,"n":4,"t":2},"speaker":"B","text":"教室は、友達が多くいたので集中していても楽しそうな声が聞こえると、そちらにどうしても気が向いてしまうのでどうしても駄目でした。"},{"id":{"d":343,"n":5,"t":2},"speaker":"A","text":"楽しそうな声が聞こえると、やはり楽しい方に気が向いてしまうのは人間ですから、仕方ないことですね。"},{"id":{"d":343,"n":6,"t":2},"speaker":"B","text":"図書館は、逆に無音すぎて集中することができませんでした。他の人の鉛筆の音も聞こえないくらい、人が少なく、無音だったので怖くなって集中できませんでした。"},{"id":{"d":343,"n":7,"t":2},"speaker":"A","text":"全くの無音だと、何か少しの物音でもとても驚きますし、不安になりますね。"},{"id":{"d":343,"n":8,"t":2},"speaker":"B","text":"その通りです。このような理由があったので、学校では勉強をする事ができませんでした。"}];
INSERT INTO sentence [{"id":{"d":344,"n":1,"t":2},"speaker":"A","text":"専修学校に通っている友人が「専修学校は、高校をあと2年続けるだけだ」と言っていました。"},{"id":{"d":344,"n":2,"t":2},"speaker":"B","text":"私は大学に通っていたので、意味があまり分からないので、分かりやすく教えてください。"},{"id":{"d":344,"n":3,"t":2},"speaker":"A","text":"通学にかかる時間は、高校の時の時間と全く同じで、講義は毎時間しっかり入っていて、講義終了時間も高校とほとんど変わらないそうです。"},{"id":{"d":344,"n":4,"t":2},"speaker":"B","text":"つまり、専門的なことを学ぶことができる高校というイメージですね。"},{"id":{"d":344,"n":5,"t":2},"speaker":"A","text":"そのようです。放課後には美容師の専修学校なら、カットの練習や、シャンプーの練習をするそうで、「高校の部活動の時間と同じだ」と友人が言っていました。"},{"id":{"d":344,"n":6,"t":2},"speaker":"B","text":"とても大変ですね。専門的な知識を短期間で身に付けるために努力しているのですね。"},{"id":{"d":344,"n":7,"t":2},"speaker":"A","text":"専修学校のことはあまり知りませんでしたが、友人にこの話を聞いてすごいところだと思いました。"},{"id":{"d":344,"n":8,"t":2},"speaker":"B","text":"私も大学しか知らなかったので、面白い情報を得ることができました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":345,"n":1,"t":2},"speaker":"A","text":"小さい頃の記憶は、年をとるとどんどん薄れていきますね。"},{"id":{"d":345,"n":2,"t":2},"speaker":"B","text":"そうですね。幼稚園の頃の記憶など、もうほとんどありません。"},{"id":{"d":345,"n":3,"t":2},"speaker":"A","text":"私もあまり残っていませんが、幼稚園はなぜか記憶に残っていることがいくつかあります。"},{"id":{"d":345,"n":4,"t":2},"speaker":"B","text":"相当、思い出に残っていることなのでしょう。どのようなことを、覚えているのですか？"},{"id":{"d":345,"n":5,"t":2},"speaker":"A","text":"外遊びをよくしていたことです。幼稚園の時は、とにかく運動をすることが好きで、毎日サッカーをグラウンドでしていました。もちろん幼稚園生なので、キックは弱いのですが、たまにボールが高く上がってしまい、倉庫の上に乗ってしまうことがありました。その時に一緒にサッカーをしていた友達と、木に登ってボールを取っていたことを覚えています。"},{"id":{"d":345,"n":6,"t":2},"speaker":"B","text":"小さい頃から倉庫の上の高さの木に登ることができたのは、すごいことですね。"},{"id":{"d":345,"n":7,"t":2},"speaker":"A","text":"運動神経は良かったようです。他にもたくさんありますが、一番記憶に残っているのは、幼稚園の池に落ちたことです。帰りのバスを待っている時に、何かを拾おうとした拍子にバランスを崩して池に落ちました。その日は冬で、池に氷が張るくらい寒かったので何が起こったのか理解できず、泣かなかったのですが、次の日に、風邪をひいたことを覚えています。"},{"id":{"d":345,"n":8,"t":2},"speaker":"B","text":"それはひどいですね。大きな事故にならず良かったです。"}];
INSERT INTO sentence [{"id":{"d":346,"n":1,"t":2},"speaker":"A","text":"私は大学で数学を専攻していましたが、周りを見ていると算数が苦手な人が多数いるように感じました。"},{"id":{"d":346,"n":2,"t":2},"speaker":"B","text":"数学を専門に研究をしている人が、算数が苦手とは皮肉なものですね。"},{"id":{"d":346,"n":3,"t":2},"speaker":"A","text":"実際、講義では教授が掛け算や足し算、引き算を間違えている場面は多く見ました。"},{"id":{"d":346,"n":4,"t":2},"speaker":"B","text":"数学では算数のような計算はあまり使わないのですか？"},{"id":{"d":346,"n":5,"t":2},"speaker":"A","text":"多くの場合で記号を用いて最終的に代入をしますが、一番簡単な形の式をコンピューターに打ち込んで、値を求めているので計算が必要になることはほとんどありません。"},{"id":{"d":346,"n":6,"t":2},"speaker":"B","text":"式の変換やどの式を用いるかという知識は素晴らしいですが、実際の計算は自分の手ですることはあまりないということですね。"},{"id":{"d":346,"n":7,"t":2},"speaker":"A","text":"その傾向があるということです。もちろん全員が、計算が苦手ということではないので、計算の速い教授、学生もいました。"},{"id":{"d":346,"n":8,"t":2},"speaker":"B","text":"計算が苦手でも、数学を専門に学ぶことは可能ということですね。そのことを知ったら数学を学ぼうと思う学生が増える予感がします。"}];
INSERT INTO sentence [{"id":{"d":347,"n":1,"t":2},"speaker":"A","text":"今日で大学院を修了します。無事学位が取れて良かったです。"},{"id":{"d":347,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。大学院の2年間はどうでしたか？"},{"id":{"d":347,"n":3,"t":2},"speaker":"A","text":"大学院の入学から夏休みまで、対面の講義は1回もなく、初めてのオンライン講義も経験しました。"},{"id":{"d":347,"n":4,"t":2},"speaker":"B","text":"今まで誰も経験したことのないようなことだったので、大変でしたね。その中で就職活動までありましたね。"},{"id":{"d":347,"n":5,"t":2},"speaker":"A","text":"就職活動は短く終わることができたので、ストレスを感じる期間は短く済みました。こればかりは運が良かったと思います。"},{"id":{"d":347,"n":6,"t":2},"speaker":"B","text":"それは良かったですね。修士論文を書きながら就職活動は大変だと思うので、短く終わったのであればそれが一番です。"},{"id":{"d":347,"n":7,"t":2},"speaker":"A","text":"これからは就職に向けて、知識を付けることに注力したいと思います。"},{"id":{"d":347,"n":8,"t":2},"speaker":"B","text":"これからの長い社会人生活も、楽しんでくださいね。"},{"id":{"d":347,"n":9,"t":2},"speaker":"A","text":"もう少し長く学生をしたかった気もしますが、十分学生を楽しむことができたので、悔いはありません。"}];
INSERT INTO sentence [{"id":{"d":348,"n":1,"t":2},"speaker":"A","text":"家庭科の授業で分からないところがあったのですが、質問してもよろしいですか？"},{"id":{"d":348,"n":2,"t":2},"speaker":"B","text":"とても素早くミシンを扱っていたので不安なことはないと思っていましたが、何か不安なことがありましたか？"},{"id":{"d":348,"n":3,"t":2},"speaker":"A","text":"不安なことは特になかったのですが、友人の作品作りを手伝うことはしても良いですか？"},{"id":{"d":348,"n":4,"t":2},"speaker":"B","text":"どの程度の手伝いかにもよりますが、手伝いをしすぎなければ構いませんよ。"},{"id":{"d":348,"n":5,"t":2},"speaker":"A","text":"その友達は、ミシンの糸を通すところから縫うところまで頼んできました。"},{"id":{"d":348,"n":6,"t":2},"speaker":"B","text":"流石にそこまで手伝うと、ほとんど田中さんの作品になってしまうので手伝わないでください。"},{"id":{"d":348,"n":7,"t":2},"speaker":"A","text":"どのような内容までなら、手伝っても良いですか？正直、手伝えないのであれば、何も手伝わないようにしようかなと思っています。"},{"id":{"d":348,"n":8,"t":2},"speaker":"B","text":"ミシンに糸を通すくらいなら、しても大丈夫ですよ。手伝っているように感じたくないのであれば、田中さんがミシンを使い終わったら、すぐにその子に貸せば良いと思います。"},{"id":{"d":348,"n":9,"t":2},"speaker":"A","text":"私の作業の直後にミシンを貸すようにします。良いアイデアをいただき、ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":349,"n":1,"t":2},"speaker":"A","text":"今日の給食は、私たちのクラスのリクエスト献立です。"},{"id":{"d":349,"n":2,"t":2},"speaker":"B","text":"先月クラスで決めた献立が、給食になるのですね。"},{"id":{"d":349,"n":3,"t":2},"speaker":"A","text":"ただ私はどの給食をリクエストしたか、覚えていません。山内さんは覚えていますか？"},{"id":{"d":349,"n":4,"t":2},"speaker":"B","text":"私も定かではありませんが、ラーメンをリクエストした記憶があります。"},{"id":{"d":349,"n":5,"t":2},"speaker":"A","text":"思い出しました。とんこつラーメンにした記憶があります。副菜は唐揚げにしましたね。"},{"id":{"d":349,"n":6,"t":2},"speaker":"B","text":"そうでしたね、ただ、今この紙を見つけましたが、副菜は変わってしまっていました。"},{"id":{"d":349,"n":7,"t":2},"speaker":"A","text":"副菜は何になっていましたか？"},{"id":{"d":349,"n":8,"t":2},"speaker":"B","text":"きんぴらごぼうになっていました。とてもおいしそうですが、唐揚げの方がいいですね。"},{"id":{"d":349,"n":9,"t":2},"speaker":"A","text":"少し残念ですが、ラーメンが食べられるなら良いです。"}];
INSERT INTO sentence [{"id":{"d":350,"n":1,"t":2},"speaker":"A","text":"今日の家庭科の授業は調理実習です。"},{"id":{"d":350,"n":2,"t":2},"speaker":"B","text":"この日が楽しみで、1週間頑張って授業を受けていました。ぜひ、おいしい料理を作りましょう。"},{"id":{"d":350,"n":3,"t":2},"speaker":"A","text":"今日は、何を作る予定ですか？"},{"id":{"d":350,"n":4,"t":2},"speaker":"B","text":"ポテトサラダとミネストローネ、スパゲッティを作ります。"},{"id":{"d":350,"n":5,"t":2},"speaker":"A","text":"全ておいしそうですね。ちなみにスパゲッティはどのような味のものを作るのですか？"},{"id":{"d":350,"n":6,"t":2},"speaker":"B","text":"本当はミートソースが良かったのですが、ミネストローネを作るので、カルボナーラを作るとこにしました。"},{"id":{"d":350,"n":7,"t":2},"speaker":"A","text":"カルボナーラですか。私も大好きなので、試食の時に楽しみにしていますね。"},{"id":{"d":350,"n":8,"t":2},"speaker":"B","text":"しっかり、チーズも買ってきたので本格的なカルボナーラを作ろうと思っています。楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":351,"n":1,"t":2},"speaker":"A","text":"自動車教習所の卒業検定が、明日に迫っています。"},{"id":{"d":351,"n":2,"t":2},"speaker":"B","text":"自信はどうですか？卒業できそうですか？"},{"id":{"d":351,"n":3,"t":2},"speaker":"A","text":"エンジンストールや、何か想像していないようなことが起きない限り大丈夫だと思っています。"},{"id":{"d":351,"n":4,"t":2},"speaker":"B","text":"しっかり、半分クラッチの感覚は覚えていますか？"},{"id":{"d":351,"n":5,"t":2},"speaker":"A","text":"左足が感覚を覚えていますし、エンジンの音も覚えています。"},{"id":{"d":351,"n":6,"t":2},"speaker":"B","text":"それでは、しっかりと卒業できそうですね。学科の試験はどうですか？"},{"id":{"d":351,"n":7,"t":2},"speaker":"A","text":"全く勉強していません。テストは難しいのでしょうか？"},{"id":{"d":351,"n":8,"t":2},"speaker":"B","text":"勉強をしていないと、難しいと思います。今から勉強しておくべきですよ。"},{"id":{"d":351,"n":9,"t":2},"speaker":"A","text":"分かりました。学科の勉強に移ります。"}];
INSERT INTO sentence [{"id":{"d":352,"n":1,"t":2},"speaker":"A","text":"今日は避難訓練があります。しっかりイメージはできていますか？"},{"id":{"d":352,"n":2,"t":2},"speaker":"B","text":"大丈夫です。前回は大きなミスをしてしまいましたが、今回は気を付けます。"},{"id":{"d":352,"n":3,"t":2},"speaker":"A","text":"前回、どのようなミスをしたのですか？"},{"id":{"d":352,"n":4,"t":2},"speaker":"B","text":"前回は、何も伝えられていない中で、不審者の侵入の避難訓練だったため、本当に不審者が入ってきたと思ってしまい、椅子を投げ付けてしまいました。今一度、正しい避難の方法を教えてください。"},{"id":{"d":352,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。不審者が入ってきたら、できるだけ大きな声を出しながら、遠い教室に逃げましょう。"},{"id":{"d":352,"n":6,"t":2},"speaker":"B","text":"逃げる教室の場所は、どこでも良いのですか？"},{"id":{"d":352,"n":7,"t":2},"speaker":"A","text":"はい、どこでも良いので、とにかくすぐに遠くへ逃げてください。"}];
INSERT INTO sentence [{"id":{"d":353,"n":1,"t":2},"speaker":"A","text":"私の小学校では、プールが珍しい場所にありました。"},{"id":{"d":353,"n":2,"t":2},"speaker":"B","text":"普通は、学校の裏などにプールがありますよね。どこにあったのですか？"},{"id":{"d":353,"n":3,"t":2},"speaker":"A","text":"校舎の屋上にありました。"},{"id":{"d":353,"n":4,"t":2},"speaker":"B","text":"屋上ですか。それは、とても不思議な場所ですね。"},{"id":{"d":353,"n":5,"t":2},"speaker":"A","text":"プールで波を作っていると、下にたくさんの水があふれていきました。"},{"id":{"d":353,"n":6,"t":2},"speaker":"B","text":"小学生はそのような遊びをすることが好きですよね。"},{"id":{"d":353,"n":7,"t":2},"speaker":"A","text":"ホースなどを使って、水の掛け合いもしたので、その水も下に落ちていた気がします。"},{"id":{"d":353,"n":8,"t":2},"speaker":"B","text":"下の階で授業を受けている人や、下を通った人は、いきなり水が落ちてきてびっくりですね。"}];
INSERT INTO sentence [{"id":{"d":354,"n":1,"t":2},"speaker":"B","text":"なぜかは分からないのですが、校長先生は、花壇の花に水をあげているというイメージがあります。"},{"id":{"d":354,"n":2,"t":2},"speaker":"A","text":"私もそのイメージがあります。実際に水やりをしているところは一度も見たことがないのですが。"},{"id":{"d":354,"n":3,"t":2},"speaker":"B","text":"そうなのですよね。なぜこのようなイメージがあるのでしょうか？"},{"id":{"d":354,"n":4,"t":2},"speaker":"A","text":"私は、何か漫画や小説でそのような描写があったのかなと思います。"},{"id":{"d":354,"n":5,"t":2},"speaker":"B","text":"ああ、それはあるかもしれません。アニメーションなどでもそのような描写がまれにありますね。"}];
INSERT INTO sentence [{"id":{"d":355,"n":1,"t":2},"speaker":"A","text":"宿題は、しっかり行いましたか？"},{"id":{"d":355,"n":2,"t":2},"speaker":"B","text":"はい。今日の分は終わったので、今から遊びに行ってきます。"},{"id":{"d":355,"n":3,"t":2},"speaker":"A","text":"今日は、宿題を終えるのが早いですね。"},{"id":{"d":355,"n":4,"t":2},"speaker":"B","text":"今日、一緒に遊ぶ友達と学校で、途中まで宿題を行いました。"},{"id":{"d":355,"n":5,"t":2},"speaker":"A","text":"なるほど、そうでしたか。"},{"id":{"d":355,"n":6,"t":2},"speaker":"B","text":"みんなで教え合いながら解いたので、いつもより早く進みました。"},{"id":{"d":355,"n":7,"t":2},"speaker":"A","text":"それは良かったですね。"},{"id":{"d":355,"n":8,"t":2},"speaker":"B","text":"はい。では、遊びに行ってきます。"},{"id":{"d":355,"n":9,"t":2},"speaker":"A","text":"気を付けて、いってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":356,"n":1,"t":2},"speaker":"A","text":"今日の図工は、何をしますか？"},{"id":{"d":356,"n":2,"t":2},"speaker":"B","text":"今日は学校の外に出て風景画を描きましょう。今から近くの公園へ移動します。"},{"id":{"d":356,"n":3,"t":2},"speaker":"A","text":"校外へ出掛けるのですか。まるで遠足のようで楽しみです。"},{"id":{"d":356,"n":4,"t":2},"speaker":"B","text":"今日は、よく晴れていて気温も高いので、日差しと暑さの対策をしっかりして出掛けましょう。水分は必ず持って行ってくださいね。"},{"id":{"d":356,"n":5,"t":2},"speaker":"A","text":"はい。スポーツドリンクと、帽子を持っていこうと思います。"},{"id":{"d":356,"n":6,"t":2},"speaker":"B","text":"いいですね。では、各自準備をして5分後に昇降口に集合してください。"}];
INSERT INTO sentence [{"id":{"d":357,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題はしてきましたか？"},{"id":{"d":357,"n":2,"t":2},"speaker":"B","text":"もちろんしました。自由研究も、朝顔の観察も、してきました。"},{"id":{"d":357,"n":3,"t":2},"speaker":"A","text":"朝顔は、どれくらい花が咲きましたか？"},{"id":{"d":357,"n":4,"t":2},"speaker":"B","text":"私の朝顔は4輪咲きました。石本君の朝顔はどうでしたか？"},{"id":{"d":357,"n":5,"t":2},"speaker":"A","text":"私は水やりを忘れてしまい、枯らしてしまいました。"},{"id":{"d":357,"n":6,"t":2},"speaker":"B","text":"そうかなと思いました。夏休み遊んだ時、に朝顔の話をしなかったので不思議に思っていました。いつ頃枯らしてしまったのですか？"},{"id":{"d":357,"n":7,"t":2},"speaker":"A","text":"夏休みが始まり、1週間もたってない頃にもう枯れていました。"}];
INSERT INTO sentence [{"id":{"d":358,"n":1,"t":2},"speaker":"A","text":"今年の自由研究は、何をする予定ですか？"},{"id":{"d":358,"n":2,"t":2},"speaker":"B","text":"実は、私は既に興味があるものを発見しました。"},{"id":{"d":358,"n":3,"t":2},"speaker":"A","text":"まだ夏休みになっていないのに早いですね。何について研究するのですか？"},{"id":{"d":358,"n":4,"t":2},"speaker":"B","text":"私は電子レンジについて研究しようと思います。"},{"id":{"d":358,"n":5,"t":2},"speaker":"A","text":"電子レンジの、何について研究するのですか？"},{"id":{"d":358,"n":6,"t":2},"speaker":"B","text":"どうやって物を温めているのか、仕組みを解明するつもりです。"},{"id":{"d":358,"n":7,"t":2},"speaker":"A","text":"電子レンジは、毎日のように使う身近な物ですが、私も詳しい仕組みはよく分かりません。面白い研究になりそうですね。"},{"id":{"d":358,"n":8,"t":2},"speaker":"B","text":"仕組みを踏まえて、何か改良できる点はないかどうかも、研究してみたいです。"},{"id":{"d":358,"n":9,"t":2},"speaker":"A","text":"いいですね。夏休み明けの発表が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":359,"n":1,"t":2},"speaker":"A","text":"先生、今度の授業参観のために、クラスみんなで作戦を立てたので聞いてください。"},{"id":{"d":359,"n":2,"t":2},"speaker":"B","text":"何か楽しそうですね、どのような作戦ですか？"},{"id":{"d":359,"n":3,"t":2},"speaker":"A","text":"家族が見に来るので、私たちはみんな手を挙げてアピールしたいです。"},{"id":{"d":359,"n":4,"t":2},"speaker":"B","text":"そうですね。少しでも良いところを見せたいですよね。"},{"id":{"d":359,"n":5,"t":2},"speaker":"A","text":"そこで、先生の質問が分かる人は左手を、分からない人は右手を挙げるようにしようと思います。"},{"id":{"d":359,"n":6,"t":2},"speaker":"B","text":"つまり、どの質問にも手は挙げられるということですね。"},{"id":{"d":359,"n":7,"t":2},"speaker":"A","text":"はい、先生には、左手を上げている人だけを指してほしいのです。"},{"id":{"d":359,"n":8,"t":2},"speaker":"B","text":"なるほど、その作戦であれば難しい問題にも手を挙げることができますね。"},{"id":{"d":359,"n":9,"t":2},"speaker":"A","text":"その通りです。この作戦に乗ってくれませんか？"},{"id":{"d":359,"n":10,"t":2},"speaker":"B","text":"よく考えられている面白い作戦で感心しましたが、やはりきちんと答えを分かった上で挙手してほしいです。作戦にはのってあげられませんが、授業参観の日の内容を予習しておくことになら協力できますよ。"},{"id":{"d":359,"n":11,"t":2},"speaker":"A","text":"やっぱり駄目でしたか。予習を頑張ることにします。"}];
INSERT INTO sentence [{"id":{"d":360,"n":1,"t":2},"speaker":"A","text":"今日は、年末の大掃除です。掃除場所を確認した人から、その場所に移動してくださいね。"},{"id":{"d":360,"n":2,"t":2},"speaker":"B","text":"教室掃除の人が多い気がしますが、なぜですか？"},{"id":{"d":360,"n":3,"t":2},"speaker":"A","text":"教室がとても汚れているので、人員を割いて掃除しようと思います。"},{"id":{"d":360,"n":4,"t":2},"speaker":"B","text":"教室のどこを重点的にきれいにしますか？"},{"id":{"d":360,"n":5,"t":2},"speaker":"A","text":"床に黒い線がたくさんあるので、これをできるだけ消そうと考えています。"},{"id":{"d":360,"n":6,"t":2},"speaker":"B","text":"これは下に落ちた消しゴムのカスが、机を移動する時に床に擦り付けられてしまったものですね。"},{"id":{"d":360,"n":7,"t":2},"speaker":"A","text":"その通りです。机を移動するときは引きずらないようにしてくださいね。"},{"id":{"d":360,"n":8,"t":2},"speaker":"B","text":"机が重たいので、ついつい引きずってしまっていましたが、今度から気を付けます。"}];
INSERT INTO sentence [{"id":{"d":361,"n":1,"t":2},"speaker":"A","text":"卒業式では『旅立ちの日に』を歌います。"},{"id":{"d":361,"n":2,"t":2},"speaker":"B","text":"うれしいです。先輩方の歌っているところを見て、ずっと憧れていました。"},{"id":{"d":361,"n":3,"t":2},"speaker":"A","text":"それは良かったです。当日の指揮者とピアノ伴奏者を募集しますので、担当したい人はこの後、先生のところに来てください。"},{"id":{"d":361,"n":4,"t":2},"speaker":"B","text":"今まで指揮者をしたことがありませんが、それでも応募することはできますか？"},{"id":{"d":361,"n":5,"t":2},"speaker":"A","text":"もちろん、大丈夫ですよ。希望者が多い場合はオーディションをすることになるので、オーディション当日までに指揮の練習をしておくと良いと思います。"},{"id":{"d":361,"n":6,"t":2},"speaker":"B","text":"分かりました。しっかりと当日まで練習をしておきたいと思います。"}];
INSERT INTO sentence [{"id":{"d":362,"n":1,"t":2},"speaker":"A","text":"今日は全く集中が続かない日です。"},{"id":{"d":362,"n":2,"t":2},"speaker":"B","text":"そのような日もありますね。しかし、今日するべき勉強は、たくさんありますね。どうするのですか？"},{"id":{"d":362,"n":3,"t":2},"speaker":"A","text":"30分のアラームをかけて、まずはその間だけ勉強してみようと思います。"},{"id":{"d":362,"n":4,"t":2},"speaker":"B","text":"なるほど、良い案ですね。"},{"id":{"d":362,"n":5,"t":2},"speaker":"A","text":"どれほど集中力がなくとも30分だけなら、頑張れる気がします。"},{"id":{"d":362,"n":6,"t":2},"speaker":"B","text":"そうですね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":363,"n":1,"t":2},"speaker":"A","text":"本校は理数科が併設されていることも魅力の1つです。"},{"id":{"d":363,"n":2,"t":2},"speaker":"B","text":"理数科は、どのようなことを学ぶのですか？"},{"id":{"d":363,"n":3,"t":2},"speaker":"A","text":"名前の通り、理数科目を、普通科よりも多く学ぶことになります。"},{"id":{"d":363,"n":4,"t":2},"speaker":"B","text":"それでは入試の際にも、何か特別な試験が行われるのですか？"},{"id":{"d":363,"n":5,"t":2},"speaker":"A","text":"本校は公立高校のため、特別なテストは行いません。"},{"id":{"d":363,"n":6,"t":2},"speaker":"B","text":"それでは、普通科と同じ内容のテストを行い、理数科志望であれば理数科に合格するということですか？"},{"id":{"d":363,"n":7,"t":2},"speaker":"A","text":"理数科は理科と数学の配点が倍になっています。そのため普通科よりも理数科目を重視していることになります。"},{"id":{"d":363,"n":8,"t":2},"speaker":"B","text":"そのような仕組みがあったのですね。普通科を志望するか、理数科を志望するか考えてみます。"}];
INSERT INTO sentence [{"id":{"d":364,"n":1,"t":2},"speaker":"A","text":"今日の放課後の講義は、来週に迫った海外大学への2週間のサマースクールのための勉強です。"},{"id":{"d":364,"n":2,"t":2},"speaker":"B","text":"もう来週に迫っているのですね。"},{"id":{"d":364,"n":3,"t":2},"speaker":"A","text":"皆さんの英語の力も上昇してきて、日常会話であればできるようになっていると思います。"},{"id":{"d":364,"n":4,"t":2},"speaker":"B","text":"まだ自信はそれほどありませんが、昔よりは話すことができるようになった感覚があります。"},{"id":{"d":364,"n":5,"t":2},"speaker":"A","text":"それは良かったです。では早速、今日の講義を始めましょう。教科書の70ページを開いてください。"}];
INSERT INTO sentence [{"id":{"d":365,"n":1,"t":2},"speaker":"A","text":"春休みは何をしていますか？勉強はしていますか？"},{"id":{"d":365,"n":2,"t":2},"speaker":"B","text":"全くしていません。夏休みは宿題が出るので勉強しないといけませんが、春休み、冬休みは宿題がないので遊んでいます。"},{"id":{"d":365,"n":3,"t":2},"speaker":"A","text":"休みが短い分、宿題がないので遊べますね。しかし、来年から学ぶ分の予習は大丈夫ですか？"},{"id":{"d":365,"n":4,"t":2},"speaker":"B","text":"もちろん大丈夫です。既に来年の1学期の分の知識は付けています。"},{"id":{"d":365,"n":5,"t":2},"speaker":"A","text":"偉いですね。なぜそれほど先取りをしているのですか？"},{"id":{"d":365,"n":6,"t":2},"speaker":"B","text":"来年は受験の年なので、早めに学校の勉強を終わらせて受験勉強に入りたいのです。"},{"id":{"d":365,"n":7,"t":2},"speaker":"A","text":"それは良い考えですね。受験勉強の進捗はどうですか？"},{"id":{"d":365,"n":8,"t":2},"speaker":"B","text":"とても良いです。先日の模擬試験では、B判定を取ることができました。"}];
INSERT INTO sentence [{"id":{"d":366,"n":1,"t":2},"speaker":"A","text":"中学校の時の合唱コンクールを覚えていますか？"},{"id":{"d":366,"n":2,"t":2},"speaker":"B","text":"覚えていますよ。懐かしいですね。"},{"id":{"d":366,"n":3,"t":2},"speaker":"A","text":"私は3年生の時の合唱コンクールが、とても思い出に残っています。"},{"id":{"d":366,"n":4,"t":2},"speaker":"B","text":"なぜですか？何か記憶に残るようなことがありましたか？"},{"id":{"d":366,"n":5,"t":2},"speaker":"A","text":"B組の、普段は少し素行の悪い人たちが一番全力で歌っていて、しかもとても上手だったのが衝撃でした。"},{"id":{"d":366,"n":6,"t":2},"speaker":"B","text":"確かに、B組の生徒は遅刻や雑な掃除でよく先生たちに怒られていたイメージがありました。"},{"id":{"d":366,"n":7,"t":2},"speaker":"A","text":"はい。でも本番では、一糸乱れぬパフォーマンスで本当に素晴らしかったです。後から聞いた話によると、2カ月前から毎日朝と放課後に、全員で合唱の練習をしていたそうで、そのことにも驚きました。"},{"id":{"d":366,"n":8,"t":2},"speaker":"B","text":"そうだったのですか。それは印象に残りますね。"}];
INSERT INTO sentence [{"id":{"d":367,"n":1,"t":2},"speaker":"A","text":"私の大学には、自治寮がありました。"},{"id":{"d":367,"n":2,"t":2},"speaker":"B","text":"自治寮とは、何ですか？"},{"id":{"d":367,"n":3,"t":2},"speaker":"A","text":"学生が、自分たち自身で管理している寮のことです。今はもう、日本では少なくなっているようです。"},{"id":{"d":367,"n":4,"t":2},"speaker":"B","text":"なるほど。本田さんは自治寮を見たことはありますか？"},{"id":{"d":367,"n":5,"t":2},"speaker":"A","text":"はい、見たことがあります。そして、一度だけ中に入ったこともあります。独特の雰囲気で緊張しました。"},{"id":{"d":367,"n":6,"t":2},"speaker":"B","text":"少し興味があります。見に行ってみたいです。"},{"id":{"d":367,"n":7,"t":2},"speaker":"A","text":"中は、許可された人しか入れませんが、外観を見るだけなら平気ですよ。今度、案内しますね。"}];
INSERT INTO sentence [{"id":{"d":368,"n":1,"t":2},"speaker":"A","text":"三山君は、何か習い事をしていますか？"},{"id":{"d":368,"n":2,"t":2},"speaker":"B","text":"私は、サッカーと水泳、運動クラブに通っています。"},{"id":{"d":368,"n":3,"t":2},"speaker":"A","text":"全て体を動かすものですね。疲れませんか？"},{"id":{"d":368,"n":4,"t":2},"speaker":"B","text":"とても疲れます。そのため、夜はとても早く寝てしまいます。"},{"id":{"d":368,"n":5,"t":2},"speaker":"A","text":"早寝早起きをしているということですね。素晴らしいです。"},{"id":{"d":368,"n":6,"t":2},"speaker":"B","text":"福原君は、何を習っていますか？"},{"id":{"d":368,"n":7,"t":2},"speaker":"A","text":"私はサッカーをしています。"},{"id":{"d":368,"n":8,"t":2},"speaker":"B","text":"他にも、何か習いたいと思うことはないのですか？"},{"id":{"d":368,"n":9,"t":2},"speaker":"A","text":"面白そうなので、そろばんは習ってみたいと思います。"},{"id":{"d":368,"n":10,"t":2},"speaker":"B","text":"計算も早くなるそうなので、そろばんを習うことは良いかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":369,"n":1,"t":2},"speaker":"A","text":"次の授業は、移動教室ですか？"},{"id":{"d":369,"n":2,"t":2},"speaker":"B","text":"次は情報の授業なので、パソコン室です。移動しなければなりません。"},{"id":{"d":369,"n":3,"t":2},"speaker":"A","text":"では、早速行きましょう。情報の講義は好きですか？"},{"id":{"d":369,"n":4,"t":2},"speaker":"B","text":"私は好きですよ。先生に見られながら画面を操作するのは少し緊張しますが、毎回楽しんでいます。"},{"id":{"d":369,"n":5,"t":2},"speaker":"A","text":"「先生に見られながら」とはどういう意味ですか？"},{"id":{"d":369,"n":6,"t":2},"speaker":"B","text":"先生のサブモニターには、私たち全員分の画面が映っているのですよ。知りませんでしたか？"},{"id":{"d":369,"n":7,"t":2},"speaker":"A","text":"全く知りませんでした。私はいつも好きなサッカーのチームのホームページを見て遊んでいましたが、先生はそれに気付いていたということでしょうか？"},{"id":{"d":369,"n":8,"t":2},"speaker":"B","text":"多分気付いていると思います。"},{"id":{"d":369,"n":9,"t":2},"speaker":"A","text":"失敗しました。今日からは、真面目に授業を受けようと思います。"}];
INSERT INTO sentence [{"id":{"d":370,"n":1,"t":2},"speaker":"A","text":"受験する高校は、決めましたか？"},{"id":{"d":370,"n":2,"t":2},"speaker":"B","text":"決めました。隣町の高校にしました。募集人数が多いので、きっと受かると信じています。"},{"id":{"d":370,"n":3,"t":2},"speaker":"A","text":"受験するのは1校だけですか？他の学校は受けないのですか？"},{"id":{"d":370,"n":4,"t":2},"speaker":"B","text":"はい。1校だけの予定です。"},{"id":{"d":370,"n":5,"t":2},"speaker":"A","text":"そうなのですか。不安はないのですか？"},{"id":{"d":370,"n":6,"t":2},"speaker":"B","text":"あまり感じていません。これまで一生懸命勉強してきましたし、模擬試験でもずっとA判定です。"},{"id":{"d":370,"n":7,"t":2},"speaker":"A","text":"それはすごいですね。本番で実力を発揮できるよう応援しています。"},{"id":{"d":370,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":371,"n":1,"t":2},"speaker":"A","text":"出身校の名前を、自信を持って言うことができますか？"},{"id":{"d":371,"n":2,"t":2},"speaker":"B","text":"もちろん言うことができます。あなたは、出身校の名前を、自信を持って言えますか？"},{"id":{"d":371,"n":3,"t":2},"speaker":"A","text":"私は少し不安があります。入学した時よりも高校の評判が落ちているのです。"},{"id":{"d":371,"n":4,"t":2},"speaker":"B","text":"それは少し落胆しますね。常に、同じくらいの評判だといいですね。"},{"id":{"d":371,"n":5,"t":2},"speaker":"A","text":"そうともいかないのが学校の評判ですね。少し落ちてきているため自分からはあまり言いませんが、母校に対する誇りは常に持っています。"},{"id":{"d":371,"n":6,"t":2},"speaker":"B","text":"それが一番大切なことですね。良い学校だったと将来胸を張って言えるのであれば、それでよいと思います。"},{"id":{"d":371,"n":7,"t":2},"speaker":"A","text":"自分の気の持ち方次第なので、この気持ちを忘れないようにしたいです。"},{"id":{"d":371,"n":8,"t":2},"speaker":"B","text":"自分が頑張って入った学校を、悪く言うことはしたくありませんね。"}];
INSERT INTO sentence [{"id":{"d":372,"n":1,"t":2},"speaker":"A","text":"大学に通いながら、専修学校に通っていると聞きましたが、本当ですか？"},{"id":{"d":372,"n":2,"t":2},"speaker":"B","text":"本当です。したいことがあってこの春から専修学校にも通い始めました。"},{"id":{"d":372,"n":3,"t":2},"speaker":"A","text":"大学の講義も受けながら、通っているということですか？"},{"id":{"d":372,"n":4,"t":2},"speaker":"B","text":"大学は1年間休学しています。その間で専修学校に通うことにしました。"},{"id":{"d":372,"n":5,"t":2},"speaker":"A","text":"なるほど。専修学校で学んでいることは、大学での専門と結びつくものですか？"},{"id":{"d":372,"n":6,"t":2},"speaker":"B","text":"もちろん、結び付きます。必要な知識を深く学ぶことができてとても充実していますよ。"}];
INSERT INTO sentence [{"id":{"d":373,"n":1,"t":2},"speaker":"A","text":"来月、ボランティアとして、南アフリカ共和国に行ってきます。"},{"id":{"d":373,"n":2,"t":2},"speaker":"B","text":"1人で行くのですか？それとも仲間と行くのですか？"},{"id":{"d":373,"n":3,"t":2},"speaker":"A","text":"何人かで行きますが全員初対面です。どのような人がいるのか楽しみです。"},{"id":{"d":373,"n":4,"t":2},"speaker":"B","text":"知らない人と海外に行くのは、とても緊張しそうですね。"},{"id":{"d":373,"n":5,"t":2},"speaker":"A","text":"同じボランティアに参加しているということは、価値観も近しい人が集まるのではないかと思っています。必ずすぐに意気投合できると信じています。"},{"id":{"d":373,"n":6,"t":2},"speaker":"B","text":"なるほど、そうかもしれませんね。楽しい時間が過ごせるといいですね。"},{"id":{"d":373,"n":7,"t":2},"speaker":"A","text":"私もそのように思います。ボランティアはもちろんですが、新しい友達ができるのも楽しみです。"}];
INSERT INTO sentence [{"id":{"d":374,"n":1,"t":2},"speaker":"A","text":"今日は、走って学校に行きましょう。"},{"id":{"d":374,"n":2,"t":2},"speaker":"B","text":"なぜですか？今日は何かありましたか？"},{"id":{"d":374,"n":3,"t":2},"speaker":"A","text":"昨日は雨で、外でサッカーができなかったので、今日こそサッカーをするために早く学校へ着きたいです。"},{"id":{"d":374,"n":4,"t":2},"speaker":"B","text":"確かに、昨日は外で遊べていませんものね。分かりました、走りましょう。"},{"id":{"d":374,"n":5,"t":2},"speaker":"A","text":"走って行ったら、何分で着くと思いますか？"},{"id":{"d":374,"n":6,"t":2},"speaker":"B","text":"今日はランドセルも軽いので、15分くらいで着くと思います。"},{"id":{"d":374,"n":7,"t":2},"speaker":"A","text":"それでは7時半から外で遊べそうですね。チームを決めながら、走って行きましょう。"},{"id":{"d":374,"n":8,"t":2},"speaker":"B","text":"ちょうどいい準備運動になりますね。それでは行きましょう。"}];
INSERT INTO sentence [{"id":{"d":375,"n":1,"t":2},"speaker":"A","text":"明日は壮行会ですね。サッカー部は、何をするのですか？"},{"id":{"d":375,"n":2,"t":2},"speaker":"B","text":"特に、何も考えていません。何をするべきなのでしょうか？"},{"id":{"d":375,"n":3,"t":2},"speaker":"A","text":"決意を述べるというのが、毎年の流れではありますが先輩方次第ですね。"},{"id":{"d":375,"n":4,"t":2},"speaker":"B","text":"去年の野球部は、試合へ向けての意気込みの発表と、円陣だったと記憶していますが合っていますか？"},{"id":{"d":375,"n":5,"t":2},"speaker":"A","text":"その通りです。先輩方の大きな声が体育館中に響いていたのを覚えています。"},{"id":{"d":375,"n":6,"t":2},"speaker":"B","text":"野球部らしさが前面に出ていて、私も格好良いと思った記憶があります。"},{"id":{"d":375,"n":7,"t":2},"speaker":"A","text":"サッカー部も円陣を組んだら良いのではありませんか？"},{"id":{"d":375,"n":8,"t":2},"speaker":"B","text":"そうします。しかし野球部と近い順番だと、似たようなパフォーマンスが続いてしまうので、順番はできるだけ離してください。"},{"id":{"d":375,"n":9,"t":2},"speaker":"A","text":"そうですね。生徒会にお願いしてみましょう。"}];
INSERT INTO sentence [{"id":{"d":376,"n":1,"t":2},"speaker":"A","text":"今日は、先日の数学のテストが返却されました。"},{"id":{"d":376,"n":2,"t":2},"speaker":"B","text":"結果はどうでしたか？"},{"id":{"d":376,"n":3,"t":2},"speaker":"A","text":"平均点が40点台の中、学年で唯一の100点でした。"},{"id":{"d":376,"n":4,"t":2},"speaker":"B","text":"素晴らしいですね。これで何回連続の100点ですか？"},{"id":{"d":376,"n":5,"t":2},"speaker":"A","text":"3回連続です。先生にも褒められました。"},{"id":{"d":376,"n":6,"t":2},"speaker":"B","text":"一生懸命勉強していたようですが、その成果が出て良かったですね。"},{"id":{"d":376,"n":7,"t":2},"speaker":"A","text":"はい。最近はグラフや立体図形など、少し難しい問題も増えてきているのですが、勉強の成果が点数に表れていてとてもうれしいです。"},{"id":{"d":376,"n":8,"t":2},"speaker":"B","text":"素晴らしいです。ぜひ、他の科目も頑張って勉強してください。"},{"id":{"d":376,"n":9,"t":2},"speaker":"A","text":"苦手な文系科目も、力を入れようと思います。"}];
INSERT INTO sentence [{"id":{"d":377,"n":1,"t":2},"speaker":"A","text":"期末テストも、残すところあと1科目になりました。"},{"id":{"d":377,"n":2,"t":2},"speaker":"B","text":"もう既に、とても疲れて力がすっかり抜けています。最後の教科は何ですか？"},{"id":{"d":377,"n":3,"t":2},"speaker":"A","text":"最後は英語です。英語は私の得意科目なので、ここで挽回したいところです。"},{"id":{"d":377,"n":4,"t":2},"speaker":"B","text":"英語ですか。私は一番苦手です。リスニングがとても苦手です。"},{"id":{"d":377,"n":5,"t":2},"speaker":"A","text":"最後に苦手科目があるのは大変そうですね？"},{"id":{"d":377,"n":6,"t":2},"speaker":"B","text":"ええ、本当に大変です。このように疲れている状態では、リスニングが普段以上にできる気がしません。"},{"id":{"d":377,"n":7,"t":2},"speaker":"A","text":"試験が始まるまで、あと20分あります。ジュースを飲んだり、軽く眠ったりしてリフレッシュしてはどうですか？"},{"id":{"d":377,"n":8,"t":2},"speaker":"B","text":"そうします。自動販売機でジュースを買ってきます。"}];
INSERT INTO sentence [{"id":{"d":378,"n":1,"t":2},"speaker":"A","text":"明日の放送は、何を流しますか？"},{"id":{"d":378,"n":2,"t":2},"speaker":"B","text":"おとといのテレビの歌番組で特集されていた「平成の夏うたトップ5」と同じ曲をかけようと思っています。"},{"id":{"d":378,"n":3,"t":2},"speaker":"A","text":"それは良い企画ですね。音源は用意できているのですか？"},{"id":{"d":378,"n":4,"t":2},"speaker":"B","text":"4曲分は、既に用意できています。この第2位の曲だけが今手元にありません。"},{"id":{"d":378,"n":5,"t":2},"speaker":"A","text":"この曲なら、家にCDがありますよ。"},{"id":{"d":378,"n":6,"t":2},"speaker":"B","text":"本当ですか？貸してもらえませんか？"},{"id":{"d":378,"n":7,"t":2},"speaker":"A","text":"よいですよ。明日、CDを持ってきますね。"},{"id":{"d":378,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":379,"n":1,"t":2},"speaker":"A","text":"こちらは、窓口です。どうしましたか？"},{"id":{"d":379,"n":2,"t":2},"speaker":"B","text":"成績証明書を発行したいのですが、時間はどれくらいかかりますか？"},{"id":{"d":379,"n":3,"t":2},"speaker":"A","text":"後ろの機械で発行できますよ。時間はほとんどかかりません。1分ほどで発行できますよ。"},{"id":{"d":379,"n":4,"t":2},"speaker":"B","text":"初めてこのような機械を見ました。昔からありましたか？"},{"id":{"d":379,"n":5,"t":2},"speaker":"A","text":"もちろん、あったと思います。少なくとも10年前からありますよ。"},{"id":{"d":379,"n":6,"t":2},"speaker":"B","text":"それでは私が気付かなかっただけですね。お金はかかりますか？"},{"id":{"d":379,"n":7,"t":2},"speaker":"A","text":"お金も全くかかりませんよ。しかし、1日に発行できる枚数が決まっています。"},{"id":{"d":379,"n":8,"t":2},"speaker":"B","text":"一部だけで良いので大丈夫です。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":380,"n":1,"t":2},"speaker":"A","text":"大学までは、何分かかるところに住んでいましたか？"},{"id":{"d":380,"n":2,"t":2},"speaker":"B","text":"私は実家から通っていたので、1時間かかっていました。あなたの通学時間は、どれくらいでしたか？"},{"id":{"d":380,"n":3,"t":2},"speaker":"A","text":"私は1人暮らしをしていたので、5分から10分で着いていました。"},{"id":{"d":380,"n":4,"t":2},"speaker":"B","text":"とても近いですね。それでは、朝もゆっくり起きることができますね。"},{"id":{"d":380,"n":5,"t":2},"speaker":"A","text":"ゆっくり起きられるのは良かったのですが、つい寝坊をして1限の授業に遅刻してしまうことも多々ありました。注意してくれる家族もいないので、自己管理をしっかりしなくてはいけませんでした。"}];
INSERT INTO sentence [{"id":{"d":381,"n":1,"t":2},"speaker":"A","text":"幼稚園の園長先生を覚えていますか？"},{"id":{"d":381,"n":2,"t":2},"speaker":"B","text":"全く覚えていません。私たちの通っていた幼稚園に、園長先生はいましたか？"},{"id":{"d":381,"n":3,"t":2},"speaker":"A","text":"いましたよ。男性の方でした。"},{"id":{"d":381,"n":4,"t":2},"speaker":"B","text":"幼稚園に男性がいた記憶が全くないので、本当に覚えていません。"},{"id":{"d":381,"n":5,"t":2},"speaker":"A","text":"確かに、園長先生以外男性の先生はいませんでしたね。園長室などを、覚えていませんか？"},{"id":{"d":381,"n":6,"t":2},"speaker":"B","text":"園長室などは、なかった気がします。先生たちの部屋に遊びに行って、怒られた記憶しかありません。"},{"id":{"d":381,"n":7,"t":2},"speaker":"A","text":"その職員室の奥に、園長室がありました。"},{"id":{"d":381,"n":8,"t":2},"speaker":"B","text":"全く覚えていません。"}];
INSERT INTO sentence [{"id":{"d":382,"n":1,"t":2},"speaker":"A","text":"今日も図書館に行きます。いつもの本を見たいです。"},{"id":{"d":382,"n":2,"t":2},"speaker":"B","text":"中学生になって、図書館に行く頻度が増えましたね。何の本を見ているのですか？"},{"id":{"d":382,"n":3,"t":2},"speaker":"A","text":"私は「ギネス世界記録2022」を見ています。友達と、自分たちが挑戦できそうなものはないか探しています。"},{"id":{"d":382,"n":4,"t":2},"speaker":"B","text":"何かありましたか？"},{"id":{"d":382,"n":5,"t":2},"speaker":"A","text":"今のところ、「頬の伸びる長さを競う」競技が一番挑戦しやすそうです。"},{"id":{"d":382,"n":6,"t":2},"speaker":"B","text":"そのような競技もあるのですか？"},{"id":{"d":382,"n":7,"t":2},"speaker":"A","text":"はい。私が挑戦してみたら世界記録まであと数ミリメートルでした。"},{"id":{"d":382,"n":8,"t":2},"speaker":"B","text":"それは惜しいです。面白い競技ですね。"}];
INSERT INTO sentence [{"id":{"d":383,"n":1,"t":2},"speaker":"A","text":"小学校や中学校の給食は、学校で作っているものでしたか？"},{"id":{"d":383,"n":2,"t":2},"speaker":"B","text":"私の学校は、近くの給食センターで給食が作られていましたよ。"},{"id":{"d":383,"n":3,"t":2},"speaker":"A","text":"それでは他の学校も、同じ給食を食べていたということですか？"},{"id":{"d":383,"n":4,"t":2},"speaker":"B","text":"その通りです。村内の小学校、中学校は全て同じ給食でした。"},{"id":{"d":383,"n":5,"t":2},"speaker":"A","text":"容器などは、どうしていたのですか？"},{"id":{"d":383,"n":6,"t":2},"speaker":"B","text":"容器も給食センターから運ばれてきていました。そのため、小学校と中学校で同じ容器を使っていました。"},{"id":{"d":383,"n":7,"t":2},"speaker":"A","text":"なるほど、そういう仕組みもあるのですね。"}];
INSERT INTO sentence [{"id":{"d":384,"n":1,"t":2},"speaker":"A","text":"今日は雨が降っているので、昼休みは体育館で遊びませんか？"},{"id":{"d":384,"n":2,"t":2},"speaker":"B","text":"そうしましょう。体育館で遊ぶのは久しぶりですね。"},{"id":{"d":384,"n":3,"t":2},"speaker":"A","text":"そうですね。何をして遊びますか？"},{"id":{"d":384,"n":4,"t":2},"speaker":"B","text":"バレーボールやバスケットボールはどうでしょうか？"},{"id":{"d":384,"n":5,"t":2},"speaker":"A","text":"いいですね。他の人も誘って、対戦形式のゲームをしましょう。"},{"id":{"d":384,"n":6,"t":2},"speaker":"B","text":"バレーボールなら12人、バスケットボールなら10人集める必要がありますね。"},{"id":{"d":384,"n":7,"t":2},"speaker":"A","text":"はい。まずは、クラスの人に声を掛けてみて、もし、集まらなければ他のクラスの人にも声を掛けてみます。"}];
INSERT INTO sentence [{"id":{"d":385,"n":1,"t":2},"speaker":"A","text":"再来週は、模擬試験ですが、勉強の調子はどうですか？"},{"id":{"d":385,"n":2,"t":2},"speaker":"B","text":"私は、再来週に模擬試験を受ける予定はありませんよ。"},{"id":{"d":385,"n":3,"t":2},"speaker":"A","text":"私の予定には、模擬試験があると書いてあります。日付を勘違いしていたのでしょうか？"},{"id":{"d":385,"n":4,"t":2},"speaker":"B","text":"多分、大学別の模擬試験なので違う日程なのだと思いますよ。私の志望する大学の模擬試験は、その次の週にあります。"},{"id":{"d":385,"n":5,"t":2},"speaker":"A","text":"ああ、よく見たら大学別の模擬試験でした。いつも通り、大学入試センター試験の模擬試験かと思っていました。"},{"id":{"d":385,"n":6,"t":2},"speaker":"B","text":"大学別のものなので、きっと記述式ですね。対策は大丈夫ですか？"},{"id":{"d":385,"n":7,"t":2},"speaker":"A","text":"普段からしっかり記述を意識して、問題を解いているので大丈夫だと思います。"},{"id":{"d":385,"n":8,"t":2},"speaker":"B","text":"それなら問題ありませんね。緊張しすぎず、実力を発揮してください。"},{"id":{"d":385,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":386,"n":1,"t":2},"speaker":"A","text":"今日の宿題で分からないところがあったので、教えてください。"},{"id":{"d":386,"n":2,"t":2},"speaker":"B","text":"解き方を教えれば良いですか？それとも答えを教えますか？"},{"id":{"d":386,"n":3,"t":2},"speaker":"A","text":"答えを教えてもらっても、実力にならないので解き方から教えてください。"},{"id":{"d":386,"n":4,"t":2},"speaker":"B","text":"分かりました。それでは、まず鈴木君の解き方を見せてください。"},{"id":{"d":386,"n":5,"t":2},"speaker":"A","text":"私は、このように解きました。しかし、答えが出ませんでした。"},{"id":{"d":386,"n":6,"t":2},"speaker":"B","text":"解き方はこれで正しいですが、ここで計算ミスをしています。先に約分をしてから計算してみてはいかがでしょうか？"},{"id":{"d":386,"n":7,"t":2},"speaker":"A","text":"なるほど、ここの計算を間違っていたのですね。もう一度やってみます。"}];
INSERT INTO sentence [{"id":{"d":387,"n":1,"t":2},"speaker":"A","text":"国立大学は、学費がどの学部に行っても同じだと聞きました。"},{"id":{"d":387,"n":2,"t":2},"speaker":"B","text":"私はあまり知りませんが、ほとんど同じようです。ただ、医学部は必ず6年間在籍するので医学系ではない学部の人と比べて2年長く学費を払うことになります。"},{"id":{"d":387,"n":3,"t":2},"speaker":"A","text":"そのことを踏まえても、医学部に行くなら国立大学ですね。"},{"id":{"d":387,"n":4,"t":2},"speaker":"B","text":"私立の医学部も素晴らしいと思いますが、やはり学費を見てしまうと国立を目指したくなります。"},{"id":{"d":387,"n":5,"t":2},"speaker":"A","text":"実際、どれくらいの差があるのでしょうか？"},{"id":{"d":387,"n":6,"t":2},"speaker":"B","text":"あるサイトでは、5倍もの差があると記載されていました。"},{"id":{"d":387,"n":7,"t":2},"speaker":"A","text":"その差は大きいですね。国立医学部の学費はいくらですか？"},{"id":{"d":387,"n":8,"t":2},"speaker":"B","text":"350万円といわれています。つまり私立医学部の平均は約1750万円ということですね。"}];
INSERT INTO sentence [{"id":{"d":388,"n":1,"t":2},"speaker":"A","text":"今日は、半年ぶりに大学へ行きました。"},{"id":{"d":388,"n":2,"t":2},"speaker":"B","text":"講義がオンラインになると、そのようなことになるのですね。久しぶりの学校はどうでしたか？"},{"id":{"d":388,"n":3,"t":2},"speaker":"A","text":"思ったより懐かしさはありませんでしたが、学校に通っている実感は得られました。"},{"id":{"d":388,"n":4,"t":2},"speaker":"B","text":"半年行かなかったとしても、それまで5年間もいた学校なのでそうかもしれませんね。ちなみに学校には何をしに行ったのですか？"},{"id":{"d":388,"n":5,"t":2},"speaker":"A","text":"成績証明書を取りに行っていました。就職先に提出する書類です。"}];
INSERT INTO sentence [{"id":{"d":389,"n":1,"t":2},"speaker":"A","text":"後期の成績は確定しているはずなのですが、成績証明書にはまだ反映されていませんでした。"},{"id":{"d":389,"n":2,"t":2},"speaker":"B","text":"確定したのは、最近ですか？"},{"id":{"d":389,"n":3,"t":2},"speaker":"A","text":"半月ほど前には、確定していました。"},{"id":{"d":389,"n":4,"t":2},"speaker":"B","text":"それで、まだ反映されていなかったのですか？少し遅いような気がします。"},{"id":{"d":389,"n":5,"t":2},"speaker":"A","text":"そうですよね。就職先に提出しないといけない書類なので、早く反映してほしいのですが。"},{"id":{"d":389,"n":6,"t":2},"speaker":"B","text":"急ぐようであれば、教務課に問い合わせてみるのが良いと思いますよ。"},{"id":{"d":389,"n":7,"t":2},"speaker":"A","text":"そうですね。提出期限は、1週間後なのでもう少しだけ待ちたいと思います。"}];
INSERT INTO sentence [{"id":{"d":390,"n":1,"t":2},"speaker":"A","text":"先輩は、何を書いているのですか？"},{"id":{"d":390,"n":2,"t":2},"speaker":"B","text":"明日が締切日の奨学金の申請書類です。競争率は高いのですが、この奨学金に採択されると博士課程3年間分の授業料が賄えるだけでなく、年間50万円ほどの研究費がもらえるのです。"},{"id":{"d":390,"n":3,"t":2},"speaker":"A","text":"それは良い制度ですね。採択率はどの程度なのですか？"},{"id":{"d":390,"n":4,"t":2},"speaker":"B","text":"例年20パーセント程度です。"},{"id":{"d":390,"n":5,"t":2},"speaker":"A","text":"それほどに低いのですね。通ると良いですね。"},{"id":{"d":390,"n":6,"t":2},"speaker":"B","text":"はい。後藤さん、もし、よろしければこの申請書類の誤字脱字をチェックしてもらえませんか？"},{"id":{"d":390,"n":7,"t":2},"speaker":"A","text":"もちろん、良いですよ。他に何か、手伝うことはありますか？"},{"id":{"d":390,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。助かります。他は今のところ大丈夫ですが、もし、読んでいて内容的にも気になるところがあれば、遠慮なく指摘してもらえるとありがたいです。"},{"id":{"d":390,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":391,"n":1,"t":2},"speaker":"A","text":"小学生の時に、鍵盤ハーモニカは使っていましたか？"},{"id":{"d":391,"n":2,"t":2},"speaker":"B","text":"もちろん使っていました。ピアノを弾くことができなかったので、全く得意ではありませんでしたが、楽しかった思い出があります。"},{"id":{"d":391,"n":3,"t":2},"speaker":"A","text":"鍵盤ハーモニカでは、どのような曲を弾いていましたか？"},{"id":{"d":391,"n":4,"t":2},"speaker":"B","text":"記憶が曖昧ですが、『きらきら星』などの簡単な曲を弾いていたはずです。鍵盤ハーモニカでは、あまり難しい曲は演奏できませんし。"},{"id":{"d":391,"n":5,"t":2},"speaker":"A","text":"それが、鍵盤ハーモニカでも難しい曲を奏でることができるようですよ。"},{"id":{"d":391,"n":6,"t":2},"speaker":"B","text":"それは、本当ですか？"},{"id":{"d":391,"n":7,"t":2},"speaker":"A","text":"本当です。ほら、この動画を見てください。"},{"id":{"d":391,"n":8,"t":2},"speaker":"B","text":"これは素晴らしいですね。これが鍵盤ハーモニカで演奏されているとは、信じられません。"}];
INSERT INTO sentence [{"id":{"d":392,"n":1,"t":2},"speaker":"A","text":"明日から試験週間に入るので、部活動は休みです。明日から、早速図書館に通って勉強をしようと思います。"},{"id":{"d":392,"n":2,"t":2},"speaker":"B","text":"今回の試験は、いつも以上に気合が入っていますね。何かあるのですか？"},{"id":{"d":392,"n":3,"t":2},"speaker":"A","text":"今回の期末試験で、赤点を取ってしまい補習を受けることになると、部活動の夏合宿に行くことができなくなります。そのため、1つも赤点を取るわけにはいきません。"},{"id":{"d":392,"n":4,"t":2},"speaker":"B","text":"それは、頑張らなければいけませんね。"},{"id":{"d":392,"n":5,"t":2},"speaker":"A","text":"はい。物理が特に苦手なので、しっかり対策をしようと思います。沢村さんは物理が得意でしたよね。お勧めの勉強方法があれば教えてほしいです。"},{"id":{"d":392,"n":6,"t":2},"speaker":"B","text":"そうですね、私はまずは、教科書に載っている問題が必ず解けるようになるまで、ひたすら教科書の復習をします。物理は多くの問題で解法のパターンが共通しているので、基礎をしっかり理解することが重要です。"},{"id":{"d":392,"n":7,"t":2},"speaker":"A","text":"なるほど。参考になります。やみくもに問題集に手を出すのではなく、今回は教科書をしっかり勉強してみます。"},{"id":{"d":392,"n":8,"t":2},"speaker":"B","text":"苦手な物理でも、良い点がとれるといいですね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":393,"n":1,"t":2},"speaker":"A","text":"私の大学には1つのキャンパスの中に学生食堂が4つほどあるので、どこで食べようか迷ってしまいます。"},{"id":{"d":393,"n":2,"t":2},"speaker":"B","text":"それぞれの学生食堂には、同じメニューが置いてあるのではありませんか？"},{"id":{"d":393,"n":3,"t":2},"speaker":"A","text":"基本のメニューは同じなのですが、それぞれの食堂にはその食堂限定の特別メニューが置いてあるのです。"},{"id":{"d":393,"n":4,"t":2},"speaker":"B","text":"それは面白いですね。どのようなメニューがあるのですか？"},{"id":{"d":393,"n":5,"t":2},"speaker":"A","text":"例えば、第一食堂には、基本メニューにあるカレーライスよりもはるかに激辛のカレーライスがあります。第二食堂には、サラダバー付きの本格パスタがあります。第三食堂では、日替わりのランチプレートを、第四食堂では、日替わりのデザートプレートをそれぞれ頼むことができます。"},{"id":{"d":393,"n":6,"t":2},"speaker":"B","text":"どれも魅力的ですね。"},{"id":{"d":393,"n":7,"t":2},"speaker":"A","text":"そうなのです。特別メニューはどれもとてもおいしくて、その食堂へ行っても、ついそればかりを頼んでしまいます。"}];
INSERT INTO sentence [{"id":{"d":394,"n":1,"t":2},"speaker":"A","text":"先輩は、学生のうちにやり残したことはありますか？在学中にやっておくべきことについてアドバイスをいただきたいです。"},{"id":{"d":394,"n":2,"t":2},"speaker":"B","text":"やり残したと感じていることはありませんが、これもしておけば良かったと思うものは、いくつかあります。"},{"id":{"d":394,"n":3,"t":2},"speaker":"A","text":"それは、どのようなことですか？"},{"id":{"d":394,"n":4,"t":2},"speaker":"B","text":"1つはアルバイトです。私は飲食店でしかアルバイトをしていませんでしたが、他にもさまざまな職種を経験しておけば良かったと思っています。"},{"id":{"d":394,"n":5,"t":2},"speaker":"A","text":"なるほど。他にはどのようなことがありますか？"},{"id":{"d":394,"n":6,"t":2},"speaker":"B","text":"あとは、他大学との共同研究です。在学中の研究環境には、特に不満はありませんでしたが、他の大学ではどのような環境で、どのようなことを学んでいるのかを知ることも大切だったと思います。"},{"id":{"d":394,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。在学中には、いろいろなアルバイトをしたり、共同研究に参画したりするのが良いということですね。"},{"id":{"d":394,"n":8,"t":2},"speaker":"B","text":"はい。ただし在学中に、最も本気で取り組むべきことは学業です。あくまでもその上で、これらのことに挑戦してみるのも良いかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":395,"n":1,"t":2},"speaker":"A","text":"この授業は、外部試験を受けて、その成績によって単位が変化します。"},{"id":{"d":395,"n":2,"t":2},"speaker":"B","text":"質問です。その外部試験の対策は各自でするということですか？"},{"id":{"d":395,"n":3,"t":2},"speaker":"A","text":"各自でもしていただきますが、講義の中でも対策の勉強はもちろんやります。"},{"id":{"d":395,"n":4,"t":2},"speaker":"B","text":"講義で行う対策は、どのようなものがあるのでしょうか？"},{"id":{"d":395,"n":5,"t":2},"speaker":"A","text":"コンピューターを用いた、リスニング対策などがあります。"},{"id":{"d":395,"n":6,"t":2},"speaker":"B","text":"それは、いつでも受けてよいのですか？"},{"id":{"d":395,"n":7,"t":2},"speaker":"A","text":"はい。コンピューターを利用可能な環境があれば、いつでも好きな時間に行うことができます。"},{"id":{"d":395,"n":8,"t":2},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":396,"n":1,"t":2},"speaker":"A","text":"1年生のうちに取っておくと良い、お薦めの講義があったら教えていただきたいです。"},{"id":{"d":396,"n":2,"t":2},"speaker":"B","text":"私が1年生の時に受講した講義の中で一番面白かったのは、雑学を学ぶ講義です。"},{"id":{"d":396,"n":3,"t":2},"speaker":"A","text":"それはどのような講義ですか？想像がつきません。"},{"id":{"d":396,"n":4,"t":2},"speaker":"B","text":"世界中の雑学を学ぶというものでした。雑学を通して世界の国や地域の文化についても学ぶことができ、とても勉強になりました。"},{"id":{"d":396,"n":5,"t":2},"speaker":"A","text":"とても面白そうな講義ですが、成績はどのようにつけられるのですか？"},{"id":{"d":396,"n":6,"t":2},"speaker":"B","text":"成績はレポートでつけられました。"},{"id":{"d":396,"n":7,"t":2},"speaker":"A","text":"どのようなレポートですか？"},{"id":{"d":396,"n":8,"t":2},"speaker":"B","text":"私のときは、講義では紹介されなかった雑学を3つ探してくるというものでした。"}];
INSERT INTO sentence [{"id":{"d":397,"n":1,"t":2},"speaker":"A","text":"今日から算数の授業は、2つのクラスに分けて開催されます。"},{"id":{"d":397,"n":2,"t":2},"speaker":"B","text":"どのように分かれるのですか？"},{"id":{"d":397,"n":3,"t":2},"speaker":"A","text":"基礎をしっかり勉強したい人のためのクラスと、発展的な内容まで勉強したい人のためのクラスを用意しました。各自、希望する方のクラスで授業を受けることができます。"},{"id":{"d":397,"n":4,"t":2},"speaker":"B","text":"算数は好きなので、発展的な内容も解いてみたいと思いますが、得意とはいえないのでついていけるか不安です。"},{"id":{"d":397,"n":5,"t":2},"speaker":"A","text":"途中でいつでもクラスを変更することができるので、まずは興味のある方へ行ってみたら良いと思います。"},{"id":{"d":397,"n":6,"t":2},"speaker":"B","text":"分かりました。いつでもクラスを変更できるということは、教科書を進めるスピードは両方のクラスで同じということですか？"},{"id":{"d":397,"n":7,"t":2},"speaker":"A","text":"はい、そうです。基礎のクラスでは、これまで通り教科書の内容をしっかりと勉強します。発展のクラスでは教科書の内容を勉強した上で、残りの時間で同じ内容のより発展的な問題に取り組みます。"},{"id":{"d":397,"n":8,"t":2},"speaker":"B","text":"よく分かりました。では、今日は発展的な内容まで勉強したい人のためのクラスに参加してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":398,"n":1,"t":2},"speaker":"A","text":"来週1週間は集団下校期間です。6年生の皆さんは1年生と一緒に帰ることになっています。各グループのリーダーになっている人は、名簿を渡すので前へ取りに来てください。"},{"id":{"d":398,"n":2,"t":2},"speaker":"B","text":"先生、リーダーはこの名簿で何をすればよいのですか？"},{"id":{"d":398,"n":3,"t":2},"speaker":"A","text":"学校を出発するときに、この名簿に載っている人が全員いることを確認してください。"},{"id":{"d":398,"n":4,"t":2},"speaker":"B","text":"もし、いない人がいる場合はどうすれば良いですか？"},{"id":{"d":398,"n":5,"t":2},"speaker":"A","text":"時間になっても集合していない人がいる場合は、担当の先生にそのことを報告してから、下校を開始してください。"},{"id":{"d":398,"n":6,"t":2},"speaker":"B","text":"担当は生活指導の芦屋先生でしたね。この名簿はずっと私が持っていてよいのですか？"},{"id":{"d":398,"n":7,"t":2},"speaker":"A","text":"はい。名簿は集団下校期間の最終日である金曜日に回収しますので、それまではなくさないように管理していてください。"},{"id":{"d":398,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":399,"n":1,"t":2},"speaker":"A","text":"今日はカメラマンの方が、私たちの卒業アルバム用の写真を撮ってくれます。"},{"id":{"d":399,"n":2,"t":2},"speaker":"B","text":"あちらにいる方がそうですね。一日中撮影してくれるのですか？"},{"id":{"d":399,"n":3,"t":2},"speaker":"A","text":"そうです。授業中はもちろん、給食や掃除の時間も撮影してくれるそうです。"},{"id":{"d":399,"n":4,"t":2},"speaker":"B","text":"それでは、今日はいつもより気合を入れて掃除をしないといけませんね。"},{"id":{"d":399,"n":5,"t":2},"speaker":"A","text":"本当はいつもそうすべきですが、しっかり掃除をするのはいいことです。"},{"id":{"d":399,"n":6,"t":2},"speaker":"B","text":"授業も今日は真面目に受けようと思います。"},{"id":{"d":399,"n":7,"t":2},"speaker":"A","text":"今日は、どのような授業がありましたか？"},{"id":{"d":399,"n":8,"t":2},"speaker":"B","text":"今日の時間割は、理科、音楽、体育、家庭です。"},{"id":{"d":399,"n":9,"t":2},"speaker":"A","text":"そういえば、理科は化学室で実験の予定でしたね。今日は教室での授業が1教科もないようです。"},{"id":{"d":399,"n":10,"t":2},"speaker":"B","text":"本当ですね。教室での授業風景は、撮影できないのでしょうか？"},{"id":{"d":399,"n":11,"t":2},"speaker":"A","text":"どうでしょうか？もしかしたら、後日改めて撮影してもらえるのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":400,"n":1,"t":2},"speaker":"A","text":"大学では授業ではなく、講義という言葉がよく使われますね。"},{"id":{"d":400,"n":2,"t":2},"speaker":"B","text":"確かに、そうですね。"},{"id":{"d":400,"n":3,"t":2},"speaker":"A","text":"高校の時は「授業」と言っていました。講義と、授業は、どう違うのでしょうか？"},{"id":{"d":400,"n":4,"t":2},"speaker":"B","text":"インターネットで調べてみると、講義は知識を教えること、授業は知識に加えて技術や技能も教えること、という説明がありました。"},{"id":{"d":400,"n":5,"t":2},"speaker":"A","text":"なるほど。確かに、高校までは授業として実験や体育なども含まれていましたね。大学の講義というと、確かに座学の印象があります。"},{"id":{"d":400,"n":6,"t":2},"speaker":"B","text":"そうですね。座学以外の講義は、演習や実験、実技など、また別の呼び方をされているイメージがあります。"}];
INSERT INTO sentence [{"id":{"d":401,"n":1,"t":2},"speaker":"A","text":"おはようございます。今日から、新学期ですね。"},{"id":{"d":401,"n":2,"t":2},"speaker":"B","text":"校門にクラス分けについての掲示がありました。見に行きましょう。"},{"id":{"d":401,"n":3,"t":2},"speaker":"A","text":"一緒に行きましょう。今年も奥山さんと、同じクラスだといいのですが。"},{"id":{"d":401,"n":4,"t":2},"speaker":"B","text":"私も高地さんと同じクラスだと、うれしいです。人が多くてよく見えませんね。"},{"id":{"d":401,"n":5,"t":2},"speaker":"A","text":"そうですね。あ、見えました。私は1組でした。"},{"id":{"d":401,"n":6,"t":2},"speaker":"B","text":"私も1組でした。同じクラスですね。"},{"id":{"d":401,"n":7,"t":2},"speaker":"A","text":"うれしいです。担任の先生は誰になるのでしょうか？"},{"id":{"d":401,"n":8,"t":2},"speaker":"B","text":"担任の先生のお名前は、始業式で発表されるはずです。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":402,"n":1,"t":2},"speaker":"A","text":"最近は、小学校の授業でも、タブレットが利用されているようです。"},{"id":{"d":402,"n":2,"t":2},"speaker":"B","text":"もうノートは、使わないのですか？"},{"id":{"d":402,"n":3,"t":2},"speaker":"A","text":"タブレット型端末で、メモを取りそのまま保存することができるので昔ほどは、ノートを使っていないようです。黒板などもホワイトボードやタッチパネルなどに変化しているようです。"},{"id":{"d":402,"n":4,"t":2},"speaker":"B","text":"チョークを使った授業は少なくなっているのですね。そろそろ、チョークの存在を知らない子どもも出てくるのかもしれません。"},{"id":{"d":402,"n":5,"t":2},"speaker":"A","text":"時代の流れを感じますね。"}];
INSERT INTO sentence [{"id":{"d":403,"n":1,"t":2},"speaker":"A","text":"大学1年生向け学部紹介会の、プレゼンターになっていただけませんか？"},{"id":{"d":403,"n":2,"t":2},"speaker":"B","text":"私は、何をしたら良いのですか？"},{"id":{"d":403,"n":3,"t":2},"speaker":"A","text":"ご自身の研究や所属学部の印象を、分かりやすく紹介していただければと思います。"},{"id":{"d":403,"n":4,"t":2},"speaker":"B","text":"何か用意するものはあるのですか？"},{"id":{"d":403,"n":5,"t":2},"speaker":"A","text":"簡単で良いので、発表スライドをご用意いただきたいです。"},{"id":{"d":403,"n":6,"t":2},"speaker":"B","text":"発表時間はどれくらい頂けるのですか？"},{"id":{"d":403,"n":7,"t":2},"speaker":"A","text":"1回の発表時間は、10分から15分を考えています。それを5回ほど行っていただく予定です。"},{"id":{"d":403,"n":8,"t":2},"speaker":"B","text":"分かりました。少し考えさせてください。いつまでに返答したらよろしいですか？"},{"id":{"d":403,"n":9,"t":2},"speaker":"A","text":"なるべく早いと助かります。可能なら明日にでもいただけると、うれしいです。"},{"id":{"d":403,"n":10,"t":2},"speaker":"B","text":"承知しました。"}];
INSERT INTO sentence [{"id":{"d":404,"n":1,"t":2},"speaker":"A","text":"明日の図工では、新聞紙を使います。持ってくるのを忘れないようにしてくださいね。"},{"id":{"d":404,"n":2,"t":2},"speaker":"B","text":"先生、うちでは新聞を取っていません。"},{"id":{"d":404,"n":3,"t":2},"speaker":"A","text":"そうでしたか。近所の方やお友達に分けていただくことはできませんか？"},{"id":{"d":404,"n":4,"t":2},"speaker":"B","text":"それでは、真梨香ちゃんに聞いてみます。"},{"id":{"d":404,"n":5,"t":2},"speaker":"A","text":"学校でも何部か用意しておくようにしますので、準備できなかったときはそれを使いましょう。"},{"id":{"d":404,"n":6,"t":2},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":405,"n":1,"t":2},"speaker":"A","text":"こんにちは。この英語用のノートを5冊下さい。"},{"id":{"d":405,"n":2,"t":2},"speaker":"B","text":"毎度、ありがとうございます。この間も、同じノートを買いに来ましたね。"},{"id":{"d":405,"n":3,"t":2},"speaker":"A","text":"はい。たくさん使うので、すぐに使い終わってしまうのです。"},{"id":{"d":405,"n":4,"t":2},"speaker":"B","text":"英語の勉強を頑張っているのですね？"},{"id":{"d":405,"n":5,"t":2},"speaker":"A","text":"英単語がなかなか覚えられないので、たくさん書いているのです。次のテストでこそ良い点を取りたいです。"},{"id":{"d":405,"n":6,"t":2},"speaker":"B","text":"頑張ってくださいね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":406,"n":1,"t":2},"speaker":"A","text":"国語の教科書を忘れてしまいました。"},{"id":{"d":406,"n":2,"t":2},"speaker":"B","text":"私のクラスは今日、国語の授業がないので、教科書を持ってきていません。"},{"id":{"d":406,"n":3,"t":2},"speaker":"A","text":"そうでしたか。他のクラスで今日、国語の授業があるところを知りませんか？"},{"id":{"d":406,"n":4,"t":2},"speaker":"B","text":"確か、3組は2時間目が国語だったと思います。"},{"id":{"d":406,"n":5,"t":2},"speaker":"A","text":"3組のお友達を紹介してもらえませんか？"},{"id":{"d":406,"n":6,"t":2},"speaker":"B","text":"佐藤さんならきっと貸してくれると思いますよ。今から佐藤さんのところへ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":407,"n":1,"t":2},"speaker":"A","text":"今日の給食は、大好きなグラタンです。"},{"id":{"d":407,"n":2,"t":2},"speaker":"B","text":"マカロニのグラタンですか？"},{"id":{"d":407,"n":3,"t":2},"speaker":"A","text":"いえ、今日はジャガイモとベーコンのグラタンだそうです。"},{"id":{"d":407,"n":4,"t":2},"speaker":"B","text":"マカロニより好きな味です。"},{"id":{"d":407,"n":5,"t":2},"speaker":"A","text":"楽しみですね。"},{"id":{"d":407,"n":6,"t":2},"speaker":"B","text":"早くお昼になるといいですね。"},{"id":{"d":407,"n":7,"t":2},"speaker":"A","text":"まだ、1時間目が終わったところですけれど。"}];
INSERT INTO sentence [{"id":{"d":408,"n":1,"t":2},"speaker":"A","text":"そちらのコピー機で、この楽譜をコピーしてください。"},{"id":{"d":408,"n":2,"t":2},"speaker":"B","text":"はい、これはテノールの楽譜ですね。"},{"id":{"d":408,"n":3,"t":2},"speaker":"A","text":"私はソプラノとアルトの楽譜を担当しますから、あなたはテノールの楽譜をお願いします。"},{"id":{"d":408,"n":4,"t":2},"speaker":"B","text":"分かりました。これは何の楽譜ですか？"},{"id":{"d":408,"n":5,"t":2},"speaker":"A","text":"これは合唱コンクール用の曲の楽譜です。"},{"id":{"d":408,"n":6,"t":2},"speaker":"B","text":"今年の課題曲は、これほど難しいのですか？"},{"id":{"d":408,"n":7,"t":2},"speaker":"A","text":"そのようです。たくさん練習しなければいけませんね。"}];
INSERT INTO sentence [{"id":{"d":409,"n":1,"t":2},"speaker":"A","text":"夏休みの最終日なのに、宿題が終わっていません。"},{"id":{"d":409,"n":2,"t":2},"speaker":"B","text":"あと、どれくらい宿題が残っているのですか？"},{"id":{"d":409,"n":3,"t":2},"speaker":"A","text":"工作が残っています。"},{"id":{"d":409,"n":4,"t":2},"speaker":"B","text":"何を作る予定だったのですか？"},{"id":{"d":409,"n":5,"t":2},"speaker":"A","text":"牛乳パックで、ペン立てを作ろうと思っていました。"},{"id":{"d":409,"n":6,"t":2},"speaker":"B","text":"牛乳パックは、用意できているのですか？"},{"id":{"d":409,"n":7,"t":2},"speaker":"A","text":"3つ必要なのに2つしかありません。"},{"id":{"d":409,"n":8,"t":2},"speaker":"B","text":"今から、コンビニエンスストアに牛乳を買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":410,"n":1,"t":2},"speaker":"A","text":"明日のテストのための勉強が全然できていません。"},{"id":{"d":410,"n":2,"t":2},"speaker":"B","text":"明日は、日本史のテストでしたか？"},{"id":{"d":410,"n":3,"t":2},"speaker":"A","text":"はい、戦国時代がテスト範囲です。"},{"id":{"d":410,"n":4,"t":2},"speaker":"B","text":"戦国時代なら、この参考書の付録についていた年表がとても分かりやすいですよ。"},{"id":{"d":410,"n":5,"t":2},"speaker":"A","text":"見せてもらえますか？"},{"id":{"d":410,"n":6,"t":2},"speaker":"B","text":"これです。よろしければ参考書ごと貸しますので、使ってください。"},{"id":{"d":410,"n":7,"t":2},"speaker":"A","text":"これは、分かりやすいです。参考書ごと借りてよいのですか？"},{"id":{"d":410,"n":8,"t":2},"speaker":"B","text":"どうぞ。良い点が取れると良いですね。"}];
INSERT INTO sentence [{"id":{"d":411,"n":1,"t":2},"speaker":"A","text":"最近は、数学の勉強ばかりしていたので、国語は、全然勉強できていません。"},{"id":{"d":411,"n":2,"t":2},"speaker":"B","text":"今回の中間テストは、国語の試験範囲が広いので、勉強しないと点数が取れなさそうですよ。"},{"id":{"d":411,"n":3,"t":2},"speaker":"A","text":"この調子だと、国語は漢字のところしか点が取れなさそうです。"},{"id":{"d":411,"n":4,"t":2},"speaker":"B","text":"それだと20点がせいぜいだと思います。漢字なら確実に点が取れるのですか？"},{"id":{"d":411,"n":5,"t":2},"speaker":"A","text":"はい、自信はあります。私は、漢字だけは得意で、日本漢字能力検定1級も持っています。"},{"id":{"d":411,"n":6,"t":2},"speaker":"B","text":"それは、すごいですね。"}];
INSERT INTO sentence [{"id":{"d":412,"n":1,"t":2},"speaker":"A","text":"文化祭の模擬店ですが、流行のタピオカミルクティー屋さんなどは、いかがでしょうか？"},{"id":{"d":412,"n":2,"t":2},"speaker":"B","text":"確かに人気は出そうですが、タピオカミルクティーは素人でも簡単に作れるものなのでしょうか？"},{"id":{"d":412,"n":3,"t":2},"speaker":"A","text":"分かりません、調べてみます。出来合いのタピオカを使えば、結構簡単に作れるようですよ。"},{"id":{"d":412,"n":4,"t":2},"speaker":"B","text":"そうなのですね。それならば、良い案かもしれません。"},{"id":{"d":412,"n":5,"t":2},"speaker":"A","text":"タピオカミルクティーと、一緒に台湾カステラも出すのはどうですか？"},{"id":{"d":412,"n":6,"t":2},"speaker":"B","text":"それも、簡単に作れるのなら良さそうです。調べてみましょう。"}];
INSERT INTO sentence [{"id":{"d":413,"n":1,"t":2},"speaker":"A","text":"文化祭の準備は、順調ですか？"},{"id":{"d":413,"n":2,"t":2},"speaker":"B","text":"はい。予定通りに、進んでいます。"},{"id":{"d":413,"n":3,"t":2},"speaker":"A","text":"それは何よりです。加藤君のクラスは何をするのですか？"},{"id":{"d":413,"n":4,"t":2},"speaker":"B","text":"僕たちのクラスは、忍者喫茶をします。"},{"id":{"d":413,"n":5,"t":2},"speaker":"A","text":"とても面白そうですね。いま作っているのは、手裏剣ですか？"},{"id":{"d":413,"n":6,"t":2},"speaker":"B","text":"そうです。僕らの班は小道具を作っています。これは手裏剣で、こっちはまきびし、あれは隠れみのです。"},{"id":{"d":413,"n":7,"t":2},"speaker":"A","text":"すごくよくできていますね。とてもダンボールで作っているとは思えません。"},{"id":{"d":413,"n":8,"t":2},"speaker":"B","text":"うれしいです。衣装も、とてもよくできているので、当日もぜひ見に来てください。"}];
INSERT INTO sentence [{"id":{"d":414,"n":1,"t":2},"speaker":"A","text":"今日は、中学の入学式があるようです。窓から新入生の列が見えますね。"},{"id":{"d":414,"n":2,"t":2},"speaker":"B","text":"理由は分かりませんが、初々しい感じがします。私たちにもあのような時代がありましね。"},{"id":{"d":414,"n":3,"t":2},"speaker":"A","text":"新しい制服がうれしかったのを覚えています。"},{"id":{"d":414,"n":4,"t":2},"speaker":"B","text":"そういえば、桜の下で一緒に写真を撮りましたね。"},{"id":{"d":414,"n":5,"t":2},"speaker":"A","text":"そうでしたね。もう5年前ですか、懐かしいです。"}];
INSERT INTO sentence [{"id":{"d":415,"n":1,"t":2},"speaker":"A","text":"来年の4月からは、制服が自由に選べるようになりますね。"},{"id":{"d":415,"n":2,"t":2},"speaker":"B","text":"はい。私は新しくできた、パンツスタイルを選ぼうと思っています。"},{"id":{"d":415,"n":3,"t":2},"speaker":"A","text":"良いですね。私はスカートの方が好きなので、スカートのままにしようかと思っています。"},{"id":{"d":415,"n":4,"t":2},"speaker":"B","text":"私もスカートが好きなので迷いましたが、自転車で通学する時にパンツの方が動きやすそうなので、まずはそちらにしてみます。"},{"id":{"d":415,"n":5,"t":2},"speaker":"A","text":"確かに、通学時はパンツも良いかもしれませんね。防寒にもなりそうです。"}];
INSERT INTO sentence [{"id":{"d":416,"n":1,"t":2},"speaker":"A","text":"今回の数学の試験で、0点を取ってしまいました。"},{"id":{"d":416,"n":2,"t":2},"speaker":"B","text":"本当ですか？数学は得意だったのではありませんか？"},{"id":{"d":416,"n":3,"t":2},"speaker":"A","text":"マークシートの解答を1つずつ、ずらして書いてしまったのです。ずれていなければ90点は取れていました。"},{"id":{"d":416,"n":4,"t":2},"speaker":"B","text":"それは、もったいないですね。"},{"id":{"d":416,"n":5,"t":2},"speaker":"A","text":"最初の問題が分からなくて、後で解こうと思って飛ばしたのが、運の尽きでした。とても悔しいです。"},{"id":{"d":416,"n":6,"t":2},"speaker":"B","text":"追試になってしまうのですか？"},{"id":{"d":416,"n":7,"t":2},"speaker":"A","text":"多分来週に追試だと思います。"},{"id":{"d":416,"n":8,"t":2},"speaker":"B","text":"今度は間違えないように気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":417,"n":1,"t":2},"speaker":"A","text":"今度、転校することになりました。"},{"id":{"d":417,"n":2,"t":2},"speaker":"B","text":"突然のことで、驚きました。いつですか？"},{"id":{"d":417,"n":3,"t":2},"speaker":"A","text":"年明けからは、新しい学校に通います。"},{"id":{"d":417,"n":4,"t":2},"speaker":"B","text":"遠くに引っ越してしまうのですか？"},{"id":{"d":417,"n":5,"t":2},"speaker":"A","text":"父の転勤で、北海道に行くことになりました。"},{"id":{"d":417,"n":6,"t":2},"speaker":"B","text":"ここからは、遠いですね。とても寂しいです。"},{"id":{"d":417,"n":7,"t":2},"speaker":"A","text":"私も寂しいです。卒業までこの学校に通いたかったです。"},{"id":{"d":417,"n":8,"t":2},"speaker":"B","text":"遠くに離れても、ずっと友達でいてくださいね。"},{"id":{"d":417,"n":9,"t":2},"speaker":"A","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":418,"n":1,"t":2},"speaker":"A","text":"そろそろ出掛ける時間ですね。受験票は持ちましたか？"},{"id":{"d":418,"n":2,"t":2},"speaker":"B","text":"確認します。かばんに入っていました。"},{"id":{"d":418,"n":3,"t":2},"speaker":"A","text":"受験票を持っていれば大丈夫ですよ。気を付けて行ってきてくださいね。"},{"id":{"d":418,"n":4,"t":2},"speaker":"B","text":"合格発表を1人で見に行くのは緊張します。不合格の場合は、どうしたらよいでしょうか？"},{"id":{"d":418,"n":5,"t":2},"speaker":"A","text":"合否が分かり次第、家と学校に連絡してください。きっと大丈夫ですよ。"},{"id":{"d":418,"n":6,"t":2},"speaker":"B","text":"緊張でずっと足が震えていますが、そろそろ出なければ間に合いません。覚悟を決めて行ってきます。"},{"id":{"d":418,"n":7,"t":2},"speaker":"A","text":"はい、いってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":419,"n":1,"t":2},"speaker":"A","text":"卒業式で、ピアノを弾いてくれる人を探しています。"},{"id":{"d":419,"n":2,"t":2},"speaker":"B","text":"1組の高梨君はピアノがとても上手ですよ。"},{"id":{"d":419,"n":3,"t":2},"speaker":"A","text":"そうなのですね。突然お願いしても弾いてくれるでしょうか？"},{"id":{"d":419,"n":4,"t":2},"speaker":"B","text":"とても優しい人なので、きっと大丈夫だと思います。"},{"id":{"d":419,"n":5,"t":2},"speaker":"A","text":"一緒に頼んでくれますか？"},{"id":{"d":419,"n":6,"t":2},"speaker":"B","text":"よいですよ。いまから1組に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":420,"n":1,"t":2},"speaker":"A","text":"明日のマラソン大会は、欠席したいです。"},{"id":{"d":420,"n":2,"t":2},"speaker":"B","text":"私も同感です。"},{"id":{"d":420,"n":3,"t":2},"speaker":"A","text":"私は、走るのは苦手です。"},{"id":{"d":420,"n":4,"t":2},"speaker":"B","text":"僕も走ることは苦手です。長距離は特に苦手です。"},{"id":{"d":420,"n":5,"t":2},"speaker":"A","text":"箱根駅伝の選手は、どうしてあのような山道を走れるのでしょうか？"},{"id":{"d":420,"n":6,"t":2},"speaker":"B","text":"さっそうと山道を駆け抜けていく姿は、本当にすてきですよね。きっと、日頃の練習もすごいのでしょう。"},{"id":{"d":420,"n":7,"t":2},"speaker":"A","text":"あのような風に走れたら、きっと楽しいのでしょうね。僕らも毎日走る練習をすれば、少しは走れるようになるのでしょうか？"}];
INSERT INTO sentence [{"id":{"d":421,"n":1,"t":2},"speaker":"A","text":"研究室に常備していたお茶の葉が切れていました。生協で買ってきてくれますか？"},{"id":{"d":421,"n":2,"t":2},"speaker":"B","text":"分かりました。どのお茶の葉を買ってくれば良いですか？"},{"id":{"d":421,"n":3,"t":2},"speaker":"A","text":"確か、教授の好みは鹿児島の知覧茶だったはずです。それを2袋お願いします。"},{"id":{"d":421,"n":4,"t":2},"speaker":"B","text":"分かりました。他に必要なものはありますか？"},{"id":{"d":421,"n":5,"t":2},"speaker":"A","text":"他に切れているものは見当たらないので、今回はお茶だけで大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":422,"n":1,"t":2},"speaker":"A","text":"おばあさん、ランドセルを買ってくださりありがとうございます。"},{"id":{"d":422,"n":2,"t":2},"speaker":"B","text":"どういたしまして。気に入ってもらえましたか？"},{"id":{"d":422,"n":3,"t":2},"speaker":"A","text":"はい。水色がきれいで、とても気に入っています。"},{"id":{"d":422,"n":4,"t":2},"speaker":"B","text":"それは良かったです。昔は女の子のランドセルといえば赤だったのですが、今はいろいろな色が選べるのですね。"},{"id":{"d":422,"n":5,"t":2},"speaker":"A","text":"売り場にはいろいろな色のランドセルがありました。オレンジのランドセルとどちらにするか迷ったのですが、水色のランドセルにして良かったです。"},{"id":{"d":422,"n":6,"t":2},"speaker":"B","text":"とてもよく似合っていますよ。学校に行くのが楽しみですね。"},{"id":{"d":422,"n":7,"t":2},"speaker":"A","text":"はい。早くこれを背負って小学校に行きたいです。"}];
INSERT INTO sentence [{"id":{"d":423,"n":1,"t":2},"speaker":"A","text":"志望している学校で、体験入学の募集がありました。"},{"id":{"d":423,"n":2,"t":2},"speaker":"B","text":"それは良いチャンスですね。どこの学校ですか？"},{"id":{"d":423,"n":3,"t":2},"speaker":"A","text":"隣の市にある、看護専修学校です。"},{"id":{"d":423,"n":4,"t":2},"speaker":"B","text":"川島さんは看護師を目指しているのですか？"},{"id":{"d":423,"n":5,"t":2},"speaker":"A","text":"はい、幼稚園の頃からの憧れの職業です。"},{"id":{"d":423,"n":6,"t":2},"speaker":"B","text":"すてきですね。学校生活や受験のことについていろいろ質問してくると良いと思います。"},{"id":{"d":423,"n":7,"t":2},"speaker":"A","text":"そのつもりです。親睦会もあるので、友人もできたらいいなと思っています。"},{"id":{"d":423,"n":8,"t":2},"speaker":"B","text":"実りある1日になるといいですね。"}];
INSERT INTO sentence [{"id":{"d":424,"n":1,"t":2},"speaker":"A","text":"卒業後の進路を決めましたか？"},{"id":{"d":424,"n":2,"t":2},"speaker":"B","text":"はい、私は専修学校に進もうと思います。"},{"id":{"d":424,"n":3,"t":2},"speaker":"A","text":"専修学校で、何を学ぶのですか？"},{"id":{"d":424,"n":4,"t":2},"speaker":"B","text":"理学療法士になりたいので、その資格を取ろうと思っています。"},{"id":{"d":424,"n":5,"t":2},"speaker":"A","text":"目標がはっきりしていていいですね。"},{"id":{"d":424,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。無事入学できるように、受験勉強を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":425,"n":1,"t":2},"speaker":"A","text":"おとといの日本史のテストが満点でした。"},{"id":{"d":425,"n":2,"t":2},"speaker":"B","text":"素晴らしいです。日本史は、苦手だったのではありませんか？"},{"id":{"d":425,"n":3,"t":2},"speaker":"A","text":"苦手だったので、今回は一番時間を割いて勉強しました。"},{"id":{"d":425,"n":4,"t":2},"speaker":"B","text":"勉強の成果が出たのですね。"},{"id":{"d":425,"n":5,"t":2},"speaker":"A","text":"はい。とてもうれしいです。勉強しているうちに、苦手意識も少し和らいできました。"},{"id":{"d":425,"n":6,"t":2},"speaker":"B","text":"それは良いことですね。"},{"id":{"d":425,"n":7,"t":2},"speaker":"A","text":"日本史の面白さも、少し分かってきたような気がします。この調子で、これからも勉強を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":426,"n":1,"t":2},"speaker":"A","text":"昨日の水泳の授業は、楽しかったですね。"},{"id":{"d":426,"n":2,"t":2},"speaker":"B","text":"はい。プールの底に沈んだボールを集める宝探しゲームが、とても楽しかったです。"},{"id":{"d":426,"n":3,"t":2},"speaker":"A","text":"私は3つ取れました。"},{"id":{"d":426,"n":4,"t":2},"speaker":"B","text":"すごいですね。私は2つでした。"},{"id":{"d":426,"n":5,"t":2},"speaker":"A","text":"水に深く潜ることが苦手だったのですが、宝探しをしているうちに、いつの間にかできるようになっていました。"},{"id":{"d":426,"n":6,"t":2},"speaker":"B","text":"私もそうです。また、来週も宝探しだといいですね。"},{"id":{"d":426,"n":7,"t":2},"speaker":"A","text":"そうですね。でも、来週はいつも通りクロールの練習だそうですよ。"}];
INSERT INTO sentence [{"id":{"d":427,"n":1,"t":2},"speaker":"A","text":"生徒会の選挙に、立候補します。"},{"id":{"d":427,"n":2,"t":2},"speaker":"B","text":"生徒会長になるのですか？"},{"id":{"d":427,"n":3,"t":2},"speaker":"A","text":"いえ、生徒会長は2組の佐々木君です。"},{"id":{"d":427,"n":4,"t":2},"speaker":"B","text":"では副会長ですか？"},{"id":{"d":427,"n":5,"t":2},"speaker":"A","text":"いえ、副会長は6組の花田さんです。"},{"id":{"d":427,"n":6,"t":2},"speaker":"B","text":"では、あなたは何をするのですか？"},{"id":{"d":427,"n":7,"t":2},"speaker":"A","text":"私は、会計に立候補するつもりです。ぜひ、投票をお願いします。"},{"id":{"d":427,"n":8,"t":2},"speaker":"B","text":"もちろん、投票します。友達にも、大泉君への投票を呼びかけてみますね。"}];
INSERT INTO sentence [{"id":{"d":428,"n":1,"t":2},"speaker":"A","text":"明日は、いよいよ入学式です。"},{"id":{"d":428,"n":2,"t":2},"speaker":"B","text":"そうですね。制服は用意しましたか？"},{"id":{"d":428,"n":3,"t":2},"speaker":"A","text":"はい、用意ができました。"},{"id":{"d":428,"n":4,"t":2},"speaker":"B","text":"靴やかばんも大丈夫ですか？それから靴下の準備も必要ですね。"},{"id":{"d":428,"n":5,"t":2},"speaker":"A","text":"あ、靴下を忘れていました。"},{"id":{"d":428,"n":6,"t":2},"speaker":"B","text":"靴下も制服と一緒に置いておくといいですよ。"},{"id":{"d":428,"n":7,"t":2},"speaker":"A","text":"はい、そうします。これで、忘れることはありません。"}];
INSERT INTO sentence [{"id":{"d":429,"n":1,"t":2},"speaker":"A","text":"仮運転免許証の試験に、また落ちてしまいました。"},{"id":{"d":429,"n":2,"t":2},"speaker":"B","text":"その試験に落ちたのは何回目ですか？"},{"id":{"d":429,"n":3,"t":2},"speaker":"A","text":"4回目です。"},{"id":{"d":429,"n":4,"t":2},"speaker":"B","text":"自動車教習所には、きちんと行っていますか？"},{"id":{"d":429,"n":5,"t":2},"speaker":"A","text":"はい。授業もきちんと受けているのですが。"},{"id":{"d":429,"n":6,"t":2},"speaker":"B","text":"それでは過去問題を解いて対策をするのはどうですか？仮運転免許証の試験は、まるで引っ掛け問題のような出題も多いので、出題傾向に慣れておくのは大事かもしれません。"},{"id":{"d":429,"n":7,"t":2},"speaker":"A","text":"なるほど、それは良い案ですね。早速今日の帰りに過去問題集を購入したいと思います。"},{"id":{"d":429,"n":8,"t":2},"speaker":"B","text":"今度こそ、受かるといいですね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":430,"n":1,"t":2},"speaker":"A","text":"今日は、家庭科の授業で調理実習がありました。"},{"id":{"d":430,"n":2,"t":2},"speaker":"B","text":"何を作ったのですか？"},{"id":{"d":430,"n":3,"t":2},"speaker":"A","text":"おみそ汁と、ご飯と、ホウレンソウのお浸しです。"},{"id":{"d":430,"n":4,"t":2},"speaker":"B","text":"それだけですか？"},{"id":{"d":430,"n":5,"t":2},"speaker":"A","text":"あとは、お魚の照り焼きです。"},{"id":{"d":430,"n":6,"t":2},"speaker":"B","text":"何の魚だったのですか？"},{"id":{"d":430,"n":7,"t":2},"speaker":"A","text":"何でしたっけ。「カ」から始まる白身魚だった気がします。"},{"id":{"d":430,"n":8,"t":2},"speaker":"B","text":"「カジキ」でしょうか？おいしそうですね。"},{"id":{"d":430,"n":9,"t":2},"speaker":"A","text":"ああ、それです。「カジキの照り焼き」を作りました。とてもおいしかったです。"}];
INSERT INTO sentence [{"id":{"d":431,"n":1,"t":2},"speaker":"A","text":"塩田さんのことを、探していました。どこへ行っていたのですか？"},{"id":{"d":431,"n":2,"t":2},"speaker":"B","text":"放送室に行っていました。お昼の放送で流す曲を、リクエストしてきたのです。"},{"id":{"d":431,"n":3,"t":2},"speaker":"A","text":"そうだったのですね。何の曲をリクエストしたのですか？"},{"id":{"d":431,"n":4,"t":2},"speaker":"B","text":"高橋優さんの『虹』です。"},{"id":{"d":431,"n":5,"t":2},"speaker":"A","text":"知っています。「熱闘甲子園」という番組ののテーマソングだった曲ですね？"},{"id":{"d":431,"n":6,"t":2},"speaker":"B","text":"はい。明日は、野球部の県予選なのでその曲にしました。"},{"id":{"d":431,"n":7,"t":2},"speaker":"A","text":"応援の気持ちが、伝わるといいですね。"},{"id":{"d":431,"n":8,"t":2},"speaker":"B","text":"はい。頑張ってきてほしいです。"}];
INSERT INTO sentence [{"id":{"d":432,"n":1,"t":2},"speaker":"A","text":"図書委員の佐藤さんが、あなたのことを呼んでいましたよ。"},{"id":{"d":432,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。何の用事だったのでしょうか？"},{"id":{"d":432,"n":3,"t":2},"speaker":"A","text":"返却について何か言っていましたよ。"},{"id":{"d":432,"n":4,"t":2},"speaker":"B","text":"あ、そういえば、先々週に図書室で借りた本を、まだ返していませんでした。"},{"id":{"d":432,"n":5,"t":2},"speaker":"A","text":"何というタイトルの本を借りているのですか？"},{"id":{"d":432,"n":6,"t":2},"speaker":"B","text":"『塞王の楯』です。"},{"id":{"d":432,"n":7,"t":2},"speaker":"A","text":"驚きました。それは、私が次に借りようと予約していた本です。"},{"id":{"d":432,"n":8,"t":2},"speaker":"B","text":"すみません、すぐに返してきます。"}];
INSERT INTO sentence [{"id":{"d":433,"n":1,"t":2},"speaker":"A","text":"テスト勉強は進んでいますか？"},{"id":{"d":433,"n":2,"t":2},"speaker":"B","text":"全然進んでいません。数学がさっぱり分からなくて困っています。"},{"id":{"d":433,"n":3,"t":2},"speaker":"A","text":"よろしければ、教えますよ。私は、数学が得意です。"},{"id":{"d":433,"n":4,"t":2},"speaker":"B","text":"本当ですか？ありがとうございます。国語は得意なのですが、数学は大の苦手なので助かりました。"},{"id":{"d":433,"n":5,"t":2},"speaker":"A","text":"国語が得意なのですか？私は国語の点数がいつも低いので、ぜひ教えてほしいです。"},{"id":{"d":433,"n":6,"t":2},"speaker":"B","text":"そうだったのですね。では、国語と、数学を教え合いましょう。"}];
INSERT INTO sentence [{"id":{"d":434,"n":1,"t":2},"speaker":"A","text":"今日の宿題は、「学校からお家までの地図を描いてくること」です。"},{"id":{"d":434,"n":2,"t":2},"speaker":"B","text":"先生、私の家は、学校の隣のマンションです。"},{"id":{"d":434,"n":3,"t":2},"speaker":"A","text":"近くても、地図は描けますよ。"},{"id":{"d":434,"n":4,"t":2},"speaker":"B","text":"教室を出て、廊下を通って、昇降口に行って、校門に行ってというのを描くのですか？"},{"id":{"d":434,"n":5,"t":2},"speaker":"A","text":"そこまで、細かくしなくてもよいですよ。"},{"id":{"d":434,"n":6,"t":2},"speaker":"B","text":"学校と、マンションと、道路を描けばよいですか？"},{"id":{"d":434,"n":7,"t":2},"speaker":"A","text":"学校の前のお店や、マンションの前の公園も描きましょう。"},{"id":{"d":434,"n":8,"t":2},"speaker":"B","text":"道路の向こう側も描けばよいのですね。"}];
INSERT INTO sentence [{"id":{"d":435,"n":1,"t":2},"speaker":"A","text":"明日は、登校日ですね。"},{"id":{"d":435,"n":2,"t":2},"speaker":"B","text":"はい、楽しみにしていました。夏休みに、お友達に会えるのはうれしいです。"},{"id":{"d":435,"n":3,"t":2},"speaker":"A","text":"今日までの絵日記を、忘れずに持って学校へ行くのですよ。"},{"id":{"d":435,"n":4,"t":2},"speaker":"B","text":"はい。今日の分を書いたら、かばんに入れようと思います。"},{"id":{"d":435,"n":5,"t":2},"speaker":"A","text":"今日の分は、どのようなことを書くのですか？"},{"id":{"d":435,"n":6,"t":2},"speaker":"B","text":"お夕飯に、流しそうめんをしたことを書こうと思います。"},{"id":{"d":435,"n":7,"t":2},"speaker":"A","text":"流しそうめんの道具を、もう一度出しましょうか？"},{"id":{"d":435,"n":8,"t":2},"speaker":"B","text":"いえ、覚えているから大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":436,"n":1,"t":2},"speaker":"A","text":"夏休みが終わったら、すぐに文化祭がありますね？"},{"id":{"d":436,"n":2,"t":2},"speaker":"B","text":"そうですね。文化祭で展示するための美術部の作品作りが忙しくなりそうです。"},{"id":{"d":436,"n":3,"t":2},"speaker":"A","text":"今年は、何を作っているのですか？"},{"id":{"d":436,"n":4,"t":2},"speaker":"B","text":"粘土でライオンの像を作っています。"},{"id":{"d":436,"n":5,"t":2},"speaker":"A","text":"それで、夏休みに動物園に行ったのですね？"},{"id":{"d":436,"n":6,"t":2},"speaker":"B","text":"はい、デッサンをたくさんしてきました。"},{"id":{"d":436,"n":7,"t":2},"speaker":"A","text":"早く作品を見てみたいです。"},{"id":{"d":436,"n":8,"t":2},"speaker":"B","text":"文化祭を、楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":437,"n":1,"t":2},"speaker":"A","text":"渋谷キャンパスに、新しいカフェテリアができたことをご存じですか？"},{"id":{"d":437,"n":2,"t":2},"speaker":"B","text":"知りませんでした。気になります。"},{"id":{"d":437,"n":3,"t":2},"speaker":"A","text":"木曜日の午後は、講義がないので、渋谷キャンパスへ行ってみようと思っています。"},{"id":{"d":437,"n":4,"t":2},"speaker":"B","text":"私もその時間帯は空いているので、ご一緒してよいですか？"},{"id":{"d":437,"n":5,"t":2},"speaker":"A","text":"ええ、ぜひ、一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":438,"n":1,"t":2},"speaker":"A","text":"新しい鉛筆ですね？"},{"id":{"d":438,"n":2,"t":2},"speaker":"B","text":"はい、先ほど、購買部で買ってきました。"},{"id":{"d":438,"n":3,"t":2},"speaker":"A","text":"鉛筆削りを貸しましょうか？"},{"id":{"d":438,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。でも、これは削らないので大丈夫です。"},{"id":{"d":438,"n":5,"t":2},"speaker":"A","text":"何に使うのですか？削らないと書けませんよ。"},{"id":{"d":438,"n":6,"t":2},"speaker":"B","text":"これは今度の図工の授業で、鉛筆をナイフで削る練習をするために買ったのです。"},{"id":{"d":438,"n":7,"t":2},"speaker":"A","text":"そういえば、去年もそのようなことを授業でやりました。"},{"id":{"d":438,"n":8,"t":2},"speaker":"B","text":"普段、ナイフのようなものは使わないので、上手にできるか不安です。うまく削るコツを教えてくれませんか？"}];
INSERT INTO sentence [{"id":{"d":439,"n":1,"t":2},"speaker":"A","text":"ホームルームが終わったら、資料のコピー作業を手伝ってくれませんか？"},{"id":{"d":439,"n":2,"t":2},"speaker":"B","text":"すみません、今日は塾へ行く予定があるので、早く帰らなければいけないのです。"},{"id":{"d":439,"n":3,"t":2},"speaker":"A","text":"30分だけでもお願いできませんか？"},{"id":{"d":439,"n":4,"t":2},"speaker":"B","text":"ホームルームが終わってすぐに帰らないと、間に合わないのです。そういえば、由佳さんが、「今日は部活動が休みだ」と言っていたはずです。由佳さんに頼んでみてはいかがでしょうか？"},{"id":{"d":439,"n":5,"t":2},"speaker":"A","text":"では由佳さんに頼んでみます。由佳さんが、今どこにいるか知っていますか？"},{"id":{"d":439,"n":6,"t":2},"speaker":"B","text":"さっき、先生に呼ばれて職員室へ行きました。次の授業もそろそろ始まるし、もうすぐ帰ってくるはずですよ。"}];
INSERT INTO sentence [{"id":{"d":440,"n":1,"t":2},"speaker":"A","text":"明日は、遠足ですね。"},{"id":{"d":440,"n":2,"t":2},"speaker":"B","text":"はい。お弁当とおやつが楽しみです。"},{"id":{"d":440,"n":3,"t":2},"speaker":"A","text":"おやつは、もう買いに行きましたか？"},{"id":{"d":440,"n":4,"t":2},"speaker":"B","text":"おととい、母と買いに行きました。"},{"id":{"d":440,"n":5,"t":2},"speaker":"A","text":"300円以内で何を買ったのですか？"},{"id":{"d":440,"n":6,"t":2},"speaker":"B","text":"私は、チョコレートと、ポテトスナックを購入しました。あなたは、何を買いましたか？"},{"id":{"d":440,"n":7,"t":2},"speaker":"A","text":"私もチョコレートを買いたかったのですが、いちごのあめと、ビスケットで300円になってしまいました。"},{"id":{"d":440,"n":8,"t":2},"speaker":"B","text":"では、もしよろしければ、チョコレートと、あめを少し交換しましょう。"},{"id":{"d":440,"n":9,"t":2},"speaker":"A","text":"良いのですか？ありがとうございます、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":441,"n":1,"t":2},"speaker":"A","text":"待ちに待った、お弁当の時間です。"},{"id":{"d":441,"n":2,"t":2},"speaker":"B","text":"おなかがすきましたね。"},{"id":{"d":441,"n":3,"t":2},"speaker":"A","text":"はい、もう、ひどく空腹です。いただきます。"},{"id":{"d":441,"n":4,"t":2},"speaker":"B","text":"いただきます。そのおにぎり、すごくおいしそうですね。"},{"id":{"d":441,"n":5,"t":2},"speaker":"A","text":"おいしいです。遠足の時にはいつも母が、具沢山のおにぎりを作ってくれるのです。八木さんのサンドイッチもおいしそうですね。"},{"id":{"d":441,"n":6,"t":2},"speaker":"B","text":"タマゴサンドです。私の大好物です。今日は2つも買ってきました。"}];
INSERT INTO sentence [{"id":{"d":442,"n":1,"t":2},"speaker":"A","text":"今日は、パン屋さんが来る日ですよね？"},{"id":{"d":442,"n":2,"t":2},"speaker":"B","text":"そうでした。お昼休みは、ダッシュでパンを買いに行きましょう。"},{"id":{"d":442,"n":3,"t":2},"speaker":"A","text":"私は焼きそばパンとコロッケパンを狙っています。安藤さんは何にしますか？"},{"id":{"d":442,"n":4,"t":2},"speaker":"B","text":"私はあんバターパンとカレーパンです。チャイムが鳴ったらダッシュしましょう。"},{"id":{"d":442,"n":5,"t":2},"speaker":"A","text":"はい。もう少し教室が近ければ良かったのですが。"},{"id":{"d":442,"n":6,"t":2},"speaker":"B","text":"そうですね。頑張って走りましょう。"}];
INSERT INTO sentence [{"id":{"d":443,"n":1,"t":2},"speaker":"A","text":"来週は、授業参観があります。今週中に出欠席を回答しなければいけません。"},{"id":{"d":443,"n":2,"t":2},"speaker":"B","text":"このプリントの下の部分で、回答すれば良いのですね。"},{"id":{"d":443,"n":3,"t":2},"speaker":"A","text":"はい。授業参観の日、お母さんはお仕事をお休みできますか？"},{"id":{"d":443,"n":4,"t":2},"speaker":"B","text":"来週は忙しくないので、お休みするつもりです。"},{"id":{"d":443,"n":5,"t":2},"speaker":"A","text":"では授業参観には来られるのですね。うれしいです。"},{"id":{"d":443,"n":6,"t":2},"speaker":"B","text":"はい、行こうと思います。参観日には、何の授業をするのですか？"},{"id":{"d":443,"n":7,"t":2},"speaker":"A","text":"国語です。本を読んでお芝居をします。"},{"id":{"d":443,"n":8,"t":2},"speaker":"B","text":"お芝居ですか、面白そうです。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":444,"n":1,"t":2},"speaker":"A","text":"さっき、職員室へ呼ばれていましたね。何かしたのですか？"},{"id":{"d":444,"n":2,"t":2},"speaker":"B","text":"いいえ、悪いことは何もしていません。小森先生に「学校のパンフレットに載せる写真を撮らせてほしい」と言われました。"},{"id":{"d":444,"n":3,"t":2},"speaker":"A","text":"なるほど。それなら納得です。生活指導の小森先生に呼ばれていたので、何かしたのかと思い、驚きました。"},{"id":{"d":444,"n":4,"t":2},"speaker":"B","text":"小森先生は、生活指導だけでなく、今年度の学校のパンフレット作成担当でもあるそうです。"},{"id":{"d":444,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。"}];
INSERT INTO sentence [{"id":{"d":445,"n":1,"t":2},"speaker":"A","text":"おなかが痛いので、保健室に行ってきます。"},{"id":{"d":445,"n":2,"t":2},"speaker":"B","text":"分かりました。担任の森山先生には伝えておきます。"},{"id":{"d":445,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"},{"id":{"d":445,"n":4,"t":2},"speaker":"B","text":"しばらく休んだら、治りそうですか？"},{"id":{"d":445,"n":5,"t":2},"speaker":"A","text":"分かりません。治らなければ、早退しようと思います。"},{"id":{"d":445,"n":6,"t":2},"speaker":"B","text":"早退するときは、お家の方に迎えに来てもらった方がいいですよ。"},{"id":{"d":445,"n":7,"t":2},"speaker":"A","text":"そうですね。その時は、家に電話します。"},{"id":{"d":445,"n":8,"t":2},"speaker":"B","text":"お大事になさってくださいね。"}];
INSERT INTO sentence [{"id":{"d":446,"n":1,"t":2},"speaker":"A","text":"岩城さん、廊下の掲示板にこれを貼るのを手伝ってくれませんか？"},{"id":{"d":446,"n":2,"t":2},"speaker":"B","text":"これは、この間の書き初めの作品ですね？"},{"id":{"d":446,"n":3,"t":2},"speaker":"A","text":"はい。先生が賞を付けてくれたので、金賞から順番に右上から貼っていきましょう。"},{"id":{"d":446,"n":4,"t":2},"speaker":"B","text":"分かりました。金賞は井上さんです。とても美しい字ですね。"},{"id":{"d":446,"n":5,"t":2},"speaker":"A","text":"本当ですね。銀賞は村田君と、平塚さんと、小森さん。"},{"id":{"d":446,"n":6,"t":2},"speaker":"B","text":"3人ともとても上手ですね。入賞作品はここまでしょうか？"},{"id":{"d":446,"n":7,"t":2},"speaker":"A","text":"そのようですね。残りは五十音順に貼っていきましょう。"},{"id":{"d":446,"n":8,"t":2},"speaker":"B","text":"了解です。"}];
INSERT INTO sentence [{"id":{"d":447,"n":1,"t":2},"speaker":"A","text":"学校からの連絡で、来週の防災訓練には、防災頭巾を持ってきてほしいとのことです。週末にホームセンターへ買いに行きましょうね。"},{"id":{"d":447,"n":2,"t":2},"speaker":"B","text":"お母さん、この連絡をよく読んでください。"},{"id":{"d":447,"n":3,"t":2},"speaker":"A","text":"何ですか？"},{"id":{"d":447,"n":4,"t":2},"speaker":"B","text":"ここに、「金曜日に頭巾の使い方を予習するのでこの日までに持ってくること」と書いてあります。"},{"id":{"d":447,"n":5,"t":2},"speaker":"A","text":"見逃していました。では、明日早速買いにいきましょうか？"},{"id":{"d":447,"n":6,"t":2},"speaker":"B","text":"それなら、金曜日に間に合います。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":448,"n":1,"t":2},"speaker":"A","text":"明日は実用英語技能検定の試験日ですね。"},{"id":{"d":448,"n":2,"t":2},"speaker":"B","text":"早起きできるかが、一番心配です。"},{"id":{"d":448,"n":3,"t":2},"speaker":"A","text":"試験は午後ではなかったですか？"},{"id":{"d":448,"n":4,"t":2},"speaker":"B","text":"私は準1級なので、朝早くから試験があります。"},{"id":{"d":448,"n":5,"t":2},"speaker":"A","text":"中学2年なのに、もう準1級ですか？私はこの前4級を受けたばかりでした。"},{"id":{"d":448,"n":6,"t":2},"speaker":"B","text":"小学校から英語の塾に行っていたので、先にいろいろと検定を取っていただけです。勉強すればすぐに追いつきますよ。"}];
INSERT INTO sentence [{"id":{"d":449,"n":1,"t":2},"speaker":"A","text":"今日の体育は、卓球だそうです。"},{"id":{"d":449,"n":2,"t":2},"speaker":"B","text":"体育館に、卓球台を出さなくてはなりませんね。"},{"id":{"d":449,"n":3,"t":2},"speaker":"A","text":"卓球台は、何台出せば良いでしょうか？"},{"id":{"d":449,"n":4,"t":2},"speaker":"B","text":"いつもは、6台だったと思います。"},{"id":{"d":449,"n":5,"t":2},"speaker":"A","text":"体育委員だけでは、手が足りませんね。"},{"id":{"d":449,"n":6,"t":2},"speaker":"B","text":"着替えの終わった人から、手伝ってもらいます。"},{"id":{"d":449,"n":7,"t":2},"speaker":"A","text":"私は、先に体育館に行っています。"},{"id":{"d":449,"n":8,"t":2},"speaker":"B","text":"更衣室に行って、着替えの終わった人から準備を手伝うよう、みんなに伝えてきます。"}];
INSERT INTO sentence [{"id":{"d":450,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題は、あとどれくらいですか？"},{"id":{"d":450,"n":2,"t":2},"speaker":"B","text":"算数のドリルが、あと10ページと自由研究です。あなたはどうですか？"},{"id":{"d":450,"n":3,"t":2},"speaker":"A","text":"私は、工作と絵日記です。"},{"id":{"d":450,"n":4,"t":2},"speaker":"B","text":"あと5日で終わるでしょうか？"},{"id":{"d":450,"n":5,"t":2},"speaker":"A","text":"絵日記は、大丈夫だと思います。"},{"id":{"d":450,"n":6,"t":2},"speaker":"B","text":"私も算数のドリルは、大丈夫だと思います。"},{"id":{"d":450,"n":7,"t":2},"speaker":"A","text":"自由研究は、5日では大変そうですね。"},{"id":{"d":450,"n":8,"t":2},"speaker":"B","text":"テーマだけは、決まっています。"}];
INSERT INTO sentence [{"id":{"d":451,"n":1,"t":2},"speaker":"A","text":"明日は、バスで登校します。"},{"id":{"d":451,"n":2,"t":2},"speaker":"B","text":"自転車通学は、やめるのですか？"},{"id":{"d":451,"n":3,"t":2},"speaker":"A","text":"ネットニュースの天気予報を見たら、明日は春一番が吹くと書かれていたので、安全のためにバスにします。"},{"id":{"d":451,"n":4,"t":2},"speaker":"B","text":"それは賢明な判断です。"},{"id":{"d":451,"n":5,"t":2},"speaker":"A","text":"雨が降ってもかっぱを着て、頑張っていたのですが、風には勝てません。おとなしくバスにします。"},{"id":{"d":451,"n":6,"t":2},"speaker":"B","text":"1時間目が体育の授業なので、ジャージで登校してしまうのもありかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":452,"n":1,"t":2},"speaker":"A","text":"塾の合宿に行ってきました。"},{"id":{"d":452,"n":2,"t":2},"speaker":"B","text":"通っている塾には、合宿があるのですか？"},{"id":{"d":452,"n":3,"t":2},"speaker":"A","text":"毎年ありますよ。ただ、今年は受験生なので、リゾート地でもひたすら勉強をするようなプログラムでした。"},{"id":{"d":452,"n":4,"t":2},"speaker":"B","text":"リゾート地でも勉強漬けの日々だったのですか？成績は上がりそうですか？"},{"id":{"d":452,"n":5,"t":2},"speaker":"A","text":"理解は深まった気がします。今度のテストで、効果があったことを証明できたらうれしいです。"},{"id":{"d":452,"n":6,"t":2},"speaker":"B","text":"それは何よりです。受験まで頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":453,"n":1,"t":2},"speaker":"A","text":"今年度最後のお掃除は、教室のワックスがけをします。職員室からワックスとモップをもらってきてください。"},{"id":{"d":453,"n":2,"t":2},"speaker":"B","text":"僕が行きます。"},{"id":{"d":453,"n":3,"t":2},"speaker":"A","text":"すごく重いので、1人では難しいと思います。もう1人連れて行ってきてください。"},{"id":{"d":453,"n":4,"t":2},"speaker":"B","text":"では、岡本君と一緒に行ってきます。"},{"id":{"d":453,"n":5,"t":2},"speaker":"A","text":"よろしくお願いします。待っている間、教室に残っている人で、机と椅子を廊下に出してしまいましょう。"},{"id":{"d":453,"n":6,"t":2},"speaker":"B","text":"では、職員室へ行ってきます。"}];
INSERT INTO sentence [{"id":{"d":454,"n":1,"t":2},"speaker":"A","text":"神田さんは、どこの大学の出身ですか？"},{"id":{"d":454,"n":2,"t":2},"speaker":"B","text":"私は、東京女子大学を出ました。"},{"id":{"d":454,"n":3,"t":2},"speaker":"A","text":"ああ、アントニン・レーモンドによって設計された建造物群がある大学ですね。"},{"id":{"d":454,"n":4,"t":2},"speaker":"B","text":"よくご存じですね。"},{"id":{"d":454,"n":5,"t":2},"speaker":"A","text":"もちろんです。レーモンドは日本のモダニズム建築の代表的な建築家ですから。"},{"id":{"d":454,"n":6,"t":2},"speaker":"B","text":"私は、大学に入って初めてアントニン・レーモンドと、その建造物を知ったのですが、とても好きになりました。言葉にはできませんが、心に響いてくるものがありました。"},{"id":{"d":454,"n":7,"t":2},"speaker":"A","text":"そうですね、単に合理性だけではないものがありますね。"},{"id":{"d":454,"n":8,"t":2},"speaker":"B","text":"心が落ち着きました。それなりに年月を経たものだからでしょうか？"},{"id":{"d":454,"n":9,"t":2},"speaker":"A","text":"それもあるでしょうが、建物がもともと持っていたものでしょうね。"}];
INSERT INTO sentence [{"id":{"d":455,"n":1,"t":2},"speaker":"A","text":"インターハイへの出場が決まって良かったですね。おめでとうございます。"},{"id":{"d":455,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。皆さんのおかげです。"},{"id":{"d":455,"n":3,"t":2},"speaker":"A","text":"トレーニングは大変だったでしょう。1日に何時間ぐらい練習をするものなのですか？"},{"id":{"d":455,"n":4,"t":2},"speaker":"B","text":"練習時間自体はそれほど長くありません。1日2時間程度です。私たちのチームでは、疲れをためすぎないように、短い時間で集中して練習するという方法をとっています。"},{"id":{"d":455,"n":5,"t":2},"speaker":"A","text":"今はそういう練習法が、一般的なのですか？"},{"id":{"d":455,"n":6,"t":2},"speaker":"B","text":"そういうわけでもありません。今でも猛練習をする学校の方が多いらしいです。"},{"id":{"d":455,"n":7,"t":2},"speaker":"A","text":"コーチの新しい考え方なのですね。「全国高等学校総合体育大会」でも、活躍することを期待しています。"},{"id":{"d":455,"n":8,"t":2},"speaker":"B","text":"どうもありがとうございます。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":456,"n":1,"t":2},"speaker":"A","text":"山田さんが、大学入試センター試験を受けた際の会場はどこでしたか？"},{"id":{"d":456,"n":2,"t":2},"speaker":"B","text":"私は、西区の私立高校でした。"},{"id":{"d":456,"n":3,"t":2},"speaker":"A","text":"家からは、近いのですか？"},{"id":{"d":456,"n":4,"t":2},"speaker":"B","text":"電車で5駅くらいです。アクセスは良さそうですが、これまで行ったことのない場所なので下見をしておいた方が安心かもしれません。井崎さんはどちらで受験されましたか？"},{"id":{"d":456,"n":5,"t":2},"speaker":"A","text":"私は北区にある国立大学でした。家から徒歩圏内の会場なので、とてもラッキーです。"},{"id":{"d":456,"n":6,"t":2},"speaker":"B","text":"それはとても良いですね。遅刻する心配もなさそうです。"}];
INSERT INTO sentence [{"id":{"d":457,"n":1,"t":2},"speaker":"A","text":"今年の文化祭では、何をしましょう？"},{"id":{"d":457,"n":2,"t":2},"speaker":"B","text":"ピザの店を出すのは、どうですか？"},{"id":{"d":457,"n":3,"t":2},"speaker":"A","text":"市販のピザを焼いて出すのですか？"},{"id":{"d":457,"n":4,"t":2},"speaker":"B","text":"それだけではつまらないので、ピザ窯を作るところから始めるのはどうでしょうか？"},{"id":{"d":457,"n":5,"t":2},"speaker":"A","text":"なるほど、それは面白そうですが、それほど簡単にピザ窯は作れるものなのですか？"},{"id":{"d":457,"n":6,"t":2},"speaker":"B","text":"簡易的な物であれば、ホームセンターで売っている物だけでも作れるそうですよ。少し調べてみましょう。"}];
INSERT INTO sentence [{"id":{"d":458,"n":1,"t":2},"speaker":"A","text":"今週末、千代田ビジュアルスクールでオープンキャンパスがあるのですが、一緒に行きませんか？"},{"id":{"d":458,"n":2,"t":2},"speaker":"B","text":"いいですね。どのようなことをするのですか？"},{"id":{"d":458,"n":3,"t":2},"speaker":"A","text":"「フォトグラファーの入り口体験」や、「映画・ドラマ制作のスタッフ体験」というものがあるようです。"},{"id":{"d":458,"n":4,"t":2},"speaker":"B","text":"それは面白そうですね。私はテレビドラマの制作に興味があるので、体験に参加してみたいです。"},{"id":{"d":458,"n":5,"t":2},"speaker":"A","text":"私は写真家になりたいと思っているので、「フォトグラファーの入り口体験」というのに参加するつもりです。"},{"id":{"d":458,"n":6,"t":2},"speaker":"B","text":"良いですね。体験に参加するには、予約をしなければいけないのでしょうか？"},{"id":{"d":458,"n":7,"t":2},"speaker":"A","text":"「事前申し込みが必要」と書かれていました。定員があるそうなので、早く申し込んだ方がいいですね。私の方で2人分予約しておきます。"},{"id":{"d":458,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":459,"n":1,"t":2},"speaker":"A","text":"山田さん、今年のクラス会には出席しますか？"},{"id":{"d":459,"n":2,"t":2},"speaker":"B","text":"これまでは、毎年出席していたのですが、今年は迷っています。"},{"id":{"d":459,"n":3,"t":2},"speaker":"A","text":"どうかしたのですか？"},{"id":{"d":459,"n":4,"t":2},"speaker":"B","text":"毎年、同じような顔ぶれで同じような企画なので、何だかつまらないのです。"},{"id":{"d":459,"n":5,"t":2},"speaker":"A","text":"去年までは、そうだったかもしれませんね。でも、今年は金子さんが幹事で、新しいことを実施するそうですよ。"},{"id":{"d":459,"n":6,"t":2},"speaker":"B","text":"ああ、そうですか。金子さんなら、何か面白いことを企画するかもしれませんね。"},{"id":{"d":459,"n":7,"t":2},"speaker":"A","text":"高校時代から、みんなを楽しませることをいろいろ考え出していましたからね。"},{"id":{"d":459,"n":8,"t":2},"speaker":"B","text":"それなら、出席することにします。"}];
INSERT INTO sentence [{"id":{"d":460,"n":1,"t":2},"speaker":"A","text":"今度転校してきた子のことを、知っていますか？"},{"id":{"d":460,"n":2,"t":2},"speaker":"B","text":"いえ、知りません。何か特別なことでもあるのですか？"},{"id":{"d":460,"n":3,"t":2},"speaker":"A","text":"山梨学園から転校してきたのです。"},{"id":{"d":460,"n":4,"t":2},"speaker":"B","text":"あのサッカー強豪校の山梨学園ですか？"},{"id":{"d":460,"n":5,"t":2},"speaker":"A","text":"そうです。そこでレギュラーの選手だったそうです。"},{"id":{"d":460,"n":6,"t":2},"speaker":"B","text":"ポジションはどこだったのですか？"},{"id":{"d":460,"n":7,"t":2},"speaker":"A","text":"フォワードだったそうです。"},{"id":{"d":460,"n":8,"t":2},"speaker":"B","text":"うちのサッカー部は得点力が弱いから、ちょうどいいですね。"},{"id":{"d":460,"n":9,"t":2},"speaker":"A","text":"そうなのです。サッカー部に入ってくれるよう、私から声を掛けてみましょう。"}];
INSERT INTO sentence [{"id":{"d":461,"n":1,"t":2},"speaker":"A","text":"山下君は、夏休みの宿題に何をするのか決めましたか？"},{"id":{"d":461,"n":2,"t":2},"speaker":"B","text":"それがまだなのですよ。自由研究といっても、何をしたらよいのか分かりません。"},{"id":{"d":461,"n":3,"t":2},"speaker":"A","text":"そうですよね。自由といわれるとかえって困りますよね。"},{"id":{"d":461,"n":4,"t":2},"speaker":"B","text":"木下さんは、何か考えているのですか？"},{"id":{"d":461,"n":5,"t":2},"speaker":"A","text":"私はメダカを飼って、それを観察することにします。"},{"id":{"d":461,"n":6,"t":2},"speaker":"B","text":"面白そうですね。メダカはどうやって手に入れるのですか？このへんにはメダカがいるようなところはなさそうですけれど。"},{"id":{"d":461,"n":7,"t":2},"speaker":"A","text":"山梨の祖父の知り合いに譲ってもらえることになっています。その人の田んぼにはメダカがたくさんいるそうです。"},{"id":{"d":461,"n":8,"t":2},"speaker":"B","text":"オタマジャクシやタニシが田んぼにいるのは知っていましたが、メダカもいるのは知りませんでした。他にはどのような生き物がいるのでしょうか？"},{"id":{"d":461,"n":9,"t":2},"speaker":"A","text":"祖父によるとドジョウなどもいるそうですよ。それこそ自由研究として、田んぼにすむ生き物を調べてみるのはどうですか？"},{"id":{"d":461,"n":10,"t":2},"speaker":"B","text":"なるほど、それは面白そうです。良いアイデアをありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":462,"n":1,"t":2},"speaker":"A","text":"5年に1度の「ショパン国際ピアノコンクール」の年になりましたね。"},{"id":{"d":462,"n":2,"t":2},"speaker":"B","text":"そうですね。ようやく巡ってきました。"},{"id":{"d":462,"n":3,"t":2},"speaker":"A","text":"田中さんも参加するのでしょう？"},{"id":{"d":462,"n":4,"t":2},"speaker":"B","text":"はい、そのつもりです。前回は二次審査で落ちてしまいましたから、今度はファイナルまでいきたいですね。"},{"id":{"d":462,"n":5,"t":2},"speaker":"A","text":"田中さんなら、十分優勝を狙えますよ。"},{"id":{"d":462,"n":6,"t":2},"speaker":"B","text":"私も優勝を目指していますが、何しろ、優れたピアニストが何人もいますから、厳しい戦いになるでしょう。"},{"id":{"d":462,"n":7,"t":2},"speaker":"A","text":"田中さんほどの才能をもったピアニストはそれほどいないと思いますよ。"},{"id":{"d":462,"n":8,"t":2},"speaker":"B","text":"そう言ってくださるのはうれしいですけれど、「ショパン国際ピアノコンクール」の難しさは身に染みて分かっています。"},{"id":{"d":462,"n":9,"t":2},"speaker":"A","text":"良い結果が出ることを、祈っています。"},{"id":{"d":462,"n":10,"t":2},"speaker":"B","text":"ありがとうございます。私も自分の力を十分に出せるように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":463,"n":1,"t":2},"speaker":"A","text":"踏切での運転について疑問があるのですが。"},{"id":{"d":463,"n":2,"t":2},"speaker":"B","text":"はい、どのようなことでしょう。"},{"id":{"d":463,"n":3,"t":2},"speaker":"A","text":"交通法規では、「踏切の手前の停止線で一時停止して安全性を確認して進行する」とありますね。"},{"id":{"d":463,"n":4,"t":2},"speaker":"B","text":"はい、その通りです。それのどこが疑問なのですか？"},{"id":{"d":463,"n":5,"t":2},"speaker":"A","text":"停止線のところで停止して左右を見ても、視界が遮られて見えないのです。"},{"id":{"d":463,"n":6,"t":2},"speaker":"B","text":"確かに、線路沿いに家屋が密集しているところでは見えませんね。"},{"id":{"d":463,"n":7,"t":2},"speaker":"A","text":"はい。線路の中に入れば、左右が見えるかもしれません。しかし、電車が見えないからといって線路の中で一時停車するのはいけません。どうすればよいのですか？"},{"id":{"d":463,"n":8,"t":2},"speaker":"B","text":"そのようなときは、踏切警報機の音と警告灯の点滅、遮断機で判断します。"},{"id":{"d":463,"n":9,"t":2},"speaker":"A","text":"警報機や遮断器が故障していることもあるでしょう？"},{"id":{"d":463,"n":10,"t":2},"speaker":"B","text":"踏切が故障することは、ほとんどありませんから、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":464,"n":1,"t":2},"speaker":"A","text":"あなたの今の成績では、日本大学芸術学部に合格するのは難しいです。"},{"id":{"d":464,"n":2,"t":2},"speaker":"B","text":"分かっています。でも、私はどうしても日本大学芸術学部に入りたいのです。"},{"id":{"d":464,"n":3,"t":2},"speaker":"A","text":"日本大学芸術学部に入って、何を勉強したいのですか？"},{"id":{"d":464,"n":4,"t":2},"speaker":"B","text":"私は映画学科に入って、シナリオについて勉強したいのです。"},{"id":{"d":464,"n":5,"t":2},"speaker":"A","text":"今年に入ってから、数学の成績は伸びていますが、国語の成績がなかなか上がりませんね。"},{"id":{"d":464,"n":6,"t":2},"speaker":"B","text":"国語の成績が悪いということは、私も分かっています。"},{"id":{"d":464,"n":7,"t":2},"speaker":"A","text":"入学試験まであと半年です。もっと勉強しないといけませんね。"},{"id":{"d":464,"n":8,"t":2},"speaker":"B","text":"はい。合格できるよう、もっと勉強します。"}];
INSERT INTO sentence [{"id":{"d":465,"n":1,"t":2},"speaker":"A","text":"アメリカに語学留学をしたいのですが。"},{"id":{"d":465,"n":2,"t":2},"speaker":"B","text":"それは良い考えですね。お父さんも賛成です。それで、いつ、どこに行くつもりですか？"},{"id":{"d":465,"n":3,"t":2},"speaker":"A","text":"夏休みを利用して2カ月ほど、カリフォルニア州に行こうと思っています。"},{"id":{"d":465,"n":4,"t":2},"speaker":"B","text":"どこか学校の当てでもあるのですか？"},{"id":{"d":465,"n":5,"t":2},"speaker":"A","text":"はい、語学留学をサポートする会社があります。そこの紹介で、ユニバーサル語学学校に行くことにしました。"},{"id":{"d":465,"n":6,"t":2},"speaker":"B","text":"学費はどうするのですか？私は援助できませんよ。"},{"id":{"d":465,"n":7,"t":2},"speaker":"A","text":"アルバイトで貯めたお金がありますので、ご心配なく。"},{"id":{"d":465,"n":8,"t":2},"speaker":"B","text":"滞在費も必要でしょう？2カ月分のホテル代や食事代は大丈夫ですか？"},{"id":{"d":465,"n":9,"t":2},"speaker":"A","text":"語学学校に寄宿寮があります。そこなら食事込みで格安で滞在できます。"},{"id":{"d":465,"n":10,"t":2},"speaker":"B","text":"それなら私も安心できます。しっかり勉強してください。"}];
INSERT INTO sentence [{"id":{"d":466,"n":1,"t":2},"speaker":"A","text":"栗本君も追試験を受けるのですか？"},{"id":{"d":466,"n":2,"t":2},"speaker":"B","text":"はい、今度の土曜日と日曜日に受けます。佐伯君も、追試験を受けるのですか？"},{"id":{"d":466,"n":3,"t":2},"speaker":"A","text":"はい、僕も同じ日に追試験を受けます。"},{"id":{"d":466,"n":4,"t":2},"speaker":"B","text":"僕は風邪をひいてテストを受けられなかったのですが、佐伯君はどうしてですか？"},{"id":{"d":466,"n":5,"t":2},"speaker":"A","text":"僕は祖父の葬式と重なってしまい、テストが受けられませんでした。"},{"id":{"d":466,"n":6,"t":2},"speaker":"B","text":"お祖父さんが亡くなったのですか。それはお気の毒です。"},{"id":{"d":466,"n":7,"t":2},"speaker":"A","text":"お心遣いをいただきありがとうございます。追試験では、互いに落第しないように頑張りましょうね。"}];
INSERT INTO sentence [{"id":{"d":467,"n":1,"t":2},"speaker":"A","text":"うちの学校の校庭の隅にはお稲荷さんがあります。"},{"id":{"d":467,"n":2,"t":2},"speaker":"B","text":"お稲荷さんというと、ほこらがあってその中にキツネを祭ってある、あのお稲荷さんですか？"},{"id":{"d":467,"n":3,"t":2},"speaker":"A","text":"そうです、そのお稲荷さんです。"},{"id":{"d":467,"n":4,"t":2},"speaker":"B","text":"珍しいですね。初めて聞きました。何か、いわれでもあるのですか？"},{"id":{"d":467,"n":5,"t":2},"speaker":"A","text":"それが、誰も知らないのです。学校が設立されたときには既にあったそうです。"},{"id":{"d":467,"n":6,"t":2},"speaker":"B","text":"子どもたちは、気味悪がりませんか？"},{"id":{"d":467,"n":7,"t":2},"speaker":"A","text":"いえ、そのようなことはありません。当番を決めて掃除をさせているのですが、別にいやがらずにやっています。"},{"id":{"d":467,"n":8,"t":2},"speaker":"B","text":"それは感心な子どもたちですね。"}];
INSERT INTO sentence [{"id":{"d":468,"n":1,"t":2},"speaker":"A","text":"原田君は、理系の大学に進学するのでしょう？"},{"id":{"d":468,"n":2,"t":2},"speaker":"B","text":"いえ、僕は文系の大学に行くつもりです。"},{"id":{"d":468,"n":3,"t":2},"speaker":"A","text":"原田君は数学が得意だから、理系の大学に行くのだと思っていました。"},{"id":{"d":468,"n":4,"t":2},"speaker":"B","text":"数学は好きですが、だからといって理系の研究をしたいとは思わないのです。"},{"id":{"d":468,"n":5,"t":2},"speaker":"A","text":"何を研究するつもりなのですか？"},{"id":{"d":468,"n":6,"t":2},"speaker":"B","text":"僕はもともと言葉に興味があって、大学に入ったら言語学を勉強したいと思っていたのです。"},{"id":{"d":468,"n":7,"t":2},"speaker":"A","text":"言語学と数学はあまり関係がなそうに思えますが。"},{"id":{"d":468,"n":8,"t":2},"speaker":"B","text":"そうでもないのです。今の言語学には数学の知識も欠かせないのです。"},{"id":{"d":468,"n":9,"t":2},"speaker":"A","text":"それなら原田君の能力が発揮できますね。"}];
INSERT INTO sentence [{"id":{"d":469,"n":1,"t":2},"speaker":"A","text":"文化祭をどうするか、そろそろ決めなければいけませんね。"},{"id":{"d":469,"n":2,"t":2},"speaker":"B","text":"今年も中止するとなると2年連続です。生徒たちも気落ちするでしょう。"},{"id":{"d":469,"n":3,"t":2},"speaker":"A","text":"ええ、生徒たちは、文化祭を目指して一生懸命準備しているでしょうからね。"},{"id":{"d":469,"n":4,"t":2},"speaker":"B","text":"せめて延期ということにはできないでしょうか？あるいは、規模を縮小して行うということはできないでしょうか？"},{"id":{"d":469,"n":5,"t":2},"speaker":"A","text":"それもこれも、感染状況によりますね。"},{"id":{"d":469,"n":6,"t":2},"speaker":"B","text":"このところ少し落ち着いているようですが。"},{"id":{"d":469,"n":7,"t":2},"speaker":"A","text":"しかし、このまま収束するとは思えませんね。"},{"id":{"d":469,"n":8,"t":2},"speaker":"B","text":"新しい変異株が見つかったという報道もあります。"},{"id":{"d":469,"n":9,"t":2},"speaker":"A","text":"政府がどう判断するか、もう少し様子を見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":470,"n":1,"t":2},"speaker":"A","text":"先生、僕は高校ではなく高等専門学校に行こうと思います。"},{"id":{"d":470,"n":2,"t":2},"speaker":"B","text":"進路を変えるということですね。理由を聞いてもよいですか？"},{"id":{"d":470,"n":3,"t":2},"speaker":"A","text":"将来は半導体の分野で仕事をしたいと思っているのですが、高等専門学校に半導体の開発・技術に関する教育課程ができると聞きました。高等専門学校に進学して、早いうちから専門的なことを学びたいのです。"},{"id":{"d":470,"n":4,"t":2},"speaker":"B","text":"なるほど。それはとても良い動機ですね。"},{"id":{"d":470,"n":5,"t":2},"speaker":"A","text":"高等専門学校の受験対策について詳しく知りたいのですが、過去の募集要項や入試問題はありますか？"},{"id":{"d":470,"n":6,"t":2},"speaker":"B","text":"資料室にあると思いますよ。明日までに探しておきますね。"},{"id":{"d":470,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":471,"n":1,"t":2},"speaker":"A","text":"先生、息子は学校でもきちんと勉強していますか？"},{"id":{"d":471,"n":2,"t":2},"speaker":"B","text":"ええ、最近はよく頑張っていますよ。"},{"id":{"d":471,"n":3,"t":2},"speaker":"A","text":"それは良かったです。安心しました。"},{"id":{"d":471,"n":4,"t":2},"speaker":"B","text":"実際、成績も良くなっていますよ。特に英語はとても素晴らしい成績です。"},{"id":{"d":471,"n":5,"t":2},"speaker":"A","text":"以前は、「英語が苦手だ」とよく言っていたのですが、最近は家でも英語の勉強をよくしているようです。"},{"id":{"d":471,"n":6,"t":2},"speaker":"B","text":"そうですか。学校でも英語の授業には積極的に取り組んでいます。この前の面談で寺島君は「いつか留学してみたい」と言っていたので、そのために英語を頑張っているのかもしれませんね。"},{"id":{"d":471,"n":7,"t":2},"speaker":"A","text":"息子が留学に興味があるとは、知りませんでした。何にせよ、勉強をする気になったのは良かったです。"}];
INSERT INTO sentence [{"id":{"d":472,"n":1,"t":2},"speaker":"A","text":"先生のクラスでは、オンライン授業を実施していると聞きました。生徒の反応はいかがですか？"},{"id":{"d":472,"n":2,"t":2},"speaker":"B","text":"はい、先週からオンライン授業をしています。生徒たちは普段と変わらず、しっかり授業を聞いているように見えます。"},{"id":{"d":472,"n":3,"t":2},"speaker":"A","text":"なるほど。何かトラブルなどはありましたか？"},{"id":{"d":472,"n":4,"t":2},"speaker":"B","text":"通信環境の問題で、人によっては途中で音声や映像がずれたり切れたりすることがあるようでした。"},{"id":{"d":472,"n":5,"t":2},"speaker":"A","text":"そのようなトラブルには、どのように対処しましたか？"},{"id":{"d":472,"n":6,"t":2},"speaker":"B","text":"どうしたものかと悩んでいるところです。こればかりは各家庭の問題ですし、こちらからは通信環境をよく保つようにお願いをすることしかできていません。"},{"id":{"d":472,"n":7,"t":2},"speaker":"A","text":"確かに、それは難しい問題ですね。"},{"id":{"d":472,"n":8,"t":2},"speaker":"B","text":"はい。何か良い解決案があれば、ぜひ教えてください。"},{"id":{"d":472,"n":9,"t":2},"speaker":"A","text":"分かりました。考えてみます。"}];
INSERT INTO sentence [{"id":{"d":473,"n":1,"t":2},"speaker":"A","text":"ブラスバンド部が、全国大会出場を決めたそうですね。おめでとうございます。"},{"id":{"d":473,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。今年は部員の数も減ってしまったので全国大会は難しいかと思っていたのですが、何とか出場することができました。とてもうれしいです。"},{"id":{"d":473,"n":3,"t":2},"speaker":"A","text":"みんなとても熱心に練習していましたね。感心して見ていました。"},{"id":{"d":473,"n":4,"t":2},"speaker":"B","text":"はい。生徒たちは休日も自主的に集まって練習していたようです。"},{"id":{"d":473,"n":5,"t":2},"speaker":"A","text":"全国大会の課題曲は「吹奏楽のための『幻想曲』－アルノルト・シェーンベルク讃」だと伺いました。"},{"id":{"d":473,"n":6,"t":2},"speaker":"B","text":"そうです。日本人の作曲家がシェーンベルクをたたえて作曲した作品です。"},{"id":{"d":473,"n":7,"t":2},"speaker":"A","text":"とても美しい曲ですが、演奏の難易度はとても高そうですね。"},{"id":{"d":473,"n":8,"t":2},"speaker":"B","text":"ええ、実際かなり難しいです。なかなかきれいなハーモニーを作れず、今も苦戦しているところです。"},{"id":{"d":473,"n":9,"t":2},"speaker":"A","text":"全国大会までには満足の行く演奏ができるよう、応援しています。"},{"id":{"d":473,"n":10,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":474,"n":1,"t":2},"speaker":"A","text":"今度の授業参観、真理子さんのところは誰が来るのですか？"},{"id":{"d":474,"n":2,"t":2},"speaker":"B","text":"父が来てくれます。"},{"id":{"d":474,"n":3,"t":2},"speaker":"A","text":"私のところも父が来ます、一緒ですね。"},{"id":{"d":474,"n":4,"t":2},"speaker":"B","text":"そうですね。華さんは私の父に会ったことがありますか？"},{"id":{"d":474,"n":5,"t":2},"speaker":"A","text":"多分ないと思います。真理子さんも私の父とは面識がありませんよね？"},{"id":{"d":474,"n":6,"t":2},"speaker":"B","text":"はい、ありません。なので、今度の授業参観で華さんのお父さんに会えるのが楽しみです。"},{"id":{"d":474,"n":7,"t":2},"speaker":"A","text":"私も同じく楽しみです。真理子さんのお父さんはどのような人ですか？"},{"id":{"d":474,"n":8,"t":2},"speaker":"B","text":"とても朗らかで優しい人ですよ。休日には、よくキャンプへ連れて行ってくれます。"},{"id":{"d":474,"n":9,"t":2},"speaker":"A","text":"すてきなお父さんですね。会えるのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":475,"n":1,"t":2},"speaker":"A","text":"今年も、そろそろ水泳の授業が始まりますね。"},{"id":{"d":475,"n":2,"t":2},"speaker":"B","text":"はい。プールで事故が起こらないよう努めなければいけません。"},{"id":{"d":475,"n":3,"t":2},"speaker":"A","text":"特に低学年の児童たちは、プールに入るとつい水遊びに夢中になってしまうので、プールに入る前に注意事項をよく言い聞かせることが大事ですね。"},{"id":{"d":475,"n":4,"t":2},"speaker":"B","text":"そうですね。まずは、プールサイドは滑りやすいので絶対に走らないこと。それから、水の中へは飛び込まずに足からゆっくり入ること。いずれも、守らなければ重大な事故につながる可能性があります。"},{"id":{"d":475,"n":5,"t":2},"speaker":"A","text":"はい。今年も安全に授業ができるよう、私たちも気を引き締めていきましょう。"}];
INSERT INTO sentence [{"id":{"d":476,"n":1,"t":2},"speaker":"A","text":"クラス会はどうでした？楽しかったですか？"},{"id":{"d":476,"n":2,"t":2},"speaker":"B","text":"楽しかったですよ。川端君が欠席したことに、みんな気落ちしていました。"},{"id":{"d":476,"n":3,"t":2},"speaker":"A","text":"出席したかったのですが、その日はどうしても都合がつかなかったのです。"},{"id":{"d":476,"n":4,"t":2},"speaker":"B","text":"高校を卒業してからの最初のクラス会だったので、みんなお互いにどうしているか興味津々というところでした。"},{"id":{"d":476,"n":5,"t":2},"speaker":"A","text":"そうでしょうね。私も気になります。北海道大学に入った遠藤君はどうしていますか？"},{"id":{"d":476,"n":6,"t":2},"speaker":"B","text":"北海道の自然がすっかり気に入ったらしくて、アルバイトでお金をためてはあちこち旅しているらしいです。"},{"id":{"d":476,"n":7,"t":2},"speaker":"A","text":"彼らしいですね。生徒会長だった岸田君はどうですか？"},{"id":{"d":476,"n":8,"t":2},"speaker":"B","text":"彼は東京大学へ行きましたが、変わらず真面目にやっているようでした。官僚を目指しているそうです。"},{"id":{"d":476,"n":9,"t":2},"speaker":"A","text":"おお、すごいですね。陸上部で長距離をやっていた瀬戸君は元気でしたか？"},{"id":{"d":476,"n":10,"t":2},"speaker":"B","text":"元気でしたよ。彼は青山学院大学に入って、今も競技を続けているそうです。"},{"id":{"d":476,"n":11,"t":2},"speaker":"A","text":"青山学院大学ということは、箱根駅伝に出る可能性もあるのでしょうか？"},{"id":{"d":476,"n":12,"t":2},"speaker":"B","text":"そのようです。近いうちにテレビで彼の雄姿が見られるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":477,"n":1,"t":2},"speaker":"A","text":"それでは、テスト用紙を配ります。鉛筆、シャープペンシル、消しゴム、定規以外のものは、机の上からしまってください。上田さん、机の上の分度器はしまってください。"},{"id":{"d":477,"n":2,"t":2},"speaker":"B","text":"先生、定規を忘れてしまったので、代わりに分度器を使ってもよいでしょうか？"},{"id":{"d":477,"n":3,"t":2},"speaker":"A","text":"分度器や三角定規の使用は許されていません。先生の定規を貸しますので、これを使うようにしてください。"},{"id":{"d":477,"n":4,"t":2},"speaker":"B","text":"分かりました。すみません、ありがとうございます。"},{"id":{"d":477,"n":5,"t":2},"speaker":"A","text":"テスト用紙を配ります。1部を取ったら、残りを後ろの人に渡してください。開始時間になるまで、問題用紙は裏向きにして机の上に伏せて置いておいてください。"},{"id":{"d":477,"n":6,"t":2},"speaker":"B","text":"先生。"},{"id":{"d":477,"n":7,"t":2},"speaker":"A","text":"はい、上田さん。どうしましたか？"},{"id":{"d":477,"n":8,"t":2},"speaker":"B","text":"問題用紙が1部余りました。"},{"id":{"d":477,"n":9,"t":2},"speaker":"A","text":"ありがとうございます、今取りに行きます。問題用紙が足りないところはありませんか？"},{"id":{"d":477,"n":10,"t":2},"speaker":"B","text":"窓際の列の人が1部足りないようです。"},{"id":{"d":477,"n":11,"t":2},"speaker":"A","text":"では、この問題用紙は窓際の列の人へ渡します。全員に行き渡りましたね。時間になりましたので、解答を開始してください。このテストの終了時刻は、11時50分です。"}];
INSERT INTO sentence [{"id":{"d":478,"n":1,"t":2},"speaker":"A","text":"あなたの運転は少し、不注意なところがあります。"},{"id":{"d":478,"n":2,"t":2},"speaker":"B","text":"そうですか？気を付けて運転したつもりなのですが。"},{"id":{"d":478,"n":3,"t":2},"speaker":"A","text":"一時停止すべき箇所を、2カ所も無視して走行しました。"},{"id":{"d":478,"n":4,"t":2},"speaker":"B","text":"それは気が付きませんでした。どことどこですか？"},{"id":{"d":478,"n":5,"t":2},"speaker":"A","text":"踏切の手前と、優先道路の手前です。"},{"id":{"d":478,"n":6,"t":2},"speaker":"B","text":"どちらも、止まったつもりだったのですが。"},{"id":{"d":478,"n":7,"t":2},"speaker":"A","text":"あれは「停止した」と言えません。減速しただけです。"},{"id":{"d":478,"n":8,"t":2},"speaker":"B","text":"分かりました。これから気を付けます。"},{"id":{"d":478,"n":9,"t":2},"speaker":"A","text":"それから、スピード違反を1度犯しました。"},{"id":{"d":478,"n":10,"t":2},"speaker":"B","text":"信じられません。速度制限の標識はよく見ていました。"},{"id":{"d":478,"n":11,"t":2},"speaker":"A","text":"「これより時速40キロメートル制限」という意味の道路標識がありましたが、あなたは無視して時速50キロメートルのまま走りました。"},{"id":{"d":478,"n":12,"t":2},"speaker":"B","text":"そのような標識があったのですか？全く気付きませんでした。"}];
INSERT INTO sentence [{"id":{"d":479,"n":1,"t":2},"speaker":"A","text":"安岡君は生徒会長に立候補するそうですね。"},{"id":{"d":479,"n":2,"t":2},"speaker":"B","text":"はい。迷っていたのですが、思い切って立候補することにしました。"},{"id":{"d":479,"n":3,"t":2},"speaker":"A","text":"安岡君が立候補するとは、意外だったので驚きました。なぜ、立候補しようと思ったのですか？"},{"id":{"d":479,"n":4,"t":2},"speaker":"B","text":"学校をもっと楽しいものにしようと思ったからです。"},{"id":{"d":479,"n":5,"t":2},"speaker":"A","text":"具体的には、どのようにしようと思っているのですか？"},{"id":{"d":479,"n":6,"t":2},"speaker":"B","text":"意味もなく、学校生活を窮屈にしている不合理な校則を変えたいと思っています。"},{"id":{"d":479,"n":7,"t":2},"speaker":"A","text":"それは良いかもしれませんね。生徒の中にも、今の校則に不満を感じている者は多いようです。"},{"id":{"d":479,"n":8,"t":2},"speaker":"B","text":"はい。僕の周りでもそういう声をよく聞くので、みんなが過ごしやすいよう、改革しようと思っているのです。"}];
INSERT INTO sentence [{"id":{"d":480,"n":1,"t":2},"speaker":"A","text":"生徒会長への当選、おめでとうございます。やりましたね。"},{"id":{"d":480,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。正直なところ、本当に当選できるとは思っていませんでした。"},{"id":{"d":480,"n":3,"t":2},"speaker":"A","text":"立会演説会で、安岡君は素晴らしいスピーチをしました。"},{"id":{"d":480,"n":4,"t":2},"speaker":"B","text":"そのようなことは、ありません。あらかじめ考えていたことの、半分も言えませんでした。"},{"id":{"d":480,"n":5,"t":2},"speaker":"A","text":"あのスピーチを聞いて、感銘を受けた生徒は多かったと思います。"},{"id":{"d":480,"n":6,"t":2},"speaker":"B","text":"そうでしょうか。そうであればとてもうれしいのですが。"},{"id":{"d":480,"n":7,"t":2},"speaker":"A","text":"安岡君と一緒に、学校をもっと楽しいものにしていきたいです。"},{"id":{"d":480,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。みんなで頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":481,"n":1,"t":2},"speaker":"A","text":"昼食はいつもどうしていますか？"},{"id":{"d":481,"n":2,"t":2},"speaker":"B","text":"学生食堂で食べています。お金に余裕があるときは定食を食べますが、普段はラーメンやカレーライスを食べることが多いです。"},{"id":{"d":481,"n":3,"t":2},"speaker":"A","text":"野菜不足になりませんか？"},{"id":{"d":481,"n":4,"t":2},"speaker":"B","text":"サラダバーで野菜を取るようにしています。1グラム当たり1円と安いので助かっています。"},{"id":{"d":481,"n":5,"t":2},"speaker":"A","text":"良心的な値段ですね。"},{"id":{"d":481,"n":6,"t":2},"speaker":"B","text":"ドレッシングをかけると重くなってしまうので、野菜だけで食べています。"},{"id":{"d":481,"n":7,"t":2},"speaker":"A","text":"そこまでするのですか？"},{"id":{"d":481,"n":8,"t":2},"speaker":"B","text":"アルバイトよりも勉強に時間を使いたいので、なるべく節約しているのです。同じことをしている人は案外多いですよ。"}];
INSERT INTO sentence [{"id":{"d":482,"n":1,"t":2},"speaker":"A","text":"服のボタンが取れそうです。"},{"id":{"d":482,"n":2,"t":2},"speaker":"B","text":"今日は家庭科があるので裁縫セットを持ってきています。使いますか？"},{"id":{"d":482,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。でも、ボタンの付け方が分かりません。木崎君は知っていますか？"},{"id":{"d":482,"n":4,"t":2},"speaker":"B","text":"知っていますよ。去年の家庭科で習いました。"},{"id":{"d":482,"n":5,"t":2},"speaker":"A","text":"僕が前に通っていた学校では、ボタンの付け方を習いませんでした。佐藤君、付け方を教えてくれませんか？"},{"id":{"d":482,"n":6,"t":2},"speaker":"B","text":"よいですよ。ちょうど2つボタンが取れそうなので、1つは僕が付け方を見せますね。それを見た後に、もう1つの方を時田君が付けてみてください。"},{"id":{"d":482,"n":7,"t":2},"speaker":"A","text":"分かりました。ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":483,"n":1,"t":2},"speaker":"A","text":"夏休みに自動車の運転免許を取りました。"},{"id":{"d":483,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。大学の近くの自動車教習所に通ったのですか？"},{"id":{"d":483,"n":3,"t":2},"speaker":"A","text":"いいえ。隣県の合宿自動車教習所に参加しました。その方が短期間で自動車の運転免許を取得できるし安上がりなのです。"},{"id":{"d":483,"n":4,"t":2},"speaker":"B","text":"いいですね。宿舎は快適でしたか？"},{"id":{"d":483,"n":5,"t":2},"speaker":"A","text":"宿舎は利用しませんでした。キャンプ場にテントを張って自炊しました。"},{"id":{"d":483,"n":6,"t":2},"speaker":"B","text":"さすが、ボーイスカウトをしていただけのことはありますね。"},{"id":{"d":483,"n":7,"t":2},"speaker":"A","text":"せっかくの夏休みです。2週間も過ごすのだから自然を満喫しない手はありません。"},{"id":{"d":483,"n":8,"t":2},"speaker":"B","text":"まさに一石二鳥ですね。楽しめたようで何よりです。"}];
INSERT INTO sentence [{"id":{"d":484,"n":1,"t":2},"speaker":"A","text":"どの部活動に入るか、決めましたか？"},{"id":{"d":484,"n":2,"t":2},"speaker":"B","text":"硬式テニス部か、吹奏楽部かで迷っています。"},{"id":{"d":484,"n":3,"t":2},"speaker":"A","text":"どちらも楽しそうですね。中学生の頃は何の部活動に参加をしていたのですか？"},{"id":{"d":484,"n":4,"t":2},"speaker":"B","text":"軟式テニス部に入っていました。高校では、硬式テニスに挑戦したいと考えていたのですが、他の入部希望者は経験者ばかりなので、ついて行けるかが心配です。"},{"id":{"d":484,"n":5,"t":2},"speaker":"A","text":"吹奏楽部に興味を持ったのは、なぜですか？"},{"id":{"d":484,"n":6,"t":2},"speaker":"B","text":"入学式の時の演奏が、素晴らしかったからです。「初心者大歓迎」とも聞きました。"},{"id":{"d":484,"n":7,"t":2},"speaker":"A","text":"実は、私も吹奏楽部に興味があります。今日の放課後、一緒に見学へ行きませんか？"},{"id":{"d":484,"n":8,"t":2},"speaker":"B","text":"そうだったのですね。放課後、音楽室へ行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":485,"n":1,"t":2},"speaker":"A","text":"期末テストは、終わりましたか？"},{"id":{"d":485,"n":2,"t":2},"speaker":"B","text":"あと1つ残っています。明日、化学のテストがあります。"},{"id":{"d":485,"n":3,"t":2},"speaker":"A","text":"テストが終わったら、土曜日にみんなでカラオケに行きませんか？"},{"id":{"d":485,"n":4,"t":2},"speaker":"B","text":"行きたいのは山々なのですが、行けないかもしれません。"},{"id":{"d":485,"n":5,"t":2},"speaker":"A","text":"何か、予定があるのですか？"},{"id":{"d":485,"n":6,"t":2},"speaker":"B","text":"数学のテストが、全然できなかったので、もしかしたら、追試験になるかもしれないのです。"},{"id":{"d":485,"n":7,"t":2},"speaker":"A","text":"それは大変ですね。数学のテストは、いつ返ってくるのですか？"},{"id":{"d":485,"n":8,"t":2},"speaker":"B","text":"明日の午後です。カラオケに行けるかどうかは、明日になったら分かります。"},{"id":{"d":485,"n":9,"t":2},"speaker":"A","text":"では、分かったら教えてください。追試験になっていないことを祈っています。"}];
INSERT INTO sentence [{"id":{"d":486,"n":1,"t":2},"speaker":"A","text":"山口さんの出身校は、どちらですか？"},{"id":{"d":486,"n":2,"t":2},"speaker":"B","text":"この大学のすぐ近くにある、私立高校です。"},{"id":{"d":486,"n":3,"t":2},"speaker":"A","text":"あの川の近くの、とてもきれいな校舎の学校ですね。"},{"id":{"d":486,"n":4,"t":2},"speaker":"B","text":"そうです。私が通っていた頃は、まだ校舎を建て替える前だったので、あのようにきれいではありませんでした。ちょうど私が卒業した年に、建て替えが始まったはずです。"},{"id":{"d":486,"n":5,"t":2},"speaker":"A","text":"そうなのですね。新しい校舎に入ったことはありますか？"},{"id":{"d":486,"n":6,"t":2},"speaker":"B","text":"一度だけあります。去年、OG訪問をしたときに入りましたが、とてもきれいになっていて驚きました。"}];
INSERT INTO sentence [{"id":{"d":487,"n":1,"t":2},"speaker":"A","text":"高校卒業後は、大学に進学するのですか？"},{"id":{"d":487,"n":2,"t":2},"speaker":"B","text":"はい、そのつもりです。ちょうど昨日、受験校の1校の合否発表がありました。"},{"id":{"d":487,"n":3,"t":2},"speaker":"A","text":"すいぶん早いですね。大学まで見に行ったのですか？"},{"id":{"d":487,"n":4,"t":2},"speaker":"B","text":"いいえ、インターネットで確認しました。"},{"id":{"d":487,"n":5,"t":2},"speaker":"A","text":"結果はどうでしたか？"},{"id":{"d":487,"n":6,"t":2},"speaker":"B","text":"残念ながら不合格でした。滑り止めのつもりだったのに落ちてしまい、すっかり自信がなくなりました。"},{"id":{"d":487,"n":7,"t":2},"speaker":"A","text":"そうでしたか。それは残念ですが、本当に行きたい大学に合格すればよいのです。"},{"id":{"d":487,"n":8,"t":2},"speaker":"B","text":"そうですね。気持ちを切り替えて、次の試験に全力で挑みます。"}];
INSERT INTO sentence [{"id":{"d":488,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題は、終わりましたか？"},{"id":{"d":488,"n":2,"t":2},"speaker":"B","text":"絵と工作はすぐにできたのですが、算数のドリルが残っています。"},{"id":{"d":488,"n":3,"t":2},"speaker":"A","text":"健太さんは、図工が得意でしたね。工作はどのような作品を作ったのですか？"},{"id":{"d":488,"n":4,"t":2},"speaker":"B","text":"貯金箱です。"},{"id":{"d":488,"n":5,"t":2},"speaker":"A","text":"貯金箱のコンクールに出品するのですか？"},{"id":{"d":488,"n":6,"t":2},"speaker":"B","text":"そうです。去年は佳作だったので、今年こそは入賞を狙っています。"},{"id":{"d":488,"n":7,"t":2},"speaker":"A","text":"すごい意気込みですね。どのような貯金箱ですか？"},{"id":{"d":488,"n":8,"t":2},"speaker":"B","text":"それは、まだ秘密です。"}];
INSERT INTO sentence [{"id":{"d":489,"n":1,"t":2},"speaker":"A","text":"高校に入ったら、芸術の科目は何を選択しますか？私は音楽か美術かで迷っています。"},{"id":{"d":489,"n":2,"t":2},"speaker":"B","text":"私は美術か、工芸か、書道にするつもりです。"},{"id":{"d":489,"n":3,"t":2},"speaker":"A","text":"つまり音楽以外ということですね。大山さんは音楽が好きなのに、意外です。"},{"id":{"d":489,"n":4,"t":2},"speaker":"B","text":"音楽は好きですが、クラシックはあまり興味がないのです。"},{"id":{"d":489,"n":5,"t":2},"speaker":"A","text":"うわさによると、高校の音楽の教科書にはJポップやロックの曲も載っているそうですよ。"},{"id":{"d":489,"n":6,"t":2},"speaker":"B","text":"それなら、面白そうです。授業の内容をもっと詳しく知りたいです。"},{"id":{"d":489,"n":7,"t":2},"speaker":"A","text":"それなら、鈴木先輩に聞いてみませんか。確か、去年音楽を選択していたはずです。"},{"id":{"d":489,"n":8,"t":2},"speaker":"B","text":"それは、良い考えですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":490,"n":1,"t":2},"speaker":"A","text":"お子さんは、今保育園に通っていますか？"},{"id":{"d":490,"n":2,"t":2},"speaker":"B","text":"はい。今度の4月に小学校に入学します。"},{"id":{"d":490,"n":3,"t":2},"speaker":"A","text":"それは、おめでとうございます。お子さんは、小学校に入ったら放課後はどうされるのですか？"},{"id":{"d":490,"n":4,"t":2},"speaker":"B","text":"夕方まで小学校の敷地内にある学童保育で過ごしてもらう予定です。"},{"id":{"d":490,"n":5,"t":2},"speaker":"A","text":"学校の敷地内にある学童保育なら安心ですね。"},{"id":{"d":490,"n":6,"t":2},"speaker":"B","text":"ええ。それに一般向けの子どもセンターに併設されている学童なので、学童に通っていない友達とも遊ぶこともできます。"},{"id":{"d":490,"n":7,"t":2},"speaker":"A","text":"それはいいですね。帰りはお母さんが迎えに行くのですか？"},{"id":{"d":490,"n":8,"t":2},"speaker":"B","text":"いえ、夫の方が早く帰れそうなので、学童へは夫が迎えに行く予定です。"}];
INSERT INTO sentence [{"id":{"d":491,"n":1,"t":2},"speaker":"A","text":"おばあちゃん宛てに、運動会の招待状を持ってきました。受け取ってください。"},{"id":{"d":491,"n":2,"t":2},"speaker":"B","text":"まあ、ありがとう。由美子ちゃんは何の競技に出るのですか？"},{"id":{"d":491,"n":3,"t":2},"speaker":"A","text":"徒競走と、玉入れと、ダンスです。それから、全校リレーにも出ます。"},{"id":{"d":491,"n":4,"t":2},"speaker":"B","text":"今年も、リレーの選手に選ばれたのですか？すごいですね。"},{"id":{"d":491,"n":5,"t":2},"speaker":"A","text":"はい、毎日張り切って、練習を頑張っています。"},{"id":{"d":491,"n":6,"t":2},"speaker":"B","text":"由美子ちゃんの勇姿を、必ず見に行きますね。私も張り切って、お弁当を作ります。"},{"id":{"d":491,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。お弁当を、楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":492,"n":1,"t":2},"speaker":"A","text":"里香ちゃん、こんにちは。"},{"id":{"d":492,"n":2,"t":2},"speaker":"B","text":"あ、優子お姉さん。こんにちは。どうして小学校に来ているのですか？"},{"id":{"d":492,"n":3,"t":2},"speaker":"A","text":"勉強をしに来ました。"},{"id":{"d":492,"n":4,"t":2},"speaker":"B","text":"大学生なのに、小学校に勉強しに来たのですか？"},{"id":{"d":492,"n":5,"t":2},"speaker":"A","text":"小学校教諭になるための勉強をしに来たのよ。"},{"id":{"d":492,"n":6,"t":2},"speaker":"B","text":"ああ、驚きました。教育実習ですか？"},{"id":{"d":492,"n":7,"t":2},"speaker":"A","text":"そうです。里香ちゃんは何年何組ですか？"},{"id":{"d":492,"n":8,"t":2},"speaker":"B","text":"5年2組です。"},{"id":{"d":492,"n":9,"t":2},"speaker":"A","text":"それなら水曜日に音楽室で会えますね。"},{"id":{"d":492,"n":10,"t":2},"speaker":"B","text":"優子お姉さんは、音楽科の教諭になるのですね。水曜日の授業が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":493,"n":1,"t":2},"speaker":"A","text":"いらっしゃいませ。何か、お探しですか？"},{"id":{"d":493,"n":2,"t":2},"speaker":"B","text":"子どもが保育園で使う鍵盤ハーモニカを探しに来ました。"},{"id":{"d":493,"n":3,"t":2},"speaker":"A","text":"お子さん用には、当店では32鍵のものをお薦めしています。"},{"id":{"d":493,"n":4,"t":2},"speaker":"B","text":"子どもには、大きすぎませんか？"},{"id":{"d":493,"n":5,"t":2},"speaker":"A","text":"27鍵のものもありますが、小学校では32鍵のものを使うことが多いのです。"},{"id":{"d":493,"n":6,"t":2},"speaker":"B","text":"そうですか。それなら32鍵を買っておいた方が良さそうです。"},{"id":{"d":493,"n":7,"t":2},"speaker":"A","text":"はい。また買い換えるのは手間ですから、32鍵をお薦めしています。"},{"id":{"d":493,"n":8,"t":2},"speaker":"B","text":"鍵盤ハーモニカは何年生まで使いますか？"},{"id":{"d":493,"n":9,"t":2},"speaker":"A","text":"授業で使うのは2年生までですが、合奏などで6年生まで使う学校もありますよ。"}];
INSERT INTO sentence [{"id":{"d":494,"n":1,"t":2},"speaker":"A","text":"サッカー部が、全国大会に出場することになりました。"},{"id":{"d":494,"n":2,"t":2},"speaker":"B","text":"それは、うれしいニュースですね。"},{"id":{"d":494,"n":3,"t":2},"speaker":"A","text":"私たち吹奏楽部は、サッカー部の応援に行くので、大張り切りで練習しています。"},{"id":{"d":494,"n":4,"t":2},"speaker":"B","text":"サッカー部の応援では、どのような曲を演奏するのですか？"},{"id":{"d":494,"n":5,"t":2},"speaker":"A","text":"Jリーグの試合で使われている曲が多いですね。"},{"id":{"d":494,"n":6,"t":2},"speaker":"B","text":"野球の応援は攻撃の時に演奏をすることが多いですが、サッカーの場合はどのタイミングで演奏をするのですか？"},{"id":{"d":494,"n":7,"t":2},"speaker":"A","text":"特に、決まったタイミングはありません。そのため、2校同時に演奏をすることもあります。"},{"id":{"d":494,"n":8,"t":2},"speaker":"B","text":"そうなのですね。とてもにぎやかな応援になりそうです。"}];
INSERT INTO sentence [{"id":{"d":495,"n":1,"t":2},"speaker":"A","text":"2年3組の合唱コンクールの出場曲は決まりましたか？"},{"id":{"d":495,"n":2,"t":2},"speaker":"B","text":"それがまだなのです。"},{"id":{"d":495,"n":3,"t":2},"speaker":"A","text":"合唱部の佐藤さんが、たくさん候補を出してくださいましたよね。"},{"id":{"d":495,"n":4,"t":2},"speaker":"B","text":"はい。でも意見が割れていてなかなか決まらないのです。"},{"id":{"d":495,"n":5,"t":2},"speaker":"A","text":"どのような意見が出ているのですか？"},{"id":{"d":495,"n":6,"t":2},"speaker":"B","text":"「難しくても勝てる曲を歌いたい」という意見と、「勝てるかどうかではなく、自分たちらしい曲を歌いたい」という意見に分かれています。"},{"id":{"d":495,"n":7,"t":2},"speaker":"A","text":"それは困りましたね。"},{"id":{"d":495,"n":8,"t":2},"speaker":"B","text":"先生から、何かアドバイスをいただけないでしょうか？"},{"id":{"d":495,"n":9,"t":2},"speaker":"A","text":"そうですね。難しい曲を歌ったからといって勝てるとは限らないので、自分たちの強みを生かした曲を選ぶというのはどうでしょうか？"},{"id":{"d":495,"n":10,"t":2},"speaker":"B","text":"なるほど、それならどちらの意見の人も納得してくれそうです。早速クラスのみんなに提案してみます。"}];
INSERT INTO sentence [{"id":{"d":496,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題は順調ですか？"},{"id":{"d":496,"n":2,"t":2},"speaker":"B","text":"はい、おおむね終わりました。"},{"id":{"d":496,"n":3,"t":2},"speaker":"A","text":"早いですね。あとは、何が残っているのですか？"},{"id":{"d":496,"n":4,"t":2},"speaker":"B","text":"読書感想文です。作文が苦手で、なかなか進みません。"},{"id":{"d":496,"n":5,"t":2},"speaker":"A","text":"読む本は、決めましたか？"},{"id":{"d":496,"n":6,"t":2},"speaker":"B","text":"課題図書の中から、興味を持てそうな本を2冊ほど選んで読んでみました。どちらもすごく面白かったのですが、いざ感想文を書こうとすると、何を書いて良いのか分かりません。"},{"id":{"d":496,"n":7,"t":2},"speaker":"A","text":"なるほど。読んでみて面白かったとのことですが、例えば、こちらの本で特に印象に残ったのはどのような場面でしたか？"},{"id":{"d":496,"n":8,"t":2},"speaker":"B","text":"物語の終盤で、主人公のライバルが、けがを理由に退部を決意するまでの過程が強く印象に残っています。"},{"id":{"d":496,"n":9,"t":2},"speaker":"A","text":"それは、なぜですか？"},{"id":{"d":496,"n":10,"t":2},"speaker":"B","text":"以前、私も彼と似たような境遇を経験したことがあったので、ついつい感情移入してしまいました。結局、私は周囲の支えもあって退部をせずに済んだのですが、彼にもそのとき支えてくれる友人や家族がいれば良かったのかもしれません。"},{"id":{"d":496,"n":11,"t":2},"speaker":"A","text":"とても良い感想です。自分の経験と照らし合わせているのも、とても良いですね。読書感想文には、まさにこういうことを書けば良いのですよ。"},{"id":{"d":496,"n":12,"t":2},"speaker":"B","text":"なるほど。忘れないうちに早速書いてみます。"}];
INSERT INTO sentence [{"id":{"d":497,"n":1,"t":2},"speaker":"A","text":"高校に入ったら、自転車通学になるのですか？"},{"id":{"d":497,"n":2,"t":2},"speaker":"B","text":"そうしたいと思っています。でも6月に講習を受けるまでは、自転車通学の許可が下りません。"},{"id":{"d":497,"n":3,"t":2},"speaker":"A","text":"それまでは、どうするのですか？"},{"id":{"d":497,"n":4,"t":2},"speaker":"B","text":"バスに乗って通います。"},{"id":{"d":497,"n":5,"t":2},"speaker":"A","text":"バスの本数は少ないので、しばらくは大変ですね。"},{"id":{"d":497,"n":6,"t":2},"speaker":"B","text":"そうなのです。一層、トレーニングを兼ねて学校まで走って行こうかとも思っています。"},{"id":{"d":497,"n":7,"t":2},"speaker":"A","text":"お金もかかりませんし、体も鍛えられますし、一石二鳥ですね。"}];
INSERT INTO sentence [{"id":{"d":498,"n":1,"t":2},"speaker":"A","text":"高校の体育祭では、どのようなことをしましたか？"},{"id":{"d":498,"n":2,"t":2},"speaker":"B","text":"私の高校では、クラス対抗の綱引きが一番の目玉競技でした。"},{"id":{"d":498,"n":3,"t":2},"speaker":"A","text":"綱引きは、まさに総力戦という感じがする競技ですから、とても盛り上がりそうですね？"},{"id":{"d":498,"n":4,"t":2},"speaker":"B","text":"はい、とても盛り上がりました。佐伯さんの高校では、どのようなことをしましたか？"},{"id":{"d":498,"n":5,"t":2},"speaker":"A","text":"私の高校の体育祭は、クラス対抗の球技大会でした。"},{"id":{"d":498,"n":6,"t":2},"speaker":"B","text":"球技大会だと、バレーボール部員やバスケットボール部員が活躍しそうですね？"},{"id":{"d":498,"n":7,"t":2},"speaker":"A","text":"そうですね。そこでバレーオール部員はアタック禁止、バスケットボール部員はシュート禁止、というハンディキャップがありました。"},{"id":{"d":498,"n":8,"t":2},"speaker":"B","text":"佐藤さんは、何の競技に出たのですか？"},{"id":{"d":498,"n":9,"t":2},"speaker":"A","text":"私はバレーボールの試合に出場し、セッターとコーチを担当しました。"},{"id":{"d":498,"n":10,"t":2},"speaker":"B","text":"それは、大活躍でしたね。"}];
INSERT INTO sentence [{"id":{"d":499,"n":1,"t":2},"speaker":"A","text":"高校の部活動は、先輩後輩の上下関係が厳しいですか？"},{"id":{"d":499,"n":2,"t":2},"speaker":"B","text":"運動部は、厳しいところが多いかもしれません。"},{"id":{"d":499,"n":3,"t":2},"speaker":"A","text":"あまり厳しくない部活動は、ありますか？"},{"id":{"d":499,"n":4,"t":2},"speaker":"B","text":"美術部や生物部など、文化系の部活動はあまり厳しくないようです。"},{"id":{"d":499,"n":5,"t":2},"speaker":"A","text":"吹奏楽部はどうですか？"},{"id":{"d":499,"n":6,"t":2},"speaker":"B","text":"吹奏楽部はかなり体育会系のように上下関係がはっきりしているようです。練習もハードで、先輩も厳しいと聞いています。"},{"id":{"d":499,"n":7,"t":2},"speaker":"A","text":"そうですか。ところで、鈴木先輩は何部に入っているのですか？"},{"id":{"d":499,"n":8,"t":2},"speaker":"B","text":"私は卓球部です。適度な上下関係はありますが、運動部の中では、あまり厳しくない方だと思います。"}];
INSERT INTO sentence [{"id":{"d":500,"n":1,"t":2},"speaker":"A","text":"今日は、今期最後の体育の水泳の授業でした。"},{"id":{"d":500,"n":2,"t":2},"speaker":"B","text":"いよいよ夏も終わりですね。それにしてもプールバッグが重そうです。"},{"id":{"d":500,"n":3,"t":2},"speaker":"A","text":"今日は着衣水泳をしたので、ぬれた服も入っています。"},{"id":{"d":500,"n":4,"t":2},"speaker":"B","text":"服を着たまま泳ぐ練習をしたのですね。"},{"id":{"d":500,"n":5,"t":2},"speaker":"A","text":"はい。服を着て靴を履いていると、想像以上に体が動かしにくいことが分かりました。"},{"id":{"d":500,"n":6,"t":2},"speaker":"B","text":"それは良い経験をしましたね。"},{"id":{"d":500,"n":7,"t":2},"speaker":"A","text":"ペットボトルを使って浮く練習もしました。ペットボトルを抱えて、ラッコのように背浮きをするのです。"},{"id":{"d":500,"n":8,"t":2},"speaker":"B","text":"いざというときに役立つ、良い練習をしましたね。難しかったですか？"},{"id":{"d":500,"n":9,"t":2},"speaker":"A","text":"初めは難しかったですが、だんだんコツが分かってくると、簡単に浮くことができて楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":501,"n":1,"t":2},"speaker":"A","text":"大学では何を専攻していましたか？"},{"id":{"d":501,"n":2,"t":2},"speaker":"B","text":"経営学です。"},{"id":{"d":501,"n":3,"t":2},"speaker":"A","text":"どうして経営学を専攻しようと思ったのですか？"},{"id":{"d":501,"n":4,"t":2},"speaker":"B","text":"企業の会計に興味があって、学んでみようと思ったのです。"},{"id":{"d":501,"n":5,"t":2},"speaker":"A","text":"企業の会計というと、複雑で難しそうですね。"},{"id":{"d":501,"n":6,"t":2},"speaker":"B","text":"幅広い知識が必要なので大変な面もありますが、興味のある分野なので苦になりませんでした。"},{"id":{"d":501,"n":7,"t":2},"speaker":"A","text":"大学で学んだことは就職に生かせましたか？"},{"id":{"d":501,"n":8,"t":2},"speaker":"B","text":"そうですね、会計士の資格を取ったことが、現在の仕事に繋がっています。"}];
INSERT INTO sentence [{"id":{"d":502,"n":1,"t":2},"speaker":"A","text":"今日は3時間目の授業が終わったら早退します。"},{"id":{"d":502,"n":2,"t":2},"speaker":"B","text":"何か予定があるのですか？"},{"id":{"d":502,"n":3,"t":2},"speaker":"A","text":"法事があって、祖父の家に行くのです。"},{"id":{"d":502,"n":4,"t":2},"speaker":"B","text":"そうですか。それでは、5時間目の理科の実験は参加できませんね。"},{"id":{"d":502,"n":5,"t":2},"speaker":"A","text":"そうなのです。先生が重要な実験だと仰っていたので、参加したかったのですが。"},{"id":{"d":502,"n":6,"t":2},"speaker":"B","text":"明日にでも、実験結果をまとめたレポートを貸しましょうか？"},{"id":{"d":502,"n":7,"t":2},"speaker":"A","text":"それは助かります。どうもありがとうございます。"},{"id":{"d":502,"n":8,"t":2},"speaker":"B","text":"いえいえ、お安い御用ですよ。"}];
INSERT INTO sentence [{"id":{"d":503,"n":1,"t":2},"speaker":"A","text":"中間テストまであと1週間ですね。"},{"id":{"d":503,"n":2,"t":2},"speaker":"B","text":"テスト勉強ははかどっていますか？"},{"id":{"d":503,"n":3,"t":2},"speaker":"A","text":"帰宅するとテレビを見たりゲームをしてしまったりして、なかなか勉強が進みません。"},{"id":{"d":503,"n":4,"t":2},"speaker":"B","text":"私も同じです。それに、いざ机に向かっても、すぐに眠くなってしまいます。"},{"id":{"d":503,"n":5,"t":2},"speaker":"A","text":"今回はテスト範囲が広いので、しっかり準備しないといけないと思っているのですが、なかなか実行に移せません。"},{"id":{"d":503,"n":6,"t":2},"speaker":"B","text":"今日の放課後、一緒に図書館で勉強をしませんか？"},{"id":{"d":503,"n":7,"t":2},"speaker":"A","text":"それは良いですね。図書館なら、テレビやゲームという誘惑もありませんし集中できるかもしれません。"},{"id":{"d":503,"n":8,"t":2},"speaker":"B","text":"では、5時間目が終わり次第、図書館の入口で待ち合わせましょう。"},{"id":{"d":503,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":504,"n":1,"t":2},"speaker":"A","text":"来週、このクラスで教育実習生が授業をすることになりました。"},{"id":{"d":504,"n":2,"t":2},"speaker":"B","text":"その方は、どのような方ですか？"},{"id":{"d":504,"n":3,"t":2},"speaker":"A","text":"この学校の卒業生で、社会科が専門です。"},{"id":{"d":504,"n":4,"t":2},"speaker":"B","text":"それでは、このクラスでも社会科の授業をしてくださるのですか？"},{"id":{"d":504,"n":5,"t":2},"speaker":"A","text":"はい。そして授業の他にも朝の会や帰りの会の司会をお願いするつもりです。"},{"id":{"d":504,"n":6,"t":2},"speaker":"B","text":"教育実習はどのくらいの期間ですか？"},{"id":{"d":504,"n":7,"t":2},"speaker":"A","text":"2週間です。その間、学級委員には教育実習生が分からないことを教えてあげるなど、いろいろと協力してあげてほしいのですが、良いですか？"},{"id":{"d":504,"n":8,"t":2},"speaker":"B","text":"はい。分かりました。"}];
INSERT INTO sentence [{"id":{"d":505,"n":1,"t":2},"speaker":"A","text":"友美先生、おはようございます。"},{"id":{"d":505,"n":2,"t":2},"speaker":"B","text":"祐樹君のママ、おはようございます。"},{"id":{"d":505,"n":3,"t":2},"speaker":"A","text":"実は祐樹が今朝、少し元気がない様子なのです。"},{"id":{"d":505,"n":4,"t":2},"speaker":"B","text":"それは心配ですね。熱はありませんでしたか？"},{"id":{"d":505,"n":5,"t":2},"speaker":"A","text":"平熱でした。大丈夫だと思うのですが、もし具合が悪いようでしたら迎えに来るので連絡を下さい。"},{"id":{"d":505,"n":6,"t":2},"speaker":"B","text":"連絡する際は、電話とメールどちらがいいですか？"},{"id":{"d":505,"n":7,"t":2},"speaker":"A","text":"今日は会議があるので、メールにしていただきたいです。"},{"id":{"d":505,"n":8,"t":2},"speaker":"B","text":"分かりました。祐樹君の様子を、気を付けて見ておきますね。"},{"id":{"d":505,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":506,"n":1,"t":2},"speaker":"A","text":"お母さん、今日学校で給食のおかずを制服にこぼしてしまいました。"},{"id":{"d":506,"n":2,"t":2},"speaker":"B","text":"どのくらいの汚れですか？制服を見せてください。"},{"id":{"d":506,"n":3,"t":2},"speaker":"A","text":"これです。すぐに拭いたのですが、少しシミになっています。"},{"id":{"d":506,"n":4,"t":2},"speaker":"B","text":"何をこぼしてしまったのですか？"},{"id":{"d":506,"n":5,"t":2},"speaker":"A","text":"カレーです。"},{"id":{"d":506,"n":6,"t":2},"speaker":"B","text":"シミ抜きをしてみますね。このくらいなら、きれいになると思いますよ。"},{"id":{"d":506,"n":7,"t":2},"speaker":"A","text":"クリーニング店に持ち込みしなくて大丈夫ですか？"},{"id":{"d":506,"n":8,"t":2},"speaker":"B","text":"はい。家にある洗剤で落ちると思います。もしどうしても落ちなかったらクリーニング店に持ち込みましょう。"}];
INSERT INTO sentence [{"id":{"d":507,"n":1,"t":2},"speaker":"A","text":"2時間目は何の授業でしたか？"},{"id":{"d":507,"n":2,"t":2},"speaker":"B","text":"体育の授業でした。今日はソフトボールの試合をしました。"},{"id":{"d":507,"n":3,"t":2},"speaker":"A","text":"結果はどうでしたか？"},{"id":{"d":507,"n":4,"t":2},"speaker":"B","text":"私のチームは負けてしまいました。"},{"id":{"d":507,"n":5,"t":2},"speaker":"A","text":"残念でしたね。"},{"id":{"d":507,"n":6,"t":2},"speaker":"B","text":"相手チームのピッチャーの球が速くて、私はなかなか打てませんでした。"},{"id":{"d":507,"n":7,"t":2},"speaker":"A","text":"次は勝てると良いですね。"},{"id":{"d":507,"n":8,"t":2},"speaker":"B","text":"はい。家で素振りをして練習しようと思っています。"}];
INSERT INTO sentence [{"id":{"d":508,"n":1,"t":2},"speaker":"A","text":"岡田先生、少しよろしいですか？"},{"id":{"d":508,"n":2,"t":2},"speaker":"B","text":"はい、何でしょうか。"},{"id":{"d":508,"n":3,"t":2},"speaker":"A","text":"2年生に毎週漢字テストをしようと思うのですが、どう思いますか？"},{"id":{"d":508,"n":4,"t":2},"speaker":"B","text":"生徒のためには良いと思います。でも、準備が大変そうですね。私も問題作りのお手伝いをしましょうか？"},{"id":{"d":508,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。私と1回ずつ交代で問題作りをお願いしたいです。"},{"id":{"d":508,"n":6,"t":2},"speaker":"B","text":"問題数はどのくらいにしますか？"},{"id":{"d":508,"n":7,"t":2},"speaker":"A","text":"漢字の読みと書き取りをそれぞれ10問ずつにしようと思います。"},{"id":{"d":508,"n":8,"t":2},"speaker":"B","text":"分かりました。早速来週から始めましょう。"}];
INSERT INTO sentence [{"id":{"d":509,"n":1,"t":2},"speaker":"A","text":"今日、5時間目が自習でした。"},{"id":{"d":509,"n":2,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":509,"n":3,"t":2},"speaker":"A","text":"村田先生が出張でいらっしゃらなかったのです。"},{"id":{"d":509,"n":4,"t":2},"speaker":"B","text":"自習時間は何をしましたか？"},{"id":{"d":509,"n":5,"t":2},"speaker":"A","text":"村田先生が用意しておいてくれたプリントの問題を解きました。"},{"id":{"d":509,"n":6,"t":2},"speaker":"B","text":"プリントの問題は難しかったですか？"},{"id":{"d":509,"n":7,"t":2},"speaker":"A","text":"難しくはなかったのですが、問題数が多くて最後まで終わりませんでした。"},{"id":{"d":509,"n":8,"t":2},"speaker":"B","text":"終わらなかった問題は宿題ですか？"},{"id":{"d":509,"n":9,"t":2},"speaker":"A","text":"はい。明日提出します。"}];
INSERT INTO sentence [{"id":{"d":510,"n":1,"t":2},"speaker":"A","text":"ママ、今日のお弁当のおかずは何ですか？"},{"id":{"d":510,"n":2,"t":2},"speaker":"B","text":"コロッケと卵焼きです。"},{"id":{"d":510,"n":3,"t":2},"speaker":"A","text":"うれしい！どちらも私の好物です。"},{"id":{"d":510,"n":4,"t":2},"speaker":"B","text":"それは良かったです。デザートにミカンを持って行きますか？"},{"id":{"d":510,"n":5,"t":2},"speaker":"A","text":"はい。ミカンも大好きなのでうれしいです。"},{"id":{"d":510,"n":6,"t":2},"speaker":"B","text":"残さずに食べてきてくださいね。"},{"id":{"d":510,"n":7,"t":2},"speaker":"A","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":511,"n":1,"t":2},"speaker":"A","text":"先生、今日は雨ですが部活はどうなりますか？"},{"id":{"d":511,"n":2,"t":2},"speaker":"B","text":"雨でグラウンドが使えないので、体育館でトレーニングをします。"},{"id":{"d":511,"n":3,"t":2},"speaker":"A","text":"それでは体育館に集合しますか？"},{"id":{"d":511,"n":4,"t":2},"speaker":"B","text":"そうですね。部員にそう伝えてください。"},{"id":{"d":511,"n":5,"t":2},"speaker":"A","text":"分かりました。集合時間は何時にしますか？"},{"id":{"d":511,"n":6,"t":2},"speaker":"B","text":"6時間目が終わった後にしましょう。4時集合でいかがですか？"},{"id":{"d":511,"n":7,"t":2},"speaker":"A","text":"問題はありません。4時に体育館に集合するよう部員に伝えます。"},{"id":{"d":511,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":512,"n":1,"t":2},"speaker":"A","text":"今日は家庭科の授業で調理実習を行いました。"},{"id":{"d":512,"n":2,"t":2},"speaker":"B","text":"何を作ったのですか？"},{"id":{"d":512,"n":3,"t":2},"speaker":"A","text":"ハンバーグとコーンスープです。"},{"id":{"d":512,"n":4,"t":2},"speaker":"B","text":"作ってみて、どうでしたか？"},{"id":{"d":512,"n":5,"t":2},"speaker":"A","text":"ハンバーグが少し焦げてしまいました。"},{"id":{"d":512,"n":6,"t":2},"speaker":"B","text":"火が強かったのかもしれませんね。"},{"id":{"d":512,"n":7,"t":2},"speaker":"A","text":"初めて作ったので、火加減が難しかったです。"},{"id":{"d":512,"n":8,"t":2},"speaker":"B","text":"今度、家で一緒に作ってみましょう。"},{"id":{"d":512,"n":9,"t":2},"speaker":"A","text":"はい。今度は上手に作りたいです。"}];
INSERT INTO sentence [{"id":{"d":513,"n":1,"t":2},"speaker":"A","text":"あなたは、クラス替えの発表を見ましたか？"},{"id":{"d":513,"n":2,"t":2},"speaker":"B","text":"はい。私は3組でした。"},{"id":{"d":513,"n":3,"t":2},"speaker":"A","text":"私は2組です。別のクラスになってしまいましたね。"},{"id":{"d":513,"n":4,"t":2},"speaker":"B","text":"同じクラスになりたかったので、残念です。"},{"id":{"d":513,"n":5,"t":2},"speaker":"A","text":"私も残念です。3組の担任の先生はどなたですか？"},{"id":{"d":513,"n":6,"t":2},"speaker":"B","text":"山川先生です。"},{"id":{"d":513,"n":7,"t":2},"speaker":"A","text":"山川先生は、人気のある先生のようですよ。"},{"id":{"d":513,"n":8,"t":2},"speaker":"B","text":"そうなのですか。安心しました。"},{"id":{"d":513,"n":9,"t":2},"speaker":"A","text":"お互いに、新しいクラスに早くなじめると良いですね。"}];
INSERT INTO sentence [{"id":{"d":514,"n":1,"t":2},"speaker":"A","text":"浜田先生、5組の桜井さんは今日も欠席ですか？"},{"id":{"d":514,"n":2,"t":2},"speaker":"B","text":"はい。お母さまから、欠席の連絡がありました。"},{"id":{"d":514,"n":3,"t":2},"speaker":"A","text":"確か、月曜日から欠席していますよね。"},{"id":{"d":514,"n":4,"t":2},"speaker":"B","text":"そうなのです。風邪がなかなか治らないそうです。"},{"id":{"d":514,"n":5,"t":2},"speaker":"A","text":"体調も心配ですが、授業がどんどん進んでしまいますね。"},{"id":{"d":514,"n":6,"t":2},"speaker":"B","text":"出席できるようになったら、補習などをしようと思っています。"},{"id":{"d":514,"n":7,"t":2},"speaker":"A","text":"私も協力しますので、一緒にサポートしていきましょう。"},{"id":{"d":514,"n":8,"t":2},"speaker":"B","text":"心強いです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":515,"n":1,"t":2},"speaker":"A","text":"あなたの家のお子さんは、来年から幼稚園に行くのですか？"},{"id":{"d":515,"n":2,"t":2},"speaker":"B","text":"はい、そうです。"},{"id":{"d":515,"n":3,"t":2},"speaker":"A","text":"どの幼稚園にするか、決まりましたか？"},{"id":{"d":515,"n":4,"t":2},"speaker":"B","text":"いろいろと考えているのですが、まだ決まっていません。"},{"id":{"d":515,"n":5,"t":2},"speaker":"A","text":"この地域はたくさん幼稚園があるから、迷いますよね。"},{"id":{"d":515,"n":6,"t":2},"speaker":"B","text":"あなたの息子さんはどの幼稚園に入園しましたか？"},{"id":{"d":515,"n":7,"t":2},"speaker":"A","text":"わが家の息子は保育園に入園したのです。参考にならなくてごめんなさいね。"},{"id":{"d":515,"n":8,"t":2},"speaker":"B","text":"いえいえ、とんでもありません。"},{"id":{"d":515,"n":9,"t":2},"speaker":"A","text":"見学などしてみて、お子さんに合った幼稚園が見つかると良いですね。"},{"id":{"d":515,"n":10,"t":2},"speaker":"B","text":"はい。見学できるかどうか、調べてみます。"}];
INSERT INTO sentence [{"id":{"d":516,"n":1,"t":2},"speaker":"A","text":"7月上旬に、学校でプール開きがあります。"},{"id":{"d":516,"n":2,"t":2},"speaker":"B","text":"もうそのような時期なのですね。"},{"id":{"d":516,"n":3,"t":2},"speaker":"A","text":"その前に、プールの掃除をすることになりました。"},{"id":{"d":516,"n":4,"t":2},"speaker":"B","text":"しっかり掃除して、きれいなプールで泳ぎたいですね。"},{"id":{"d":516,"n":5,"t":2},"speaker":"A","text":"「かなり汚れている」と先生が仰っていたので、きれいになるまで時間がかかりそうです。"},{"id":{"d":516,"n":6,"t":2},"speaker":"B","text":"プールは大きいから、掃除も大変ですよね。"},{"id":{"d":516,"n":7,"t":2},"speaker":"A","text":"はい。6年生全員で掃除します。"},{"id":{"d":516,"n":8,"t":2},"speaker":"B","text":"下級生のためにも、頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":517,"n":1,"t":2},"speaker":"A","text":"夕方のテレビ番組のニュースで、今日は公立高校の入試が行われたと報じられていました。"},{"id":{"d":517,"n":2,"t":2},"speaker":"B","text":"自分が高校受験した時のことを思い出しますね。"},{"id":{"d":517,"n":3,"t":2},"speaker":"A","text":"私は中高一貫校だったので、高校受験は経験していません。"},{"id":{"d":517,"n":4,"t":2},"speaker":"B","text":"そうでしたか。中学校を受験したのですね。"},{"id":{"d":517,"n":5,"t":2},"speaker":"A","text":"はい。小学校高学年の頃は勉強ばかりしていた記憶があります。"},{"id":{"d":517,"n":6,"t":2},"speaker":"B","text":"塾に通っていたのですか？"},{"id":{"d":517,"n":7,"t":2},"speaker":"A","text":"4年生から塾に通っていました。宿題が多くて大変でした。"},{"id":{"d":517,"n":8,"t":2},"speaker":"B","text":"無事に中高一貫校に合格できて良かったですね。"},{"id":{"d":517,"n":9,"t":2},"speaker":"A","text":"努力が報われて、うれしかったです。"}];
INSERT INTO sentence [{"id":{"d":518,"n":1,"t":2},"speaker":"A","text":"このクラスでおそろいのTシャツを作りませんか？"},{"id":{"d":518,"n":2,"t":2},"speaker":"B","text":"良いアイデアですね。"},{"id":{"d":518,"n":3,"t":2},"speaker":"A","text":"早速、Tシャツのデザインを考えましょう。"},{"id":{"d":518,"n":4,"t":2},"speaker":"B","text":"クラスの全員で相談する必要がありますね。"},{"id":{"d":518,"n":5,"t":2},"speaker":"A","text":"明日のホームルームの時間に、みんなで相談するのはどうですか？"},{"id":{"d":518,"n":6,"t":2},"speaker":"B","text":"良いですね。それでは、ホームルームの議題にすることについて、担任の先生に報告しましょう。"},{"id":{"d":518,"n":7,"t":2},"speaker":"A","text":"後で、私から担任の先生に伝えておきます。"},{"id":{"d":518,"n":8,"t":2},"speaker":"B","text":"お願いします。格好良いTシャツを作りたいですね。"}];
INSERT INTO sentence [{"id":{"d":519,"n":1,"t":2},"speaker":"A","text":"父から、健吾の入学祝いをもらいました。"},{"id":{"d":519,"n":2,"t":2},"speaker":"B","text":"気を使っていただいて、ありがたいですね。"},{"id":{"d":519,"n":3,"t":2},"speaker":"A","text":"健吾への伝言も預かりました。"},{"id":{"d":519,"n":4,"t":2},"speaker":"B","text":"どのような伝言ですか？"},{"id":{"d":519,"n":5,"t":2},"speaker":"A","text":"「小学校へ入学したら、友達をたくさんつくって楽しく過ごしてください」、とのことでした。"},{"id":{"d":519,"n":6,"t":2},"speaker":"B","text":"すてきなメッセージですね。後で健吾へ伝えましょう。"},{"id":{"d":519,"n":7,"t":2},"speaker":"A","text":"父が「健吾がランドセルを背負った姿を見たい」と言っていました。"},{"id":{"d":519,"n":8,"t":2},"speaker":"B","text":"分かりました。入学式で写真を撮って、それを送ってあげましょう。"}];
INSERT INTO sentence [{"id":{"d":520,"n":1,"t":2},"speaker":"A","text":"私の中学校は、生徒が千人以上います。"},{"id":{"d":520,"n":2,"t":2},"speaker":"B","text":"多いですね。1学年に何クラスあるのですか？"},{"id":{"d":520,"n":3,"t":2},"speaker":"A","text":"11クラスずつあります。多いでしょうか？"},{"id":{"d":520,"n":4,"t":2},"speaker":"B","text":"僕の中学校は1学年に4クラスしかありません。"},{"id":{"d":520,"n":5,"t":2},"speaker":"A","text":"全校生徒は何人ですか？"},{"id":{"d":520,"n":6,"t":2},"speaker":"B","text":"400人以下です。"},{"id":{"d":520,"n":7,"t":2},"speaker":"A","text":"生徒が少ないと、みんなすぐに知り合って仲良くなれそうですね。"},{"id":{"d":520,"n":8,"t":2},"speaker":"B","text":"はい。学年が違っても仲良くなりやすいです。"}];
INSERT INTO sentence [{"id":{"d":521,"n":1,"t":2},"speaker":"A","text":"あなたは、スペイン語が上手ですね。"},{"id":{"d":521,"n":2,"t":2},"speaker":"B","text":"そのようなことはありませんよ。日常会話をいくらか話せるくらいです。"},{"id":{"d":521,"n":3,"t":2},"speaker":"A","text":"十分素晴らしいことです。どのようにして勉強したのですか？"},{"id":{"d":521,"n":4,"t":2},"speaker":"B","text":"語学学校へ通って勉強しました。"},{"id":{"d":521,"n":5,"t":2},"speaker":"A","text":"てっきり、留学の経験があるのかと思いました。"},{"id":{"d":521,"n":6,"t":2},"speaker":"B","text":"留学は費用が高いので諦めて、日本でスペイン語の語学学校に通ったのです。"},{"id":{"d":521,"n":7,"t":2},"speaker":"A","text":"何年間くらい通っていたのですか？"},{"id":{"d":521,"n":8,"t":2},"speaker":"B","text":"大学生の頃、3年くらい通いました。"}];
INSERT INTO sentence [{"id":{"d":522,"n":1,"t":2},"speaker":"A","text":"最近、学校はどうですか？"},{"id":{"d":522,"n":2,"t":2},"speaker":"B","text":"生徒会の役員選挙に向けて、立候補や推薦が始まりました。"},{"id":{"d":522,"n":3,"t":2},"speaker":"A","text":"あなたは生徒会の役員に立候補しますか？"},{"id":{"d":522,"n":4,"t":2},"speaker":"B","text":"どうしようか迷っています。"},{"id":{"d":522,"n":5,"t":2},"speaker":"A","text":"もし立候補するなら、どの役職にしますか？"},{"id":{"d":522,"n":6,"t":2},"speaker":"B","text":"生徒会長です。"},{"id":{"d":522,"n":7,"t":2},"speaker":"A","text":"素晴らしい。せっかくの機会なので、挑戦してみてはどうですか？"},{"id":{"d":522,"n":8,"t":2},"speaker":"B","text":"そうですね。では明日、立候補の届けを出すことにします。"}];
INSERT INTO sentence [{"id":{"d":523,"n":1,"t":2},"speaker":"A","text":"ママ、先週行われた漢字テストが返却されました。"},{"id":{"d":523,"n":2,"t":2},"speaker":"B","text":"何点でしたか？"},{"id":{"d":523,"n":3,"t":2},"speaker":"A","text":"44点です。"},{"id":{"d":523,"n":4,"t":2},"speaker":"B","text":"半分以上間違えてしまったのですか？"},{"id":{"d":523,"n":5,"t":2},"speaker":"A","text":"言い忘れました。50点満点です。"},{"id":{"d":523,"n":6,"t":2},"speaker":"B","text":"それなら、良い点数ですね。"},{"id":{"d":523,"n":7,"t":2},"speaker":"A","text":"でも、私は満点を狙っていたので悔しいです。"},{"id":{"d":523,"n":8,"t":2},"speaker":"B","text":"では、次回こそ満点を取れるように頑張りましょう。応援していますよ。"}];
INSERT INTO sentence [{"id":{"d":524,"n":1,"t":2},"speaker":"A","text":"学生食堂でお昼の食事を取るのは初めてです。"},{"id":{"d":524,"n":2,"t":2},"speaker":"B","text":"何を食べるか決めましたか？"},{"id":{"d":524,"n":3,"t":2},"speaker":"A","text":"まだ、何を食べるか決めていません。メニューはどこにありますか？"},{"id":{"d":524,"n":4,"t":2},"speaker":"B","text":"あそこの壁に書いてあますよ。"},{"id":{"d":524,"n":5,"t":2},"speaker":"A","text":"たくさんの種類がありますね。どれもおいしそうで、迷ってしまいます。"},{"id":{"d":524,"n":6,"t":2},"speaker":"B","text":"私のお薦めはコロッケ定食ですよ。"},{"id":{"d":524,"n":7,"t":2},"speaker":"A","text":"それでは今日はコロッケ定食にします。"},{"id":{"d":524,"n":8,"t":2},"speaker":"B","text":"決まりましたね。では、向こうで食券を買いましょう。"}];
INSERT INTO sentence [{"id":{"d":525,"n":1,"t":2},"speaker":"A","text":"将来は学校の先生になりたいです。"},{"id":{"d":525,"n":2,"t":2},"speaker":"B","text":"小学校の先生ですか？"},{"id":{"d":525,"n":3,"t":2},"speaker":"A","text":"いえ、高校の先生になりたいです。"},{"id":{"d":525,"n":4,"t":2},"speaker":"B","text":"どうしてそう思うようになったのですか？"},{"id":{"d":525,"n":5,"t":2},"speaker":"A","text":"高校の恩師の影響です。"},{"id":{"d":525,"n":6,"t":2},"speaker":"B","text":"きっと素晴らしい先生だったのでしょうね。"},{"id":{"d":525,"n":7,"t":2},"speaker":"A","text":"はい。憧れの教師です。"},{"id":{"d":525,"n":8,"t":2},"speaker":"B","text":"あなたなら、その恩師に負けないくらい良い先生になれると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":526,"n":1,"t":2},"speaker":"A","text":"どこへ行くのですか？"},{"id":{"d":526,"n":2,"t":2},"speaker":"B","text":"図書室へ本を返しに行くところです。"},{"id":{"d":526,"n":3,"t":2},"speaker":"A","text":"私も一緒に行ってよいですか？"},{"id":{"d":526,"n":4,"t":2},"speaker":"B","text":"はい。あなたも本を返すのですか？"},{"id":{"d":526,"n":5,"t":2},"speaker":"A","text":"いいえ。私は借りたい本があるのです。"},{"id":{"d":526,"n":6,"t":2},"speaker":"B","text":"それでは、一緒に行きましょう。"},{"id":{"d":526,"n":7,"t":2},"speaker":"A","text":"あと10分で休み時間が終わります。急ぎましょう。"}];
INSERT INTO sentence [{"id":{"d":527,"n":1,"t":2},"speaker":"A","text":"大学での勉強はどうですか？"},{"id":{"d":527,"n":2,"t":2},"speaker":"B","text":"課題が多くて、大変です。"},{"id":{"d":527,"n":3,"t":2},"speaker":"A","text":"どのような課題が出るのですか？"},{"id":{"d":527,"n":4,"t":2},"speaker":"B","text":"テーマに沿ったレポートを書くような課題が多いです。"},{"id":{"d":527,"n":5,"t":2},"speaker":"A","text":"専門性の高いテーマだと、調べるのに時間がかかりそうですね。"},{"id":{"d":527,"n":6,"t":2},"speaker":"B","text":"はい。まずは参考文献を探すことから始めます。"},{"id":{"d":527,"n":7,"t":2},"speaker":"A","text":"どのように探すのですか？"},{"id":{"d":527,"n":8,"t":2},"speaker":"B","text":"大学の図書館だと専門書が多いので、そこで探すことが多いです。"}];
INSERT INTO sentence [{"id":{"d":528,"n":1,"t":2},"speaker":"A","text":"先輩、3年間お世話になりました。"},{"id":{"d":528,"n":2,"t":2},"speaker":"B","text":"こちらこそありがとう。引退するとは、まだ実感が湧きません。"},{"id":{"d":528,"n":3,"t":2},"speaker":"A","text":"私も、先輩がいない部活動が想像できません。"},{"id":{"d":528,"n":4,"t":2},"speaker":"B","text":"これからは、あなたが部長としてバレーボール部をまとめていってくださいね。"},{"id":{"d":528,"n":5,"t":2},"speaker":"A","text":"はい、先輩を見習って頑張ります。"},{"id":{"d":528,"n":6,"t":2},"speaker":"B","text":"後輩のことをしっかりサポートしてあげてください。"},{"id":{"d":528,"n":7,"t":2},"speaker":"A","text":"頑張ります。引退しても時間のある時に練習を見に来てくださいね。"},{"id":{"d":528,"n":8,"t":2},"speaker":"B","text":"はい。時々、見学に来たいと思います。"}];
INSERT INTO sentence [{"id":{"d":529,"n":1,"t":2},"speaker":"A","text":"今日から部活の仮入部ができますね。"},{"id":{"d":529,"n":2,"t":2},"speaker":"B","text":"そうですね。何部に行くか決めましたか？"},{"id":{"d":529,"n":3,"t":2},"speaker":"A","text":"今日は美術部に行くつもりです。あなたはどうですか？"},{"id":{"d":529,"n":4,"t":2},"speaker":"B","text":"私は茶道部にします。"},{"id":{"d":529,"n":5,"t":2},"speaker":"A","text":"私は明日、茶道部に行こうと思っています。"},{"id":{"d":529,"n":6,"t":2},"speaker":"B","text":"そうでしたか。私は茶道部に入部すると決めているので、明日会うかもしれませんね。"},{"id":{"d":529,"n":7,"t":2},"speaker":"A","text":"もう入部する部活が決まっているのですか。私はなるべくたくさん見てから決めようと思っています。"},{"id":{"d":529,"n":8,"t":2},"speaker":"B","text":"それも良いですね。自分に合う部活に入部できるといいですね。"}];
INSERT INTO sentence [{"id":{"d":530,"n":1,"t":2},"speaker":"A","text":"今日は塾に行く日ですね。"},{"id":{"d":530,"n":2,"t":2},"speaker":"B","text":"はい。今日は塾へ行き、数学を勉強します。"},{"id":{"d":530,"n":3,"t":2},"speaker":"A","text":"宿題は終わっていますか？"},{"id":{"d":530,"n":4,"t":2},"speaker":"B","text":"いえ、まだ半分残っています。"},{"id":{"d":530,"n":5,"t":2},"speaker":"A","text":"急いで終わらせた方がいいですよ。"},{"id":{"d":530,"n":6,"t":2},"speaker":"B","text":"今から取りかかることにします。"},{"id":{"d":530,"n":7,"t":2},"speaker":"A","text":"出かける時間まで30分しかないので、頑張ってください。"},{"id":{"d":530,"n":8,"t":2},"speaker":"B","text":"はい。部屋に戻ってすぐに勉強を始めます。"}];
INSERT INTO sentence [{"id":{"d":531,"n":1,"t":2},"speaker":"A","text":"今日の給食の献立は何でしたか？"},{"id":{"d":531,"n":2,"t":2},"speaker":"B","text":"カレーライスでした。"},{"id":{"d":531,"n":3,"t":2},"speaker":"A","text":"あなたの好きなものですね。"},{"id":{"d":531,"n":4,"t":2},"speaker":"B","text":"はい。とてもおいしかったので、おかわりをしました。"},{"id":{"d":531,"n":5,"t":2},"speaker":"A","text":"カレーライスはおかわりをする人が多いでしょうね。"},{"id":{"d":531,"n":6,"t":2},"speaker":"B","text":"半分以上の生徒がおかわりをしたので、カレーがなくなってしまいました。"},{"id":{"d":531,"n":7,"t":2},"speaker":"A","text":"人気があるのですね。"},{"id":{"d":531,"n":8,"t":2},"speaker":"B","text":"そうですね。最も人気があるメニューの一つです。"}];
INSERT INTO sentence [{"id":{"d":532,"n":1,"t":2},"speaker":"A","text":"入試が近付いてきましたね。"},{"id":{"d":532,"n":2,"t":2},"speaker":"B","text":"受験をするのは生まれて初めてなので、緊張します。"},{"id":{"d":532,"n":3,"t":2},"speaker":"A","text":"私も初めてです。"},{"id":{"d":532,"n":4,"t":2},"speaker":"B","text":"合格できるかどうか、とても不安です。"},{"id":{"d":532,"n":5,"t":2},"speaker":"A","text":"合格祈願にでも行きましょうか？"},{"id":{"d":532,"n":6,"t":2},"speaker":"B","text":"どこか、合格祈願のために良いところはありますか？"},{"id":{"d":532,"n":7,"t":2},"speaker":"A","text":"湯島天神が有名だそうですよ。"},{"id":{"d":532,"n":8,"t":2},"speaker":"B","text":"そうなのですね。では、早速今度の日曜日に行ってみましょうか。"},{"id":{"d":532,"n":9,"t":2},"speaker":"A","text":"そうしましょう。お守りも買いたいです。"}];
INSERT INTO sentence [{"id":{"d":533,"n":1,"t":2},"speaker":"A","text":"行ってきます。"},{"id":{"d":533,"n":2,"t":2},"speaker":"B","text":"少し待ってください。傘は持ちましたか？"},{"id":{"d":533,"n":3,"t":2},"speaker":"A","text":"なぜですか？雨は降っていませんよ。"},{"id":{"d":533,"n":4,"t":2},"speaker":"B","text":"テレビ番組の天気予報コーナーで気象予報士が「午後からの降水確率が90パーセント」だと言っていました。傘を持って行った方がいいと思います。"},{"id":{"d":533,"n":5,"t":2},"speaker":"A","text":"そうでしたか。でも、もし下校する時に雨が降っていたら置き傘を使うので大丈夫ですよ。"},{"id":{"d":533,"n":6,"t":2},"speaker":"B","text":"置き傘をしているのですか？"},{"id":{"d":533,"n":7,"t":2},"speaker":"A","text":"はい。ロッカーに折り畳み傘を置いています。"},{"id":{"d":533,"n":8,"t":2},"speaker":"B","text":"それなら安心ですね。行ってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":534,"n":1,"t":2},"speaker":"A","text":"土曜日の部活は練習試合をすることになりました。"},{"id":{"d":534,"n":2,"t":2},"speaker":"B","text":"相手校はどの学校ですか？"},{"id":{"d":534,"n":3,"t":2},"speaker":"A","text":"金谷高校です。"},{"id":{"d":534,"n":4,"t":2},"speaker":"B","text":"強い選手がたくさんいる高校ですね。"},{"id":{"d":534,"n":5,"t":2},"speaker":"A","text":"はい。負けないように頑張ります。"},{"id":{"d":534,"n":6,"t":2},"speaker":"B","text":"会場はどこですか？"},{"id":{"d":534,"n":7,"t":2},"speaker":"A","text":"金谷高校のグラウンドなので、部員のみんなで一緒に行きます。"},{"id":{"d":534,"n":8,"t":2},"speaker":"B","text":"当日は、応援に行きますね。"}];
INSERT INTO sentence [{"id":{"d":535,"n":1,"t":2},"speaker":"A","text":"ママ、学校の上履きがきついです。"},{"id":{"d":535,"n":2,"t":2},"speaker":"B","text":"成長して、足が大きくなったのでしょうね。"},{"id":{"d":535,"n":3,"t":2},"speaker":"A","text":"しばらく履いていると、足の指が痛くなってしまいます。"},{"id":{"d":535,"n":4,"t":2},"speaker":"B","text":"大きいサイズのものに買い替えましょう。今はどのサイズの上履きを履いていますか？"},{"id":{"d":535,"n":5,"t":2},"speaker":"A","text":"今の上履きのサイズは21センチメートルです。"},{"id":{"d":535,"n":6,"t":2},"speaker":"B","text":"それでは、21.5センチメートルか22センチメートルの上履きが良いですね。"},{"id":{"d":535,"n":7,"t":2},"speaker":"A","text":"どちらのサイズが良いでしょうか？"},{"id":{"d":535,"n":8,"t":2},"speaker":"B","text":"実際に履いて決めた方がいいので、一緒に買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":536,"n":1,"t":2},"speaker":"A","text":"高校卒業後の進路は決めましたか？"},{"id":{"d":536,"n":2,"t":2},"speaker":"B","text":"はい、大学に進学したいと思っています。"},{"id":{"d":536,"n":3,"t":2},"speaker":"A","text":"私も同じです。"},{"id":{"d":536,"n":4,"t":2},"speaker":"B","text":"受験勉強はしていますが、合格できるかどうか不安です。"},{"id":{"d":536,"n":5,"t":2},"speaker":"A","text":"もし大学入試に不合格だったら、どうしますか？"},{"id":{"d":536,"n":6,"t":2},"speaker":"B","text":"浪人して、来年また受験するつもりです。あなたはどうするのですか？"},{"id":{"d":536,"n":7,"t":2},"speaker":"A","text":"私は不合格だったら大学進学を諦めます。"},{"id":{"d":536,"n":8,"t":2},"speaker":"B","text":"就職するということですか？"},{"id":{"d":536,"n":9,"t":2},"speaker":"A","text":"就職するか、専門学校へ行くか、どちらかにします。"}];
INSERT INTO sentence [{"id":{"d":537,"n":1,"t":2},"speaker":"A","text":"今日、保育園のお迎えを代わってもらえませんか？"},{"id":{"d":537,"n":2,"t":2},"speaker":"B","text":"よいですよ。何かありましたか？"},{"id":{"d":537,"n":3,"t":2},"speaker":"A","text":"夕方から緊急の会議が入ってしまいました。"},{"id":{"d":537,"n":4,"t":2},"speaker":"B","text":"そうでしたか。お迎えは何時までに保育園へ行けば良いですか？"},{"id":{"d":537,"n":5,"t":2},"speaker":"A","text":"19時半までに、保育園へ行くことになっています。間に合いますか？"},{"id":{"d":537,"n":6,"t":2},"speaker":"B","text":"終業後すぐに会社を出れば間に合うと思いますよ。お任せください。"},{"id":{"d":537,"n":7,"t":2},"speaker":"A","text":"それは良かったです。では、すみませんがお願いします。"},{"id":{"d":537,"n":8,"t":2},"speaker":"B","text":"お迎えに行った後は、私が夕食を作りますね。"},{"id":{"d":537,"n":9,"t":2},"speaker":"A","text":"助かります。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":538,"n":1,"t":2},"speaker":"A","text":"幼稚園は楽しかったですか？"},{"id":{"d":538,"n":2,"t":2},"speaker":"B","text":"はい。今日は園庭で遊びました。"},{"id":{"d":538,"n":3,"t":2},"speaker":"A","text":"何をして遊んだのですか？"},{"id":{"d":538,"n":4,"t":2},"speaker":"B","text":"鬼ごっこです。先生が鬼になって、みんなで逃げました。"},{"id":{"d":538,"n":5,"t":2},"speaker":"A","text":"捕まらないで逃げられましたか？"},{"id":{"d":538,"n":6,"t":2},"speaker":"B","text":"頑張って逃げていましたが、最後は捕まってしまいました。"},{"id":{"d":538,"n":7,"t":2},"speaker":"A","text":"先生は足が速いから、逃げ切るのは難しいですよね。"},{"id":{"d":538,"n":8,"t":2},"speaker":"B","text":"はい。結局、クラスの全員が捕まってしまいました。"}];
INSERT INTO sentence [{"id":{"d":539,"n":1,"t":2},"speaker":"A","text":"車を運転できるようになりたいです。"},{"id":{"d":539,"n":2,"t":2},"speaker":"B","text":"それでは、運転免許証をとる必要がありますね。"},{"id":{"d":539,"n":3,"t":2},"speaker":"A","text":"はい。自動車学校に通って運転免許証を取得することが一般的ですよね？"},{"id":{"d":539,"n":4,"t":2},"speaker":"B","text":"そうですね。私も自動車学校に通いました。"},{"id":{"d":539,"n":5,"t":2},"speaker":"A","text":"大変でしたか？"},{"id":{"d":539,"n":6,"t":2},"speaker":"B","text":"大変だとは感じませんでしたが、授業がたくさんあるので、時間がかかりました。"},{"id":{"d":539,"n":7,"t":2},"speaker":"A","text":"どのくらいの期間、通ったのですか？"},{"id":{"d":539,"n":8,"t":2},"speaker":"B","text":"2カ月か、3カ月くらいだったと思います。"},{"id":{"d":539,"n":9,"t":2},"speaker":"A","text":"結構、時間がかかりますね。私も、運転免許証取得に向けて頑張ります。"}];
INSERT INTO sentence [{"id":{"d":540,"n":1,"t":2},"speaker":"A","text":"もうすぐ3学期が終わりますね。"},{"id":{"d":540,"n":2,"t":2},"speaker":"B","text":"そうですね。実は、私は今年度まだ一度も欠席していませんよ。"},{"id":{"d":540,"n":3,"t":2},"speaker":"A","text":"それは素晴らしいですね。このままだと皆勤賞を取れますよ。"},{"id":{"d":540,"n":4,"t":2},"speaker":"B","text":"皆勤賞は初めてなので、取れたらうれしいです。"},{"id":{"d":540,"n":5,"t":2},"speaker":"A","text":"あと数日しか授業日数はないので、きっと皆勤賞が取れますよ。"},{"id":{"d":540,"n":6,"t":2},"speaker":"B","text":"そう願っています。"}];
INSERT INTO sentence [{"id":{"d":541,"n":1,"t":2},"speaker":"A","text":"今日の体育の授業では、何をするか知っていますか？"},{"id":{"d":541,"n":2,"t":2},"speaker":"B","text":"先ほど先生が、「長縄跳びを行う」と仰っていましたよ。"},{"id":{"d":541,"n":3,"t":2},"speaker":"A","text":"来月に長縄跳び大会があるからですね。"},{"id":{"d":541,"n":4,"t":2},"speaker":"B","text":"なるほど、納得です。"},{"id":{"d":541,"n":5,"t":2},"speaker":"A","text":"去年は準優勝だったので、今年こそ優勝したいです。"},{"id":{"d":541,"n":6,"t":2},"speaker":"B","text":"そうですね。そのために、たくさん練習が必要です。"},{"id":{"d":541,"n":7,"t":2},"speaker":"A","text":"今日の体育の授業も頑張りましょう。"},{"id":{"d":541,"n":8,"t":2},"speaker":"B","text":"はい。授業の後の休み時間にも長縄跳びの練習をしましょう。"}];
INSERT INTO sentence [{"id":{"d":542,"n":1,"t":2},"speaker":"A","text":"宿題が終わったら、明日の時間割を確認して、教科書などをそろえておいた方がいいですよ。"},{"id":{"d":542,"n":2,"t":2},"speaker":"B","text":"そうしたいのですが、明日の時間割が分かりません。"},{"id":{"d":542,"n":3,"t":2},"speaker":"A","text":"いつも通りではありませんか？"},{"id":{"d":542,"n":4,"t":2},"speaker":"B","text":"明日は時間割の変更があるのですが、メモするのを忘れてしまいました。"},{"id":{"d":542,"n":5,"t":2},"speaker":"A","text":"クラスメイトに聞いてみてはどうですか？"},{"id":{"d":542,"n":6,"t":2},"speaker":"B","text":"そうですね。綾乃さんに電話して教えてもらいます。"},{"id":{"d":542,"n":7,"t":2},"speaker":"A","text":"何か持ち物がないかどうかも、確認しておいた方がいいですね。"},{"id":{"d":542,"n":8,"t":2},"speaker":"B","text":"はい。忘れずに聞きます。"}];
INSERT INTO sentence [{"id":{"d":543,"n":1,"t":2},"speaker":"A","text":"授業参観に来て、どうでしたか？"},{"id":{"d":543,"n":2,"t":2},"speaker":"B","text":"ママが通っていた頃の小学校と随分変わっていて、驚きました。"},{"id":{"d":543,"n":3,"t":2},"speaker":"A","text":"どのようなところが変わっていましたか？"},{"id":{"d":543,"n":4,"t":2},"speaker":"B","text":"黒板ではなく、ホワイトボードが使われていたところです。"},{"id":{"d":543,"n":5,"t":2},"speaker":"A","text":"私が入学する前に、黒板からホワイトボードに変わったそうですよ。"},{"id":{"d":543,"n":6,"t":2},"speaker":"B","text":"それに、児童が一人一台ずつタブレットを使っていてびっくりしました。"},{"id":{"d":543,"n":7,"t":2},"speaker":"A","text":"インターネットで調べ物ができるし、先生からの連絡もタブレットで確認しています。"},{"id":{"d":543,"n":8,"t":2},"speaker":"B","text":"学校もデジタル化が進んでいるのですね。"}];
INSERT INTO sentence [{"id":{"d":544,"n":1,"t":2},"speaker":"A","text":"健一は来年小学校へ入学しますね。"},{"id":{"d":544,"n":2,"t":2},"speaker":"B","text":"そうですね。そろそろランドセルを買いましょうか？"},{"id":{"d":544,"n":3,"t":2},"speaker":"A","text":"はい。どのようなランドセルが良いと思いますか？"},{"id":{"d":544,"n":4,"t":2},"speaker":"B","text":"教科書やノートが重いので、軽いランドセルが良いと思います。"},{"id":{"d":544,"n":5,"t":2},"speaker":"A","text":"今は、軽い素材のランドセルが売られていますね。"},{"id":{"d":544,"n":6,"t":2},"speaker":"B","text":"丈夫さも重要ですね。"},{"id":{"d":544,"n":7,"t":2},"speaker":"A","text":"はい。できれば途中で買い替えずに6年間使ってほしいです。"},{"id":{"d":544,"n":8,"t":2},"speaker":"B","text":"近くのショッピングセンターで、ランドセルの展示会を行っているそうです。"},{"id":{"d":544,"n":9,"t":2},"speaker":"A","text":"健一を誘って次の休みに行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":545,"n":1,"t":2},"speaker":"A","text":"夏休みが始まる前に、自分の朝顔を家に持って帰ってください。"},{"id":{"d":545,"n":2,"t":2},"speaker":"B","text":"家で育てるのですか？"},{"id":{"d":545,"n":3,"t":2},"speaker":"A","text":"はい。夏休み中は学校で水やりができないので、持ち帰って家で水をあげてください。"},{"id":{"d":545,"n":4,"t":2},"speaker":"B","text":"夏休みが終わったら、また学校へ持ってくるのですか？"},{"id":{"d":545,"n":5,"t":2},"speaker":"A","text":"そうです。家に置いている間に朝顔が枯れないように、しっかりお世話してください。"},{"id":{"d":545,"n":6,"t":2},"speaker":"B","text":"観察記録はどうしますか？"},{"id":{"d":545,"n":7,"t":2},"speaker":"A","text":"用紙を渡しますので、毎週月曜日に記録をつけてください。"},{"id":{"d":545,"n":8,"t":2},"speaker":"B","text":"もし夏休み中に花が咲いたら、みんなに見てほしいです。"},{"id":{"d":545,"n":9,"t":2},"speaker":"A","text":"それでは、花が咲いたら写真を撮っておきましょうね。"}];
INSERT INTO sentence [{"id":{"d":546,"n":1,"t":2},"speaker":"A","text":"お子さんは塾に通っていますか？"},{"id":{"d":546,"n":2,"t":2},"speaker":"B","text":"はい。4年生の時から塾に通わせていますよ。"},{"id":{"d":546,"n":3,"t":2},"speaker":"A","text":"うちも塾に通わせるかどうか、悩んでいます。"},{"id":{"d":546,"n":4,"t":2},"speaker":"B","text":"私立の中学校を受験することを検討しているのですか？"},{"id":{"d":546,"n":5,"t":2},"speaker":"A","text":"それも迷っているのです。あなたのお子さんは私立の中学校を受験しますか？"},{"id":{"d":546,"n":6,"t":2},"speaker":"B","text":"その予定です。もし受験をするなら早めに準備した方がいいと聞きましたよ。"},{"id":{"d":546,"n":7,"t":2},"speaker":"A","text":"そのようですね。私の娘は勉強が好きではないので、受験を嫌がるかもしれません。"},{"id":{"d":546,"n":8,"t":2},"speaker":"B","text":"わが家の子も勉強嫌いですが、塾に入ったら勉強への意欲が出てきましたよ。"},{"id":{"d":546,"n":9,"t":2},"speaker":"A","text":"そのようなこともあるのですね。娘とゆっくり話し合ってみます。"}];
INSERT INTO sentence [{"id":{"d":547,"n":1,"t":2},"speaker":"A","text":"お父さん、あそこにキツネの像がありますよ。"},{"id":{"d":547,"n":2,"t":2},"speaker":"B","text":"ここは稲荷神社なので、キツネが神様の使いとして祭られているのかもしれませんね。"},{"id":{"d":547,"n":3,"t":2},"speaker":"A","text":"何の神様の使いですか？"},{"id":{"d":547,"n":4,"t":2},"speaker":"B","text":"稲作や農業の神様といわれています。"},{"id":{"d":547,"n":5,"t":2},"speaker":"A","text":"キツネが、口に何かくわえていますね。"},{"id":{"d":547,"n":6,"t":2},"speaker":"B","text":"あれは、稲束です。"},{"id":{"d":547,"n":7,"t":2},"speaker":"A","text":"稲作の神様だから、稲の束をくわえているのですね？"},{"id":{"d":547,"n":8,"t":2},"speaker":"B","text":"そうだと思います。他にも、鍵や巻物をくわえている場合もあると聞いたことがあります。"},{"id":{"d":547,"n":9,"t":2},"speaker":"A","text":"知りませんでした。興味深いですね。"}];
INSERT INTO sentence [{"id":{"d":548,"n":1,"t":2},"speaker":"A","text":"久保先生、お久しぶりです。"},{"id":{"d":548,"n":2,"t":2},"speaker":"B","text":"まあ橋口さん、卒業以来ですね。"},{"id":{"d":548,"n":3,"t":2},"speaker":"A","text":"はい。ご無沙汰しています。"},{"id":{"d":548,"n":4,"t":2},"speaker":"B","text":"橋口さんはすっかり大人になりましたね。今は何をしているのですか？"},{"id":{"d":548,"n":5,"t":2},"speaker":"A","text":"IT関係の企業で働いています。"},{"id":{"d":548,"n":6,"t":2},"speaker":"B","text":"昔からコンピューターに詳しかったので、ぴったりの仕事ですね。"},{"id":{"d":548,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。先生は今も高校で教えているのですか？"},{"id":{"d":548,"n":8,"t":2},"speaker":"B","text":"いいえ。昨年、定年退職したので、今は家でのんびり過ごしています。"}];
INSERT INTO sentence [{"id":{"d":549,"n":1,"t":2},"speaker":"A","text":"先月から、パートタイムで働き始めました。"},{"id":{"d":549,"n":2,"t":2},"speaker":"B","text":"どのような仕事をしているのですか？"},{"id":{"d":549,"n":3,"t":2},"speaker":"A","text":"学校の給食センターで調理の補助をしています。"},{"id":{"d":549,"n":4,"t":2},"speaker":"B","text":"調理師免許を持っているのですか？"},{"id":{"d":549,"n":5,"t":2},"speaker":"A","text":"持っていませんよ。調理のお手伝いだけなので、免許は必要ありません。"},{"id":{"d":549,"n":6,"t":2},"speaker":"B","text":"そうなのですね。仕事は大変ですか？"},{"id":{"d":549,"n":7,"t":2},"speaker":"A","text":"大変なこともありますが、子どもたちにおいしい給食を食べてほしいので、やりがいがあります。"},{"id":{"d":549,"n":8,"t":2},"speaker":"B","text":"やりがいを感じられるのは良いですね。"}];
INSERT INTO sentence [{"id":{"d":550,"n":1,"t":2},"speaker":"A","text":"生物と化学では、どちらの科目が好きですか？"},{"id":{"d":550,"n":2,"t":2},"speaker":"B","text":"生物の方が好きです。"},{"id":{"d":550,"n":3,"t":2},"speaker":"A","text":"化学は嫌いですか？"},{"id":{"d":550,"n":4,"t":2},"speaker":"B","text":"嫌いというより、苦手です。化学式を見ると、難しいと感じてしまいます。"},{"id":{"d":550,"n":5,"t":2},"speaker":"A","text":"私は実験が好きなので、化学の方が好きです。実験の授業は胸が躍ります。"},{"id":{"d":550,"n":6,"t":2},"speaker":"B","text":"実験は確かに楽しいですが、結果をレポートにまとめることが負担です。化学式も出てきますし。"}];
INSERT INTO sentence [{"id":{"d":551,"n":1,"t":2},"speaker":"A","text":"遠山先生、5組の学力テストの結果はどうでしたか？"},{"id":{"d":551,"n":2,"t":2},"speaker":"B","text":"全体としては悪くなかったですが、英語は得点が低い生徒が多かったです。"},{"id":{"d":551,"n":3,"t":2},"speaker":"A","text":"私のクラスもそうです。リスニング問題の正答率が低い傾向がありました。"},{"id":{"d":551,"n":4,"t":2},"speaker":"B","text":"こちらも同じです。何か対策が必要ですね。"},{"id":{"d":551,"n":5,"t":2},"speaker":"A","text":"英語の授業で、リスニング問題を解く時間を設けるのはどうでしょうか？"},{"id":{"d":551,"n":6,"t":2},"speaker":"B","text":"それは良い案です。英語担当の先生にお願いしましょうか？"},{"id":{"d":551,"n":7,"t":2},"speaker":"A","text":"はい。ちょうどお昼休みに英語担当の先生に話をしに行く用事があるので、そのときに一緒に行きましょう。"},{"id":{"d":551,"n":8,"t":2},"speaker":"B","text":"分かりました。では、またお昼休みに。"}];
INSERT INTO sentence [{"id":{"d":552,"n":1,"t":2},"speaker":"A","text":"石橋さんは、バイリンガルなのですね。"},{"id":{"d":552,"n":2,"t":2},"speaker":"B","text":"はい。英語と日本語を話せます。"},{"id":{"d":552,"n":3,"t":2},"speaker":"A","text":"ご両親のどちらかが英語を話すのですか？"},{"id":{"d":552,"n":4,"t":2},"speaker":"B","text":"いえ。両親ともに日本人です。父の仕事の都合で、米国に8年くらい住んでいたのです。"},{"id":{"d":552,"n":5,"t":2},"speaker":"A","text":"そうでしたか。米国にいたときに自然と英語を話せるようになったのですか？"},{"id":{"d":552,"n":6,"t":2},"speaker":"B","text":"小さかったせいか、すんなりと話せるようになりました。"},{"id":{"d":552,"n":7,"t":2},"speaker":"A","text":"それは素晴らしいことです。逆に、日本語を忘れませんでしたか？"},{"id":{"d":552,"n":8,"t":2},"speaker":"B","text":"家では両親と日本語で話していたので、大丈夫でした。"}];
INSERT INTO sentence [{"id":{"d":553,"n":1,"t":2},"speaker":"A","text":"次の音楽の授業で、鍵盤ハーモニカで曲を演奏するテストがあります。"},{"id":{"d":553,"n":2,"t":2},"speaker":"B","text":"何という曲を演奏するのですか？"},{"id":{"d":553,"n":3,"t":2},"speaker":"A","text":"『翼をください』です。"},{"id":{"d":553,"n":4,"t":2},"speaker":"B","text":"有名な曲ですね。演奏できるようになりましたか？"},{"id":{"d":553,"n":5,"t":2},"speaker":"A","text":"まだ自信がありません。"},{"id":{"d":553,"n":6,"t":2},"speaker":"B","text":"家で練習してはどうですか？"},{"id":{"d":553,"n":7,"t":2},"speaker":"A","text":"はい。それでは明日、鍵盤ハーモニカを持ち帰ってきます。"},{"id":{"d":553,"n":8,"t":2},"speaker":"B","text":"私も練習に付き合いますよ。"}];
INSERT INTO sentence [{"id":{"d":554,"n":1,"t":2},"speaker":"A","text":"部活の顧問の先生が代わりました。"},{"id":{"d":554,"n":2,"t":2},"speaker":"B","text":"何かありましたか？"},{"id":{"d":554,"n":3,"t":2},"speaker":"A","text":"顧問の藤木先生が腰を痛めてしまったそうで、長期休養に入ったのです。"},{"id":{"d":554,"n":4,"t":2},"speaker":"B","text":"お気の毒ですね。代わりの顧問の先生はどなたですか？"},{"id":{"d":554,"n":5,"t":2},"speaker":"A","text":"中条先生です。"},{"id":{"d":554,"n":6,"t":2},"speaker":"B","text":"中条先生は、陸上部の顧問をされていませんか？"},{"id":{"d":554,"n":7,"t":2},"speaker":"A","text":"はい。陸上部と卓球部を兼任することになりました。"},{"id":{"d":554,"n":8,"t":2},"speaker":"B","text":"それは大変そうですね。藤木先生が早く回復すると良いですね。"}];
INSERT INTO sentence [{"id":{"d":555,"n":1,"t":2},"speaker":"A","text":"幼稚園で、劇をすることになりました。"},{"id":{"d":555,"n":2,"t":2},"speaker":"B","text":"どのような劇ですか？"},{"id":{"d":555,"n":3,"t":2},"speaker":"A","text":"『赤ずきんちゃん』です。"},{"id":{"d":555,"n":4,"t":2},"speaker":"B","text":"そうですか。誰がどのような役をするか、もう決まりましたか？"},{"id":{"d":555,"n":5,"t":2},"speaker":"A","text":"明日決めるので、「何をやりたいか考えておくように」と先生から言われました。"},{"id":{"d":555,"n":6,"t":2},"speaker":"B","text":"あなたは何の役をやりたいですか？"},{"id":{"d":555,"n":7,"t":2},"speaker":"A","text":"おばあさんの役です。"},{"id":{"d":555,"n":8,"t":2},"speaker":"B","text":"希望通りになると良いですね。"}];
INSERT INTO sentence [{"id":{"d":556,"n":1,"t":2},"speaker":"A","text":"僕の通っていた中学校は、校則がとても厳しかったです。"},{"id":{"d":556,"n":2,"t":2},"speaker":"B","text":"どのような校則がありましたか？"},{"id":{"d":556,"n":3,"t":2},"speaker":"A","text":"例えば、制服のシャツを腕まくりしてはいけない、という規則がありました。"},{"id":{"d":556,"n":4,"t":2},"speaker":"B","text":"夏でも腕まくりしてはいけませんか？"},{"id":{"d":556,"n":5,"t":2},"speaker":"A","text":"はい。暑くともみんな我慢していました。"},{"id":{"d":556,"n":6,"t":2},"speaker":"B","text":"他にも厳しい校則はありましたか？"},{"id":{"d":556,"n":7,"t":2},"speaker":"A","text":"女子は、前髪を眉毛の1センチメートル上でそろえる、というものがありました。"},{"id":{"d":556,"n":8,"t":2},"speaker":"B","text":"かなり厳しいですね。驚きました。"},{"id":{"d":556,"n":9,"t":2},"speaker":"A","text":"今はもうそのような校則はなくなったようですが、当時は違反しないようにすることが大変でした。"}];
INSERT INTO sentence [{"id":{"d":557,"n":1,"t":2},"speaker":"A","text":"私は奨学金を借りて大学に通いました。"},{"id":{"d":557,"n":2,"t":2},"speaker":"B","text":"今でも奨学金を返済しているのですか？"},{"id":{"d":557,"n":3,"t":2},"speaker":"A","text":"はい。奨学金の返済が終わるまでにあと10年かかります。"},{"id":{"d":557,"n":4,"t":2},"speaker":"B","text":"それは長いですね。"},{"id":{"d":557,"n":5,"t":2},"speaker":"A","text":"経済的に負担は大きいですが、奨学金のおかげで大学へ通えたので、仕方ないと思っています。"},{"id":{"d":557,"n":6,"t":2},"speaker":"B","text":"大学に進学したいという気持ちが強かったのですね。"},{"id":{"d":557,"n":7,"t":2},"speaker":"A","text":"そうですね。どうしても行きたい大学があって、そこに進学しました。"},{"id":{"d":557,"n":8,"t":2},"speaker":"B","text":"無事に進学できて、良かったですね。"}];
INSERT INTO sentence [{"id":{"d":558,"n":1,"t":2},"speaker":"A","text":"制服の注文をお願いします。"},{"id":{"d":558,"n":2,"t":2},"speaker":"B","text":"学校名をお伺いできますか？"},{"id":{"d":558,"n":3,"t":2},"speaker":"A","text":"西代高校です。"},{"id":{"d":558,"n":4,"t":2},"speaker":"B","text":"かしこまりました。それでは採寸しますね。"},{"id":{"d":558,"n":5,"t":2},"speaker":"A","text":"服を着たままで大丈夫ですか？"},{"id":{"d":558,"n":6,"t":2},"speaker":"B","text":"いえ。上着だけ脱いでいただけますでしょうか？"},{"id":{"d":558,"n":7,"t":2},"speaker":"A","text":"はい。上着を脱ぎました。どこから測りますか？"},{"id":{"d":558,"n":8,"t":2},"speaker":"B","text":"まずは腕の長さを測りますので、右腕を少し上げてください。"}];
INSERT INTO sentence [{"id":{"d":559,"n":1,"t":2},"speaker":"A","text":"岩本さん、研修旅行の計画表はできましたか？"},{"id":{"d":559,"n":2,"t":2},"speaker":"B","text":"すみません。まだできていません。"},{"id":{"d":559,"n":3,"t":2},"speaker":"A","text":"何か問題があるのですか？"},{"id":{"d":559,"n":4,"t":2},"speaker":"B","text":"はい。研修に参加する人数が、まだ確定していません。"},{"id":{"d":559,"n":5,"t":2},"speaker":"A","text":"各部署に人数の確認の連絡をしましたか？"},{"id":{"d":559,"n":6,"t":2},"speaker":"B","text":"はい。2つの部署から、まだ回答がありません。"},{"id":{"d":559,"n":7,"t":2},"speaker":"A","text":"もう一度連絡して、人数を決定するようにお願いしてみてください。"},{"id":{"d":559,"n":8,"t":2},"speaker":"B","text":"分かりました。すぐに連絡します。"}];
INSERT INTO sentence [{"id":{"d":560,"n":1,"t":2},"speaker":"A","text":"期末テストが近いですね。"},{"id":{"d":560,"n":2,"t":2},"speaker":"B","text":"一緒にテスト勉強をしませんか？"},{"id":{"d":560,"n":3,"t":2},"speaker":"A","text":"はい。どこで勉強をしますか？"},{"id":{"d":560,"n":4,"t":2},"speaker":"B","text":"塾の自習室にしましょう。"},{"id":{"d":560,"n":5,"t":2},"speaker":"A","text":"良いですね。今日の放課後は空いていますか？"},{"id":{"d":560,"n":6,"t":2},"speaker":"B","text":"今日は部活があるので、明日でもよいですか？"},{"id":{"d":560,"n":7,"t":2},"speaker":"A","text":"分かりました。それでは、明日の放課後は、自習室に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":561,"n":1,"t":2},"speaker":"A","text":"浪人していた時、予備校に通っていましたか？"},{"id":{"d":561,"n":2,"t":2},"speaker":"B","text":"はい。毎日通って、自分なりに一生懸命勉強していました。"},{"id":{"d":561,"n":3,"t":2},"speaker":"A","text":"予備校で友達はできましたか？"},{"id":{"d":561,"n":4,"t":2},"speaker":"B","text":"いえ、新しい友達はできませんでした。でも、高校の時の友達が同じ予備校に通っていました。"},{"id":{"d":561,"n":5,"t":2},"speaker":"A","text":"私は予備校で友達ができて、一緒に勉強しました。"},{"id":{"d":561,"n":6,"t":2},"speaker":"B","text":"友達と一緒だと、おしゃべりしてしまいませんでしたか？"},{"id":{"d":561,"n":7,"t":2},"speaker":"A","text":"意外と大丈夫でした。お互いに励まし合って頑張っていました。"},{"id":{"d":561,"n":8,"t":2},"speaker":"B","text":"それは良い関係ですね。"}];
INSERT INTO sentence [{"id":{"d":562,"n":1,"t":2},"speaker":"A","text":"先日のテストを返しますが、みんなあまりできていませんでしたよ。"},{"id":{"d":562,"n":2,"t":2},"speaker":"B","text":"平均点は何点ですか？"},{"id":{"d":562,"n":3,"t":2},"speaker":"A","text":"53点です。40点以下の人は追試をします。"},{"id":{"d":562,"n":4,"t":2},"speaker":"B","text":"私は38点なので追試ですね。"},{"id":{"d":562,"n":5,"t":2},"speaker":"A","text":"追試では80点以上を目指してください。"},{"id":{"d":562,"n":6,"t":2},"speaker":"B","text":"追試はこのテストと同じ問題ですか？"},{"id":{"d":562,"n":7,"t":2},"speaker":"A","text":"いえ。難しさは同じくらいの類題を出します。"},{"id":{"d":562,"n":8,"t":2},"speaker":"B","text":"追試はいつですか？"},{"id":{"d":562,"n":9,"t":2},"speaker":"A","text":"次の金曜日です。追試を受ける人は頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":563,"n":1,"t":2},"speaker":"A","text":"合格のお守りをどうぞ。"},{"id":{"d":563,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。わざわざ神社へ行ってくれたのですか？"},{"id":{"d":563,"n":3,"t":2},"speaker":"A","text":"湯島天満宮へ行きました。"},{"id":{"d":563,"n":4,"t":2},"speaker":"B","text":"学業成就で有名ですよね。ご利益がありそうです。"},{"id":{"d":563,"n":5,"t":2},"speaker":"A","text":"第一志望校に受かると良いですね。"},{"id":{"d":563,"n":6,"t":2},"speaker":"B","text":"はい。おかげで勉強への意欲が湧いてきました。"},{"id":{"d":563,"n":7,"t":2},"speaker":"A","text":"応援しているので、頑張ってください。"},{"id":{"d":563,"n":8,"t":2},"speaker":"B","text":"良い結果が出せるように全力を尽くします。"}];
INSERT INTO sentence [{"id":{"d":564,"n":1,"t":2},"speaker":"A","text":"模擬試験の結果は出ましたか？"},{"id":{"d":564,"n":2,"t":2},"speaker":"B","text":"はい。よくできた科目と、できなかった科目の差が大きかったです。"},{"id":{"d":564,"n":3,"t":2},"speaker":"A","text":"よくできた科目は何ですか？"},{"id":{"d":564,"n":4,"t":2},"speaker":"B","text":"国語と社会です。"},{"id":{"d":564,"n":5,"t":2},"speaker":"A","text":"それでは、他の科目ができなかったのですね。"},{"id":{"d":564,"n":6,"t":2},"speaker":"B","text":"英語はまずまずという点数で、数学と理科の点数が低かったです。"},{"id":{"d":564,"n":7,"t":2},"speaker":"A","text":"問題が難しかったのではありませんか？"},{"id":{"d":564,"n":8,"t":2},"speaker":"B","text":"平均点は低くないので、僕の実力不足です。"}];
INSERT INTO sentence [{"id":{"d":565,"n":1,"t":2},"speaker":"A","text":"押入れを整理していたら、パパの卒業アルバムが出てきました。"},{"id":{"d":565,"n":2,"t":2},"speaker":"B","text":"いつの卒業アルバムですか？"},{"id":{"d":565,"n":3,"t":2},"speaker":"A","text":"小学校です。"},{"id":{"d":565,"n":4,"t":2},"speaker":"B","text":"見せてください。パパはどこですか？"},{"id":{"d":565,"n":5,"t":2},"speaker":"A","text":"パパは3組だったので、このページに写っています。"},{"id":{"d":565,"n":6,"t":2},"speaker":"B","text":"見つけました。でも別人のようです。"},{"id":{"d":565,"n":7,"t":2},"speaker":"A","text":"顔が幼いですね。"},{"id":{"d":565,"n":8,"t":2},"speaker":"B","text":"パパにも子どもの頃が存在したとは、不思議な感じがします。"}];
INSERT INTO sentence [{"id":{"d":566,"n":1,"t":2},"speaker":"A","text":"ママの出身校には、有名人の卒業生はいますか？"},{"id":{"d":566,"n":2,"t":2},"speaker":"B","text":"有名人とは、芸能人ということですか？"},{"id":{"d":566,"n":3,"t":2},"speaker":"A","text":"政治家でも作家でもよいので、有名な人はいますか？"},{"id":{"d":566,"n":4,"t":2},"speaker":"B","text":"はい。前に、インタビューでオリンピック選手がママと同じ大学を出ていると話していた記憶があります。"},{"id":{"d":566,"n":5,"t":2},"speaker":"A","text":"素晴しいことですね。何の種目の選手でしょうか？"},{"id":{"d":566,"n":6,"t":2},"speaker":"B","text":"水泳選手だったような気がします。名前は思い出せません。"},{"id":{"d":566,"n":7,"t":2},"speaker":"A","text":"メダリストだったら、調べたら分かりそうですね。"},{"id":{"d":566,"n":8,"t":2},"speaker":"B","text":"そうですね。金メダルを取った選手なので、すぐに分かると思います。"}];
INSERT INTO sentence [{"id":{"d":567,"n":1,"t":2},"speaker":"A","text":"大竹さん、宿題が出ていませんよ。"},{"id":{"d":567,"n":2,"t":2},"speaker":"B","text":"すみません。忘れました。"},{"id":{"d":567,"n":3,"t":2},"speaker":"A","text":"昨日も出していませんね。どうしたのですか？"},{"id":{"d":567,"n":4,"t":2},"speaker":"B","text":"2日間とも宿題はしているのですが、プリントを家に置いてきてしまったのです。"},{"id":{"d":567,"n":5,"t":2},"speaker":"A","text":"そうですか。終わったら、すぐにランドセルに入れる習慣を付けた方がいいですね。"},{"id":{"d":567,"n":6,"t":2},"speaker":"B","text":"今日から、そうします。"},{"id":{"d":567,"n":7,"t":2},"speaker":"A","text":"それから、登校する前に持ち物のチェックをしましょうね。"},{"id":{"d":567,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":568,"n":1,"t":2},"speaker":"A","text":"先生は来月から、産前産後休暇に入ります。"},{"id":{"d":568,"n":2,"t":2},"speaker":"B","text":"いつまでお休みするのですか？"},{"id":{"d":568,"n":3,"t":2},"speaker":"A","text":"赤ちゃんが生まれてから、1年くらい育児休業を取る予定です。"},{"id":{"d":568,"n":4,"t":2},"speaker":"B","text":"それでは、戻ってくる頃には私たちは卒業していますね。"},{"id":{"d":568,"n":5,"t":2},"speaker":"A","text":"そうなりますね。卒業を見送れなくて、先生も残念です。"},{"id":{"d":568,"n":6,"t":2},"speaker":"B","text":"先生がお休みの間、代わりの先生がいらっしゃるのですか？"},{"id":{"d":568,"n":7,"t":2},"speaker":"A","text":"はい。別の小学校から、臨時の先生が来てくださいます。"},{"id":{"d":568,"n":8,"t":2},"speaker":"B","text":"先生、お休みになって会えなくなるのは寂しいですが、元気な赤ちゃんを産んでくださいね。"}];
INSERT INTO sentence [{"id":{"d":569,"n":1,"t":2},"speaker":"A","text":"先生、今日の放課後、クラスのみんなで残ってもよいですか？"},{"id":{"d":569,"n":2,"t":2},"speaker":"B","text":"残って何をするのですか？"},{"id":{"d":569,"n":3,"t":2},"speaker":"A","text":"学園祭の準備を進めたいのです。"},{"id":{"d":569,"n":4,"t":2},"speaker":"B","text":"準備が遅れているのですか？"},{"id":{"d":569,"n":5,"t":2},"speaker":"A","text":"はい。このままでは、とても間に合いそうにありません。"},{"id":{"d":569,"n":6,"t":2},"speaker":"B","text":"そうですか。何時まで残りたいですか？"},{"id":{"d":569,"n":7,"t":2},"speaker":"A","text":"できれば9時くらいまで作業したいです。"},{"id":{"d":569,"n":8,"t":2},"speaker":"B","text":"それは遅過ぎますね。8時には作業を終えて下校してください。"},{"id":{"d":569,"n":9,"t":2},"speaker":"A","text":"分かりました。8時まで頑張って作業を進めます。"}];
INSERT INTO sentence [{"id":{"d":570,"n":1,"t":2},"speaker":"A","text":"第一志望の高校は合格でしたか？"},{"id":{"d":570,"n":2,"t":2},"speaker":"B","text":"不合格でした。"},{"id":{"d":570,"n":3,"t":2},"speaker":"A","text":"頑張っていたのに、残念ですね。"},{"id":{"d":570,"n":4,"t":2},"speaker":"B","text":"はい。でも合格できるかどうかギリギリの成績だったので、心の準備はしていました。"},{"id":{"d":570,"n":5,"t":2},"speaker":"A","text":"第一志望が不合格ということは、第二志望の高校へ進学するのでしょうか？"},{"id":{"d":570,"n":6,"t":2},"speaker":"B","text":"そうするつもりです。気持ちを切り替えて、高校生活を充実させたいと思っています。"},{"id":{"d":570,"n":7,"t":2},"speaker":"A","text":"前向きに捉えていて、安心しました。"},{"id":{"d":570,"n":8,"t":2},"speaker":"B","text":"くよくよしても仕方ありませんよ。"}];
INSERT INTO sentence [{"id":{"d":571,"n":1,"t":2},"speaker":"A","text":"あなたが美術の授業で描いた絵が、コンクールで入賞しました。"},{"id":{"d":571,"n":2,"t":2},"speaker":"B","text":"本当ですか？何という賞ですか？"},{"id":{"d":571,"n":3,"t":2},"speaker":"A","text":"審査員特別賞です。おめでとうございます。"},{"id":{"d":571,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。一生懸命に描いた作品なので、とてもうれしいです。"},{"id":{"d":571,"n":5,"t":2},"speaker":"A","text":"先生も誇らしいです。それからこれは、コンクールの主催者から届いたお知らせです。"},{"id":{"d":571,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。授賞式があると書いてあります。"},{"id":{"d":571,"n":7,"t":2},"speaker":"A","text":"授賞式で賞状を頂けるようですよ。参加しますか？"},{"id":{"d":571,"n":8,"t":2},"speaker":"B","text":"もちろん、ぜひ、参加したいです。"}];
INSERT INTO sentence [{"id":{"d":572,"n":1,"t":2},"speaker":"A","text":"賢人さんは、どこの中学校を卒業しましたか？"},{"id":{"d":572,"n":2,"t":2},"speaker":"B","text":"三原山中学校です。"},{"id":{"d":572,"n":3,"t":2},"speaker":"A","text":"バスケットボール部が強い中学校ですね。"},{"id":{"d":572,"n":4,"t":2},"speaker":"B","text":"はい。何度も全国大会に出場しています。"},{"id":{"d":572,"n":5,"t":2},"speaker":"A","text":"賢人さんもバスケットボール部でしたか？"},{"id":{"d":572,"n":6,"t":2},"speaker":"B","text":"いえ、陸上部でした。バスケットボール部は練習がとても厳しかったので、諦めました。"}];
INSERT INTO sentence [{"id":{"d":573,"n":1,"t":2},"speaker":"A","text":"先輩、明日の試合、頑張ってください。"},{"id":{"d":573,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。練習の成果が発揮できるように頑張ります。"},{"id":{"d":573,"n":3,"t":2},"speaker":"A","text":"明日の結果次第で、県大会に出場できますね。"},{"id":{"d":573,"n":4,"t":2},"speaker":"B","text":"はい。去年は県大会に行けなかったので、今年は何としても出場したいです。"},{"id":{"d":573,"n":5,"t":2},"speaker":"A","text":"先輩なら、きっと大丈夫だと思います。"},{"id":{"d":573,"n":6,"t":2},"speaker":"B","text":"そう言ってもらえてうれしいです。"},{"id":{"d":573,"n":7,"t":2},"speaker":"A","text":"明日、応援に行きますね。"},{"id":{"d":573,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。心強いです。"}];
INSERT INTO sentence [{"id":{"d":574,"n":1,"t":2},"speaker":"A","text":"帰る途中で、ばったり辻先生に会いました。"},{"id":{"d":574,"n":2,"t":2},"speaker":"B","text":"懐かしい名前です。小学校でお世話になった先生ですね。"},{"id":{"d":574,"n":3,"t":2},"speaker":"A","text":"はい。2年生の時の担任の先生です。"},{"id":{"d":574,"n":4,"t":2},"speaker":"B","text":"先生はあなたのことを覚えていてくれましたか？"},{"id":{"d":574,"n":5,"t":2},"speaker":"A","text":"はい。声をお掛けしたら、すぐに分かってくれました。"},{"id":{"d":574,"n":6,"t":2},"speaker":"B","text":"それはうれしいですね。"},{"id":{"d":574,"n":7,"t":2},"speaker":"A","text":"今は、隣の市の学校で教えているそうです。"},{"id":{"d":574,"n":8,"t":2},"speaker":"B","text":"そうですか。私もお会いしたいです。"}];
INSERT INTO sentence [{"id":{"d":575,"n":1,"t":2},"speaker":"A","text":"消しゴムがなくなってしまいました。"},{"id":{"d":575,"n":2,"t":2},"speaker":"B","text":"どこかに置き忘れたのですか？"},{"id":{"d":575,"n":3,"t":2},"speaker":"A","text":"はい。授業が始まった時はありましたので、教室のどこかに落としてしまったようです。"},{"id":{"d":575,"n":4,"t":2},"speaker":"B","text":"それなら、近くにあるはずですね。私も探します。"},{"id":{"d":575,"n":5,"t":2},"speaker":"A","text":"すみません。"},{"id":{"d":575,"n":6,"t":2},"speaker":"B","text":"あ、見つけました。私の足元にありましたよ。"},{"id":{"d":575,"n":7,"t":2},"speaker":"A","text":"どうもありがとうございます。"},{"id":{"d":575,"n":8,"t":2},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":576,"n":1,"t":2},"speaker":"A","text":"杏奈さん、お久しぶり。"},{"id":{"d":576,"n":2,"t":2},"speaker":"B","text":"百花さん。卒業以来ですね。"},{"id":{"d":576,"n":3,"t":2},"speaker":"A","text":"卒業してから、どうしていましたか？"},{"id":{"d":576,"n":4,"t":2},"speaker":"B","text":"大学に入って、今は大学3年生です。百花さんはどうしていましたか？"},{"id":{"d":576,"n":5,"t":2},"speaker":"A","text":"私は短大を卒業して、地元の会社に就職しました。"},{"id":{"d":576,"n":6,"t":2},"speaker":"B","text":"そうでしたか。今は実家に住んでいるのですか？"},{"id":{"d":576,"n":7,"t":2},"speaker":"A","text":"はい。杏奈さんは1人暮らしをしているのですか？"},{"id":{"d":576,"n":8,"t":2},"speaker":"B","text":"はい。大学の近くに住んでいます。"},{"id":{"d":576,"n":9,"t":2},"speaker":"A","text":"今度、同窓会でもやりたいですね。"},{"id":{"d":576,"n":10,"t":2},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":577,"n":1,"t":2},"speaker":"A","text":"小野寺さん、昨日の生物のテストを受けていませんね。"},{"id":{"d":577,"n":2,"t":2},"speaker":"B","text":"はい。体調が悪くなって、テストの前に早退しました。"},{"id":{"d":577,"n":3,"t":2},"speaker":"A","text":"体調はもう大丈夫ですか？"},{"id":{"d":577,"n":4,"t":2},"speaker":"B","text":"はい。一晩寝たら体調が回復しました。大丈夫です。"},{"id":{"d":577,"n":5,"t":2},"speaker":"A","text":"生物のテストは、追試験を受けてもらいます。"},{"id":{"d":577,"n":6,"t":2},"speaker":"B","text":"はい。テストはいつでしょうか？"},{"id":{"d":577,"n":7,"t":2},"speaker":"A","text":"明日の放課後です。授業が終わったら理科室に来てください。"},{"id":{"d":577,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":578,"n":1,"t":2},"speaker":"A","text":"颯太君、お弁当の時間に歩き回ってはいけませんよ。"},{"id":{"d":578,"n":2,"t":2},"speaker":"B","text":"僕はもう、お弁当の中身を食べ終わりました。"},{"id":{"d":578,"n":3,"t":2},"speaker":"A","text":"残さずに全部食べましたか？"},{"id":{"d":578,"n":4,"t":2},"speaker":"B","text":"ピーマンだけ残しました。ピーマンは嫌いなのです。"},{"id":{"d":578,"n":5,"t":2},"speaker":"A","text":"お母さんが作ってくれたお弁当のおかずなので、頑張って残さずに食べましょうね。"},{"id":{"d":578,"n":6,"t":2},"speaker":"B","text":"はい、もう少しだけ頑張ってみます。"}];
INSERT INTO sentence [{"id":{"d":579,"n":1,"t":2},"speaker":"A","text":"粘土で何を作っているのですか？"},{"id":{"d":579,"n":2,"t":2},"speaker":"B","text":"猫です。"},{"id":{"d":579,"n":3,"t":2},"speaker":"A","text":"おうちで飼っている猫ですか？"},{"id":{"d":579,"n":4,"t":2},"speaker":"B","text":"はい。名前はココです。"},{"id":{"d":579,"n":5,"t":2},"speaker":"A","text":"ココちゃんは、尻尾が長いのですね。"},{"id":{"d":579,"n":6,"t":2},"speaker":"B","text":"そうです。私はココの長い尻尾が大好きです。"},{"id":{"d":579,"n":7,"t":2},"speaker":"A","text":"本当に上手に作れていますね。"},{"id":{"d":579,"n":8,"t":2},"speaker":"B","text":"うれしいです。後はヒゲを付けたら完成です。"}];
INSERT INTO sentence [{"id":{"d":580,"n":1,"t":2},"speaker":"A","text":"学校の授業を怠けて欠席したことはありますか？"},{"id":{"d":580,"n":2,"t":2},"speaker":"B","text":"ありますよ。"},{"id":{"d":580,"n":3,"t":2},"speaker":"A","text":"いつですか？"},{"id":{"d":580,"n":4,"t":2},"speaker":"B","text":"大学生の時です。うっかりアルバイトの予定を入れてしまって、大学の授業を欠席しました。"},{"id":{"d":580,"n":5,"t":2},"speaker":"A","text":"授業よりアルバイトの予定を優先したのですか？"},{"id":{"d":580,"n":6,"t":2},"speaker":"B","text":"そのようなことになりますね。アルバイト先が人出不足だったので、「休む」と連絡することができませんでした。"},{"id":{"d":580,"n":7,"t":2},"speaker":"A","text":"欠席した授業の方は大丈夫でしたか？"},{"id":{"d":580,"n":8,"t":2},"speaker":"B","text":"後で友達にノートを貸してもらえたので、大丈夫でした。"}];
INSERT INTO sentence [{"id":{"d":581,"n":1,"t":2},"speaker":"A","text":"3日後に車の運転免許証の学科試験があります。"},{"id":{"d":581,"n":2,"t":2},"speaker":"B","text":"合格できそうですか？"},{"id":{"d":581,"n":3,"t":2},"speaker":"A","text":"それなりに勉強したのですが、少し不安です。"},{"id":{"d":581,"n":4,"t":2},"speaker":"B","text":"難しい問題は出ないので、大丈夫だと思いますよ。"},{"id":{"d":581,"n":5,"t":2},"speaker":"A","text":"昨日、模擬試験問題を解いてみたら、合格点の瀬戸際の点数でした。"},{"id":{"d":581,"n":6,"t":2},"speaker":"B","text":"合格点の瀬戸際の点数でも、合格点をクリアできたのですから自信をもってください。"},{"id":{"d":581,"n":7,"t":2},"speaker":"A","text":"はい。なるべく1回で合格したいです。"},{"id":{"d":581,"n":8,"t":2},"speaker":"B","text":"応援しています。頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":582,"n":1,"t":2},"speaker":"A","text":"今日は路上教習を行います。"},{"id":{"d":582,"n":2,"t":2},"speaker":"B","text":"初めてなので、緊張します。"},{"id":{"d":582,"n":3,"t":2},"speaker":"A","text":"焦らず、ゆっくり行きましょう。"},{"id":{"d":582,"n":4,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":582,"n":5,"t":2},"speaker":"A","text":"実際の道路での運転なので、道路標識などには十分気を付けてください。"},{"id":{"d":582,"n":6,"t":2},"speaker":"B","text":"はい。コースは先生から指示していただけるのでしょうか？"},{"id":{"d":582,"n":7,"t":2},"speaker":"A","text":"そうですね。こちらから、その都度お伝えします。"},{"id":{"d":582,"n":8,"t":2},"speaker":"B","text":"安心しました。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":583,"n":1,"t":2},"speaker":"A","text":"鉛筆の落とし物があります。この鉛筆は誰のものですか？"},{"id":{"d":583,"n":2,"t":2},"speaker":"B","text":"僕のものです。"},{"id":{"d":583,"n":3,"t":2},"speaker":"A","text":"清水君ですか。取りに来てください。"},{"id":{"d":583,"n":4,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":583,"n":5,"t":2},"speaker":"A","text":"はい、どうぞ。小早川さんが見つけて届けてくれましたよ。"},{"id":{"d":583,"n":6,"t":2},"speaker":"B","text":"後で小早川さんにお礼を言います。"},{"id":{"d":583,"n":7,"t":2},"speaker":"A","text":"それから、持ち物には名前を書いておいてくださいね。"},{"id":{"d":583,"n":8,"t":2},"speaker":"B","text":"分かりました。すみません。"}];
INSERT INTO sentence [{"id":{"d":584,"n":1,"t":2},"speaker":"A","text":"先生、三谷さんがいません。どうしたのですか？"},{"id":{"d":584,"n":2,"t":2},"speaker":"B","text":"三谷さんは今日、風邪でお休みです。"},{"id":{"d":584,"n":3,"t":2},"speaker":"A","text":"今日の日直は三谷さんなのですが、どうしましょう？"},{"id":{"d":584,"n":4,"t":2},"speaker":"B","text":"おお、そうでしたか。それでは、学級委員長の山田さんに日直の仕事を頼んでみますね。"},{"id":{"d":584,"n":5,"t":2},"speaker":"A","text":"山田さんは今日、部活の試合があって公欠です。"},{"id":{"d":584,"n":6,"t":2},"speaker":"B","text":"そうでした。困りましたね。"},{"id":{"d":584,"n":7,"t":2},"speaker":"A","text":"もしよければ僕が代わりにやりますよ。"},{"id":{"d":584,"n":8,"t":2},"speaker":"B","text":"ありがとうございます、助かります。お願いしますね。"},{"id":{"d":584,"n":9,"t":2},"speaker":"A","text":"今回、日直を引き受ける代わりに今度私が日直の時に、三谷さんに代わってもらってよいですか？"},{"id":{"d":584,"n":10,"t":2},"speaker":"B","text":"はい。そうしてください。"}];
INSERT INTO sentence [{"id":{"d":585,"n":1,"t":2},"speaker":"A","text":"青葉高校と鶴岡高校のどちらを受験するか迷っています。"},{"id":{"d":585,"n":2,"t":2},"speaker":"B","text":"植木さんはダンス部ですよね。高校でもダンスを続けますか？"},{"id":{"d":585,"n":3,"t":2},"speaker":"A","text":"そのつもりです。"},{"id":{"d":585,"n":4,"t":2},"speaker":"B","text":"青葉高校にはダンス部がありませんが、鶴岡高校だとあります。"},{"id":{"d":585,"n":5,"t":2},"speaker":"A","text":"ただ、部活で志望校を決めていいものかどうか考えてしまいます。"},{"id":{"d":585,"n":6,"t":2},"speaker":"B","text":"校風が自分に合っているかどうかも大事ですね。"},{"id":{"d":585,"n":7,"t":2},"speaker":"A","text":"オープンキャンパスに行ったのですが、どちらの校風も素晴らしいと感じました。"},{"id":{"d":585,"n":8,"t":2},"speaker":"B","text":"この中学の卒業生で、それぞれの高校に進学した先輩に話を聞いてみてはどうでしょうか？"},{"id":{"d":585,"n":9,"t":2},"speaker":"A","text":"そうですね。話を聞けそうな先輩を探してみます。"}];
INSERT INTO sentence [{"id":{"d":586,"n":1,"t":2},"speaker":"A","text":"まずは、本校を志望した理由を教えてください。"},{"id":{"d":586,"n":2,"t":2},"speaker":"B","text":"私の兄が御校に在籍していて、とても充実した学校生活を送っているので、自分も同じ学校に進学したいと思いました。"},{"id":{"d":586,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。では、次に、中学校で頑張ったことを教えてください。"},{"id":{"d":586,"n":4,"t":2},"speaker":"B","text":"修学旅行の実行委員になり、生徒みんなの意見を取り入れながら修学旅行の計画を立てたことです。"},{"id":{"d":586,"n":5,"t":2},"speaker":"A","text":"実行委員として大変なことはありましたか？"},{"id":{"d":586,"n":6,"t":2},"speaker":"B","text":"はい。みんなの意見が割れてしまった点について、調整したり話し合ってとりまとめることは大変でした。"},{"id":{"d":586,"n":7,"t":2},"speaker":"A","text":"そうでしたか。本校に入学したら、何部に入りたいですか？"},{"id":{"d":586,"n":8,"t":2},"speaker":"B","text":"中学校では吹奏楽部に所属していたので、引き続き吹奏楽部に入りたいです。"}];
INSERT INTO sentence [{"id":{"d":587,"n":1,"t":2},"speaker":"A","text":"八代大学への推薦入試を希望しているのですが、私の成績で合格できるでしょうか？"},{"id":{"d":587,"n":2,"t":2},"speaker":"B","text":"学部によります。あなたは何学部を志望していますか？"},{"id":{"d":587,"n":3,"t":2},"speaker":"A","text":"経営学部です。"},{"id":{"d":587,"n":4,"t":2},"speaker":"B","text":"経営学部なら、あなたの内申点で十分に合格の可能性がありますよ。"},{"id":{"d":587,"n":5,"t":2},"speaker":"A","text":"内申点の他に、何か条件はありますか？"},{"id":{"d":587,"n":6,"t":2},"speaker":"B","text":"条件ではありませんが、部活や委員会での活動が評価されるようです。"},{"id":{"d":587,"n":7,"t":2},"speaker":"A","text":"それなら、私は部活では部長ですし生活委員としても積極的に取り組んでいます。"},{"id":{"d":587,"n":8,"t":2},"speaker":"B","text":"そうですね。申し分ないと思います。"}];
INSERT INTO sentence [{"id":{"d":588,"n":1,"t":2},"speaker":"A","text":"先生、湖南高校の面接試験はどのような感じなのか教えてください。"},{"id":{"d":588,"n":2,"t":2},"speaker":"B","text":"もちろんです。先生の分かる範囲で良ければですが。"},{"id":{"d":588,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。面接は1対1でしょうか？"},{"id":{"d":588,"n":4,"t":2},"speaker":"B","text":"生徒は1名ですが、面接官は2名です。"},{"id":{"d":588,"n":5,"t":2},"speaker":"A","text":"面接時間はどのくらいでしょうか？"},{"id":{"d":588,"n":6,"t":2},"speaker":"B","text":"5分かからないくらいです。"},{"id":{"d":588,"n":7,"t":2},"speaker":"A","text":"意外と短いのですね。"},{"id":{"d":588,"n":8,"t":2},"speaker":"B","text":"受験生全員と面接をするので、1人当たりの時間は短いです。"}];
INSERT INTO sentence [{"id":{"d":589,"n":1,"t":2},"speaker":"A","text":"先輩は大学の何年生の時に就職活動を始めましたか？"},{"id":{"d":589,"n":2,"t":2},"speaker":"B","text":"大学3年の時です。"},{"id":{"d":589,"n":3,"t":2},"speaker":"A","text":"その前は、何か準備していましたか？"},{"id":{"d":589,"n":4,"t":2},"speaker":"B","text":"自分がどのような業種に就職したいかを考えたり、興味のある企業についてインターネットで調べたりしていました。"},{"id":{"d":589,"n":5,"t":2},"speaker":"A","text":"大学3先生になってからは、どのようなふうに就職活動を始めたのでしょうか？"},{"id":{"d":589,"n":6,"t":2},"speaker":"B","text":"会社説明会に参加したり、インターンに応募したりしましたね。"},{"id":{"d":589,"n":7,"t":2},"speaker":"A","text":"私は来年3年生になるのでとても不安なのですが、何かアドバイスをいただけないでしょうか？"},{"id":{"d":589,"n":8,"t":2},"speaker":"B","text":"数多くの企業に応募すると、断られることも多くなりますが、その度に落ち込んでいると心がもたないので、うまく気持ちを切り替える術を身に着けてほしいです。"},{"id":{"d":589,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。心に留めておきます。"}];
INSERT INTO sentence [{"id":{"d":590,"n":1,"t":2},"speaker":"A","text":"成美ちゃん、今日の放課後遊びませんか？"},{"id":{"d":590,"n":2,"t":2},"speaker":"B","text":"ごめんなさい。今日は塾へ行く予定があるのです。"},{"id":{"d":590,"n":3,"t":2},"speaker":"A","text":"そうですか。成美ちゃんは塾に通っているのですね？"},{"id":{"d":590,"n":4,"t":2},"speaker":"B","text":"はい。先月から通い始めました。"},{"id":{"d":590,"n":5,"t":2},"speaker":"A","text":"もしかして、中学校を受験するからですか？"},{"id":{"d":590,"n":6,"t":2},"speaker":"B","text":"はい。塾で私立の中学校の入学試験のための勉強をしています。"},{"id":{"d":590,"n":7,"t":2},"speaker":"A","text":"大変そうですね。"},{"id":{"d":590,"n":8,"t":2},"speaker":"B","text":"はい、宿題もたくさんあるのでかなり大変です。でも合格したいので頑張ります。"}];
INSERT INTO sentence [{"id":{"d":591,"n":1,"t":2},"speaker":"A","text":"日本語がお上手ですね。"},{"id":{"d":591,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。語学学校で勉強しました。"},{"id":{"d":591,"n":3,"t":2},"speaker":"A","text":"何年間くらい通ったのですか？"},{"id":{"d":591,"n":4,"t":2},"speaker":"B","text":"1年半です。"},{"id":{"d":591,"n":5,"t":2},"speaker":"A","text":"たった1年半の勉強でそれほど上手に話せるとは、驚きです。"},{"id":{"d":591,"n":6,"t":2},"speaker":"B","text":"日本でホームステイをしていたので、そのことも上達に繋がったと思います。"},{"id":{"d":591,"n":7,"t":2},"speaker":"A","text":"ホストファミリーとは、ずっと日本語で話していたのですか？"},{"id":{"d":591,"n":8,"t":2},"speaker":"B","text":"そうですね。しかも、私が日本語の使い方を間違えると、丁寧に指摘してくれたのでありがたかったです。"},{"id":{"d":591,"n":9,"t":2},"speaker":"A","text":"良い人に巡り会えたのですね。"},{"id":{"d":591,"n":10,"t":2},"speaker":"B","text":"そう思います。彼らには感謝しかありません。"}];
INSERT INTO sentence [{"id":{"d":592,"n":1,"t":2},"speaker":"A","text":"卒業論文の執筆がなかなか進みません。"},{"id":{"d":592,"n":2,"t":2},"speaker":"B","text":"私も同じです。ようやく半分くらい仕上がりました。"},{"id":{"d":592,"n":3,"t":2},"speaker":"A","text":"私はまだ序盤です。参考文献を読むことに時間がかかってしまいます。"},{"id":{"d":592,"n":4,"t":2},"speaker":"B","text":"あなたはフランス文学科なので、参考文献もフランス語のものが多いのですか？"},{"id":{"d":592,"n":5,"t":2},"speaker":"A","text":"はい。ほとんどがフランス語で書かれています。"},{"id":{"d":592,"n":6,"t":2},"speaker":"B","text":"それでは時間がかかるのも当然ですよね。私は経済学部なので、参考文献はほとんど日本語です。"},{"id":{"d":592,"n":7,"t":2},"speaker":"A","text":"なるほど。そこが大きな違いですね。"},{"id":{"d":592,"n":8,"t":2},"speaker":"B","text":"でも苦労した分、自分のためになると思うので、お互いに頑張りましょう。"},{"id":{"d":592,"n":9,"t":2},"speaker":"A","text":"そうですね。頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":593,"n":1,"t":2},"speaker":"A","text":"私は大学を卒業したら、大学院へ進みたいです。"},{"id":{"d":593,"n":2,"t":2},"speaker":"B","text":"大学院へ進むには、試験があるのですか？"},{"id":{"d":593,"n":3,"t":2},"speaker":"A","text":"はい。試験もありますし、面接もあります。"},{"id":{"d":593,"n":4,"t":2},"speaker":"B","text":"そうなのですか。試験は難しそうですね。"},{"id":{"d":593,"n":5,"t":2},"speaker":"A","text":"どの程度難しいのか見当が付かないので、試験内容について、大学院にいる先輩に話を聞こうと思っています。"},{"id":{"d":593,"n":6,"t":2},"speaker":"B","text":"それは参考になりますね。面接についても教えてもらえると良いですね。"},{"id":{"d":593,"n":7,"t":2},"speaker":"A","text":"はい。体験談をいろいろと聞かせてもらうつもりです。"},{"id":{"d":593,"n":8,"t":2},"speaker":"B","text":"試験も面接も無事に合格できることを祈っています。"},{"id":{"d":593,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":594,"n":1,"t":2},"speaker":"A","text":"先輩、私はこの会社に就職を希望しているのですが、実際に働いている感想を聞かせてください。"},{"id":{"d":594,"n":2,"t":2},"speaker":"B","text":"この会社は、忙しい時期と余裕のある時期の差が激しいです。"},{"id":{"d":594,"n":3,"t":2},"speaker":"A","text":"忙しい時期は、いつごろですか？"},{"id":{"d":594,"n":4,"t":2},"speaker":"B","text":"毎月の月末です。"},{"id":{"d":594,"n":5,"t":2},"speaker":"A","text":"その時期は、やはり残業が多くなるのでしょうか？"},{"id":{"d":594,"n":6,"t":2},"speaker":"B","text":"そうですね。ただ、残業する時間は必要最低限ですし、残業代はきちんと支給されます。"},{"id":{"d":594,"n":7,"t":2},"speaker":"A","text":"先輩は今の仕事に不満はありますか？"},{"id":{"d":594,"n":8,"t":2},"speaker":"B","text":"全くないとは言えませんが、改善してほしい点は上司に相談できる雰囲気がありますし、総合的には満足しています。"}];
INSERT INTO sentence [{"id":{"d":595,"n":1,"t":2},"speaker":"A","text":"どのような目的で英会話を学習したいのですか？"},{"id":{"d":595,"n":2,"t":2},"speaker":"B","text":"英国へ一人旅をしたいと思っています。そのために困らない程度の英会話を身に付けたいです。"},{"id":{"d":595,"n":3,"t":2},"speaker":"A","text":"それでは、個人レッスンでマンツーマン指導が最適だと思います。"},{"id":{"d":595,"n":4,"t":2},"speaker":"B","text":"それだと、どのくらいの頻度で通うことになりますか？"},{"id":{"d":595,"n":5,"t":2},"speaker":"A","text":"個人レッスンなので、全て生徒さんのご希望でスケジュールを組むことができます。"},{"id":{"d":595,"n":6,"t":2},"speaker":"B","text":"それは良いですね。レッスンの内容も、自由ということですよね。"},{"id":{"d":595,"n":7,"t":2},"speaker":"A","text":"はい。ご要望に応じて柔軟に対応させていただきます。"},{"id":{"d":595,"n":8,"t":2},"speaker":"B","text":"講師の指定をすることは可能ですか？"},{"id":{"d":595,"n":9,"t":2},"speaker":"A","text":"可能ですが、レッスン料とは別に指名料がかかります。"}];
INSERT INTO sentence [{"id":{"d":596,"n":1,"t":2},"speaker":"A","text":"今月は大学のレポートの作成に追われています。"},{"id":{"d":596,"n":2,"t":2},"speaker":"B","text":"それは大変ですね。提出期限はいつですか？"},{"id":{"d":596,"n":3,"t":2},"speaker":"A","text":"来週の水曜までのものが1本、今月末までのものが2本です。"},{"id":{"d":596,"n":4,"t":2},"speaker":"B","text":"全部で3本もあるのですね。"},{"id":{"d":596,"n":5,"t":2},"speaker":"A","text":"今月中はそうです。ただ、その他に来月に提出するものが1本あります。"},{"id":{"d":596,"n":6,"t":2},"speaker":"B","text":"それは厳しいですね。"},{"id":{"d":596,"n":7,"t":2},"speaker":"A","text":"しかも、1本1本のレポートの枚数が大量なのです。"},{"id":{"d":596,"n":8,"t":2},"speaker":"B","text":"寝る時間を削って勉強することになりそうですね。"}];
INSERT INTO sentence [{"id":{"d":597,"n":1,"t":2},"speaker":"A","text":"筆箱を家に忘れてきてしまいました。"},{"id":{"d":597,"n":2,"t":2},"speaker":"B","text":"筆記用具を貸しましょうか？"},{"id":{"d":597,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。借りても大丈夫ですか？"},{"id":{"d":597,"n":4,"t":2},"speaker":"B","text":"はい。筆記用具は割とたくさん持ち歩いているので、大丈夫ですよ。"},{"id":{"d":597,"n":5,"t":2},"speaker":"A","text":"それではお借りしたいです。すみません。"},{"id":{"d":597,"n":6,"t":2},"speaker":"B","text":"シャープペンシルと消しゴムをお貸しすれば良いですか？"},{"id":{"d":597,"n":7,"t":2},"speaker":"A","text":"はい。とても助かります、ありがとうございます。明日、必ず返しますね。"},{"id":{"d":597,"n":8,"t":2},"speaker":"B","text":"お役に立てて良かったです。"}];
INSERT INTO sentence [{"id":{"d":598,"n":1,"t":2},"speaker":"A","text":"失礼します。"},{"id":{"d":598,"n":2,"t":2},"speaker":"B","text":"どうぞ。どうしましたか？"},{"id":{"d":598,"n":3,"t":2},"speaker":"A","text":"体育の授業で跳び箱を跳んだ時に、足をひねってしまいました。"},{"id":{"d":598,"n":4,"t":2},"speaker":"B","text":"そこの椅子に座って、足を見せてください。"},{"id":{"d":598,"n":5,"t":2},"speaker":"A","text":"はい。この辺が痛いです。"},{"id":{"d":598,"n":6,"t":2},"speaker":"B","text":"少し腫れているようなので、まずは冷やしましょう。"},{"id":{"d":598,"n":7,"t":2},"speaker":"A","text":"はい。お願いします。"},{"id":{"d":598,"n":8,"t":2},"speaker":"B","text":"湿布を貼っておきますので、安静にしてください。痛みや腫れがひどくなるようだったら、病院に行ってくださいね。"},{"id":{"d":598,"n":9,"t":2},"speaker":"A","text":"分かりました。どうもありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":599,"n":1,"t":2},"speaker":"A","text":"先生、こんにちは。今日もよろしくお願いします。"},{"id":{"d":599,"n":2,"t":2},"speaker":"B","text":"こんにちは。こちらこそよろしく。課題にした曲は、しっかり練習しましたか？"},{"id":{"d":599,"n":3,"t":2},"speaker":"A","text":"はい。頑張って練習しましたが、どうしても弾けないところがあります。"},{"id":{"d":599,"n":4,"t":2},"speaker":"B","text":"楽譜のどの部分ですか？"},{"id":{"d":599,"n":5,"t":2},"speaker":"A","text":"ここです。"},{"id":{"d":599,"n":6,"t":2},"speaker":"B","text":"片手ずつ練習してみましたか？"},{"id":{"d":599,"n":7,"t":2},"speaker":"A","text":"はい。片手ずつだとゆっくりなら弾けます。"},{"id":{"d":599,"n":8,"t":2},"speaker":"B","text":"そうですか。では、少し弾いてみてください。"}];
INSERT INTO sentence [{"id":{"d":600,"n":1,"t":2},"speaker":"A","text":"一緒に部活に行きましょう。"},{"id":{"d":600,"n":2,"t":2},"speaker":"B","text":"ごめんなさい、今日は部活を休みます。"},{"id":{"d":600,"n":3,"t":2},"speaker":"A","text":"何か用事があるのですか？"},{"id":{"d":600,"n":4,"t":2},"speaker":"B","text":"はい。歯医者に行くのです。"},{"id":{"d":600,"n":5,"t":2},"speaker":"A","text":"そうですか。休むことを顧問の先生に連絡しましたか？"},{"id":{"d":600,"n":6,"t":2},"speaker":"B","text":"いいえ。言い忘れていて、まだ連絡できていません。"},{"id":{"d":600,"n":7,"t":2},"speaker":"A","text":"では、私から連絡しておきますよ。"},{"id":{"d":600,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。すみませんが、部長にも伝えてください。"},{"id":{"d":600,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":601,"n":1,"t":2},"speaker":"A","text":"片付けをしていたら、中学校の卒業アルバムが出てきました。"},{"id":{"d":601,"n":2,"t":2},"speaker":"B","text":"見たいです。ママはどこに写っていますか？"},{"id":{"d":601,"n":3,"t":2},"speaker":"A","text":"7組だったので、このページにいますよ。"},{"id":{"d":601,"n":4,"t":2},"speaker":"B","text":"どれがママですか？見つかりません。"},{"id":{"d":601,"n":5,"t":2},"speaker":"A","text":"苗字が旧姓の「岡本」の子です。"},{"id":{"d":601,"n":6,"t":2},"speaker":"B","text":"これですね。当たり前ですが、若いですね。"},{"id":{"d":601,"n":7,"t":2},"speaker":"A","text":"そうでしょうね。あなたと似ているでしょうか？"},{"id":{"d":601,"n":8,"t":2},"speaker":"B","text":"どうでしょうか？自分ではよく分かりません。"}];
INSERT INTO sentence [{"id":{"d":602,"n":1,"t":2},"speaker":"A","text":"今日のレッスンはここまでにしましょう。"},{"id":{"d":602,"n":2,"t":2},"speaker":"B","text":"ありがとうございました。"},{"id":{"d":602,"n":3,"t":2},"speaker":"A","text":"また来週、お待ちしていますね。"},{"id":{"d":602,"n":4,"t":2},"speaker":"B","text":"すみません、来週は都合が悪いので、曜日を変えていただけないでしょうか？"},{"id":{"d":602,"n":5,"t":2},"speaker":"A","text":"はい、構いませんよ。何曜日の都合が良いですか？"},{"id":{"d":602,"n":6,"t":2},"speaker":"B","text":"水曜日か金曜日にしていただけるとありがたいです。"},{"id":{"d":602,"n":7,"t":2},"speaker":"A","text":"それでは、金曜日の18時からはどうでしょうか？"},{"id":{"d":602,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。はい、金曜日18時からでお願いします。"}];
INSERT INTO sentence [{"id":{"d":603,"n":1,"t":2},"speaker":"A","text":"学校ではどのようなあだ名で呼ばれていますか？"},{"id":{"d":603,"n":2,"t":2},"speaker":"B","text":"私の学校はあだ名で呼び合ってはいけないという規則があるので、あだ名では呼ばれません。"},{"id":{"d":603,"n":3,"t":2},"speaker":"A","text":"そうでしたか。ではどのようなふうに呼び合っているのですか？"},{"id":{"d":603,"n":4,"t":2},"speaker":"B","text":"下の名前に「さん」を付けています。"},{"id":{"d":603,"n":5,"t":2},"speaker":"A","text":"学年を超えた場合もそうするのですか？"},{"id":{"d":603,"n":6,"t":2},"speaker":"B","text":"はい。私の場合は、先輩からも後輩からも「真奈美さん」と呼ばれています。"},{"id":{"d":603,"n":7,"t":2},"speaker":"A","text":"先生からもですか？"},{"id":{"d":603,"n":8,"t":2},"speaker":"B","text":"そうです。珍しいでしょうか？"},{"id":{"d":603,"n":9,"t":2},"speaker":"A","text":"はい。少なくとも私は初めて聞きました。"}];
INSERT INTO sentence [{"id":{"d":604,"n":1,"t":2},"speaker":"A","text":"こんばんは。悠馬のお迎えに来ました。"},{"id":{"d":604,"n":2,"t":2},"speaker":"B","text":"こんばんは。悠馬君は支度をしていますので、少しお待ちくださいね。"},{"id":{"d":604,"n":3,"t":2},"speaker":"A","text":"はい。今日、悠馬はどうでしたか？"},{"id":{"d":604,"n":4,"t":2},"speaker":"B","text":"お昼休みに園庭でかけっこをしたのですが、悠馬君はとても足が速かったですよ。"},{"id":{"d":604,"n":5,"t":2},"speaker":"A","text":"そうですか。いつも走り回っているせいかもしれません。"},{"id":{"d":604,"n":6,"t":2},"speaker":"B","text":"今日は折り紙もしましたが、とても上手に作れていましたよ。"},{"id":{"d":604,"n":7,"t":2},"speaker":"A","text":"折り紙が大好きなので、きっと楽しかったことでしょう。"},{"id":{"d":604,"n":8,"t":2},"speaker":"B","text":"できた折り紙は荷物の中に入っていますので、おうちで見てあげてください。"},{"id":{"d":604,"n":9,"t":2},"speaker":"A","text":"ありがとうございます、そうします。"}];
INSERT INTO sentence [{"id":{"d":605,"n":1,"t":2},"speaker":"A","text":"先生、今日のプールの授業は見学します。"},{"id":{"d":605,"n":2,"t":2},"speaker":"B","text":"分かりました。理由を聞いてもよいですか？"},{"id":{"d":605,"n":3,"t":2},"speaker":"A","text":"中耳炎になってしまって、お医者さんから「プールに入らないように」と言われました。"},{"id":{"d":605,"n":4,"t":2},"speaker":"B","text":"分かりました。普段はプールサイドで見学していてもらうのですが、今日は猛暑で熱中症が心配なので教室で過ごしてもらいます。"},{"id":{"d":605,"n":5,"t":2},"speaker":"A","text":"分かりました。"},{"id":{"d":605,"n":6,"t":2},"speaker":"B","text":"3組の小倉さんもプールに入らないので、一緒にこの教室でこのプリントを自習していてください。"},{"id":{"d":605,"n":7,"t":2},"speaker":"A","text":"はい。後で小倉さんにも声を掛けておきます。"},{"id":{"d":605,"n":8,"t":2},"speaker":"B","text":"お願いします。自習した内容は、後で提出してください。"}];
INSERT INTO sentence [{"id":{"d":606,"n":1,"t":2},"speaker":"A","text":"韓国語を勉強しようと思ったきっかけは何ですか？"},{"id":{"d":606,"n":2,"t":2},"speaker":"B","text":"韓国の歌手が大好きで、そこから韓国自体にも興味を持ったことがきっかけです。"},{"id":{"d":606,"n":3,"t":2},"speaker":"A","text":"勉強を始めてみてどうですか？"},{"id":{"d":606,"n":4,"t":2},"speaker":"B","text":"楽しいですよ。でも、発音や文法で難しい部分があります。"},{"id":{"d":606,"n":5,"t":2},"speaker":"A","text":"どのようにして勉強しているのですか？"},{"id":{"d":606,"n":6,"t":2},"speaker":"B","text":"韓国語を教えてくれる教室に通っています。"},{"id":{"d":606,"n":7,"t":2},"speaker":"A","text":"頑張っていますね。"},{"id":{"d":606,"n":8,"t":2},"speaker":"B","text":"はい。韓国へ一人旅ができるくらいに上達したいです。"}];
INSERT INTO sentence [{"id":{"d":607,"n":1,"t":2},"speaker":"A","text":"次の日曜日に部活で練習試合があります。"},{"id":{"d":607,"n":2,"t":2},"speaker":"B","text":"会場はどこですか？"},{"id":{"d":607,"n":3,"t":2},"speaker":"A","text":"月見野高校です。"},{"id":{"d":607,"n":4,"t":2},"speaker":"B","text":"遠いですね。車で送って行きますよ。"},{"id":{"d":607,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。後輩の未来ちゃんも一緒に送ってもらってもよいですか？"},{"id":{"d":607,"n":6,"t":2},"speaker":"B","text":"よいですが、何か事情があるのですか？"},{"id":{"d":607,"n":7,"t":2},"speaker":"A","text":"未来ちゃんはご両親が日曜日も働いていて、月見野高校まで送ってもらえないそうなのです。"},{"id":{"d":607,"n":8,"t":2},"speaker":"B","text":"そうですか。それでは、行きも帰りも未来ちゃんを乗せて行きましょう。"}];
INSERT INTO sentence [{"id":{"d":608,"n":1,"t":2},"speaker":"A","text":"授業を始める前に、先週塾で行ったテストを返します。三上さん、取りに来てください。"},{"id":{"d":608,"n":2,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":608,"n":3,"t":2},"speaker":"A","text":"三上さんは数学が95点でした。前回より随分点数が上がりましたね。"},{"id":{"d":608,"n":4,"t":2},"speaker":"B","text":"前回の点数が悪かったので、たくさん勉強しました。"},{"id":{"d":608,"n":5,"t":2},"speaker":"A","text":"素晴らしいですよ。間違った問題は、これとこれです。どうして間違えたか分かりますか？"},{"id":{"d":608,"n":6,"t":2},"speaker":"B","text":"はい。途中の計算でプラスとマイナスを逆にしてしまいました。"},{"id":{"d":608,"n":7,"t":2},"speaker":"A","text":"そうですね。もったいない間違えなので、気を付けましょう。"},{"id":{"d":608,"n":8,"t":2},"speaker":"B","text":"はい。次回はケアレスミスをしないようにします。"}];
INSERT INTO sentence [{"id":{"d":609,"n":1,"t":2},"speaker":"A","text":"卒業した高校は公立でしたか？私立でしたか？"},{"id":{"d":609,"n":2,"t":2},"speaker":"B","text":"私は高校に行っていません。"},{"id":{"d":609,"n":3,"t":2},"speaker":"A","text":"でも、大学を卒業したと伺いました。"},{"id":{"d":609,"n":4,"t":2},"speaker":"B","text":"高卒認定試験に合格して、大学を受験したのです。"},{"id":{"d":609,"n":5,"t":2},"speaker":"A","text":"そうだったのですね。高卒認定試験は難しかったですか？"},{"id":{"d":609,"n":6,"t":2},"speaker":"B","text":"はい。私にとっては難しかったです。一生懸命に勉強しました。"},{"id":{"d":609,"n":7,"t":2},"speaker":"A","text":"合格したら、すぐに大学を受験したのですか？"},{"id":{"d":609,"n":8,"t":2},"speaker":"B","text":"そうです。だからその時期は勉強漬けでした。"}];
INSERT INTO sentence [{"id":{"d":610,"n":1,"t":2},"speaker":"A","text":"先生、1年間お世話になりました。"},{"id":{"d":610,"n":2,"t":2},"speaker":"B","text":"谷さん、水泳部の厳しい練習をよく頑張りましたね。"},{"id":{"d":610,"n":3,"t":2},"speaker":"A","text":"来年も先生に顧問をしていただきたかったです。"},{"id":{"d":610,"n":4,"t":2},"speaker":"B","text":"先生もそうしたいのですが、転勤なので仕方がありません。"},{"id":{"d":610,"n":5,"t":2},"speaker":"A","text":"どこの中学校へ転勤するのですか？"},{"id":{"d":610,"n":6,"t":2},"speaker":"B","text":"白川第二中学校です。"},{"id":{"d":610,"n":7,"t":2},"speaker":"A","text":"また、水泳部の顧問をされるのですか？"},{"id":{"d":610,"n":8,"t":2},"speaker":"B","text":"そのように希望するつもりです。大会などで会えるかもしれませんね。"},{"id":{"d":610,"n":9,"t":2},"speaker":"A","text":"それを楽しみに、これからも練習を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":611,"n":1,"t":2},"speaker":"A","text":"あなたの志望動機を教えてください。"},{"id":{"d":611,"n":2,"t":2},"speaker":"B","text":"御社の自由な社風と将来性に魅力を感じました。"},{"id":{"d":611,"n":3,"t":2},"speaker":"A","text":"もし弊社に入ったら、どのような仕事をしたいですか？"},{"id":{"d":611,"n":4,"t":2},"speaker":"B","text":"新しいプロジェクトの企画に携わりたいです。"},{"id":{"d":611,"n":5,"t":2},"speaker":"A","text":"企画の立案に興味があるのですね？"},{"id":{"d":611,"n":6,"t":2},"speaker":"B","text":"はい。アイデアを出すことが得意です。"},{"id":{"d":611,"n":7,"t":2},"speaker":"A","text":"それは素晴らしいです。何か具体的なエピソードはありますか？"},{"id":{"d":611,"n":8,"t":2},"speaker":"B","text":"大学では、学園祭をよりよく運営できるようにアイデアを出して、実際に採用されました。"}];
INSERT INTO sentence [{"id":{"d":612,"n":1,"t":2},"speaker":"A","text":"体力テストの結果はどうでしたか？"},{"id":{"d":612,"n":2,"t":2},"speaker":"B","text":"結果が良い種目と悪い種目と、どちらもあります。"},{"id":{"d":612,"n":3,"t":2},"speaker":"A","text":"結果が良かったのはどの種目ですか？"},{"id":{"d":612,"n":4,"t":2},"speaker":"B","text":"握力と反復横跳びと持久走です。"},{"id":{"d":612,"n":5,"t":2},"speaker":"A","text":"得点はどのくらいでしたか？"},{"id":{"d":612,"n":6,"t":2},"speaker":"B","text":"10点中、8点か9点です。"},{"id":{"d":612,"n":7,"t":2},"speaker":"A","text":"それは高得点ですね。素晴らしいことです。"},{"id":{"d":612,"n":8,"t":2},"speaker":"B","text":"その代わり、50メートル走とシャトルランと上体起こしは全部3点以下です。"},{"id":{"d":612,"n":9,"t":2},"speaker":"A","text":"得点の差が大きいですね。"}];
INSERT INTO sentence [{"id":{"d":613,"n":1,"t":2},"speaker":"A","text":"朝陽君のお兄さんは、2歳上ですよね？"},{"id":{"d":613,"n":2,"t":2},"speaker":"B","text":"はい。高校3年生です。"},{"id":{"d":613,"n":3,"t":2},"speaker":"A","text":"私たちと同じ高校ですか？"},{"id":{"d":613,"n":4,"t":2},"speaker":"B","text":"いいえ。兄は矢上高校に通っています。"},{"id":{"d":613,"n":5,"t":2},"speaker":"A","text":"矢上高校はとても偏差値の高い高校ですね。"},{"id":{"d":613,"n":6,"t":2},"speaker":"B","text":"兄は小さい頃から優秀で、成績も良いのです。"},{"id":{"d":613,"n":7,"t":2},"speaker":"A","text":"そうなのですか。今は大学受験に向けて勉強中ですか？"},{"id":{"d":613,"n":8,"t":2},"speaker":"B","text":"はい。兄なら間違いなく希望の大学に進学すると思います。"}];
INSERT INTO sentence [{"id":{"d":614,"n":1,"t":2},"speaker":"A","text":"夏休みは何か予定がありますか？"},{"id":{"d":614,"n":2,"t":2},"speaker":"B","text":"はい。部活動と塾の夏期講習に行く予定があります。"},{"id":{"d":614,"n":3,"t":2},"speaker":"A","text":"塾の夏期講習は何日間くらいですか？"},{"id":{"d":614,"n":4,"t":2},"speaker":"B","text":"20日間です。"},{"id":{"d":614,"n":5,"t":2},"speaker":"A","text":"それは多いですね。"},{"id":{"d":614,"n":6,"t":2},"speaker":"B","text":"講習以外に宿題も出るので、結構大変です。"},{"id":{"d":614,"n":7,"t":2},"speaker":"A","text":"夏休みに一緒に遊びたいのですが、難しそうですね。"},{"id":{"d":614,"n":8,"t":2},"speaker":"B","text":"いえ、空いている日もあるので、ぜひ遊びましょう。"}];
INSERT INTO sentence [{"id":{"d":615,"n":1,"t":2},"speaker":"A","text":"高校では書道部でした。"},{"id":{"d":615,"n":2,"t":2},"speaker":"B","text":"書道を習っていたのですか？"},{"id":{"d":615,"n":3,"t":2},"speaker":"A","text":"いえ、一度も習ったことがありません。"},{"id":{"d":615,"n":4,"t":2},"speaker":"B","text":"それでは、どうして書道部にしようと思ったのですか？"},{"id":{"d":615,"n":5,"t":2},"speaker":"A","text":"書道部に関する映画を見て、高校では書道部に入ろうと決めました。"},{"id":{"d":615,"n":6,"t":2},"speaker":"B","text":"実際に入部して、どうでしたか？"},{"id":{"d":615,"n":7,"t":2},"speaker":"A","text":"期待以上でした。すっかり書道のとりこになりました。"},{"id":{"d":615,"n":8,"t":2},"speaker":"B","text":"今でも続けているのですか？"},{"id":{"d":615,"n":9,"t":2},"speaker":"A","text":"はい。時々ですが、自宅で書いています。"}];
INSERT INTO sentence [{"id":{"d":616,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題は終わりましたか？"},{"id":{"d":616,"n":2,"t":2},"speaker":"B","text":"いいえ。まだ終わっていません。"},{"id":{"d":616,"n":3,"t":2},"speaker":"A","text":"私も同じです。何の宿題が残っていますか？"},{"id":{"d":616,"n":4,"t":2},"speaker":"B","text":"読書感想文です。まだ本を読み終わっていません。"},{"id":{"d":616,"n":5,"t":2},"speaker":"A","text":"それは大変そうですね。"},{"id":{"d":616,"n":6,"t":2},"speaker":"B","text":"今日中に読み終わらないと大変なことになりそうなので頑張ります。あなたは何の宿題が残っているのですか？"},{"id":{"d":616,"n":7,"t":2},"speaker":"A","text":"私は自由研究です。"},{"id":{"d":616,"n":8,"t":2},"speaker":"B","text":"テーマは決まりましたか？"},{"id":{"d":616,"n":9,"t":2},"speaker":"A","text":"いえ、テーマさえ決まっていません。"},{"id":{"d":616,"n":10,"t":2},"speaker":"B","text":"それは相当これから頑張らなければいけませんね。"},{"id":{"d":616,"n":11,"t":2},"speaker":"A","text":"はい。夏休みはあと10日しかないので、お互いに頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":617,"n":1,"t":2},"speaker":"A","text":"明日は日直なので、気が重いです。"},{"id":{"d":617,"n":2,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":617,"n":3,"t":2},"speaker":"A","text":"私のクラスでは、日直は1分間スピーチをしなければなりません。"},{"id":{"d":617,"n":4,"t":2},"speaker":"B","text":"どのようなことを話すのですか？"},{"id":{"d":617,"n":5,"t":2},"speaker":"A","text":"自由です。何でもよいのですが、何を話すか悩んでいます。"},{"id":{"d":617,"n":6,"t":2},"speaker":"B","text":"最近読んだ本について話すのはどうでしょう？"},{"id":{"d":617,"n":7,"t":2},"speaker":"A","text":"おとといの日直がその話をしていました。"},{"id":{"d":617,"n":8,"t":2},"speaker":"B","text":"同じ話をしてはいけませんか？"},{"id":{"d":617,"n":9,"t":2},"speaker":"A","text":"別にそれでも構いませんが、なるべく違う話題にしたいです。"}];
INSERT INTO sentence [{"id":{"d":618,"n":1,"t":2},"speaker":"A","text":"大学生になったら、アルバイトをしたいです。"},{"id":{"d":618,"n":2,"t":2},"speaker":"B","text":"今までアルバイトをしたことがありませんか？"},{"id":{"d":618,"n":3,"t":2},"speaker":"A","text":"はい、一度もありません。"},{"id":{"d":618,"n":4,"t":2},"speaker":"B","text":"どのようなアルバイトをしてみたいですか？"},{"id":{"d":618,"n":5,"t":2},"speaker":"A","text":"引っ越し業者で働いてみたいと思っています。"},{"id":{"d":618,"n":6,"t":2},"speaker":"B","text":"力仕事なので体力的に大変そうですね。"},{"id":{"d":618,"n":7,"t":2},"speaker":"A","text":"体力には自信があるので大丈夫だと思います。"}];
INSERT INTO sentence [{"id":{"d":619,"n":1,"t":2},"speaker":"A","text":"あちらにランドセルが売られていますよ。"},{"id":{"d":619,"n":2,"t":2},"speaker":"B","text":"最近のランドセルは色が豊富ですね。赤色や黒色だけでなく、黄色や水色などのものもあるようです。"},{"id":{"d":619,"n":3,"t":2},"speaker":"A","text":"私は水色のランドセルでしたよ。"},{"id":{"d":619,"n":4,"t":2},"speaker":"B","text":"そうだったのですか。当時は珍しかったのではありませんか？"},{"id":{"d":619,"n":5,"t":2},"speaker":"A","text":"はい、学年で1人だけでした。でも姉が水色のランドセルだったので、自分も同じ色が良かったのです。あなたは何色のランドセルを使っていましたか？"},{"id":{"d":619,"n":6,"t":2},"speaker":"B","text":"私の使っていたランドセルは赤でした。"},{"id":{"d":619,"n":7,"t":2},"speaker":"A","text":"赤が好きだったのですか？"},{"id":{"d":619,"n":8,"t":2},"speaker":"B","text":"祖母が買って送ってくれたので、自分で選んだわけではありません。でもとても気に入っていましたよ。"}];
INSERT INTO sentence [{"id":{"d":620,"n":1,"t":2},"speaker":"A","text":"理科のテストの点数がとても悪かったです。"},{"id":{"d":620,"n":2,"t":2},"speaker":"B","text":"あなたは理科が得意なのに、珍しいですね。"},{"id":{"d":620,"n":3,"t":2},"speaker":"A","text":"今までで一番低い点数を取ってしまいました。"},{"id":{"d":620,"n":4,"t":2},"speaker":"B","text":"テストが難しかったのではありませんか？"},{"id":{"d":620,"n":5,"t":2},"speaker":"A","text":"そうかもしれません。"},{"id":{"d":620,"n":6,"t":2},"speaker":"B","text":"平均点は何点ですか？"},{"id":{"d":620,"n":7,"t":2},"speaker":"A","text":"先生が平均点は35点だと仰っていました。"},{"id":{"d":620,"n":8,"t":2},"speaker":"B","text":"それは低いですね。難しいテストだったようなので、点数が低くても仕方ないと思います。"}];
INSERT INTO sentence [{"id":{"d":621,"n":1,"t":2},"speaker":"A","text":"部活の和田先輩が来週誕生日だそうです。"},{"id":{"d":621,"n":2,"t":2},"speaker":"B","text":"そうですか。とてもお世話になっているので何かプレゼントを贈りたいですね。"},{"id":{"d":621,"n":3,"t":2},"speaker":"A","text":"私もそう思っていました。二人で一緒に何かプレゼントを贈りせんか？"},{"id":{"d":621,"n":4,"t":2},"speaker":"B","text":"良いですね。何を贈りましょうか？"},{"id":{"d":621,"n":5,"t":2},"speaker":"A","text":"部活の先輩なので、部活で使う物にしましょう。"},{"id":{"d":621,"n":6,"t":2},"speaker":"B","text":"デッサンに使う鉛筆はどうですか？"},{"id":{"d":621,"n":7,"t":2},"speaker":"A","text":"賛成です。"},{"id":{"d":621,"n":8,"t":2},"speaker":"B","text":"それでは今日の放課後、一緒に買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":622,"n":1,"t":2},"speaker":"A","text":"早く席替えをしたいです。"},{"id":{"d":622,"n":2,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":622,"n":3,"t":2},"speaker":"A","text":"今は一番後ろの席なのですが、黒板が見にくいのです。"},{"id":{"d":622,"n":4,"t":2},"speaker":"B","text":"眼が悪いのですか？"},{"id":{"d":622,"n":5,"t":2},"speaker":"A","text":"いえ、前の席の浦上君がとても背が高くて体も大きいので、前が見にくいのです。"},{"id":{"d":622,"n":6,"t":2},"speaker":"B","text":"それは困りますね。担任の先生に相談してみてはどうですか？"},{"id":{"d":622,"n":7,"t":2},"speaker":"A","text":"気が引けて言えませんが、言っても大丈夫でしょうか？"},{"id":{"d":622,"n":8,"t":2},"speaker":"B","text":"大丈夫でしょう。困っていることをきちんと伝えて相談するべきだと思います。"}];
INSERT INTO sentence [{"id":{"d":623,"n":1,"t":2},"speaker":"A","text":"本日は当校の説明会にお越しいただきありがとうございます。何か質問はございますか？"},{"id":{"d":623,"n":2,"t":2},"speaker":"B","text":"この専門学校では、授業は週に何時間くらいありますか？"},{"id":{"d":623,"n":3,"t":2},"speaker":"A","text":"コースによりますが、平均すると週に25時間くらいです。"},{"id":{"d":623,"n":4,"t":2},"speaker":"B","text":"男女比はどのくらいでしょうか？"},{"id":{"d":623,"n":5,"t":2},"speaker":"A","text":"男性と女性で6対4くらいです。"},{"id":{"d":623,"n":6,"t":2},"speaker":"B","text":"課題は多いですか？"},{"id":{"d":623,"n":7,"t":2},"speaker":"A","text":"それもコースによるのですが、提出期日は厳しくありませんよ。"},{"id":{"d":623,"n":8,"t":2},"speaker":"B","text":"分かりました。授業の見学ができると聞いたので、申し込みをしたいです。"},{"id":{"d":623,"n":9,"t":2},"speaker":"A","text":"かしこまりました。ご希望の日時を教えてください。"}];
INSERT INTO sentence [{"id":{"d":624,"n":1,"t":2},"speaker":"A","text":"哲也君はピアノの演奏が上手ですね。将来は音楽大学に進学するのですか？"},{"id":{"d":624,"n":2,"t":2},"speaker":"B","text":"音楽大学への進学はとても無理です。"},{"id":{"d":624,"n":3,"t":2},"speaker":"A","text":"音楽大学への進学はそれほどに難しいのですか？"},{"id":{"d":624,"n":4,"t":2},"speaker":"B","text":"入学することが難しいですし、何より学費がとても高額なのです。"},{"id":{"d":624,"n":5,"t":2},"speaker":"A","text":"知りませんでした。音楽大学に進学することは簡単ではありませんね。"},{"id":{"d":624,"n":6,"t":2},"speaker":"B","text":"はい。進学を考えたこともありましたが、学費のこともあり諦めました。"},{"id":{"d":624,"n":7,"t":2},"speaker":"A","text":"才能があるのに、もったいないように感じてしまいます。"},{"id":{"d":624,"n":8,"t":2},"speaker":"B","text":"音楽大学に進学しなくても、コンクールで受賞することなどを目標にするつもりです。"}];
INSERT INTO sentence [{"id":{"d":625,"n":1,"t":2},"speaker":"A","text":"美術学校ではどのような授業がありましたか？"},{"id":{"d":625,"n":2,"t":2},"speaker":"B","text":"私は絵画コースだったので、デッサンや油絵を実際に描く授業が多かったです。"},{"id":{"d":625,"n":3,"t":2},"speaker":"A","text":"きっと、みんな絵が上手なのでしょうね。"},{"id":{"d":625,"n":4,"t":2},"speaker":"B","text":"びっくりするくらい絵が上手な生徒がたくさんいました。"},{"id":{"d":625,"n":5,"t":2},"speaker":"A","text":"美術学校で描いた絵は残っていますか？"},{"id":{"d":625,"n":6,"t":2},"speaker":"B","text":"何点か取ってあります。"},{"id":{"d":625,"n":7,"t":2},"speaker":"A","text":"見せていただきたいです。"},{"id":{"d":625,"n":8,"t":2},"speaker":"B","text":"恥ずかしいですが、よいですよ。今、美術学校で描いた絵を持って来ます。"}];
INSERT INTO sentence [{"id":{"d":626,"n":1,"t":2},"speaker":"A","text":"合格発表は見ましたか？"},{"id":{"d":626,"n":2,"t":2},"speaker":"B","text":"はい。もう見ました。"},{"id":{"d":626,"n":3,"t":2},"speaker":"A","text":"どうでしたか？"},{"id":{"d":626,"n":4,"t":2},"speaker":"B","text":"合格していました。"},{"id":{"d":626,"n":5,"t":2},"speaker":"A","text":"おめでとう。私もうれしいです。"},{"id":{"d":626,"n":6,"t":2},"speaker":"B","text":"ありがとうございます。合格できてほっとしました。"},{"id":{"d":626,"n":7,"t":2},"speaker":"A","text":"今日まで本当によく頑張りましたね。"},{"id":{"d":626,"n":8,"t":2},"speaker":"B","text":"努力が報われてうれしいです。"}];
INSERT INTO sentence [{"id":{"d":627,"n":1,"t":2},"speaker":"A","text":"この参考書はとても役に立ちますよ。"},{"id":{"d":627,"n":2,"t":2},"speaker":"B","text":"内容が分かりやすいのですか？"},{"id":{"d":627,"n":3,"t":2},"speaker":"A","text":"はい。解説が詳しくて理解しやすいです。"},{"id":{"d":627,"n":4,"t":2},"speaker":"B","text":"解説文の中の単語に色が付いていますね。"},{"id":{"d":627,"n":5,"t":2},"speaker":"A","text":"重要度に応じて、色が付いているのです。"},{"id":{"d":627,"n":6,"t":2},"speaker":"B","text":"見やすく工夫されているのですね。"},{"id":{"d":627,"n":7,"t":2},"speaker":"A","text":"そうなのです。見てすぐにポイントが分かるようになっています。"},{"id":{"d":627,"n":8,"t":2},"speaker":"B","text":"ちょうど参考書が欲しいと思っていたところなので、私もその参考書を買おうと思います。"}];
INSERT INTO sentence [{"id":{"d":628,"n":1,"t":2},"speaker":"A","text":"サッカー部のマネージャーになりませんか？"},{"id":{"d":628,"n":2,"t":2},"speaker":"B","text":"サッカーの知識がないので、私にマネージャーは務まらないと思うのですが。"},{"id":{"d":628,"n":3,"t":2},"speaker":"A","text":"それは心配ありません。サッカーの知識はほとんど必要ありません。"},{"id":{"d":628,"n":4,"t":2},"speaker":"B","text":"そうなのですか？では、マネージャーはどのようなことをするのですか？"},{"id":{"d":628,"n":5,"t":2},"speaker":"A","text":"練習中に時間を計ったり、顧問の先生からの連絡事項をサッカー部員に伝えたりします。"},{"id":{"d":628,"n":6,"t":2},"speaker":"B","text":"それなら私にもできそうです。週に2日くらいでよければお引き受けできますよ。"},{"id":{"d":628,"n":7,"t":2},"speaker":"A","text":"十分です。ぜひお願いしたいです。"},{"id":{"d":628,"n":8,"t":2},"speaker":"B","text":"分かりました。よろしくお願いします。"},{"id":{"d":628,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。こちらこそよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":629,"n":1,"t":2},"speaker":"A","text":"看護専門学校は大変ですか？"},{"id":{"d":629,"n":2,"t":2},"speaker":"B","text":"そうですね。授業や実習についていくことがようやくできています。"},{"id":{"d":629,"n":3,"t":2},"speaker":"A","text":"あとどのくらい通うのですか？"},{"id":{"d":629,"n":4,"t":2},"speaker":"B","text":"あと2年です。"},{"id":{"d":629,"n":5,"t":2},"speaker":"A","text":"友達はできましたか？"},{"id":{"d":629,"n":6,"t":2},"speaker":"B","text":"はい。たくさんできました。友達と助け合っています。"},{"id":{"d":629,"n":7,"t":2},"speaker":"A","text":"それは良いですね。"},{"id":{"d":629,"n":8,"t":2},"speaker":"B","text":"はい。仲間のおかげで授業や実習を乗り越えられています。"}];
INSERT INTO sentence [{"id":{"d":630,"n":1,"t":2},"speaker":"A","text":"10月に合唱コンクールがあります。"},{"id":{"d":630,"n":2,"t":2},"speaker":"B","text":"楽しみですね。"},{"id":{"d":630,"n":3,"t":2},"speaker":"A","text":"はい。私はピアノを習っているので、伴奏をしたいです。"},{"id":{"d":630,"n":4,"t":2},"speaker":"B","text":"それは良いですね。立候補してはどうですか？"},{"id":{"d":630,"n":5,"t":2},"speaker":"A","text":"はい。同じクラスの早紀さんもピアノを習っているので、彼女もピアノの伴走者に立候補するかもしれません。"},{"id":{"d":630,"n":6,"t":2},"speaker":"B","text":"もし2人が立候補したら、どうなるのでしょうね？"},{"id":{"d":630,"n":7,"t":2},"speaker":"A","text":"実際にピアノを弾いて、上手に弾ける方が選ばれるらしいです。"},{"id":{"d":630,"n":8,"t":2},"speaker":"B","text":"それでは、今のうちから練習しておいた方がいいですね。"},{"id":{"d":630,"n":9,"t":2},"speaker":"A","text":"そうしようと思います。"}];
INSERT INTO sentence [{"id":{"d":631,"n":1,"t":2},"speaker":"A","text":"私は運動会で障害物競走に出ます。"},{"id":{"d":631,"n":2,"t":2},"speaker":"B","text":"1位を目指して頑張ってくださいね。"},{"id":{"d":631,"n":3,"t":2},"speaker":"A","text":"はい。途中で転んでしまわないように気を付けます。"},{"id":{"d":631,"n":4,"t":2},"speaker":"B","text":"そうですね。ケガをしては元も子もありません。"},{"id":{"d":631,"n":5,"t":2},"speaker":"A","text":"あなたは何の競技に出るのですか？"},{"id":{"d":631,"n":6,"t":2},"speaker":"B","text":"私は綱引きに出場する予定です。"},{"id":{"d":631,"n":7,"t":2},"speaker":"A","text":"練習はしていますか？"},{"id":{"d":631,"n":8,"t":2},"speaker":"B","text":"もちろんです。昼休みにいつも練習しています。"}];
INSERT INTO sentence [{"id":{"d":632,"n":1,"t":2},"speaker":"A","text":"昨日は第一志望の大学のオープンキャンパスに行って来ました。"},{"id":{"d":632,"n":2,"t":2},"speaker":"B","text":"良いですね。どうでしたか？"},{"id":{"d":632,"n":3,"t":2},"speaker":"A","text":"最寄りの駅からかなり遠くて、道に迷ってしまいました。"},{"id":{"d":632,"n":4,"t":2},"speaker":"B","text":"大丈夫でしたか？"},{"id":{"d":632,"n":5,"t":2},"speaker":"A","text":"はい。地図を注意深く読み、どのようにかしてたどり着きました。"},{"id":{"d":632,"n":6,"t":2},"speaker":"B","text":"大学の雰囲気は分かったでしょうか？"},{"id":{"d":632,"n":7,"t":2},"speaker":"A","text":"はい。広々としていて、開放的な印象でした。"},{"id":{"d":632,"n":8,"t":2},"speaker":"B","text":"受験勉強の意欲に繋がると良いですね。"}];
INSERT INTO sentence [{"id":{"d":633,"n":1,"t":2},"speaker":"A","text":"自動車運転免許証の卒業検定に合格しました。"},{"id":{"d":633,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。"},{"id":{"d":633,"n":3,"t":2},"speaker":"A","text":"思いのほか長くかかってしまいました。"},{"id":{"d":633,"n":4,"t":2},"speaker":"B","text":"そうですね。頑張りましたね。"},{"id":{"d":633,"n":5,"t":2},"speaker":"A","text":"早く自動車を運転したいです。"},{"id":{"d":633,"n":6,"t":2},"speaker":"B","text":"車を持っているのですか？"},{"id":{"d":633,"n":7,"t":2},"speaker":"A","text":"いいえ。持っていないので、両親の車を貸してもらいます。"}];
INSERT INTO sentence [{"id":{"d":634,"n":1,"t":2},"speaker":"A","text":"今日の水泳大会はどうでしたか？"},{"id":{"d":634,"n":2,"t":2},"speaker":"B","text":"自由形で2位になりました。"},{"id":{"d":634,"n":3,"t":2},"speaker":"A","text":"素晴しいことですね。おめでとうございます。"},{"id":{"d":634,"n":4,"t":2},"speaker":"B","text":"1位になりたかったので、悔しいです。"},{"id":{"d":634,"n":5,"t":2},"speaker":"A","text":"1位とはどのくらい差がありましたか？"},{"id":{"d":634,"n":6,"t":2},"speaker":"B","text":"0.3秒です。"},{"id":{"d":634,"n":7,"t":2},"speaker":"A","text":"それは惜しかったですね。"},{"id":{"d":634,"n":8,"t":2},"speaker":"B","text":"練習では勝てていた相手なので、油断してしまったかもしれません。"}];
INSERT INTO sentence [{"id":{"d":635,"n":1,"t":2},"speaker":"A","text":"お姉ちゃんの高校の学園祭に遊びに行ってもよいですか？"},{"id":{"d":635,"n":2,"t":2},"speaker":"B","text":"もちろんです。来てくれたらうれしいです。"},{"id":{"d":635,"n":3,"t":2},"speaker":"A","text":"お姉ちゃんのクラスはどのようなことを行うのですか？"},{"id":{"d":635,"n":4,"t":2},"speaker":"B","text":"クレープ屋さんです。"},{"id":{"d":635,"n":5,"t":2},"speaker":"A","text":"どのようなクレープを売るのですか？"},{"id":{"d":635,"n":6,"t":2},"speaker":"B","text":"チョコバナナとイチゴクリームです。"},{"id":{"d":635,"n":7,"t":2},"speaker":"A","text":"おいしそうです。絶対にクレープを食べに行きますね。"},{"id":{"d":635,"n":8,"t":2},"speaker":"B","text":"せっかくだから、お友達も連れて来てください。楽しみに待っていますね。"}];
INSERT INTO sentence [{"id":{"d":636,"n":1,"t":2},"speaker":"A","text":"あなたは声が大きいですね。"},{"id":{"d":636,"n":2,"t":2},"speaker":"B","text":"高校生の頃、応援団に入っていました。"},{"id":{"d":636,"n":3,"t":2},"speaker":"A","text":"それで納得できました。大きい声を出す練習をしたのでしょうね。"},{"id":{"d":636,"n":4,"t":2},"speaker":"B","text":"はい。おなかから、遠くまで届く声を出すように訓練しました。"},{"id":{"d":636,"n":5,"t":2},"speaker":"A","text":"団長をしていたのですか？"},{"id":{"d":636,"n":6,"t":2},"speaker":"B","text":"いえ、私は副団長でした。"},{"id":{"d":636,"n":7,"t":2},"speaker":"A","text":"一度、応援している姿を見てみたいです。"},{"id":{"d":636,"n":8,"t":2},"speaker":"B","text":"恥ずかしいのでそれは勘弁してください。"}];
INSERT INTO sentence [{"id":{"d":637,"n":1,"t":2},"speaker":"A","text":"体操服にゼッケンを付けておきましたよ。"},{"id":{"d":637,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。でも、これでは反対です。"},{"id":{"d":637,"n":3,"t":2},"speaker":"A","text":"どのような意味ですか？"},{"id":{"d":637,"n":4,"t":2},"speaker":"B","text":"体操服の前にゼッケンが付いていますが、背中に付けなければいけません。"},{"id":{"d":637,"n":5,"t":2},"speaker":"A","text":"そうなのですか？"},{"id":{"d":637,"n":6,"t":2},"speaker":"B","text":"はい。お願いする時に、私がきちんと説明しなかったせいです。お母さんのせいではありません。"},{"id":{"d":637,"n":7,"t":2},"speaker":"A","text":"私も確認するべきでした。すぐに付け直します。"},{"id":{"d":637,"n":8,"t":2},"speaker":"B","text":"手間をかけてすみません。"}];
INSERT INTO sentence [{"id":{"d":638,"n":1,"t":2},"speaker":"A","text":"私は子どもの頃、運動会が嫌いでした。"},{"id":{"d":638,"n":2,"t":2},"speaker":"B","text":"どうして嫌いだったのですか？"},{"id":{"d":638,"n":3,"t":2},"speaker":"A","text":"スポーツが不得意で、全く活躍できなかったからです。"},{"id":{"d":638,"n":4,"t":2},"speaker":"B","text":"私は逆で、運動会しか活躍できる機会がなかったです。"},{"id":{"d":638,"n":5,"t":2},"speaker":"A","text":"運動会で活躍できるとはうらやましいです。"},{"id":{"d":638,"n":6,"t":2},"speaker":"B","text":"運動会の当日は憂鬱でしたか？"},{"id":{"d":638,"n":7,"t":2},"speaker":"A","text":"雨が降ることを本気で願っていました。"},{"id":{"d":638,"n":8,"t":2},"speaker":"B","text":"私は運動より勉強ができる人になりたかったです。"},{"id":{"d":638,"n":9,"t":2},"speaker":"A","text":"ないものねだりなのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":639,"n":1,"t":2},"speaker":"A","text":"学校の行事で一番好きなことは何ですか？"},{"id":{"d":639,"n":2,"t":2},"speaker":"B","text":"社会科見学です。工場などに行くと胸が躍ります。"},{"id":{"d":639,"n":3,"t":2},"speaker":"A","text":"良いですね。今年は自動車工場に行きますね。"},{"id":{"d":639,"n":4,"t":2},"speaker":"B","text":"はい、今から楽しみです。あなたは何が好きですか？"},{"id":{"d":639,"n":5,"t":2},"speaker":"A","text":"私が好きな行事は百人一首大会です。"},{"id":{"d":639,"n":6,"t":2},"speaker":"B","text":"なるほど、百人一首は記憶力の良いあなたの得意分野ですね。"},{"id":{"d":639,"n":7,"t":2},"speaker":"A","text":"はい。百人一首は大得意ですし大好きです。"},{"id":{"d":639,"n":8,"t":2},"speaker":"B","text":"あなたがいるので、今年は私たちのクラスが優勝できそうです。"},{"id":{"d":639,"n":9,"t":2},"speaker":"A","text":"期待を裏切らないように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":640,"n":1,"t":2},"speaker":"A","text":"台風が近付いていますね。"},{"id":{"d":640,"n":2,"t":2},"speaker":"B","text":"雨と風がひどくなってきました。"},{"id":{"d":640,"n":3,"t":2},"speaker":"A","text":"先ほど小学校から連絡があって、今日は休校だそうです。"},{"id":{"d":640,"n":4,"t":2},"speaker":"B","text":"分かりました。この悪天候では、仕方ありませんね。"},{"id":{"d":640,"n":5,"t":2},"speaker":"A","text":"お兄ちゃんの中学校も休みでしょうか？"},{"id":{"d":640,"n":6,"t":2},"speaker":"B","text":"中学校からはまだ連絡がないそうですよ。"},{"id":{"d":640,"n":7,"t":2},"speaker":"A","text":"そうですか。でも、恐らく休校でしょうね。"},{"id":{"d":640,"n":8,"t":2},"speaker":"B","text":"そう思います。今日は一日、家で過ごすことになりそうですね。"}];
INSERT INTO sentence [{"id":{"d":641,"n":1,"t":2},"speaker":"A","text":"今井先生、来年度はなぎなた部の副顧問になっていただけますか？"},{"id":{"d":641,"n":2,"t":2},"speaker":"B","text":"今、副顧問をしている先生に来年度もお願いしてはどうですか？"},{"id":{"d":641,"n":3,"t":2},"speaker":"A","text":"今の副顧問の先生が、来年度は異動で他の学校へ行ってしまうのです。"},{"id":{"d":641,"n":4,"t":2},"speaker":"B","text":"私はなぎなたの経験が全くありませんよ。"},{"id":{"d":641,"n":5,"t":2},"speaker":"A","text":"でも、他にお願いできる先生がいません。"},{"id":{"d":641,"n":6,"t":2},"speaker":"B","text":"なぎなたの経験がなくても務まるのでしょうか？"},{"id":{"d":641,"n":7,"t":2},"speaker":"A","text":"はい。顧問の岡先生が経験者なので、助けてくださると思います。"},{"id":{"d":641,"n":8,"t":2},"speaker":"B","text":"分かりました。お役に立てるかどうか不安ですが、お引き受けします。"},{"id":{"d":641,"n":9,"t":2},"speaker":"A","text":"助かります。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":642,"n":1,"t":2},"speaker":"A","text":"真奈ちゃんのお母さま、こんにちは。"},{"id":{"d":642,"n":2,"t":2},"speaker":"B","text":"奈々子先生こんにちは。真奈のお迎えに来ました。"},{"id":{"d":642,"n":3,"t":2},"speaker":"A","text":"少しお話したいことがあるのですが、よろしいですか？"},{"id":{"d":642,"n":4,"t":2},"speaker":"B","text":"はい。何かありましたか？"},{"id":{"d":642,"n":5,"t":2},"speaker":"A","text":"真奈ちゃんが今日、トイレに間に合わなくておもらししてしまったのです。"},{"id":{"d":642,"n":6,"t":2},"speaker":"B","text":"それはすみません。"},{"id":{"d":642,"n":7,"t":2},"speaker":"A","text":"気にしないでください。パンツがぬれてしまって、今は保育園のパンツをお貸ししているので、来週にでも返却をお願いします。"},{"id":{"d":642,"n":8,"t":2},"speaker":"B","text":"分かりました。家で洗濯してお返しします。"}];
INSERT INTO sentence [{"id":{"d":643,"n":1,"t":2},"speaker":"A","text":"宿題が終わりません。"},{"id":{"d":643,"n":2,"t":2},"speaker":"B","text":"どのような宿題ですか？"},{"id":{"d":643,"n":3,"t":2},"speaker":"A","text":"英語のプリントを3枚終わらせなければいけません。"},{"id":{"d":643,"n":4,"t":2},"speaker":"B","text":"難しいのですか？少しプリントを見せてください。"},{"id":{"d":643,"n":5,"t":2},"speaker":"A","text":"これです。"},{"id":{"d":643,"n":6,"t":2},"speaker":"B","text":"長文の問題ですね。"},{"id":{"d":643,"n":7,"t":2},"speaker":"A","text":"はい。知らない単語が多くて、意味を調べるだけでとても時間がかかります。"},{"id":{"d":643,"n":8,"t":2},"speaker":"B","text":"私も手伝うので、一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":644,"n":1,"t":2},"speaker":"A","text":"大学生活には慣れましたか？"},{"id":{"d":644,"n":2,"t":2},"speaker":"B","text":"はい、だいぶ慣れてきました。でも、勉強が大変です。"},{"id":{"d":644,"n":3,"t":2},"speaker":"A","text":"私も大学生の頃は単位を落とさないように必死に勉強しました。"},{"id":{"d":644,"n":4,"t":2},"speaker":"B","text":"試験期間には、睡眠時間を削って勉強しています。"},{"id":{"d":644,"n":5,"t":2},"speaker":"A","text":"そうですよね、試験期間はつらかった記憶があります。"},{"id":{"d":644,"n":6,"t":2},"speaker":"B","text":"何か、試験期間を乗り越えるためのアドバイスをいただけませんか？"},{"id":{"d":644,"n":7,"t":2},"speaker":"A","text":"友達同士で励まし合うことですね。一緒に勉強したり、分からないことを教え合ったりすると良いです。"},{"id":{"d":644,"n":8,"t":2},"speaker":"B","text":"参考になります。早速明日、友達を誘って図書館で勉強することにします。"}];
INSERT INTO sentence [{"id":{"d":645,"n":1,"t":2},"speaker":"A","text":"漢字テストで満点を取れたと思ったのですが、1問間違えてしまいました。"},{"id":{"d":645,"n":2,"t":2},"speaker":"B","text":"どの漢字ですか？"},{"id":{"d":645,"n":3,"t":2},"speaker":"A","text":"「威儀」の「儀」の字を、人偏の無い「義」にしてしまいました。"},{"id":{"d":645,"n":4,"t":2},"speaker":"B","text":"惜しかったですね。"},{"id":{"d":645,"n":5,"t":2},"speaker":"A","text":"テスト前に練習していた時から「義」と間違えて書いていました。"},{"id":{"d":645,"n":6,"t":2},"speaker":"B","text":"勘違いしたまま練習していたことが、悔しいですね。"},{"id":{"d":645,"n":7,"t":2},"speaker":"A","text":"はい。これからは正しい漢字をきちんと確認する癖を付けようと思います。"},{"id":{"d":645,"n":8,"t":2},"speaker":"B","text":"大事ですね。次のテストでは満点を取れると良いですね。"},{"id":{"d":645,"n":9,"t":2},"speaker":"A","text":"そのつもりです。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":646,"n":1,"t":2},"speaker":"A","text":"すみません。英語のレッスンのコース変更をお願いします。"},{"id":{"d":646,"n":2,"t":2},"speaker":"B","text":"はい。現在はどのコースに通っていますか？"},{"id":{"d":646,"n":3,"t":2},"speaker":"A","text":"グループレッスンのコースです。"},{"id":{"d":646,"n":4,"t":2},"speaker":"B","text":"どのコースへの変更希望でしょうか？"},{"id":{"d":646,"n":5,"t":2},"speaker":"A","text":"個人レッスンに変更したいです。"},{"id":{"d":646,"n":6,"t":2},"speaker":"B","text":"月謝が高くなりますが、よろしいでしょうか？"},{"id":{"d":646,"n":7,"t":2},"speaker":"A","text":"構いません。"},{"id":{"d":646,"n":8,"t":2},"speaker":"B","text":"ご希望の講師を教えてください。"},{"id":{"d":646,"n":9,"t":2},"speaker":"A","text":"テイラー先生でお願いします。"}];
INSERT INTO sentence [{"id":{"d":647,"n":1,"t":2},"speaker":"A","text":"塾に行く前に夕食を食べますか？"},{"id":{"d":647,"n":2,"t":2},"speaker":"B","text":"いいえ。おなかがいっぱいになると眠くなるので、やめておきます。"},{"id":{"d":647,"n":3,"t":2},"speaker":"A","text":"帰って来てからだと、遅い時間になってしまいますよ。"},{"id":{"d":647,"n":4,"t":2},"speaker":"B","text":"確かにそうですね。あと途中でおなかもすいてしまいそうです。"},{"id":{"d":647,"n":5,"t":2},"speaker":"A","text":"塾の休み時間に何かつまみますか？"},{"id":{"d":647,"n":6,"t":2},"speaker":"B","text":"そうします。おにぎりを持って行って食べることにします。"},{"id":{"d":647,"n":7,"t":2},"speaker":"A","text":"それでは、おにぎりを用意しておきますね。"},{"id":{"d":647,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":648,"n":1,"t":2},"speaker":"A","text":"卒業式の後、謝恩会があります。"},{"id":{"d":648,"n":2,"t":2},"speaker":"B","text":"会場はどこですか？"},{"id":{"d":648,"n":3,"t":2},"speaker":"A","text":"レインボーホテルです。"},{"id":{"d":648,"n":4,"t":2},"speaker":"B","text":"卒業式と同じ格好でよいのでしょうか？"},{"id":{"d":648,"n":5,"t":2},"speaker":"A","text":"友達に聞いたら、着替えを持参して、ホテルで着替える人が多いようです。"},{"id":{"d":648,"n":6,"t":2},"speaker":"B","text":"なるほど。あなたはどうしますか？"},{"id":{"d":648,"n":7,"t":2},"speaker":"A","text":"私もワンピースを持って行って、ホテルで着替えようと思っています。"},{"id":{"d":648,"n":8,"t":2},"speaker":"B","text":"当日は荷物が多くなりそうですね。車で送迎しますよ。"},{"id":{"d":648,"n":9,"t":2},"speaker":"A","text":"どうもありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":649,"n":1,"t":2},"speaker":"A","text":"大学で履修する第二外国語は何を選択しますか？"},{"id":{"d":649,"n":2,"t":2},"speaker":"B","text":"中国語にします。"},{"id":{"d":649,"n":3,"t":2},"speaker":"A","text":"どうして中国語にするのですか？"},{"id":{"d":649,"n":4,"t":2},"speaker":"B","text":"中国人の友達がいて、その友達と中国語で話せるようになりたいからです。あなたはどうしますか？"},{"id":{"d":649,"n":5,"t":2},"speaker":"A","text":"私はフランス語とスペイン語で迷っています。"},{"id":{"d":649,"n":6,"t":2},"speaker":"B","text":"より興味があるのはどちらですか？"},{"id":{"d":649,"n":7,"t":2},"speaker":"A","text":"正直に言うと、どちらもそれほど興味はありません。ヨーロッパが好きなのでこの2つが候補になっただけです。"},{"id":{"d":649,"n":8,"t":2},"speaker":"B","text":"それでは、フランスとスペインで行きたい国の方の言葉にしてはどうですか？"},{"id":{"d":649,"n":9,"t":2},"speaker":"A","text":"より行きたいのはスペインなので、スペイン語にします。"}];
INSERT INTO sentence [{"id":{"d":650,"n":1,"t":2},"speaker":"A","text":"先生、英語のスピーチコンテストのことで質問があります。"},{"id":{"d":650,"n":2,"t":2},"speaker":"B","text":"はい、何でしょうか？"},{"id":{"d":650,"n":3,"t":2},"speaker":"A","text":"出場するかどうか迷っているのですが、私の英語力でも大丈夫でしょうか？"},{"id":{"d":650,"n":4,"t":2},"speaker":"B","text":"はい。羽根田さんなら大丈夫ですよ。それに、先生もお手伝いしますよ。"},{"id":{"d":650,"n":5,"t":2},"speaker":"A","text":"それは心強いです。スピーチのテーマは決まっているのでしょうか？"},{"id":{"d":650,"n":6,"t":2},"speaker":"B","text":"はい。「日本人が世界で活躍するために必要なこと」というテーマです。"},{"id":{"d":650,"n":7,"t":2},"speaker":"A","text":"自分なりに原稿を書くので、先生に添削していただいてから出場するかどうか決めてもよいでしょうか？"},{"id":{"d":650,"n":8,"t":2},"speaker":"B","text":"もちろん、よいですよ。スピーチは3分間なので、それに見合った量の原稿を準備してくださいね。"},{"id":{"d":650,"n":9,"t":2},"speaker":"A","text":"分かりました。原稿ができたらまた来ますので、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":651,"n":1,"t":2},"speaker":"A","text":"香織さん、こんにちは。今から帰るところですか？"},{"id":{"d":651,"n":2,"t":2},"speaker":"B","text":"いえ、勉強しようと思って図書館に来たのですが、席が空いていないので出てきました。"},{"id":{"d":651,"n":3,"t":2},"speaker":"A","text":"そうなのですか。私も勉強しようと思って来たので困りました。"},{"id":{"d":651,"n":4,"t":2},"speaker":"B","text":"学習室は満席です。お互いがさらに早い時間に来るべきでしたね。"},{"id":{"d":651,"n":5,"t":2},"speaker":"A","text":"せっかく勉強への意欲が湧いている状態で家を出たのに、残念です。"},{"id":{"d":651,"n":6,"t":2},"speaker":"B","text":"私は塾の自習室に行くつもりです。一緒に行きませんか？"},{"id":{"d":651,"n":7,"t":2},"speaker":"A","text":"今日は日曜日なのに、塾の自習室が開いているのですか？"},{"id":{"d":651,"n":8,"t":2},"speaker":"B","text":"「定期テストが近いので開ける」と塾の先生が言っていました。"},{"id":{"d":651,"n":9,"t":2},"speaker":"A","text":"そうなのですか。それでは一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":652,"n":1,"t":2},"speaker":"A","text":"今日、保育園で食べたおやつは何でしたか？"},{"id":{"d":652,"n":2,"t":2},"speaker":"B","text":"蒸しパンとヨーグルトでした。"},{"id":{"d":652,"n":3,"t":2},"speaker":"A","text":"おいしかったですか？"},{"id":{"d":652,"n":4,"t":2},"speaker":"B","text":"僕はおいしかったと思うのですが、英樹君は食べられなくて残していました。"},{"id":{"d":652,"n":5,"t":2},"speaker":"A","text":"両方とも食べられなかったのですか？"},{"id":{"d":652,"n":6,"t":2},"speaker":"B","text":"蒸しパンは食べていて、ヨーグルトは残していました。"},{"id":{"d":652,"n":7,"t":2},"speaker":"A","text":"ヨーグルトが嫌いなのかもしれませんね。"},{"id":{"d":652,"n":8,"t":2},"speaker":"B","text":"はい。先生が、「ヨーグルトを食べることが無理なら残してよい」と言っていました。"}];
INSERT INTO sentence [{"id":{"d":653,"n":1,"t":2},"speaker":"A","text":"次のサッカー大会に出場するメンバーの発表がありました。"},{"id":{"d":653,"n":2,"t":2},"speaker":"B","text":"あなたはメンバーに選ばれましたか？"},{"id":{"d":653,"n":3,"t":2},"speaker":"A","text":"はい。選ばれました。"},{"id":{"d":653,"n":4,"t":2},"speaker":"B","text":"おめでとう。ずっと補欠だったので、ようやく選ばれたという感じですね。"},{"id":{"d":653,"n":5,"t":2},"speaker":"A","text":"とてもうれしいのですが、代わりに村野君が補欠になってしまいました。"},{"id":{"d":653,"n":6,"t":2},"speaker":"B","text":"先生が公平に選んだのですから、仕方ありませんよ。"},{"id":{"d":653,"n":7,"t":2},"speaker":"A","text":"村野君がとても落ち込んでいて、かける言葉が見つかりませんでした。"},{"id":{"d":653,"n":8,"t":2},"speaker":"B","text":"気持ちを切り替えて、村野君の分まで試合で頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":654,"n":1,"t":2},"speaker":"A","text":"私は中学校でかるた部に入りたいです。"},{"id":{"d":654,"n":2,"t":2},"speaker":"B","text":"あなたの入学する中学校にはかるた部があるのですか？"},{"id":{"d":654,"n":3,"t":2},"speaker":"A","text":"はい。あります。"},{"id":{"d":654,"n":4,"t":2},"speaker":"B","text":"それは良かったですね。かるた部のある中学校は少ないと思います。"},{"id":{"d":654,"n":5,"t":2},"speaker":"A","text":"とてもラッキーだと思います。"},{"id":{"d":654,"n":6,"t":2},"speaker":"B","text":"もしあなたが私の中学校だったら、かるた部はありませんよ。"},{"id":{"d":654,"n":7,"t":2},"speaker":"A","text":"そうなったら、自分でかるた部を作ります。"},{"id":{"d":654,"n":8,"t":2},"speaker":"B","text":"素晴らしい行動力ですね。"}];
INSERT INTO sentence [{"id":{"d":655,"n":1,"t":2},"speaker":"A","text":"卒業論文のテーマが決まりません。"},{"id":{"d":655,"n":2,"t":2},"speaker":"B","text":"私もまだ決まっていません。"},{"id":{"d":655,"n":3,"t":2},"speaker":"A","text":"テーマを決める期限が近付いてきて、焦りますね。"},{"id":{"d":655,"n":4,"t":2},"speaker":"B","text":"そうですね。テーマの候補はありますか？"},{"id":{"d":655,"n":5,"t":2},"speaker":"A","text":"2つほどありますが、うまく研究を進めていく自信がありません。"},{"id":{"d":655,"n":6,"t":2},"speaker":"B","text":"私は候補もありません。お手上げ状態です。"},{"id":{"d":655,"n":7,"t":2},"speaker":"A","text":"先輩か教授に相談してみてはどうですか？"},{"id":{"d":655,"n":8,"t":2},"speaker":"B","text":"先輩も教授も忙しそうで、なかなか相談を持ち掛けることができません。"}];
INSERT INTO sentence [{"id":{"d":656,"n":1,"t":2},"speaker":"A","text":"9月に引き渡し訓練があるそうです。"},{"id":{"d":656,"n":2,"t":2},"speaker":"B","text":"具体的には何をするのですか？"},{"id":{"d":656,"n":3,"t":2},"speaker":"A","text":"災害が起こったと想定して、保護者が学校へ子どもを迎えに来るそうです。"},{"id":{"d":656,"n":4,"t":2},"speaker":"B","text":"それで子どもと一緒に帰るということでしょうか？"},{"id":{"d":656,"n":5,"t":2},"speaker":"A","text":"はい。子どもと保護者が一緒に帰りながら、どの道を通ることが安全か確認するそうです。"},{"id":{"d":656,"n":6,"t":2},"speaker":"B","text":"万が一に備えて、大切な訓練ですね。"},{"id":{"d":656,"n":7,"t":2},"speaker":"A","text":"はい。本当に災害が起こったつもりで臨みます。"},{"id":{"d":656,"n":8,"t":2},"speaker":"B","text":"そうですね。そのような意識で参加することが大切です。"}];
INSERT INTO sentence [{"id":{"d":657,"n":1,"t":2},"speaker":"A","text":"運動会で記憶に残っている競技は何ですか？"},{"id":{"d":657,"n":2,"t":2},"speaker":"B","text":"徒競走です。"},{"id":{"d":657,"n":3,"t":2},"speaker":"A","text":"いつも1位だったからでしょうか？"},{"id":{"d":657,"n":4,"t":2},"speaker":"B","text":"いえ。たった一度だけ1位になれたことが強烈に記憶に残っているのです。"},{"id":{"d":657,"n":5,"t":2},"speaker":"A","text":"よほどうれしかったのですね。"},{"id":{"d":657,"n":6,"t":2},"speaker":"B","text":"はい。思わずガッツポーズをしてしまいました。"},{"id":{"d":657,"n":7,"t":2},"speaker":"A","text":"良い思い出ですね。"},{"id":{"d":657,"n":8,"t":2},"speaker":"B","text":"今でも時々両親とその時の話をします。"}];
INSERT INTO sentence [{"id":{"d":658,"n":1,"t":2},"speaker":"A","text":"新入生の方ですか？"},{"id":{"d":658,"n":2,"t":2},"speaker":"B","text":"はい。そうです。"},{"id":{"d":658,"n":3,"t":2},"speaker":"A","text":"入会するサークルはもう決めましたか？"},{"id":{"d":658,"n":4,"t":2},"speaker":"B","text":"いえ、まだ、入会するサークルは決めていません。"},{"id":{"d":658,"n":5,"t":2},"speaker":"A","text":"私はテニスサークルに入っているのですが、テニスに興味はありませんか？"},{"id":{"d":658,"n":6,"t":2},"speaker":"B","text":"興味はあります。高校ではテニス部でした。"},{"id":{"d":658,"n":7,"t":2},"speaker":"A","text":"大学でもテニスを続けませんか？日曜日にテニスをしているので、ぜひ見学に来てください。"},{"id":{"d":658,"n":8,"t":2},"speaker":"B","text":"どこでテニスをしているのですか？"},{"id":{"d":658,"n":9,"t":2},"speaker":"A","text":"大学のテニスコートを借りて行っています。"},{"id":{"d":658,"n":10,"t":2},"speaker":"B","text":"分かりました。では、日曜日に見学させてください。"},{"id":{"d":658,"n":11,"t":2},"speaker":"A","text":"もちろんです。集合時間や場所の詳細はこのチラシをご覧ください。"},{"id":{"d":658,"n":12,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":659,"n":1,"t":2},"speaker":"A","text":"お母さん、明日は午前のみの授業で給食がないので、昼食は家で食べます。"},{"id":{"d":659,"n":2,"t":2},"speaker":"B","text":"私は明日も会社で仕事をしているので、お昼に食べるものを冷蔵庫に用意しておきますね。"},{"id":{"d":659,"n":3,"t":2},"speaker":"A","text":"分かりました。メニューをリクエストしてもよいですか？"},{"id":{"d":659,"n":4,"t":2},"speaker":"B","text":"よいですよ。何が食べたいですか？"},{"id":{"d":659,"n":5,"t":2},"speaker":"A","text":"焼きそばです。"},{"id":{"d":659,"n":6,"t":2},"speaker":"B","text":"そのような簡単なもので良いのですか？"},{"id":{"d":659,"n":7,"t":2},"speaker":"A","text":"はい。お母さんの作る焼きそばはとてもおいしいので、大好きなのです。"},{"id":{"d":659,"n":8,"t":2},"speaker":"B","text":"それはうれしいです。では、焼きそばを作っておきます。"},{"id":{"d":659,"n":9,"t":2},"speaker":"A","text":"どうもありがとうございます。"},{"id":{"d":659,"n":10,"t":2},"speaker":"B","text":"電子レンジで温めて食べてくださいね。"},{"id":{"d":659,"n":11,"t":2},"speaker":"A","text":"分かりました。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":660,"n":1,"t":2},"speaker":"A","text":"美容師になるにはどうすればよいのですか？"},{"id":{"d":660,"n":2,"t":2},"speaker":"B","text":"美容師免許を取得する必要があります。"},{"id":{"d":660,"n":3,"t":2},"speaker":"A","text":"どのようにしたら美容師免許を取れますか？"},{"id":{"d":660,"n":4,"t":2},"speaker":"B","text":"まず、美容師養成施設へ入所し、次に国家試験を受験して合格した後、美容師名簿への登録、免許申請を行う必要があります。私は美容学校に通いました。"},{"id":{"d":660,"n":5,"t":2},"speaker":"A","text":"学校の授業や実習は大変でしたか？"},{"id":{"d":660,"n":6,"t":2},"speaker":"B","text":"実習は少し大変でしたが、美容学校では友達がたくさんできて、楽しかったですよ。"},{"id":{"d":660,"n":7,"t":2},"speaker":"A","text":"美容学校を卒業して美容師免許を取得するという流れなのですね。"},{"id":{"d":660,"n":8,"t":2},"speaker":"B","text":"はい、そうです。もし良かったらチャレンジしてみてください。"}];
INSERT INTO sentence [{"id":{"d":661,"n":1,"t":2},"speaker":"A","text":"そういえば、学校へ履いていく靴下がだいぶ古くなっていましたね。"},{"id":{"d":661,"n":2,"t":2},"speaker":"B","text":"そうですね。汚れも目立ってきていました。"},{"id":{"d":661,"n":3,"t":2},"speaker":"A","text":"ちょうど、この店に靴下が売られているので買いましょう。"},{"id":{"d":661,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":661,"n":5,"t":2},"speaker":"A","text":"この靴下はどうですか？"},{"id":{"d":661,"n":6,"t":2},"speaker":"B","text":"黒は駄目です。"},{"id":{"d":661,"n":7,"t":2},"speaker":"A","text":"どうしてですか？"},{"id":{"d":661,"n":8,"t":2},"speaker":"B","text":"校則で、靴下は白と決められているのです。"},{"id":{"d":661,"n":9,"t":2},"speaker":"A","text":"そうだったのですか。知らなかったです。"}];
INSERT INTO sentence [{"id":{"d":662,"n":1,"t":2},"speaker":"A","text":"私は大学で音楽サークルに入っていました。"},{"id":{"d":662,"n":2,"t":2},"speaker":"B","text":"音楽サークルというと、オーケストラのような感じですか？"},{"id":{"d":662,"n":3,"t":2},"speaker":"A","text":"いえ。サークル内でロックバンドを組んで、ライブをしていました。"},{"id":{"d":662,"n":4,"t":2},"speaker":"B","text":"軽音楽をするサークルですね。"},{"id":{"d":662,"n":5,"t":2},"speaker":"A","text":"そうです。私はギターを担当していました。"},{"id":{"d":662,"n":6,"t":2},"speaker":"B","text":"あなたは歌が上手いので、ボーカルをしていたのかと思いました。"},{"id":{"d":662,"n":7,"t":2},"speaker":"A","text":"私よりずっと歌が上手い人がいて、その人がボーカルでした。"},{"id":{"d":662,"n":8,"t":2},"speaker":"B","text":"それほどに上手なのですか。一度聴いてみたいです。"},{"id":{"d":662,"n":9,"t":2},"speaker":"A","text":"昔の動画がありますよ。見ますか？"},{"id":{"d":662,"n":10,"t":2},"speaker":"B","text":"ぜひ見せてください。"}];
INSERT INTO sentence [{"id":{"d":663,"n":1,"t":2},"speaker":"A","text":"履修登録は済みましたか？"},{"id":{"d":663,"n":2,"t":2},"speaker":"B","text":"まだ、履修登録は済んでいません。時間割を決められなくて困っています。"},{"id":{"d":663,"n":3,"t":2},"speaker":"A","text":"もうすぐ登録の期限ですよ。"},{"id":{"d":663,"n":4,"t":2},"speaker":"B","text":"分かっています。ほとんど決めたのですが、選択科目で迷っているところがあるのです。"},{"id":{"d":663,"n":5,"t":2},"speaker":"A","text":"具体的にはどの科目ですか？"},{"id":{"d":663,"n":6,"t":2},"speaker":"B","text":"「中国語演習」か「漢語読解」で迷っています。"},{"id":{"d":663,"n":7,"t":2},"speaker":"A","text":"両方履修すれば良いのではないでしょうか？"},{"id":{"d":663,"n":8,"t":2},"speaker":"B","text":"それが、講義時間が同じなのでどちらかを選ばなければなりません。"},{"id":{"d":663,"n":9,"t":2},"speaker":"A","text":"それでは今年度どちらかを履修して、来年度もう1つを履修してはどうですか？"},{"id":{"d":663,"n":10,"t":2},"speaker":"B","text":"なるほど、その手がありました。それでは今年度は「中国語演習」を取ることにします。"}];
INSERT INTO sentence [{"id":{"d":664,"n":1,"t":2},"speaker":"A","text":"高校生活で頑張ったことは何ですか？"},{"id":{"d":664,"n":2,"t":2},"speaker":"B","text":"生徒会の活動です。生徒会長として精いっぱい学校生活の改善に務めました。"},{"id":{"d":664,"n":3,"t":2},"speaker":"A","text":"具体的にはどのようなことを頑張ったのか教えてください。"},{"id":{"d":664,"n":4,"t":2},"speaker":"B","text":"生徒全員に、学校生活で改善してほしい点についてのアンケート調査をしました。"},{"id":{"d":664,"n":5,"t":2},"speaker":"A","text":"アンケート結果に基づいて実際に改善したことがあるのでしょうか？"},{"id":{"d":664,"n":6,"t":2},"speaker":"B","text":"はい。定期試験の前に部活を休みにしてほしいという要望があり、先生に相談して休みになるようにしました。"},{"id":{"d":664,"n":7,"t":2},"speaker":"A","text":"それは大きな改善ですね。"},{"id":{"d":664,"n":8,"t":2},"speaker":"B","text":"その他にもさまざまな改善要望が出たので、それらを可能な限り実行しました。"}];
INSERT INTO sentence [{"id":{"d":665,"n":1,"t":2},"speaker":"A","text":"もうすぐマラソン大会が開催されますね。"},{"id":{"d":665,"n":2,"t":2},"speaker":"B","text":"はい。今から緊張してしまいます。"},{"id":{"d":665,"n":3,"t":2},"speaker":"A","text":"目標は何位ですか？"},{"id":{"d":665,"n":4,"t":2},"speaker":"B","text":"3年生の時は68位だったので、今回は50位には入りたいです。"},{"id":{"d":665,"n":5,"t":2},"speaker":"A","text":"私は10位内になりたいです。"},{"id":{"d":665,"n":6,"t":2},"speaker":"B","text":"あなたはマラソンが得意なので、大丈夫ですよ。"},{"id":{"d":665,"n":7,"t":2},"speaker":"A","text":"陸上部なので走り慣れているとは思います。"},{"id":{"d":665,"n":8,"t":2},"speaker":"B","text":"お互いに目標を達成できると良いですね。"},{"id":{"d":665,"n":9,"t":2},"speaker":"A","text":"はい。お互いけがには気を付けて精いっぱい頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":666,"n":1,"t":2},"speaker":"A","text":"学園祭が賑わっていますね。"},{"id":{"d":666,"n":2,"t":2},"speaker":"B","text":"人がたくさんいて、歩くことがやっとです。"},{"id":{"d":666,"n":3,"t":2},"speaker":"A","text":"おなかもすいてきましたし、どこかのお店で何か買いますか？"},{"id":{"d":666,"n":4,"t":2},"speaker":"B","text":"では僕の友達が出しているお店に行きたいです。"},{"id":{"d":666,"n":5,"t":2},"speaker":"A","text":"そうしましょう。場所は分かりますか？"},{"id":{"d":666,"n":6,"t":2},"speaker":"B","text":"はい。校庭で出店しているので、向かいましょう。"},{"id":{"d":666,"n":7,"t":2},"speaker":"A","text":"お友達は何のお店を出店しているのですか？"},{"id":{"d":666,"n":8,"t":2},"speaker":"B","text":"かき氷屋さんです。"}];
INSERT INTO sentence [{"id":{"d":667,"n":1,"t":2},"speaker":"A","text":"高校入試の合格おめでとうございます。お祝いにこれをどうぞ。"},{"id":{"d":667,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。開けてもよいですか？"},{"id":{"d":667,"n":3,"t":2},"speaker":"A","text":"どうぞ。気に入ってもらえるとうれしいです。"},{"id":{"d":667,"n":4,"t":2},"speaker":"B","text":"腕時計ですね。とても大人っぽいです。"},{"id":{"d":667,"n":5,"t":2},"speaker":"A","text":"ベルトが白と黒の2種類あって、迷った結果、白にしました。"},{"id":{"d":667,"n":6,"t":2},"speaker":"B","text":"白の方が好きです。本当にありがとうございます。"},{"id":{"d":667,"n":7,"t":2},"speaker":"A","text":"合格した高校に通う時に着けて行ってください。"},{"id":{"d":667,"n":8,"t":2},"speaker":"B","text":"はい。大切にします。"}];
INSERT INTO sentence [{"id":{"d":668,"n":1,"t":2},"speaker":"A","text":"先輩、就職活動は順調ですか？"},{"id":{"d":668,"n":2,"t":2},"speaker":"B","text":"それが、なかなかうまくいかず困っています。"},{"id":{"d":668,"n":3,"t":2},"speaker":"A","text":"先輩は優秀なので、引く手あまたかと思いました。"},{"id":{"d":668,"n":4,"t":2},"speaker":"B","text":"とんでもありません。書類選考で落とされてしまうことばかりです。"},{"id":{"d":668,"n":5,"t":2},"speaker":"A","text":"そうなると、気分がふさいでしまいますね。"},{"id":{"d":668,"n":6,"t":2},"speaker":"B","text":"そうなのです。意欲を維持することが難しいです。"},{"id":{"d":668,"n":7,"t":2},"speaker":"A","text":"きっと先輩の良さを理解してくれる企業とのご縁があると思います。"},{"id":{"d":668,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。めげずに頑張ります。"}];
INSERT INTO sentence [{"id":{"d":669,"n":1,"t":2},"speaker":"A","text":"先生、浩章は幼稚園でどのような様子でしょうか？"},{"id":{"d":669,"n":2,"t":2},"speaker":"B","text":"お友達がたくさんいて、いつも楽しそうに遊んでいますよ。"},{"id":{"d":669,"n":3,"t":2},"speaker":"A","text":"けんかなどはしていませんか？"},{"id":{"d":669,"n":4,"t":2},"speaker":"B","text":"言い合いになることはありますが、手が出るようなことはありませんよ。"},{"id":{"d":669,"n":5,"t":2},"speaker":"A","text":"そうですか。良かったです。"},{"id":{"d":669,"n":6,"t":2},"speaker":"B","text":"浩章君はよく気が付いて、困っている子に声を掛けてくれるので、助かっています。"},{"id":{"d":669,"n":7,"t":2},"speaker":"A","text":"浩章は家でも妹の世話をしてくれているので、慣れているのかもしれません。"},{"id":{"d":669,"n":8,"t":2},"speaker":"B","text":"本当に優しいですね。幼稚園でも浩章君の良さを伸ばしていきたいです。"}];
INSERT INTO sentence [{"id":{"d":670,"n":1,"t":2},"speaker":"A","text":"もしもし、横田バイオリンスクールです。"},{"id":{"d":670,"n":2,"t":2},"speaker":"B","text":"いつもお世話になっております、竹岡です。すみませんが息子が風邪をひいてしまったので今日のレッスンはお休みします。"},{"id":{"d":670,"n":3,"t":2},"speaker":"A","text":"分かりました。振替レッスンはいつにしますか？"},{"id":{"d":670,"n":4,"t":2},"speaker":"B","text":"来月の19日は大丈夫ですか？"},{"id":{"d":670,"n":5,"t":2},"speaker":"A","text":"18時以降なら大丈夫です。"},{"id":{"d":670,"n":6,"t":2},"speaker":"B","text":"それでは18時でお願いします。"},{"id":{"d":670,"n":7,"t":2},"speaker":"A","text":"来月の19日の18時ですね。お待ちしています。息子さんに、「お大事に」とお伝えください。"},{"id":{"d":670,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。それでは、次のレッスンをよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":671,"n":1,"t":2},"speaker":"A","text":"制服のサイズ直しをお願いします。"},{"id":{"d":671,"n":2,"t":2},"speaker":"B","text":"はい。どのように直しますか？"},{"id":{"d":671,"n":3,"t":2},"speaker":"A","text":"子どもの背が伸びてズボンが短く感じるようになってしまったので、丈を長くしたいです。"},{"id":{"d":671,"n":4,"t":2},"speaker":"B","text":"どのくらい長くしますか？"},{"id":{"d":671,"n":5,"t":2},"speaker":"A","text":"2センチメートルの延長をお願いします。"},{"id":{"d":671,"n":6,"t":2},"speaker":"B","text":"分かりました。ズボンをお預かりします。"},{"id":{"d":671,"n":7,"t":2},"speaker":"A","text":"直すのにどのくらいの日数がかかりますか？"},{"id":{"d":671,"n":8,"t":2},"speaker":"B","text":"3日ほどかかります。"}];
INSERT INTO sentence [{"id":{"d":672,"n":1,"t":2},"speaker":"A","text":"もしもし、北条ですが美幸さんはいらっしゃいますか？"},{"id":{"d":672,"n":2,"t":2},"speaker":"B","text":"もしもし、美幸です。どうしたのですか？"},{"id":{"d":672,"n":3,"t":2},"speaker":"A","text":"すみません、今日の宿題を教えてもらえませんか？"},{"id":{"d":672,"n":4,"t":2},"speaker":"B","text":"メモをしなかったのですか？"},{"id":{"d":672,"n":5,"t":2},"speaker":"A","text":"はい、すっかり忘れていました。"},{"id":{"d":672,"n":6,"t":2},"speaker":"B","text":"今日の宿題は、算数の時間に配られたプリントです。"},{"id":{"d":672,"n":7,"t":2},"speaker":"A","text":"教えてくれてありがとうございます。"},{"id":{"d":672,"n":8,"t":2},"speaker":"B","text":"それから、「明日、漢字テストがあるので準備しておくように」と先生が仰っていましたよ。"},{"id":{"d":672,"n":9,"t":2},"speaker":"A","text":"分かりました。準備します。どうもありがとうございました。"},{"id":{"d":672,"n":10,"t":2},"speaker":"B","text":"どういたしまして。では、明日学校で。"}];
INSERT INTO sentence [{"id":{"d":673,"n":1,"t":2},"speaker":"A","text":"明日は就職の面接があります。"},{"id":{"d":673,"n":2,"t":2},"speaker":"B","text":"緊張しているようですね。"},{"id":{"d":673,"n":3,"t":2},"speaker":"A","text":"はい。今から緊張して、他のことが手に付きません。"},{"id":{"d":673,"n":4,"t":2},"speaker":"B","text":"あまり緊張すると、思わぬ失敗をしてしまいますよ。リラックスしましょう。"},{"id":{"d":673,"n":5,"t":2},"speaker":"A","text":"そうですよね。準備をしっかりして気持ちを落ち着けて臨みたいと思います。"},{"id":{"d":673,"n":6,"t":2},"speaker":"B","text":"面接で質問されそうなことを想定して、自分なりに答えを用意しておくと良いと思いますよ。"},{"id":{"d":673,"n":7,"t":2},"speaker":"A","text":"志望動機や、取り組んでみたい仕事などは考えをまとめてあります。"},{"id":{"d":673,"n":8,"t":2},"speaker":"B","text":"それなら大丈夫ですよ。安心して面接に臨んでください。"}];
INSERT INTO sentence [{"id":{"d":674,"n":1,"t":2},"speaker":"A","text":"お兄ちゃん、宿題で分からないところがあります。"},{"id":{"d":674,"n":2,"t":2},"speaker":"B","text":"どこですか？"},{"id":{"d":674,"n":3,"t":2},"speaker":"A","text":"理科のこの問題です。"},{"id":{"d":674,"n":4,"t":2},"speaker":"B","text":"化学の実験に関する問題ですね。"},{"id":{"d":674,"n":5,"t":2},"speaker":"A","text":"はい。化学は苦手です。"},{"id":{"d":674,"n":6,"t":2},"speaker":"B","text":"実験で使われる物質の化学式をしっかり書ければ、化学反応式も正確に書けますよ。"},{"id":{"d":674,"n":7,"t":2},"speaker":"A","text":"化学式を覚えられません。"},{"id":{"d":674,"n":8,"t":2},"speaker":"B","text":"よく出てくるものは、何度も問題を解いているうちに自然と覚えていきますよ。"}];
INSERT INTO sentence [{"id":{"d":675,"n":1,"t":2},"speaker":"A","text":"小学校生活で一番思い出に残っていることは何ですか？"},{"id":{"d":675,"n":2,"t":2},"speaker":"B","text":"お世話になった担任の先生のことです。"},{"id":{"d":675,"n":3,"t":2},"speaker":"A","text":"どのような先生だったのですか？"},{"id":{"d":675,"n":4,"t":2},"speaker":"B","text":"明るく元気いっぱいで、いつも子どもの気持ちに寄り添ってくれるような先生でした。"},{"id":{"d":675,"n":5,"t":2},"speaker":"A","text":"すてきな先生ですね。"},{"id":{"d":675,"n":6,"t":2},"speaker":"B","text":"はい。クラスの全員がその先生を慕っていたと思います。"},{"id":{"d":675,"n":7,"t":2},"speaker":"A","text":"何年生の時の担任の先生ですか？"},{"id":{"d":675,"n":8,"t":2},"speaker":"B","text":"6年生の時です。卒業以来お会いしていないので、今どうしているか知りたいです。"}];
INSERT INTO sentence [{"id":{"d":676,"n":1,"t":2},"speaker":"A","text":"このクラスでお世話になる、教育実習生の富沢です。よろしくお願いします。"},{"id":{"d":676,"n":2,"t":2},"speaker":"B","text":"よろしくお願いします。先生は大学生ですよね？"},{"id":{"d":676,"n":3,"t":2},"speaker":"A","text":"はい。この高校を卒業して、今は大学生です。"},{"id":{"d":676,"n":4,"t":2},"speaker":"B","text":"この学校の卒業生なのですか？"},{"id":{"d":676,"n":5,"t":2},"speaker":"A","text":"そうです。皆さんの先輩にあたります。"},{"id":{"d":676,"n":6,"t":2},"speaker":"B","text":"それでは、知っている先生もいるのでしょうね。"},{"id":{"d":676,"n":7,"t":2},"speaker":"A","text":"はい。英語の戸川先生は、私が3年生だった時の担任の先生です。"},{"id":{"d":676,"n":8,"t":2},"speaker":"B","text":"何だか不思議な感じです。"},{"id":{"d":676,"n":9,"t":2},"speaker":"A","text":"私も、まさか母校で授業する日が来るとは夢にも思っていませんでした。"}];
INSERT INTO sentence [{"id":{"d":677,"n":1,"t":2},"speaker":"A","text":"日下部さんは英語の発音が素晴らしいですね。"},{"id":{"d":677,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。米国に5年間住んでいたのでそのおかげかもしれません。"},{"id":{"d":677,"n":3,"t":2},"speaker":"A","text":"そうでしたか。その時は現地の学校に通っていたのですか？"},{"id":{"d":677,"n":4,"t":2},"speaker":"B","text":"はい。当時は日本語より英語の方が得意でした。"},{"id":{"d":677,"n":5,"t":2},"speaker":"A","text":"それでは、日本の学校に転校して困ったことはありませんでしたか？"},{"id":{"d":677,"n":6,"t":2},"speaker":"B","text":"会話は大丈夫でしたが、漢字を書くことが苦手で苦労しました。"},{"id":{"d":677,"n":7,"t":2},"speaker":"A","text":"でも今は、漢字も問題ありませんね。"},{"id":{"d":677,"n":8,"t":2},"speaker":"B","text":"それなら良かったです。毎日家で漢字を猛勉強しました。"}];
INSERT INTO sentence [{"id":{"d":678,"n":1,"t":2},"speaker":"A","text":"弟が大学の入試で不合格になってしまいました。"},{"id":{"d":678,"n":2,"t":2},"speaker":"B","text":"残念ですね。落ち込んでいるでしょう。"},{"id":{"d":678,"n":3,"t":2},"speaker":"A","text":"勉強を頑張っていたので、余計にショックが大きいようです。"},{"id":{"d":678,"n":4,"t":2},"speaker":"B","text":"これからどうするのですか？"},{"id":{"d":678,"n":5,"t":2},"speaker":"A","text":"本人は「浪人して、もう一度受験したい」と言っています。"},{"id":{"d":678,"n":6,"t":2},"speaker":"B","text":"本当に行きたい大学なのですね。"},{"id":{"d":678,"n":7,"t":2},"speaker":"A","text":"はい。でも浪人しても来年の大学入試に受かるとは限らないので、両親はまだ了承していません。"},{"id":{"d":678,"n":8,"t":2},"speaker":"B","text":"ご両親の気持ちも分かります。"},{"id":{"d":678,"n":9,"t":2},"speaker":"A","text":"私自身も、両方の気持ちが理解できて複雑な気分です。"}];
INSERT INTO sentence [{"id":{"d":679,"n":1,"t":2},"speaker":"A","text":"娘さんはおいくつですか？"},{"id":{"d":679,"n":2,"t":2},"speaker":"B","text":"24歳です。"},{"id":{"d":679,"n":3,"t":2},"speaker":"A","text":"今は何をしているのですか？"},{"id":{"d":679,"n":4,"t":2},"speaker":"B","text":"司法試験に向けて勉強しています。"},{"id":{"d":679,"n":5,"t":2},"speaker":"A","text":"そうですか。司法試験は難しいので勉強が大変でしょうね。"},{"id":{"d":679,"n":6,"t":2},"speaker":"B","text":"もう2度落ちているので、「次の試験で受からなかったら諦める」と言っています。"},{"id":{"d":679,"n":7,"t":2},"speaker":"A","text":"絶対に合格してほしいですね。"},{"id":{"d":679,"n":8,"t":2},"speaker":"B","text":"はい。今まで長い間頑張ってきたので、ぜひ合格してもらいたいです。"}];
INSERT INTO sentence [{"id":{"d":680,"n":1,"t":2},"speaker":"A","text":"学園祭のクラスの出し物は喫茶店にしたいと思っています。"},{"id":{"d":680,"n":2,"t":2},"speaker":"B","text":"残念ながら、生徒会からの通達で、今年は飲食店の出し物は禁止です。"},{"id":{"d":680,"n":3,"t":2},"speaker":"A","text":"去年もおととしも学園祭自体が開催されませんでした。今年こそは喫茶店を行いたいのです。"},{"id":{"d":680,"n":4,"t":2},"speaker":"B","text":"思い入れは分かりましたが、ルールは守らなければなりません。出し物をクラスでもう一度議論してください。"},{"id":{"d":680,"n":5,"t":2},"speaker":"A","text":"分かりました。それでは、私たちのクラスの出し物はお化け屋敷にします。"},{"id":{"d":680,"n":6,"t":2},"speaker":"B","text":"今ここで決めるのではなく、クラスでもう一度話し合ってもらえないでしょうか？"},{"id":{"d":680,"n":7,"t":2},"speaker":"A","text":"クラスメイトからは「喫茶店が認められなかった場合は、お化け屋敷を申し込むように」と言われていました。"},{"id":{"d":680,"n":8,"t":2},"speaker":"B","text":"そうでしたか。では、お化け屋敷ということで承知しました。"}];
INSERT INTO sentence [{"id":{"d":681,"n":1,"t":2},"speaker":"A","text":"学園祭の開始時に花火を上げることになりました。"},{"id":{"d":681,"n":2,"t":2},"speaker":"B","text":"やっと決まりましたか。生徒会の案が教職員に認められて良かったです。"},{"id":{"d":681,"n":3,"t":2},"speaker":"A","text":"ところが条件があるのです。「花火は音が大きいので近隣の了承が必要不可欠」と言われました。"},{"id":{"d":681,"n":4,"t":2},"speaker":"B","text":"具体的に何をするのでしょうか？"},{"id":{"d":681,"n":5,"t":2},"speaker":"A","text":"1丁目から5丁目までの各町内で回覧板を使って、賛否を問うらしいです。"},{"id":{"d":681,"n":6,"t":2},"speaker":"B","text":"回覧板では時間がかかりますし、賛成意見が少ない場合も考えられますね。"},{"id":{"d":681,"n":7,"t":2},"speaker":"A","text":"私もそう思います。そこでですが生徒会で町内を1軒1軒回って署名を集めてはどうでしょうか？"},{"id":{"d":681,"n":8,"t":2},"speaker":"B","text":"それは良い考えです。町内に住んでいる生徒も多いので、協力してもらってはどうでしょうか？"},{"id":{"d":681,"n":9,"t":2},"speaker":"A","text":"分かりました。これからすぐに署名用紙を作って、明日の朝には各クラスの委員に渡せるようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":682,"n":1,"t":2},"speaker":"A","text":"先生、サッカー部の内容で高校を決めたいので、セレクションのある高校を教えてください。"},{"id":{"d":682,"n":2,"t":2},"speaker":"B","text":"セレクションとなると顧問の先生や保護者の方の意見も必要になってきます。"},{"id":{"d":682,"n":3,"t":2},"speaker":"A","text":"両親は「自分の学力と同等な高校ならばセレクションで入ってもよい」と言っています。"},{"id":{"d":682,"n":4,"t":2},"speaker":"B","text":"そうですか。部活の顧問の先生はどこか推薦してくれませんでしたか？"},{"id":{"d":682,"n":5,"t":2},"speaker":"A","text":"顧問の先生はセレクションの高校が決まったら、その高校と話をしてくれるそうです。"},{"id":{"d":682,"n":6,"t":2},"speaker":"B","text":"セレクションの日程は高校ごとに発表されますが、どうやらまだ決まっていないようですね。"},{"id":{"d":682,"n":7,"t":2},"speaker":"A","text":"いつ頃決まるか分かりますか？"},{"id":{"d":682,"n":8,"t":2},"speaker":"B","text":"セレクションは夏の大会以降に始まるそうです。"},{"id":{"d":682,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":683,"n":1,"t":2},"speaker":"A","text":"先生、来週の学力テストは1学期の成績と関係あるのですか？"},{"id":{"d":683,"n":2,"t":2},"speaker":"B","text":"県が学力調査のために行うものなので、成績とは直接は関係ありません。"},{"id":{"d":683,"n":3,"t":2},"speaker":"A","text":"直接とはどういうことでしょうか？"},{"id":{"d":683,"n":4,"t":2},"speaker":"B","text":"成績には反映されませんが、保護者面談の際に志望校について話し合うときには資料となります。"},{"id":{"d":683,"n":5,"t":2},"speaker":"A","text":"極端に悪い成績は志望校を決める際には問題となるわけですか？"},{"id":{"d":683,"n":6,"t":2},"speaker":"B","text":"そうですね。合格のレベルに達していなければ、そのことを認識してもらうことになります。"},{"id":{"d":683,"n":7,"t":2},"speaker":"A","text":"それでは、学力テストのために勉強をした方がいいのですか？"},{"id":{"d":683,"n":8,"t":2},"speaker":"B","text":"もちろんです。ただし範囲は広いので、定期考査のような勉強ではなく、普段の受験勉強を多めにした方がいいです。"},{"id":{"d":683,"n":9,"t":2},"speaker":"A","text":"はい、ありがとうございます。できるだけ頑張ります。"}];
INSERT INTO sentence [{"id":{"d":684,"n":1,"t":2},"speaker":"A","text":"あなたはよく「人より劣っている」と言いますが、そのようなことはないと思いますよ。"},{"id":{"d":684,"n":2,"t":2},"speaker":"B","text":"そうでしょうか。大学時代の友人と比べると自分はできないことばかりで劣等感があります。"},{"id":{"d":684,"n":3,"t":2},"speaker":"A","text":"しかし、そのことを口に出すと気分を悪くする方もいらっしゃると思いますよ。"},{"id":{"d":684,"n":4,"t":2},"speaker":"B","text":"分かりました。今後は気を付けることとします。"},{"id":{"d":684,"n":5,"t":2},"speaker":"A","text":"周りを知ることは良いことですが、周りの人たちと自分を比べる必要はありませんよ。"},{"id":{"d":684,"n":6,"t":2},"speaker":"B","text":"確かに競争しているわけではないので、比べる必要はないのかもしれません。"},{"id":{"d":684,"n":7,"t":2},"speaker":"A","text":"はい。お互いに認め合い、補い合うことで社会は活性化します。"},{"id":{"d":684,"n":8,"t":2},"speaker":"B","text":"人に何でもしてもらうと自分の存在感が薄れたような気がしませんか？"},{"id":{"d":684,"n":9,"t":2},"speaker":"A","text":"できないからといって、卑下する必要はありません。自分なりに少しずつ前に進めば良いのです。"}];
INSERT INTO sentence [{"id":{"d":685,"n":1,"t":2},"speaker":"A","text":"ラグビー部が全国大会に出場することになりました。"},{"id":{"d":685,"n":2,"t":2},"speaker":"B","text":"昨日の県大会決勝を競技場で見ていました。素晴らしいですね。"},{"id":{"d":685,"n":3,"t":2},"speaker":"A","text":"はい、誇らしいです。全国大会出場にあたり、壮行会を行いたいと考えています。"},{"id":{"d":685,"n":4,"t":2},"speaker":"B","text":"それは良い案です。今回が初めての全国大会出場なので思い付きもしませんでした。"},{"id":{"d":685,"n":5,"t":2},"speaker":"A","text":"学校の意向をお聞きした上で、選手と彼らの保護者を中心とした壮行会を開催しましょう。"},{"id":{"d":685,"n":6,"t":2},"speaker":"B","text":"それは良い考えです。壮行会開催の費用はどうしましょうか？"},{"id":{"d":685,"n":7,"t":2},"speaker":"A","text":"保護者や卒業生の有志から寄付を募りましょう。"}];
INSERT INTO sentence [{"id":{"d":686,"n":1,"t":2},"speaker":"A","text":"近年の大学では、文学部の必要性を問われるようになってきています。"},{"id":{"d":686,"n":2,"t":2},"speaker":"B","text":"文学部に入学する学生数が減っているのでしょうか？"},{"id":{"d":686,"n":3,"t":2},"speaker":"A","text":"そのようです。"},{"id":{"d":686,"n":4,"t":2},"speaker":"B","text":"新しい学部が増えたことと関係はありませんか？"},{"id":{"d":686,"n":5,"t":2},"speaker":"A","text":"それは否定できません。しかし、文学に対する考えも昔とは変わってきていると思います。"},{"id":{"d":686,"n":6,"t":2},"speaker":"B","text":"具体的にはどう変わっているのでしょうか？"},{"id":{"d":686,"n":7,"t":2},"speaker":"A","text":"文学は社会の役に立たないという考え方に変わってきているようです。"},{"id":{"d":686,"n":8,"t":2},"speaker":"B","text":"それは残念ですね。大学の学部は社会における必要性だけを考えることではないと思います。"},{"id":{"d":686,"n":9,"t":2},"speaker":"A","text":"同意します。表面的な必要性だけを議論して安易に学部の存続を決めることは望ましくありません。"},{"id":{"d":686,"n":10,"t":2},"speaker":"B","text":"そうですね。時代の需要に応じて入学する学生の数を調整することは良いと思いますが、学部の存続については慎重に議論するべきでしょう。"}];
INSERT INTO sentence [{"id":{"d":687,"n":1,"t":2},"speaker":"A","text":"習い事はどのようなことをしてきましたか？"},{"id":{"d":687,"n":2,"t":2},"speaker":"B","text":"いろいろ習いましたよ。最初はピアノでした。"},{"id":{"d":687,"n":3,"t":2},"speaker":"A","text":"自分からピアノ習いを始めたのですか？"},{"id":{"d":687,"n":4,"t":2},"speaker":"B","text":"はい、自分からピアノを習い始めました。少し後でスイミングスクールにも通うようになります。"},{"id":{"d":687,"n":5,"t":2},"speaker":"A","text":"スイミングスクールはいつまで通いましたか？"},{"id":{"d":687,"n":6,"t":2},"speaker":"B","text":"スイミングスクールもピアノも小学3年の終わり頃まで通いました。スイミングスクールとピアノをやめた後は、少年野球と学習塾と英会話教室に通うようになります。"},{"id":{"d":687,"n":7,"t":2},"speaker":"A","text":"忙しそうですが、少年野球と学習塾と英会話教室も自分の意志で始めたのですか？"},{"id":{"d":687,"n":8,"t":2},"speaker":"B","text":"はい。そのはずですが、今思うと両親がさりげなく少年野球と学習塾と英会話教室を勧めたのかも知れません。"}];
INSERT INTO sentence [{"id":{"d":688,"n":1,"t":2},"speaker":"A","text":"どのように勉強したら満点が取れるようになるのでしょうか？"},{"id":{"d":688,"n":2,"t":2},"speaker":"B","text":"全教科満点を取るということですか？"},{"id":{"d":688,"n":3,"t":2},"speaker":"A","text":"もちろんそうです。一度目指してみたいのです。"},{"id":{"d":688,"n":4,"t":2},"speaker":"B","text":"応援しています。私は、暗記しなくても解ける問題を増やすことが高得点への近道だと思います。"},{"id":{"d":688,"n":5,"t":2},"speaker":"A","text":"もう少し、具体的に説明していただけますか？"},{"id":{"d":688,"n":6,"t":2},"speaker":"B","text":"国語や英語は試験用紙の中に答えがあるのですから、記憶に頼って答えを出す必要はありません。"},{"id":{"d":688,"n":7,"t":2},"speaker":"A","text":"考えればできる問題に、記憶力を使う必要はないということですね。"},{"id":{"d":688,"n":8,"t":2},"speaker":"B","text":"はい。記憶量を少なくすれば、本当に記憶しなければならない問題に記憶力を使うことができます。"}];
INSERT INTO sentence [{"id":{"d":689,"n":1,"t":2},"speaker":"A","text":"小学校には今も飼育小屋があるのですか？"},{"id":{"d":689,"n":2,"t":2},"speaker":"B","text":"私の通っている小学校にはありますが、隣町の小学校にはありません。"},{"id":{"d":689,"n":3,"t":2},"speaker":"A","text":"そうですか。徐々に飼育小屋はなくなりつつあるのかもしれませんね。"},{"id":{"d":689,"n":4,"t":2},"speaker":"B","text":"お母さんが小学生のときは、どこの学校にも飼育小屋がありましたか？"},{"id":{"d":689,"n":5,"t":2},"speaker":"A","text":"はい。少なくとも市内の小学校には必ずありました。私の学校にはウサギ小屋とニワトリ小屋がありましたよ。"},{"id":{"d":689,"n":6,"t":2},"speaker":"B","text":"私の小学校にはニワトリ小屋だけです。なぜ飼育小屋はなくなっていくのでしょうか？"},{"id":{"d":689,"n":7,"t":2},"speaker":"A","text":"想像ですが、教育の現場そのものが忙しくなってきて、動物の世話まで手が回らなくなっているのかも知れませんね。"}];
INSERT INTO sentence [{"id":{"d":690,"n":1,"t":2},"speaker":"A","text":"来春の受験に向けて合格祈願に行きませんか？"},{"id":{"d":690,"n":2,"t":2},"speaker":"B","text":"良いですね。東京にある学問の神様として有名な神社はどこでしょうか？"},{"id":{"d":690,"n":3,"t":2},"speaker":"A","text":"有名な神社は湯島天満宮です。他に亀戸天神社や谷保天満宮も学問の神様で有名です。"},{"id":{"d":690,"n":4,"t":2},"speaker":"B","text":"どのような理由で学問の神様と呼ばれるのですか？"},{"id":{"d":690,"n":5,"t":2},"speaker":"A","text":"菅原道真に由来のある神社はそう呼ばれます。3つの神社はどれも菅原道真に関係しています。"},{"id":{"d":690,"n":6,"t":2},"speaker":"B","text":"菅原道真が埋葬されている太宰府天満宮が学問の神様であることは知っていますが、なぜ他にもあるのでしょうか？"},{"id":{"d":690,"n":7,"t":2},"speaker":"A","text":"菅原道真を祭神する神社が増加したからだと思います。"},{"id":{"d":690,"n":8,"t":2},"speaker":"B","text":"天満宮は菅原道真が祭神ですか？"},{"id":{"d":690,"n":9,"t":2},"speaker":"A","text":"確か菅原道真が祭神です。ところでどこに祈願に行きますか？"},{"id":{"d":690,"n":10,"t":2},"speaker":"B","text":"ここから一番近い湯島天満宮にしましょう。"}];
INSERT INTO sentence [{"id":{"d":691,"n":1,"t":2},"speaker":"A","text":"今年の卒業式でも、卒業証書を授与している間、吹奏楽部が校歌を演奏することになりました。"},{"id":{"d":691,"n":2,"t":2},"speaker":"B","text":"去年と同じ形式で校歌を演奏するのですか？"},{"id":{"d":691,"n":3,"t":2},"speaker":"A","text":"そうです。部員の中から10人くらいソリストを選んで順番に演奏してもらいます。"},{"id":{"d":691,"n":4,"t":2},"speaker":"B","text":"演奏時間はどのくらいでしょうか？"},{"id":{"d":691,"n":5,"t":2},"speaker":"A","text":"去年は10クラスそれぞれの代表者がクラス全員の卒業証書を受け取り、それで30分ほどかかりました。"},{"id":{"d":691,"n":6,"t":2},"speaker":"B","text":"校歌は1番だけ演奏して次の奏者に移りますから、1人3回から4回演奏することになりますね。"},{"id":{"d":691,"n":7,"t":2},"speaker":"A","text":"そこで相談なのですが、昨年までのように1番の演奏だけで奏者が交代するとせわしない感じがするので、今年は1人で3番まで演奏し終わってから次の奏者に移りませんか？"},{"id":{"d":691,"n":8,"t":2},"speaker":"B","text":"確かにそうかもしれません。"},{"id":{"d":691,"n":9,"t":2},"speaker":"A","text":"そして最後はソロではなく吹奏楽部全員の合奏で終わりましょう。"},{"id":{"d":691,"n":10,"t":2},"speaker":"B","text":"それは良い考えですね。賛成です。"}];
INSERT INTO sentence [{"id":{"d":692,"n":1,"t":2},"speaker":"A","text":"漢字検定を受けてみたいです。何級から受けたら良いでしょうか？"},{"id":{"d":692,"n":2,"t":2},"speaker":"B","text":"過去問題を解いたことはありますか？"},{"id":{"d":692,"n":3,"t":2},"speaker":"A","text":"いえ、ありません。問題集を購入するにも、まず何級を受験するか決めなければなりません。"},{"id":{"d":692,"n":4,"t":2},"speaker":"B","text":"過去問題はウェブサイトから無料でダウンロードできます。いくつかやってみて自分でできそうなところを受験したらどうでしょうか？"},{"id":{"d":692,"n":5,"t":2},"speaker":"A","text":"それは知りませんでした。早速ウェブサイトを確認してみます。"}];
INSERT INTO sentence [{"id":{"d":693,"n":1,"t":2},"speaker":"A","text":"掲示板で漢字検定のポスターを見ました。この高校でも漢字検定は受験できますか？"},{"id":{"d":693,"n":2,"t":2},"speaker":"B","text":"はい。漢字検定は学校で受験できますよ。申し込もここでできます。"},{"id":{"d":693,"n":3,"t":2},"speaker":"A","text":"漢字検定受験の申し込みはいつ締め切りですか？"},{"id":{"d":693,"n":4,"t":2},"speaker":"B","text":"締め切りは今日からちょうど2週間後です。受験日はさらに1カ月後ですから勉強する時間はあります。"},{"id":{"d":693,"n":5,"t":2},"speaker":"A","text":"申込をしてから受験当日まで、試験対策をする期間は十分あるのですね。"},{"id":{"d":693,"n":6,"t":2},"speaker":"B","text":"そうです。下手に焦らず、まずは過去問題をたくさん解いて慣れるところから始めると良いですよ。"},{"id":{"d":693,"n":7,"t":2},"speaker":"A","text":"分かりました。過去問題を解いてみます。"}];
INSERT INTO sentence [{"id":{"d":694,"n":1,"t":2},"speaker":"A","text":"「高専」とは何ですか。専門学校とは違うのですか？"},{"id":{"d":694,"n":2,"t":2},"speaker":"B","text":"正式名称は高等専門学校といいます。5年生の工業高校と考えるといいでしょう。"},{"id":{"d":694,"n":3,"t":2},"speaker":"A","text":"5年では大学の入学時期と合いませんが、大学にはどのように進学するのですか？"},{"id":{"d":694,"n":4,"t":2},"speaker":"B","text":"工業系の大学に編入できます。また専攻科と呼ばれる学科を卒業すれば大卒と同じ資格を有することができます。"},{"id":{"d":694,"n":5,"t":2},"speaker":"A","text":"編入できる大学は限られているのですか？"},{"id":{"d":694,"n":6,"t":2},"speaker":"B","text":"成績にもよりますが、各地の国立大に編入できます。"},{"id":{"d":694,"n":7,"t":2},"speaker":"A","text":"それでは高専の学生は皆さん大学編入を考えるのではないでしょうか？"},{"id":{"d":694,"n":8,"t":2},"speaker":"B","text":"高専の卒業後、就職する人も多いです。高専の就職希望者には1人に対して10社ほどの求人が届くともいわれているほどです。"},{"id":{"d":694,"n":9,"t":2},"speaker":"A","text":"知りませんでした。それだけ優秀な人が多いのですね。"},{"id":{"d":694,"n":10,"t":2},"speaker":"B","text":"高専は即戦力となる若手を育むことを目的の1つにしているそうです。なので、企業からの人気も高いのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":695,"n":1,"t":2},"speaker":"A","text":"公立高校で楽しく野球をしたいのですが、そのような高校はありますか？"},{"id":{"d":695,"n":2,"t":2},"speaker":"B","text":"楽しく野球をするとは、具体的にはどのようなことでしょうか？"},{"id":{"d":695,"n":3,"t":2},"speaker":"A","text":"仲間と一緒に和気あいあいとスポーツをしたいのです。"},{"id":{"d":695,"n":4,"t":2},"speaker":"B","text":"選手が多いスポーツでは選手によって価値観も違うと思います。勝負にこだわる選手もいるのではないでしょうか？"},{"id":{"d":695,"n":5,"t":2},"speaker":"A","text":"まさに中学までがそうでした。あまりに勝負にこだわりすぎて野球をすることがつらくなってきたので、高校では楽しく野球をしたいと思っています。"},{"id":{"d":695,"n":6,"t":2},"speaker":"B","text":"そうでしたか。評判だけでは分からないことの方が多いので、部活も含めて実際に高校を見学した上で志望校を考えてはどうでしょうか？"},{"id":{"d":695,"n":7,"t":2},"speaker":"A","text":"そうですね。まずは見学をしてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":696,"n":1,"t":2},"speaker":"A","text":"授業参観に参加できるのは保護者だけですか？"},{"id":{"d":696,"n":2,"t":2},"speaker":"B","text":"基本的にはそうですが、何かありましたか？"},{"id":{"d":696,"n":3,"t":2},"speaker":"A","text":"父が先週事故に遭って、しばらく入院することになりました。"},{"id":{"d":696,"n":4,"t":2},"speaker":"B","text":"それは大変です。お体は大丈夫なのですか？"},{"id":{"d":696,"n":5,"t":2},"speaker":"A","text":"命に別状ありませんが、父の代わりに母が会社を運営することになりました。"},{"id":{"d":696,"n":6,"t":2},"speaker":"B","text":"確か君の家は自営業でしたね。そのような事情なら授業参観は次回でも構わないでしょう。"},{"id":{"d":696,"n":7,"t":2},"speaker":"A","text":"当日は授業参観の後で保護者会が開催されると聞きました。どのような議題なのか聞いてくるように母から言い付かりました。"},{"id":{"d":696,"n":8,"t":2},"speaker":"B","text":"大学入試までの日程を、わが高校の大学受験の取り組みとともにお伝えする予定です。"},{"id":{"d":696,"n":9,"t":2},"speaker":"A","text":"私が出席するわけにはいきませんか？"},{"id":{"d":696,"n":10,"t":2},"speaker":"B","text":"保護者会に生徒は参加できません。参考資料を保護者会当日にあなたに渡しますので、保護者の方にお届けください。"},{"id":{"d":696,"n":11,"t":2},"speaker":"A","text":"母は進路についての質問があるそうですが、どうすれば良いでしょうか？"},{"id":{"d":696,"n":12,"t":2},"speaker":"B","text":"保護者面談は随時受け付けていますから、ぜひご利用くださいとお伝えください。"}];
INSERT INTO sentence [{"id":{"d":697,"n":1,"t":2},"speaker":"A","text":"思い出の先生はいますか？"},{"id":{"d":697,"n":2,"t":2},"speaker":"B","text":"あなたが想像している先生とは少し違うかもしれませんが、保育園の園長先生が記憶に残っています。"},{"id":{"d":697,"n":3,"t":2},"speaker":"A","text":"随分幼い時の思い出のようですが、何が記憶に残っているのですか？"},{"id":{"d":697,"n":4,"t":2},"speaker":"B","text":"園長先生はマジックが得意で、暇があると私たちにマジックを見せてくれたことが記憶に残っています。"},{"id":{"d":697,"n":5,"t":2},"speaker":"A","text":"子どもたちは喜ぶでしょうね。"},{"id":{"d":697,"n":6,"t":2},"speaker":"B","text":"今考えると、やんちゃをしようとする子どもたちを静かにさせるための方法だったのかも知れません。"},{"id":{"d":697,"n":7,"t":2},"speaker":"A","text":"そうなのでしょうが、やんちゃをしてから子どもを叱るよりはずっとスマートです。素晴らしい先生ですね。"},{"id":{"d":697,"n":8,"t":2},"speaker":"B","text":"はい。いつもトランプを持ち歩いているのでトランプ先生と呼ばれていて、大人気でした。"}];
INSERT INTO sentence [{"id":{"d":698,"n":1,"t":2},"speaker":"A","text":"2年生の中から次の部長を選ばなければなりません。現部長のあなたは次の部長をどのように決めるのですか？"},{"id":{"d":698,"n":2,"t":2},"speaker":"B","text":"顧問の先生は、「部長であるあなたが決めるか、部員による話し合いや投票で決めてください」と言っていました。"},{"id":{"d":698,"n":3,"t":2},"speaker":"A","text":"顧問の先生が次の部長を決めることはないのでしょうか？"},{"id":{"d":698,"n":4,"t":2},"speaker":"B","text":"先生が次の部長を選ぶことはしないそうです。部員が少ないですから、部員全員による話し合いで決めるのはどうでしょうか？"},{"id":{"d":698,"n":5,"t":2},"speaker":"A","text":"良いと思います。話し合いで決まったら、その人を部長にするということですか？"},{"id":{"d":698,"n":6,"t":2},"speaker":"B","text":"そのつもりです。"},{"id":{"d":698,"n":7,"t":2},"speaker":"A","text":"では、話し合いで決まらなければどうしますか？"},{"id":{"d":698,"n":8,"t":2},"speaker":"B","text":"その時は、話し合いでの意見を考慮して私が決めます。"}];
INSERT INTO sentence [{"id":{"d":699,"n":1,"t":2},"speaker":"A","text":"今日からこの講義を受けるのですが、教科書がまだ入手できていません。教科書を見せていただけますか？"},{"id":{"d":699,"n":2,"t":2},"speaker":"B","text":"はい、どうぞ。"},{"id":{"d":699,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":699,"n":4,"t":2},"speaker":"B","text":"ところで、今まで授業に出席していなかったと思いますが、途中からでも受講できるのですか？この講座の教授は出席に厳しいことで有名です。"},{"id":{"d":699,"n":5,"t":2},"speaker":"A","text":"その件については、先ほど教授と話をしました。「レポートの提出と今日から授業に出席すれば、問題はない」と言われました。"},{"id":{"d":699,"n":6,"t":2},"speaker":"B","text":"そうでしたか。しかし余分にレポートを書くのは大変ではありませんか？"},{"id":{"d":699,"n":7,"t":2},"speaker":"A","text":"いえ。講義に出席できるチャンスを頂いただけでも感謝しています。レポートはお安いご用です。"}];
INSERT INTO sentence [{"id":{"d":700,"n":1,"t":2},"speaker":"A","text":"大学受験を申し込むには、今はWeb出願という出願方式もあるそうです。"},{"id":{"d":700,"n":2,"t":2},"speaker":"B","text":"時代の成り行きといえます。受験票の顔写真はどのようになっているのですか？"},{"id":{"d":700,"n":3,"t":2},"speaker":"A","text":"受験校の出願サイトに登録したメールアドレスでログインし、写真データをアップロードします。"},{"id":{"d":700,"n":4,"t":2},"speaker":"B","text":"受験票は受験校から郵送されるのですか？"},{"id":{"d":700,"n":5,"t":2},"speaker":"A","text":"ログインしたところに受験票のフォームがありますから、データを入力して各自で印刷します。"},{"id":{"d":700,"n":6,"t":2},"speaker":"B","text":"調査書や推薦書はデータ化するのですか？"},{"id":{"d":700,"n":7,"t":2},"speaker":"A","text":"それは志願票とともに郵送することになります。"},{"id":{"d":700,"n":8,"t":2},"speaker":"B","text":"郵送するということは、今までの出願と同じではありませんか？"},{"id":{"d":700,"n":9,"t":2},"speaker":"A","text":"確かにそうです。しかし、私が一番うれしいのは、受験料の支払いにクレジットカードが使えることです。"},{"id":{"d":700,"n":10,"t":2},"speaker":"B","text":"なるほど。郵便局や銀行で並ぶことがなくなるのは便利ですね。"}];
INSERT INTO sentence [{"id":{"d":701,"n":1,"t":2},"speaker":"A","text":"なぜこの高校を志望するのですか？"},{"id":{"d":701,"n":2,"t":2},"speaker":"B","text":"この高校には美術特待制度あります。それを利用したいと思っています。"},{"id":{"d":701,"n":3,"t":2},"speaker":"A","text":"美術コースを受験するのですか？あなたは普通高校から大学を目指すのだと思っていました。"},{"id":{"d":701,"n":4,"t":2},"speaker":"B","text":"はい。将来は美術大学を受験したいと思っています。この高校の美術コースは美術大学受験には最適です。"},{"id":{"d":701,"n":5,"t":2},"speaker":"A","text":"特待制度を利用するそうですけれど、それならばセレクションがあります。美術のことは私には分かりませんが、自信はあるのですか？"},{"id":{"d":701,"n":6,"t":2},"speaker":"B","text":"そこまで自信があるわけではないのですが、通っている美術教室の先生の話では受験する価値は十分あるそうです。"},{"id":{"d":701,"n":7,"t":2},"speaker":"A","text":"専門家がそのように仰るのなら挑戦する価値はありそうです。セレクションの内容は何ですか？"},{"id":{"d":701,"n":8,"t":2},"speaker":"B","text":"試験内容は80分の鉛筆デッサンと面接です。特待制度に不合格でも美術コースを受験することはできます。"},{"id":{"d":701,"n":9,"t":2},"speaker":"A","text":"特待生のセレクションは腕試しなわけですね。"},{"id":{"d":701,"n":10,"t":2},"speaker":"B","text":"はい。美術大学を目指しているので、このような機会は積極的に利用したいと思います。"}];
INSERT INTO sentence [{"id":{"d":702,"n":1,"t":2},"speaker":"A","text":"月曜日に恒例になっていた朝礼がなくなりました。"},{"id":{"d":702,"n":2,"t":2},"speaker":"B","text":"うれしいです。熱中症対策を考えれば、このまま朝礼はなくなって良いと思います。"},{"id":{"d":702,"n":3,"t":2},"speaker":"A","text":"そうでしょうか。朝礼の有無についてはさらに話し合いが必要なように思います。"},{"id":{"d":702,"n":4,"t":2},"speaker":"B","text":"朝礼がなくなることで何か問題が出て来るのですか？"},{"id":{"d":702,"n":5,"t":2},"speaker":"A","text":"はい。問題が出て来ると思います。子どもたちの表彰や異動の先生たちの紹介などは朝礼の時に行われていましたから。"},{"id":{"d":702,"n":6,"t":2},"speaker":"B","text":"表彰や紹介は教室に設置されているテレビで、朝礼の内容を放映できないでしょうか？"},{"id":{"d":702,"n":7,"t":2},"speaker":"A","text":"なるほど。放送室で録画し、教室に設置しているテレビで放映するのですね。"},{"id":{"d":702,"n":8,"t":2},"speaker":"B","text":"はい。そうすれば、炎天下校庭に長時間いることなく、教室で快適に情報共有ができます。"},{"id":{"d":702,"n":9,"t":2},"speaker":"A","text":"それは素晴らしい案ですね。"}];
INSERT INTO sentence [{"id":{"d":703,"n":1,"t":2},"speaker":"A","text":"勉強中に他のことをしてしまうことなどありますか？"},{"id":{"d":703,"n":2,"t":2},"speaker":"B","text":"ありますよ。中学生の時はよくノートの空白部分に迷路を作っていました。"},{"id":{"d":703,"n":3,"t":2},"speaker":"A","text":"空白部分とはノートの上下にある罫線のない部分のことですか？"},{"id":{"d":703,"n":4,"t":2},"speaker":"B","text":"はい、そこに迷路を描くのです。そのページが終わると次のページにも続きを描きます。"},{"id":{"d":703,"n":5,"t":2},"speaker":"A","text":"1ページで終わりではありませんか？"},{"id":{"d":703,"n":6,"t":2},"speaker":"B","text":"全ページに渡ることもあります。完成した迷路は友人に解いてもらっていました。"}];
INSERT INTO sentence [{"id":{"d":704,"n":1,"t":2},"speaker":"A","text":"この頃は見なくなりましたが、昔はペン回しをしている人がたくさんいました。"},{"id":{"d":704,"n":2,"t":2},"speaker":"B","text":"確かに昔は流行っていましたね。私も授業中に一生懸命練習をしていました。"},{"id":{"d":704,"n":3,"t":2},"speaker":"A","text":"僕も練習をしていました。でもついぞ最後までできることはありませんでした。"},{"id":{"d":704,"n":4,"t":2},"speaker":"B","text":"私も最初の頃は全然できなくて、しょっちゅうペンを飛ばしては先生に怒られていました。"},{"id":{"d":704,"n":5,"t":2},"speaker":"A","text":"懐かしいですね。結局ペン回しはできるようになったのですか？"},{"id":{"d":704,"n":6,"t":2},"speaker":"B","text":"はい。一番簡単な技だけですが。"},{"id":{"d":704,"n":7,"t":2},"speaker":"A","text":"良いですね。今もできますか？"},{"id":{"d":704,"n":8,"t":2},"speaker":"B","text":"どうでしょうか。最近はペン回しをしたことがないので分かりません。"},{"id":{"d":704,"n":9,"t":2},"speaker":"A","text":"このペンで回してみてもらえませんか？"},{"id":{"d":704,"n":10,"t":2},"speaker":"B","text":"良いですよ。では、回してみますね。"}];
INSERT INTO sentence [{"id":{"d":705,"n":1,"t":2},"speaker":"A","text":"好きな学校の先生はいましたか？"},{"id":{"d":705,"n":2,"t":2},"speaker":"B","text":"特にはいませんが、強いて言うなら話の面白い先生は嫌いではありませんでした。"},{"id":{"d":705,"n":3,"t":2},"speaker":"A","text":"授業中の説明が上手な先生ということでしょうか？"},{"id":{"d":705,"n":4,"t":2},"speaker":"B","text":"はい。勉強の説明でも直接勉強と関係がなくても、話が面白ければそれだけで子どもの興味は先生に向くように思います。旅行の話をする先生や宇宙人の話をする先生のことは今でも覚えています。"},{"id":{"d":705,"n":5,"t":2},"speaker":"A","text":"先生なのですから、勉強の話だけをしてほしいとは思いませんでしたか？"},{"id":{"d":705,"n":6,"t":2},"speaker":"B","text":"思いませんでした。授業を進めるためには、まず子どもたちの注意を引くことが重要だと思います。それに話のうまい先生は、やっぱり授業の説明もうまかったように思います。"}];
INSERT INTO sentence [{"id":{"d":706,"n":1,"t":2},"speaker":"A","text":"小学校の時は班登校でしたか？"},{"id":{"d":706,"n":2,"t":2},"speaker":"B","text":"はい。交通量の激しいところが通学路になっていたので、班登校が基本でした。"},{"id":{"d":706,"n":3,"t":2},"speaker":"A","text":"班行動を乱す子どもはいませんでしたか？"},{"id":{"d":706,"n":4,"t":2},"speaker":"B","text":"いなかったと思います。町内会のおじさんたちが交通整理をしていたからかも知れません。"},{"id":{"d":706,"n":5,"t":2},"speaker":"A","text":"町内会で交通整理とは、よほど交通量の多いところだったのですね。"},{"id":{"d":706,"n":6,"t":2},"speaker":"B","text":"何年か前に事故があって、それから町内会で危険なところでは交通整理の方が立つようになったらしいです。"},{"id":{"d":706,"n":7,"t":2},"speaker":"A","text":"今も町内会の交通整理は続いているのですか？"},{"id":{"d":706,"n":8,"t":2},"speaker":"B","text":"その小学校自体、今はもうありません。少子化で他の学校と統合されてしまいました。"}];
INSERT INTO sentence [{"id":{"d":707,"n":1,"t":2},"speaker":"A","text":"高校の時は何部に所属されていましたか？"},{"id":{"d":707,"n":2,"t":2},"speaker":"B","text":"合唱部に所属していました。"},{"id":{"d":707,"n":3,"t":2},"speaker":"A","text":"君は、確か男子高校を卒業されていたと思います。男子高校にも合唱部があるのですか？"},{"id":{"d":707,"n":4,"t":2},"speaker":"B","text":"はい。男子高校でも合唱部のあるところは多いですよ。私の高校の合唱部は全国大会にも出ました。"},{"id":{"d":707,"n":5,"t":2},"speaker":"A","text":"男子だけの合唱部珍しくはありませんか？"},{"id":{"d":707,"n":6,"t":2},"speaker":"B","text":"高校の共学化が進んでいるので、男子だけの合唱部は確かに少なくなっていますが、まだまだ存在しますよ。"},{"id":{"d":707,"n":7,"t":2},"speaker":"A","text":"大会出場の規定には男女による違いはありませんか？"},{"id":{"d":707,"n":8,"t":2},"speaker":"B","text":"ありません。高校の部の規定は大編成と小編成の規定だけです。"},{"id":{"d":707,"n":9,"t":2},"speaker":"A","text":"そうなのですね。大会に出場する以外にはどのような活動をしていたのですか？"},{"id":{"d":707,"n":10,"t":2},"speaker":"B","text":"普段は、市内でコンサートを開催したり、夏祭りで合唱したりしていました。"}];
INSERT INTO sentence [{"id":{"d":708,"n":1,"t":2},"speaker":"A","text":"学校から菜園や花壇、飼育小屋がなくなると子どもたちの自然に触れる機会が少なくなりませんか？"},{"id":{"d":708,"n":2,"t":2},"speaker":"B","text":"そうかもしれませんね。私の学校にはビオトープがありましたが、そのおかげで自然に触れられました。"},{"id":{"d":708,"n":3,"t":2},"speaker":"A","text":"ビオトープとはどのようなものでしょうか？"},{"id":{"d":708,"n":4,"t":2},"speaker":"B","text":"ドイツ語で生物を意味する「Bio」と場所を意味する「Tope」の合成語で野生生物が共存強制できる生態系を持った場所のことです。"},{"id":{"d":708,"n":5,"t":2},"speaker":"A","text":"まだビオトープがイメージできません。あなたの学校のビオトープはどのようなものなのですか？"},{"id":{"d":708,"n":6,"t":2},"speaker":"B","text":"学校の敷地内に設けられた池から浄水設備などの人工物を取り除き、メダカや水草を入れていました。"},{"id":{"d":708,"n":7,"t":2},"speaker":"A","text":"ビオトープには生物が生活しやすいように、人間が手を加えませんか？"},{"id":{"d":708,"n":8,"t":2},"speaker":"B","text":"生態系を維持することが目的なので、なるべく自然な状態にしなければいけないようです。"},{"id":{"d":708,"n":9,"t":2},"speaker":"A","text":"ビオトープは授業などで使われているのでしょうか？"},{"id":{"d":708,"n":10,"t":2},"speaker":"B","text":"私の在学中は理科の授業でビオトープを使っていました。今から考えると良い経験だったように思います。"}];
INSERT INTO sentence [{"id":{"d":709,"n":1,"t":2},"speaker":"A","text":"帯分数がどうしても苦手です。なぜ帯分数を習うのでしょうか？"},{"id":{"d":709,"n":2,"t":2},"speaker":"B","text":"数の大きさをイメージしてもらいたいからではありませんか。帯分数なら2つの分数のどちらが大きいかが直感的に分かります。"},{"id":{"d":709,"n":3,"t":2},"speaker":"A","text":"仮分数と通分では駄目なのでしょうか？私にとっては仮分数の方が圧倒的に簡単です。"},{"id":{"d":709,"n":4,"t":2},"speaker":"B","text":"帯分数を学習することも大事だと思います。仮分数の計算を繰り返していけば、必然的に帯分数の考え方も身に付くと思いますよ。"},{"id":{"d":709,"n":5,"t":2},"speaker":"A","text":"そもそも帯分数が分かると何の役に立つのでしょうか？"},{"id":{"d":709,"n":6,"t":2},"speaker":"B","text":"例えば、料理を作るときに役立ちます。料理のレシピでは帯分数表記を見かけることがありますよ。"},{"id":{"d":709,"n":7,"t":2},"speaker":"A","text":"なるほど。確かに料理の分量などでは帯分数だとイメージしやすいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":710,"n":1,"t":2},"speaker":"A","text":"プロスポーツではビデオ判定が導入されてきています。学生スポーツでもビデオ判定の導入に向かうのでしょうか？"},{"id":{"d":710,"n":2,"t":2},"speaker":"B","text":"どうでしょうか？ビデオ判定を導入するためには設備を導入する必要があり、これにはたくさんの費用がかかります。"},{"id":{"d":710,"n":3,"t":2},"speaker":"A","text":"費用の面で難しいのかもしれませんが、個人的には学生スポーツにもビデオ判定を用いてほしいです。"},{"id":{"d":710,"n":4,"t":2},"speaker":"B","text":"それはなぜですか？"},{"id":{"d":710,"n":5,"t":2},"speaker":"A","text":"学生スポーツでは負ければ終わりとなる試合が多いです。誤審で彼らの夢が終わることはあってはいけないと思います。"},{"id":{"d":710,"n":6,"t":2},"speaker":"B","text":"そうですね。将来的には全ての大会にビデオ判定が導入されると安心して競技に集中できそうです。"}];
INSERT INTO sentence [{"id":{"d":711,"n":1,"t":2},"speaker":"A","text":"マンドリンという楽器を知っていますか？"},{"id":{"d":711,"n":2,"t":2},"speaker":"B","text":"もちろんです。実は私、少しだけ弾けるのですよ。"},{"id":{"d":711,"n":3,"t":2},"speaker":"A","text":"そうなのですか？知りませんでした。"},{"id":{"d":711,"n":4,"t":2},"speaker":"B","text":"大学の時、マンドリン部に所属していました。そこで練習して弾けるようになりました。"},{"id":{"d":711,"n":5,"t":2},"speaker":"A","text":"すごいです。マンドリン部ではどのような活動をしていたのですか？"},{"id":{"d":711,"n":6,"t":2},"speaker":"B","text":"マンドリンで合奏をして、みんなで歌ったり踊ったりしていました。"},{"id":{"d":711,"n":7,"t":2},"speaker":"A","text":"とても楽しそうですね。"},{"id":{"d":711,"n":8,"t":2},"speaker":"B","text":"はい。楽しい思い出ばかりです。"}];
INSERT INTO sentence [{"id":{"d":712,"n":1,"t":2},"speaker":"A","text":"あなたはいつも筆入れがいっぱいになっていますね。"},{"id":{"d":712,"n":2,"t":2},"speaker":"B","text":"そうなのです。いつのまにか物がいっぱいになってしまいます。"},{"id":{"d":712,"n":3,"t":2},"speaker":"A","text":"筆入れの中に壊れたシャープペンシルやインクの出ないボールペンはありませんか？"},{"id":{"d":712,"n":4,"t":2},"speaker":"B","text":"見てみます。何と、色の出ないマーカーと針のないコンパスが入っていました。"},{"id":{"d":712,"n":5,"t":2},"speaker":"A","text":"コンパスは特にかさばりそうですね。使えないなら処分してしまいましょう。"},{"id":{"d":712,"n":6,"t":2},"speaker":"B","text":"そうします。後で処分しようと思っていて、そのままになっている物がたくさん入っているようです。"},{"id":{"d":712,"n":7,"t":2},"speaker":"A","text":"ついついそのままにしてしまいますよね。せっかくなので、この機会に全部整理したらどうでしょうか？"},{"id":{"d":712,"n":8,"t":2},"speaker":"B","text":"はい。それが良さそうです。見てください、もう書けないペンが7本も出てきました。"}];
INSERT INTO sentence [{"id":{"d":713,"n":1,"t":2},"speaker":"A","text":"小学校の理科の実験で面白かったものはありますか？"},{"id":{"d":713,"n":2,"t":2},"speaker":"B","text":"はい。小学3年の時に行ったスライム作りは面白かったです。"},{"id":{"d":713,"n":3,"t":2},"speaker":"A","text":"私の世代の時にはスライム作りはありませんでした。スライムの材料は何ですか？"},{"id":{"d":713,"n":4,"t":2},"speaker":"B","text":"洗濯のりとホウ砂と水です。作り方は簡単なので小学生の実験には向いていたかも知れません。"},{"id":{"d":713,"n":5,"t":2},"speaker":"A","text":"どのようにしてスライムを作るのですか？"},{"id":{"d":713,"n":6,"t":2},"speaker":"B","text":"洗濯のりと水を混ぜた後、ホウ砂を少しずつ入れてかき混ぜるだけです。"},{"id":{"d":713,"n":7,"t":2},"speaker":"A","text":"どれくらいかき混ぜるのですか？"},{"id":{"d":713,"n":8,"t":2},"speaker":"B","text":"15分くらいかき混ぜる方が、スライムに粘りが出てきていいでしょう。色や匂いを付けることも可能です。"},{"id":{"d":713,"n":9,"t":2},"speaker":"A","text":"面白そうですね。どの材料も簡単に手に入るものなので、私もいつか作ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":714,"n":1,"t":2},"speaker":"A","text":"昔は、牛乳パックから紙を作る実験をしていました。"},{"id":{"d":714,"n":2,"t":2},"speaker":"B","text":"私もやりました。結構難しかったですよね。"},{"id":{"d":714,"n":3,"t":2},"speaker":"A","text":"はい。私はあまりきれいな紙にならなかったような気がします。"},{"id":{"d":714,"n":4,"t":2},"speaker":"B","text":"作業工程が多く、また慣れない動きが多いのでなかなかきれいな紙にはできませんでした。"},{"id":{"d":714,"n":5,"t":2},"speaker":"A","text":"ふるいでの紙すきが特に難しかったです。不細工な紙も今思うと味わい深いものでした。"},{"id":{"d":714,"n":6,"t":2},"speaker":"B","text":"そうですね。私は完成した紙をハガキにして年賀状を書いたことを覚えています。"},{"id":{"d":714,"n":7,"t":2},"speaker":"A","text":"私もハガキにしました。自分で作った紙は失敗したので、先生が作った紙と交換したように思います。先生の紙はきれいでしたから、先生はきっと何度も練習していたのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":715,"n":1,"t":2},"speaker":"A","text":"ペットボトルを使ったロケットの工作は、特に人気のある理科実験です。"},{"id":{"d":715,"n":2,"t":2},"speaker":"B","text":"私も好きでした。食品トレイで作った羽や胴体部分に絵を描くことが楽しかったです。"},{"id":{"d":715,"n":3,"t":2},"speaker":"A","text":"生徒の中には絵を描く工程を抜かし、すぐにロケットを飛ばそうとする人もいました。"},{"id":{"d":715,"n":4,"t":2},"speaker":"B","text":"いましたね。しかし、発砲入浴剤を使ってロケットを飛ばすのは画期的なアイデアだと思います。"},{"id":{"d":715,"n":5,"t":2},"speaker":"A","text":"そう思います。手軽ですから何度も飛ばして試行錯誤ができますしね。"},{"id":{"d":715,"n":6,"t":2},"speaker":"B","text":"はい。何度も改良して、誰が遠くまで飛ばせるか競っていました。"},{"id":{"d":715,"n":7,"t":2},"speaker":"A","text":"ロケットから出る水で、水浸しになっている生徒もいましたね。"}];
INSERT INTO sentence [{"id":{"d":716,"n":1,"t":2},"speaker":"A","text":"私は科学実験を利用して、母を驚かせたことがあります。"},{"id":{"d":716,"n":2,"t":2},"speaker":"B","text":"どのような実験をしたのか気になります。"},{"id":{"d":716,"n":3,"t":2},"speaker":"A","text":"白身と黄身に分かれていないゆで卵を作ったのです。"},{"id":{"d":716,"n":4,"t":2},"speaker":"B","text":"どのようにすると、そのような卵ができるのですか？"},{"id":{"d":716,"n":5,"t":2},"speaker":"A","text":"生卵をストッキングの中央に入れ、その両端を輪ゴムで止め、ストッキングの両端を持って回転させます。"},{"id":{"d":716,"n":6,"t":2},"speaker":"B","text":"卵が割れてしまったりしませんか？"},{"id":{"d":716,"n":7,"t":2},"speaker":"A","text":"卵にはセロハンテープを張るので大丈夫です。ストッキングをねじってから両端を持ち、伸ばすように引っ張ると回転します。"},{"id":{"d":716,"n":8,"t":2},"speaker":"B","text":"そうすると卵の白身と黄身がかき混ぜられるわけですね。"},{"id":{"d":716,"n":9,"t":2},"speaker":"A","text":"その通りです。母がお弁当用のゆで卵を作るとき、その卵を渡したのです。"},{"id":{"d":716,"n":10,"t":2},"speaker":"B","text":"卵の殻をむいたお母さんは驚いたでしょうね。"},{"id":{"d":716,"n":11,"t":2},"speaker":"A","text":"一瞬母は驚きましたが、すぐに私の顔を見て笑ってくれました。"}];
INSERT INTO sentence [{"id":{"d":717,"n":1,"t":2},"speaker":"A","text":"今日はじめて鶴亀算というものを知りました。あなたは知っていますか？"},{"id":{"d":717,"n":2,"t":2},"speaker":"B","text":"算数の文章題の解き方ですね。鶴と亀の合計頭数と足の合計数から鶴と亀それぞれの頭数を算出する方法です。"},{"id":{"d":717,"n":3,"t":2},"speaker":"A","text":"鶴亀算は算数で使うのですか？私は習った記憶がありません。"},{"id":{"d":717,"n":4,"t":2},"speaker":"B","text":"中学では連立方程式で解くので、小学校の時に使った鶴亀算は忘れてしまうのだと思います。"},{"id":{"d":717,"n":5,"t":2},"speaker":"A","text":"なぜ、あなたは鶴亀算についてそれほど詳しいのですか？"},{"id":{"d":717,"n":6,"t":2},"speaker":"B","text":"学習塾で小学生を教えていたとき、先輩の塾講師から文字式を使わない鶴亀算を教えてもらいました。"},{"id":{"d":717,"n":7,"t":2},"speaker":"A","text":"そうだったのですね。"}];
INSERT INTO sentence [{"id":{"d":718,"n":1,"t":2},"speaker":"A","text":"ピタゴラスの定理を今の中学校の数学の授業では三平方の定理として教えています。ピタゴラスの定理としないのは何か理由があるのでしょうか？"},{"id":{"d":718,"n":2,"t":2},"speaker":"B","text":"いくつか理由は考えられますが、一番の理由は本当にピタゴラスが発見したか分からないからだと思います。"},{"id":{"d":718,"n":3,"t":2},"speaker":"A","text":"他の人が発見した可能性があるということでしょうか？"},{"id":{"d":718,"n":4,"t":2},"speaker":"B","text":"中国古代の数学書にもピタゴラスの定理と同じ内容のことが書かれています。"},{"id":{"d":718,"n":5,"t":2},"speaker":"A","text":"ピタゴラスの定理と呼ぶには根拠が少ないということは分かりました。では、なぜ三平方の定理と呼ぶのでしょう。"},{"id":{"d":718,"n":6,"t":2},"speaker":"B","text":"直角三角形の二辺の長さが分かれば、もう一辺の長さが分かるのでそれを平方で表したからです。"},{"id":{"d":718,"n":7,"t":2},"speaker":"A","text":"内容そのものを定理名にしたことは分かりました。「三平方の定理」と言い始めた人は誰なのでしょうか？"},{"id":{"d":718,"n":8,"t":2},"speaker":"B","text":"それは分かりません。"}];
INSERT INTO sentence [{"id":{"d":719,"n":1,"t":2},"speaker":"A","text":"平方と立方を単位では二乗と三乗で表します。それならば平方や立方も二乗と三乗で統一してもよいと思います。"},{"id":{"d":719,"n":2,"t":2},"speaker":"B","text":"平方や立方も二乗三乗と呼べばよいということですか？"},{"id":{"d":719,"n":3,"t":2},"speaker":"A","text":"そうです。私は二乗や三乗と書かれたものを、平方や立方と頭で変換するのに少し時間がかかります。"},{"id":{"d":719,"n":4,"t":2},"speaker":"B","text":"最初のうちはそうかもしれませんね。でも慣れてしまえば何てことありませんよ。"}];
INSERT INTO sentence [{"id":{"d":720,"n":1,"t":2},"speaker":"A","text":"大気圧の単位をヘクトパスカルといいますが、なぜヘクトが付いているのですか？"},{"id":{"d":720,"n":2,"t":2},"speaker":"B","text":"ヘクトは100倍のことです。パスカルは圧力の単位で、大気圧のような大きな圧力にはヘクトパスカルを使います。"},{"id":{"d":720,"n":3,"t":2},"speaker":"A","text":"ヘクトパスカルは大気圧だけの単位ではありませんね？"},{"id":{"d":720,"n":4,"t":2},"speaker":"B","text":"そうです。パスカルは圧力の単位です。"},{"id":{"d":720,"n":5,"t":2},"speaker":"A","text":"私はパスカルも違和感があるのです。私が学んでいたころの圧力単位はニュートン毎平方メートルでした。"},{"id":{"d":720,"n":6,"t":2},"speaker":"B","text":"なるほど、だからヘクトパスカルを大気圧の単位だと思っていたのですね？"},{"id":{"d":720,"n":7,"t":2},"speaker":"A","text":"そうかもしれません。ちなみに私の世代は大気圧の単位にミリバールを使っていました。"},{"id":{"d":720,"n":8,"t":2},"speaker":"B","text":"ミリバールは聞いたことがあります。ミリバールがヘクトパスカルになると数字は変わるのですか？"},{"id":{"d":720,"n":9,"t":2},"speaker":"A","text":"1ミリバールは1ヘクトパスカルなので、ただ単位が変わるだけだったと思います。"},{"id":{"d":720,"n":10,"t":2},"speaker":"B","text":"それはちょうど良かったですね。"}];
INSERT INTO sentence [{"id":{"d":721,"n":1,"t":2},"speaker":"A","text":"最近気付いたのですが、私には中学生の頃、数学の授業で学んでいない単元がありました。"},{"id":{"d":721,"n":2,"t":2},"speaker":"B","text":"学習指導要領の変更はありましたが、学んでいない単元とは何ですか？"},{"id":{"d":721,"n":3,"t":2},"speaker":"A","text":"統計です。いろいろな表や用語があるのですが、私の記憶には全く残っていません。"},{"id":{"d":721,"n":4,"t":2},"speaker":"B","text":"統計の単元を学ぶ期間は短いのですが、私は学習した記憶がありますよ。"},{"id":{"d":721,"n":5,"t":2},"speaker":"A","text":"そうですか？あなたと同年代の私が統計を覚えていないのはなぜなのでしょうか？"},{"id":{"d":721,"n":6,"t":2},"speaker":"B","text":"学年の最後に統計の単元がありましたから、学習時間が足りなくて学習していないのかも知れませんね。"},{"id":{"d":721,"n":7,"t":2},"speaker":"A","text":"なるほど、そうかもしれません。今の中学生が学習している統計はかなり詳しくなっているように思います。"},{"id":{"d":721,"n":8,"t":2},"speaker":"B","text":"そのようですね。コンピューターの普及で統計処理が重要になってきたことと関係がありそうです。"}];
INSERT INTO sentence [{"id":{"d":722,"n":1,"t":2},"speaker":"A","text":"高校入試では服飾関係の学科を受験したいそうですが、保護者の方はそのことについて理解を示していますか？"},{"id":{"d":722,"n":2,"t":2},"speaker":"B","text":"父は私が服飾科に進むことを納得していないようですが、母は私に理解があり父と話し合っているようです。"},{"id":{"d":722,"n":3,"t":2},"speaker":"A","text":"将来どのような職業を希望しているのですか？"},{"id":{"d":722,"n":4,"t":2},"speaker":"B","text":"もちろん服飾関係ですが、特にオートクチュールを扱えるようになりたいです。"},{"id":{"d":722,"n":5,"t":2},"speaker":"A","text":"オートクチュールは随分狭き門だと思います。それがうまくいかない場合も考えていますか？"},{"id":{"d":722,"n":6,"t":2},"speaker":"B","text":"いえ、今は考えていません。縫製の技術は若いうちから磨いておくに越したことはないので、何年かかってもとにかく早く技術を学べる環境に身を置きたいと思っています。"},{"id":{"d":722,"n":7,"t":2},"speaker":"A","text":"それでは具体的な高校を考えるためにも、学校見学は必ずしてください。"},{"id":{"d":722,"n":8,"t":2},"speaker":"B","text":"そのつもりです。服飾科がある高校は少ないので、全て確認してくるつもりです。"},{"id":{"d":722,"n":9,"t":2},"speaker":"A","text":"良いことだと思います。今度の三者面談までには具体的な志望校を決めておいてください。"},{"id":{"d":722,"n":10,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":723,"n":1,"t":2},"speaker":"A","text":"情報技術科のある高校に行きたいそうですね？"},{"id":{"d":723,"n":2,"t":2},"speaker":"B","text":"はいそうです。プログラミングを学びたいと思っています。"},{"id":{"d":723,"n":3,"t":2},"speaker":"A","text":"コンピューター技術は大学や専門学校の方が進んでいるように思います。"},{"id":{"d":723,"n":4,"t":2},"speaker":"B","text":"高校のうちからプログラミングが学べることは素晴らしいです。"},{"id":{"d":723,"n":5,"t":2},"speaker":"A","text":"専門科に進むと将来の選択肢が少なくなりませんか？"},{"id":{"d":723,"n":6,"t":2},"speaker":"B","text":"私はそうは思いません。プログラムを高校で学ぶことを通して、独力で学習する習慣を身に付けられれば、大学ではどの学問を専攻してもうまくいくと思っています。"},{"id":{"d":723,"n":7,"t":2},"speaker":"A","text":"なるほど。そうかもしれませんね。将来をしっかり考えているようで安心しました。"}];
INSERT INTO sentence [{"id":{"d":724,"n":1,"t":2},"speaker":"A","text":"流通経済科を志望しているそうですが、志望理由を教えてください。"},{"id":{"d":724,"n":2,"t":2},"speaker":"B","text":"いろいろな資格を取りたいと考え、最もたくさんの資格が取れる流通経済科を志望しました。"},{"id":{"d":724,"n":3,"t":2},"speaker":"A","text":"確かに、簿記実務検定、ビジネス文書実務検定、情報処理検定など7つくらいの資格が取れるようですね。"},{"id":{"d":724,"n":4,"t":2},"speaker":"B","text":"はい。高校で勉強するなら、目に見える実績が欲しいと思っています。"},{"id":{"d":724,"n":5,"t":2},"speaker":"A","text":"大学受験を考えているなら、資格取得は回り道になりませんか？"},{"id":{"d":724,"n":6,"t":2},"speaker":"B","text":"大学を志望するとは限りませんが、いろいろな経験をしたいことは確かです。"},{"id":{"d":724,"n":7,"t":2},"speaker":"A","text":"行きたい大学はありませんか？"},{"id":{"d":724,"n":8,"t":2},"speaker":"B","text":"今は大学に行くことよりも起業を考えています。もちろん大学生になってから起業するかもしれません。"}];
INSERT INTO sentence [{"id":{"d":725,"n":1,"t":2},"speaker":"A","text":"保護者の方から、定時制高校を受験すると伺いました。"},{"id":{"d":725,"n":2,"t":2},"speaker":"B","text":"実は授業が午前中だけの定時制高校を見つけたのです。その高校ならば午後は父のところで働くことができます。"},{"id":{"d":725,"n":3,"t":2},"speaker":"A","text":"そのことも保護者の方から伺いました。あなたのお父さんは伝統工芸品を作っているそうですね。"},{"id":{"d":725,"n":4,"t":2},"speaker":"B","text":"漆器と木工による工芸品です。小学生の時から興味がありましたが、父の弟子になったのは最近です。"},{"id":{"d":725,"n":5,"t":2},"speaker":"A","text":"普通高校に通っていても跡継ぎの修業はできるのではありませんか？"},{"id":{"d":725,"n":6,"t":2},"speaker":"B","text":"いえ、夕方に帰ってくる高校生では修業する時間はありません。"},{"id":{"d":725,"n":7,"t":2},"speaker":"A","text":"中学生のうちから将来を決めることについて不安はありませんか？"},{"id":{"d":725,"n":8,"t":2},"speaker":"B","text":"ないとは言えませんが、何の目的もなく高校に進学しても将来の不安は消えないと思います。"},{"id":{"d":725,"n":9,"t":2},"speaker":"A","text":"分かりました。今のあなたには、したいことがあるということですね。"},{"id":{"d":725,"n":10,"t":2},"speaker":"B","text":"はい。修業をすること自体に不安はありませんし、家族も応援してくれています。"}];
INSERT INTO sentence [{"id":{"d":726,"n":1,"t":2},"speaker":"A","text":"見学に行く高校を絞り切れません。多くの高校が集まるイベントなどはありませんか？"},{"id":{"d":726,"n":2,"t":2},"speaker":"B","text":"私立高校がたくさん集まる展示会はありますよ。"},{"id":{"d":726,"n":3,"t":2},"speaker":"A","text":"そのイベントに、公立高校は参加していませんか？"},{"id":{"d":726,"n":4,"t":2},"speaker":"B","text":"公立高校は参加していません。"},{"id":{"d":726,"n":5,"t":2},"speaker":"A","text":"そうですか。そのイベントには入場料が必要ですか？"},{"id":{"d":726,"n":6,"t":2},"speaker":"B","text":"入場料は無料です。これほどの大きなイベントは年に何回もありませんので、ぜひともお出かけください。"},{"id":{"d":726,"n":7,"t":2},"speaker":"A","text":"イベントには友人と行ってもよいのでしょうか？"},{"id":{"d":726,"n":8,"t":2},"speaker":"B","text":"生徒同士でも大丈夫です。そのイベントに行けばそれぞれの高校の特色は分かると思います。"},{"id":{"d":726,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。友人を誘って行ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":727,"n":1,"t":2},"speaker":"A","text":"中学3年の1学期には中間試験がありませんか？"},{"id":{"d":727,"n":2,"t":2},"speaker":"B","text":"そうです。今の中学校では3年の1学期に中間テストはありません。"},{"id":{"d":727,"n":3,"t":2},"speaker":"A","text":"中間試験がないのは、生徒にとっては負担が減って良いかも知れませんね。"},{"id":{"d":727,"n":4,"t":2},"speaker":"B","text":"中間試験がないのは、新学年の初めで、修学旅行や体育祭も1学期に行うことが多いからだと思います。"},{"id":{"d":727,"n":5,"t":2},"speaker":"A","text":"体育祭はもう秋には開催されないのでしょうか？"},{"id":{"d":727,"n":6,"t":2},"speaker":"B","text":"はい。中学3年の秋は定期試験や実力試験があり、保護者面談も行います。体育祭を行う時間的余裕はないのでしょう。"},{"id":{"d":727,"n":7,"t":2},"speaker":"A","text":"なるほど、受験生の負担を考えると確かに1学期に学校行事を多くした方がいいかも知れませんね。"},{"id":{"d":727,"n":8,"t":2},"speaker":"B","text":"2学期になると、先生方も受験の指導や志望校の確認、必要書類の作成業務で大変忙しい時期になります。"}];
INSERT INTO sentence [{"id":{"d":728,"n":1,"t":2},"speaker":"A","text":"英語の教科書で使われている外国曲にはどのようなものがありますか？"},{"id":{"d":728,"n":2,"t":2},"speaker":"B","text":"この教科書にはシンディ・ローパーやベン・E・キングの曲が掲載されています。"},{"id":{"d":728,"n":3,"t":2},"speaker":"A","text":"ベン・E・キングの曲は『スタンド・バイ・ミー』ですね。もしかして同名の映画についても書かれているのではありませんか？"},{"id":{"d":728,"n":4,"t":2},"speaker":"B","text":"ご想像の通り映画の『スタンド・バイ・ミー』と一緒に紹介されていました。"},{"id":{"d":728,"n":5,"t":2},"speaker":"A","text":"ベン・E・キングの曲ならば『スタンド・バイ・ミー』ですが、シンディ・ローパーならばどの曲が掲載されているのですか？"},{"id":{"d":728,"n":6,"t":2},"speaker":"B","text":"『トゥルー・カラーズ』が英語の教科書に載っています。"},{"id":{"d":728,"n":7,"t":2},"speaker":"A","text":"『トゥルー・カラーズ』は良い選曲です。昔はビートルズやカーペンターズの曲が載っていたので、英語の教科書も変化していますね。"},{"id":{"d":728,"n":8,"t":2},"speaker":"B","text":"シンディ・ローパーやベン・E・キングの曲も私にはビートルズと同じくスタンダードな曲です。"},{"id":{"d":728,"n":9,"t":2},"speaker":"A","text":"名曲は永遠ですから年月に関わらずいろいろな曲を紹介してほしいと思います。"},{"id":{"d":728,"n":10,"t":2},"speaker":"B","text":"名曲はシンディ・ローパーの曲のように「タイム・アフター・タイム」と、歌い継がれていくものなのですね。"}];
INSERT INTO sentence [{"id":{"d":729,"n":1,"t":2},"speaker":"A","text":"大学の付属高校を受験したいと考えています。志望校を考える上で何か気を付ける点はありますか？"},{"id":{"d":729,"n":2,"t":2},"speaker":"B","text":"あなたが付属高校を志望する理由は、大学進学を考えてのことですか？"},{"id":{"d":729,"n":3,"t":2},"speaker":"A","text":"はい。大学には行きたいのですが、浪人することは避けたいので、大学付属高校受験を考えました。"},{"id":{"d":729,"n":4,"t":2},"speaker":"B","text":"大学付属高校を受験する場合は、付属大学への進学率と、どの学部に行きたいのかを考慮する必要があります。"},{"id":{"d":729,"n":5,"t":2},"speaker":"A","text":"もう少し詳しく説明していただけますか？"},{"id":{"d":729,"n":6,"t":2},"speaker":"B","text":"付属大学に自分の行きたい学部があるのか、また付属高校からの進学率がどれくらいかを確認しておく必要があります。"},{"id":{"d":729,"n":7,"t":2},"speaker":"A","text":"付属大学への進学率はそれほど違うものなのですか？"},{"id":{"d":729,"n":8,"t":2},"speaker":"B","text":"一般にはそうです。付属高校によってかなり違っています。"},{"id":{"d":729,"n":9,"t":2},"speaker":"A","text":"付属大学へ進学できることが付属高校の長所と思っていました。"},{"id":{"d":729,"n":10,"t":2},"speaker":"B","text":"そうとは限らないようです。付属の大学よりも国公立大学を目指す付属高校もあるようですよ。"}];
INSERT INTO sentence [{"id":{"d":730,"n":1,"t":2},"speaker":"A","text":"高校の吹奏楽部ではマーチングの大会があるそうですが、それはどのようなものですか？"},{"id":{"d":730,"n":2,"t":2},"speaker":"B","text":"ブラスバンドが行進をして、その完成度を競う大会です。"},{"id":{"d":730,"n":3,"t":2},"speaker":"A","text":"アメリカンフットボールの大会で見たことがありますが、同じものでしょうか？"},{"id":{"d":730,"n":4,"t":2},"speaker":"B","text":"日本では器楽演奏が中心ですが、大体同じものでしょう。"},{"id":{"d":730,"n":5,"t":2},"speaker":"A","text":"外国のマーチングは鼓笛隊やチアリーディングの要素も入っていますね？"},{"id":{"d":730,"n":6,"t":2},"speaker":"B","text":"そうですね。日本では吹奏楽部がマーチングを行っているので、器楽演奏が中心となります。"},{"id":{"d":730,"n":7,"t":2},"speaker":"A","text":"マーチングの大会もあるのですから、振り付けや配置を考える人もいるのでしょうね？"},{"id":{"d":730,"n":8,"t":2},"speaker":"B","text":"その通りです。旗やリングを使った演出をするところもありますよ。衣装も大きな見どころの一つです。"},{"id":{"d":730,"n":9,"t":2},"speaker":"A","text":"大会で行うマーチングは大会以外では見られませんか？"},{"id":{"d":730,"n":10,"t":2},"speaker":"B","text":"スポーツの応援ではマーチングをするスペースはありませんから、マーチングをご覧になるなら大会に行くことがお勧めです。"}];
INSERT INTO sentence [{"id":{"d":731,"n":1,"t":2},"speaker":"A","text":"私たちの高校は図書室が充実していると思いますが、これは高校の宣伝材料にはなりませんか？"},{"id":{"d":731,"n":2,"t":2},"speaker":"B","text":"そうかもしれませんが、海外留学の実績やスポーツ環境の充実の方が宣伝しやすいのではないでしょうか？"},{"id":{"d":731,"n":3,"t":2},"speaker":"A","text":"実際、私は図書室を見て、この高校に入って良かったと思いましたよ。蔵書数も多いですよね。"},{"id":{"d":731,"n":4,"t":2},"speaker":"B","text":"卒業生に有名な作家がいて、その遺族の方が蔵書を寄贈してくれたそうです。"},{"id":{"d":731,"n":5,"t":2},"speaker":"A","text":"そのことは入学してから知りました。他の高校の図書室もこれくらいの広さなのですか？"},{"id":{"d":731,"n":6,"t":2},"speaker":"B","text":"私は分かりませんが、図書室の司書の先生の話では私たちの高校の図書室はかなりの広さらしいです。"},{"id":{"d":731,"n":7,"t":2},"speaker":"A","text":"司書の先生が仰るのなら確かでしょう。やはり在校生の作る高校紹介の冊子にそのことを盛り込みましょう。"}];
INSERT INTO sentence [{"id":{"d":732,"n":1,"t":2},"speaker":"A","text":"高校に対する要望はありますか？"},{"id":{"d":732,"n":2,"t":2},"speaker":"B","text":"特にこれといって要望などありませんが、なぜ、そのようなことを聞くのですか？"},{"id":{"d":732,"n":3,"t":2},"speaker":"A","text":"生徒会で企画したプレゼンテーション大会が、今度催されます。私はそれに出場したいと思っているのです。"},{"id":{"d":732,"n":4,"t":2},"speaker":"B","text":"プレゼンテーション大会になぜ高校に対する要望が関係するのですか？"},{"id":{"d":732,"n":5,"t":2},"speaker":"A","text":"有志で高校に対する要望をプレゼンテーションします。良いプレゼンテーションは生徒会や高校側から賞がもらえます。"},{"id":{"d":732,"n":6,"t":2},"speaker":"B","text":"そのようなことでしたか。プレゼンテーション大会は、いつ開催されるのですか？"},{"id":{"d":732,"n":7,"t":2},"speaker":"A","text":"来月の学園祭で行われます。"},{"id":{"d":732,"n":8,"t":2},"speaker":"B","text":"面白そうですね。内容を考えることに協力します。当日も見に行きますよ。"},{"id":{"d":732,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":733,"n":1,"t":2},"speaker":"A","text":"大学が終わったらすぐ帰っているようですが、何かしているのですか？"},{"id":{"d":733,"n":2,"t":2},"speaker":"B","text":"カフェでアルバイトをしています。"},{"id":{"d":733,"n":3,"t":2},"speaker":"A","text":"毎日アルバイトをしているのですか？何か目的があるのですか？"},{"id":{"d":733,"n":4,"t":2},"speaker":"B","text":"はい、ほとんど毎日働いています。お金を貯めて、海外旅行に行きたいのです。"},{"id":{"d":733,"n":5,"t":2},"speaker":"A","text":"家庭教師や塾講師にしたらどうですか？"},{"id":{"d":733,"n":6,"t":2},"speaker":"B","text":"家計教師や塾講師の仕事時間は少ないので、結局毎日仕事がある飲食店の方が収入は良いのです。"},{"id":{"d":733,"n":7,"t":2},"speaker":"A","text":"週に数時間の家庭教師よりも毎日決まった時間で働ける飲食店の方が確実な収入になりますね。"},{"id":{"d":733,"n":8,"t":2},"speaker":"B","text":"目標金額には達しそうですか？"},{"id":{"d":733,"n":9,"t":2},"speaker":"A","text":"今のままでは足りないので、夏休みになったらもう1つアルバイトを増やすつもりです。"},{"id":{"d":733,"n":10,"t":2},"speaker":"B","text":"体には気を付けてください。"}];
INSERT INTO sentence [{"id":{"d":734,"n":1,"t":2},"speaker":"A","text":"高校の時の部活は何部でしたか？"},{"id":{"d":734,"n":2,"t":2},"speaker":"B","text":"アーチェリー部でした。"},{"id":{"d":734,"n":3,"t":2},"speaker":"A","text":"高校からアーチェリーを始めたのですか？"},{"id":{"d":734,"n":4,"t":2},"speaker":"B","text":"そうです。中学は卓球部でした。"},{"id":{"d":734,"n":5,"t":2},"speaker":"A","text":"高校でも卓球をしようとは思わなかったのですか？"},{"id":{"d":734,"n":6,"t":2},"speaker":"B","text":"はい。悩んだのですが、せっかくの機会なので新しい競技を始めてみようと思い、アーチェリー部を選択しました。"},{"id":{"d":734,"n":7,"t":2},"speaker":"A","text":"幼い頃から同じスポーツをしている人に、後から追い付くことは難しくないのでしょうか？"},{"id":{"d":734,"n":8,"t":2},"speaker":"B","text":"簡単ではありませんが、初めて行う競技ならではの楽しさというものもありますよ。"}];
INSERT INTO sentence [{"id":{"d":735,"n":1,"t":2},"speaker":"A","text":"三角関数を学ぶことは必要だと思いますか？"},{"id":{"d":735,"n":2,"t":2},"speaker":"B","text":"はい。必要だと思います。図形と位置の概念を数値化するのですから、日常にも不可欠なものです。"},{"id":{"d":735,"n":3,"t":2},"speaker":"A","text":"日常生活で三角関数は使わないという人がいます。"},{"id":{"d":735,"n":4,"t":2},"speaker":"B","text":"それは驚きです。波も三角関数の一部ですし、日常生活に密接したものの一つだと思っていました。"},{"id":{"d":735,"n":5,"t":2},"speaker":"A","text":"普段の授業から勉強がどのように日常生活と結びついているかを、さらに知らせることが必要かもしれませんね。"},{"id":{"d":735,"n":6,"t":2},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":736,"n":1,"t":2},"speaker":"A","text":"数学の得点が上がりませんが、どうすれば上がるようになりますか？"},{"id":{"d":736,"n":2,"t":2},"speaker":"B","text":"やみくもに勉強しても得点は上がりません。計算問題に関しては公式などの使い方を覚える前に、まず自分の力で解けるかどうか確かめていますか？"},{"id":{"d":736,"n":3,"t":2},"speaker":"A","text":"いいえ、それをしたことはありません。まずは公式を覚えなければいけないと思いこんでいました。"},{"id":{"d":736,"n":4,"t":2},"speaker":"B","text":"自力で問題を解けるなら公式を覚える必要はありません。例え、自力で問題を解けなくても、一度解いてみることは公式を理解する助けになります。"},{"id":{"d":736,"n":5,"t":2},"speaker":"A","text":"でも、他の教科の勉強もしなくてはいけないので、公式を覚える方が早道ではありませんか？"},{"id":{"d":736,"n":6,"t":2},"speaker":"B","text":"大切なのは公式を覚えことそのものではなく、いつでも解くことができることだと思います。"},{"id":{"d":736,"n":7,"t":2},"speaker":"A","text":"確かにテスト中に、急に覚えた公式に自信がなくなったりすることはあります。"},{"id":{"d":736,"n":8,"t":2},"speaker":"B","text":"忘れると何もできないのでは、勉強法としては問題だと思います。"},{"id":{"d":736,"n":9,"t":2},"speaker":"A","text":"暗記に頼った勉強ではなく、自分から問題に挑戦することが大切なのですね。"},{"id":{"d":736,"n":10,"t":2},"speaker":"B","text":"そう思います。自分の力で問題を解くことができれば、きっと勉強も楽しくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":737,"n":1,"t":2},"speaker":"A","text":"私は、なぜ学校で宇宙の起源や類人猿の生活を勉強しているのかが分かりません。"},{"id":{"d":737,"n":2,"t":2},"speaker":"B","text":"物事の始まりは重要だと思いますが、どのような点が分からないのでしょうか？"},{"id":{"d":737,"n":3,"t":2},"speaker":"A","text":"不確かな情報と想像力を必要とする分野について勉強することにむなしさを感じるのです。確かではないことを勉強するのは、何の役にも立たないので無駄ではないでしょうか？"},{"id":{"d":737,"n":4,"t":2},"speaker":"B","text":"なるほど、そのような意見もあるのですね。私はそのように現時点では不確かなものを学ぶことにも面白さを感じるので、そうした考えは思いも付きませんでした。"},{"id":{"d":737,"n":5,"t":2},"speaker":"A","text":"反対に、面白いかという観点は私にはありませんでした。役には立ちませんが、確かに話としての面白さは感じられるかもしれません。"}];
INSERT INTO sentence [{"id":{"d":738,"n":1,"t":2},"speaker":"A","text":"国語の読解問題が苦手です。人それぞれの解釈が違うようなことをなぜ問題に出すのでしょうか？"},{"id":{"d":738,"n":2,"t":2},"speaker":"B","text":"日本語の記述を正しく読み取る力を身につけるためです。本文に書かれていることと、問題作成者の意図を正しく読み取れば難しくない問題ばかりですよ。"},{"id":{"d":738,"n":3,"t":2},"speaker":"A","text":"私には本文に書かれていることと問題作成者の意図が理解できていないのでしょうか？"},{"id":{"d":738,"n":4,"t":2},"speaker":"B","text":"問題が解けないのであれば、その可能性はあります。出題者の意図は本文ではなく設問部分に書かれていますから、そこを理解しているかを確認することが重要です。設問もきちんと読んでいますか？"},{"id":{"d":738,"n":5,"t":2},"speaker":"A","text":"いいえ。私は小説本文を理解しようとばかりしていました。読解問題は設問をまずきちんと読むことが大切なのですね。"}];
INSERT INTO sentence [{"id":{"d":739,"n":1,"t":2},"speaker":"A","text":"道徳の授業をどのように活用していけば良いと思いますか？"},{"id":{"d":739,"n":2,"t":2},"speaker":"B","text":"前から考えていたのですが、道徳の授業を他の教科と結びつけられれば良いと思います。"},{"id":{"d":739,"n":3,"t":2},"speaker":"A","text":"もう少し詳しく内容を述べてもらえますか？"},{"id":{"d":739,"n":4,"t":2},"speaker":"B","text":"英語や国語で使われた音楽や小説に関連した作品を道徳の時間に鑑賞できるようにするのです。"},{"id":{"d":739,"n":5,"t":2},"speaker":"A","text":"確かに言葉だけで授業を進めるより実感は出てきますが、それは道徳の授業なのでしょうか？"},{"id":{"d":739,"n":6,"t":2},"speaker":"B","text":"問題意識やいろいろな考えを知ることが道徳の始まりです。問題意識や多様性を実感し、議論をすること自体が大切なのだと思います。"}];
INSERT INTO sentence [{"id":{"d":740,"n":1,"t":2},"speaker":"A","text":"教師の異動や休職、退職を早く知る方法はありますか？"},{"id":{"d":740,"n":2,"t":2},"speaker":"B","text":"公立学校ならば地方新聞に学校職員の異動情報が掲載されます。"},{"id":{"d":740,"n":3,"t":2},"speaker":"A","text":"学校職員の異動情報はいつ頃に掲載されるのでしょうか？"},{"id":{"d":740,"n":4,"t":2},"speaker":"B","text":"4月1日前後の新聞掲載だったと思いますので、始業式よりは早く学校職員の異動情報を知ることができるでしょう。"},{"id":{"d":740,"n":5,"t":2},"speaker":"A","text":"個人情報を新聞が掲載するのですか？"},{"id":{"d":740,"n":6,"t":2},"speaker":"B","text":"公立学校の教師は公務員ですからね。市役所職員や警察官の異動情報も新聞に掲載されています。"},{"id":{"d":740,"n":7,"t":2},"speaker":"A","text":"始業式に行くと先生の異動について話していた生徒がいましたが、その情報源は恐らく地方新聞ですね。"},{"id":{"d":740,"n":8,"t":2},"speaker":"B","text":"私の父は公務員だったので地方新聞を主要新聞とともに購読していました。地方新聞を購読する公務員は多いと思います。"}];
INSERT INTO sentence [{"id":{"d":741,"n":1,"t":2},"speaker":"A","text":"運動会で定番の競技といえば、何でしょうか？"},{"id":{"d":741,"n":2,"t":2},"speaker":"B","text":"二人三脚、借り物競争、玉入れ、綱引き、騎馬戦、棒倒しくらいでしょうか？"},{"id":{"d":741,"n":3,"t":2},"speaker":"A","text":"棒倒しや騎馬戦は危険との理由から行わない学校もあるようです。"},{"id":{"d":741,"n":4,"t":2},"speaker":"B","text":"リレーやフォークダンスも運動会競技の定番だと思います。"},{"id":{"d":741,"n":5,"t":2},"speaker":"A","text":"私はダンスが苦手だったので、フォークダンスは苦手でした。あなたも何か苦手だった競技はありますか？"},{"id":{"d":741,"n":6,"t":2},"speaker":"B","text":"あります。借り物競争は借りたいものを大声で叫ばなければならなかったので恥ずかしくて苦手でした。"},{"id":{"d":741,"n":7,"t":2},"speaker":"A","text":"反対に、運動会で好きな競技はありましたか？"},{"id":{"d":741,"n":8,"t":2},"speaker":"B","text":"特に好きな競技はありませんが、綱引きは勝ち負けの責任を感じることもなく気軽にできました。"},{"id":{"d":741,"n":9,"t":2},"speaker":"A","text":"「勝ち負けの責任」といえば、私はリレーでバトンを落としてしまったことがあります。"},{"id":{"d":741,"n":10,"t":2},"speaker":"B","text":"それは責任を感じてしまいますね。考えただけでぞっとします。"},{"id":{"d":741,"n":11,"t":2},"speaker":"A","text":"レースには勝ったのですが、バトンを落とした瞬間のショックは今でも忘れられません。"}];
INSERT INTO sentence [{"id":{"d":742,"n":1,"t":2},"speaker":"A","text":"最近の小学校では、給食の後に児童が歯磨きをするところもあるようですね。"},{"id":{"d":742,"n":2,"t":2},"speaker":"B","text":"私の子どもの頃は給食の後に歯磨きはしていませんでしたが、今から考えれば食後に歯磨きはするべきだったと思います。中学校や高校の生徒も全員が歯磨きをしているのでしょうか？"},{"id":{"d":742,"n":3,"t":2},"speaker":"A","text":"中学や高校では生徒の歯磨きは本人の意思によるものなので、する人はするし、しない人はしないと思いますよ。"},{"id":{"d":742,"n":4,"t":2},"speaker":"B","text":"そうなのですね。健康のためには、食後の歯磨きはした方がいいそうなので習慣になると良いですね。"}];
INSERT INTO sentence [{"id":{"d":743,"n":1,"t":2},"speaker":"A","text":"この学校に百葉箱は置いていませんね。"},{"id":{"d":743,"n":2,"t":2},"speaker":"B","text":"そのようですね。温度計や雨量計が入った白い蛇腹板の箱を見たことはありません。"},{"id":{"d":743,"n":3,"t":2},"speaker":"A","text":"百葉箱がないということは気象観測の勉強はしないのでしょうか？"},{"id":{"d":743,"n":4,"t":2},"speaker":"B","text":"いいえ。そのようなことはありません。今は別の気象観測用の装置が置かれていると思います。"},{"id":{"d":743,"n":5,"t":2},"speaker":"A","text":"私は百葉箱世代なので、百葉箱の方が気象を分かりやすく観測できると考えてしまいます。"},{"id":{"d":743,"n":6,"t":2},"speaker":"B","text":"温度計や雨量計は見て分かりやすいので、勉強のためにまだ百葉箱を置いている学校もあると聞きますよ。"},{"id":{"d":743,"n":7,"t":2},"speaker":"A","text":"正確な気象観測にはコンピューターを使う必要があるのは分かりますが、勉強には可視化できる教材も有効だと思います。"},{"id":{"d":743,"n":8,"t":2},"speaker":"B","text":"そうかもしれませんね。温度計と水に浸した温度計で作られた乾湿計で湿度が分かると知って驚きました。"},{"id":{"d":743,"n":9,"t":2},"speaker":"A","text":"目で見て理解できる観測方法ならば興味を持つ子どもも増えると思います。"}];
INSERT INTO sentence [{"id":{"d":744,"n":1,"t":2},"speaker":"A","text":"小学校から英語を学ぶようになりましたが、算数ではまだ文字式を取り入れないのでしょうか？"},{"id":{"d":744,"n":2,"t":2},"speaker":"B","text":"そのようです。四角や丸などの図形を使って教えているようですよ。"},{"id":{"d":744,"n":3,"t":2},"speaker":"A","text":"分からない数字を四角にするよりXやYにした方が中学の勉強に進みやすいと思うのですが。"},{"id":{"d":744,"n":4,"t":2},"speaker":"B","text":"もしかすると、まだアルファベットの筆記体を教えられていないことと関係があるのではないでしょうか？"},{"id":{"d":744,"n":5,"t":2},"speaker":"A","text":"なるほど。Xは掛けると似ているからXの筆記体を教えるまでは四角にするということですね。"},{"id":{"d":744,"n":6,"t":2},"speaker":"B","text":"そうです。"},{"id":{"d":744,"n":7,"t":2},"speaker":"A","text":"真偽は分かりませんが、もしそうなら少しだけ納得できる気がします。優しい配慮ですね。"}];
INSERT INTO sentence [{"id":{"d":745,"n":1,"t":2},"speaker":"A","text":"この勉強は役に立つのかという議論がよくありますが、私はどの勉強も意味はあると思っています。"},{"id":{"d":745,"n":2,"t":2},"speaker":"B","text":"私もおおむね同意見ですが、一つだけ納得できない勉強の分野があります。"},{"id":{"d":745,"n":3,"t":2},"speaker":"A","text":"その勉強の分野とは何でしょうか？"},{"id":{"d":745,"n":4,"t":2},"speaker":"B","text":"中学校で学ぶ、国文法です。日本語の話し方や書き方を教えるのなら分かるのですが、品詞や文節を教えるのは理解できません。外国人に日本語を教える日本語教師に聞くと、中学校で学ぶ国文法には独自な教え方があるそうですよ。"},{"id":{"d":745,"n":5,"t":2},"speaker":"A","text":"国文法の教え方のどこが変わっているのでしょうか？"},{"id":{"d":745,"n":6,"t":2},"speaker":"B","text":"日本語教師からすると、言葉が接続するときの意味や変化よりも活用表にこだわるところが変わっているそうです。"},{"id":{"d":745,"n":7,"t":2},"speaker":"A","text":"確かに、私も授業で動詞や形容詞の活用表をただ丸暗記したことを思い出しました。"},{"id":{"d":745,"n":8,"t":2},"speaker":"B","text":"あれの必要性は私にもよく分かりません。そもそも国文法は話すことを目的としていないので、その時だけの学習になってしまうのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":746,"n":1,"t":2},"speaker":"A","text":"通っていた小学校や中学校の花壇にはどのような花が植えてありましたか？"},{"id":{"d":746,"n":2,"t":2},"speaker":"B","text":"記憶があやふやなところもありますが、ヒマワリ、グラジオラス、ヒヤシンス、スイセンは見たような気がします。"},{"id":{"d":746,"n":3,"t":2},"speaker":"A","text":"ヒヤシンスは水栽培の時の記憶ではありませんか？"},{"id":{"d":746,"n":4,"t":2},"speaker":"B","text":"そうでしたか。水栽培で育てたのはクロッカスだったような気がします。"},{"id":{"d":746,"n":5,"t":2},"speaker":"A","text":"もう記憶が曖昧です。私の通っていた学校では、花以外にも、トマトやヘチマなどを花壇で育てていました。"},{"id":{"d":746,"n":6,"t":2},"speaker":"B","text":"それは楽しそうですね。なった実はどうしたのですか？"},{"id":{"d":746,"n":7,"t":2},"speaker":"A","text":"調理実習で料理をしてみんなで食べました。"}];
INSERT INTO sentence [{"id":{"d":747,"n":1,"t":2},"speaker":"A","text":"私たちの世代はプレハブの校舎が普通でしたが、校庭にプレハブ校舎を増築するなど、今の子どもたちには想像できないかも知れませんね。"},{"id":{"d":747,"n":2,"t":2},"speaker":"B","text":"そうですね。私の学校も本校舎とプレハブ校舎はすのこを使った形ばかりの渡り廊下で繋がっていました。"},{"id":{"d":747,"n":3,"t":2},"speaker":"A","text":"悪天候の時などすのこが汚れてしまうので、結局教室も汚れてしまっていたのを思い出します。"},{"id":{"d":747,"n":4,"t":2},"speaker":"B","text":"私は夏のプレハブ校舎の教室が暑くてたまらなかったのを覚えています。"},{"id":{"d":747,"n":5,"t":2},"speaker":"A","text":"休み時間ごとに水飲み場で頭から水を浴びていました。"},{"id":{"d":747,"n":6,"t":2},"speaker":"B","text":"水浴びをしましたね。同級生たちと水を掛け合って、先生に注意されたこともあります。"},{"id":{"d":747,"n":7,"t":2},"speaker":"A","text":"今となってはそれも良い思い出です。"}];
INSERT INTO sentence [{"id":{"d":748,"n":1,"t":2},"speaker":"A","text":"今は紙の辞書などを使っている学生はいるのでしょうか？"},{"id":{"d":748,"n":2,"t":2},"speaker":"B","text":"私のクラスでは紙の辞書を使っている学生はほとんど見ませんね。電子辞書を使う学生が多い印象です。"},{"id":{"d":748,"n":3,"t":2},"speaker":"A","text":"学校が推奨する辞書も電子辞書なのでしょうか？"},{"id":{"d":748,"n":4,"t":2},"speaker":"B","text":"そのようなわけではありませんが、電子辞書は持ち運びに便利なので使う人が多いのだと思いますよ。1台に、国語辞典、漢和辞典、古語辞典、英和辞典、和英辞典が入っています。"},{"id":{"d":748,"n":5,"t":2},"speaker":"A","text":"何冊も辞書を持ち歩く必要はなくなりますが、価格はどうなのでしょうか？"},{"id":{"d":748,"n":6,"t":2},"speaker":"B","text":"価格は中古品も含めると数千円から5万円くらいまでありますが、用途を考えて選べば良いと思います。"},{"id":{"d":748,"n":7,"t":2},"speaker":"A","text":"電子辞書のバージョンアップはできるのですか？"},{"id":{"d":748,"n":8,"t":2},"speaker":"B","text":"はい。新しい電子辞書はインターネットと接続可能なのでバージョンアップやコンテンツの追加もできます。"},{"id":{"d":748,"n":9,"t":2},"speaker":"A","text":"なるほど、学生にとっては電子辞書の方が紙の辞書よりも便利な面が多いのですね。"}];
INSERT INTO sentence [{"id":{"d":749,"n":1,"t":2},"speaker":"A","text":"小学校と中学校で学ぶ、家庭科の調理実習はどのように違いますか？"},{"id":{"d":749,"n":2,"t":2},"speaker":"B","text":"学習する調理技能が違うようです。小学校は基本的な調理として、ゆでることや炒めることを学びます。"},{"id":{"d":749,"n":3,"t":2},"speaker":"A","text":"具体的にはどのような料理を作りますか？"},{"id":{"d":749,"n":4,"t":2},"speaker":"B","text":"ゆで卵、ゆで芋、ほうれん草のおひたし、ジャーマンポテト、コンビネーションサラダなどです。"},{"id":{"d":749,"n":5,"t":2},"speaker":"A","text":"中学生はどのような調理実習になりますか？"},{"id":{"d":749,"n":6,"t":2},"speaker":"B","text":"小学生が行う調理に加えて、煮ることや焼くことを学びます。"},{"id":{"d":749,"n":7,"t":2},"speaker":"A","text":"煮る調理や焼く調理にはどのようなものがありますか？"},{"id":{"d":749,"n":8,"t":2},"speaker":"B","text":"煮る調理は豚汁、肉じゃが、焼く調理は豚肉のしょうが焼き、焼きギョウザ、魚の照り焼きなどです。"},{"id":{"d":749,"n":9,"t":2},"speaker":"A","text":"そうなのですね。勉強になりました。"}];
INSERT INTO sentence [{"id":{"d":750,"n":1,"t":2},"speaker":"A","text":"青木さん、一つ聞いても良いですか？"},{"id":{"d":750,"n":2,"t":2},"speaker":"B","text":"何でしょうか？先生。"},{"id":{"d":750,"n":3,"t":2},"speaker":"A","text":"下の名前の読み方を教えてくれますか？"},{"id":{"d":750,"n":4,"t":2},"speaker":"B","text":"「ぴかちゅう」です。「光に宇宙の宙」と書いて「ぴかちゅう」と読みます。"},{"id":{"d":750,"n":5,"t":2},"speaker":"A","text":"ポケットモンスターに登場するキャラクターから取ったのですね。ご両親はゲームがお好きなのですか？"},{"id":{"d":750,"n":6,"t":2},"speaker":"B","text":"はい、ピカチュウのように強くて優しい子になるようにと付けてくれました。"},{"id":{"d":750,"n":7,"t":2},"speaker":"A","text":"良いお名前ですね。"},{"id":{"d":750,"n":8,"t":2},"speaker":"B","text":"はい、自慢の名前です。"}];
INSERT INTO sentence [{"id":{"d":751,"n":1,"t":2},"speaker":"A","text":"今日の放課後は遊べますか？"},{"id":{"d":751,"n":2,"t":2},"speaker":"B","text":"ごめんなさい、今日はお稽古事があるので、遊べません。"},{"id":{"d":751,"n":3,"t":2},"speaker":"A","text":"友里ちゃんは習い事をたくさんしていますね。"},{"id":{"d":751,"n":4,"t":2},"speaker":"B","text":"自分で習いたいと思ったことがたくさんあるのです。"},{"id":{"d":751,"n":5,"t":2},"speaker":"A","text":"今日は何のお稽古なのですか？"},{"id":{"d":751,"n":6,"t":2},"speaker":"B","text":"今日はお琴のお稽古です。"},{"id":{"d":751,"n":7,"t":2},"speaker":"A","text":"格好良いです。今度ぜひ聞かせてください。"},{"id":{"d":751,"n":8,"t":2},"speaker":"B","text":"来月、発表会があるので聞きに来てください。"}];
INSERT INTO sentence [{"id":{"d":752,"n":1,"t":2},"speaker":"A","text":"こんにちは。ランドセルが重たそうですね。"},{"id":{"d":752,"n":2,"t":2},"speaker":"B","text":"こんにちは。はい、重たいです。"},{"id":{"d":752,"n":3,"t":2},"speaker":"A","text":"毎日、お店の前を通る子どもたちが大変そうだと思って見ているのですよ。"},{"id":{"d":752,"n":4,"t":2},"speaker":"B","text":"明日から夏休みなので、今日は特に荷物が多いです。"},{"id":{"d":752,"n":5,"t":2},"speaker":"A","text":"もう夏休みなのですね。では、しばらくお店は暇になってしまいますね。"},{"id":{"d":752,"n":6,"t":2},"speaker":"B","text":"夏休みになってもお母さんにお小遣いをもらったら駄菓子を買いに来ます。"},{"id":{"d":752,"n":7,"t":2},"speaker":"A","text":"お友達も誘ってきてくれるとうれしいです。"},{"id":{"d":752,"n":8,"t":2},"speaker":"B","text":"そうします。では、さようなら。"},{"id":{"d":752,"n":9,"t":2},"speaker":"A","text":"はい、さようなら。気を付けて帰ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":753,"n":1,"t":2},"speaker":"A","text":"委員長、助けてください。"},{"id":{"d":753,"n":2,"t":2},"speaker":"B","text":"何がありましたか？"},{"id":{"d":753,"n":3,"t":2},"speaker":"A","text":"社会の授業で世界地図パズルを分解したら、元に戻らなくなりました。"},{"id":{"d":753,"n":4,"t":2},"speaker":"B","text":"あれは難しいですからね。"},{"id":{"d":753,"n":5,"t":2},"speaker":"A","text":"委員長なら、元に戻せますよね？"},{"id":{"d":753,"n":6,"t":2},"speaker":"B","text":"いいえ。あいにく、パズルは苦手なのですよね。"},{"id":{"d":753,"n":7,"t":2},"speaker":"A","text":"どうしたら良いですか？"},{"id":{"d":753,"n":8,"t":2},"speaker":"B","text":"田中先生にお願いすることが一番です。"}];
INSERT INTO sentence [{"id":{"d":754,"n":1,"t":2},"speaker":"A","text":"今日は時計の読み方を学習しましょう。"},{"id":{"d":754,"n":2,"t":2},"speaker":"B","text":"先生、時計なら読めます。"},{"id":{"d":754,"n":3,"t":2},"speaker":"A","text":"では、真奈さんに読んでもらいます。これは何時ですか？"},{"id":{"d":754,"n":4,"t":2},"speaker":"B","text":"分かりません。これは時計ではありません。"},{"id":{"d":754,"n":5,"t":2},"speaker":"A","text":"真奈さん、これはアナログ時計といって、立派な時計ですよ。"},{"id":{"d":754,"n":6,"t":2},"speaker":"B","text":"わが家の時計は数字が書いてあります。"},{"id":{"d":754,"n":7,"t":2},"speaker":"A","text":"それはデジタル時計というのです。アナログ時計のお勉強もしましょうね。"},{"id":{"d":754,"n":8,"t":2},"speaker":"B","text":"時計に種類があるのは知りませんでした。読み方を教えてください。"}];
INSERT INTO sentence [{"id":{"d":755,"n":1,"t":2},"speaker":"A","text":"パパ、跳び箱はどこで練習すればよいですか？"},{"id":{"d":755,"n":2,"t":2},"speaker":"B","text":"公園にタイヤが半分埋まっている遊具はなかったですか？"},{"id":{"d":755,"n":3,"t":2},"speaker":"A","text":"はい。遠くの児童公園にはあります。"},{"id":{"d":755,"n":4,"t":2},"speaker":"B","text":"近くの公園にはありませんか？"},{"id":{"d":755,"n":5,"t":2},"speaker":"A","text":"そこには鉄棒とブランコと滑り台しかありません。"},{"id":{"d":755,"n":6,"t":2},"speaker":"B","text":"分かりました。では、パパが日曜大工でそれらしいものを作ります。それで練習しましょう。"},{"id":{"d":755,"n":7,"t":2},"speaker":"A","text":"うれしいです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":756,"n":1,"t":2},"speaker":"A","text":"最近、よく単語帳で勉強していますね？"},{"id":{"d":756,"n":2,"t":2},"speaker":"B","text":"「今年は英語の勉強を頑張る」と周囲に言っていたら、誕生日プレゼントが単語帳だったのです。"},{"id":{"d":756,"n":3,"t":2},"speaker":"A","text":"お母さまからのプレゼントですか？"},{"id":{"d":756,"n":4,"t":2},"speaker":"B","text":"はい、単語帳30冊詰め合わせセットでした。"},{"id":{"d":756,"n":5,"t":2},"speaker":"A","text":"それを使って、全部の単語帳に単語を書き込み暗記しているのですか？"},{"id":{"d":756,"n":6,"t":2},"speaker":"B","text":"さすがに全部一度には無理ですよ。"},{"id":{"d":756,"n":7,"t":2},"speaker":"A","text":"でも、いろんな色の表紙の単語帳を見かけます。"},{"id":{"d":756,"n":8,"t":2},"speaker":"B","text":"10冊は作りましたから。今年は英語で学年1番を目指します。"}];
INSERT INTO sentence [{"id":{"d":757,"n":1,"t":2},"speaker":"A","text":"英語の試験の結果はどうでしたか？"},{"id":{"d":757,"n":2,"t":2},"speaker":"B","text":"私史上最高の96点でした。"},{"id":{"d":757,"n":3,"t":2},"speaker":"A","text":"素晴しいことです。勉強したかいがありましたね。"},{"id":{"d":757,"n":4,"t":2},"speaker":"B","text":"はい。そういう佐々木さんはどうだったのですか？"},{"id":{"d":757,"n":5,"t":2},"speaker":"A","text":"私ですか？41点です。安定の平均点です。"},{"id":{"d":757,"n":6,"t":2},"speaker":"B","text":"私のこの単語帳を貸しましょうか？"},{"id":{"d":757,"n":7,"t":2},"speaker":"A","text":"いいえ。単語帳は自分で書かないと頭に入らないのではありませんか？"},{"id":{"d":757,"n":8,"t":2},"speaker":"B","text":"何もしないよりは、単語帳を見るだけでもした方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":758,"n":1,"t":2},"speaker":"A","text":"江本さんは2Bの鉛筆を使っているのですね。"},{"id":{"d":758,"n":2,"t":2},"speaker":"B","text":"このくらいが柔らかくて私にはちょうど良いのです。"},{"id":{"d":758,"n":3,"t":2},"speaker":"A","text":"私はHBの鉛筆を使っています。"},{"id":{"d":758,"n":4,"t":2},"speaker":"B","text":"HBは硬くありませんか？"},{"id":{"d":758,"n":5,"t":2},"speaker":"A","text":"硬い方が私は書きやすいのです。"},{"id":{"d":758,"n":6,"t":2},"speaker":"B","text":"2Bだと芯の減りが早いですが、字も絵もかきやすいですよ。"},{"id":{"d":758,"n":7,"t":2},"speaker":"A","text":"私は絵を描くときは2Bにしています。"},{"id":{"d":758,"n":8,"t":2},"speaker":"B","text":"使い分けをしているのですね。"}];
INSERT INTO sentence [{"id":{"d":759,"n":1,"t":2},"speaker":"A","text":"どうして鉛筆の芯の硬と濃さを表す記号はBとHなのでしょうか？"},{"id":{"d":759,"n":2,"t":2},"speaker":"B","text":"そうですよね、考えたことがありませんでした。"},{"id":{"d":759,"n":3,"t":2},"speaker":"A","text":"ABCにすれば分かりやすいと思いませんか？"},{"id":{"d":759,"n":4,"t":2},"speaker":"B","text":"硬い順にABCですか？分かりやすいと思います。"},{"id":{"d":759,"n":5,"t":2},"speaker":"A","text":"BとHはどのような意味なのでしょう？"},{"id":{"d":759,"n":6,"t":2},"speaker":"B","text":"今、調べてみましょう。BはブラックでHはハードだそうです。"},{"id":{"d":759,"n":7,"t":2},"speaker":"A","text":"なるほど。Bの数字が大きい方が黒くて柔らかく、Hの数字が大きいほど硬いのですね。"},{"id":{"d":759,"n":8,"t":2},"speaker":"B","text":"そのようです。正しい意味を知れば、分かりやすいですね。BとHの他に、Fというものもあるそうですよ。"},{"id":{"d":759,"n":9,"t":2},"speaker":"A","text":"初めて聞きました。Fはどのような意味なのですか？"},{"id":{"d":759,"n":10,"t":2},"speaker":"B","text":"Fはファームで「しっかりとした」という意味のようです。Fは、HとHBの中間の濃さと硬さを持った芯だそうです。"}];
INSERT INTO sentence [{"id":{"d":760,"n":1,"t":2},"speaker":"A","text":"先輩、すみません、今日は委員会があるので部活に遅れます。"},{"id":{"d":760,"n":2,"t":2},"speaker":"B","text":"体育委員会でしたね。来月の体育祭まで忙しいのですか？"},{"id":{"d":760,"n":3,"t":2},"speaker":"A","text":"はい、去年、体育祭が中止になってしまったので、今年は盛大に行うらしいのです。"},{"id":{"d":760,"n":4,"t":2},"speaker":"B","text":"体育祭の後、すぐに県予選もありますが、その準備は大丈夫ですか？"},{"id":{"d":760,"n":5,"t":2},"speaker":"A","text":"体育祭が終わったら、放課後の部活に加えて、朝も自主練習も行い、県予選に備えるつもりです。"},{"id":{"d":760,"n":6,"t":2},"speaker":"B","text":"次のエースとして、君に期待をしていますよ。"},{"id":{"d":760,"n":7,"t":2},"speaker":"A","text":"期待が大きすぎますよ。今はまず体育祭に向けて頑張ります。"},{"id":{"d":760,"n":8,"t":2},"speaker":"B","text":"体育祭での活躍も楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":761,"n":1,"t":2},"speaker":"A","text":"黒板消しのクリーナーはどこですか？"},{"id":{"d":761,"n":2,"t":2},"speaker":"B","text":"ありません。おととい、このクラスのクリーナーは壊れてしいました。"},{"id":{"d":761,"n":3,"t":2},"speaker":"A","text":"そうでしたか。では、どのようにして黒板消しをきれいにしたら良いですか？窓の外で黒板消し同士を叩きますか？"},{"id":{"d":761,"n":4,"t":2},"speaker":"B","text":"それは粉が飛び散ってしまうのでやめてください。隣のクラスのものを使わせてもらうと良いですよ。"},{"id":{"d":761,"n":5,"t":2},"speaker":"A","text":"隣のクラスに入るのは勇気が要ります。"},{"id":{"d":761,"n":6,"t":2},"speaker":"B","text":"一緒に行きますから大丈夫ですよ。"},{"id":{"d":761,"n":7,"t":2},"speaker":"A","text":"委員長、ありがとうございます。"},{"id":{"d":761,"n":8,"t":2},"speaker":"B","text":"壊れたクリーナーのことは、日直の連絡欄に書いておいてくださいね。"}];
INSERT INTO sentence [{"id":{"d":762,"n":1,"t":2},"speaker":"A","text":"お母さん、新しい制服を買ってください。"},{"id":{"d":762,"n":2,"t":2},"speaker":"B","text":"制服が着られなくなってしまったのですか？"},{"id":{"d":762,"n":3,"t":2},"speaker":"A","text":"はい。背が伸びてスカートの丈が短くなってしまいました。今はまだウエストのボタンを外して腰穿きにしているから大丈夫ですが、近いうちに新調したいです。"},{"id":{"d":762,"n":4,"t":2},"speaker":"B","text":"確かに、膝が出ていますね。気が付かなくてごめんなさいね。これから買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":763,"n":1,"t":2},"speaker":"A","text":"高校の制服はリボンとネクタイが選べるので楽しみです。"},{"id":{"d":763,"n":2,"t":2},"speaker":"B","text":"私も楽しみです。"},{"id":{"d":763,"n":3,"t":2},"speaker":"A","text":"実は進学先を制服のデザインだけで選んだのです。"},{"id":{"d":763,"n":4,"t":2},"speaker":"B","text":"実は私もそうです。好みに合わせてコーディネートできるのがとても魅力的です。"},{"id":{"d":763,"n":5,"t":2},"speaker":"A","text":"入学式にはリボンを着用しますか？ネクタイを着用しますか？"},{"id":{"d":763,"n":6,"t":2},"speaker":"B","text":"私は行事の時はネクタイと決めています。"},{"id":{"d":763,"n":7,"t":2},"speaker":"A","text":"なるほど。私もそうしようかしら。"},{"id":{"d":763,"n":8,"t":2},"speaker":"B","text":"おそろいの制服姿で登校しましょう。"}];
INSERT INTO sentence [{"id":{"d":764,"n":1,"t":2},"speaker":"A","text":"もう昇降口は閉めるところですよ。"},{"id":{"d":764,"n":2,"t":2},"speaker":"B","text":"すみません。宿題のプリントを、持ち帰ることを忘れてしまったので取りに来たのです。"},{"id":{"d":764,"n":3,"t":2},"speaker":"A","text":"開けて待っていてあげますから、早く取ってきてください。"},{"id":{"d":764,"n":4,"t":2},"speaker":"B","text":"はい、ありがとうございます。"},{"id":{"d":764,"n":5,"t":2},"speaker":"A","text":"プリントはありましたか？"},{"id":{"d":764,"n":6,"t":2},"speaker":"B","text":"ありました。お待たせしてすみませんでした。"},{"id":{"d":764,"n":7,"t":2},"speaker":"A","text":"もう暗くなりますから、気を付けて帰ってください。"},{"id":{"d":764,"n":8,"t":2},"speaker":"B","text":"はい、ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":765,"n":1,"t":2},"speaker":"A","text":"花壇のチューリップがきれいですね。"},{"id":{"d":765,"n":2,"t":2},"speaker":"B","text":"これはこの春の卒業生が去年の秋に植えた球根ですよ。"},{"id":{"d":765,"n":3,"t":2},"speaker":"A","text":"3年生になると、そのような行事があるのですか？"},{"id":{"d":765,"n":4,"t":2},"speaker":"B","text":"はい。卒業生から新入生への贈り物の行事で長らく続いているのです。"},{"id":{"d":765,"n":5,"t":2},"speaker":"A","text":"すてきな伝統ですね。"},{"id":{"d":765,"n":6,"t":2},"speaker":"B","text":"美術部と写真部では、このチューリップを題材にした作品を制作することが、新入生の最初の課題となっています。"},{"id":{"d":765,"n":7,"t":2},"speaker":"A","text":"私は漫画研究会に入ろうと思うのですが、漫画研究会ではそのような課題は行っていないのでしょうか？"},{"id":{"d":765,"n":8,"t":2},"speaker":"B","text":"はい。あなたが提案すると良いと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":766,"n":1,"t":2},"speaker":"A","text":"門の前にたくさんの人がいますね。"},{"id":{"d":766,"n":2,"t":2},"speaker":"B","text":"今日はサッカー部の練習試合があるのです。"},{"id":{"d":766,"n":3,"t":2},"speaker":"A","text":"皆さん、試合を見に来たのですか？"},{"id":{"d":766,"n":4,"t":2},"speaker":"B","text":"いいえ。部外者はグラウンドに入れませんから。"},{"id":{"d":766,"n":5,"t":2},"speaker":"A","text":"では、彼らは何をしているのですか？"},{"id":{"d":766,"n":6,"t":2},"speaker":"B","text":"相手の学校にスター選手がいるので、その人を見ようとしているのです。"},{"id":{"d":766,"n":7,"t":2},"speaker":"A","text":"試合ではなくて選手を見に来たのですか？"},{"id":{"d":766,"n":8,"t":2},"speaker":"B","text":"はい。私も遠くからでも話題のスター選手を見に行こうと思っています。"}];
INSERT INTO sentence [{"id":{"d":767,"n":1,"t":2},"speaker":"A","text":"松本さん、この楽譜をコピーして来てくれますか？"},{"id":{"d":767,"n":2,"t":2},"speaker":"B","text":"分かりました。これは新曲ですね。"},{"id":{"d":767,"n":3,"t":2},"speaker":"A","text":"はい。「野球部の応援曲を増やすように」と戸田先生に言われたのです。"},{"id":{"d":767,"n":4,"t":2},"speaker":"B","text":"そうでしたか。これ、編曲が格好良いです。"},{"id":{"d":767,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。コピーは50部でお願いしますね。"},{"id":{"d":767,"n":6,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":767,"n":7,"t":2},"speaker":"A","text":"早速今日から練習を始めますよ。"},{"id":{"d":767,"n":8,"t":2},"speaker":"B","text":"練習が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":768,"n":1,"t":2},"speaker":"A","text":"何か良い匂いがしませんか？"},{"id":{"d":768,"n":2,"t":2},"speaker":"B","text":"そうですね。家庭科実習室から流れてきた匂いのようです。"},{"id":{"d":768,"n":3,"t":2},"speaker":"A","text":"何か作っているのでしょうか？"},{"id":{"d":768,"n":4,"t":2},"speaker":"B","text":"そういえば、料理部の田中さんが「新入生勧誘のためにクッキーを作る」と言っていました。"},{"id":{"d":768,"n":5,"t":2},"speaker":"A","text":"田中さんに言って失敗作を頂けませんかね？"},{"id":{"d":768,"n":6,"t":2},"speaker":"B","text":"あなたはおなかがすいているのですか？"},{"id":{"d":768,"n":7,"t":2},"speaker":"A","text":"はい。この匂いで空腹を覚えないわけがないでしょう？"},{"id":{"d":768,"n":8,"t":2},"speaker":"B","text":"それもそうですね。実習室に行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":769,"n":1,"t":2},"speaker":"A","text":"今日、向かいの学校は人が多いですが、何かあるのですか？"},{"id":{"d":769,"n":2,"t":2},"speaker":"B","text":"分かりません。確かに、親子がたくさん来ていますね。"},{"id":{"d":769,"n":3,"t":2},"speaker":"A","text":"親子ということは、入学試験でしょうか？"},{"id":{"d":769,"n":4,"t":2},"speaker":"B","text":"入学試験は1日に行われたようですから、違うと思います。もしかして、今日はその試験の合格発表ではありませんか？"},{"id":{"d":769,"n":5,"t":2},"speaker":"A","text":"そうかもしれません。近頃の合格発表はインターネットでするものだと思っていました。"},{"id":{"d":769,"n":6,"t":2},"speaker":"B","text":"私もその印象があります。掲示板で発表するのは、ここの学校の伝統かもしれませんね。"},{"id":{"d":769,"n":7,"t":2},"speaker":"A","text":"インターネットでも合否を知ることができるのか、帰りがけの親子に聞いてみようと思います。"},{"id":{"d":769,"n":8,"t":2},"speaker":"B","text":"やめた方がいいですよ。もし、入学試験に落ちていたらかわいそうじゃありませんか。"}];
INSERT INTO sentence [{"id":{"d":770,"n":1,"t":2},"speaker":"A","text":"先生、図書委員が全員集まりました。"},{"id":{"d":770,"n":2,"t":2},"speaker":"B","text":"ご苦労さまです。今日は新しい本のラベルを貼るお手伝いをしてもらいます。"},{"id":{"d":770,"n":3,"t":2},"speaker":"A","text":"では、ラベルやハンコを出しておきますね。"},{"id":{"d":770,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。後藤さんにも手伝ってもらってください。"},{"id":{"d":770,"n":5,"t":2},"speaker":"A","text":"はい。後輩にも教えたいので、1年の藤原さんと熊谷さんにも来てもらってもよいですか？"},{"id":{"d":770,"n":6,"t":2},"speaker":"B","text":"もちろんです。人数が増えるのでしたら、本の入った段ボールも持って行ってもらえると助かります。"},{"id":{"d":770,"n":7,"t":2},"speaker":"A","text":"分かりました。本も運んでおきます。"},{"id":{"d":770,"n":8,"t":2},"speaker":"B","text":"助かります。"}];
INSERT INTO sentence [{"id":{"d":771,"n":1,"t":2},"speaker":"A","text":"体育祭の後夜祭の準備は進んでいますか？"},{"id":{"d":771,"n":2,"t":2},"speaker":"B","text":"はい。後夜祭はキャンプファイヤがありますから生徒だけには任せておけませんので、進行状況の確認はしています。"},{"id":{"d":771,"n":3,"t":2},"speaker":"A","text":"もう消防には連絡してくださいましたか？"},{"id":{"d":771,"n":4,"t":2},"speaker":"B","text":"先週のうちにそれは済ませています。"},{"id":{"d":771,"n":5,"t":2},"speaker":"A","text":"他の準備はいかがです？"},{"id":{"d":771,"n":6,"t":2},"speaker":"B","text":"大体は終わっているのですが、あと一つ決まっていないことがあります。"},{"id":{"d":771,"n":7,"t":2},"speaker":"A","text":"何か不足があればお手伝いしますけれど、何が残っているのですか？"},{"id":{"d":771,"n":8,"t":2},"speaker":"B","text":"私の仮装を何にしたら良いか、大いに悩んでいます。"}];
INSERT INTO sentence [{"id":{"d":772,"n":1,"t":2},"speaker":"A","text":"中学を卒業したら、みんな離れ離れになってしまいますね。"},{"id":{"d":772,"n":2,"t":2},"speaker":"B","text":"そうですね。莉緒ちゃんはサッカーをするために少し遠い高校へ行ってしまいますね。"},{"id":{"d":772,"n":3,"t":2},"speaker":"A","text":"はい。サッカーはしたいけれど、みんなと離れるのは寂しいです。"},{"id":{"d":772,"n":4,"t":2},"speaker":"B","text":"女子サッカーで頑張って、インターハイに出てくださいね。"},{"id":{"d":772,"n":5,"t":2},"speaker":"A","text":"応援に来てくれますか？"},{"id":{"d":772,"n":6,"t":2},"speaker":"B","text":"もちろんです。だから絶対にインターハイに出て、いずれは日本代表に入ってください。"},{"id":{"d":772,"n":7,"t":2},"speaker":"A","text":"はい。そうなれるように頑張ります。"},{"id":{"d":772,"n":8,"t":2},"speaker":"B","text":"ずっと応援していますから。"}];
INSERT INTO sentence [{"id":{"d":773,"n":1,"t":2},"speaker":"A","text":"受験生の夏休みは勉強ざんまいですね。"},{"id":{"d":773,"n":2,"t":2},"speaker":"B","text":"それでこそ受験生です。頑張りましょう。夏期講習の申し込みはもうしましたか？"},{"id":{"d":773,"n":3,"t":2},"speaker":"A","text":"もちろんです。一番人気の講師の先生を狙って申し込みました。"},{"id":{"d":773,"n":4,"t":2},"speaker":"B","text":"もしかして席の取り合いになっていますか？"},{"id":{"d":773,"n":5,"t":2},"speaker":"A","text":"はい。人気のあるコースはだいぶ埋まっていましたよ。"},{"id":{"d":773,"n":6,"t":2},"speaker":"B","text":"本当ですか？母に夏期講習について至急相談しなくては。"},{"id":{"d":773,"n":7,"t":2},"speaker":"A","text":"はい。理系の科目に関する講習の申し込みは特に急いだ方がいいですよ。"},{"id":{"d":773,"n":8,"t":2},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":774,"n":1,"t":2},"speaker":"A","text":"明日から待ちに待った夏休みです。"},{"id":{"d":774,"n":2,"t":2},"speaker":"B","text":"小学生はお休みできて良いですね。"},{"id":{"d":774,"n":3,"t":2},"speaker":"A","text":"お姉さんはお休みではありませんか？"},{"id":{"d":774,"n":4,"t":2},"speaker":"B","text":"はい。私は夏期講習に通うので、ずっとお休みではありません。"},{"id":{"d":774,"n":5,"t":2},"speaker":"A","text":"中学生になったら、お休みできませんか？"},{"id":{"d":774,"n":6,"t":2},"speaker":"B","text":"はい。お母さんに「このままでは高校に行かれないから」と言われました。"},{"id":{"d":774,"n":7,"t":2},"speaker":"A","text":"私は中学になっても夏休みはお休みしたいです。"},{"id":{"d":774,"n":8,"t":2},"speaker":"B","text":"普段からお勉強して成績が良ければ大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":775,"n":1,"t":2},"speaker":"A","text":"明日は学校のプールの開放日ですね？"},{"id":{"d":775,"n":2,"t":2},"speaker":"B","text":"はい。一緒に行きましょうね。"},{"id":{"d":775,"n":3,"t":2},"speaker":"A","text":"もう25メートルは泳げるようになりましたか？"},{"id":{"d":775,"n":4,"t":2},"speaker":"B","text":"いいえ。息継ぎがうまくできなくて、あと少しのところで足をついてしまいます。"},{"id":{"d":775,"n":5,"t":2},"speaker":"A","text":"私は半分くらいまでしか泳げません。"},{"id":{"d":775,"n":6,"t":2},"speaker":"B","text":"明日は先生も見てくださるのですよね？"},{"id":{"d":775,"n":7,"t":2},"speaker":"A","text":"はい。鈴本先生と森下先生がいらっしゃると思います。"},{"id":{"d":775,"n":8,"t":2},"speaker":"B","text":"良かったです。練習して夏休み中に25メートル泳げるようになりましょうね。"}];
INSERT INTO sentence [{"id":{"d":776,"n":1,"t":2},"speaker":"A","text":"バレンタインデーには、チョコレートをクラスの男の子にあげるのでしょう？"},{"id":{"d":776,"n":2,"t":2},"speaker":"B","text":"はい、そのつもりです。"},{"id":{"d":776,"n":3,"t":2},"speaker":"A","text":"下駄箱に入れたりするのですか？"},{"id":{"d":776,"n":4,"t":2},"speaker":"B","text":"いいえ。お母さん、下駄箱とは何ですか？"},{"id":{"d":776,"n":5,"t":2},"speaker":"A","text":"学校の入り口の靴を入れるところですよ。"},{"id":{"d":776,"n":6,"t":2},"speaker":"B","text":"靴箱のことですか？"},{"id":{"d":776,"n":7,"t":2},"speaker":"A","text":"はい。今は「下駄箱」と言いませんか？"},{"id":{"d":776,"n":8,"t":2},"speaker":"B","text":"はい。「靴箱」と言います。"}];
INSERT INTO sentence [{"id":{"d":777,"n":1,"t":2},"speaker":"A","text":"今の学校では「下駄箱」と言わないことを初めて知りました。"},{"id":{"d":777,"n":2,"t":2},"speaker":"B","text":"下駄というのは、お着物を着るときに履く履物でしょう？"},{"id":{"d":777,"n":3,"t":2},"speaker":"A","text":"確かにそうですね。"},{"id":{"d":777,"n":4,"t":2},"speaker":"B","text":"靴を入れるから靴箱というのですよ。"},{"id":{"d":777,"n":5,"t":2},"speaker":"A","text":"なるほど。娘に一つ教えられた気分です。"},{"id":{"d":777,"n":6,"t":2},"speaker":"B","text":"お母さんの子どものころは下駄を入れていたのですか？"},{"id":{"d":777,"n":7,"t":2},"speaker":"A","text":"いいえ。入れていませんよ！お母さんも靴を入れていました。"},{"id":{"d":777,"n":8,"t":2},"speaker":"B","text":"それなのに「下駄箱」と言うことがおかしいですね。"}];
INSERT INTO sentence [{"id":{"d":778,"n":1,"t":2},"speaker":"A","text":"今日から新学期ですね。"},{"id":{"d":778,"n":2,"t":2},"speaker":"B","text":"美術室を見てきましたか？"},{"id":{"d":778,"n":3,"t":2},"speaker":"A","text":"いいえ。何かあるのですか？"},{"id":{"d":778,"n":4,"t":2},"speaker":"B","text":"はい。卒業した先輩方の黒板アートが残っていました。"},{"id":{"d":778,"n":5,"t":2},"speaker":"A","text":"卒業前に描かれたのでしょうか？"},{"id":{"d":778,"n":6,"t":2},"speaker":"B","text":"そうだと思いますよ。素晴らしいですから消される前に見に行った方がいいです。"},{"id":{"d":778,"n":7,"t":2},"speaker":"A","text":"行きたいのはやまやまですが、もうホームルームが始まってしまいます。"},{"id":{"d":778,"n":8,"t":2},"speaker":"B","text":"休み時間まで消されないと良いですね。"}];
INSERT INTO sentence [{"id":{"d":779,"n":1,"t":2},"speaker":"A","text":"2時間目は自習だそうですよ。"},{"id":{"d":779,"n":2,"t":2},"speaker":"B","text":"英語の田辺先生はお休みなのですか？"},{"id":{"d":779,"n":3,"t":2},"speaker":"A","text":"はい。ご家庭のご事情で休みだそうです。"},{"id":{"d":779,"n":4,"t":2},"speaker":"B","text":"自習時間は何をしたら良いですか？"},{"id":{"d":779,"n":5,"t":2},"speaker":"A","text":"「先週のテストで間違えた箇所を復習するように」と言われました。"},{"id":{"d":779,"n":6,"t":2},"speaker":"B","text":"私はたくさん行うべきことがありそうです。"},{"id":{"d":779,"n":7,"t":2},"speaker":"A","text":"私は暇です。間違えた個所は1カ所だけでしたから。"},{"id":{"d":779,"n":8,"t":2},"speaker":"B","text":"それはうらやましいです。"}];
INSERT INTO sentence [{"id":{"d":780,"n":1,"t":2},"speaker":"A","text":"赤鉛筆を持っていませんか？"},{"id":{"d":780,"n":2,"t":2},"speaker":"B","text":"はい。色鉛筆のセットの中にありますよ。"},{"id":{"d":780,"n":3,"t":2},"speaker":"A","text":"赤だけ貸してください。"},{"id":{"d":780,"n":4,"t":2},"speaker":"B","text":"良いですよ。はいどうぞ。"},{"id":{"d":780,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":780,"n":6,"t":2},"speaker":"B","text":"何に使うのか聞いても良いですか？"},{"id":{"d":780,"n":7,"t":2},"speaker":"A","text":"はい。教科書のテスト範囲を囲っておくのです。"},{"id":{"d":780,"n":8,"t":2},"speaker":"B","text":"中間テストの追試験を受けるのでしたね。期末テストは頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":781,"n":1,"t":2},"speaker":"A","text":"今日の給食は楽しみです。"},{"id":{"d":781,"n":2,"t":2},"speaker":"B","text":"今日のメニューは何でしたか？"},{"id":{"d":781,"n":3,"t":2},"speaker":"A","text":"私の大好きなカレーライスにプリンが付いているのです。"},{"id":{"d":781,"n":4,"t":2},"speaker":"B","text":"それはうれしいですね。私も大好きです。"},{"id":{"d":781,"n":5,"t":2},"speaker":"A","text":"誰かお休みの人はいないでしょうか？"},{"id":{"d":781,"n":6,"t":2},"speaker":"B","text":"余ったプリンを狙っているのですね？でも、今日はみんないると思いますよ。"},{"id":{"d":781,"n":7,"t":2},"speaker":"A","text":"それは残念です。"}];
INSERT INTO sentence [{"id":{"d":782,"n":1,"t":2},"speaker":"A","text":"雨が降ってきてしまいましたね。"},{"id":{"d":782,"n":2,"t":2},"speaker":"B","text":"傘を持ってきていませんか？"},{"id":{"d":782,"n":3,"t":2},"speaker":"A","text":"テレビ番組の天気予報コーナーでは「夜から降る」と言っていたので、持ってきませんでした。"},{"id":{"d":782,"n":4,"t":2},"speaker":"B","text":"私の傘を貸しましょうか？"},{"id":{"d":782,"n":5,"t":2},"speaker":"A","text":"でも、それでは紗枝さんがぬれてしまいます。"},{"id":{"d":782,"n":6,"t":2},"speaker":"B","text":"私は折り畳み傘を持ってきましたし、置き傘もあります。"},{"id":{"d":782,"n":7,"t":2},"speaker":"A","text":"それでは置き傘を貸してください。"},{"id":{"d":782,"n":8,"t":2},"speaker":"B","text":"もちろんです。昇降口に置いてあるので一緒に取りに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":783,"n":1,"t":2},"speaker":"A","text":"ネクタイが曲がっていますよ。"},{"id":{"d":783,"n":2,"t":2},"speaker":"B","text":"すみません、先生。"},{"id":{"d":783,"n":3,"t":2},"speaker":"A","text":"今朝は遅刻の瀬戸際だったようですね。"},{"id":{"d":783,"n":4,"t":2},"speaker":"B","text":"少し寝坊をしてしまって、走ってきました。"},{"id":{"d":783,"n":5,"t":2},"speaker":"A","text":"遅刻をしなかったのは良かったですが、服装も整えましょうね。"},{"id":{"d":783,"n":6,"t":2},"speaker":"B","text":"はい。"},{"id":{"d":783,"n":7,"t":2},"speaker":"A","text":"髪も整えましょうね。"},{"id":{"d":783,"n":8,"t":2},"speaker":"B","text":"髪もとかしていませんでした。"}];
INSERT INTO sentence [{"id":{"d":784,"n":1,"t":2},"speaker":"A","text":"1組の幸田君ですが、「大学進学をやめる」と言っています。"},{"id":{"d":784,"n":2,"t":2},"speaker":"B","text":"成績が下がっているので、そのせいでしょうか？"},{"id":{"d":784,"n":3,"t":2},"speaker":"A","text":"そうかもしれません。成績が下がった原因があるようなのですが、教えてはくれません。"},{"id":{"d":784,"n":4,"t":2},"speaker":"B","text":"部活の顧問の江口先生もご存じではないのでしょうか？"},{"id":{"d":784,"n":5,"t":2},"speaker":"A","text":"はい。昨日、伺ってみたのですが、ご存じではないようでした。"},{"id":{"d":784,"n":6,"t":2},"speaker":"B","text":"来週は三者面談がありますね。"},{"id":{"d":784,"n":7,"t":2},"speaker":"A","text":"幸田君のお母さまに、この件についていきなり伺っても良いものでしょうか？"},{"id":{"d":784,"n":8,"t":2},"speaker":"B","text":"ご家族には言っていない可能性もありますね。難しい問題です。"}];
INSERT INTO sentence [{"id":{"d":785,"n":1,"t":2},"speaker":"A","text":"今日はお母さんと遠足用のリュックサックを買いに行きます。"},{"id":{"d":785,"n":2,"t":2},"speaker":"B","text":"私もお母さんと買いに行くつもりです。"},{"id":{"d":785,"n":3,"t":2},"speaker":"A","text":"一緒におそろいのリュックを買えたら良いですね。"},{"id":{"d":785,"n":4,"t":2},"speaker":"B","text":"お母さんに一緒に行けないか聞いてみます。"},{"id":{"d":785,"n":5,"t":2},"speaker":"A","text":"家に帰ったら電話をください。待っていますから。"},{"id":{"d":785,"n":6,"t":2},"speaker":"B","text":"分かりました。どこに行く予定ですか？"},{"id":{"d":785,"n":7,"t":2},"speaker":"A","text":"ららぽーとです。かわいいリュックサックを見つけてあるのです。"}];
INSERT INTO sentence [{"id":{"d":786,"n":1,"t":2},"speaker":"A","text":"遠足のリュックサックも、お弁当箱も色違いでおそろいですね。"},{"id":{"d":786,"n":2,"t":2},"speaker":"B","text":"かわいいお店で一緒に買えて良かったです。"},{"id":{"d":786,"n":3,"t":2},"speaker":"A","text":"お弁当の中身は違うものですけれどね。"},{"id":{"d":786,"n":4,"t":2},"speaker":"B","text":"でも、幼稚園の時も唐揚げと卵焼きで一緒でしたよね？"},{"id":{"d":786,"n":5,"t":2},"speaker":"A","text":"はい。お母さんの得意料理が一緒だから、今回もまた同じかもしれませんね。"},{"id":{"d":786,"n":6,"t":2},"speaker":"B","text":"おやつも一緒に買いに行きますか？"},{"id":{"d":786,"n":7,"t":2},"speaker":"A","text":"いいえ。おやつは違うところで買って分け合いませんか？"},{"id":{"d":786,"n":8,"t":2},"speaker":"B","text":"はい。そうしたらおやつの種類が倍になりますね。それも楽しみです。"}];
INSERT INTO sentence [{"id":{"d":787,"n":1,"t":2},"speaker":"A","text":"今日も学校に行くのですか？"},{"id":{"d":787,"n":2,"t":2},"speaker":"B","text":"はい。生き物係になったので、夏休みも当番があるのです。"},{"id":{"d":787,"n":3,"t":2},"speaker":"A","text":"学校で飼っているウサギのお世話をするのですね？"},{"id":{"d":787,"n":4,"t":2},"speaker":"B","text":"そうです。メダカの世話もしてきます。"},{"id":{"d":787,"n":5,"t":2},"speaker":"A","text":"お昼までには帰ってくるのですよね？"},{"id":{"d":787,"n":6,"t":2},"speaker":"B","text":"はい、11時ごろには帰ってきます。"},{"id":{"d":787,"n":7,"t":2},"speaker":"A","text":"そういえばちょうど人参がたくさんあるので、持っていきますか？"},{"id":{"d":787,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。ウサギのポポちゃんとルルちゃんが喜びます。"}];
INSERT INTO sentence [{"id":{"d":788,"n":1,"t":2},"speaker":"A","text":"ノートが文字でいっぱいになってしまいました。"},{"id":{"d":788,"n":2,"t":2},"speaker":"B","text":"どうしてそれほどたくさんの文字が書いてあるのですか？"},{"id":{"d":788,"n":3,"t":2},"speaker":"A","text":"授業で習ったことを書いていたらこれくらいになりました。"},{"id":{"d":788,"n":4,"t":2},"speaker":"B","text":"私は黒板に書いてあることをノートに書き写しましたが、まだ半分にもなりませんよ。"},{"id":{"d":788,"n":5,"t":2},"speaker":"A","text":"あなたは、黒板に書かれていたことだけをノートに書いているのですか？"},{"id":{"d":788,"n":6,"t":2},"speaker":"B","text":"はい。普通はそうではありませんか？"},{"id":{"d":788,"n":7,"t":2},"speaker":"A","text":"私は先生が仰ったことや家で予習復習した時のことも書いています。"},{"id":{"d":788,"n":8,"t":2},"speaker":"B","text":"頭の良い人はノートの書き方も違うのですね。見習います。"}];
INSERT INTO sentence [{"id":{"d":789,"n":1,"t":2},"speaker":"A","text":"小沢さん、お母さまが忘れ物を届けてくださいましたよ。"},{"id":{"d":789,"n":2,"t":2},"speaker":"B","text":"忘れ物ですか？今日は何も忘れていないと思うのですが。"},{"id":{"d":789,"n":3,"t":2},"speaker":"A","text":"この袋に入っているそうです。"},{"id":{"d":789,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":789,"n":5,"t":2},"speaker":"A","text":"何が入っていたのですか？"},{"id":{"d":789,"n":6,"t":2},"speaker":"B","text":"給食費のようです。"},{"id":{"d":789,"n":7,"t":2},"speaker":"A","text":"給食費は明日が提出日の予定ですね。今、受け取っておきましょうか？"},{"id":{"d":789,"n":8,"t":2},"speaker":"B","text":"はい。お願いします。母は早とちりのところがあるのです。"}];
INSERT INTO sentence [{"id":{"d":790,"n":1,"t":2},"speaker":"A","text":"先生から「社会科研究室から地球儀を持って来るように」と言われました。"},{"id":{"d":790,"n":2,"t":2},"speaker":"B","text":"手伝いますよ。"},{"id":{"d":790,"n":3,"t":2},"speaker":"A","text":"ありがとうございます。あの地球儀は重たいですから助かります。"},{"id":{"d":790,"n":4,"t":2},"speaker":"B","text":"あれ、そちらは社会科研究室ではありませんよ。"},{"id":{"d":790,"n":5,"t":2},"speaker":"A","text":"研究室に行く前に、研究室の鍵を取りに行かなければなりません。"},{"id":{"d":790,"n":6,"t":2},"speaker":"B","text":"そうでしたね。"},{"id":{"d":790,"n":7,"t":2},"speaker":"A","text":"先に研究室に行っていてもよいですよ。"},{"id":{"d":790,"n":8,"t":2},"speaker":"B","text":"それでは研究室の前で待っています。"}];
INSERT INTO sentence [{"id":{"d":791,"n":1,"t":2},"speaker":"A","text":"家庭科の宿題のブラウスはできましたか？"},{"id":{"d":791,"n":2,"t":2},"speaker":"B","text":"はい。一応できたのですが、襟がうまくできませんでした。"},{"id":{"d":791,"n":3,"t":2},"speaker":"A","text":"私も着てみたら、首のところのボタンが留まりません。"},{"id":{"d":791,"n":4,"t":2},"speaker":"B","text":"私は、ボタンは留まるのですが、襟の長さがちぐはぐです。"},{"id":{"d":791,"n":5,"t":2},"speaker":"A","text":"直す時間はありませんでしたよね？"},{"id":{"d":791,"n":6,"t":2},"speaker":"B","text":"はい。昨夜、12時過ぎまで作業をしていたら母に叱られました。"},{"id":{"d":791,"n":7,"t":2},"speaker":"A","text":"私は叱られていませんが、夜中までやってこの通りです。"},{"id":{"d":791,"n":8,"t":2},"speaker":"B","text":"お互い努力賞ですね。"}];
INSERT INTO sentence [{"id":{"d":792,"n":1,"t":2},"speaker":"A","text":"仲良しグルーブの中で、小夜子さんだけ進学する中学校が違うのは悲しいです。"},{"id":{"d":792,"n":2,"t":2},"speaker":"B","text":"難しい中学校に受かったのですから、お祝いしてあげましょう。"},{"id":{"d":792,"n":3,"t":2},"speaker":"A","text":"友情の証に5人でおそろいのものを買いたいです。"},{"id":{"d":792,"n":4,"t":2},"speaker":"B","text":"それは良い案ですね。何が良いと思いますか？"},{"id":{"d":792,"n":5,"t":2},"speaker":"A","text":"桜の花びらは5枚でしょう？だから桜の付いているものが良いと思います。"},{"id":{"d":792,"n":6,"t":2},"speaker":"B","text":"心当たりがあるのですか？"},{"id":{"d":792,"n":7,"t":2},"speaker":"A","text":"七宝焼きのお店に桜の付いたヘアピンがありました。"},{"id":{"d":792,"n":8,"t":2},"speaker":"B","text":"それは良いですね。中学生でも付けられます。"}];
INSERT INTO sentence [{"id":{"d":793,"n":1,"t":2},"speaker":"A","text":"卒業式は泣いてしまいました。"},{"id":{"d":793,"n":2,"t":2},"speaker":"B","text":"みんな同じ中学校に進むのに不思議ですね。"},{"id":{"d":793,"n":3,"t":2},"speaker":"A","text":"でも、先生方とはお別れです。"},{"id":{"d":793,"n":4,"t":2},"speaker":"B","text":"そうですよね。"},{"id":{"d":793,"n":5,"t":2},"speaker":"A","text":"音楽の先生だけでも中学校に移ってくれないでしょうか？"},{"id":{"d":793,"n":6,"t":2},"speaker":"B","text":"そのようなことはできませんよ。"},{"id":{"d":793,"n":7,"t":2},"speaker":"A","text":"また泣けてきてしまいました。"},{"id":{"d":793,"n":8,"t":2},"speaker":"B","text":"中学校にもきっと良い先生がいらっしゃいますよ。"}];
INSERT INTO sentence [{"id":{"d":794,"n":1,"t":2},"speaker":"A","text":"集合写真を撮りましょう。皆さん外に出てください。"},{"id":{"d":794,"n":2,"t":2},"speaker":"B","text":"外の大階段に並ぶのですね？"},{"id":{"d":794,"n":3,"t":2},"speaker":"A","text":"そうです。"},{"id":{"d":794,"n":4,"t":2},"speaker":"B","text":"並び方はどうするのですか？"},{"id":{"d":794,"n":5,"t":2},"speaker":"A","text":"先生は真ん中ですが、あとは好きなように並んでください。"},{"id":{"d":794,"n":6,"t":2},"speaker":"B","text":"出席番号や背の順など、並び方のルールはありませんか？"},{"id":{"d":794,"n":7,"t":2},"speaker":"A","text":"ありません。きっちりした写真にしないということなので、仲良し同士集まっても大丈夫です。"},{"id":{"d":794,"n":8,"t":2},"speaker":"B","text":"分かりました。みんなでクラスのポーズを決めましょう。"}];
INSERT INTO sentence [{"id":{"d":795,"n":1,"t":2},"speaker":"A","text":"文化祭で演劇を行うことになりました。"},{"id":{"d":795,"n":2,"t":2},"speaker":"B","text":"和田さんは演劇部なのですか？"},{"id":{"d":795,"n":3,"t":2},"speaker":"A","text":"いいえ。クラスの出し物です。"},{"id":{"d":795,"n":4,"t":2},"speaker":"B","text":"クラスで演劇は珍しいのではありませんか？"},{"id":{"d":795,"n":5,"t":2},"speaker":"A","text":"はい。クラスの子が1人、役者を目指しているので演劇を行うことになりました。"},{"id":{"d":795,"n":6,"t":2},"speaker":"B","text":"なるほど。その子のアイデアなのですね。"},{"id":{"d":795,"n":7,"t":2},"speaker":"A","text":"台本を書いてきてくれて、それが面白いとクラス中が興奮状態になってしまいました。"},{"id":{"d":795,"n":8,"t":2},"speaker":"B","text":"すてきですね。本番が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":796,"n":1,"t":2},"speaker":"A","text":"お芝居の衣装を作らなくてはなりません。"},{"id":{"d":796,"n":2,"t":2},"speaker":"B","text":"文化祭の演劇で使う衣装ですね？"},{"id":{"d":796,"n":3,"t":2},"speaker":"A","text":"はい、昔話のようなお芝居なので、和服を作る予定です。"},{"id":{"d":796,"n":4,"t":2},"speaker":"B","text":"なるほど、確かにそれだと制服ではできませんね。"},{"id":{"d":796,"n":5,"t":2},"speaker":"A","text":"もし、要らない着物などありましたら譲ってください。"},{"id":{"d":796,"n":6,"t":2},"speaker":"B","text":"少しならあるので明日持ってきますね。"},{"id":{"d":796,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。当日はぜひ見に行きてくださいね。"},{"id":{"d":796,"n":8,"t":2},"speaker":"B","text":"はい、文化祭当日を楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":797,"n":1,"t":2},"speaker":"A","text":"お母さん、夏休みの自由研究に牛乳パックを使いたいです。"},{"id":{"d":797,"n":2,"t":2},"speaker":"B","text":"今日が資源ごみの日なので、昨日までの牛乳パックは出してしまいました。"},{"id":{"d":797,"n":3,"t":2},"speaker":"A","text":"もう1枚もありませんか？"},{"id":{"d":797,"n":4,"t":2},"speaker":"B","text":"はい。乾かしている牛乳パックはありませんね。飲みかけのパックは冷蔵庫に入っています。"},{"id":{"d":797,"n":5,"t":2},"speaker":"A","text":"では、それを早く飲みましょう。"},{"id":{"d":797,"n":6,"t":2},"speaker":"B","text":"まだ900ミリリットルくらいありますよ。"},{"id":{"d":797,"n":7,"t":2},"speaker":"A","text":"お父さんとお母さんと私で飲めば300ミリリットルずつです。"},{"id":{"d":797,"n":8,"t":2},"speaker":"B","text":"お父さんは飲みたがらないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":798,"n":1,"t":2},"speaker":"A","text":"牛乳パックを急に集めるのは大変なのですね。"},{"id":{"d":798,"n":2,"t":2},"speaker":"B","text":"ええ、お隣のお宅にまで牛乳パックをもらいに行って大変でした。"},{"id":{"d":798,"n":3,"t":2},"speaker":"A","text":"ごめんなさい。"},{"id":{"d":798,"n":4,"t":2},"speaker":"B","text":"さらに早くに言っておいてくれれば良かったのですが。"},{"id":{"d":798,"n":5,"t":2},"speaker":"A","text":"思い付いたのは昨日でしたから。"},{"id":{"d":798,"n":6,"t":2},"speaker":"B","text":"その集めた牛乳パックで何を作るのですか？"},{"id":{"d":798,"n":7,"t":2},"speaker":"A","text":"かわいいタンスを作ろうと思っています。"},{"id":{"d":798,"n":8,"t":2},"speaker":"B","text":"すてきです。机の上のお片付けに役立ちそうですね。"}];
INSERT INTO sentence [{"id":{"d":799,"n":1,"t":2},"speaker":"A","text":"喜美ちゃんは、夏休みの自由研究として牛乳パックでかわいいタンスを作ってきたのですね？"},{"id":{"d":799,"n":2,"t":2},"speaker":"B","text":"はい。牛乳パックを集めることが大変でしたが、かわいいものができて良かったです。"},{"id":{"d":799,"n":3,"t":2},"speaker":"A","text":"布が貼ってあるのですね。牛乳パックで作ったものには見えません。"},{"id":{"d":799,"n":4,"t":2},"speaker":"B","text":"愛ちゃんは何を作ってきたのですか？"},{"id":{"d":799,"n":5,"t":2},"speaker":"A","text":"私はペン立てです。"},{"id":{"d":799,"n":6,"t":2},"speaker":"B","text":"これですか？3つに分かれているところが使いやすそうです。"},{"id":{"d":799,"n":7,"t":2},"speaker":"A","text":"それはトイレットペーパーの芯で作ったのです。"},{"id":{"d":799,"n":8,"t":2},"speaker":"B","text":"トイレットペーパーの芯で作ったものには見えませんよ。すてきです。"}];
INSERT INTO sentence [{"id":{"d":800,"n":1,"t":2},"speaker":"A","text":"明日の朝刊に福田先輩が載るそうですよ。"},{"id":{"d":800,"n":2,"t":2},"speaker":"B","text":"素晴しいことですね。何の記事ですか？"},{"id":{"d":800,"n":3,"t":2},"speaker":"A","text":"先日、地域清掃の活動で表彰されたでしょう？その記事だそうです。"},{"id":{"d":800,"n":4,"t":2},"speaker":"B","text":"ああ、市長さんから表彰状を頂いていましたね。"},{"id":{"d":800,"n":5,"t":2},"speaker":"A","text":"その時のことをインタビューされたそうです。"},{"id":{"d":800,"n":6,"t":2},"speaker":"B","text":"新聞は何新聞ですか？"},{"id":{"d":800,"n":7,"t":2},"speaker":"A","text":"神川新聞です。"},{"id":{"d":800,"n":8,"t":2},"speaker":"B","text":"残念、うちは毎朝新聞なのです。福田先輩の記事を見たいので明日学校に持ってきてくれませんか？"},{"id":{"d":800,"n":9,"t":2},"speaker":"A","text":"よいですよ。持ってきますね。"}];
INSERT INTO sentence [{"id":{"d":801,"n":1,"t":2},"speaker":"A","text":"先生、さようなら。"},{"id":{"d":801,"n":2,"t":2},"speaker":"B","text":"学君、少し待ってください。"},{"id":{"d":801,"n":3,"t":2},"speaker":"A","text":"何ですか？"},{"id":{"d":801,"n":4,"t":2},"speaker":"B","text":"ランドセルにぶら下げた巾着が落ちそうですよ。"},{"id":{"d":801,"n":5,"t":2},"speaker":"A","text":"本当ですね。直しました、ありがとうございます。"},{"id":{"d":801,"n":6,"t":2},"speaker":"B","text":"気を付けて帰ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":802,"n":1,"t":2},"speaker":"A","text":"タケちゃん、今日は学校がお休みなのですか？"},{"id":{"d":802,"n":2,"t":2},"speaker":"B","text":"はい。インフルエンザの流行で学級閉鎖になってしまったのです。"},{"id":{"d":802,"n":3,"t":2},"speaker":"A","text":"タケちゃんは、具合は悪くありませんか？"},{"id":{"d":802,"n":4,"t":2},"speaker":"B","text":"僕は元気です。"},{"id":{"d":802,"n":5,"t":2},"speaker":"A","text":"お母さまはお仕事に行かれたのですか？"},{"id":{"d":802,"n":6,"t":2},"speaker":"B","text":"はい。学校に行ったら、「帰るように」と言われたので、お母さんは学級閉鎖でお休みになったことは知りません。"},{"id":{"d":802,"n":7,"t":2},"speaker":"A","text":"それでは、おばちゃんが昼食を作ってあげますね。"},{"id":{"d":802,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":803,"n":1,"t":2},"speaker":"A","text":"今年の新入生に素晴らしい子がいるらしいですよ。"},{"id":{"d":803,"n":2,"t":2},"speaker":"B","text":"そうなのですか？"},{"id":{"d":803,"n":3,"t":2},"speaker":"A","text":"はい。少年野球の世界大会の日本代表らしいです。"},{"id":{"d":803,"n":4,"t":2},"speaker":"B","text":"世界大会ですか？本当に素晴らしいですね。"},{"id":{"d":803,"n":5,"t":2},"speaker":"A","text":"でも、どうして私たちの学校に来るのか不思議です。"},{"id":{"d":803,"n":6,"t":2},"speaker":"B","text":"野球部はそれほど強くありませんよね？"},{"id":{"d":803,"n":7,"t":2},"speaker":"A","text":"そのようです。もしかしたら漫画の主人公のように私たちの野球部を強くしてくれるのかもしれませんね。"},{"id":{"d":803,"n":8,"t":2},"speaker":"B","text":"そうなったら良いですね。"}];
INSERT INTO sentence [{"id":{"d":804,"n":1,"t":2},"speaker":"A","text":"文化祭で売れ残ったクッキーを分けてもらいました。"},{"id":{"d":804,"n":2,"t":2},"speaker":"B","text":"料理部の田代さんにですか？"},{"id":{"d":804,"n":3,"t":2},"speaker":"A","text":"そうです。甘くないクッキーだけ売れ残ってしまったそうです。"},{"id":{"d":804,"n":4,"t":2},"speaker":"B","text":"甘くありませんか？"},{"id":{"d":804,"n":5,"t":2},"speaker":"A","text":"甘くありません。「甘いものが食べたかった」と顔に書いてありますね。"},{"id":{"d":804,"n":6,"t":2},"speaker":"B","text":"糖分が欲しかったのです。"},{"id":{"d":804,"n":7,"t":2},"speaker":"A","text":"これにも少しはお砂糖が入っていますよ。"}];
INSERT INTO sentence [{"id":{"d":805,"n":1,"t":2},"speaker":"A","text":"おはようございます。"},{"id":{"d":805,"n":2,"t":2},"speaker":"B","text":"おはよう。今日は朝練ですか？"},{"id":{"d":805,"n":3,"t":2},"speaker":"A","text":"はい。私は剣道部なので朝稽古です。"},{"id":{"d":805,"n":4,"t":2},"speaker":"B","text":"剣道部はそのように言うのですね。毎日、道具類が重たそうだと思っているのですよ。"},{"id":{"d":805,"n":5,"t":2},"speaker":"A","text":"道具は学校に置いておけませんから。"},{"id":{"d":805,"n":6,"t":2},"speaker":"B","text":"朝食は家で食べてきたのですか？"},{"id":{"d":805,"n":7,"t":2},"speaker":"A","text":"簡単にパンを食べてきたのですが、朝稽古の後におにぎりも食べます。"},{"id":{"d":805,"n":8,"t":2},"speaker":"B","text":"食欲旺盛ですね。"}];
INSERT INTO sentence [{"id":{"d":806,"n":1,"t":2},"speaker":"A","text":"鉛筆削りを持っていませんか？"},{"id":{"d":806,"n":2,"t":2},"speaker":"B","text":"ごめんなさい、持っていません。芯が折れてしまったのですか？"},{"id":{"d":806,"n":3,"t":2},"speaker":"A","text":"はい。今日は筆圧が高いのか、3本の鉛筆がみんな折れてしまったのです。"},{"id":{"d":806,"n":4,"t":2},"speaker":"B","text":"職員室に行けば、先生方のどなたかが鉛筆削りをお持ちだと思いますよ。"},{"id":{"d":806,"n":5,"t":2},"speaker":"A","text":"それでは職員室に行って、鉛筆削りが借りられるかどうか聞いてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":807,"n":1,"t":2},"speaker":"A","text":"渡り廊下にこれが落ちていましたよ。"},{"id":{"d":807,"n":2,"t":2},"speaker":"B","text":"私のペンケースですね。ありがとうございます。"},{"id":{"d":807,"n":3,"t":2},"speaker":"A","text":"移動教室の時に落としたのですね。"},{"id":{"d":807,"n":4,"t":2},"speaker":"B","text":"よく私のペンケースと分かりましたね。"},{"id":{"d":807,"n":5,"t":2},"speaker":"A","text":"失礼でしたけれど、ペンケースの中を開けてみたら消しゴムにあなたの名前が彫ってありましたので。"},{"id":{"d":807,"n":6,"t":2},"speaker":"B","text":"この間、ふざけて名字のはんこを彫ってみたのでした。"},{"id":{"d":807,"n":7,"t":2},"speaker":"A","text":"あれほど大きいはんこでは、普通には使えませんね。"},{"id":{"d":807,"n":8,"t":2},"speaker":"B","text":"遊びのはんこですから。"}];
INSERT INTO sentence [{"id":{"d":808,"n":1,"t":2},"speaker":"A","text":"読書感想文は書けましたか？"},{"id":{"d":808,"n":2,"t":2},"speaker":"B","text":"いえ。それどころかまだ課題図書も選べていません。"},{"id":{"d":808,"n":3,"t":2},"speaker":"A","text":"今から本を選んで読むのですか？"},{"id":{"d":808,"n":4,"t":2},"speaker":"B","text":"そうです。"},{"id":{"d":808,"n":5,"t":2},"speaker":"A","text":"夏休みが終了するまでに、間に合いますか？"},{"id":{"d":808,"n":6,"t":2},"speaker":"B","text":"どうでしょうか。課題図書が前に読んだ本だったら良かったのにと思っています。"},{"id":{"d":808,"n":7,"t":2},"speaker":"A","text":"読んだことがない本ばかりですよね。"},{"id":{"d":808,"n":8,"t":2},"speaker":"B","text":"そうなのです。夏休みはあと1週間で終わりなので、とても焦っています。"}];
INSERT INTO sentence [{"id":{"d":809,"n":1,"t":2},"speaker":"A","text":"まだ鈴江さんがランチルームに来ていませんね。"},{"id":{"d":809,"n":2,"t":2},"speaker":"B","text":"そうですね。"},{"id":{"d":809,"n":3,"t":2},"speaker":"A","text":"鈴江さんは、午前中に「おなかが痛い」と言っていたので心配です。"},{"id":{"d":809,"n":4,"t":2},"speaker":"B","text":"トイレに行ったのでしょうか？"},{"id":{"d":809,"n":5,"t":2},"speaker":"A","text":"いいえ。保健室かもしれません。"},{"id":{"d":809,"n":6,"t":2},"speaker":"B","text":"委員長、保健室を見てきてくれますか？"},{"id":{"d":809,"n":7,"t":2},"speaker":"A","text":"分かりました。"},{"id":{"d":809,"n":8,"t":2},"speaker":"B","text":"私はトイレを見てきます。"}];
INSERT INTO sentence [{"id":{"d":810,"n":1,"t":2},"speaker":"A","text":"汗がひどいので着替えてきます。"},{"id":{"d":810,"n":2,"t":2},"speaker":"B","text":"更衣室に行くのでしたら、私のラケットを持って来てくれますか？"},{"id":{"d":810,"n":3,"t":2},"speaker":"A","text":"予備のラケットですね。分かりました。"},{"id":{"d":810,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。これがロッカーの鍵です。"},{"id":{"d":810,"n":5,"t":2},"speaker":"A","text":"確かに預かりました。"},{"id":{"d":810,"n":6,"t":2},"speaker":"B","text":"赤いケースに入っていて、かばんの上にあるのですぐ分かると思います。"},{"id":{"d":810,"n":7,"t":2},"speaker":"A","text":"赤いケースですね。了解です。"},{"id":{"d":810,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":811,"n":1,"t":2},"speaker":"A","text":"クイズ研究会の部屋に行きましたか？"},{"id":{"d":811,"n":2,"t":2},"speaker":"B","text":"いえ、自分のサークルの模擬店が忙しくてまだ行けていません。"},{"id":{"d":811,"n":3,"t":2},"speaker":"A","text":"とても面白かったので手が空いたらぜひ行ってみてください。"},{"id":{"d":811,"n":4,"t":2},"speaker":"B","text":"クイズ研究会は何をしているのですか？"},{"id":{"d":811,"n":5,"t":2},"speaker":"A","text":"脱出ゲーム部屋です。"},{"id":{"d":811,"n":6,"t":2},"speaker":"B","text":"謎を解かないと部屋から出られないゲームですね？"},{"id":{"d":811,"n":7,"t":2},"speaker":"A","text":"はい。初級中級上級の部屋があって、どれも難しかったです。"},{"id":{"d":811,"n":8,"t":2},"speaker":"B","text":"全部屋を脱出できたのですか？素晴しいことです。"}];
INSERT INTO sentence [{"id":{"d":812,"n":1,"t":2},"speaker":"A","text":"ゴム手袋がたくさんありますね。"},{"id":{"d":812,"n":2,"t":2},"speaker":"B","text":"これは華道部の展示のために使うのです。"},{"id":{"d":812,"n":3,"t":2},"speaker":"A","text":"華道部の展示でゴム手袋を使うのですか？"},{"id":{"d":812,"n":4,"t":2},"speaker":"B","text":"これを膨らませるとお花のようになるでしょう？"},{"id":{"d":812,"n":5,"t":2},"speaker":"A","text":"確かに、ピンクのお花にも見えますね。"},{"id":{"d":812,"n":6,"t":2},"speaker":"B","text":"木の枝に膨らませたゴム手袋を付けて、大きな作品にするのです。"},{"id":{"d":812,"n":7,"t":2},"speaker":"A","text":"面白いアイデアですね。完成した作品が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":813,"n":1,"t":2},"speaker":"A","text":"新入生歓迎会は素晴らしかったですね。"},{"id":{"d":813,"n":2,"t":2},"speaker":"B","text":"ええ、各部が新入生を勧誘する熱気で満ちあふれていました。"},{"id":{"d":813,"n":3,"t":2},"speaker":"A","text":"私は書道部のはかま姿で大きな紙に字を書くパフォーマンスが素晴らしいと思いました。"},{"id":{"d":813,"n":4,"t":2},"speaker":"B","text":"素晴しかったですね。数年前は、書道部はパフォーマンスなどしなかったですよね？"},{"id":{"d":813,"n":5,"t":2},"speaker":"A","text":"はい。私が生徒だった頃は、地味に半紙に字を書くだけだったと記憶しています。"},{"id":{"d":813,"n":6,"t":2},"speaker":"B","text":"私の頃もそうでした。"},{"id":{"d":813,"n":7,"t":2},"speaker":"A","text":"漫画の影響らしいですが、指導する顧問の青柳先生が方針を変えられたのでしょうね。"},{"id":{"d":813,"n":8,"t":2},"speaker":"B","text":"私たちも見習って指導しないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":814,"n":1,"t":2},"speaker":"A","text":"試験監督のアルバイトをしませんか？人数が足りないそうなのです。"},{"id":{"d":814,"n":2,"t":2},"speaker":"B","text":"居酒屋のアルバイトと、スケジュールが重ならなければよいですよ。"},{"id":{"d":814,"n":3,"t":2},"speaker":"A","text":"来月10日の日曜日なのですが、どうですか？"},{"id":{"d":814,"n":4,"t":2},"speaker":"B","text":"日曜日なら空いています。"},{"id":{"d":814,"n":5,"t":2},"speaker":"A","text":"申し込みはインターネットからお願いしますね。"},{"id":{"d":814,"n":6,"t":2},"speaker":"B","text":"分かりました。試験監督を行う場所はどこですか？"},{"id":{"d":814,"n":7,"t":2},"speaker":"A","text":"私たちの母校ですよ。"},{"id":{"d":814,"n":8,"t":2},"speaker":"B","text":"卒業以来、母校には行っていません。懐かしいですね。"}];
INSERT INTO sentence [{"id":{"d":815,"n":1,"t":2},"speaker":"A","text":"今日の給食のお料理には裏の畑でみんなが作ったジャガイモを使っているのですよ。"},{"id":{"d":815,"n":2,"t":2},"speaker":"B","text":"先週、みんなで採ったジャガイモですか？"},{"id":{"d":815,"n":3,"t":2},"speaker":"A","text":"そうですよ。大きいのも小さいのもありましたよね。"},{"id":{"d":815,"n":4,"t":2},"speaker":"B","text":"はい。私が一番大きいのを採りました。"},{"id":{"d":815,"n":5,"t":2},"speaker":"A","text":"大きいのも小さいのもみんな蒸かして今日のポテトサラダにしたのです。"},{"id":{"d":815,"n":6,"t":2},"speaker":"B","text":"全部、一緒になってしまったのですか？"},{"id":{"d":815,"n":7,"t":2},"speaker":"A","text":"はい。丸いままではお料理にするのは難しいですからね。でも大きいのもきちんと入っていますよ。"},{"id":{"d":815,"n":8,"t":2},"speaker":"B","text":"とてもおいしいポテトサラダです。"}];
INSERT INTO sentence [{"id":{"d":816,"n":1,"t":2},"speaker":"A","text":"緑ちゃん、久しぶりですね。"},{"id":{"d":816,"n":2,"t":2},"speaker":"B","text":"友梨ちゃん！久しぶりです。学校の帰りですか？"},{"id":{"d":816,"n":3,"t":2},"speaker":"A","text":"そうです。緑ちゃんは何をされていたのですか？"},{"id":{"d":816,"n":4,"t":2},"speaker":"B","text":"私も学校の帰りです。"},{"id":{"d":816,"n":5,"t":2},"speaker":"A","text":"高校はどうですか？"},{"id":{"d":816,"n":6,"t":2},"speaker":"B","text":"勉強についていくことがようやくできています。"},{"id":{"d":816,"n":7,"t":2},"speaker":"A","text":"大変そうですね。私は部活動ざんまいです。"}];
INSERT INTO sentence [{"id":{"d":817,"n":1,"t":2},"speaker":"A","text":"朝礼で新しく着任された先生が紹介されていましたね。"},{"id":{"d":817,"n":2,"t":2},"speaker":"B","text":"はい。数学の先生は少し怖そうでした。"},{"id":{"d":817,"n":3,"t":2},"speaker":"A","text":"私たちの数学の先生でしょうか？"},{"id":{"d":817,"n":4,"t":2},"speaker":"B","text":"2年生を担当すると仰っていましたので、可能性はあります。私は1年の時の室田先生のままがいいです。"},{"id":{"d":817,"n":5,"t":2},"speaker":"A","text":"室田先生は優しいですからね。"},{"id":{"d":817,"n":6,"t":2},"speaker":"B","text":"室田先生のおかげで少しだけ成績が上がったのです。"},{"id":{"d":817,"n":7,"t":2},"speaker":"A","text":"私も同じく少し成績が上がりました。"},{"id":{"d":817,"n":8,"t":2},"speaker":"B","text":"どうか担当の先生が変わりませんように。"}];
INSERT INTO sentence [{"id":{"d":818,"n":1,"t":2},"speaker":"A","text":"園長先生、おはようございます。"},{"id":{"d":818,"n":2,"t":2},"speaker":"B","text":"花ちゃん、おはようございます。"},{"id":{"d":818,"n":3,"t":2},"speaker":"A","text":"このような雪の日にずっと外にいるなんで、先生は寒くありませんか？"},{"id":{"d":818,"n":4,"t":2},"speaker":"B","text":"たくさん着ていますから寒くはありませんよ。"},{"id":{"d":818,"n":5,"t":2},"speaker":"A","text":"私はたくさん着ても寒いです。"},{"id":{"d":818,"n":6,"t":2},"speaker":"B","text":"本当ですね。お鼻から鼻水が出ていますね。"},{"id":{"d":818,"n":7,"t":2},"speaker":"A","text":"ティッシュを忘れてしまいました。"},{"id":{"d":818,"n":8,"t":2},"speaker":"B","text":"園長先生のティッシュで鼻をかみましょうね。"}];
INSERT INTO sentence [{"id":{"d":819,"n":1,"t":2},"speaker":"A","text":"願書の書き方はこれで良いですか？"},{"id":{"d":819,"n":2,"t":2},"speaker":"B","text":"はい。内容は良いと思いますが、写真はどうしましたか？"},{"id":{"d":819,"n":3,"t":2},"speaker":"A","text":"まだ撮っていません。"},{"id":{"d":819,"n":4,"t":2},"speaker":"B","text":"願書の締め切りは明日ですよ。"},{"id":{"d":819,"n":5,"t":2},"speaker":"A","text":"帰りに証明写真機で撮ります。"},{"id":{"d":819,"n":6,"t":2},"speaker":"B","text":"今晩か明日、必ず忘れないように投函するのですよ。"},{"id":{"d":819,"n":7,"t":2},"speaker":"A","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":820,"n":1,"t":2},"speaker":"A","text":"先生、丹羽さんが気持ち悪いそうです。"},{"id":{"d":820,"n":2,"t":2},"speaker":"B","text":"保健室まで歩けそうですか？"},{"id":{"d":820,"n":3,"t":2},"speaker":"A","text":"大丈夫だそうです。"},{"id":{"d":820,"n":4,"t":2},"speaker":"B","text":"では付き添って行ってあげてください。"},{"id":{"d":820,"n":5,"t":2},"speaker":"A","text":"私は生徒会からの報告をしなくてはなりません。木村さんに頼んでも良いですか？"},{"id":{"d":820,"n":6,"t":2},"speaker":"B","text":"はい。木村さんが大丈夫そうでしたら頼んでください。"},{"id":{"d":820,"n":7,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":821,"n":1,"t":2},"speaker":"A","text":"今日は帰りにコンビニエンスストアに寄ります。"},{"id":{"d":821,"n":2,"t":2},"speaker":"B","text":"何を買いに行くのですか？"},{"id":{"d":821,"n":3,"t":2},"speaker":"A","text":"明日の試験のために勉強するので夜食を買いに行きたいのです。"},{"id":{"d":821,"n":4,"t":2},"speaker":"B","text":"意欲に満ちていますね？"},{"id":{"d":821,"n":5,"t":2},"speaker":"A","text":"いいえ。実はコンビニエンスストアの新作スイーツが目当てなのですけれどね。"},{"id":{"d":821,"n":6,"t":2},"speaker":"B","text":"勉強よりスイーツとは七海らしいですね。"},{"id":{"d":821,"n":7,"t":2},"speaker":"A","text":"おいしいものを食べて成績アップしますよ。"},{"id":{"d":821,"n":8,"t":2},"speaker":"B","text":"成績を抜かされないように、私も母の用意してくれる夜食を食べて頑張ります。"}];
INSERT INTO sentence [{"id":{"d":822,"n":1,"t":2},"speaker":"A","text":"こんにちは。"},{"id":{"d":822,"n":2,"t":2},"speaker":"B","text":"こんにちは。カフェで会うのは、珍しいですね。"},{"id":{"d":822,"n":3,"t":2},"speaker":"A","text":"そうですね。試験勉強をしに図書館に行ったら今日は満席だったのです。"},{"id":{"d":822,"n":4,"t":2},"speaker":"B","text":"それでカフェで勉強をするのですか？"},{"id":{"d":822,"n":5,"t":2},"speaker":"A","text":"はい。ここも高校生が多いですね。"},{"id":{"d":822,"n":6,"t":2},"speaker":"B","text":"どの学校も試験期間中ですからね。"},{"id":{"d":822,"n":7,"t":2},"speaker":"A","text":"では勉強しますので、これで失礼します。"},{"id":{"d":822,"n":8,"t":2},"speaker":"B","text":"頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":823,"n":1,"t":2},"speaker":"A","text":"松岡さん、今日のお昼の食事は学生食堂を利用しますか？"},{"id":{"d":823,"n":2,"t":2},"speaker":"B","text":"いつも通り、学生食堂を利用しますよ。"},{"id":{"d":823,"n":3,"t":2},"speaker":"A","text":"この後、職員室に行かなくてはならないので、A定食の食券を確保しておいてもらえますか？"},{"id":{"d":823,"n":4,"t":2},"speaker":"B","text":"よいですよ。今日のA定食はチーズハンバーグでしたね。"},{"id":{"d":823,"n":5,"t":2},"speaker":"A","text":"はい。人気があるメニューの時に呼び出しを行うとは、先生もひどいですよね？"},{"id":{"d":823,"n":6,"t":2},"speaker":"B","text":"先生方は学生食堂のメニューのことは気にしていらっしゃらないでしょうから。"},{"id":{"d":823,"n":7,"t":2},"speaker":"A","text":"そうですね。では、職員室に行ってきます。食券をよろしくお願いします。"},{"id":{"d":823,"n":8,"t":2},"speaker":"B","text":"了解しました。"}];
INSERT INTO sentence [{"id":{"d":824,"n":1,"t":2},"speaker":"A","text":"弟が修学旅行に行ってきたのです。"},{"id":{"d":824,"n":2,"t":2},"speaker":"B","text":"どこに行かれたのですか？"},{"id":{"d":824,"n":3,"t":2},"speaker":"A","text":"京都と奈良です。"},{"id":{"d":824,"n":4,"t":2},"speaker":"B","text":"お土産はこれですか？"},{"id":{"d":824,"n":5,"t":2},"speaker":"A","text":"そうです。木刀でした。"},{"id":{"d":824,"n":6,"t":2},"speaker":"B","text":"お姉さんへのお土産としては珍しいですね。"},{"id":{"d":824,"n":7,"t":2},"speaker":"A","text":"鼻高々になって渡してくれましたよ。思わず笑ってしまいました。"}];
INSERT INTO sentence [{"id":{"d":825,"n":1,"t":2},"speaker":"A","text":"明日から大学構内で古本市を実施するのだそうです。"},{"id":{"d":825,"n":2,"t":2},"speaker":"B","text":"明日からいつまでですか？"},{"id":{"d":825,"n":3,"t":2},"speaker":"A","text":"在庫があれば1週間くらいらしいですが、早く行った方がいいらしいですよ。"},{"id":{"d":825,"n":4,"t":2},"speaker":"B","text":"どうしてですか？"},{"id":{"d":825,"n":5,"t":2},"speaker":"A","text":"主に先輩の使った教科書が売られるので、貴重なメモが書かれていて人気があるのです。"},{"id":{"d":825,"n":6,"t":2},"speaker":"B","text":"売り切れてしまうということですね？"},{"id":{"d":825,"n":7,"t":2},"speaker":"A","text":"はい。新しく本を買うよりも安いですしね。"},{"id":{"d":825,"n":8,"t":2},"speaker":"B","text":"なるほど。では明日の朝一番に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":826,"n":1,"t":2},"speaker":"A","text":"渡り廊下の展示を見ましたか？"},{"id":{"d":826,"n":2,"t":2},"speaker":"B","text":"いえ、見ていません。何かあるのですか？"},{"id":{"d":826,"n":3,"t":2},"speaker":"A","text":"漫画研究会のメンバーが描いたイラストが展示されているのです。"},{"id":{"d":826,"n":4,"t":2},"speaker":"B","text":"今年はうまい子がいるということでしたね？"},{"id":{"d":826,"n":5,"t":2},"speaker":"A","text":"はい。一年生の中に圧倒的に上手な子がいました。"},{"id":{"d":826,"n":6,"t":2},"speaker":"B","text":"他の子とそれほど違って見えますか？"},{"id":{"d":826,"n":7,"t":2},"speaker":"A","text":"線の美しさも色使いも全く違います。"},{"id":{"d":826,"n":8,"t":2},"speaker":"B","text":"それはぜひ見てみたいです。放課後に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":827,"n":1,"t":2},"speaker":"A","text":"料理部が地元のスーパーマーケットと共同開発したお弁当が売り出されたそうですね。"},{"id":{"d":827,"n":2,"t":2},"speaker":"B","text":"私はそのお弁当を昨日、食べました。"},{"id":{"d":827,"n":3,"t":2},"speaker":"A","text":"早いですね。どうでしたか？"},{"id":{"d":827,"n":4,"t":2},"speaker":"B","text":"とてもおいしかったです。地元の食材がふんだんに使われていました。"},{"id":{"d":827,"n":5,"t":2},"speaker":"A","text":"佐久間さんのおじいさんが作っているナスが使われていると聞きましたよ。"},{"id":{"d":827,"n":6,"t":2},"speaker":"B","text":"お弁当にはマーボーナスが入っていましたから、きっとそれですね。"},{"id":{"d":827,"n":7,"t":2},"speaker":"A","text":"お弁当は人気があるようでしたか？"},{"id":{"d":827,"n":8,"t":2},"speaker":"B","text":"はい。私の目の前で次々に手に取る人が現れていましたよ。"}];
INSERT INTO sentence [{"id":{"d":828,"n":1,"t":2},"speaker":"A","text":"お母さん、通学用の定期券入れを買いに行きたいです。"},{"id":{"d":828,"n":2,"t":2},"speaker":"B","text":"高校生になったら、そのようなものが必要になるのですね。忘れていました。"},{"id":{"d":828,"n":3,"t":2},"speaker":"A","text":"美咲ちゃんと一緒に買いに行くのでお金をください。"},{"id":{"d":828,"n":4,"t":2},"speaker":"B","text":"いくらくらいのものを買うのですか？"},{"id":{"d":828,"n":5,"t":2},"speaker":"A","text":"2,000円くらいにしようと美咲ちゃんと話しました。"},{"id":{"d":828,"n":6,"t":2},"speaker":"B","text":"分かりました。では、5,000円を渡しておきますね。"},{"id":{"d":828,"n":7,"t":2},"speaker":"A","text":"多いですよ。"},{"id":{"d":828,"n":8,"t":2},"speaker":"B","text":"帰りに美咲ちゃんとドーナツでも食べていらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":829,"n":1,"t":2},"speaker":"A","text":"今年の時間割表はこれなのですか？"},{"id":{"d":829,"n":2,"t":2},"speaker":"B","text":"はい。今日の帰りに雨が降っていて、ランドセルの中までぬれてしまったのです。時間割表もぬれてしわになってしまいました。"},{"id":{"d":829,"n":3,"t":2},"speaker":"A","text":"端のほうは破れてしまっていますね。この状態で1年間ずっと使うのは不便ですね。"},{"id":{"d":829,"n":4,"t":2},"speaker":"B","text":"文字は読めるので、手書きで書き直します。"},{"id":{"d":829,"n":5,"t":2},"speaker":"A","text":"私がパソコンで見やすく作りますよ。"},{"id":{"d":829,"n":6,"t":2},"speaker":"B","text":"お父さん、ありがとうございます。"},{"id":{"d":829,"n":7,"t":2},"speaker":"A","text":"どうせ作るなら分かりやすくカラフルにしましょうか。"},{"id":{"d":829,"n":8,"t":2},"speaker":"B","text":"うれしいです。かわいい絵も入れてください。"},{"id":{"d":829,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":830,"n":1,"t":2},"speaker":"A","text":"今日で中学も卒業です。"},{"id":{"d":830,"n":2,"t":2},"speaker":"B","text":"春から高校生なのですね。信じられない思いです。"},{"id":{"d":830,"n":3,"t":2},"speaker":"A","text":"3年間、休まず学校に行けたので、皆勤賞をもらいました。"},{"id":{"d":830,"n":4,"t":2},"speaker":"B","text":"賞状と、これは何ですか？"},{"id":{"d":830,"n":5,"t":2},"speaker":"A","text":"図書カードです。"},{"id":{"d":830,"n":6,"t":2},"speaker":"B","text":"良いものを頂きましたね。"},{"id":{"d":830,"n":7,"t":2},"speaker":"A","text":"高校で使う英語の辞書をこれで買おうと思います。一緒に選んでくれませんか？"},{"id":{"d":830,"n":8,"t":2},"speaker":"B","text":"よいですよ。明日、一緒に本屋さんへ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":831,"n":1,"t":2},"speaker":"A","text":"今日は転校生の女の子が来ました。"},{"id":{"d":831,"n":2,"t":2},"speaker":"B","text":"6年生の今頃になって転校してくる転校生は珍しいですね。"},{"id":{"d":831,"n":3,"t":2},"speaker":"A","text":"こちらで受験をするためだそうです。"},{"id":{"d":831,"n":4,"t":2},"speaker":"B","text":"なるほど。2月に中学を受験するのですね。"},{"id":{"d":831,"n":5,"t":2},"speaker":"A","text":"お友達になったので、その子が中学受験をし受かったら、私とは同じ中学には行けないことが寂しいです。"},{"id":{"d":831,"n":6,"t":2},"speaker":"B","text":"もうお友達になったのですか？早いですね。"},{"id":{"d":831,"n":7,"t":2},"speaker":"A","text":"同じマンションの子だったので、今日も一緒に帰ってきました。"},{"id":{"d":831,"n":8,"t":2},"speaker":"B","text":"そういえば先週、引っ越しがありましたね。"}];
INSERT INTO sentence [{"id":{"d":832,"n":1,"t":2},"speaker":"A","text":"林間学校ではカレーを作りました。"},{"id":{"d":832,"n":2,"t":2},"speaker":"B","text":"おうちで作ったことがないのに、できたのですか？"},{"id":{"d":832,"n":3,"t":2},"speaker":"A","text":"一緒の班の子たちが上手だったので、とてもおいしくできましたよ。"},{"id":{"d":832,"n":4,"t":2},"speaker":"B","text":"あなたは何か手伝ったのですか？"},{"id":{"d":832,"n":5,"t":2},"speaker":"A","text":"野菜のごみ捨てとお皿洗いをしました。"},{"id":{"d":832,"n":6,"t":2},"speaker":"B","text":"それも立派なお手伝いですね。"},{"id":{"d":832,"n":7,"t":2},"speaker":"A","text":"今度は私もカレーを作ってみたいです。"},{"id":{"d":832,"n":8,"t":2},"speaker":"B","text":"いいですね。家で好きなだけカレー作りの練習をしてください。"}];
INSERT INTO sentence [{"id":{"d":833,"n":1,"t":2},"speaker":"A","text":"学芸会では何の出し物をするのですか？"},{"id":{"d":833,"n":2,"t":2},"speaker":"B","text":"ハンドベルの演奏をします。"},{"id":{"d":833,"n":3,"t":2},"speaker":"A","text":"曲は何ですか？"},{"id":{"d":833,"n":4,"t":2},"speaker":"B","text":"私たちは「大きな栗の木の下で」を演奏します。"},{"id":{"d":833,"n":5,"t":2},"speaker":"A","text":"何の音を担当するのですか？"},{"id":{"d":833,"n":6,"t":2},"speaker":"B","text":"私は「ファ」です。"},{"id":{"d":833,"n":7,"t":2},"speaker":"A","text":"その曲の「ファ」は少なかったような気がします。"},{"id":{"d":833,"n":8,"t":2},"speaker":"B","text":"その通りです。私の出番は1回です。"}];
INSERT INTO sentence [{"id":{"d":834,"n":1,"t":2},"speaker":"A","text":"昼休みに校庭で縄跳びをしましょう。"},{"id":{"d":834,"n":2,"t":2},"speaker":"B","text":"いいですね。そういえば富樫君が「大縄跳びをする」と言っていましたよ。"},{"id":{"d":834,"n":3,"t":2},"speaker":"A","text":"大縄跳びでも良いですね。"},{"id":{"d":834,"n":4,"t":2},"speaker":"B","text":"では、私たちも入れてもらいましょう。"},{"id":{"d":834,"n":5,"t":2},"speaker":"A","text":"たくさん跳んで記録を出したいですね。"},{"id":{"d":834,"n":6,"t":2},"speaker":"B","text":"はい。よく動けるように給食を食べ過ぎないようにしないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":835,"n":1,"t":2},"speaker":"A","text":"卒業記念のタイムカプセルを掘り出す時が来ました。"},{"id":{"d":835,"n":2,"t":2},"speaker":"B","text":"心が落ち着きませんね。あれから20年ですか？"},{"id":{"d":835,"n":3,"t":2},"speaker":"A","text":"はい。時がたつのは早いものです。"},{"id":{"d":835,"n":4,"t":2},"speaker":"B","text":"何を入れたか、覚えていますか？"},{"id":{"d":835,"n":5,"t":2},"speaker":"A","text":"はい。20年後の自分への手紙を入れたのは覚えています。"},{"id":{"d":835,"n":6,"t":2},"speaker":"B","text":"書いた内容は覚えていますか？"},{"id":{"d":835,"n":7,"t":2},"speaker":"A","text":"いいえ。それが全く思い出せません。"},{"id":{"d":835,"n":8,"t":2},"speaker":"B","text":"私も同じです。"}];
INSERT INTO sentence [{"id":{"d":836,"n":1,"t":2},"speaker":"A","text":"ただいま帰りました。はい、幼稚園からの連絡帳です。"},{"id":{"d":836,"n":2,"t":2},"speaker":"B","text":"おかえりなさい。今日も元気に幼稚園で過ごせましたか？"},{"id":{"d":836,"n":3,"t":2},"speaker":"A","text":"はい。元気に過ごしました。"},{"id":{"d":836,"n":4,"t":2},"speaker":"B","text":"連絡帳に「ズボンに穴が開いてしまいました」と書いてありますよ。"},{"id":{"d":836,"n":5,"t":2},"speaker":"A","text":"滑り台を滑り過ぎました。"},{"id":{"d":836,"n":6,"t":2},"speaker":"B","text":"元気過ぎたことが原因なのですね。"},{"id":{"d":836,"n":7,"t":2},"speaker":"A","text":"ここに穴が開いてしまっています。"},{"id":{"d":836,"n":8,"t":2},"speaker":"B","text":"繕ってあげますから、ズボンを脱いでください。"},{"id":{"d":836,"n":9,"t":2},"speaker":"A","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":837,"n":1,"t":2},"speaker":"A","text":"体育館にパイプ椅子を並べましょう。"},{"id":{"d":837,"n":2,"t":2},"speaker":"B","text":"入学式の準備ですね。新1年生のために入学式の会場設営の準備を頑張ります。"},{"id":{"d":837,"n":3,"t":2},"speaker":"A","text":"並べる場所は床にテープで印を付けておきましたので、それに合わせてパイプ椅子を置いてください。"},{"id":{"d":837,"n":4,"t":2},"speaker":"B","text":"裏の倉庫から椅子を出す人と、並べる人に別れた方が効率は良さそうですね。"},{"id":{"d":837,"n":5,"t":2},"speaker":"A","text":"そう思います。そうしてください。"},{"id":{"d":837,"n":6,"t":2},"speaker":"B","text":"分かりました。では、私たちは椅子を並べる方をやります。"},{"id":{"d":837,"n":7,"t":2},"speaker":"A","text":"では、大下さんたちは私と一緒に椅子を出す方をお願いします。"}];
INSERT INTO sentence [{"id":{"d":838,"n":1,"t":2},"speaker":"A","text":"転校してきて困ったことはありませんか？"},{"id":{"d":838,"n":2,"t":2},"speaker":"B","text":"学童保育のおやつが前にいたところと違っています。"},{"id":{"d":838,"n":3,"t":2},"speaker":"A","text":"おやつは出ているのですよね？"},{"id":{"d":838,"n":4,"t":2},"speaker":"B","text":"はい。でも、ポテトチップスやチョコレートなのです。"},{"id":{"d":838,"n":5,"t":2},"speaker":"A","text":"前のところは何だったのですか？"},{"id":{"d":838,"n":6,"t":2},"speaker":"B","text":"焼きそばやおにぎりまたは蒸しパンでした。"},{"id":{"d":838,"n":7,"t":2},"speaker":"A","text":"スナック菓子ではおなかがすいてしまうのですね？"},{"id":{"d":838,"n":8,"t":2},"speaker":"B","text":"その通りです。"}];
INSERT INTO sentence [{"id":{"d":839,"n":1,"t":2},"speaker":"A","text":"体操着を着て行くのですか？今日は運動会なのですか？"},{"id":{"d":839,"n":2,"t":2},"speaker":"B","text":"そうではなくて、今日は田植え体験の日なのです。"},{"id":{"d":839,"n":3,"t":2},"speaker":"A","text":"そうでしたか。お洋服も持っていくのですか？"},{"id":{"d":839,"n":4,"t":2},"speaker":"B","text":"はい。田植えが終わったら普通のお洋服に着替えるのです。"},{"id":{"d":839,"n":5,"t":2},"speaker":"A","text":"なるほど。それでその大荷物なのですね。気を付けて行ってらっしゃい。"},{"id":{"d":839,"n":6,"t":2},"speaker":"B","text":"秋になってお米を収穫することが楽しみです。行ってきます。"}];
INSERT INTO sentence [{"id":{"d":840,"n":1,"t":2},"speaker":"A","text":"今年の書初めには何と書きますか？"},{"id":{"d":840,"n":2,"t":2},"speaker":"B","text":"今年の課題の言葉は「将来の夢」か「希望の光」のどちらかを選ぶそうですよ。"},{"id":{"d":840,"n":3,"t":2},"speaker":"A","text":"この2つから選ぶのですか？昨年は言葉を自由に書けましたよね？"},{"id":{"d":840,"n":4,"t":2},"speaker":"B","text":"はい。自由にしたところ、書初めにふさわしくない言葉を書く人が続出したのです。昨年の掲示板はひどかったですから、仕方ありません。"},{"id":{"d":840,"n":5,"t":2},"speaker":"A","text":"そうですね。あなたはどちらの言葉を選ぶのですか？"},{"id":{"d":840,"n":6,"t":2},"speaker":"B","text":"私は「希望の光」にしようと思います。"}];
INSERT INTO sentence [{"id":{"d":841,"n":1,"t":2},"speaker":"A","text":"今日は日曜なのに学校に行くのですか？"},{"id":{"d":841,"n":2,"t":2},"speaker":"B","text":"はい。生徒会で学校の草むしりをするのです。"},{"id":{"d":841,"n":3,"t":2},"speaker":"A","text":"それで体操着なのですね。"},{"id":{"d":841,"n":4,"t":2},"speaker":"B","text":"そういえば、「お弁当を作ってください」と言うのを忘れていました。"},{"id":{"d":841,"n":5,"t":2},"speaker":"A","text":"聞いていなかったので作っていませんよ。お弁当は行きがけにコンビニエンスストアで買って行ってください。"},{"id":{"d":841,"n":6,"t":2},"speaker":"B","text":"分かりました。あとは軍手も要るのですが家にあるでしょうか？"},{"id":{"d":841,"n":7,"t":2},"speaker":"A","text":"軍手は置いてありません。軍手もコンビニエンスストアで買えるでしょうか？"},{"id":{"d":841,"n":8,"t":2},"speaker":"B","text":"多分あると思うので一緒に買って行きます。それでは行ってきますね。"},{"id":{"d":841,"n":9,"t":2},"speaker":"A","text":"はい、気を付けていってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":842,"n":1,"t":2},"speaker":"A","text":"来年から校舎の建て替えが始まるそうです。"},{"id":{"d":842,"n":2,"t":2},"speaker":"B","text":"私たちはどこで勉強すれば良いのですか？"},{"id":{"d":842,"n":3,"t":2},"speaker":"A","text":"仮設校舎です。グラウンドに仮設校舎が建てられるらしいですよ。"},{"id":{"d":842,"n":4,"t":2},"speaker":"B","text":"グラウンドを使う授業や部活動はどうなるのですか？"},{"id":{"d":842,"n":5,"t":2},"speaker":"A","text":"体育の授業は体育館を使うことが多くなるし、部活動は市営グラウンドで行うそうです。"},{"id":{"d":842,"n":6,"t":2},"speaker":"B","text":"詳しいですね。"},{"id":{"d":842,"n":7,"t":2},"speaker":"A","text":"先ほど先生に伺ってきました。仮設校舎は来月から工事が始まるそうです。"},{"id":{"d":842,"n":8,"t":2},"speaker":"B","text":"新しい校舎ができるころには、私たちは卒業していますね。"}];
INSERT INTO sentence [{"id":{"d":843,"n":1,"t":2},"speaker":"A","text":"わが校は創立100周年を迎えました。"},{"id":{"d":843,"n":2,"t":2},"speaker":"B","text":"長い歴史がある学校なのですね。"},{"id":{"d":843,"n":3,"t":2},"speaker":"A","text":"記念に学校の歴史をまとめた本を出す予定です。"},{"id":{"d":843,"n":4,"t":2},"speaker":"B","text":"私たちにも配られるのですか？"},{"id":{"d":843,"n":5,"t":2},"speaker":"A","text":"その予定です。"},{"id":{"d":843,"n":6,"t":2},"speaker":"B","text":"私たち在校生も載るのでしょうか？"},{"id":{"d":843,"n":7,"t":2},"speaker":"A","text":"はい。来週、ドローンで空撮をします。みんなには校庭で「100」の人文字を作ってもらうことになっています。"},{"id":{"d":843,"n":8,"t":2},"speaker":"B","text":"楽しそうです。撮影の日が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":844,"n":1,"t":2},"speaker":"A","text":"お父さんが卒業した中学校が閉校になってしまうそうです。"},{"id":{"d":844,"n":2,"t":2},"speaker":"B","text":"おばあちゃんの家に行ったときに連れて行ってもらったことがあります。"},{"id":{"d":844,"n":3,"t":2},"speaker":"A","text":"木造校舎の古い学校で、私は好きだったのですが残念ですね。"},{"id":{"d":844,"n":4,"t":2},"speaker":"B","text":"子どもがいなくなってしまったのですか？"},{"id":{"d":844,"n":5,"t":2},"speaker":"A","text":"はい。もう5人しか在校生がいないので、町の中学校と合併されるそうなのです。"},{"id":{"d":844,"n":6,"t":2},"speaker":"B","text":"私も通ったかもしれませんよね？"},{"id":{"d":844,"n":7,"t":2},"speaker":"A","text":"はい。お父さんのお仕事がなければ、おばあさんのお家に住んだかもしれませんからね。"},{"id":{"d":844,"n":8,"t":2},"speaker":"B","text":"お父さんは、卒業した学校がなくなってしまうと、きっと寂しいですね。"}];
INSERT INTO sentence [{"id":{"d":845,"n":1,"t":2},"speaker":"A","text":"学級委員さん、教室の後ろの掲示を全部取ってください。"},{"id":{"d":845,"n":2,"t":2},"speaker":"B","text":"全部、画びょうで留めてありますが、画びょう抜きはどこですか？"},{"id":{"d":845,"n":3,"t":2},"speaker":"A","text":"画びょう抜きはありません。"},{"id":{"d":845,"n":4,"t":2},"speaker":"B","text":"手で取るのは痛いので嫌ですよ。"},{"id":{"d":845,"n":5,"t":2},"speaker":"A","text":"そうですよね。どうしましょう。"},{"id":{"d":845,"n":6,"t":2},"speaker":"B","text":"先生、ホチキスはありませんか？"},{"id":{"d":845,"n":7,"t":2},"speaker":"A","text":"職員室にありますよ。"},{"id":{"d":845,"n":8,"t":2},"speaker":"B","text":"ホチキスの針を外すところが使えると思います。"},{"id":{"d":845,"n":9,"t":2},"speaker":"A","text":"それは良いアイデアです。早速持ってきますね。"}];
INSERT INTO sentence [{"id":{"d":846,"n":1,"t":2},"speaker":"A","text":"リュックサックを新しくしたのですね？"},{"id":{"d":846,"n":2,"t":2},"speaker":"B","text":"はい。この間、お弁当の汁をこぼしてしまい、洗っても落ちなかったので、この機に買い替えました。"},{"id":{"d":846,"n":3,"t":2},"speaker":"A","text":"リュックサックにはノートパソコンも入っていたのでしょう？大丈夫だったのですか？"},{"id":{"d":846,"n":4,"t":2},"speaker":"B","text":"はい。パソコンは大丈夫だったのですが、教科書ににじみました。"},{"id":{"d":846,"n":5,"t":2},"speaker":"A","text":"それは大変でしたね。教科書は買い直したのですか？"},{"id":{"d":846,"n":6,"t":2},"speaker":"B","text":"いえ、教科書はそのまま乾かして使っています。"},{"id":{"d":846,"n":7,"t":2},"speaker":"A","text":"もうすぐ学年も切り替わりますからね。少し汚れていても、後少しの辛抱です。"},{"id":{"d":846,"n":8,"t":2},"speaker":"B","text":"でも、この教科書を使っている講義の中間試験はひどい点数だったので、このままでは単位を落としてしまうかもしれません。"},{"id":{"d":846,"n":9,"t":2},"speaker":"A","text":"もし再履修ということになれば、教科書は書い直した方がいいですね。"},{"id":{"d":846,"n":10,"t":2},"speaker":"B","text":"僕もそう思います。"}];
INSERT INTO sentence [{"id":{"d":847,"n":1,"t":2},"speaker":"A","text":"昨日、優香ちゃんから絵手紙が送られてきましたよ。"},{"id":{"d":847,"n":2,"t":2},"speaker":"B","text":"私のところにも届きました。"},{"id":{"d":847,"n":3,"t":2},"speaker":"A","text":"とても上手でしたけれど、急にどうしたのでしょう？"},{"id":{"d":847,"n":4,"t":2},"speaker":"B","text":"絵手紙のコンクールに応募するためにたくさん書いたのだそうです。"},{"id":{"d":847,"n":5,"t":2},"speaker":"A","text":"毎日、会うのですから手渡しすれば良いのにと思いました。"},{"id":{"d":847,"n":6,"t":2},"speaker":"B","text":"官製はがきに描いてありましたでしょう？ポストに入れたかったのではありませんか？"},{"id":{"d":847,"n":7,"t":2},"speaker":"A","text":"なるほど。そういえば、官製はがきでしたね。"},{"id":{"d":847,"n":8,"t":2},"speaker":"B","text":"練習用は私製はがきにしておいた方が、お金がかからないと後で教えてあげましょう。"}];
INSERT INTO sentence [{"id":{"d":848,"n":1,"t":2},"speaker":"A","text":"テレビのクイズ番組で地図記号の問題が出ていましたが、私は全部外れました。"},{"id":{"d":848,"n":2,"t":2},"speaker":"B","text":"私もそのようなものを当てるのは苦手です。"},{"id":{"d":848,"n":3,"t":2},"speaker":"A","text":"官公署の地図記号は、私は顔文字だと思っていました。"},{"id":{"d":848,"n":4,"t":2},"speaker":"B","text":"官公署の地図記号はどのようなマークでしたか？"},{"id":{"d":848,"n":5,"t":2},"speaker":"A","text":"「小」の字のようなものの下に丸が付いている記号です。"},{"id":{"d":848,"n":6,"t":2},"speaker":"B","text":"こうですか？"},{"id":{"d":848,"n":7,"t":2},"speaker":"A","text":"そうです。「公」を表しているらしいですよ。"},{"id":{"d":848,"n":8,"t":2},"speaker":"B","text":"確かに、これは目と鼻と開けた口に見えますね。"}];
INSERT INTO sentence [{"id":{"d":849,"n":1,"t":2},"speaker":"A","text":"明日は学校がお休みです。"},{"id":{"d":849,"n":2,"t":2},"speaker":"B","text":"創立記念日か何かでしたか？"},{"id":{"d":849,"n":3,"t":2},"speaker":"A","text":"いいえ。来年の新入生のための学校説明会があるのです。"},{"id":{"d":849,"n":4,"t":2},"speaker":"B","text":"そういえば、私たちも2年前に行きましたね。"},{"id":{"d":849,"n":5,"t":2},"speaker":"A","text":"私は学校の施設に一目ぼれして「ここにする」と言ったのでした。"},{"id":{"d":849,"n":6,"t":2},"speaker":"B","text":"私は教育方針に感激して「ここにしなさい」と言ったのでした。"},{"id":{"d":849,"n":7,"t":2},"speaker":"A","text":"珍しく意見が合いましたよね。毎日楽しくてこの学校に入学して良かったです。"}];
INSERT INTO sentence [{"id":{"d":850,"n":1,"t":2},"speaker":"A","text":"先生、高校の文化祭に行ってきました。"},{"id":{"d":850,"n":2,"t":2},"speaker":"B","text":"志望校の文化祭ですか？"},{"id":{"d":850,"n":3,"t":2},"speaker":"A","text":"そうです。その高校の吹奏楽部に入りたいのです。"},{"id":{"d":850,"n":4,"t":2},"speaker":"B","text":"吹奏楽部の演奏を聴いてきたのですね？"},{"id":{"d":850,"n":5,"t":2},"speaker":"A","text":"はい、本当に素晴らしくて、その高校の吹奏楽部の部員になりたいと心から思いました。"},{"id":{"d":850,"n":6,"t":2},"speaker":"B","text":"その高校に合格するためには、勉強の方を頑張ってもう少し成績を上げなくてはならなりませんね。"},{"id":{"d":850,"n":7,"t":2},"speaker":"A","text":"あの演奏を聴いた今なら勉強にも力が入りそうです。"},{"id":{"d":850,"n":8,"t":2},"speaker":"B","text":"協力しますよ。分からないところがあれば、積極的に質問に来てください。"},{"id":{"d":850,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。勉強を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":851,"n":1,"t":2},"speaker":"A","text":"先生、この顕微鏡はよく見えません。"},{"id":{"d":851,"n":2,"t":2},"speaker":"B","text":"ピントが合っていないのではありませんか？"},{"id":{"d":851,"n":3,"t":2},"speaker":"A","text":"ピントを合わせる前に、のぞくところが1つしかないので見にくいです。"},{"id":{"d":851,"n":4,"t":2},"speaker":"B","text":"片目をつぶって片方の目で見るようにできているのですよ。"},{"id":{"d":851,"n":5,"t":2},"speaker":"A","text":"私はウインクが苦手なのです。"},{"id":{"d":851,"n":6,"t":2},"speaker":"B","text":"それで両目で見てしまっているのですね。"},{"id":{"d":851,"n":7,"t":2},"speaker":"A","text":"塾の理科教室の顕微鏡はのぞくところが2つ付いていました。"},{"id":{"d":851,"n":8,"t":2},"speaker":"B","text":"設備が整った塾なのですね。今日、使用する顕微鏡では使わない方の手で片目を隠して使ってみてください。"}];
INSERT INTO sentence [{"id":{"d":852,"n":1,"t":2},"speaker":"A","text":"準備体操を始めます。"},{"id":{"d":852,"n":2,"t":2},"speaker":"B","text":"はい、みんな準備できています。"},{"id":{"d":852,"n":3,"t":2},"speaker":"A","text":"三田さん、体操服が前後ろではありませんか？"},{"id":{"d":852,"n":4,"t":2},"speaker":"B","text":"本当ですね。首が苦しいと思っていました。"},{"id":{"d":852,"n":5,"t":2},"speaker":"A","text":"更衣室に戻って着替え直してきてください。"}];
INSERT INTO sentence [{"id":{"d":853,"n":1,"t":2},"speaker":"A","text":"今年の吹奏楽部に入った一年生にすごい子がいました。"},{"id":{"d":853,"n":2,"t":2},"speaker":"B","text":"演奏が優秀なのですか？"},{"id":{"d":853,"n":3,"t":2},"speaker":"A","text":"演奏はまだ聴いていませんが、流行歌を一度聴いただけで吹奏楽アレンジの楽譜にできてしまうのです。"},{"id":{"d":853,"n":4,"t":2},"speaker":"B","text":"それは素晴らしい子が吹奏楽部に入ってくれましたね。"},{"id":{"d":853,"n":5,"t":2},"speaker":"A","text":"採譜ができる先輩が卒業されて、しばらくは新曲の演奏ができないと思っていたので、そのような人が入ってきてくれてうれしいです。"},{"id":{"d":853,"n":6,"t":2},"speaker":"B","text":"新しい曲の採譜や編曲は難しいですからね。"},{"id":{"d":853,"n":7,"t":2},"speaker":"A","text":"そうなのです。早速、私のお気に入りの曲の採譜を彼に頼んできました。"},{"id":{"d":853,"n":8,"t":2},"speaker":"B","text":"その曲の演奏を聴けることを楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":854,"n":1,"t":2},"speaker":"A","text":"お父さん、お帰りなさい。"},{"id":{"d":854,"n":2,"t":2},"speaker":"B","text":"ただいま帰りました。まだ起きていたのですか？"},{"id":{"d":854,"n":3,"t":2},"speaker":"A","text":"はい。お父さんにこれを見せたかったのです。"},{"id":{"d":854,"n":4,"t":2},"speaker":"B","text":"テストが返されたのですね。"},{"id":{"d":854,"n":5,"t":2},"speaker":"A","text":"はい、そうなのです。"},{"id":{"d":854,"n":6,"t":2},"speaker":"B","text":"全部が花丸ではありませんか？"},{"id":{"d":854,"n":7,"t":2},"speaker":"A","text":"はい。満点を取ったのは初めてです。"},{"id":{"d":854,"n":8,"t":2},"speaker":"B","text":"とても素晴らしいことです。よく頑張りましたね。"}];
INSERT INTO sentence [{"id":{"d":855,"n":1,"t":2},"speaker":"A","text":"黒板に2人目の日直の名前が書いてありませんよ。"},{"id":{"d":855,"n":2,"t":2},"speaker":"B","text":"今日の日直は朝山さんと足立さんです。でも、今日、足立さんはお休みです。"},{"id":{"d":855,"n":3,"t":2},"speaker":"A","text":"そうでしたね。でも、空白のままではいけません。"},{"id":{"d":855,"n":4,"t":2},"speaker":"B","text":"足立さんの名前をそのまま書くのですか？それとも次の担当である、井川さんですか？"},{"id":{"d":855,"n":5,"t":2},"speaker":"A","text":"足立さんのままにしておくと、朝山さんが1人で日直の仕事をしなくてはなりません。"},{"id":{"d":855,"n":6,"t":2},"speaker":"B","text":"では、井川さんの名前を書いておきます。"},{"id":{"d":855,"n":7,"t":2},"speaker":"A","text":"井川さんに今日の日直になったことを伝えるのを忘れずにお願いします。"},{"id":{"d":855,"n":8,"t":2},"speaker":"B","text":"はい、承知しました。"}];
INSERT INTO sentence [{"id":{"d":856,"n":1,"t":2},"speaker":"A","text":"お母さん、「今度の課題はレポート用紙に手書きで書くように」と言われました。"},{"id":{"d":856,"n":2,"t":2},"speaker":"B","text":"今どきは、レポートは印刷ではありませんか？"},{"id":{"d":856,"n":3,"t":2},"speaker":"A","text":"教授が手書きのレポートが好きなのです。"},{"id":{"d":856,"n":4,"t":2},"speaker":"B","text":"何かの文章を写すにしても、手書きならそれだけで勉強になると思われたのでしょうね。"},{"id":{"d":856,"n":5,"t":2},"speaker":"A","text":"レポート用紙という紙もよく分からないので、今度一緒に買いに行ってくれませんか？"},{"id":{"d":856,"n":6,"t":2},"speaker":"B","text":"よいですよ。締め切りがあるので早い方がいいですよね。明日にでも行きましょう。"},{"id":{"d":856,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":857,"n":1,"t":2},"speaker":"A","text":"そのクリップは変わった形をしていますね。"},{"id":{"d":857,"n":2,"t":2},"speaker":"B","text":"かわいいでしょう？猫の形をしているのです。"},{"id":{"d":857,"n":3,"t":2},"speaker":"A","text":"本当ですね。かわいいです。"},{"id":{"d":857,"n":4,"t":2},"speaker":"B","text":"先日、文房具屋さんに行ってレポート用紙を買ったとき、このクリップに目を奪われてしまいました。"},{"id":{"d":857,"n":5,"t":2},"speaker":"A","text":"それで思わず買ってきてしまったのですね？"},{"id":{"d":857,"n":6,"t":2},"speaker":"B","text":"はい、猫が大好きなので。ペンギンなど別の動物のクリップもありましたよ。"},{"id":{"d":857,"n":7,"t":2},"speaker":"A","text":"そのクリップでレポートを提出するのですか？"},{"id":{"d":857,"n":8,"t":2},"speaker":"B","text":"いいえ。提出時に止めるものはホチキスです。かわいいものはレポート用紙に付けて出してはもったいないと思います。"}];
INSERT INTO sentence [{"id":{"d":858,"n":1,"t":2},"speaker":"A","text":"おはようございます。"},{"id":{"d":858,"n":2,"t":2},"speaker":"B","text":"おはようございます。すみません、急いでいるので今日は先に行きますね。"},{"id":{"d":858,"n":3,"t":2},"speaker":"A","text":"まだ1限が始まるまでには時間がありますよ。"},{"id":{"d":858,"n":4,"t":2},"speaker":"B","text":"図書館に寄って本を返さないといけません。"},{"id":{"d":858,"n":5,"t":2},"speaker":"A","text":"それくらいなら、それほど急がなくても大丈夫ですよ。"},{"id":{"d":858,"n":6,"t":2},"speaker":"B","text":"朝はカウンターの前が行列なのですよ。行列の待ち時間も含めると結構瀬戸際の時間です。"},{"id":{"d":858,"n":7,"t":2},"speaker":"A","text":"そういうことでしたか。では先に大学へ行っていますね。頑張ってください。"},{"id":{"d":858,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。また後でお会いしましょう。"}];
INSERT INTO sentence [{"id":{"d":859,"n":1,"t":2},"speaker":"A","text":"今日の社会科見学はパン工場です。ずっと楽しみにしていたのでうれしいです。"},{"id":{"d":859,"n":2,"t":2},"speaker":"B","text":"私もです。工場に入った瞬間からとても良い香りがしますね。"},{"id":{"d":859,"n":3,"t":2},"speaker":"A","text":"大好きな香りです。私はパン屋さんになりたいので、今日はしっかりとパンの作り方を勉強したいです。"},{"id":{"d":859,"n":4,"t":2},"speaker":"B","text":"素晴らしいですね。普段からも自分でパンを作ったりしているのですか？"},{"id":{"d":859,"n":5,"t":2},"speaker":"A","text":"生地をホームベーカリーで作って焼いたことはありますが、自分の手で生地をこねることはまだありません。"},{"id":{"d":859,"n":6,"t":2},"speaker":"B","text":"この工場でも、生地は機械で作っているようですよ。"},{"id":{"d":859,"n":7,"t":2},"speaker":"A","text":"そのようですね。生地を作るところが見えないのは少し残念です。"},{"id":{"d":859,"n":8,"t":2},"speaker":"B","text":"今度の職業体験では大通りのベーカリーを希望するといいかもしれませんね。"},{"id":{"d":859,"n":9,"t":2},"speaker":"A","text":"確かに、ベーカリーでは生地をこねるところから体験できそうです。"}];
INSERT INTO sentence [{"id":{"d":860,"n":1,"t":2},"speaker":"A","text":"大きな荷物を持っていますね。"},{"id":{"d":860,"n":2,"t":2},"speaker":"B","text":"これはギターです。"},{"id":{"d":860,"n":3,"t":2},"speaker":"A","text":"ギターが弾けるとは知りませんでした。"},{"id":{"d":860,"n":4,"t":2},"speaker":"B","text":"高校の部活は軽音楽部に入ろうと思って、春休みに練習していたのです。"},{"id":{"d":860,"n":5,"t":2},"speaker":"A","text":"素晴しいことです。練習の成果はどうですか？"},{"id":{"d":860,"n":6,"t":2},"speaker":"B","text":"簡単なコードなら弾けるようになりましたよ。新学期が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":861,"n":1,"t":2},"speaker":"A","text":"あなたは1年生の加藤さんでしたね？おうちに帰りませんか？"},{"id":{"d":861,"n":2,"t":2},"speaker":"B","text":"帰ろうとしたら雨が降ってきてしまったので、やむのを待っています。"},{"id":{"d":861,"n":3,"t":2},"speaker":"A","text":"なぜやむのを待つのですか？傘は手に持っていますよね？"},{"id":{"d":861,"n":4,"t":2},"speaker":"B","text":"傘を差してもランドセルがぬれてしまうのです。"},{"id":{"d":861,"n":5,"t":2},"speaker":"A","text":"確かにランドセルも傘の中に入れるのは難しいかもしれませんが、少しくらいぬれてもランドセルの中身は大丈夫ですよ。"},{"id":{"d":861,"n":6,"t":2},"speaker":"B","text":"おばあさまに買っていただいたばかりの大切なランドセルをぬらしたくありません。"},{"id":{"d":861,"n":7,"t":2},"speaker":"A","text":"そのようなことでしたか。とても大切にしているのですね。ではこのビニール袋でランドセルがぬれないようカバーをしてあげましょう。"},{"id":{"d":861,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。それなら傘を差して帰れます。"}];
INSERT INTO sentence [{"id":{"d":862,"n":1,"t":2},"speaker":"A","text":"このままでは明日の自由研究発表会の際に提出する資料が間に合いそうにありません。お母さん、手伝ってくれませんか？"},{"id":{"d":862,"n":2,"t":2},"speaker":"B","text":"仕方ありませんね、どうしたら良いか言ってください。"},{"id":{"d":862,"n":3,"t":2},"speaker":"A","text":"この表をグラフにしてください。"},{"id":{"d":862,"n":4,"t":2},"speaker":"B","text":"お母さんは表計算ソフトなどは使えませんよ。"},{"id":{"d":862,"n":5,"t":2},"speaker":"A","text":"では、この下書きした文章をここに打ち込んでください。"},{"id":{"d":862,"n":6,"t":2},"speaker":"B","text":"それも無理です。パソコンでする作業はさっぱりできません。"},{"id":{"d":862,"n":7,"t":2},"speaker":"A","text":"では、何ができるのですか？"},{"id":{"d":862,"n":8,"t":2},"speaker":"B","text":"模造紙にペンで絵や字を書くのはできます。パソコンで資料を作ることなどやめてしまいなさい。"},{"id":{"d":862,"n":9,"t":2},"speaker":"A","text":"電子データで提出する必要があるので、それでは駄目なのですよ。困りました。"}];
INSERT INTO sentence [{"id":{"d":863,"n":1,"t":2},"speaker":"A","text":"佐藤君は野球部なのに長髪なのですね。"},{"id":{"d":863,"n":2,"t":2},"speaker":"B","text":"珍しいでしょう？よく言われます。でも実は野球部に長髪禁止などという規則はありませんよ。"},{"id":{"d":863,"n":3,"t":2},"speaker":"A","text":"そうなのですか？佐藤君以外はみんな坊主にしているから規則とばかり思っていました。"},{"id":{"d":863,"n":4,"t":2},"speaker":"B","text":"そもそも野球部特有の規則自体がありませんよ。なぜか、みんなが自主的に坊主頭にしているだけなのです。"},{"id":{"d":863,"n":5,"t":2},"speaker":"A","text":"それほどに坊主は良いものなのでしょうか？"},{"id":{"d":863,"n":6,"t":2},"speaker":"B","text":"僕も日々不思議に思っています。顧問の先生も「なぜ、それほど坊主にしたがるのか」と首を傾げていますよ。"},{"id":{"d":863,"n":7,"t":2},"speaker":"A","text":"他の部員に聞いてみたことはありませんか？"},{"id":{"d":863,"n":8,"t":2},"speaker":"B","text":"そういえばありませんね。今日の部活で聞いてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":864,"n":1,"t":2},"speaker":"A","text":"おかえりなさい。体操教室の初回はどうでしたか？"},{"id":{"d":864,"n":2,"t":2},"speaker":"B","text":"ただいま帰りました。今日はラジオ体操とでんぐり返しだけやってきました。"},{"id":{"d":864,"n":3,"t":2},"speaker":"A","text":"逆上がりや跳び箱の練習はまだ先なのですね。"},{"id":{"d":864,"n":4,"t":2},"speaker":"B","text":"逆上がりの練習は来週から始まります。最初の1カ月は逆上がりができるようになることが目標だそうです。"},{"id":{"d":864,"n":5,"t":2},"speaker":"A","text":"学校の逆上がりのテストはいつなのですか？"},{"id":{"d":864,"n":6,"t":2},"speaker":"B","text":"6月くらいだと思います。"},{"id":{"d":864,"n":7,"t":2},"speaker":"A","text":"それまでにできるようになると良いですね。"},{"id":{"d":864,"n":8,"t":2},"speaker":"B","text":"はい。とても優しい先生だったので来週からも頑張れると思います。"}];
INSERT INTO sentence [{"id":{"d":865,"n":1,"t":2},"speaker":"A","text":"先生、見てください。模造紙を使って学級新聞を制作しました。"},{"id":{"d":865,"n":2,"t":2},"speaker":"B","text":"素晴らしいですね。一番の目玉記事は何にしたのですか？"},{"id":{"d":865,"n":3,"t":2},"speaker":"A","text":"はい。先週の水曜日に、給食でカニが出たことです。"},{"id":{"d":865,"n":4,"t":2},"speaker":"B","text":"近隣の漁師のおじさんからご厚意で頂いたカニのことですね。"},{"id":{"d":865,"n":5,"t":2},"speaker":"A","text":"はい。それがとてもおいしかったので記事にしました。"},{"id":{"d":865,"n":6,"t":2},"speaker":"B","text":"カニの絵も上手に描けていますね。"},{"id":{"d":865,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。この模造紙をどこかに貼っても良いでしょうか？"},{"id":{"d":865,"n":8,"t":2},"speaker":"B","text":"もちろんです。教室の後ろの掲示板に貼りましょう。"}];
INSERT INTO sentence [{"id":{"d":866,"n":1,"t":2},"speaker":"A","text":"おはようございます。今朝は早いですね。テスト勉強は完璧に仕上げましたか？"},{"id":{"d":866,"n":2,"t":2},"speaker":"B","text":"おはようございます。徹夜で試験勉強をしようと思っていたのに寝てしまいました。"},{"id":{"d":866,"n":3,"t":2},"speaker":"A","text":"あらまあ。でも、早起きはできたのですね。テストに遅刻しなくて良かったです。"},{"id":{"d":866,"n":4,"t":2},"speaker":"B","text":"机に伏せたまま寝ていたので、体が痛くて4時に起きました。"},{"id":{"d":866,"n":5,"t":2},"speaker":"A","text":"それでこれほど早く登校したのですか？"},{"id":{"d":866,"n":6,"t":2},"speaker":"B","text":"はい。せっかく早く来たので今から勉強しようと思います。山田さんはいつもこの時間ですか？"},{"id":{"d":866,"n":7,"t":2},"speaker":"A","text":"そうです。私は元々朝の方が集中できるので、早く登校して教室で勉強しています。"},{"id":{"d":866,"n":8,"t":2},"speaker":"B","text":"僕も、もしかしたら朝の方が、勉強がはかどるような気がしてきました。"},{"id":{"d":866,"n":9,"t":2},"speaker":"A","text":"早寝早起きで勉強する方がいいかもしれませんね。"},{"id":{"d":866,"n":10,"t":2},"speaker":"B","text":"次のテスト勉強からはそうしようと思います。"}];
INSERT INTO sentence [{"id":{"d":867,"n":1,"t":2},"speaker":"A","text":"第2外国語は何を履修しますか？"},{"id":{"d":867,"n":2,"t":2},"speaker":"B","text":"私はスペイン語を選択しました。"},{"id":{"d":867,"n":3,"t":2},"speaker":"A","text":"スペインに興味があるのですか？"},{"id":{"d":867,"n":4,"t":2},"speaker":"B","text":"そのようなわけではありませんが、スペイン語は中国語に次いで世界中で使っている人が多い言語と聞き、興味が湧いたので、選択してみました。"},{"id":{"d":867,"n":5,"t":2},"speaker":"A","text":"そう言われればそうですね。南米はほとんどがスペイン語ですし。"},{"id":{"d":867,"n":6,"t":2},"speaker":"B","text":"そうみたいですね。使う文字も英語とほとんど同じで覚えやすそうです。あなたは何を履修しますか？"},{"id":{"d":867,"n":7,"t":2},"speaker":"A","text":"私は中国語を履修します。"},{"id":{"d":867,"n":8,"t":2},"speaker":"B","text":"中国語は発音が難しいと聞いたことがあります。"},{"id":{"d":867,"n":9,"t":2},"speaker":"A","text":"はい。その複雑な発音を学んでみたいと思い、中国語を選びました。講義が始まることが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":868,"n":1,"t":2},"speaker":"A","text":"瀬名さんは英語がお得意でしたよね？"},{"id":{"d":868,"n":2,"t":2},"speaker":"B","text":"はい。日常会話でしたら困りません。"},{"id":{"d":868,"n":3,"t":2},"speaker":"A","text":"知り合いの塾で子どもたちの英語の先生を探しているのですが、アルバイトをしませんか？"},{"id":{"d":868,"n":4,"t":2},"speaker":"B","text":"教員免許は持っていませんが大丈夫でしょうか？"},{"id":{"d":868,"n":5,"t":2},"speaker":"A","text":"アルバイトですから、教員免許は要りません。子どもは好きですか？"},{"id":{"d":868,"n":6,"t":2},"speaker":"B","text":"子どもは大好きです。"},{"id":{"d":868,"n":7,"t":2},"speaker":"A","text":"では、それが資格です。アルバイトをしてみませんか？"},{"id":{"d":868,"n":8,"t":2},"speaker":"B","text":"前向きに考えてみます。詳しいお話を聞かせてください。"}];
INSERT INTO sentence [{"id":{"d":869,"n":1,"t":2},"speaker":"A","text":"3組の教室にすてきな作品が飾ってありました。"},{"id":{"d":869,"n":2,"t":2},"speaker":"B","text":"私も見ました。段ボールで作った町の模型でしょう？"},{"id":{"d":869,"n":3,"t":2},"speaker":"A","text":"そうです。小学校を中心に、この町の建物の模型がたくさん並んでいました。"},{"id":{"d":869,"n":4,"t":2},"speaker":"B","text":"商店街の看板までそっくりでしたよね。"},{"id":{"d":869,"n":5,"t":2},"speaker":"A","text":"素晴しい観察力だと思いました。"},{"id":{"d":869,"n":6,"t":2},"speaker":"B","text":"あと少し模型のモデルとする町の範囲が広かったら、私の家も入っていたのに残念です。"},{"id":{"d":869,"n":7,"t":2},"speaker":"A","text":"私の家のマンションは町の模型の片隅に入っていましたよ。"},{"id":{"d":869,"n":8,"t":2},"speaker":"B","text":"うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":870,"n":1,"t":2},"speaker":"A","text":"5年生は版画の授業があるので、彫刻セットが必要です。購入してくださいね。"},{"id":{"d":870,"n":2,"t":2},"speaker":"B","text":"先生、家にある彫刻刀では駄目なのですか？"},{"id":{"d":870,"n":3,"t":2},"speaker":"A","text":"はい。彫刻セットは彫刻刀だけでなく、版画に必要なものが全てセットになっているのです。"},{"id":{"d":870,"n":4,"t":2},"speaker":"B","text":"姉の使っていた彫刻セットがあるので、他の道具もあると思います。"},{"id":{"d":870,"n":5,"t":2},"speaker":"A","text":"でも、版画を掘る板や紙やインクはお姉さんが使ってしまったでしょう？"},{"id":{"d":870,"n":6,"t":2},"speaker":"B","text":"そうだと思います。"},{"id":{"d":870,"n":7,"t":2},"speaker":"A","text":"それではやはりセットで買っていただくように、ご両親にお話ししてくださいね。"},{"id":{"d":870,"n":8,"t":2},"speaker":"B","text":"分かりました。母に伝えます。"}];
INSERT INTO sentence [{"id":{"d":871,"n":1,"t":2},"speaker":"A","text":"良い万年筆をお持ちですね。"},{"id":{"d":871,"n":2,"t":2},"speaker":"B","text":"大学に入ったときに祖父がくれました。"},{"id":{"d":871,"n":3,"t":2},"speaker":"A","text":"年季が入っているようですが、おじいさんのお古なのですか？"},{"id":{"d":871,"n":4,"t":2},"speaker":"B","text":"はい。「若いときに頑張って働いて高級品を買った」と言っていました。"},{"id":{"d":871,"n":5,"t":2},"speaker":"A","text":"すてきですね。おじいさんは万年筆を使うお仕事をされていたのですか？"},{"id":{"d":871,"n":6,"t":2},"speaker":"B","text":"仕事は新聞記者でしたが、万年筆は趣味の文章を書くのに使っていたそうです。"},{"id":{"d":871,"n":7,"t":2},"speaker":"A","text":"もう今は書かれていませんか？"},{"id":{"d":871,"n":8,"t":2},"speaker":"B","text":"最近はパソコンを覚えて執筆しています。"}];
INSERT INTO sentence [{"id":{"d":872,"n":1,"t":2},"speaker":"A","text":"私たちの学校のクイズ部が、高校生クイズへの出場を決めたらしいですよ。"},{"id":{"d":872,"n":2,"t":2},"speaker":"B","text":"高校生クイズとはテレビ番組のですか？"},{"id":{"d":872,"n":3,"t":2},"speaker":"A","text":"そうです。全国の優秀な学校から代表が出てくるあの番組です。"},{"id":{"d":872,"n":4,"t":2},"speaker":"B","text":"私たちの学校のクイズ部は、代表の中に入れるほど優秀でしたか？"},{"id":{"d":872,"n":5,"t":2},"speaker":"A","text":"はい。出場したということはきっと強いのでしょう。"},{"id":{"d":872,"n":6,"t":2},"speaker":"B","text":"誇らしいですね。本選はいつですか？"},{"id":{"d":872,"n":7,"t":2},"speaker":"A","text":"来月だそうです。テレビでの放送はもう少し先になるそうです。"}];
INSERT INTO sentence [{"id":{"d":873,"n":1,"t":2},"speaker":"A","text":"この間『赤毛のアン』の映画を見ました。"},{"id":{"d":873,"n":2,"t":2},"speaker":"B","text":"そういえば、先週テレビでも放送されていたようですね。"},{"id":{"d":873,"n":3,"t":2},"speaker":"A","text":"そうです、それを見たのです。"},{"id":{"d":873,"n":4,"t":2},"speaker":"B","text":"面白かったですか？"},{"id":{"d":873,"n":5,"t":2},"speaker":"A","text":"はい。初めて見たのですがとても面白かったです。"},{"id":{"d":873,"n":6,"t":2},"speaker":"B","text":"特に印象に残ったシーンはありますか？"},{"id":{"d":873,"n":7,"t":2},"speaker":"A","text":"グリーン・ゲイブルズの朝のシーンです。映像もとても美しくて心が震えました。"},{"id":{"d":873,"n":8,"t":2},"speaker":"B","text":"そこは原作でも感動的なシーンの一つです。あなたの話を聞いて、私もそのシーンを映像で見てみたいと思いました。"},{"id":{"d":873,"n":9,"t":2},"speaker":"A","text":"本当に素晴らしかったので、ぜひ映画を見てみてください。"}];
INSERT INTO sentence [{"id":{"d":874,"n":1,"t":2},"speaker":"A","text":"卒業の記念品に先生からブックカバーを頂きました。"},{"id":{"d":874,"n":2,"t":2},"speaker":"B","text":"すてきな革のカバーですね。"},{"id":{"d":874,"n":3,"t":2},"speaker":"A","text":"緒方先生はレザークラフトが趣味なのだそうです。"},{"id":{"d":874,"n":4,"t":2},"speaker":"B","text":"これは先生の手作りですか？驚きました。"},{"id":{"d":874,"n":5,"t":2},"speaker":"A","text":"僕もです。しかも、クラスの子全員に、一人一人に合った模様を付けて作ってくださったのですよ。"},{"id":{"d":874,"n":6,"t":2},"speaker":"B","text":"なんと。あなたのカバーの模様はサッカーボールですね。"},{"id":{"d":874,"n":7,"t":2},"speaker":"A","text":"そうです。僕がサッカー部だからです。"},{"id":{"d":874,"n":8,"t":2},"speaker":"B","text":"とてもすてきなプレゼントですね。"},{"id":{"d":874,"n":9,"t":2},"speaker":"A","text":"はい、クラス全員が感激していました。大事に使いたいと思います。"}];
INSERT INTO sentence [{"id":{"d":875,"n":1,"t":2},"speaker":"A","text":"これから避難訓練を行います。非常ベルが鳴ったら、事前に説明した通り落ち着いて行動してください。"},{"id":{"d":875,"n":2,"t":2},"speaker":"B","text":"先生、ベルが鳴り始めました。"},{"id":{"d":875,"n":3,"t":2},"speaker":"A","text":"そうですね、今回は地震を想定した訓練です。まずは、防災頭巾を着けて机の下に入ってください。"},{"id":{"d":875,"n":4,"t":2},"speaker":"B","text":"はい、分かりました。"},{"id":{"d":875,"n":5,"t":2},"speaker":"A","text":"揺れが収まったので、前の席の人から慌てずゆっくり廊下に出てください。みんなで校庭まで移動をします。"},{"id":{"d":875,"n":6,"t":2},"speaker":"B","text":"先生、荷物は持たないのですか？"},{"id":{"d":875,"n":7,"t":2},"speaker":"A","text":"はい。避難するときは基本的には荷物を持ってはいけません。"},{"id":{"d":875,"n":8,"t":2},"speaker":"B","text":"ハンカチと水筒も持って行っては駄目ですか？"},{"id":{"d":875,"n":9,"t":2},"speaker":"A","text":"その2つなら問題ありません。それらを持って速やかに移動を開始してください。"}];
INSERT INTO sentence [{"id":{"d":876,"n":1,"t":2},"speaker":"A","text":"ママ、学校のバザーに出せるものは何かありませんか？"},{"id":{"d":876,"n":2,"t":2},"speaker":"B","text":"どのようなものを用意すればよいのですか？"},{"id":{"d":876,"n":3,"t":2},"speaker":"A","text":"「使わなくなったものや要らないもの」と先生に言われました。"},{"id":{"d":876,"n":4,"t":2},"speaker":"B","text":"それならたくさんありますよ。でも、重たいものが多いです。"},{"id":{"d":876,"n":5,"t":2},"speaker":"A","text":"何があるのですか？"},{"id":{"d":876,"n":6,"t":2},"speaker":"B","text":"食器セットや圧力鍋やバーベキューセットなどがあります。"},{"id":{"d":876,"n":7,"t":2},"speaker":"A","text":"全部重たそうですね。私が持って行けるでしょうか？"},{"id":{"d":876,"n":8,"t":2},"speaker":"B","text":"あなた一人では少し大変かもしれませんね。当日は、私も一緒に運びますよ。"},{"id":{"d":876,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":877,"n":1,"t":2},"speaker":"A","text":"すっかり歩き疲れてしましました。休憩はまだでしょうか？"},{"id":{"d":877,"n":2,"t":2},"speaker":"B","text":"そろそろ目的地の公園に着くはずですよ。ほら、列の先頭が公園に入っていきました。"},{"id":{"d":877,"n":3,"t":2},"speaker":"A","text":"良かったです。待ちに待った休憩です。"},{"id":{"d":877,"n":4,"t":2},"speaker":"B","text":"またしばらく歩くことになるので、ここでしっかり水分補給をしましょう。"},{"id":{"d":877,"n":5,"t":2},"speaker":"A","text":"そうですね。水筒のお茶が甘くておいしいです。"},{"id":{"d":877,"n":6,"t":2},"speaker":"B","text":"お茶が甘いのですか？"},{"id":{"d":877,"n":7,"t":2},"speaker":"A","text":"はい。母がお砂糖入りの紅茶を入れてくれたのです。遠足の時はいつもこれです。"}];
INSERT INTO sentence [{"id":{"d":878,"n":1,"t":2},"speaker":"A","text":"校門のところに救急車が来ていますね。"},{"id":{"d":878,"n":2,"t":2},"speaker":"B","text":"体育の授業のとき、熱中症で倒れた子がいるらしいですよ。"},{"id":{"d":878,"n":3,"t":2},"speaker":"A","text":"もう涼しくなってきたのに熱中症ですか？"},{"id":{"d":878,"n":4,"t":2},"speaker":"B","text":"はい。今日は湿度が高いですからね。その湿度の中で長距離走をしたら倒れてしまったと聞きました。"},{"id":{"d":878,"n":5,"t":2},"speaker":"A","text":"倒れたのは私たちと同じ学年の子なのでしょうか？"},{"id":{"d":878,"n":6,"t":2},"speaker":"B","text":"きっとそうでしょう。先ほどの時間は、1組の進学コースの子たちが体育の授業を受けていたはずです。"},{"id":{"d":878,"n":7,"t":2},"speaker":"A","text":"心配ですね。重症でなければ良いのですが。"}];
INSERT INTO sentence [{"id":{"d":879,"n":1,"t":2},"speaker":"A","text":"小宮さんは、もう帰るのですか？"},{"id":{"d":879,"n":2,"t":2},"speaker":"B","text":"いえ、今日はまだ4限があるので帰りません。"},{"id":{"d":879,"n":3,"t":2},"speaker":"A","text":"今、1限が終わったところですよね？次の講義は4限なのですか？"},{"id":{"d":879,"n":4,"t":2},"speaker":"B","text":"そうです。2、3限は丸々空いています。"},{"id":{"d":879,"n":5,"t":2},"speaker":"A","text":"これから4限まで何をするのですか？"},{"id":{"d":879,"n":6,"t":2},"speaker":"B","text":"図書館に行ってレポートを作成する予定です。"},{"id":{"d":879,"n":7,"t":2},"speaker":"A","text":"勉強がはかどりそうですね。"},{"id":{"d":879,"n":8,"t":2},"speaker":"B","text":"はい。今日のようなスケジュールだと空きコマに大学内で課題を進められるので、家ではのんびりできます。"}];
INSERT INTO sentence [{"id":{"d":880,"n":1,"t":2},"speaker":"A","text":"今日はYシャツ1枚では少し寒いですね。"},{"id":{"d":880,"n":2,"t":2},"speaker":"B","text":"そうですね。私はカーディガンを着てきました。"},{"id":{"d":880,"n":3,"t":2},"speaker":"A","text":"用意が良いですね。家を出るときはそれほど寒く感じなかったので、私は家に上着を置いてきてしまいました。"},{"id":{"d":880,"n":4,"t":2},"speaker":"B","text":"今日は体育の授業がありましたよね？その上にジャージを着たら良いのではありませんか？"},{"id":{"d":880,"n":5,"t":2},"speaker":"A","text":"はい。しかし、制服の上にジャージを重ね着すると、風紀委員に怒られませんか？"},{"id":{"d":880,"n":6,"t":2},"speaker":"B","text":"風邪をひくよりは良いですよ、きっと。"}];
INSERT INTO sentence [{"id":{"d":881,"n":1,"t":2},"speaker":"A","text":"桜がきれいに咲いている時期に行われた卒業式でした。"},{"id":{"d":881,"n":2,"t":2},"speaker":"B","text":"最近は桜の開花が早いですよね？"},{"id":{"d":881,"n":3,"t":2},"speaker":"A","text":"そう言われれば、私の子どもの頃は入学式が桜満開だったような気がします。"},{"id":{"d":881,"n":4,"t":2},"speaker":"B","text":"私の子どもの頃もそうでした。"},{"id":{"d":881,"n":5,"t":2},"speaker":"A","text":"このことは授業で言った方がいいでしょうか？"},{"id":{"d":881,"n":6,"t":2},"speaker":"B","text":"はい。温暖化についての授業ですか？"},{"id":{"d":881,"n":7,"t":2},"speaker":"A","text":"そうです。"},{"id":{"d":881,"n":8,"t":2},"speaker":"B","text":"身近な例としては良いのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":882,"n":1,"t":2},"speaker":"A","text":"社会のテストを返します。相川さん、急に点数がよくなりましたけれど、どうしたのですか？"},{"id":{"d":882,"n":2,"t":2},"speaker":"B","text":"今回のテスト範囲は、得意中の得意なのです。"},{"id":{"d":882,"n":3,"t":2},"speaker":"A","text":"相川さんは、戦国時代に詳しいのですか？"},{"id":{"d":882,"n":4,"t":2},"speaker":"B","text":"はい、そうです。私は戦国時代が大好きで、ドラマや小説、漫画などを楽しむうちに詳しくなりました。"},{"id":{"d":882,"n":5,"t":2},"speaker":"A","text":"何をきっかけに、戦国時代を好きになったのですか？"},{"id":{"d":882,"n":6,"t":2},"speaker":"B","text":"最初は、戦国時代を題材にした大河ドラマに夢中になったのです。それをきっかけに、同じ時代を題材にした小説や漫画も読むようになり、さらに戦国時代の出来事や人物に夢中になっていきました。最近はゲームにも手を出し始めました。"},{"id":{"d":882,"n":7,"t":2},"speaker":"A","text":"ゲームもあるのですか？"},{"id":{"d":882,"n":8,"t":2},"speaker":"B","text":"たくさんありますよ。自分が武将になったつもりで戦をすることが楽しいです。"}];
INSERT INTO sentence [{"id":{"d":883,"n":1,"t":2},"speaker":"A","text":"明日は文化祭です。この教室は休憩所として使われますので、私物は持ち帰るようにしてくださいね。"},{"id":{"d":883,"n":2,"t":2},"speaker":"B","text":"先生、今日休んでいる川辺さんの私物はどうしたら良いでしょう？"},{"id":{"d":883,"n":3,"t":2},"speaker":"A","text":"それは私が職員室で預かります。この袋に川辺さんの私物をまとめてもらっても良いでしょうか？"},{"id":{"d":883,"n":4,"t":2},"speaker":"B","text":"はい。しかし、先生、さらに大きな袋はありませんか？"},{"id":{"d":883,"n":5,"t":2},"speaker":"A","text":"それほどにたくさん荷物があるのですか？"},{"id":{"d":883,"n":6,"t":2},"speaker":"B","text":"机の中が荷物で満杯ですし、それ以外にロッカーにも物がいっぱいです。"},{"id":{"d":883,"n":7,"t":2},"speaker":"A","text":"分かりました。では、用務員室に行って段ボール箱をもらってきますので少し待っていてください。"},{"id":{"d":883,"n":8,"t":2},"speaker":"B","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":884,"n":1,"t":2},"speaker":"A","text":"今日は髪を結んでいかなくて良いのですか？"},{"id":{"d":884,"n":2,"t":2},"speaker":"B","text":"はい。4月から校則が変わり、このままでも良いことになりました。"},{"id":{"d":884,"n":3,"t":2},"speaker":"A","text":"髪型が自由になったのですか？"},{"id":{"d":884,"n":4,"t":2},"speaker":"B","text":"いいえ。自由ではありませんけれど、このくらいの長さなら、前髪をピンで留めればよくなりました。"},{"id":{"d":884,"n":5,"t":2},"speaker":"A","text":"校則が変わることなど本当にあるのですね。驚きました。"},{"id":{"d":884,"n":6,"t":2},"speaker":"B","text":"昨年度の生徒会のメンバーが本当に頑張ってくれたのです。髪型に関する規則は私も不要と感じていたので、先輩方には本当に感謝しています。"}];
INSERT INTO sentence [{"id":{"d":885,"n":1,"t":2},"speaker":"A","text":"平田君、遅刻ですよ。"},{"id":{"d":885,"n":2,"t":2},"speaker":"B","text":"すみません。通学途中で自転車のタイヤがパンクしてしまいました。"},{"id":{"d":885,"n":3,"t":2},"speaker":"A","text":"それは大変でしたね。自転車を引いて学校まで来たのですか？"},{"id":{"d":885,"n":4,"t":2},"speaker":"B","text":"いえ。途中に自転車屋さんがありましたから、自転車は預けてきました。"},{"id":{"d":885,"n":5,"t":2},"speaker":"A","text":"それなら良かったです。通学路に自転車屋さんなどがありましたね。"},{"id":{"d":885,"n":6,"t":2},"speaker":"B","text":"裏門から真っ直ぐ行った交差点のすぐ近くにあります。「昔から同じような学生が自転車を預けていくよ」と自転車屋さんのおじさんが言っていました。"},{"id":{"d":885,"n":7,"t":2},"speaker":"A","text":"そうだったのですね。知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":886,"n":1,"t":2},"speaker":"A","text":"ふみちゃん、漢字の書き取りの宿題は終わったのですか？"},{"id":{"d":886,"n":2,"t":2},"speaker":"B","text":"いえ、まだ半分までしかできていません。"},{"id":{"d":886,"n":3,"t":2},"speaker":"A","text":"もう少しですね。終わったらおやつにしましょう。"},{"id":{"d":886,"n":4,"t":2},"speaker":"B","text":"楽しみです。急いで終わらせます。"},{"id":{"d":886,"n":5,"t":2},"speaker":"A","text":"少し待ってください。ノートに漢字の偏だけ書いてあるのはどうしてですか？"},{"id":{"d":886,"n":6,"t":2},"speaker":"B","text":"同じところはまとめて書いた方が早いと思うからです。"},{"id":{"d":886,"n":7,"t":2},"speaker":"A","text":"偏だけ書いても漢字を覚えられないでしょう？"},{"id":{"d":886,"n":8,"t":2},"speaker":"B","text":"そうですね。しかし、この宿題は漢字を覚えるために行っているものなのですか？知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":887,"n":1,"t":2},"speaker":"A","text":"最近、母から大きなプレッシャーを掛けられています。"},{"id":{"d":887,"n":2,"t":2},"speaker":"B","text":"受験についてですか？"},{"id":{"d":887,"n":3,"t":2},"speaker":"A","text":"そうです。毎日、夜食を持ってきてくれるのですが、全てに「合格」「必勝」と書いてあるのです。"},{"id":{"d":887,"n":4,"t":2},"speaker":"B","text":"それはなかなか圧がありますね。"},{"id":{"d":887,"n":5,"t":2},"speaker":"A","text":"昨日はどら焼きに「必勝」と焼き印が押してありましたし、机の上に常備してくれているチョコレートとあめにも「合格」とありました。"},{"id":{"d":887,"n":6,"t":2},"speaker":"B","text":"そこまで応援されてしまうと、絶対に受験に合格しないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":888,"n":1,"t":2},"speaker":"A","text":"今日は保健室に学生がぞろぞろやってきます。何が行われていたのでしょうか？"},{"id":{"d":888,"n":2,"t":2},"speaker":"B","text":"文化祭の立て看板を作っている学生が多いのですよ。"},{"id":{"d":888,"n":3,"t":2},"speaker":"A","text":"立て看板作りで、けが人が出ているのですか？"},{"id":{"d":888,"n":4,"t":2},"speaker":"B","text":"はい。金づちで釘を打った経験がある学生はほとんどいないので、慣れない作業でついけがをしてしまう学生が多いようです。"},{"id":{"d":888,"n":5,"t":2},"speaker":"A","text":"なるほど。手をけがした学生が多いのは、金づちで手などを打ったからなのですね。"}];
INSERT INTO sentence [{"id":{"d":889,"n":1,"t":2},"speaker":"A","text":"道夫さん、学校から借りているタブレットはどうしたのですか？"},{"id":{"d":889,"n":2,"t":2},"speaker":"B","text":"教室に忘れてきてしまいました。"},{"id":{"d":889,"n":3,"t":2},"speaker":"A","text":"「必ず家に持ち帰るように」と言われていたのに忘れてしまったのですか？"},{"id":{"d":889,"n":4,"t":2},"speaker":"B","text":"はい、そうなのです。ごめんなさい。"},{"id":{"d":889,"n":5,"t":2},"speaker":"A","text":"まだ早い時間ですから、学校へ取りに戻ったらどうでしょうか？"},{"id":{"d":889,"n":6,"t":2},"speaker":"B","text":"明日まで学校に置いておいたらいけませんか？"},{"id":{"d":889,"n":7,"t":2},"speaker":"A","text":"はい。もしタブレットが盗まれたり、壊れたりしたら、借りている人が悪いことになってしまうのですよ。それに明日のために、家で充電もしていかないといけないのでしょう。"},{"id":{"d":889,"n":8,"t":2},"speaker":"B","text":"分かりました。今から学校に取りに戻ります。"}];
INSERT INTO sentence [{"id":{"d":890,"n":1,"t":2},"speaker":"A","text":"和子さん、土曜日に一緒にランチに行きませんか？"},{"id":{"d":890,"n":2,"t":2},"speaker":"B","text":"ごめんなさい。このところ、土曜日は仕事関連の調べ物で母校の図書館に通っているのです。"},{"id":{"d":890,"n":3,"t":2},"speaker":"A","text":"町の公立図書館ではなく大学の図書館で調べ物ですか？"},{"id":{"d":890,"n":4,"t":2},"speaker":"B","text":"ええ、今取り組んでいる仕事は、町の公立図書館よりも大学の図書館に資料が豊富に所蔵されているのですよ。"},{"id":{"d":890,"n":5,"t":2},"speaker":"A","text":"そうでしたか。それでは、残念ですが土曜日にランチへ行くのは難しいですね。"},{"id":{"d":890,"n":6,"t":2},"speaker":"B","text":"緑さんが大学まで来てくれれば、学生食堂のランチをご一緒できますよ。"},{"id":{"d":890,"n":7,"t":2},"speaker":"A","text":"学生食堂に学外の人が入っても良いのですか？"},{"id":{"d":890,"n":8,"t":2},"speaker":"B","text":"大丈夫です。私も卒業したので学外の人間ですよ。おしゃれなコース料理が食べられる学生食堂もあるので、そこへ行きませんか？"},{"id":{"d":890,"n":9,"t":2},"speaker":"A","text":"ぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":891,"n":1,"t":2},"speaker":"A","text":"今回は新学期が始まるときに雑巾を持って行かなくても良いのですか？"},{"id":{"d":891,"n":2,"t":2},"speaker":"B","text":"夏休みの前に家庭科で雑巾を作ったので、要らないのだそうです。"},{"id":{"d":891,"n":3,"t":2},"speaker":"A","text":"そうでしたね。古いタオルを学校へ持って行ったことがありましたね。"},{"id":{"d":891,"n":4,"t":2},"speaker":"B","text":"はい。それを使って雑巾を作り、今も教室の掃除に使っています。"},{"id":{"d":891,"n":5,"t":2},"speaker":"A","text":"雑巾は上手にできたのですか？"},{"id":{"d":891,"n":6,"t":2},"speaker":"B","text":"私は結構上手にできましたよ。でも、他の人が作ったものの中には、一度掃除をしたらすぐに破れそうな雑巾もたくさんありました。"}];
INSERT INTO sentence [{"id":{"d":892,"n":1,"t":2},"speaker":"A","text":"翔太は、来年は2年生になりますね。"},{"id":{"d":892,"n":2,"t":2},"speaker":"B","text":"はい、そうです。"},{"id":{"d":892,"n":3,"t":2},"speaker":"A","text":"あなたの高校では、2年生になる前に理系と文系を選ぶのですよね。"},{"id":{"d":892,"n":4,"t":2},"speaker":"B","text":"はい。まさに今迷っているところです。"},{"id":{"d":892,"n":5,"t":2},"speaker":"A","text":"得意な教科は何ですか？"},{"id":{"d":892,"n":6,"t":2},"speaker":"B","text":"強いて言えば、英語かと思います。"},{"id":{"d":892,"n":7,"t":2},"speaker":"A","text":"英語は文系、理系どちらでも必要ですね。"},{"id":{"d":892,"n":8,"t":2},"speaker":"B","text":"そうなのです。将来やりたいことがはっきりしなくて困っています。"}];
INSERT INTO sentence [{"id":{"d":893,"n":1,"t":2},"speaker":"A","text":"お父さん、行きたい大学と学部が決まりました。"},{"id":{"d":893,"n":2,"t":2},"speaker":"B","text":"どこに決めたのですか？"},{"id":{"d":893,"n":3,"t":2},"speaker":"A","text":"横浜大学理工学部です。"},{"id":{"d":893,"n":4,"t":2},"speaker":"B","text":"この前は、文系か理系かも悩んでいた様子でしたが、理系にしたのですね。"},{"id":{"d":893,"n":5,"t":2},"speaker":"A","text":"はい。理系に絞って一直線に勉強します。"},{"id":{"d":893,"n":6,"t":2},"speaker":"B","text":"そこでやりたいことが見つかったのですか？"},{"id":{"d":893,"n":7,"t":2},"speaker":"A","text":"はい。この前の授業でペットボトルロケットの工作をしたのですが、それがとても楽しかったので、将来はロケットや航空機の開発をしてみたいと思いました。"},{"id":{"d":893,"n":8,"t":2},"speaker":"B","text":"素晴らしい目標ですね。目標に向けて頑張ってください。応援しています。"}];
INSERT INTO sentence [{"id":{"d":894,"n":1,"t":2},"speaker":"A","text":"すみません、ばんそうこうは購買にありますか？"},{"id":{"d":894,"n":2,"t":2},"speaker":"B","text":"木田先生、けがでもされたのですか？"},{"id":{"d":894,"n":3,"t":2},"speaker":"A","text":"いいえ。これから授業で版画の制作を行うので、一応準備をしておこうかと思いまして。"},{"id":{"d":894,"n":4,"t":2},"speaker":"B","text":"ばんそうこうは、生徒たちがけがをした時のための事前準備ですね。"},{"id":{"d":894,"n":5,"t":2},"speaker":"A","text":"そうです。毎年、何人かは彫刻刀で手を切りますからね。"},{"id":{"d":894,"n":6,"t":2},"speaker":"B","text":"その場合は保健室に行かせれば良いのではありませんか？"},{"id":{"d":894,"n":7,"t":2},"speaker":"A","text":"はい。重症でしたら、即、保健室ですが、そこまででない時は手を洗わせてばんそうこうで血を止めます。"},{"id":{"d":894,"n":8,"t":2},"speaker":"B","text":"そのようなことでしたら、大中小のばんそうこうを用意しておいた方がいいですね。こちらをどうぞ。"}];
INSERT INTO sentence [{"id":{"d":895,"n":1,"t":2},"speaker":"A","text":"最近、算数のテストの点数が悪いですね。"},{"id":{"d":895,"n":2,"t":2},"speaker":"B","text":"ごめんなさい。お母さん。"},{"id":{"d":895,"n":3,"t":2},"speaker":"A","text":"謝らなくて良いのです。何ができないのか教えてください。"},{"id":{"d":895,"n":4,"t":2},"speaker":"B","text":"計算はできます。でも文章題がよく分かりません。"},{"id":{"d":895,"n":5,"t":2},"speaker":"A","text":"文章の意味がよく分かりませんか？"},{"id":{"d":895,"n":6,"t":2},"speaker":"B","text":"はい。文章からどのような式を立てればよいのかが分かりません。"},{"id":{"d":895,"n":7,"t":2},"speaker":"A","text":"文章を正しく読むことが苦手なのでしょうか？では、今日は私と一緒に問題を解いてみましょう。"},{"id":{"d":895,"n":8,"t":2},"speaker":"B","text":"そうかもしれません。ありがとうございます。お母さんと一緒に解いたら、できそうな気がします。"}];
INSERT INTO sentence [{"id":{"d":896,"n":1,"t":2},"speaker":"A","text":"中村先生はいつも指し棒を使っていらっしゃいますよね。"},{"id":{"d":896,"n":2,"t":2},"speaker":"B","text":"はい。私は背が低いので、これがあると授業を進める際に楽なのですよ。"},{"id":{"d":896,"n":3,"t":2},"speaker":"A","text":"私も同じくらいの身長なので、指し棒が欲しいと思っているところです。先生はどちらで買われたのですか？"},{"id":{"d":896,"n":4,"t":2},"speaker":"B","text":"私はインターネット通販で買いました。"},{"id":{"d":896,"n":5,"t":2},"speaker":"A","text":"そうなのでしたか。同じものを買いたいのですが、私は恥ずかしながらインターネット通販を利用したことがありません。"},{"id":{"d":896,"n":6,"t":2},"speaker":"B","text":"通販のサイトに登録すれば誰でも簡単に購入できますよ。"},{"id":{"d":896,"n":7,"t":2},"speaker":"A","text":"それはパソコンからのみですか？スマートフォンからでも購入可能ですか？"},{"id":{"d":896,"n":8,"t":2},"speaker":"B","text":"どちらも大丈夫ですよ。お教えしましょうか？"},{"id":{"d":896,"n":9,"t":2},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":897,"n":1,"t":2},"speaker":"A","text":"お母さん、これらをどう思いますか？"},{"id":{"d":897,"n":2,"t":2},"speaker":"B","text":"かわいいですね。ウサギとクマの紙人形ですか？"},{"id":{"d":897,"n":3,"t":2},"speaker":"A","text":"そうです。かわいく見えたのなら良かったです。今度、職場体験で行く保育園で、このようにして動かして使おうと思っているのです。"},{"id":{"d":897,"n":4,"t":2},"speaker":"B","text":"人形劇のようにするのですね。"},{"id":{"d":897,"n":5,"t":2},"speaker":"A","text":"はい。子どもたちに何かを教えるときに、人形でお話をしたら良いかと思って作りました。"},{"id":{"d":897,"n":6,"t":2},"speaker":"B","text":"とても良いと思いますよ。"},{"id":{"d":897,"n":7,"t":2},"speaker":"A","text":"クマは悪者になってもらおうと思って、少しだけ牙を付けたのですが、怖くありませんか？"},{"id":{"d":897,"n":8,"t":2},"speaker":"B","text":"そのくらいならかわいいものですよ。子どもたちが喜ぶと思います。"}];
INSERT INTO sentence [{"id":{"d":898,"n":1,"t":2},"speaker":"A","text":"今日は家庭教師が来ますから、真っ直ぐ家へ帰ってくるのですよ。"},{"id":{"d":898,"n":2,"t":2},"speaker":"B","text":"はい。ウサギ小屋の掃除が終わったら、すぐに帰ってきます。"},{"id":{"d":898,"n":3,"t":2},"speaker":"A","text":"そうすると帰宅時間は家庭教師の講義が始まる時間の瀬戸際となりそうですよ。その掃除は他の子に任せられませんか？"},{"id":{"d":898,"n":4,"t":2},"speaker":"B","text":"はい。私が担当したいので他の子に任せるのは嫌です。"},{"id":{"d":898,"n":5,"t":2},"speaker":"A","text":"分かりました。では、なるべく早く終わらせて帰ってきてくださいね。"},{"id":{"d":898,"n":6,"t":2},"speaker":"B","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":899,"n":1,"t":2},"speaker":"A","text":"今日は三者面談ですね。なんだか緊張します。"},{"id":{"d":899,"n":2,"t":2},"speaker":"B","text":"亜紀さんの家からは、三者面談にお母さまが来るのですか？"},{"id":{"d":899,"n":3,"t":2},"speaker":"A","text":"いえ、うちは父が来ます。"},{"id":{"d":899,"n":4,"t":2},"speaker":"B","text":"お仕事を休んで来るのですか？"},{"id":{"d":899,"n":5,"t":2},"speaker":"A","text":"いいえ。元々月曜日は父のお仕事が休みなのです。美里さんの家はお母さまが来るのですか？"},{"id":{"d":899,"n":6,"t":2},"speaker":"B","text":"はい。母がお仕事を休んで来ます。うちは共働きなので。"},{"id":{"d":899,"n":7,"t":2},"speaker":"A","text":"お仕事を休まなくてはならないのは大変ですね。"},{"id":{"d":899,"n":8,"t":2},"speaker":"B","text":"そうかもしれません。でも、今日は一緒に家に帰れるので私はうれしいです。"}];
INSERT INTO sentence [{"id":{"d":900,"n":1,"t":2},"speaker":"A","text":"誠さんのレッスンバッグにはカバーが付いているのですね。"},{"id":{"d":900,"n":2,"t":2},"speaker":"B","text":"ええ。誠の祖母が、壊れた傘でレッスンバッグカバーを作ってくれたのです。"},{"id":{"d":900,"n":3,"t":2},"speaker":"A","text":"すてきです。壊れた傘で雨除けカバーが作れるのですね。"},{"id":{"d":900,"n":4,"t":2},"speaker":"B","text":"誠が傘の骨を折ってしまうので、布の部分だけ新品同様の傘が何本もあるのですよ。それを祖母がうまく再利用して作ってくれました。"},{"id":{"d":900,"n":5,"t":2},"speaker":"A","text":"息子の聡も傘を壊すことが多いので、まねしたいです。"},{"id":{"d":900,"n":6,"t":2},"speaker":"B","text":"ちなみに、あれはカバーするだけでなくエコバッグにもなるのですよ。"},{"id":{"d":900,"n":7,"t":2},"speaker":"A","text":"軽いし、雨にぬれないし、最高ですね。おばあちゃんに弟子入りしたいです。"},{"id":{"d":900,"n":8,"t":2},"speaker":"B","text":"今度会ったときに作り方を聞いておきますね。"}];
INSERT INTO sentence [{"id":{"d":901,"n":1,"t":2},"speaker":"A","text":"お母さん、今日、野球部のユニフォームをもらいました。"},{"id":{"d":901,"n":2,"t":2},"speaker":"B","text":"格好良いですね。これをもらったということは、レギュラーになったのですか？"},{"id":{"d":901,"n":3,"t":2},"speaker":"A","text":"はい。ユニフォームにはこの背番号を付けてください。"},{"id":{"d":901,"n":4,"t":2},"speaker":"B","text":"3番ではありませんか。ファーストのレギュラーになったのですか？"},{"id":{"d":901,"n":5,"t":2},"speaker":"A","text":"そうなのです。今年は県大会を突破できるように頑張ります。"},{"id":{"d":901,"n":6,"t":2},"speaker":"B","text":"お母さんも応援に力が入ります。大会には必ず応援に行きますね。"},{"id":{"d":901,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。翔太もレギュラーになりました。応援には翔太のお母さまと一緒に来てください。"},{"id":{"d":901,"n":8,"t":2},"speaker":"B","text":"良かったですね。それでは早速、翔太君のママに電話してみます。"}];
INSERT INTO sentence [{"id":{"d":902,"n":1,"t":2},"speaker":"A","text":"先生、宿題のプリントを忘れてしまいました。"},{"id":{"d":902,"n":2,"t":2},"speaker":"B","text":"和孝君は今週に入ってから宿題のプリントを忘れるのは2回目ですよ。"},{"id":{"d":902,"n":3,"t":2},"speaker":"A","text":"すみません。宿題は終わっていたのですが、朝は忙しくしていて、学校に持って来るのを忘れてしまいました。"},{"id":{"d":902,"n":4,"t":2},"speaker":"B","text":"そうですか。朝は余裕を持って行動して、慌てずに家を出るようにしましょう。"},{"id":{"d":902,"n":5,"t":2},"speaker":"A","text":"はい。まずは早起きを頑張ります。"},{"id":{"d":902,"n":6,"t":2},"speaker":"B","text":"頑張ってくださいね。今日の分の宿題は明日、忘れずに提出してください。"},{"id":{"d":902,"n":7,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":903,"n":1,"t":2},"speaker":"A","text":"卒業文集の原稿なのですが、1人分、足りません。"},{"id":{"d":903,"n":2,"t":2},"speaker":"B","text":"誰の原稿がありませんか？"},{"id":{"d":903,"n":3,"t":2},"speaker":"A","text":"田中君の分です。"},{"id":{"d":903,"n":4,"t":2},"speaker":"B","text":"最近、田中君は部活で忙しそうですね。"},{"id":{"d":903,"n":5,"t":2},"speaker":"A","text":"締め切りは明後日なのですが、大丈夫でしょうか？"},{"id":{"d":903,"n":6,"t":2},"speaker":"B","text":"忘れているかもしれないので、私から田中君に言っておきます。期限は明後日の15時でしたよね？"},{"id":{"d":903,"n":7,"t":2},"speaker":"A","text":"そうです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":904,"n":1,"t":2},"speaker":"A","text":"来週オープンキャンパスへ行って来ようと思っています。"},{"id":{"d":904,"n":2,"t":2},"speaker":"B","text":"志望校は東京にありますよね。東京まで行くのですか？"},{"id":{"d":904,"n":3,"t":2},"speaker":"A","text":"はい。1泊2日で東京に行って、志望する数校を見てこようと思っています。"},{"id":{"d":904,"n":4,"t":2},"speaker":"B","text":"その2日間は学校をお休みするということですね？"},{"id":{"d":904,"n":5,"t":2},"speaker":"A","text":"はい、そうさせてください。"},{"id":{"d":904,"n":6,"t":2},"speaker":"B","text":"一人で行くのですか？保護者の方も一緒ですか？"},{"id":{"d":904,"n":7,"t":2},"speaker":"A","text":"母が一緒です。訪問する学校の全部に付いて来てくれるわけではないと思いますが。"},{"id":{"d":904,"n":8,"t":2},"speaker":"B","text":"お母さまがご一緒なら、まず安心ですね。気を付けて行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":905,"n":1,"t":2},"speaker":"A","text":"今日で部活動は引退です。寂しくなります。"},{"id":{"d":905,"n":2,"t":2},"speaker":"B","text":"お疲れさまでした。あとは受験一筋ですか？"},{"id":{"d":905,"n":3,"t":2},"speaker":"A","text":"いえ、実は私はもう推薦入試で進学する大学が決まっているので、アルバイトでも探そうかと思っています。"},{"id":{"d":905,"n":4,"t":2},"speaker":"B","text":"推薦が決まっているなどと初めて聞きました。"},{"id":{"d":905,"n":5,"t":2},"speaker":"A","text":"初めて言いましたから。決まったのはつい先週です。"},{"id":{"d":905,"n":6,"t":2},"speaker":"B","text":"そうだったのですね。おめでとうございます。"},{"id":{"d":905,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":906,"n":1,"t":2},"speaker":"A","text":"社会科見学の感想文を書いてもらったのですが、面白い文章を書いた生徒がいましたよ。"},{"id":{"d":906,"n":2,"t":2},"speaker":"B","text":"どのような感想文なのですか？"},{"id":{"d":906,"n":3,"t":2},"speaker":"A","text":"感想というよりレシピでした。"},{"id":{"d":906,"n":4,"t":2},"speaker":"B","text":"3年生はお菓子工場に行ったのでしたよね？"},{"id":{"d":906,"n":5,"t":2},"speaker":"A","text":"はい。「いつも食べているお菓子の作り方が分かってうれしかったです」と、ここまでは感想でした。"},{"id":{"d":906,"n":6,"t":2},"speaker":"B","text":"そうですね。"},{"id":{"d":906,"n":7,"t":2},"speaker":"A","text":"その後は「その作り方とは、砂糖をたらいに1杯、粉をたらいに山盛り、よく混ぜる」と書いてあるのです。その後も延々とお菓子の作り方が書かれていました。"},{"id":{"d":906,"n":8,"t":2},"speaker":"B","text":"よく見ているではありませんか。私なら花丸を付けます。"}];
INSERT INTO sentence [{"id":{"d":907,"n":1,"t":2},"speaker":"A","text":"作文コンクールで、クラスの子が最優秀賞を取りました。"},{"id":{"d":907,"n":2,"t":2},"speaker":"B","text":"素晴らしいですね。"},{"id":{"d":907,"n":3,"t":2},"speaker":"A","text":"でも、一つ問題があって、賞を取った子は内気な子で、「授賞式にみんなの前で作文を読むのは嫌だ」と言います。"},{"id":{"d":907,"n":4,"t":2},"speaker":"B","text":"そうですか。1人だと緊張してしまうのなら、誰かと一緒に登壇するのはどうでしょうか？"},{"id":{"d":907,"n":5,"t":2},"speaker":"A","text":"はい。私やお友達が「付き添うから」と言っても、泣きそうになってしまうのです。"},{"id":{"d":907,"n":6,"t":2},"speaker":"B","text":"人前に出ることに慣れていないのでしょうね。"},{"id":{"d":907,"n":7,"t":2},"speaker":"A","text":"どうしたら良いと思いますか？"},{"id":{"d":907,"n":8,"t":2},"speaker":"B","text":"それでは、あらかじめビデオでその子が作文を読むところを撮って、会場で、その動画を流してもらうのはどうですか？"},{"id":{"d":907,"n":9,"t":2},"speaker":"A","text":"そのアイデアは考えつきませんでした。明日早速提案してみます。"}];
INSERT INTO sentence [{"id":{"d":908,"n":1,"t":2},"speaker":"A","text":"先輩、部室の掃除を1人でされているのですか？"},{"id":{"d":908,"n":2,"t":2},"speaker":"B","text":"はい。そろそろ卒業なので、部室への恩返しです。"},{"id":{"d":908,"n":3,"t":2},"speaker":"A","text":"私も手伝います。"},{"id":{"d":908,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。掃除をしていたら、このようなものが出てきました。"},{"id":{"d":908,"n":5,"t":2},"speaker":"A","text":"去年卒業した先輩たちの寄せ書きですね。"},{"id":{"d":908,"n":6,"t":2},"speaker":"B","text":"はい。棚の下から出てきました。きちんとした場所に置いていなくて申し訳ありません。"},{"id":{"d":908,"n":7,"t":2},"speaker":"A","text":"来年からは、保管場所を決めて置くようにします。先輩たちの分もきちんとした場所に置かせてもらいますね。"},{"id":{"d":908,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":909,"n":1,"t":2},"speaker":"A","text":"その大きな布をどうするのですか？"},{"id":{"d":909,"n":2,"t":2},"speaker":"B","text":"これは体育祭の応援旗にするのです。"},{"id":{"d":909,"n":3,"t":2},"speaker":"A","text":"なるほど、旗にするのですね。デザインは、もう決まっているのですか？"},{"id":{"d":909,"n":4,"t":2},"speaker":"B","text":"はい。書道部の陣内さんに格好良く「紅組」と書いてもらいます。"},{"id":{"d":909,"n":5,"t":2},"speaker":"A","text":"すてきです。では、縫製は私がしましょうか？"},{"id":{"d":909,"n":6,"t":2},"speaker":"B","text":"よいのですか？さすが、演劇部の衣装担当ですね。お願いします。"},{"id":{"d":909,"n":7,"t":2},"speaker":"A","text":"ところで、旗の持ち手の部分はどうするのですか？"},{"id":{"d":909,"n":8,"t":2},"speaker":"B","text":"福田君の家の裏から竹を採ってきてもらうことになっています。"}];
INSERT INTO sentence [{"id":{"d":910,"n":1,"t":2},"speaker":"A","text":"お母さん、今日は疲れました。もう何もしたくありません。"},{"id":{"d":910,"n":2,"t":2},"speaker":"B","text":"体育の授業でもありましたか？"},{"id":{"d":910,"n":3,"t":2},"speaker":"A","text":"いいえ。体育はありませんでしたが、ミニテストがありました。"},{"id":{"d":910,"n":4,"t":2},"speaker":"B","text":"苦手な数学のテストですか？"},{"id":{"d":910,"n":5,"t":2},"speaker":"A","text":"いえ、国語です。漢字の書き取りでした。"},{"id":{"d":910,"n":6,"t":2},"speaker":"B","text":"漢字は得意でしょう？"},{"id":{"d":910,"n":7,"t":2},"speaker":"A","text":"はい。苦手ではありませんが、量が多くて大変でした。制限時間5分のテストを10回連続だったのです。"},{"id":{"d":910,"n":8,"t":2},"speaker":"B","text":"それはなかなかハードなスケジュールでしたね。お疲れさまでした。"}];
INSERT INTO sentence [{"id":{"d":911,"n":1,"t":2},"speaker":"A","text":"おはようございます。みんなで集まって何を見ているのですか？"},{"id":{"d":911,"n":2,"t":2},"speaker":"B","text":"重田君の教科書のパラパラ漫画が素晴らしいので見ているのです。"},{"id":{"d":911,"n":3,"t":2},"speaker":"A","text":"私にも見せてください。"},{"id":{"d":911,"n":4,"t":2},"speaker":"B","text":"どうぞ。表紙の方からめくるのと、裏表紙の方からめくるのとで違うパラパラ漫画になっているのですよ。"},{"id":{"d":911,"n":5,"t":2},"speaker":"A","text":"非常に面白いです。絵も上手ですね。"},{"id":{"d":911,"n":6,"t":2},"speaker":"B","text":"教科書の端に芸術ができていますよね。"},{"id":{"d":911,"n":7,"t":2},"speaker":"A","text":"このように素晴らしいものを重田君はいつ描いたのでしょう？"},{"id":{"d":911,"n":8,"t":2},"speaker":"B","text":"気になりますよね。重田君は「秘密」と言っていました。"}];
INSERT INTO sentence [{"id":{"d":912,"n":1,"t":2},"speaker":"A","text":"息子は、分数の単元で算数が分からなくなってしまったようなのです。"},{"id":{"d":912,"n":2,"t":2},"speaker":"B","text":"分数は難しいと感じる子が多いようですよね。"},{"id":{"d":912,"n":3,"t":2},"speaker":"A","text":"そのようです。私自身も分数のテストは点数が悪かった覚えがあるのです。"},{"id":{"d":912,"n":4,"t":2},"speaker":"B","text":"それでは、どこが分からないのか想像しやすいのではありませんか？"},{"id":{"d":912,"n":5,"t":2},"speaker":"A","text":"それが、私はいつの間にかできるようになっていたので、何が難しかったのかはあやふやなのです。"},{"id":{"d":912,"n":6,"t":2},"speaker":"B","text":"そうでしたか。うちの娘は「塾の授業で分数が分かるようになった」と言っていましたよ。お宅の息子さんも塾にお願いするのはいかがでしょうか？"},{"id":{"d":912,"n":7,"t":2},"speaker":"A","text":"そうなのですね。息子にも塾を検討してみます。"}];
INSERT INTO sentence [{"id":{"d":913,"n":1,"t":2},"speaker":"A","text":"球技大会では何の競技に出ますか？"},{"id":{"d":913,"n":2,"t":2},"speaker":"B","text":"私は卓球の試合に出場しようと思っています。"},{"id":{"d":913,"n":3,"t":2},"speaker":"A","text":"バスケットボール部なのにバスケットボールの試合に出場する予定はありませんか？"},{"id":{"d":913,"n":4,"t":2},"speaker":"B","text":"「部活でやっている競技は出てはいけない」と言われたのを聞いていませんでしたか？"},{"id":{"d":913,"n":5,"t":2},"speaker":"A","text":"聞いていませんでした。では、私はバレーボールの試合には出られないのですか？"},{"id":{"d":913,"n":6,"t":2},"speaker":"B","text":"そうです。あなたはバレーボール部の部員ですから、バレーボールの試合には出られません。"},{"id":{"d":913,"n":7,"t":2},"speaker":"A","text":"そういうことでしたら、私も卓球の試合に出場しようかと思います。"}];
INSERT INTO sentence [{"id":{"d":914,"n":1,"t":2},"speaker":"A","text":"夏は部活に必要な荷物が多くて嫌になります。"},{"id":{"d":914,"n":2,"t":2},"speaker":"B","text":"部活用のTシャツと飲み物を持参するだけで大荷物になりますからね。"},{"id":{"d":914,"n":3,"t":2},"speaker":"A","text":"汗だくになるから着替えないわけにはいかないし、飲み物を飲まないと熱中症になるし。"},{"id":{"d":914,"n":4,"t":2},"speaker":"B","text":"水道水で良ければ水筒も小さくなると思いますけれど、スポーツドリンクが欲しいですよね。"},{"id":{"d":914,"n":5,"t":2},"speaker":"A","text":"はい。部活後はスポーツドリンクが飲みたいです。"}];
INSERT INTO sentence [{"id":{"d":915,"n":1,"t":2},"speaker":"A","text":"新人研修では料理の時間もあるのですか？"},{"id":{"d":915,"n":2,"t":2},"speaker":"B","text":"そのようです。みんなで料理をして親睦を深めるためらしいですよ。"},{"id":{"d":915,"n":3,"t":2},"speaker":"A","text":"困りました。私は料理ができないので、片づけを担当しますね。"},{"id":{"d":915,"n":4,"t":2},"speaker":"B","text":"大学時代、1人暮らしだったのではありませんか？"},{"id":{"d":915,"n":5,"t":2},"speaker":"A","text":"1人暮らしでしたが、自炊は全くしていませんでした。"},{"id":{"d":915,"n":6,"t":2},"speaker":"B","text":"食事はどうしていたのですか？"},{"id":{"d":915,"n":7,"t":2},"speaker":"A","text":"食事は全部学生食堂を利用していました。"},{"id":{"d":915,"n":8,"t":2},"speaker":"B","text":"朝食も昼食も夕食も、学生食堂を利用したのですか？"},{"id":{"d":915,"n":9,"t":2},"speaker":"A","text":"朝食は食べないので、昼食と夕食は学生食堂を使っていました。"}];
INSERT INTO sentence [{"id":{"d":916,"n":1,"t":2},"speaker":"A","text":"水泳教室を見学したいのですが？"},{"id":{"d":916,"n":2,"t":2},"speaker":"B","text":"お子さんのクラスでしょうか？大人のクラスでしょうか？"},{"id":{"d":916,"n":3,"t":2},"speaker":"A","text":"小学2年生の子どものクラスです。"},{"id":{"d":916,"n":4,"t":2},"speaker":"B","text":"それでしたら、ちょうど本日4時からのクラスがございますが、いかがですか？"},{"id":{"d":916,"n":5,"t":2},"speaker":"A","text":"それは、初心者用クラスですか？"},{"id":{"d":916,"n":6,"t":2},"speaker":"B","text":"そうです。最初に子どもたちが所属するクラスです。"},{"id":{"d":916,"n":7,"t":2},"speaker":"A","text":"ではそれを見学させていただきたいです。4時に娘を連れて見学に来ます。"},{"id":{"d":916,"n":8,"t":2},"speaker":"B","text":"かしこまりました。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":917,"n":1,"t":2},"speaker":"A","text":"志望校は1校だけで良いのですか？"},{"id":{"d":917,"n":2,"t":2},"speaker":"B","text":"はい、志望校は1校だけです。"},{"id":{"d":917,"n":3,"t":2},"speaker":"A","text":"併願校は考えていませんね。"},{"id":{"d":917,"n":4,"t":2},"speaker":"B","text":"はい。志望校の入試に落ちたら浪人します。"},{"id":{"d":917,"n":5,"t":2},"speaker":"A","text":"どうしても進みたい学部があるのですか？"},{"id":{"d":917,"n":6,"t":2},"speaker":"B","text":"はい。他の大学にはない学部なのです。"},{"id":{"d":917,"n":7,"t":2},"speaker":"A","text":"確かにそうですね。分かりました。合格に向けて頑張りましょう。"},{"id":{"d":917,"n":8,"t":2},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":918,"n":1,"t":2},"speaker":"A","text":"春休みは宿題がないし、たくさん遊べますね。"},{"id":{"d":918,"n":2,"t":2},"speaker":"B","text":"ごめんなさい。私は、春休みは塾に行くことにしました。"},{"id":{"d":918,"n":3,"t":2},"speaker":"A","text":"毎日ですか？"},{"id":{"d":918,"n":4,"t":2},"speaker":"B","text":"そうです。中学生になったら勉強が難しくなるので、塾で予習をするつもりです。"},{"id":{"d":918,"n":5,"t":2},"speaker":"A","text":"私も遊んでばかりいないで何かした方がいいのでしょうか？"},{"id":{"d":918,"n":6,"t":2},"speaker":"B","text":"もし、良ければ一緒に塾へ行きますか？体験授業もやっているようですよ。"},{"id":{"d":918,"n":7,"t":2},"speaker":"A","text":"はい。帰ったらお母さんに相談してみます。"}];
INSERT INTO sentence [{"id":{"d":919,"n":1,"t":2},"speaker":"A","text":"中間テストの結果はどうだったのですか？"},{"id":{"d":919,"n":2,"t":2},"speaker":"B","text":"思ったより悪かったです。クラスで22番目でした。"},{"id":{"d":919,"n":3,"t":2},"speaker":"A","text":"真ん中より少し下ということですか？"},{"id":{"d":919,"n":4,"t":2},"speaker":"B","text":"はい。正直な話としては、さらに良い点が取れると思っていました。"},{"id":{"d":919,"n":5,"t":2},"speaker":"A","text":"確かに、「今回のテスト範囲の内容については自信がある」と言っていましたね。"},{"id":{"d":919,"n":6,"t":2},"speaker":"B","text":"そのはずでした。特に歴史は得意な範囲だったのですが、点数が伸びませんでした。"},{"id":{"d":919,"n":7,"t":2},"speaker":"A","text":"残念でしたね。"},{"id":{"d":919,"n":8,"t":2},"speaker":"B","text":"次回こそ良い点数が取れるよう頑張ります。"}];
INSERT INTO sentence [{"id":{"d":920,"n":1,"t":2},"speaker":"A","text":"今日の給食のメニューはパンでした。"},{"id":{"d":920,"n":2,"t":2},"speaker":"B","text":"あなたはパンが好きなので良かったですね。"},{"id":{"d":920,"n":3,"t":2},"speaker":"A","text":"しかも、今日はパンに塗るチョコクリームも付いていました。"},{"id":{"d":920,"n":4,"t":2},"speaker":"B","text":"残さず食べられましたか？"},{"id":{"d":920,"n":5,"t":2},"speaker":"A","text":"はい、もちろんです。とてもおいしかったのでおかわりをしたいくらいでした。"},{"id":{"d":920,"n":6,"t":2},"speaker":"B","text":"おいしく食べられたのなら良かったですね。"},{"id":{"d":920,"n":7,"t":2},"speaker":"A","text":"はい。次のパンの日も楽しみです。"}];
INSERT INTO sentence [{"id":{"d":921,"n":1,"t":2},"speaker":"A","text":"高校では友達ができましたか？"},{"id":{"d":921,"n":2,"t":2},"speaker":"B","text":"はい。何人かのクラスの子とLINEの連絡先の交換をしました。"},{"id":{"d":921,"n":3,"t":2},"speaker":"A","text":"それは良かったです。学校は楽しいですか？"},{"id":{"d":921,"n":4,"t":2},"speaker":"B","text":"楽しいです。今日もみんなで帰りにカフェに行ってきました。写真を見ますか？"},{"id":{"d":921,"n":5,"t":2},"speaker":"A","text":"はい。カフェの写真ですか？"},{"id":{"d":921,"n":6,"t":2},"speaker":"B","text":"お友達になった子たちとカフェで撮った写真です。"}];
INSERT INTO sentence [{"id":{"d":922,"n":1,"t":2},"speaker":"A","text":"先輩、今日は音楽室が使えませんか？"},{"id":{"d":922,"n":2,"t":2},"speaker":"B","text":"はい。今日は合唱部が使う日です。私たちは体育館の裏でステップの練習です。"},{"id":{"d":922,"n":3,"t":2},"speaker":"A","text":"今日がその日でしたか？ステップは苦手です。"},{"id":{"d":922,"n":4,"t":2},"speaker":"B","text":"はい。練習をすれば必ず上達しますよ。さあ、ジャージに着替えて来てください。"},{"id":{"d":922,"n":5,"t":2},"speaker":"A","text":"分かりました。急いで着替えて行きます。"}];
INSERT INTO sentence [{"id":{"d":923,"n":1,"t":2},"speaker":"A","text":"昨日アルバイト代が入ったので、今日は焼肉を食べに行こうと思っています。一緒に行きますか？"},{"id":{"d":923,"n":2,"t":2},"speaker":"B","text":"はい。焼き肉には行きたいですが、高い価格のお店でしょうか？"},{"id":{"d":923,"n":3,"t":2},"speaker":"A","text":"それほど高くはありませんよ。ここの近くの食べ放題の店です。"},{"id":{"d":923,"n":4,"t":2},"speaker":"B","text":"あのお店は、期間によっては学割があると聞いたことがありますが、今はその期間でしょうか？"},{"id":{"d":923,"n":5,"t":2},"speaker":"A","text":"ちゃんと調べてありますよ。今週末まで、学生は15パーセント引きです。"},{"id":{"d":923,"n":6,"t":2},"speaker":"B","text":"さすがです。そこなら一緒に行きたいです。"}];
INSERT INTO sentence [{"id":{"d":924,"n":1,"t":2},"speaker":"A","text":"先生、メトロノームを貸していただけますか？"},{"id":{"d":924,"n":2,"t":2},"speaker":"B","text":"良いですよ。しかし、メトロノームを何に使うのですか？"},{"id":{"d":924,"n":3,"t":2},"speaker":"A","text":"ダンス同好会でテンポの勉強をしたいのです。"},{"id":{"d":924,"n":4,"t":2},"speaker":"B","text":"森本さんはダンス同好会だったのですね。はい、メトロノームです。"},{"id":{"d":924,"n":5,"t":2},"speaker":"A","text":"これがメトロノームですか？棒が左右に揺れるものを想像していたのですが、それとは形が違います。"},{"id":{"d":924,"n":6,"t":2},"speaker":"B","text":"これは電子メトロノームというものです。使うのは初めてですか？"},{"id":{"d":924,"n":7,"t":2},"speaker":"A","text":"はい、見るのも初めてです。これでテンポを測れるのですか？"},{"id":{"d":924,"n":8,"t":2},"speaker":"B","text":"電子メトロノームの方が正確なので、最近私はこちらをよく使っています。説明書もお渡ししますので、この通りに使ってみてください。"},{"id":{"d":924,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。使い終わったら返しに来ますね。"},{"id":{"d":924,"n":10,"t":2},"speaker":"B","text":"17時以降は音楽室でなく職員室にいますので、そちらに来てください。"},{"id":{"d":924,"n":11,"t":2},"speaker":"A","text":"分かりました。では、お借りします。"}];
INSERT INTO sentence [{"id":{"d":925,"n":1,"t":2},"speaker":"A","text":"青の絵の具はありませんか？"},{"id":{"d":925,"n":2,"t":2},"speaker":"B","text":"ありますよ。青だけなくなってしまったのですか？"},{"id":{"d":925,"n":3,"t":2},"speaker":"A","text":"はい。海の絵をたくさん描いていたら、なくなってしまったのです。貸していただけないでしょうか？"},{"id":{"d":925,"n":4,"t":2},"speaker":"B","text":"私の青の絵の具はまだ残っていますから、使ってください。"},{"id":{"d":925,"n":5,"t":2},"speaker":"A","text":"ありがとうございます。この絵の具は小分け販売をしていますか？"},{"id":{"d":925,"n":6,"t":2},"speaker":"B","text":"個別で買おうとしたことがないので分かりません。"},{"id":{"d":925,"n":7,"t":2},"speaker":"A","text":"買ってお返ししようと思いますが、もし小分け販売がなかったら、別の会社の青の絵の具をお返しして良いですか？"},{"id":{"d":925,"n":8,"t":2},"speaker":"B","text":"ご丁寧にありがとうございます。水彩絵の具でしたら何でも大丈夫ですよ。"},{"id":{"d":925,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。では、青の絵の具をお借りします。"}];
INSERT INTO sentence [{"id":{"d":926,"n":1,"t":2},"speaker":"A","text":"先週はジムにお付き合いできなくてすみませんでした。"},{"id":{"d":926,"n":2,"t":2},"speaker":"B","text":"こちらこそお忙しいのに無理を言ってすみません。今週は来てくれてうれしいです。"},{"id":{"d":926,"n":3,"t":2},"speaker":"A","text":"今日明日はお休みが取れたもので。でも、またしばらく土日も仕事が入りそうです。"},{"id":{"d":926,"n":4,"t":2},"speaker":"B","text":"大変そうですね。"},{"id":{"d":926,"n":5,"t":2},"speaker":"A","text":"陸上部の顧問になったのですが、大会は土日にあることが多いので、土日は生徒の遠征に付いて行かなくてはなりません。"},{"id":{"d":926,"n":6,"t":2},"speaker":"B","text":"そうだったのですか。では、しばらくは一緒にジムへ行くことが難しそうですね。"},{"id":{"d":926,"n":7,"t":2},"speaker":"A","text":"向こう数カ月は難しいかもしれませんが、冬は陸上の大会がないので、冬になったらまた一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":927,"n":1,"t":2},"speaker":"A","text":"やっと今日の授業が終わりました。今日はたくさん先生に当てられたのに全く答えられず散々でした。"},{"id":{"d":927,"n":2,"t":2},"speaker":"B","text":"お疲れさまでした。今日はどの授業でもあなたが当てられていましたね。"},{"id":{"d":927,"n":3,"t":2},"speaker":"A","text":"今日が15日だからですよ。"},{"id":{"d":927,"n":4,"t":2},"speaker":"B","text":"どのような意味ですか？"},{"id":{"d":927,"n":5,"t":2},"speaker":"A","text":"僕の出席番号が15番なのです。15日はよく指名されるのです。"},{"id":{"d":927,"n":6,"t":2},"speaker":"B","text":"そのような理由だったのですね。気付きませんでした。"},{"id":{"d":927,"n":7,"t":2},"speaker":"A","text":"あなたはそのような経験はありませんか？"},{"id":{"d":927,"n":8,"t":2},"speaker":"B","text":"ありません。出席番号が32番で良かったです。"},{"id":{"d":927,"n":9,"t":2},"speaker":"A","text":"うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":928,"n":1,"t":2},"speaker":"A","text":"実用英語技能検定に向けた英会話の学習をするために、語学留学されたと聞きました。どうでしたか？"},{"id":{"d":928,"n":2,"t":2},"speaker":"B","text":"夏休みに短期留学に行ってきましたが、残念ながらあまり勉強にはなりませんでした。"},{"id":{"d":928,"n":3,"t":2},"speaker":"A","text":"なぜですか？"},{"id":{"d":928,"n":4,"t":2},"speaker":"B","text":"周りが日本人だらけだったので、せっかく米国にいてもほとんど日本語しか話さなかったからです。"},{"id":{"d":928,"n":5,"t":2},"speaker":"A","text":"買い物など、日常生活の中の会話も英会話の勉強になったのではありませんか？"},{"id":{"d":928,"n":6,"t":2},"speaker":"B","text":"買い物はほとんどがセルフレジでしたので、英会話に挑戦する機会は滅多にありませんでした。"},{"id":{"d":928,"n":7,"t":2},"speaker":"A","text":"そうでしたか。では、実用英語技能検定の勉強はこれからということですか？"},{"id":{"d":928,"n":8,"t":2},"speaker":"B","text":"そうなりますね。家でリスニングCDなどを聴いて、勉強し直して臨もうと思います。"}];
INSERT INTO sentence [{"id":{"d":929,"n":1,"t":2},"speaker":"A","text":"お母さん、新学期が始まる前に、学校の椅子に付ける座布団を買ってください。"},{"id":{"d":929,"n":2,"t":2},"speaker":"B","text":"中学校でも座布団を使うのですか？"},{"id":{"d":929,"n":3,"t":2},"speaker":"A","text":"中学校も小学校と同じ椅子を使うので、座布団がないと座り心地が悪いです。"},{"id":{"d":929,"n":4,"t":2},"speaker":"B","text":"そうなのですね。小学校で使っているものはもう使えませんか？"},{"id":{"d":929,"n":5,"t":2},"speaker":"A","text":"椅子にくくり付けるひもがもう劣化してしまいました。できれば買い替えてほしいです。"},{"id":{"d":929,"n":6,"t":2},"speaker":"B","text":"分かりました。では、今度の日曜日に座布団を買いにいきましょう。前の座布団はどこで買いましたか？"},{"id":{"d":929,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。前のものは駅前の百貨店で買ったと記憶しています。"},{"id":{"d":929,"n":8,"t":2},"speaker":"B","text":"では、今回もそこで選びましょう。"}];
INSERT INTO sentence [{"id":{"d":930,"n":1,"t":2},"speaker":"A","text":"明日の授業参観は気が重いです。"},{"id":{"d":930,"n":2,"t":2},"speaker":"B","text":"何がそれほどに嫌なのですか？子どもの授業を見に行くだけでしょう？"},{"id":{"d":930,"n":3,"t":2},"speaker":"A","text":"そのあとのPTAの会合ですよ。去年、授業参観を欠席したら役員を押し付けられたことがトラウマです。"},{"id":{"d":930,"n":4,"t":2},"speaker":"B","text":"そういえばそうでしたね。"},{"id":{"d":930,"n":5,"t":2},"speaker":"A","text":"今年も役員の押し付け合いがあると思うと、今から気が滅入ります。"},{"id":{"d":930,"n":6,"t":2},"speaker":"B","text":"きっぱりと断ってしまえば良いではありませんか？"},{"id":{"d":930,"n":7,"t":2},"speaker":"A","text":"いいえ。それほど簡単に断れないから、気が重いのですよ。人間関係は難しいです。"},{"id":{"d":930,"n":8,"t":2},"speaker":"B","text":"そうなのですね。頑張ってください、としか言えません。"}];
INSERT INTO sentence [{"id":{"d":931,"n":1,"t":2},"speaker":"A","text":"来年から私は理系コースに進もうと思っています。あなたはどうしますか？"},{"id":{"d":931,"n":2,"t":2},"speaker":"B","text":"私は数学が苦手なので文系にすると思います。"},{"id":{"d":931,"n":3,"t":2},"speaker":"A","text":"文系コースを選択して、その先の進学先は決まっているのですか？"},{"id":{"d":931,"n":4,"t":2},"speaker":"B","text":"苦手な科目を避けているだけの文系選択なので、進路までは決まっていません。"},{"id":{"d":931,"n":5,"t":2},"speaker":"A","text":"そのような選択の方法で良いのですか？"},{"id":{"d":931,"n":6,"t":2},"speaker":"B","text":"良いとは思っていませんが、結論を出さなければいけなかったので。和香子はもう進路を決めたのですか？"},{"id":{"d":931,"n":7,"t":2},"speaker":"A","text":"いいえ。進学を希望する学校はまだ決めていませんが、薬学部にするつもりです。"},{"id":{"d":931,"n":8,"t":2},"speaker":"B","text":"進む道がはっきりしているのですね。うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":932,"n":1,"t":2},"speaker":"A","text":"先日の模擬試験の結果が出ましたよ。"},{"id":{"d":932,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。難関校はさすがにD判定ですね。"},{"id":{"d":932,"n":3,"t":2},"speaker":"A","text":"そうですね。でも、日本史と地理の点数がもう少し上がれば、Bくらいにはなると思いますよ。"},{"id":{"d":932,"n":4,"t":2},"speaker":"B","text":"そうかもしれませんが、社会科はどうしても苦手です。"},{"id":{"d":932,"n":5,"t":2},"speaker":"A","text":"暗記物は繰り返し学習すれば成績が上がってくるものです、少しずつ頑張ってください。"},{"id":{"d":932,"n":6,"t":2},"speaker":"B","text":"気が重いですが頑張ります。"},{"id":{"d":932,"n":7,"t":2},"speaker":"A","text":"入試までまだ半年ありますから、諦めずに頑張りましょう。"},{"id":{"d":932,"n":8,"t":2},"speaker":"B","text":"はい。秋の模擬試験ではB判定を目指します。"}];
INSERT INTO sentence [{"id":{"d":933,"n":1,"t":2},"speaker":"A","text":"合唱コンクールでピアノの伴奏を任されました。"},{"id":{"d":933,"n":2,"t":2},"speaker":"B","text":"頑張ってくださいね。何を弾くのですか？"},{"id":{"d":933,"n":3,"t":2},"speaker":"A","text":"この曲です。楽譜はこれです。"},{"id":{"d":933,"n":4,"t":2},"speaker":"B","text":"これくらいなら美幸さんには簡単ですね。"},{"id":{"d":933,"n":5,"t":2},"speaker":"A","text":"落ち着いていれば難しくはありませんが、コンクールという場では冷静に弾ける自信がありません。"},{"id":{"d":933,"n":6,"t":2},"speaker":"B","text":"人前で弾くのは苦手ですか？"},{"id":{"d":933,"n":7,"t":2},"speaker":"A","text":"はい。練習では問題なくても、発表会では間違ってしまうことは何度もありました。どうしたら良いですか？"},{"id":{"d":933,"n":8,"t":2},"speaker":"B","text":"練習の数をこなすことが一番だと思います。今度、都庁に設置されているピアノでも弾きに行きますか？"},{"id":{"d":933,"n":9,"t":2},"speaker":"A","text":"なるほど、それは良い案かもしれません。ぜひ連れて行ってください。"}];
INSERT INTO sentence [{"id":{"d":934,"n":1,"t":2},"speaker":"A","text":"今日の試合の結果は本当に残念でした。もう、これで3年生が引退になってしまうと思うと悔しいです。"},{"id":{"d":934,"n":2,"t":2},"speaker":"B","text":"そうですね。先輩たちは「後悔はない」と言ってくれましたが、私は悔しくて仕方ありません。"},{"id":{"d":934,"n":3,"t":2},"speaker":"A","text":"先輩たちは来年応援に行くと約束してくれました。来年こそ、必ず優勝しないといけませんね。"},{"id":{"d":934,"n":4,"t":2},"speaker":"B","text":"そうですね。明日からまた練習を頑張りましょう。"},{"id":{"d":934,"n":5,"t":2},"speaker":"A","text":"はい。メニューも見直して、負けないチーム作りを徹底していきたいです。"}];
INSERT INTO sentence [{"id":{"d":935,"n":1,"t":2},"speaker":"A","text":"格好良いブックバンドをしていますね。"},{"id":{"d":935,"n":2,"t":2},"speaker":"B","text":"これは私の父が大学時代に使っていたブックバンドなのです。"},{"id":{"d":935,"n":3,"t":2},"speaker":"A","text":"お父さんのもの、というと30年ものですか？"},{"id":{"d":935,"n":4,"t":2},"speaker":"B","text":"そのくらいですね。少し金属のところがさびているのも格好良いでしょう？"},{"id":{"d":935,"n":5,"t":2},"speaker":"A","text":"はい。お父さんは物持ちが良いのですね。"},{"id":{"d":935,"n":6,"t":2},"speaker":"B","text":"大事に使っていたようです。昔の学生はみんなこれを使っていたそうですよ。"},{"id":{"d":935,"n":7,"t":2},"speaker":"A","text":"すてきです。最近はレトロブームで、ブックバンドを使っている人も増えてきましたよね。"},{"id":{"d":935,"n":8,"t":2},"speaker":"B","text":"新品のも面白いデザインがありますが、古いものも良いですよ。"}];
INSERT INTO sentence [{"id":{"d":936,"n":1,"t":2},"speaker":"A","text":"もしもし、お母さん？"},{"id":{"d":936,"n":2,"t":2},"speaker":"B","text":"はい。雨が降り出したのに帰って来ないので心配していたところでした。今は、どこにいるのですか？"},{"id":{"d":936,"n":3,"t":2},"speaker":"A","text":"学校です。放課後の校庭開放で遊んでいたら雨が降ってきてしました。"},{"id":{"d":936,"n":4,"t":2},"speaker":"B","text":"では、今はまだ学校なのですね？傘を持っていませんか？"},{"id":{"d":936,"n":5,"t":2},"speaker":"A","text":"はい。傘は持っていません。"},{"id":{"d":936,"n":6,"t":2},"speaker":"B","text":"車で迎えに行きますから、そこで待っていてください。"},{"id":{"d":936,"n":7,"t":2},"speaker":"A","text":"ありがとうございます。一緒に遊んでいた聡君のお母さんはまだお仕事なので、聡君も乗せて良いですか？"},{"id":{"d":936,"n":8,"t":2},"speaker":"B","text":"良いですよ。聡君も彼の自宅まで送りますね。雨の当たらないところにいてください。"},{"id":{"d":936,"n":9,"t":2},"speaker":"A","text":"では、昇降口で待っています。"},{"id":{"d":936,"n":10,"t":2},"speaker":"B","text":"分かりました。15分くらいで着きます。"}];
INSERT INTO sentence [{"id":{"d":937,"n":1,"t":2},"speaker":"A","text":"合格祝いで、母に東京ディズニーランドへ連れて行ってもらえることになりました。"},{"id":{"d":937,"n":2,"t":2},"speaker":"B","text":"良いですね。楽しんできてください。"},{"id":{"d":937,"n":3,"t":2},"speaker":"A","text":"琴子ちゃんも一緒にどうですか？"},{"id":{"d":937,"n":4,"t":2},"speaker":"B","text":"はい。私も一緒に行って良いのですか？"},{"id":{"d":937,"n":5,"t":2},"speaker":"A","text":"最初から誘うつもりでした。中学校は別々の学校へ進学することになってしまうので、琴子ちゃんと思い出作りをしたくて。"},{"id":{"d":937,"n":6,"t":2},"speaker":"B","text":"うれしいです。私も桜子ちゃんと思い出を作りたいと思っていたので、ぜひ行きたいです。"},{"id":{"d":937,"n":7,"t":2},"speaker":"A","text":"良かったです。琴子ちゃんのお母さまと4人で行きましょう。"},{"id":{"d":937,"n":8,"t":2},"speaker":"B","text":"今日帰ったら母に相談してみます。日にちは決まっていますか？"},{"id":{"d":937,"n":9,"t":2},"speaker":"A","text":"卒業式の後に行こうと思っていました。3月25日はどうですか？"},{"id":{"d":937,"n":10,"t":2},"speaker":"B","text":"良いですね。では、3月25日のお母さんの予定を聞いてきます。"}];
INSERT INTO sentence [{"id":{"d":938,"n":1,"t":2},"speaker":"A","text":"入学式の記念に、家の前で写真を撮りましょう。"},{"id":{"d":938,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。立つ場所はここでいいですか？"},{"id":{"d":938,"n":3,"t":2},"speaker":"A","text":"はい。良い感じです。でも、制服のリボンが少し曲がっていますよ。"},{"id":{"d":938,"n":4,"t":2},"speaker":"B","text":"今、直します。これでどうでしょうか？"},{"id":{"d":938,"n":5,"t":2},"speaker":"A","text":"良いですね。かばんを持っているのと持っていないものの2通りを撮りましょう。"},{"id":{"d":938,"n":6,"t":2},"speaker":"B","text":"はい。でも、急がないと入学式に遅刻してしまいます。"},{"id":{"d":938,"n":7,"t":2},"speaker":"A","text":"分かりました。では、まずはかばんを持って撮りますよ。はい、チーズ。"}];
INSERT INTO sentence [{"id":{"d":939,"n":1,"t":2},"speaker":"A","text":"大学はどちらに進学されましたか？"},{"id":{"d":939,"n":2,"t":2},"speaker":"B","text":"東京の大学に通っていましたが、3年で中退しました。"},{"id":{"d":939,"n":3,"t":2},"speaker":"A","text":"大学を中退して、何か挑戦したいことがありましたか？"},{"id":{"d":939,"n":4,"t":2},"speaker":"B","text":"3年のときに劇団のオーディションに受かったのです。それをきっかけに大学を辞めてしまいました。"},{"id":{"d":939,"n":5,"t":2},"speaker":"A","text":"俳優を目指して、大学を辞められたのですね。"},{"id":{"d":939,"n":6,"t":2},"speaker":"B","text":"そう言うと格好良いですが、ちょうど勉強が嫌になっていた時期だったので、オーディション合格を良い理由にして勉強から逃げたのかもしれません。"},{"id":{"d":939,"n":7,"t":2},"speaker":"A","text":"そうでしたか。今は俳優のお仕事をされているのですか？"},{"id":{"d":939,"n":8,"t":2},"speaker":"B","text":"いいえ。残念ながらそれほど俳優の仕事はありません。アルバイトで生計を立てています。"}];
INSERT INTO sentence [{"id":{"d":940,"n":1,"t":2},"speaker":"A","text":"今年の成績の評価はオール優でした。"},{"id":{"d":940,"n":2,"t":2},"speaker":"B","text":"素晴しいことではありませんか。"},{"id":{"d":940,"n":3,"t":2},"speaker":"A","text":"これで素晴らしいと言えるのでしょうか？"},{"id":{"d":940,"n":4,"t":2},"speaker":"B","text":"「優」は一番上でしょう？"},{"id":{"d":940,"n":5,"t":2},"speaker":"A","text":"違いますよ。「秀」が一番上です。"},{"id":{"d":940,"n":6,"t":2},"speaker":"B","text":"私の大学と違いますね。私のところは「優」が一番上です。"},{"id":{"d":940,"n":7,"t":2},"speaker":"A","text":"そうなのですね。道理であなたがとても驚いていたはずです。"},{"id":{"d":940,"n":8,"t":2},"speaker":"B","text":"ええ、とても驚きました。私たちの大学ならオール優は主席ですよ。"}];
INSERT INTO sentence [{"id":{"d":941,"n":1,"t":2},"speaker":"A","text":"お母さん、そろばんの珠算能力検定の受験料を下さい。"},{"id":{"d":941,"n":2,"t":2},"speaker":"B","text":"今準備しますね。今度受けるのは3級の検定試験ですか？"},{"id":{"d":941,"n":3,"t":2},"speaker":"A","text":"はい、そうです。"},{"id":{"d":941,"n":4,"t":2},"speaker":"B","text":"素晴しいことですね。自信のほどはどうですか？"},{"id":{"d":941,"n":5,"t":2},"speaker":"A","text":"先生は「この調子なら3級でも受かるだろう」と言ってくれたので、それを信じて頑張ります。"},{"id":{"d":941,"n":6,"t":2},"speaker":"B","text":"それは素晴らしいです。受かったらお祝いにお寿司を食べに行きましょうか？"},{"id":{"d":941,"n":7,"t":2},"speaker":"A","text":"はい。うれしいです。ますます頑張ろうという気持ちになりました。"},{"id":{"d":941,"n":8,"t":2},"speaker":"B","text":"頑張ってくださいね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":942,"n":1,"t":2},"speaker":"A","text":"高校ではどの部活動をするか決めましたか？"},{"id":{"d":942,"n":2,"t":2},"speaker":"B","text":"はい。私は帰宅部にしようと思います。"},{"id":{"d":942,"n":3,"t":2},"speaker":"A","text":"部活に入らないということですか？"},{"id":{"d":942,"n":4,"t":2},"speaker":"B","text":"そうです。"},{"id":{"d":942,"n":5,"t":2},"speaker":"A","text":"他にやりたいことがあるのですか？"},{"id":{"d":942,"n":6,"t":2},"speaker":"B","text":"中学までやっていたブレイクダンス部がないので、学外のダンススタジオに通うことにしたのです。"},{"id":{"d":942,"n":7,"t":2},"speaker":"A","text":"自分で部を作ることは考えなかったのですか？"},{"id":{"d":942,"n":8,"t":2},"speaker":"B","text":"考えました。それで、掲示板などで呼び掛けてみたのですが人数が集まらず、諦めました。"}];
INSERT INTO sentence [{"id":{"d":943,"n":1,"t":2},"speaker":"A","text":"徹君、これほど遅い時間にどうしたのですか？"},{"id":{"d":943,"n":2,"t":2},"speaker":"B","text":"こんばんは。母に「今日は塾の帰りのお迎えが少し遅くなる」と言われたので、ここで母を待たせてもらえますか？"},{"id":{"d":943,"n":3,"t":2},"speaker":"A","text":"「ここで待つように」とお母さまが言ったのですか？"},{"id":{"d":943,"n":4,"t":2},"speaker":"B","text":"はい。「博おじさんのところなら安心だから」と母が言っていました。"},{"id":{"d":943,"n":5,"t":2},"speaker":"A","text":"それならお母さまに連絡をしなくても大丈夫ですね。おなかがすいているでしょう？イカ焼きでも食べますか？"},{"id":{"d":943,"n":6,"t":2},"speaker":"B","text":"いただきます。"},{"id":{"d":943,"n":7,"t":2},"speaker":"A","text":"はい、どうぞ。サイダーもいかがですか？"},{"id":{"d":943,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。おじさん、ここで宿題をしてもよいですか？"},{"id":{"d":943,"n":9,"t":2},"speaker":"A","text":"構いませんよ。"}];
INSERT INTO sentence [{"id":{"d":944,"n":1,"t":2},"speaker":"A","text":"お父さん、風香の本のこの付録は僕でも作れません。"},{"id":{"d":944,"n":2,"t":2},"speaker":"B","text":"風香の本ということは、これは幼稚園生向けのものなのでしょう？"},{"id":{"d":944,"n":3,"t":2},"speaker":"A","text":"そのはずですが、これは小学生の僕にも難しすぎます。"},{"id":{"d":944,"n":4,"t":2},"speaker":"B","text":"見せてください。なるほど、これは確かに随分と手の込んだ器械ですね。"},{"id":{"d":944,"n":5,"t":2},"speaker":"A","text":"お父さんには作れますか？"},{"id":{"d":944,"n":6,"t":2},"speaker":"B","text":"どうでしょうか、お父さんは不器用なのであまり自信がありません。"},{"id":{"d":944,"n":7,"t":2},"speaker":"A","text":"お父さん、頑張って作ってください。僕も風香もこれで遊びたいのです。"},{"id":{"d":944,"n":8,"t":2},"speaker":"B","text":"とりあえずやってみますが、風香も智もあまり期待した目で見ないでください。"}];
INSERT INTO sentence [{"id":{"d":945,"n":1,"t":2},"speaker":"A","text":"持ってきた水筒のお茶を、午前中に全部飲んでしまいました。"},{"id":{"d":945,"n":2,"t":2},"speaker":"B","text":"2時間目の体育がとても暑かったですからね。"},{"id":{"d":945,"n":3,"t":2},"speaker":"A","text":"はい。喉が渇いて、お茶を一気に飲み干してしまいました。"},{"id":{"d":945,"n":4,"t":2},"speaker":"B","text":"私もあと1口くらいしか残っていません。"},{"id":{"d":945,"n":5,"t":2},"speaker":"A","text":"給食の冷たい牛乳が待ち遠しいです。"},{"id":{"d":945,"n":6,"t":2},"speaker":"B","text":"給食まで待たなくとも、水道のお水を水筒に入れてきたらどうですか？"},{"id":{"d":945,"n":7,"t":2},"speaker":"A","text":"水道の水は冷たくないので好きではありませんが、どうしても喉が渇いたときにはそうします。"},{"id":{"d":945,"n":8,"t":2},"speaker":"B","text":"水が飲みたいのを、我慢するのはよくありませんよ。"}];
INSERT INTO sentence [{"id":{"d":946,"n":1,"t":2},"speaker":"A","text":"こちらの幼稚園は園庭が広いのですね。"},{"id":{"d":946,"n":2,"t":2},"speaker":"B","text":"はい。こちらの園庭は、子どもたちが走り回れる十分な広さがあります。"},{"id":{"d":946,"n":3,"t":2},"speaker":"A","text":"芝生もきれいです。"},{"id":{"d":946,"n":4,"t":2},"speaker":"B","text":"はだしで走ると気持ちが良いのですよ。"},{"id":{"d":946,"n":5,"t":2},"speaker":"A","text":"先生もはだしなのですか？"},{"id":{"d":946,"n":6,"t":2},"speaker":"B","text":"はい。私たちも子どもたちと一緒に毎日走っています。"},{"id":{"d":946,"n":7,"t":2},"speaker":"A","text":"他の施設も素晴らしいです。ぜひ、こちらに入園させたいです。"},{"id":{"d":946,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。では、事務室で入園関係書類をお受け取りください。"}];
INSERT INTO sentence [{"id":{"d":947,"n":1,"t":2},"speaker":"A","text":"野球部が順調に県大会を勝ち進んでいますね。"},{"id":{"d":947,"n":2,"t":2},"speaker":"B","text":"はい。次の試合は土曜日だそうですが、応援に行きますか？"},{"id":{"d":947,"n":3,"t":2},"speaker":"A","text":"土曜日は、もう準決勝でしたよね。"},{"id":{"d":947,"n":4,"t":2},"speaker":"B","text":"そうです。でも相手は強豪校の野球部ですから、私たちの野球部は、これが最後の試合になってしまうかもしれませんね。"},{"id":{"d":947,"n":5,"t":2},"speaker":"A","text":"そのようなことを言われたら、応援に行かないわけにはいきません。"},{"id":{"d":947,"n":6,"t":2},"speaker":"B","text":"私も土曜日は応援に行くつもりですので、一緒に行きましょう。"},{"id":{"d":947,"n":7,"t":2},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":948,"n":1,"t":2},"speaker":"A","text":"あら、由紀がピアニカを忘れています。パパ、由紀の部屋からピアニカを取ってきてくれますか？"},{"id":{"d":948,"n":2,"t":2},"speaker":"B","text":"分かりました。ところで、ピアニカとは何のことですか？"},{"id":{"d":948,"n":3,"t":2},"speaker":"A","text":"鍵盤のついた楽器ですよ。ピアニカを知りませんか？"},{"id":{"d":948,"n":4,"t":2},"speaker":"B","text":"はい。初めて聞きました。"},{"id":{"d":948,"n":5,"t":2},"speaker":"A","text":"本当ですか？小学校でピアニカの演奏法を習いませんでしたか？口で息を吹き込んで、鍵盤を弾く楽器ですよ。"},{"id":{"d":948,"n":6,"t":2},"speaker":"B","text":"もしかして、メロディオンのことですか？"},{"id":{"d":948,"n":7,"t":2},"speaker":"A","text":"私はメロディオンが分かりません。初めて聞きました。"},{"id":{"d":948,"n":8,"t":2},"speaker":"B","text":"そうなのですか？でも、話を聞く限りピアニカとメロディオンはどうやら同じ楽器のようです。それを探して持ってきますね。"},{"id":{"d":948,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":949,"n":1,"t":2},"speaker":"A","text":"通学に片道2時間はかかるので、1限の授業を受けるには6時には家を出ないといけませんね。"},{"id":{"d":949,"n":2,"t":2},"speaker":"B","text":"それほど遠くに住んでいるのですか？"},{"id":{"d":949,"n":3,"t":2},"speaker":"A","text":"はい。実家からだと、どうしても2時間はかかるのです。"},{"id":{"d":949,"n":4,"t":2},"speaker":"B","text":"大学の近くで1人暮らしをしようとは思いませんか？"},{"id":{"d":949,"n":5,"t":2},"speaker":"A","text":"はい。通えない距離ではありませんし、何より大学の近くは家賃も高いですから。"},{"id":{"d":949,"n":6,"t":2},"speaker":"B","text":"通学時間は何をしているのですか？"},{"id":{"d":949,"n":7,"t":2},"speaker":"A","text":"読書をしたり音楽を聞いたりしています。少し疲れたときは通学時間に仮眠を取ることもあります。"},{"id":{"d":949,"n":8,"t":2},"speaker":"B","text":"通学時間を有意義に使えるのは良いことですね。"}];
INSERT INTO sentence [{"id":{"d":950,"n":1,"t":2},"speaker":"A","text":"今日は、真美ちゃんと一緒に宿題をする約束をしました。"},{"id":{"d":950,"n":2,"t":2},"speaker":"B","text":"どこで勉強をする約束をしたのですか？"},{"id":{"d":950,"n":3,"t":2},"speaker":"A","text":"真美ちゃんのおうちです。"},{"id":{"d":950,"n":4,"t":2},"speaker":"B","text":"では、これから宿題を持って真美ちゃんのおうちへ行くのですね？"},{"id":{"d":950,"n":5,"t":2},"speaker":"A","text":"はい。ランドセルは家に置いて、宿題だけを手提げに入れて行きます。"},{"id":{"d":950,"n":6,"t":2},"speaker":"B","text":"それが良いですね。カップケーキを焼いたので、もし良ければおやつとして持って行ってください。"},{"id":{"d":950,"n":7,"t":2},"speaker":"A","text":"真美ちゃんの分もありますか？"},{"id":{"d":950,"n":8,"t":2},"speaker":"B","text":"もちろんです。たくさん包みましたので、真美ちゃんのご家族の方の分もあると思いますよ。"},{"id":{"d":950,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。食べることが楽しみです。"},{"id":{"d":950,"n":10,"t":2},"speaker":"B","text":"気を付けて行ってきてくださいね。"},{"id":{"d":950,"n":11,"t":2},"speaker":"A","text":"はい。行ってきます。"}];
INSERT INTO sentence [{"id":{"d":951,"n":1,"t":2},"speaker":"A","text":"おかえりなさい。今日から春休みですね。"},{"id":{"d":951,"n":2,"t":2},"speaker":"B","text":"ただいま帰りました。はい、これが今期の成績表です。"},{"id":{"d":951,"n":3,"t":2},"speaker":"A","text":"今手が離せないので、もう少ししたらそちらへ行きますね。成績は上がりましたか？"},{"id":{"d":951,"n":4,"t":2},"speaker":"B","text":"はい。少しだけですけれど上がりました。"},{"id":{"d":951,"n":5,"t":2},"speaker":"A","text":"それは、素晴らしいですね。"},{"id":{"d":951,"n":6,"t":2},"speaker":"B","text":"見てください。特に頑張った国語は成績が2段階も上がりました。"},{"id":{"d":951,"n":7,"t":2},"speaker":"A","text":"まあ、本当ですね。おめでとうございます。"},{"id":{"d":951,"n":8,"t":2},"speaker":"B","text":"ありがとうございます、うれしいです。お父さんが帰ってきたら、お父さんにも通信簿を見せたいです。"},{"id":{"d":951,"n":9,"t":2},"speaker":"A","text":"そうしてください。「今日は早く帰ってくる」と言っていましたよ。"}];
INSERT INTO sentence [{"id":{"d":952,"n":1,"t":2},"speaker":"A","text":"今日は美優が「行きたい」と言っていた、図書館の読み聞かせの会に行ってきますね。"},{"id":{"d":952,"n":2,"t":2},"speaker":"B","text":"美優は本当に絵本が好きですね。"},{"id":{"d":952,"n":3,"t":2},"speaker":"A","text":"そうですね。図書館司書の方の読み聞かせが、またとても上手なのですよ。"},{"id":{"d":952,"n":4,"t":2},"speaker":"B","text":"それで絵本に夢中になっているのですね。"},{"id":{"d":952,"n":5,"t":2},"speaker":"A","text":"はい。このまま本好きの子になってくれると良いですね。"},{"id":{"d":952,"n":6,"t":2},"speaker":"B","text":"もうすぐ小学校ですからね。文字を読む練習にもなるし、本が好きなのは良いことです。"},{"id":{"d":952,"n":7,"t":2},"speaker":"A","text":"平仮名はもう読めるので、今日も何冊か新しい本を借りてくる予定です。"}];
INSERT INTO sentence [{"id":{"d":953,"n":1,"t":2},"speaker":"A","text":"頼まれていた新しい消しゴムを買ってきましたよ。"},{"id":{"d":953,"n":2,"t":2},"speaker":"B","text":"お母さん、ありがとうございます。"},{"id":{"d":953,"n":3,"t":2},"speaker":"A","text":"「小学生学習用」と書いてあるものを選んできましたが、これで大丈夫ですか？"},{"id":{"d":953,"n":4,"t":2},"speaker":"B","text":"大きくて長持ちしそうですが、あまりかわいくありません。"},{"id":{"d":953,"n":5,"t":2},"speaker":"A","text":"そうかもしれませんが、これはよく消える消しゴムだそうですよ。勉強にはぴったりです。"},{"id":{"d":953,"n":6,"t":2},"speaker":"B","text":"そうなのですか？"},{"id":{"d":953,"n":7,"t":2},"speaker":"A","text":"宿題をしているのなら、早速使ってみてくださいよ。"},{"id":{"d":953,"n":8,"t":2},"speaker":"B","text":"分かりました。わあ、素晴らしいことです。2Bの鉛筆で書いた字が簡単に消えます。"},{"id":{"d":953,"n":9,"t":2},"speaker":"A","text":"そうでしょう？"},{"id":{"d":953,"n":10,"t":2},"speaker":"B","text":"はい。使いやすくて気に入りました。"},{"id":{"d":953,"n":11,"t":2},"speaker":"A","text":"それは良かったです。大事に使ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":954,"n":1,"t":2},"speaker":"A","text":"朝の通勤ラッシュには全く慣れません。"},{"id":{"d":954,"n":2,"t":2},"speaker":"B","text":"初めての電車通学ですからね。"},{"id":{"d":954,"n":3,"t":2},"speaker":"A","text":"超満員の電車で通うのは大変です。"},{"id":{"d":954,"n":4,"t":2},"speaker":"B","text":"では、空いている時間に通学時間を変えたらいいと思いますよ。"},{"id":{"d":954,"n":5,"t":2},"speaker":"A","text":"これ以上、電車の時間を遅くしたら遅刻してしまいます。"},{"id":{"d":954,"n":6,"t":2},"speaker":"B","text":"早くするに決まっているではありませんか？"},{"id":{"d":954,"n":7,"t":2},"speaker":"A","text":"それは考えもしませんでした。早く起きるのも同じくらい大変です。"},{"id":{"d":954,"n":8,"t":2},"speaker":"B","text":"超満員電車に乗るか早起きするか、どちらかを選ぶことになりますね。"},{"id":{"d":954,"n":9,"t":2},"speaker":"A","text":"私にとっては難しい選択です。よく考えてみます。"}];
INSERT INTO sentence [{"id":{"d":955,"n":1,"t":2},"speaker":"A","text":"最近になって、中高校生向けのライトノベルに夢中になっているのです。"},{"id":{"d":955,"n":2,"t":2},"speaker":"B","text":"良いではありませんか。中高校生の頃には読まなかったのですか？"},{"id":{"d":955,"n":3,"t":2},"speaker":"A","text":"高校生までは全くと言って良いほど本を読みませんでした。読書も苦手でした。"},{"id":{"d":955,"n":4,"t":2},"speaker":"B","text":"大学ではさすがに本を読んだのですね。"},{"id":{"d":955,"n":5,"t":2},"speaker":"A","text":"レポートのための本がほとんどでしたけれどね。"},{"id":{"d":955,"n":6,"t":2},"speaker":"B","text":"社会人になってから、ライトノベルには夢中になったのはどうしてですか？"},{"id":{"d":955,"n":7,"t":2},"speaker":"A","text":"いとこが読んでいるものを貸してくれたのですが、それが面白くて衝撃を受けました。思わず、読み終わってすぐに、ライトノベルの続きの巻を買いに、本屋へ走ってしまうほどでした。"},{"id":{"d":955,"n":8,"t":2},"speaker":"B","text":"良い本と出会えたのですね。"},{"id":{"d":955,"n":9,"t":2},"speaker":"A","text":"はい。読書の楽しさを教えてくれたいとこには感謝です。"}];
INSERT INTO sentence [{"id":{"d":956,"n":1,"t":2},"speaker":"A","text":"今日は学校で何をしてきたのですか？"},{"id":{"d":956,"n":2,"t":2},"speaker":"B","text":"学校の国語の授業では、教科書を大きな声で読みました。"},{"id":{"d":956,"n":3,"t":2},"speaker":"A","text":"先生に指されたのですか？"},{"id":{"d":956,"n":4,"t":2},"speaker":"B","text":"はい。立ってみんなの前で教科書を読みました。"},{"id":{"d":956,"n":5,"t":2},"speaker":"A","text":"つかえずに読めましたか？"},{"id":{"d":956,"n":6,"t":2},"speaker":"B","text":"いくつか読めない漢字があって止まってしまいましたが、先生は「上手ですね」と褒めてくれました。"},{"id":{"d":956,"n":7,"t":2},"speaker":"A","text":"それは良かったですね。読めなかった漢字は、次のテストまでに覚えましょうね。"},{"id":{"d":956,"n":8,"t":2},"speaker":"B","text":"はい、頑張って覚えます。"}];
INSERT INTO sentence [{"id":{"d":957,"n":1,"t":2},"speaker":"A","text":"喜美ちゃん、九九の六の段は言えますか？"},{"id":{"d":957,"n":2,"t":2},"speaker":"B","text":"はい。ろくいちがろく、ろくにじゅうに、ろくさんじゅうはち。"},{"id":{"d":957,"n":3,"t":2},"speaker":"A","text":"その次はどうですか？"},{"id":{"d":957,"n":4,"t":2},"speaker":"B","text":"ろくしにじゅうし。ろくごさんじゅう。"},{"id":{"d":957,"n":5,"t":2},"speaker":"A","text":"素晴しいですね。"},{"id":{"d":957,"n":6,"t":2},"speaker":"B","text":"最後まで言えますよ。私は、どこまで言いましたか？"},{"id":{"d":957,"n":7,"t":2},"speaker":"A","text":"「ろくごさんじゅう」までです。"},{"id":{"d":957,"n":8,"t":2},"speaker":"B","text":"では、ろくろくさんじゅうろく、ろくしちしじゅうに、ろくはしじゅうはち、ろっくごじゅうし。"},{"id":{"d":957,"n":9,"t":2},"speaker":"A","text":"素晴らしいです。九九は得意ですか？"},{"id":{"d":957,"n":10,"t":2},"speaker":"B","text":"はい。算数は好きですし得意です。"}];
INSERT INTO sentence [{"id":{"d":958,"n":1,"t":2},"speaker":"A","text":"おはようございます。今朝はいつもより早い出勤なのですね。"},{"id":{"d":958,"n":2,"t":2},"speaker":"B","text":"おはようございます。今日は仕事が休みです。娘の運動会の場所取りに行くのですよ。"},{"id":{"d":958,"n":3,"t":2},"speaker":"A","text":"そうでした、今日は日曜日でしたね。これほどに早い時間から場所取りをするのですか？"},{"id":{"d":958,"n":4,"t":2},"speaker":"B","text":"はい。娘の勇姿をビデオに撮るために、撮影しやすい場所を取らなければいけませんからね。"},{"id":{"d":958,"n":5,"t":2},"speaker":"A","text":"お父さんたちも大変ですね。"},{"id":{"d":958,"n":6,"t":2},"speaker":"B","text":"そうですね。でも、娘のためなら早起きも頑張れますよ。"},{"id":{"d":958,"n":7,"t":2},"speaker":"A","text":"奥さまと一緒には行かれませんね？"},{"id":{"d":958,"n":8,"t":2},"speaker":"B","text":"はい。妻は娘と一緒に後から来ます。今は張り切ってお弁当を作っていますよ。"},{"id":{"d":958,"n":9,"t":2},"speaker":"A","text":"昼食が待ち遠しいですね。"},{"id":{"d":958,"n":10,"t":2},"speaker":"B","text":"ええ。それでは学校へ向かいますので、失礼します。"}];
INSERT INTO sentence [{"id":{"d":959,"n":1,"t":2},"speaker":"A","text":"美晴ちゃんは運動部に入ったのですか？"},{"id":{"d":959,"n":2,"t":2},"speaker":"B","text":"いえ、私の入った部は運動部ではありませんよ。かるた部に入りました。"},{"id":{"d":959,"n":3,"t":2},"speaker":"A","text":"放課後になるとジャージに着替えているので、運動部に入ったのかと思いました。"},{"id":{"d":959,"n":4,"t":2},"speaker":"B","text":"かるた部は文化部といいながら、行うことは運動部のようですからね。"},{"id":{"d":959,"n":5,"t":2},"speaker":"A","text":"どのような意味ですか？"},{"id":{"d":959,"n":6,"t":2},"speaker":"B","text":"札が読み上げられたら、相手よりも早く正確に札を取らなければいけません。競技かるたは筋力と瞬発力が勝負の世界です。"},{"id":{"d":959,"n":7,"t":2},"speaker":"A","text":"そうだったのですか。では、普段の部活でも筋力トレーニングなどをしているのですか？"},{"id":{"d":959,"n":8,"t":2},"speaker":"B","text":"もちろん、毎日していますよ。ジョギングもしています。"},{"id":{"d":959,"n":9,"t":2},"speaker":"A","text":"驚きました。それは、確かに運動部のようですね。"}];
INSERT INTO sentence [{"id":{"d":960,"n":1,"t":2},"speaker":"A","text":"帰り道で、制服の帽子が風に飛ばされてしまいました。"},{"id":{"d":960,"n":2,"t":2},"speaker":"B","text":"今日は風が強かったですからね。取りに行けないところまで飛んでしまったのですか？"},{"id":{"d":960,"n":3,"t":2},"speaker":"A","text":"はい。川に落ちてしまいました。"},{"id":{"d":960,"n":4,"t":2},"speaker":"B","text":"それでは明日、新しい帽子を学校で買いましょうね。"},{"id":{"d":960,"n":5,"t":2},"speaker":"A","text":"明日、学校に行くときは帽子をかぶれませんか？"},{"id":{"d":960,"n":6,"t":2},"speaker":"B","text":"はい。予備の帽子を持っていないので、そうなります。他の帽子をかぶっていきますか？"},{"id":{"d":960,"n":7,"t":2},"speaker":"A","text":"いいえ。他の帽子は嫌です。みんなと一緒の制服の帽子が良いです。"},{"id":{"d":960,"n":8,"t":2},"speaker":"B","text":"では、朝は帽子をかぶらずに登校しましょう。学校に着いたらすぐに買えば良いですね。"},{"id":{"d":960,"n":9,"t":2},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":961,"n":1,"t":2},"speaker":"A","text":"この問題の答えは分かりますか？"},{"id":{"d":961,"n":2,"t":2},"speaker":"B","text":"いいえ。今、調べてみますね。"},{"id":{"d":961,"n":3,"t":2},"speaker":"A","text":"調べて分かるものなのですか？"},{"id":{"d":961,"n":4,"t":2},"speaker":"B","text":"はい。教科書ガイドを買いましたから。"},{"id":{"d":961,"n":5,"t":2},"speaker":"A","text":"教科書ガイドを見れば、教科書の問題の答えが分かるのですか？"},{"id":{"d":961,"n":6,"t":2},"speaker":"B","text":"そうです。問題の答えと解説も書いてあるのですよ。"},{"id":{"d":961,"n":7,"t":2},"speaker":"A","text":"便利な本があるのですね。"}];
INSERT INTO sentence [{"id":{"d":962,"n":1,"t":2},"speaker":"A","text":"今日の放課後、一緒に遊びませんか？"},{"id":{"d":962,"n":2,"t":2},"speaker":"B","text":"残念ながら、遊べません。今日の放課後はピアノの練習に行きます。"},{"id":{"d":962,"n":3,"t":2},"speaker":"A","text":"そうですか。では明日はどうですか？"},{"id":{"d":962,"n":4,"t":2},"speaker":"B","text":"ごめんなさい。明日もピアノの練習があるのです。"},{"id":{"d":962,"n":5,"t":2},"speaker":"A","text":"それでは、あさってはどうですか？"},{"id":{"d":962,"n":6,"t":2},"speaker":"B","text":"あさっても同じです。私は毎日、ピアノの練習に行かなければなりません。"}];
INSERT INTO sentence [{"id":{"d":963,"n":1,"t":2},"speaker":"A","text":"まだ入るクラブを決めていないようでしたら、私たちのクラブに入りませんか？"},{"id":{"d":963,"n":2,"t":2},"speaker":"B","text":"はい。内容をお伺いしてから考えたいと思います。何のクラブですか？"},{"id":{"d":963,"n":3,"t":2},"speaker":"A","text":"合唱クラブです。"},{"id":{"d":963,"n":4,"t":2},"speaker":"B","text":"僕は歌があまり得意ではありません。"},{"id":{"d":963,"n":5,"t":2},"speaker":"A","text":"得意ではなくとも歌が好きなら問題ありません。クラブで練習すれば誰でも上手になりますよ。"},{"id":{"d":963,"n":6,"t":2},"speaker":"B","text":"どのようなジャンルの歌を歌っているのですか？"},{"id":{"d":963,"n":7,"t":2},"speaker":"A","text":"クラシックからポップソングまで、いろいろなジャンルの曲を歌います。"},{"id":{"d":963,"n":8,"t":2},"speaker":"B","text":"楽しそうです。少し考えさせてください。"},{"id":{"d":963,"n":9,"t":2},"speaker":"A","text":"分かりました。決心がついたら連絡をしてください。"}];
INSERT INTO sentence [{"id":{"d":964,"n":1,"t":2},"speaker":"A","text":"期末テストの準備はできましたか？"},{"id":{"d":964,"n":2,"t":2},"speaker":"B","text":"いえ、全くできていません。あなたはどうですか？"},{"id":{"d":964,"n":3,"t":2},"speaker":"A","text":"私もできていません。これから、大急ぎで行わなくてはなりません。"},{"id":{"d":964,"n":4,"t":2},"speaker":"B","text":"一緒に取り組みませんか？私は数学が苦手なので、教えてほしいのです。"},{"id":{"d":964,"n":5,"t":2},"speaker":"A","text":"数学ならお安い御用ですよ。私は国語が苦手なので、教えてほしいです。"},{"id":{"d":964,"n":6,"t":2},"speaker":"B","text":"私は国語なら得意なのでお任せください。早速、今日から始めましょう。"},{"id":{"d":964,"n":7,"t":2},"speaker":"A","text":"良いですね。場所はどうしますか？"},{"id":{"d":964,"n":8,"t":2},"speaker":"B","text":"私の家はどうでしょうか？"},{"id":{"d":964,"n":9,"t":2},"speaker":"A","text":"良いのですか？では、授業が終わったらお邪魔しますね。"},{"id":{"d":964,"n":10,"t":2},"speaker":"B","text":"はい。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":965,"n":1,"t":2},"speaker":"A","text":"学校のクラブ活動が変わるそうです。"},{"id":{"d":965,"n":2,"t":2},"speaker":"B","text":"どのようなふうに変わるのですか？"},{"id":{"d":965,"n":3,"t":2},"speaker":"A","text":"さらに地域の力を借りる方向に変わるのです。"},{"id":{"d":965,"n":4,"t":2},"speaker":"B","text":"具体的にはどうなるのですか？"},{"id":{"d":965,"n":5,"t":2},"speaker":"A","text":"クラブ活動のコーチを、地域のスポーツクラブのトレーナーや専門家の方に依頼するそうです。"},{"id":{"d":965,"n":6,"t":2},"speaker":"B","text":"なるほど。そうなると、先生の負担も減りますね。"},{"id":{"d":965,"n":7,"t":2},"speaker":"A","text":"それが目的なのです。今のままでは先生の負担が大きすぎて、過労で倒れてしまう人もいるようです。"},{"id":{"d":965,"n":8,"t":2},"speaker":"B","text":"それはよくありませんね。クラブ活動のコーチを、さらに早く地域に委ねることはできなかったのでしょうか？"},{"id":{"d":965,"n":9,"t":2},"speaker":"A","text":"人材が確保できるかどうかや、費用をどうするかなど、いろいろ問題が存在したようですよ。"},{"id":{"d":965,"n":10,"t":2},"speaker":"B","text":"今回はそれらが解消できたということですね。"}];
INSERT INTO sentence [{"id":{"d":966,"n":1,"t":2},"speaker":"A","text":"原田君は「歯科技工士になりたい」と言っていましたね。"},{"id":{"d":966,"n":2,"t":2},"speaker":"B","text":"はい、そのつもりで専門学校を探しているのですが、なかなか良い学校が見つかりません。"},{"id":{"d":966,"n":3,"t":2},"speaker":"A","text":"私の知っている学校で、東京歯科専門学校という専門学校があります。そこに歯科技工士学科があります。"},{"id":{"d":966,"n":4,"t":2},"speaker":"B","text":"初めて聞きました。良い学校ですか？"},{"id":{"d":966,"n":5,"t":2},"speaker":"A","text":"はい、とても良い学校ですよ。3年制の学校で、必要な知識をじっくりと学ぶことができます。"},{"id":{"d":966,"n":6,"t":2},"speaker":"B","text":"技術をしっかり身につけたいので、実習が大事だと思うのですが、それはどうですか？"},{"id":{"d":966,"n":7,"t":2},"speaker":"A","text":"授業の7割が実習で、少人数クラス・習熟度別教育をしているので、技術もしっかり学べるそうです。"},{"id":{"d":966,"n":8,"t":2},"speaker":"B","text":"良さそうですね。パンフレットを取り寄せて検討してみます。"}];
INSERT INTO sentence [{"id":{"d":967,"n":1,"t":2},"speaker":"A","text":"あなたのクラブでは、先輩と後輩の関係はどうですか？"},{"id":{"d":967,"n":2,"t":2},"speaker":"B","text":"割とうまく行っている方だと思います。"},{"id":{"d":967,"n":3,"t":2},"speaker":"A","text":"先輩が後輩に対して厳しく当たるというようなことはありませんか？"},{"id":{"d":967,"n":4,"t":2},"speaker":"B","text":"はい。先輩が後輩を叱ることはよくありますよ。"},{"id":{"d":967,"n":5,"t":2},"speaker":"A","text":"それで後輩が反発することはありませんか？"},{"id":{"d":967,"n":6,"t":2},"speaker":"B","text":"先輩が後輩を叱る際には、叱るだけの理由があるので、後輩が反発することはありません。"},{"id":{"d":967,"n":7,"t":2},"speaker":"A","text":"先輩と後輩の関係がよほどうまく行っているのですね。"},{"id":{"d":967,"n":8,"t":2},"speaker":"B","text":"そう思います。このクラブに入って良かったと思っています。"}];
INSERT INTO sentence [{"id":{"d":968,"n":1,"t":2},"speaker":"A","text":"サッカー部が全国高校サッカー選手権大会に出場することになりましたね。"},{"id":{"d":968,"n":2,"t":2},"speaker":"B","text":"おめでたいですね。創部してから30年目の快挙ですね。"},{"id":{"d":968,"n":3,"t":2},"speaker":"A","text":"最近はずっと成績が低迷していたのに、どうして急に強くなったのでしょうね？"},{"id":{"d":968,"n":4,"t":2},"speaker":"B","text":"数年前に監督が変わり、今の監督になってから急に強くなりました。"},{"id":{"d":968,"n":5,"t":2},"speaker":"A","text":"監督が変わっただけでそれほど強くなるものでしょうか？"},{"id":{"d":968,"n":6,"t":2},"speaker":"B","text":"その監督の下でサッカーをしたいという選手が増え、今年は特に才能のある選手が集まったようです。"},{"id":{"d":968,"n":7,"t":2},"speaker":"A","text":"そのようなことでしたか。ところで、来週壮行会があるそうですが、あなたは出席しますか？"},{"id":{"d":968,"n":8,"t":2},"speaker":"B","text":"もちろんです。選手たちを激励したいと思います。"}];
INSERT INTO sentence [{"id":{"d":969,"n":1,"t":2},"speaker":"A","text":"先日の竜巻で体育館の屋根が壊れてしまい、当分体育館が使えなくなりました。"},{"id":{"d":969,"n":2,"t":2},"speaker":"B","text":"それは困ります。バスケットボール部の県大会が迫っているので、練習をしなければなりません。"},{"id":{"d":969,"n":3,"t":2},"speaker":"A","text":"今、顧問の先生が代わりの体育館を探しています。"},{"id":{"d":969,"n":4,"t":2},"speaker":"B","text":"うまく見つかるでしょうか？もし見つからなくて、練習ができなければ、出場できなくなるかもしれません。"},{"id":{"d":969,"n":5,"t":2},"speaker":"A","text":"心配しても仕方ありません。先生を信じましょう。必ずや見つけてくれますよ。"},{"id":{"d":969,"n":6,"t":2},"speaker":"B","text":"そうだと良いのですが。"},{"id":{"d":969,"n":7,"t":2},"speaker":"A","text":"例え見つからなくても、これまで一所懸命練習してきたのですから、出場はできますよ。"},{"id":{"d":969,"n":8,"t":2},"speaker":"B","text":"そうですね。チーム練習はできなくても、一人一人がトレーニングを続ければ良いですね。"}];
INSERT INTO sentence [{"id":{"d":970,"n":1,"t":2},"speaker":"A","text":"図書室の利用状況はどうですか？"},{"id":{"d":970,"n":2,"t":2},"speaker":"B","text":"生徒はあまり利用していないようです。"},{"id":{"d":970,"n":3,"t":2},"speaker":"A","text":"なぜでしょうね。理由は思い当たりますか？"},{"id":{"d":970,"n":4,"t":2},"speaker":"B","text":"一番大きな理由は生徒の興味を引くような本を置いていないということではないかと思います。"},{"id":{"d":970,"n":5,"t":2},"speaker":"A","text":"今の生徒はどのような本に興味を引かれるのでしょうか？"},{"id":{"d":970,"n":6,"t":2},"speaker":"B","text":"どうでしょうか。生徒も多様化していますからね。一概にこれだとは言い切れません。一度、生徒たちにアンケート調査をしてみてはいかがでしょうか？"},{"id":{"d":970,"n":7,"t":2},"speaker":"A","text":"それは良い考えですね。早速準備しましょう。"}];
INSERT INTO sentence [{"id":{"d":971,"n":1,"t":2},"speaker":"A","text":"先生、1年間休学をしたいのですが。"},{"id":{"d":971,"n":2,"t":2},"speaker":"B","text":"休学をして何をするのですか？"},{"id":{"d":971,"n":3,"t":2},"speaker":"A","text":"米国留学します。"},{"id":{"d":971,"n":4,"t":2},"speaker":"B","text":"米国に行って、何を勉強するのですか？"},{"id":{"d":971,"n":5,"t":2},"speaker":"A","text":"英語を勉強します。ネーティブスピーカーと同じくらいに英語を使えるようになりたいのです。"},{"id":{"d":971,"n":6,"t":2},"speaker":"B","text":"それは高校を卒業してからではいけませんか？"},{"id":{"d":971,"n":7,"t":2},"speaker":"A","text":"それでは遅すぎます。今のうちに英語を習得して、その後に備えたいのです。"},{"id":{"d":971,"n":8,"t":2},"speaker":"B","text":"確かに、それも一つの考え方ですね。分かりました、休学の手続きを始めましょう。"},{"id":{"d":971,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":972,"n":1,"t":2},"speaker":"A","text":"大学入試センター試験が近付いてきました。"},{"id":{"d":972,"n":2,"t":2},"speaker":"B","text":"はい、気付けばあと1カ月になりました。去年の試験日は大雪で交通網が混乱していましたね。"},{"id":{"d":972,"n":3,"t":2},"speaker":"A","text":"そうでしたね。今年は良い天気だといいのですが。"},{"id":{"d":972,"n":4,"t":2},"speaker":"B","text":"そうですね。"},{"id":{"d":972,"n":5,"t":2},"speaker":"A","text":"受験の準備はしっかりとできていますか？"},{"id":{"d":972,"n":6,"t":2},"speaker":"B","text":"はい。一通り勉強はしたのですが、あまり自信はありません。"},{"id":{"d":972,"n":7,"t":2},"speaker":"A","text":"自信のある人はめったにいませんよ。"},{"id":{"d":972,"n":8,"t":2},"speaker":"B","text":"私は非常に本番に弱いのです。"},{"id":{"d":972,"n":9,"t":2},"speaker":"A","text":"きちんと勉強したのならきっと大丈夫ですよ。気を強く持って頑張ってください。"},{"id":{"d":972,"n":10,"t":2},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":973,"n":1,"t":2},"speaker":"A","text":"今日、学校で体力テストがありました。お父さんのときもありましたか？"},{"id":{"d":973,"n":2,"t":2},"speaker":"B","text":"お父さんも小学校のときに体力テストを受けました。今はどのようなことをするのですか？"},{"id":{"d":973,"n":3,"t":2},"speaker":"A","text":"握力を計ったり、腹筋運動をしたりしました。反復横跳びや立ち幅跳び、短距離走や持久走もありました。"},{"id":{"d":973,"n":4,"t":2},"speaker":"B","text":"結果はどうでしたか？得意な50メートル走は何秒でしたか？"},{"id":{"d":973,"n":5,"t":2},"speaker":"A","text":"50メートル走は6秒でした。お父さんはどのくらいでしたか？"},{"id":{"d":973,"n":6,"t":2},"speaker":"B","text":"それは速いですね。お父さんは確か9秒くらいだったと思います。"},{"id":{"d":973,"n":7,"t":2},"speaker":"A","text":"お父さんは「あまり運動が得意ではない」と言っていますね。"},{"id":{"d":973,"n":8,"t":2},"speaker":"B","text":"はい。お母さんは運動が得意なので、祐介はお母さんに似たのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":974,"n":1,"t":2},"speaker":"A","text":"今日の給食のメニューは何でしたか？"},{"id":{"d":974,"n":2,"t":2},"speaker":"B","text":"クロスロールパン、牛乳、オムレツのきのこソースかけ、アスパラ入りクリームシチュー、ひじきと枝豆のサラダでした。"},{"id":{"d":974,"n":3,"t":2},"speaker":"A","text":"よく考えられたメニューですね。栄養がたくさん含まれていそうで、おいしそうです。"},{"id":{"d":974,"n":4,"t":2},"speaker":"B","text":"栄養士さんがしっかり考えてくれているようですよ。"},{"id":{"d":974,"n":5,"t":2},"speaker":"A","text":"生徒たちの評判はどうでしたか？みんな喜んで食べてくれたでしょうか？"},{"id":{"d":974,"n":6,"t":2},"speaker":"B","text":"はい。評判は良かったです。みんなおいしそうに食べていました。"},{"id":{"d":974,"n":7,"t":2},"speaker":"A","text":"ひじきと枝豆のサラダはどうでしたか？食べ残す子はいませんでしたか？"},{"id":{"d":974,"n":8,"t":2},"speaker":"B","text":"何人か食べ残した子はいましたけれど、多くの子はちゃんと食べたようでした。"},{"id":{"d":974,"n":9,"t":2},"speaker":"A","text":"子どもはひじきを食べたがらないのではないかと思いましたけれど、そうでもなかったのですね。"},{"id":{"d":974,"n":10,"t":2},"speaker":"B","text":"それは、子どもたちが食べやすいように味付けなどが工夫されているからかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":975,"n":1,"t":2},"speaker":"A","text":"吉田さんのクラスでは、学芸会で何をするのですか？"},{"id":{"d":975,"n":2,"t":2},"speaker":"B","text":"僕のクラスでは『注文の多い料理店』の劇をします。"},{"id":{"d":975,"n":3,"t":2},"speaker":"A","text":"宮沢賢治の童話のですか？"},{"id":{"d":975,"n":4,"t":2},"speaker":"B","text":"そうです。鈴木君が台本を書いて、演出もします。"},{"id":{"d":975,"n":5,"t":2},"speaker":"A","text":"吉田さんは何をするのですか？"},{"id":{"d":975,"n":6,"t":2},"speaker":"B","text":"僕は猟師の役をします。遠藤君がもう一人の猟師の役になります。"},{"id":{"d":975,"n":7,"t":2},"speaker":"A","text":"舞台装置や衣装なども生徒たちが自分で作るのですか？"},{"id":{"d":975,"n":8,"t":2},"speaker":"B","text":"もちろんです。クラスのみんなで作ります。"},{"id":{"d":975,"n":9,"t":2},"speaker":"A","text":"面白い劇が見られるのを楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":976,"n":1,"t":2},"speaker":"A","text":"校庭が掘り返されていますが、どうしたのですか？"},{"id":{"d":976,"n":2,"t":2},"speaker":"B","text":"校庭の一角で奈良時代の遺構らしいものが見つかったので、校庭全体を発掘することになったのです。"},{"id":{"d":976,"n":3,"t":2},"speaker":"A","text":"では、当分校庭は使えなくなるのですか？"},{"id":{"d":976,"n":4,"t":2},"speaker":"B","text":"そうなりますね。早急に代わりになるところを見つけなければなりません。"},{"id":{"d":976,"n":5,"t":2},"speaker":"A","text":"この辺に校庭の代わりになるような広い場所はなさそうですね。"},{"id":{"d":976,"n":6,"t":2},"speaker":"B","text":"それで頭を悩ましているのです。校長先生が市の教育委員会と相談しています。"},{"id":{"d":976,"n":7,"t":2},"speaker":"A","text":"何か良い解決策があるでしょうか？"},{"id":{"d":976,"n":8,"t":2},"speaker":"B","text":"四谷小学校の校庭を一時的に借りることが一案です。"},{"id":{"d":976,"n":9,"t":2},"speaker":"A","text":"交渉がまとまれば良いのですがね。"}];
INSERT INTO sentence [{"id":{"d":977,"n":1,"t":2},"speaker":"A","text":"娘が不登校になりましてね。"},{"id":{"d":977,"n":2,"t":2},"speaker":"B","text":"いじめか何かに遭ったのですか？"},{"id":{"d":977,"n":3,"t":2},"speaker":"A","text":"原因がよく分かりません。本人も何も言いません。"},{"id":{"d":977,"n":4,"t":2},"speaker":"B","text":"学校の先生に相談はしたのですか？"},{"id":{"d":977,"n":5,"t":2},"speaker":"A","text":"相談はしました。でも、先生も原因について心当たりはないそうです。"},{"id":{"d":977,"n":6,"t":2},"speaker":"B","text":"心配ですね。これからどうなさるのですか？"},{"id":{"d":977,"n":7,"t":2},"speaker":"A","text":"娘は「学校を辞めて、大検を受けて、大学に行きたい」と言っています。"},{"id":{"d":977,"n":8,"t":2},"speaker":"B","text":"そこまで考えているのなら、そうさせてあげたらいかがですか？"},{"id":{"d":977,"n":9,"t":2},"speaker":"A","text":"私もそうさせてあげようと思っています。"}];
INSERT INTO sentence [{"id":{"d":978,"n":1,"t":2},"speaker":"A","text":"子どもを預けられる保育園がなくて困っています。"},{"id":{"d":978,"n":2,"t":2},"speaker":"B","text":"分かります。私のところもそうでしたから。"},{"id":{"d":978,"n":3,"t":2},"speaker":"A","text":"いくつも保育園を回ったのですが、どこも「空きがない」と言われます。"},{"id":{"d":978,"n":4,"t":2},"speaker":"B","text":"私のときは、たまたま会社が保育園を作ってくれることになって、そこに入れました。"},{"id":{"d":978,"n":5,"t":2},"speaker":"A","text":"大きな会社はできますが、私の勤めている会社はとてもそのようなことはできません。"},{"id":{"d":978,"n":6,"t":2},"speaker":"B","text":"いくつかの会社が共同で作ってくれれば良いのでしょうが。"},{"id":{"d":978,"n":7,"t":2},"speaker":"A","text":"それまで待っていられません。どうにかしないといけません。"},{"id":{"d":978,"n":8,"t":2},"speaker":"B","text":"私も心当たりを探してみましょう。他人事だとは思えません。"},{"id":{"d":978,"n":9,"t":2},"speaker":"A","text":"それはご親切に、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":979,"n":1,"t":2},"speaker":"A","text":"小川君のお父さんはお医者さんだから、あなたも当然医学部に進むのですよね。"},{"id":{"d":979,"n":2,"t":2},"speaker":"B","text":"いえ、医学部には行きません。文学部に行きます。"},{"id":{"d":979,"n":3,"t":2},"speaker":"A","text":"お父さんに反対されませんか？"},{"id":{"d":979,"n":4,"t":2},"speaker":"B","text":"もちろん、反対されました。でも、説得して、納得してもらいました。"},{"id":{"d":979,"n":5,"t":2},"speaker":"A","text":"確か、弟さんがいましたよね。弟さんがお父さまの後を継ぐのですか？"},{"id":{"d":979,"n":6,"t":2},"speaker":"B","text":"いえ、弟も後を継ぎたくないようです。"},{"id":{"d":979,"n":7,"t":2},"speaker":"A","text":"それでは、お父さんの代で病院を廃業するということになるのですか？"},{"id":{"d":979,"n":8,"t":2},"speaker":"B","text":"いえ、誰か他の人を見つけて、その人に譲ることになると思います。"}];
INSERT INTO sentence [{"id":{"d":980,"n":1,"t":2},"speaker":"A","text":"今日の地震の揺れは大きかったですね。"},{"id":{"d":980,"n":2,"t":2},"speaker":"B","text":"そうですね。僕はちょうど理科の実験をしていました。"},{"id":{"d":980,"n":3,"t":2},"speaker":"A","text":"それなら、アルコールランプを使ったりしていませんでしたか？"},{"id":{"d":980,"n":4,"t":2},"speaker":"B","text":"はい。まさにアルコールランプで材料を温めていたところでした。"},{"id":{"d":980,"n":5,"t":2},"speaker":"A","text":"それは危なかったですね。みんな慌てたのではありませんか？"},{"id":{"d":980,"n":6,"t":2},"speaker":"B","text":"みんな大きな揺れに驚いていましたが、先生がすぐに「アルコールランプを消すように」と言い、われに返ったようでした。"},{"id":{"d":980,"n":7,"t":2},"speaker":"A","text":"素晴らしい先生ですね。みんなすぐに火を消せましたか？"},{"id":{"d":980,"n":8,"t":2},"speaker":"B","text":"はい、地震のときを想定して、普段から練習していましたから。"},{"id":{"d":980,"n":9,"t":2},"speaker":"A","text":"日頃の準備が大切ですね。"}];
INSERT INTO sentence [{"id":{"d":981,"n":1,"t":2},"speaker":"A","text":"小川さんが数学のテストで満点を取ったそうです。"},{"id":{"d":981,"n":2,"t":2},"speaker":"B","text":"満点ですか。信じられません。私は70点です。"},{"id":{"d":981,"n":3,"t":2},"speaker":"A","text":"70点なら良い方ですよ。私は50点です。"},{"id":{"d":981,"n":4,"t":2},"speaker":"B","text":"満点を取るなど、どのような勉強をしているのでしょう。"},{"id":{"d":981,"n":5,"t":2},"speaker":"A","text":"小川さんは「特別なことはしていない」と言っていました。"},{"id":{"d":981,"n":6,"t":2},"speaker":"B","text":"そう言えば、彼女は放課後も遅くまで部活をしていますしね。"},{"id":{"d":981,"n":7,"t":2},"speaker":"A","text":"はい。「授業をきちんと聞いているだけだ」と言っていました。"},{"id":{"d":981,"n":8,"t":2},"speaker":"B","text":"それで満点を取るのだから、本当に素晴らしいですね。見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":982,"n":1,"t":2},"speaker":"A","text":"運動会の種目に400メートルのリレーがありました。"},{"id":{"d":982,"n":2,"t":2},"speaker":"B","text":"クラス対抗のリレーですね。毎年一番盛り上がるそうですね。"},{"id":{"d":982,"n":3,"t":2},"speaker":"A","text":"そうです。そのレースで僕たちのクラスの兵道君が素晴らしかったです。"},{"id":{"d":982,"n":4,"t":2},"speaker":"B","text":"どう素晴らしかったのですか？"},{"id":{"d":982,"n":5,"t":2},"speaker":"A","text":"兵道君がアンカーで走ったのですが、バトンを受け取ったときには先頭から50メートルぐらい離された最後尾でした。"},{"id":{"d":982,"n":6,"t":2},"speaker":"B","text":"もしかして、それを追い抜いたのですか？"},{"id":{"d":982,"n":7,"t":2},"speaker":"A","text":"そうです。次々に追い抜いて、ゴール前で先頭に出ました。そして、そのままゴールテープを切ったのです。"},{"id":{"d":982,"n":8,"t":2},"speaker":"B","text":"それは素晴らしいことですね。"},{"id":{"d":982,"n":9,"t":2},"speaker":"A","text":"観客の歓声が素晴らしかったです。みんな驚いていました。"},{"id":{"d":982,"n":10,"t":2},"speaker":"B","text":"それは驚くでしょう。良いレースだったのですね。"}];
INSERT INTO sentence [{"id":{"d":983,"n":1,"t":2},"speaker":"A","text":"夏休みの宿題は終わりましたか？"},{"id":{"d":983,"n":2,"t":2},"speaker":"B","text":"いえ、まだ終わっていません。"},{"id":{"d":983,"n":3,"t":2},"speaker":"A","text":"どのくらい残っているのですか？"},{"id":{"d":983,"n":4,"t":2},"speaker":"B","text":"実はまだ全然手を付けていません。"},{"id":{"d":983,"n":5,"t":2},"speaker":"A","text":"まあ！夏休みはあと1週間しかありませんよ。"},{"id":{"d":983,"n":6,"t":2},"speaker":"B","text":"大丈夫です。1週間あれば全部できます。"},{"id":{"d":983,"n":7,"t":2},"speaker":"A","text":"本当に大丈夫ですか？あとで手伝ってくれと言っても知りませんよ。"},{"id":{"d":983,"n":8,"t":2},"speaker":"B","text":"はい。一人でやります。約束します。"}];
INSERT INTO sentence [{"id":{"d":984,"n":1,"t":2},"speaker":"A","text":"この大学の学生食堂は内容が充実していますね。"},{"id":{"d":984,"n":2,"t":2},"speaker":"B","text":"そうですね。メニューの品数が多いし、何よりおいしそうです。"},{"id":{"d":984,"n":3,"t":2},"speaker":"A","text":"定食も常に5種類ありますし、それぞれに栄養のバランスがよく考えられているそうですよ。"},{"id":{"d":984,"n":4,"t":2},"speaker":"B","text":"ありがたいですね。さて、今日は何にしましょうか？"},{"id":{"d":984,"n":5,"t":2},"speaker":"A","text":"今日の定食もどれもおいしそうです。何を食べるか迷います。"},{"id":{"d":984,"n":6,"t":2},"speaker":"B","text":"そうですね。でも、後ろの人が待っていますから、早く選びましょう。"},{"id":{"d":984,"n":7,"t":2},"speaker":"A","text":"私はこのA定食というのにします。豚ロースのみそ漬けとさばの塩焼きです。あなたはどうしますか？"},{"id":{"d":984,"n":8,"t":2},"speaker":"B","text":"私はB定食にします。"},{"id":{"d":984,"n":9,"t":2},"speaker":"A","text":"もろみチキンの炭火焼きと鶏ごぼうしょうがご飯ですか？良いですね。"},{"id":{"d":984,"n":10,"t":2},"speaker":"B","text":"注文も決まったことですし、早速食券を買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":985,"n":1,"t":2},"speaker":"A","text":"月川寺の幼稚園に、子どもを通わせようと思うのですが、どうでしょうか？"},{"id":{"d":985,"n":2,"t":2},"speaker":"B","text":"私も娘を月川寺の幼稚園に通わせました。とても良い幼稚園ですよ。"},{"id":{"d":985,"n":3,"t":2},"speaker":"A","text":"月川寺幼稚園には英会話のレッスンの時間もあるそうですね。息子に英語の早期教育を受けさせたいのです。"},{"id":{"d":985,"n":4,"t":2},"speaker":"B","text":"はい、外国人の先生が指導してくださいます。英語を専攻している大学生との英会話遊びもあります。"},{"id":{"d":985,"n":5,"t":2},"speaker":"A","text":"娘さんはいかがでしたか？英会話ができるようになりましたか？"},{"id":{"d":985,"n":6,"t":2},"speaker":"B","text":"ええ。今でも当時のお友達とはよく英語でおしゃべりをしていますよ。"},{"id":{"d":985,"n":7,"t":2},"speaker":"A","text":"素晴しいことですね。それでは、ぜひ息子を入園させたいと思います。"},{"id":{"d":985,"n":8,"t":2},"speaker":"B","text":"ぜひ、そうなさい。何か分からないことがあれば、いつでも相談に乗りますよ。"},{"id":{"d":985,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。どうぞよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":986,"n":1,"t":2},"speaker":"A","text":"今日の掃除当番は誰ですか？"},{"id":{"d":986,"n":2,"t":2},"speaker":"B","text":"僕と池上君です。"},{"id":{"d":986,"n":3,"t":2},"speaker":"A","text":"今日のごみ捨ては少し骨が折れるかもしれません。工作の授業でごみがたくさん出たと思います。"},{"id":{"d":986,"n":4,"t":2},"speaker":"B","text":"はい、ごみ箱はいっぱいになっています。"},{"id":{"d":986,"n":5,"t":2},"speaker":"A","text":"2人で捨てにいくのは大変そうですが、大丈夫ですか？"},{"id":{"d":986,"n":6,"t":2},"speaker":"B","text":"はい。山本君が手伝ってくれるそうなので大丈夫です。"},{"id":{"d":986,"n":7,"t":2},"speaker":"A","text":"それは良かったです。山本君は優しいですね。"},{"id":{"d":986,"n":8,"t":2},"speaker":"B","text":"はい。良い友達がいて幸せです。"}];
INSERT INTO sentence [{"id":{"d":987,"n":1,"t":2},"speaker":"A","text":"文化祭が近付いてきましたね。このクラスは何をするのですか？"},{"id":{"d":987,"n":2,"t":2},"speaker":"B","text":"『ベニスの商人』の英語劇を行います。"},{"id":{"d":987,"n":3,"t":2},"speaker":"A","text":"あの長い戯曲を全部行うのですか？"},{"id":{"d":987,"n":4,"t":2},"speaker":"B","text":"いえ、30分ぐらいに縮めて行います。今、青木さんが台本を作っているところです。"},{"id":{"d":987,"n":5,"t":2},"speaker":"A","text":"配役はもう決まりましたか？"},{"id":{"d":987,"n":6,"t":2},"speaker":"B","text":"はい。アントーニオを森田さん、バサーニオを所さん、ポーシャを私が演じます。"},{"id":{"d":987,"n":7,"t":2},"speaker":"A","text":"シャイロックは誰が演じるのですか？"},{"id":{"d":987,"n":8,"t":2},"speaker":"B","text":"シャイロックは北野さんが演じます。"},{"id":{"d":987,"n":9,"t":2},"speaker":"A","text":"皆さん、それぞれの役によく似合っていますね。英語のせりふは正しく覚えられそうですか？"},{"id":{"d":987,"n":10,"t":2},"speaker":"B","text":"はい。みんな必死で暗記していますよ。本番を楽しみにしていてください。"}];
INSERT INTO sentence [{"id":{"d":988,"n":1,"t":2},"speaker":"A","text":"ようやく自動車の免許証を取ることができました。"},{"id":{"d":988,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。私も自動車免許取得の経験がありますが、自動車学校の先生は厳しいでしょう？"},{"id":{"d":988,"n":3,"t":2},"speaker":"A","text":"ええ、ついスピードを出しすぎてしまうので、その都度叱られました。"},{"id":{"d":988,"n":4,"t":2},"speaker":"B","text":"私はあんまり遅いので、自動車学校の先生から「それほどゆっくり走るのではない」と言われました。"},{"id":{"d":988,"n":5,"t":2},"speaker":"A","text":"まるで私と逆ではありませんか。慎重なのですね。"},{"id":{"d":988,"n":6,"t":2},"speaker":"B","text":"臆病なのですよ。今でも、スピードを出すことが怖くて、高速道路で自動車を運転することは苦手です。"},{"id":{"d":988,"n":7,"t":2},"speaker":"A","text":"良いことですよ。事故が起きるのは大抵スピードを出し過ぎているからですから。"},{"id":{"d":988,"n":8,"t":2},"speaker":"B","text":"恐ろしい勢いで追い越して行く車がありますね。そのようなとき、事故を起こさなければ良いがと思います。"},{"id":{"d":988,"n":9,"t":2},"speaker":"A","text":"私も、あなたを見習って、慎重に運転することにします。"}];
INSERT INTO sentence [{"id":{"d":989,"n":1,"t":2},"speaker":"A","text":"手に汗握る良い試合でしたね。"},{"id":{"d":989,"n":2,"t":2},"speaker":"B","text":"どちらが勝っても良いような試合でしたね。"},{"id":{"d":989,"n":3,"t":2},"speaker":"A","text":"ええ。ロスタイムの1点が勝負を決めましたね。"},{"id":{"d":989,"n":4,"t":2},"speaker":"B","text":"そうですね。あれは、相手チームの一瞬の隙を突いたゴールでした。"},{"id":{"d":989,"n":5,"t":2},"speaker":"A","text":"山中選手のシュートも見事でしたが、アシストした遠藤選手の判断も素晴らしかったと思います。"},{"id":{"d":989,"n":6,"t":2},"speaker":"B","text":"はい。これでどのようにかして準決勝に進んだわけですが、どのような試合を期待しますか？"},{"id":{"d":989,"n":7,"t":2},"speaker":"A","text":"やはり、今日のような攻撃と守備のバランスの良い試合をしていただきたいですね。"},{"id":{"d":989,"n":8,"t":2},"speaker":"B","text":"私もそう思います。準決勝に勝って決勝に進むことを期待しましょう。"}];
INSERT INTO sentence [{"id":{"d":990,"n":1,"t":2},"speaker":"A","text":"廃校になった小学校の建物を再利用できたら良いと思いますが、何か良いアイデアはありませんか？"},{"id":{"d":990,"n":2,"t":2},"speaker":"B","text":"似た事例をインターネットで調べてみましたが、全国でいろんな試みが行われているようですよ。"},{"id":{"d":990,"n":3,"t":2},"speaker":"A","text":"どのような試みですか？うちでも参考にできるようなことがありますか？"},{"id":{"d":990,"n":4,"t":2},"speaker":"B","text":"例えば、小学校の建物を再利用して創設された、スタートアップ企業を支援する施設が各地にありますね。"},{"id":{"d":990,"n":5,"t":2},"speaker":"A","text":"なるほど。他にはどのような再利用の仕方がありますか？"},{"id":{"d":990,"n":6,"t":2},"speaker":"B","text":"体験型観光施設として、自然や料理、アートを楽しめる場に再生したところもあります。"},{"id":{"d":990,"n":7,"t":2},"speaker":"A","text":"うちにふさわしい施設は何でしょうね？"},{"id":{"d":990,"n":8,"t":2},"speaker":"B","text":"自然が豊かですから、やはり、体験型観光施設を目指すことが良いのではないでしょうか？"},{"id":{"d":990,"n":9,"t":2},"speaker":"A","text":"良いアイデアですね。その方向で、具体的な計画を少し考えてみます。また相談に乗ってください。"},{"id":{"d":990,"n":10,"t":2},"speaker":"B","text":"私で良ければいつでも相談に乗ります。頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":991,"n":1,"t":2},"speaker":"A","text":"試験の問題を作るのは難しいです。問題作りは初めてなものですから、要領が分かりません。"},{"id":{"d":991,"n":2,"t":2},"speaker":"B","text":"私も試験問題の作成は今でも苦労しています。"},{"id":{"d":991,"n":3,"t":2},"speaker":"A","text":"先生はどのようなことに留意して問題を作っていますか？"},{"id":{"d":991,"n":4,"t":2},"speaker":"B","text":"クイズではありませんから、素直に考えれば解ける問題を作るように心掛けています。"},{"id":{"d":991,"n":5,"t":2},"speaker":"A","text":"それは大事なことですね。他にはどのようなことを心掛けていますか？"},{"id":{"d":991,"n":6,"t":2},"speaker":"B","text":"まぐれ当たりを防ぐような問題を作るようにしています。"},{"id":{"d":991,"n":7,"t":2},"speaker":"A","text":"それも大事なことですね。勉強した人が良い成績を取れるような問題ですね。"},{"id":{"d":991,"n":8,"t":2},"speaker":"B","text":"そうです。山を張るだけでは、良い成績が取れないような問題です。"},{"id":{"d":991,"n":9,"t":2},"speaker":"A","text":"参考になりました。その2つを意識して作問を頑張ってみます。"}];
INSERT INTO sentence [{"id":{"d":992,"n":1,"t":2},"speaker":"A","text":"小川君は1単位足りませんね。このままでは進級できません。"},{"id":{"d":992,"n":2,"t":2},"speaker":"B","text":"そのようなことはないはずです。テストで赤点を取ったことはありません。"},{"id":{"d":992,"n":3,"t":2},"speaker":"A","text":"成績は問題ありませんが、出席日数が足りません。欠席が多過ぎます。"},{"id":{"d":992,"n":4,"t":2},"speaker":"B","text":"困りました。どうしても家の仕事を手伝わなくてはならなかったものですから。"},{"id":{"d":992,"n":5,"t":2},"speaker":"A","text":"小川君の家庭の事情は分かっていますよ。でも、こればかりは規則なので仕方ありません。"},{"id":{"d":992,"n":6,"t":2},"speaker":"B","text":"何日足りませんか？"},{"id":{"d":992,"n":7,"t":2},"speaker":"A","text":"3日です。"},{"id":{"d":992,"n":8,"t":2},"speaker":"B","text":"3日足りなくて留年しなければいけないのは悔しいです。先生、どのようにかなりませんか？"},{"id":{"d":992,"n":9,"t":2},"speaker":"A","text":"では、こうしましょう。冬休みに3日補習授業をして、出席日数に加えるということにしましょう。"},{"id":{"d":992,"n":10,"t":2},"speaker":"B","text":"分かりました。よろしくお願いします。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":993,"n":1,"t":2},"speaker":"A","text":"この電車の路線は、来年の春には廃止になるそうです。"},{"id":{"d":993,"n":2,"t":2},"speaker":"B","text":"本当ですか？この路線を使って通学しているので、廃止になると困ります。"},{"id":{"d":993,"n":3,"t":2},"speaker":"A","text":"代わりにバスが走るようになるそうですよ。"},{"id":{"d":993,"n":4,"t":2},"speaker":"B","text":"でも、バスになると時間がかかるのではありませんかね。"},{"id":{"d":993,"n":5,"t":2},"speaker":"A","text":"多分そうなるでしょうね。今はどのくらいかかっていますか？"},{"id":{"d":993,"n":6,"t":2},"speaker":"B","text":"およそ1時間かかっています。家から駅まで20分、電車で30分、駅から学校までが10分です。"},{"id":{"d":993,"n":7,"t":2},"speaker":"A","text":"バスだと曲がりくねった道路を走るし、信号もありますから、その2倍はかかるでしょうね。"},{"id":{"d":993,"n":8,"t":2},"speaker":"B","text":"それほど時間がかかるのでは、学校の近くに部屋を借りる方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":994,"n":1,"t":2},"speaker":"A","text":"家庭科の授業が、昔とは様変わりしているのを知っていますか？"},{"id":{"d":994,"n":2,"t":2},"speaker":"B","text":"いえ、知りません。私の頃は料理と裁縫をしましたよね？"},{"id":{"d":994,"n":3,"t":2},"speaker":"A","text":"そうですね。今年からは株式や債券などについても教えることになるそうですよ。"},{"id":{"d":994,"n":4,"t":2},"speaker":"B","text":"そのようなことまで教えるのですか？"},{"id":{"d":994,"n":5,"t":2},"speaker":"A","text":"はい。家庭生活も経済社会と密接な関係があることを教えようということになったようです。"},{"id":{"d":994,"n":6,"t":2},"speaker":"B","text":"それは素晴らしいですが、教える先生方もいろいろと新しく学ばなければならないことが増えますね。"},{"id":{"d":994,"n":7,"t":2},"speaker":"A","text":"その通りです。"}];
INSERT INTO sentence [{"id":{"d":995,"n":1,"t":2},"speaker":"A","text":"明日は学校で遠足に行くのでしたね。"},{"id":{"d":995,"n":2,"t":2},"speaker":"B","text":"はい、そうです。鞍掛山に登ります。"},{"id":{"d":995,"n":3,"t":2},"speaker":"A","text":"どのような予定ですか？"},{"id":{"d":995,"n":4,"t":2},"speaker":"B","text":"朝8時に学校からバスに乗って、相の沢キャンプ場まで行きます。そこから頂上まで歩きます。"},{"id":{"d":995,"n":5,"t":2},"speaker":"A","text":"頂上までどのくらいかかるのですか？"},{"id":{"d":995,"n":6,"t":2},"speaker":"B","text":"2時間ぐらいかかるそうです。"},{"id":{"d":995,"n":7,"t":2},"speaker":"A","text":"先生の言いつけを守って、逸れないように気を付けるのですよ。"},{"id":{"d":995,"n":8,"t":2},"speaker":"B","text":"分かっていますよ。心配しないでください。"}];
INSERT INTO sentence [{"id":{"d":996,"n":1,"t":2},"speaker":"A","text":"全国陸上競技会にわが校からは何人参加するのですか？"},{"id":{"d":996,"n":2,"t":2},"speaker":"B","text":"今年は4人参加します。男子100メートル競走に1名、女子5,000メートル競走に1名、男子走り幅跳びに1名、女子マラソンに1名です。"},{"id":{"d":996,"n":3,"t":2},"speaker":"A","text":"調子はどうでしょうか？上位入賞を狙えそうですか？"},{"id":{"d":996,"n":4,"t":2},"speaker":"B","text":"男子100メートル競走の上園選手が優勝候補に名前が挙がっているそうですよ。"},{"id":{"d":996,"n":5,"t":2},"speaker":"A","text":"彼の自己最高記録は何秒くらいなのですか？"},{"id":{"d":996,"n":6,"t":2},"speaker":"B","text":"10秒05です。本人は今年中に10秒を切ろうと頑張っています。"},{"id":{"d":996,"n":7,"t":2},"speaker":"A","text":"素晴しいですね。他に上位を狙える選手はいますか？"},{"id":{"d":996,"n":8,"t":2},"speaker":"B","text":"女子マラソンの後藤選手が2時間23分の記録を持っていまして、十分入賞の可能性があります。"},{"id":{"d":996,"n":9,"t":2},"speaker":"A","text":"それは楽しみです。皆さん、良い成績を収められると良いですね。"}];
INSERT INTO sentence [{"id":{"d":997,"n":1,"t":2},"speaker":"A","text":"今年は総長選挙がありますね。"},{"id":{"d":997,"n":2,"t":2},"speaker":"B","text":"今回は候補者が5人もいるので、選挙運動も激しくなるでしょう。"},{"id":{"d":997,"n":3,"t":2},"speaker":"A","text":"早くもそれぞれの陣営に分かれて、票集めが始まっているようです。"},{"id":{"d":997,"n":4,"t":2},"speaker":"B","text":"私のところにもそれとなく打診が来ています。ところで、現時点での有力候補はどなたなのでしょうか？"},{"id":{"d":997,"n":5,"t":2},"speaker":"A","text":"私の見るところ、有力な候補者はいません。"},{"id":{"d":997,"n":6,"t":2},"speaker":"B","text":"そうですか。では1回目の投票では決まらなくて、決選投票になる可能性もありそうですね。"},{"id":{"d":997,"n":7,"t":2},"speaker":"A","text":"あなたは誰に投票するかもう決めていますか？"},{"id":{"d":997,"n":8,"t":2},"speaker":"B","text":"はい。意中の人はいますが、今は内緒にしておきます。"}];
INSERT INTO sentence [{"id":{"d":998,"n":1,"t":2},"speaker":"A","text":"もしもし、大山君ですか？こちらは小山です。"},{"id":{"d":998,"n":2,"t":2},"speaker":"B","text":"久しぶりですね。元気ですか？"},{"id":{"d":998,"n":3,"t":2},"speaker":"A","text":"はい、元気です。実は、そろそろクラス会をしたいと思って電話したのですが。"},{"id":{"d":998,"n":4,"t":2},"speaker":"B","text":"良いですね。賛成です。最後に集まってからもう3年も実施していませんからね。"},{"id":{"d":998,"n":5,"t":2},"speaker":"A","text":"私が幹事をすることになっているので、日にちや会場などを相談したいのです。"},{"id":{"d":998,"n":6,"t":2},"speaker":"B","text":"みんなの都合を聞いて、一番良い日に決めたらいいのではありませんか？会場は小山君に任せますよ。"},{"id":{"d":998,"n":7,"t":2},"speaker":"A","text":"それでは、皆さんに都合の良い日を聞くことにしましょう。会場は私が手配して、皆さんに連絡します。"},{"id":{"d":998,"n":8,"t":2},"speaker":"B","text":"ご苦労さまです。他に何かお手伝いできることがありましたら、知らせてください。"},{"id":{"d":998,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":999,"n":1,"t":2},"speaker":"A","text":"米国に赴任することになりました。ところが、私は英語があまり得意ではありません。"},{"id":{"d":999,"n":2,"t":2},"speaker":"B","text":"大丈夫ですよ。向こうで生活すれば、すぐに話せるようになりますよ。"},{"id":{"d":999,"n":3,"t":2},"speaker":"A","text":"米国で生活したことがあるのですか？"},{"id":{"d":999,"n":4,"t":2},"speaker":"B","text":"はい、大学時代に1年ほど留学しました。"},{"id":{"d":999,"n":5,"t":2},"speaker":"A","text":"いかがでしたか？すぐに英語を話せるようになりましたか？"},{"id":{"d":999,"n":6,"t":2},"speaker":"B","text":"初めはうまく通じなくて困りましたが、それでも人と交わっているうちに会話できるようになりました。"},{"id":{"d":999,"n":7,"t":2},"speaker":"A","text":"それを聞くと心強くなります。"},{"id":{"d":999,"n":8,"t":2},"speaker":"B","text":"一人で引きこもらないで、ひたすら人と交わることが一番大事です。頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":1000,"n":1,"t":2},"speaker":"A","text":"今度の土曜日に何か予定はありますか？"},{"id":{"d":1000,"n":2,"t":2},"speaker":"B","text":"はい。土曜日は部活があります。"},{"id":{"d":1000,"n":3,"t":2},"speaker":"A","text":"土曜日にも部活があるのですか？"},{"id":{"d":1000,"n":4,"t":2},"speaker":"B","text":"はい。来月に県大会があるので、練習をしなければなりません。"},{"id":{"d":1000,"n":5,"t":2},"speaker":"A","text":"それでは仕方がありませんね。"},{"id":{"d":1000,"n":6,"t":2},"speaker":"B","text":"土曜日に何かあるのですか？"},{"id":{"d":1000,"n":7,"t":2},"speaker":"A","text":"渋谷に一緒に遊びに行かないかと思ったのです。"},{"id":{"d":1000,"n":8,"t":2},"speaker":"B","text":"ぜひ、行きたいところですが、残念です。また別の機会に誘ってください。"}];
INSERT INTO sentence [{"id":{"d":1001,"n":1,"t":2},"speaker":"A","text":"県内に獣医学専門の大学ができるそうですよ。"},{"id":{"d":1001,"n":2,"t":2},"speaker":"B","text":"ようやくですか。この辺は牧畜業が盛んなので、獣医学を学べる大学は必要だと思っていました。"},{"id":{"d":1001,"n":3,"t":2},"speaker":"A","text":"そうですね。今までなかったことが不思議なくらいです。"},{"id":{"d":1001,"n":4,"t":2},"speaker":"B","text":"何か理由があるのでしょうかね？"},{"id":{"d":1001,"n":5,"t":2},"speaker":"A","text":"それは知りませんが、ともかく獣医学を学べる大学ができるのは良いことです。"},{"id":{"d":1001,"n":6,"t":2},"speaker":"B","text":"いつごろできるのですか？"},{"id":{"d":1001,"n":7,"t":2},"speaker":"A","text":"5年後に開校の予定だそうです。"},{"id":{"d":1001,"n":8,"t":2},"speaker":"B","text":"これをきっかけに若い人たちが牧畜業にどんどん参入してくれるようになるとうれしいですね。"},{"id":{"d":1001,"n":9,"t":2},"speaker":"A","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":1002,"n":1,"t":2},"speaker":"A","text":"今日の体育の時間はどのようなことをしましたか？"},{"id":{"d":1002,"n":2,"t":2},"speaker":"B","text":"プールで水遊びをしました。"},{"id":{"d":1002,"n":3,"t":2},"speaker":"A","text":"水遊びでどのようなことをしましたか？"},{"id":{"d":1002,"n":4,"t":2},"speaker":"B","text":"水の中で歩きます。それから、水に潜ります。それから、水に浮かびます。"},{"id":{"d":1002,"n":5,"t":2},"speaker":"A","text":"水遊びをするのは初めてですか？"},{"id":{"d":1002,"n":6,"t":2},"speaker":"B","text":"はい、初めてです。"},{"id":{"d":1002,"n":7,"t":2},"speaker":"A","text":"では、怖かったでしょう。"},{"id":{"d":1002,"n":8,"t":2},"speaker":"B","text":"初めは怖かったです。でも、仲良しのお友達が手を取ってくれたので、慣れました。"},{"id":{"d":1002,"n":9,"t":2},"speaker":"A","text":"優しいお友達がいて良かったですね。"}];
INSERT INTO sentence [{"id":{"d":1003,"n":1,"t":2},"speaker":"A","text":"そろそろ図書館を建て替えなくてはいけませんね。"},{"id":{"d":1003,"n":2,"t":2},"speaker":"B","text":"そうですね。老朽化していますから、利用者にはいろいろと不便なことがあります。危険でもありますし。"},{"id":{"d":1003,"n":3,"t":2},"speaker":"A","text":"費用がかかるでしょうね。捻出することが難しいです。"},{"id":{"d":1003,"n":4,"t":2},"speaker":"B","text":"また、卒業生に寄付をお願いしなければいけませんね。"},{"id":{"d":1003,"n":5,"t":2},"speaker":"A","text":"体育館の建て替え時に寄付をしてもらったばかりですので、お願いすることが心苦しいです。"},{"id":{"d":1003,"n":6,"t":2},"speaker":"B","text":"どうしたら良いでしょうね。何か良い知恵は浮かびませんか？"},{"id":{"d":1003,"n":7,"t":2},"speaker":"A","text":"特には思い浮かびません。やはり、行政にお願いするべきなのでしょう？"},{"id":{"d":1003,"n":8,"t":2},"speaker":"B","text":"なかなか難しいですね。市の財政も厳しいようですから。"}];
INSERT INTO sentence [{"id":{"d":1004,"n":1,"t":2},"speaker":"A","text":"私たちが通っていた小学校が、とうとう廃校になるそうですね。"},{"id":{"d":1004,"n":2,"t":2},"speaker":"B","text":"ええ、聞きました。残念です。"},{"id":{"d":1004,"n":3,"t":2},"speaker":"A","text":"今年の6年生5名が卒業したら、在校生がいなくなるそうです。"},{"id":{"d":1004,"n":4,"t":2},"speaker":"B","text":"私たちのころは1クラスに40人も50人もいたのに、いつのまにかそれほどに減っていたのですね。"},{"id":{"d":1004,"n":5,"t":2},"speaker":"A","text":"ええ、そのころがまるで夢のようです。"},{"id":{"d":1004,"n":6,"t":2},"speaker":"B","text":"この町は私たちのような年寄りばかりになってしまいそうですね。"},{"id":{"d":1004,"n":7,"t":2},"speaker":"A","text":"どうも、悲観的にばかりなりますね。"},{"id":{"d":1004,"n":8,"t":2},"speaker":"B","text":"いけませんね。私たちだけでも頑張って元気にやっていきましょう。"}];
INSERT INTO sentence [{"id":{"d":1005,"n":1,"t":2},"speaker":"A","text":"このベルは何でしょうか、「緊急放送」と言っているようですが。"},{"id":{"d":1005,"n":2,"t":2},"speaker":"B","text":"急に何でしょうね？アナウンスの続きを聞きましょう。"},{"id":{"d":1005,"n":3,"t":2},"speaker":"A","text":"「理科室で火事が発生した」と言っています。"},{"id":{"d":1005,"n":4,"t":2},"speaker":"B","text":"「急いで校庭に集まれ」と言っていますね。"},{"id":{"d":1005,"n":5,"t":2},"speaker":"A","text":"早く逃げましょう。"},{"id":{"d":1005,"n":6,"t":2},"speaker":"B","text":"教科書などを持って行く方がいいでしょうか？"},{"id":{"d":1005,"n":7,"t":2},"speaker":"A","text":"そのようなことをしている時間はありません。急ぎましょう。"},{"id":{"d":1005,"n":8,"t":2},"speaker":"B","text":"もしかして、火災訓練ではないでしょうね？"},{"id":{"d":1005,"n":9,"t":2},"speaker":"A","text":"分かりません。あれこれ考えるより先に逃げることが大事です。"}];
INSERT INTO sentence [{"id":{"d":1006,"n":1,"t":2},"speaker":"A","text":"来週から丸山先生はお休みです。"},{"id":{"d":1006,"n":2,"t":2},"speaker":"B","text":"丸山先生に会えないのは寂しいです。いつまで休むのですか？"},{"id":{"d":1006,"n":3,"t":2},"speaker":"A","text":"半年ほど休みます。産前産後休業を取られるのです。"},{"id":{"d":1006,"n":4,"t":2},"speaker":"B","text":"産前産後休暇というのは何ですか？"},{"id":{"d":1006,"n":5,"t":2},"speaker":"A","text":"丸山先生には赤ちゃんができたので、赤ちゃんが生まれるまでと生まれてからのしばらくの間はお休みするのです。"},{"id":{"d":1006,"n":6,"t":2},"speaker":"B","text":"丸山先生が休む間、私たちはどうなるのですか？"},{"id":{"d":1006,"n":7,"t":2},"speaker":"A","text":"心配しなくても大丈夫です。小山先生が代わりに受け持ってくれます。"},{"id":{"d":1006,"n":8,"t":2},"speaker":"B","text":"良かったです。小山先生も優しいので、クラスを受け持ってくださるのはうれしいです。"}];
INSERT INTO sentence [{"id":{"d":1007,"n":1,"t":2},"speaker":"A","text":"今日はクラスの委員会決めがありました。"},{"id":{"d":1007,"n":2,"t":2},"speaker":"B","text":"俊太郎君は何の委員になったのですか？"},{"id":{"d":1007,"n":3,"t":2},"speaker":"A","text":"はい。僕は風紀委員になりました。"},{"id":{"d":1007,"n":4,"t":2},"speaker":"B","text":"風紀委員というのはどのようなことをするのですか？"},{"id":{"d":1007,"n":5,"t":2},"speaker":"A","text":"学校生活をする上でのきまりの維持と向上に努めます。"},{"id":{"d":1007,"n":6,"t":2},"speaker":"B","text":"つまり、生徒たちが正しくきまりを守っているかを見張るのですか？"},{"id":{"d":1007,"n":7,"t":2},"speaker":"A","text":"「見張る」という言い方は正しくありません。どのような決まりが必要か聞いたり、提案したりして、みんなの意見をまとめるのです。"},{"id":{"d":1007,"n":8,"t":2},"speaker":"B","text":"なかなか大変ですね。頑張って、信頼される委員になってください。"}];
INSERT INTO sentence [{"id":{"d":1008,"n":1,"t":2},"speaker":"A","text":"滝沢先生、次期の理事長をお引き受け願えませんでしょうか？"},{"id":{"d":1008,"n":2,"t":2},"speaker":"B","text":"いえ、とても私にはその役は務まりません。"},{"id":{"d":1008,"n":3,"t":2},"speaker":"A","text":"そのようなことを仰らずに、ぜひお願いします。わが校の危機を救ってくださるのは先生以外にいません。"},{"id":{"d":1008,"n":4,"t":2},"speaker":"B","text":"私にも愛校心がありますから、お役に立てればと思いますが、自信がありません。"},{"id":{"d":1008,"n":5,"t":2},"speaker":"A","text":"私どもで一所懸命サポートしますから、ぜひお願いします。"},{"id":{"d":1008,"n":6,"t":2},"speaker":"B","text":"それほどまで仰るのなら、お引き受けしますが、1つだけ条件があります。"},{"id":{"d":1008,"n":7,"t":2},"speaker":"A","text":"何でしょうか？"},{"id":{"d":1008,"n":8,"t":2},"speaker":"B","text":"私の改革案を実行するという誓約書を、理事会と教授会の全員に作っていただくことです。"},{"id":{"d":1008,"n":9,"t":2},"speaker":"A","text":"分かりました。早速皆さんに諮って、誓約書を作ります。"},{"id":{"d":1008,"n":10,"t":2},"speaker":"B","text":"それでは、お引き受けします。"}];
INSERT INTO sentence [{"id":{"d":1009,"n":1,"t":2},"speaker":"A","text":"「課外授業、この道の達人に聞く」ということがありますが、これは何ですか？"},{"id":{"d":1009,"n":2,"t":2},"speaker":"B","text":"一つの仕事に長い間携わってきた人に経験を話してもらう授業です。"},{"id":{"d":1009,"n":3,"t":2},"speaker":"A","text":"それは面白そうな授業ですね。生徒たちの評判はどうですか？"},{"id":{"d":1009,"n":4,"t":2},"speaker":"B","text":"みんなとても興味を持って熱心に聞いていますよ。"},{"id":{"d":1009,"n":5,"t":2},"speaker":"A","text":"そうでしょうね。みんなどう生きるか迷っている年齢ですから、人生の先輩の話を聞くのはためになります。"},{"id":{"d":1009,"n":6,"t":2},"speaker":"B","text":"先週は、酒造りを50年以上続けてきた杜氏さんに話してもらいました。"},{"id":{"d":1009,"n":7,"t":2},"speaker":"A","text":"生徒の感想はいかがでしたか？"},{"id":{"d":1009,"n":8,"t":2},"speaker":"B","text":"みんな感動して、中にはその杜氏さんに弟子入りしたいという生徒も出てきたほどです。"},{"id":{"d":1009,"n":9,"t":2},"speaker":"A","text":"そのようにして後継者が出てくると良いですね。"}];
INSERT INTO sentence [{"id":{"d":1010,"n":1,"t":2},"speaker":"A","text":"生徒から、時々「なぜ数学を勉強する必要があるのか」という質問を受けることがあります。"},{"id":{"d":1010,"n":2,"t":2},"speaker":"B","text":"生徒の疑問ももっともですね。"},{"id":{"d":1010,"n":3,"t":2},"speaker":"A","text":"そのような質問に、なかなかうまく答えることができません。"},{"id":{"d":1010,"n":4,"t":2},"speaker":"B","text":"「論理的な思考力を養うことができる」と言ったらどうでしょうか？"},{"id":{"d":1010,"n":5,"t":2},"speaker":"A","text":"論理的な思考力といっても漠然としています。"},{"id":{"d":1010,"n":6,"t":2},"speaker":"B","text":"では「物事をいろいろな角度から考える力」という説明はいかがでしょうか？"},{"id":{"d":1010,"n":7,"t":2},"speaker":"A","text":"それでもまだ漠然としていますね。生徒に理解できるかどうか。"},{"id":{"d":1010,"n":8,"t":2},"speaker":"B","text":"確かに難しいですね。問題を解くノウハウを教えるだけでは駄目ですものね。"},{"id":{"d":1010,"n":9,"t":2},"speaker":"A","text":"私たちもよく勉強しなければいけません。"}];
INSERT INTO sentence [{"id":{"d":1011,"n":1,"t":2},"speaker":"A","text":"先輩は高校を卒業したらプロ野球に入団するのですか？"},{"id":{"d":1011,"n":2,"t":2},"speaker":"B","text":"それを目指しています。プロ野球ドラフト会議で指名されたらプロになるつもりです。"},{"id":{"d":1011,"n":3,"t":2},"speaker":"A","text":"先輩なら必ず指名されますよ。"},{"id":{"d":1011,"n":4,"t":2},"speaker":"B","text":"そうもいきませんよ。僕よりもうまい選手はたくさんいますから。"},{"id":{"d":1011,"n":5,"t":2},"speaker":"A","text":"甲子園で10本のホームランを打った選手を指名しないということはありません。"},{"id":{"d":1011,"n":6,"t":2},"speaker":"B","text":"そうだとうれしいのですがね。"},{"id":{"d":1011,"n":7,"t":2},"speaker":"A","text":"先輩がプロ野球に入団して活躍することを、僕らも応援しています。"},{"id":{"d":1011,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。期待に残りの期間も応えられるよう頑張ります。"}];
INSERT INTO sentence [{"id":{"d":1012,"n":1,"t":2},"speaker":"A","text":"高校時代に心に残っている授業はありますか？"},{"id":{"d":1012,"n":2,"t":2},"speaker":"B","text":"公民の授業です。とはいっても授業の内容そのものではありません。授業の時間にアニメーション作品を見ました。"},{"id":{"d":1012,"n":3,"t":2},"speaker":"A","text":"それは驚きです。何の作品を見たのですか？"},{"id":{"d":1012,"n":4,"t":2},"speaker":"B","text":"『風の谷のナウシカ』です。長い物語なので、3コマ分の授業の時間を使ったと記憶しています。毎回、視聴覚室に行って観賞しました。"},{"id":{"d":1012,"n":5,"t":2},"speaker":"A","text":"随分と柔軟な先生ですね。"},{"id":{"d":1012,"n":6,"t":2},"speaker":"B","text":"伊藤先生という30代半ばの男の先生でした。私の通っていた高校は定時制高校が併設されていて、伊藤先生は定時制高校の先生でした。私の学級担任が特別に頼んだそうで、私のクラスだけ伊藤先生が授業を担当してくださっていました。"},{"id":{"d":1012,"n":7,"t":2},"speaker":"A","text":"伊藤先生だけでなく、担任の先生も柔軟な方だったのですね。"},{"id":{"d":1012,"n":8,"t":2},"speaker":"B","text":"はい。高校自体が自由な校風で、生徒も先生も型にはまらない人が多かったように思います。"}];
INSERT INTO sentence [{"id":{"d":1013,"n":1,"t":2},"speaker":"A","text":"私は中学時代に生徒会の役員を務める優等生だったのですが、一度だけ先生に厳しく注意されたことがあります。"},{"id":{"d":1013,"n":2,"t":2},"speaker":"B","text":"お母さんのそのような話は初めて聞きます。一体全体何がありましたか？"},{"id":{"d":1013,"n":3,"t":2},"speaker":"A","text":"学年全体が一堂に会する学年集会の最中に、壇上で先生が話をしているにもかかわらず友達とおしゃべりをしていたのです。"},{"id":{"d":1013,"n":4,"t":2},"speaker":"B","text":"そのようなことは僕にとっては日常茶飯事ですよ。"},{"id":{"d":1013,"n":5,"t":2},"speaker":"A","text":"そうなのですか？それはいけませんよ。"},{"id":{"d":1013,"n":6,"t":2},"speaker":"B","text":"はい、気を付けます。それで、どう怒られたのですか？"},{"id":{"d":1013,"n":7,"t":2},"speaker":"A","text":"その時は、壇上から「小川、しゃべっているなら出ていけ！」とマイク越しに怒鳴られて本当に驚きました。他にも大勢おしゃべりしている子がいるのに、と恥ずかしいやら悔しいやらで、涙がこぼれました。"},{"id":{"d":1013,"n":8,"t":2},"speaker":"B","text":"どうして、一人だけ厳しく指導されたのでしょうか？"},{"id":{"d":1013,"n":9,"t":2},"speaker":"A","text":"生徒会役員として他の生徒の規範になることを求められていたようです。でも、反抗期の私は素直に受け入れることができませんでした。"}];
INSERT INTO sentence [{"id":{"d":1014,"n":1,"t":2},"speaker":"A","text":"松田さんは高校では部活に所属していましたか？"},{"id":{"d":1014,"n":2,"t":2},"speaker":"B","text":"はい、弓道部に所属していました。弓道部に入りたくて高校受験を頑張ったといっても過言ではありません。"},{"id":{"d":1014,"n":3,"t":2},"speaker":"A","text":"目標を達成したのですね。期待して入部した弓道部の活動はいかがでしたか？"},{"id":{"d":1014,"n":4,"t":2},"speaker":"B","text":"弓道の強豪校だったので練習はかなり厳しいものでした。そのような練習を頑張ったかいあって、団体戦で全国大会に出場することができました。"},{"id":{"d":1014,"n":5,"t":2},"speaker":"A","text":"インターハイに出場したのですか。それは素晴らしいですね。全国大会での成績はいかがでしたか？"},{"id":{"d":1014,"n":6,"t":2},"speaker":"B","text":"快調に勝ち続け、ベスト4に残ることができました。"},{"id":{"d":1014,"n":7,"t":2},"speaker":"A","text":"全国大会でベスト4とは、お見それしました。充実した高校時代だったのですね。"},{"id":{"d":1014,"n":8,"t":2},"speaker":"B","text":"はい。仲間にも、顧問の先生にも恵まれていたと思います。唯一の心残りは後輩を全国大会に行かせてあげることができなかったことです。いつの日か指導者の資格を取得して母校に戻りたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":1015,"n":1,"t":2},"speaker":"A","text":"中間テストの数学はどうでしたか？私は数学に一番時間をかけて勉強したにもかかわらず、手も足も出ませんでした。"},{"id":{"d":1015,"n":2,"t":2},"speaker":"B","text":"僕も今回ばかりは答案が帰ってくることが憂鬱です。半分できたかどうか微妙です。"},{"id":{"d":1015,"n":3,"t":2},"speaker":"A","text":"数学が得意な宮内君がそのようなことを言うということは、今回は相当な難問だったということですね。私は赤点かもしれません。"},{"id":{"d":1015,"n":4,"t":2},"speaker":"B","text":"クラスのみんなで嘆願したら、数学の近藤先生は再試験をしてくださらないだろうか？"},{"id":{"d":1015,"n":5,"t":2},"speaker":"A","text":"そうですね。近藤先生なら耳を傾けてくださるかもしれません。急いでクラスのみんなに話して再試験を希望する仲間を募ります。"},{"id":{"d":1015,"n":6,"t":2},"speaker":"B","text":"仲間を募集してみないことには分かりませんからね。僕も賛同者が増えるよう、働きかけてみます。"},{"id":{"d":1015,"n":7,"t":2},"speaker":"A","text":"再試験が認められたとしても、そこで高得点を取らなくては意味がありませんから、数学の勉強をもうひと頑張りします。"},{"id":{"d":1015,"n":8,"t":2},"speaker":"B","text":"僕の使っている参考書を高野さんに貸してあげましょうか？分かりやすい解説が載っていてなかなか良い参考書ですよ。"},{"id":{"d":1015,"n":9,"t":2},"speaker":"A","text":"はい。ありがとうございます。ぜひ貸してください。助かります。"}];
INSERT INTO sentence [{"id":{"d":1016,"n":1,"t":2},"speaker":"A","text":"私が小学生の頃は、放課後は遅くまで学校で遊んでいたように思うのですが、今の子たちは違うのですね。"},{"id":{"d":1016,"n":2,"t":2},"speaker":"B","text":"最近は、放課後に学校に残ることは禁止されていようです。"},{"id":{"d":1016,"n":3,"t":2},"speaker":"A","text":"そうですか。防犯上の問題でしょうか。ニュースで話題になっている、先生方の過重労働も理由の一つかもしれません。"},{"id":{"d":1016,"n":4,"t":2},"speaker":"B","text":"私たちが小学生の頃は放課後に校庭で遊んでいると、時々先生が仲間に加わってくださることもありましたよね。"},{"id":{"d":1016,"n":5,"t":2},"speaker":"A","text":"はい、私も先生と遊んだ記憶があります。先生も生徒もほこりまみれになって三角ベースをして遊びました。"},{"id":{"d":1016,"n":6,"t":2},"speaker":"B","text":"私が小学校3年生の頃のことです。放課後の教室で友達と大人数で遊んでいる時に、地震が起きて揺れたことがありました。避難訓練で学んだ通りにみんなで机の下に隠れたのですが、そうしているうちに担任の先生が走って教室まで来て「大丈夫だったか」と言ってくださったことを覚えています。"},{"id":{"d":1016,"n":7,"t":2},"speaker":"A","text":"先生は職員室から飛んできてくださったのですね。"},{"id":{"d":1016,"n":8,"t":2},"speaker":"B","text":"はい。子どもながら、先生への信頼が一層厚くなりました。"}];
INSERT INTO sentence [{"id":{"d":1017,"n":1,"t":2},"speaker":"A","text":"小林さんは学習塾でアルバイトをしているのですよね。私も塾のアルバイトに申し込んでみようと思っているので、どのような様子か聞かせてもらえますか？"},{"id":{"d":1017,"n":2,"t":2},"speaker":"B","text":"私は大勢の子に教えているのではなく、個人指導をしているので、参考になるかどうか分かりませんが、それでも良いですか？"},{"id":{"d":1017,"n":3,"t":2},"speaker":"A","text":"結構です。お願いします。小林さんは中学生に教えているのですか？"},{"id":{"d":1017,"n":4,"t":2},"speaker":"B","text":"主に中学2年生と3年生に教えています。マンツーマン指導のことが多いのですが、2人の生徒を同時に指導することもたまにあります。"},{"id":{"d":1017,"n":5,"t":2},"speaker":"A","text":"少人数ですと、生徒にじっくりと向き合うことができて良いですね。マンツーマンであるが故に困ったことはありますか？"},{"id":{"d":1017,"n":6,"t":2},"speaker":"B","text":"いいえ、特に困ったことはありません。中学生は1対1で向き合うと、どの子も素直です。こちらが真剣に指導すれば、一生懸命取り組んでくれます。"},{"id":{"d":1017,"n":7,"t":2},"speaker":"A","text":"私は教職を目指しているので、大勢の子に指導するトレーニングとして塾講師のアルバイトをしたいと考えていたのですが、お話を伺って、1人の生徒にじっくりと向き合うことにも興味が出てきました。"},{"id":{"d":1017,"n":8,"t":2},"speaker":"B","text":"子どもへの理解を深めるという意味でも、個人指導を経験してみることをお勧めします。私のアルバイト先を教えましょうか？"},{"id":{"d":1017,"n":9,"t":2},"speaker":"A","text":"はい、教えてください。自宅からの交通の便も考慮しなくてはならないので、そちらにするかは分かりませんが、候補の一つにしたいと思います。"}];
INSERT INTO sentence [{"id":{"d":1018,"n":1,"t":2},"speaker":"A","text":"お母さん、夏休みの宿題の工作に使いたいので、お菓子などの空き箱を捨てずに取っておいてもらえますか？"},{"id":{"d":1018,"n":2,"t":2},"speaker":"B","text":"分かりました。大きさはどの位の物が良いですか？"},{"id":{"d":1018,"n":3,"t":2},"speaker":"A","text":"教科書の大きさぐらいの物が良いです。3から4個は集めたいと思っています。"},{"id":{"d":1018,"n":4,"t":2},"speaker":"B","text":"分かりました。夏休みの宿題は工作の他に読書感想文もありますよね。新しく本を買わなくてもよいですか？"},{"id":{"d":1018,"n":5,"t":2},"speaker":"A","text":"学校の図書館で面白そうな本を見つけたので借りてきました。夏休みの間はずっと借りていることができるので、じっくりと読んで感想文を書こうと思います。"},{"id":{"d":1018,"n":6,"t":2},"speaker":"B","text":"計画的で偉いですね。夏休みの宿題はそれらで全部ですか？"},{"id":{"d":1018,"n":7,"t":2},"speaker":"A","text":"あとは、音楽のリコーダーの演奏を行う宿題があります。『茶色の小びん』という曲を、楽譜を見ずに吹けるようにならなくてはなりません。お母さんに聴いてもらった方が頑張れると思います。"},{"id":{"d":1018,"n":8,"t":2},"speaker":"B","text":"分かりました。そういうことなら、毎日少しずつ練習しましょう。きっと楽譜を見ずに上手に吹けるようになりますよ。"}];
INSERT INTO sentence [{"id":{"d":1019,"n":1,"t":2},"speaker":"A","text":"最近、高校時代の部活の仲間達とSNSで繋がり、懐かしい話で盛り上がっています。"},{"id":{"d":1019,"n":2,"t":2},"speaker":"B","text":"一緒に汗を流した仲間とは、特別な繋がりがありますよね。渡辺さんは何部に入っていたのですか？"},{"id":{"d":1019,"n":3,"t":2},"speaker":"A","text":"水泳部に入っていました。先輩が厳しかったものですから、苦難をともに乗り越えた同期は結束が強いのです。"},{"id":{"d":1019,"n":4,"t":2},"speaker":"B","text":"水泳部の活動というと、とにかく泳ぐのでしょうか？"},{"id":{"d":1019,"n":5,"t":2},"speaker":"A","text":"上級生になるほど、プールに入ることのできる時間が長くなりますが、下級生は部活の時間の半分は走ったり筋力トレーニングをしたりして、陸上で過ごしていました。そのかいあってか、私の高校の水泳部は関東大会の常連校でした。"},{"id":{"d":1019,"n":6,"t":2},"speaker":"B","text":"関東地区の強豪とは、素晴らしいです。渡辺さんの高校で、水泳部は花形の部活だったのですね。"},{"id":{"d":1019,"n":7,"t":2},"speaker":"A","text":"はい。夏の大会前に高校の体育館で行われる壮行会では、水泳部が最前列でみんなから激励してもらっていました。"},{"id":{"d":1019,"n":8,"t":2},"speaker":"B","text":"渡辺さんは「時々、仕事帰りにプールに行く」と言っていましたよね。同期の皆さんも、今なお泳いでいる方が多いのですか？"},{"id":{"d":1019,"n":9,"t":2},"speaker":"A","text":"はい、みんな趣味として水泳を続けているようです。"}];
INSERT INTO sentence [{"id":{"d":1020,"n":1,"t":2},"speaker":"A","text":"秋に開催される同窓会に、浩子ちゃんは行きますか？"},{"id":{"d":1020,"n":2,"t":2},"speaker":"B","text":"同窓会の開催日は予定が入っていないので、私は参加しようと思っています。直子ちゃんはどうしますか？"},{"id":{"d":1020,"n":3,"t":2},"speaker":"A","text":"はい。私も行きたいと思っています。私にとっては、卒業後初めての同窓会です。担任の佐藤先生はいらっしゃるでしょうか？"},{"id":{"d":1020,"n":4,"t":2},"speaker":"B","text":"佐藤先生は毎回顔を出してくださっています。髪の色こそ白くなられましたが、お話の仕方などは全くお変わりなくて、今にも数学の授業が始まりそうな錯覚にとらわれますよ。"},{"id":{"d":1020,"n":5,"t":2},"speaker":"A","text":"それは楽しみです。例年、同窓会には大勢が参加していますか？"},{"id":{"d":1020,"n":6,"t":2},"speaker":"B","text":"最近はメンバーが固定されてきている感もありますが、クラスの半分以上は集まります。体育祭で応援団長だった中野君と副団長だった裕子ちゃんが交互に幹事を務めてくれています。"},{"id":{"d":1020,"n":7,"t":2},"speaker":"A","text":"中野君と裕子ちゃんが幹事さんですか。今なおクラスのリーダーなのですね。同窓会には軽音楽部の小林君も来ていますか？彼は独特の価値観を持っていたので、どのような社会人になったのか気になります。"},{"id":{"d":1020,"n":8,"t":2},"speaker":"B","text":"小林君もほぼ毎回参加しています。彼は「中学校の先生になった」と言っていました。社会科、特に地理を教えているそうです。世界を1人で旅した経験を生徒たちに話して聞かせるそうです。"},{"id":{"d":1020,"n":9,"t":2},"speaker":"A","text":"まさか彼が教師になっているとは思いませんでした。でも、確かに彼のように柔軟で自由な教師がいれば、落ちこぼれや不登校の生徒がいなくなるような気がします。"}];
INSERT INTO sentence [{"id":{"d":1021,"n":1,"t":2},"speaker":"A","text":"私は生まれた時からずっと今の家に住んでいて、一度も転校したことがありません。森田君は転校したことはありますか？"},{"id":{"d":1021,"n":2,"t":2},"speaker":"B","text":"小学校4年生の時に転校しました。僕は元々人見知りだったので、新しい環境で一層人としゃべれなくなってしまいました。"},{"id":{"d":1021,"n":3,"t":2},"speaker":"A","text":"今の森田君からは人見知りだった森田少年は想像がつきません。転校後の学校生活はつらかったのではありませんか？"},{"id":{"d":1021,"n":4,"t":2},"speaker":"B","text":"はい。勉強に付いていけず、落ちこぼれ気味でしたし、不登校になってもおかしくない状況だったと思います。"},{"id":{"d":1021,"n":5,"t":2},"speaker":"A","text":"よく、頑張って学校に通い続けましたね。"},{"id":{"d":1021,"n":6,"t":2},"speaker":"B","text":"同じクラスのある女の子が毎朝僕を迎えに来て、一緒に学校に行ってくれました。一言もしゃべらない僕に、その子はクラスのみんなのことや学校の先生のことなどをしゃべり続けてくれました。そうして1カ月もすると、僕も学校のことに随分と詳しくなり、徐々にクラスのみんなに打ち解けていくことができました。"},{"id":{"d":1021,"n":7,"t":2},"speaker":"A","text":"そうでしたか。その女の子のおかげで、無事に学校に通うことができたわけですね。"},{"id":{"d":1021,"n":8,"t":2},"speaker":"B","text":"はい。大人になった今もその子に対する感謝の気持ちは消えません。"}];
INSERT INTO sentence [{"id":{"d":1022,"n":1,"t":2},"speaker":"A","text":"聡君は定期テストも頑張っていますし、模擬テストの合格判定もコンスタントにB以上が出ていますから、高校入試は心配ないでしょう。"},{"id":{"d":1022,"n":2,"t":2},"speaker":"B","text":"それを伺って安心しました。このまま油断せずに頑張ってほしいと思います。"},{"id":{"d":1022,"n":3,"t":2},"speaker":"A","text":"とはいえ、何が起きるか分からないものが入学試験ですので、念のため、併願校を受けておく方がいいと思います。"},{"id":{"d":1022,"n":4,"t":2},"speaker":"B","text":"はい。1校でも合格できていれば、本命の高校の入試にいくらか落ち着いて臨めると思いますので、そのようにさせたいと思います。併願校は何校受けるべきでしょうか？"},{"id":{"d":1022,"n":5,"t":2},"speaker":"A","text":"聡君なら1校で十分ですよ。"},{"id":{"d":1022,"n":6,"t":2},"speaker":"B","text":"先生、1校だけ大丈夫でしょうか？少し心配な気がします。"},{"id":{"d":1022,"n":7,"t":2},"speaker":"A","text":"はい。お母さま、聡君の力を信じてあげてください。聡君はバスケットボール部のキャプテンも務めていましたから、内申点も申し分ありません。入試で実力が発揮できなかったとしても、合格ラインには到達できるはずです。"},{"id":{"d":1022,"n":8,"t":2},"speaker":"B","text":"はい。分かりました。親が信じてやらなくてはなりませんね。"}];
INSERT INTO sentence [{"id":{"d":1023,"n":1,"t":2},"speaker":"A","text":"秀夫君、放課後は公園でサッカーをしませんか？"},{"id":{"d":1023,"n":2,"t":2},"speaker":"B","text":"ごめんなさい。今日はそろばん教室に行くので放課後に遊ぶことができません。"},{"id":{"d":1023,"n":3,"t":2},"speaker":"A","text":"秀夫君は昨日もスイミング教室へ通う予定があって遊べませんでしたね。何曜日だと放課後に遊ぶことができますか？"},{"id":{"d":1023,"n":4,"t":2},"speaker":"B","text":"木曜日です。他の日は習い事があって遊べません。直樹君は習い事をしていませんか？"},{"id":{"d":1023,"n":5,"t":2},"speaker":"A","text":"僕は、月曜日にピアノのレッスンがあるだけで、他の日の放課後はいつでも遊べます。"},{"id":{"d":1023,"n":6,"t":2},"speaker":"B","text":"直樹君がピアノを習っているとは知りませんでした。小さい頃からずっと習っているのですか？"},{"id":{"d":1023,"n":7,"t":2},"speaker":"A","text":"はい。ピアノは3歳の頃から習っています。僕は体を動かすことが好きですが、ピアノを弾くことはさらに好きです。"},{"id":{"d":1023,"n":8,"t":2},"speaker":"B","text":"僕は、鍵盤ハーモニカは全然弾けず、楽器はリコーダーくらいしか演奏できません。ピアノが弾けるとは尊敬します。今度、音楽室に行った時にピアノを弾いて演奏を聴かせてください。"}];
INSERT INTO sentence [{"id":{"d":1024,"n":1,"t":2},"speaker":"A","text":"中条さん、中学校の合唱コンクールは保護者も観覧できますが、行きますか？"},{"id":{"d":1024,"n":2,"t":2},"speaker":"B","text":"保護者も観覧できるのですね。それでしたら行きたいと思います。藤沢さんは行かれますか？"},{"id":{"d":1024,"n":3,"t":2},"speaker":"A","text":"はい。私は行く予定です。長男が3年生の時に初めて合唱コンクールを見に行ったのですが、素晴らしい歌声で感動しました。感動すると同時に1年生の時から聴きに来れば良かったと後悔もしました。それで、美樹が中学生になったら1年生の時から行こうと決めていたのです。"},{"id":{"d":1024,"n":4,"t":2},"speaker":"B","text":"そうですか。中学校の横を通ると、美しい歌声が聴こえてくることがありますね。第三中学校は合唱が盛んなのでしょうか？"},{"id":{"d":1024,"n":5,"t":2},"speaker":"A","text":"はい。みんなで一つのことをやり遂げることが、中学生の成長に良い影響を及ぼすという理念の下、第三中学校は合唱に力を入れているのだそうです。合唱コンクールでは見た目に行儀の悪い子たちも指揮者の指揮棒に集中して一生懸命歌っていて、その様子に心を打たれました。"},{"id":{"d":1024,"n":6,"t":2},"speaker":"B","text":"とても楽しみです。他のクラス、他の学年の合唱も聴きたいと思います。"},{"id":{"d":1024,"n":7,"t":2},"speaker":"A","text":"お時間があるのでしたら、最初から最後まで観覧されることをお勧めします。"},{"id":{"d":1024,"n":8,"t":2},"speaker":"B","text":"コンクールの日は何も予定が入っていませんので、朝から行くことができます。藤沢さんも最初から行かれますか？"},{"id":{"d":1024,"n":9,"t":2},"speaker":"A","text":"はい、その予定です。よろしければ一緒に行きませんか？会場のふれあいプラザまで車で行きますので、お迎えに上がりますよ。"},{"id":{"d":1024,"n":10,"t":2},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":1025,"n":1,"t":2},"speaker":"A","text":"再来週の土曜日は高校の体育祭だそうです。仕事が休みなら、体育祭を見に行きませんか？"},{"id":{"d":1025,"n":2,"t":2},"speaker":"B","text":"良いですね、行きましょう。私たちが卒業してから10年ですね。体育祭は様変わりしているでしょうか？"},{"id":{"d":1025,"n":3,"t":2},"speaker":"A","text":"聞くところによると、応援の規模も縮小されてこぢんまりした体育祭になっているそうです。"},{"id":{"d":1025,"n":4,"t":2},"speaker":"B","text":"応援団長だった丸山君や副団長だった赤井さんも誘おうと思っていたのですが、2人とも落胆するでしょうか？"},{"id":{"d":1025,"n":5,"t":2},"speaker":"A","text":"少しは落胆するかもしれませんが、懐かしくて喜びの気持ちの方が強いと思いますよ。2人とも誘いましょう。"},{"id":{"d":1025,"n":6,"t":2},"speaker":"B","text":"体育祭前は昼休みも放課後も応援練習や、デコレーションパネルの作成に明け暮れましたね。家に帰っても応援衣装を作るなど、何もかも放り出して体育祭に熱中しました。"},{"id":{"d":1025,"n":7,"t":2},"speaker":"A","text":"懐かしいですね。私たちのクラスは色別対抗リレーで優勝しましたよね。"},{"id":{"d":1025,"n":8,"t":2},"speaker":"B","text":"いいえ、色別対抗リレーは準優勝でした。僅差で青組に及びませんでした。"},{"id":{"d":1025,"n":9,"t":2},"speaker":"A","text":"そうでしたか。記憶を美化していました。デコレーションパネルは優勝でしたよね。"},{"id":{"d":1025,"n":10,"t":2},"speaker":"B","text":"そうです、優勝です。デコレーションパネル作りのリーダーの山本君が飛び上がって喜んでいたのを覚えています。"}];
INSERT INTO sentence [{"id":{"d":1026,"n":1,"t":2},"speaker":"A","text":"そろそろ進路を考えなくてはならない時期ですが、私は自分が理系を選択すべきなのか文系を選択すべきなのかも決めかねています。"},{"id":{"d":1026,"n":2,"t":2},"speaker":"B","text":"晴子ちゃんの定期テストの成績は、理系科目と文系科目ではどちらが良いのですか？"},{"id":{"d":1026,"n":3,"t":2},"speaker":"A","text":"どちらも同じくらいの成績です。"},{"id":{"d":1026,"n":4,"t":2},"speaker":"B","text":"それはうらやましいことです。私は国語が苦手です。古文も漢文も現代文もテストでは良い点が取れません。だから、理系に進むことは確定です。"},{"id":{"d":1026,"n":5,"t":2},"speaker":"A","text":"成績がよくて決められないのではなく、文系にも、理系にも足を引っ張る科目があって、困っている状況です。"},{"id":{"d":1026,"n":6,"t":2},"speaker":"B","text":"晴子ちゃんの興味があることは何なのですか？"},{"id":{"d":1026,"n":7,"t":2},"speaker":"A","text":"私は中学校の教科でいうところの家庭科が好きなのです。調理や被服に関することに興味がありますね。実技・筆記試験ともに得意です。"},{"id":{"d":1026,"n":8,"t":2},"speaker":"B","text":"それなら家政学のある大学をピックアップしてみてはどうでしょう。学科によって文系寄りだったり理系寄りだったりするようなので、理系か文系か決めかねている晴子ちゃんに合っているかもしれませんよ。"}];
INSERT INTO sentence [{"id":{"d":1027,"n":1,"t":2},"speaker":"A","text":"今日は篤史君の入学式でしたね。ご入学おめでとうございます。ついこの間まで、頼りない足取りで歩いていた篤史君が小学校に入学だとは、よそのお子さんの成長はあっという間です。"},{"id":{"d":1027,"n":2,"t":2},"speaker":"B","text":"ありがとうございます。おかげさまで、篤史も無事に小学生になりました。"},{"id":{"d":1027,"n":3,"t":2},"speaker":"A","text":"篤史君は小学校の制服が似合っていますね。担任の先生はどなたでしたか？"},{"id":{"d":1027,"n":4,"t":2},"speaker":"B","text":"赤坂先生という女性の先生でした。ベテランの先生のようです。"},{"id":{"d":1027,"n":5,"t":2},"speaker":"A","text":"赤坂先生はわが家の雄二が1年生の時の担任の先生でした。きめ細やかな指導をしてくださる先生でしたよ。"},{"id":{"d":1027,"n":6,"t":2},"speaker":"B","text":"それは安心しました。篤史は小学1年生ですが、私も小学生の保護者1年生なので、いろいろと不安なのです。"},{"id":{"d":1027,"n":7,"t":2},"speaker":"A","text":"そうですよね。そのような意味では私は先輩ですので、何かご心配なことがありましたら聞いてください。"},{"id":{"d":1027,"n":8,"t":2},"speaker":"B","text":"ありがとうございます。とても心強いです。早速ですが、PTAの役員について教えていただきたいです。"}];
INSERT INTO sentence [{"id":{"d":1028,"n":1,"t":2},"speaker":"A","text":"中川先生が放課後に体操を教えてくださるそうです。希望する人はこの申込書に保護者の署名をもらって提出するようにとのことです。"},{"id":{"d":1028,"n":2,"t":2},"speaker":"B","text":"由美は参加したいのですか？"},{"id":{"d":1028,"n":3,"t":2},"speaker":"A","text":"はい。体操は得意ではありませんが、できるようになりたいので、参加したいと思います。"},{"id":{"d":1028,"n":4,"t":2},"speaker":"B","text":"分かりました。では、署名をしましょう。体操教室は毎日あるのですか？"},{"id":{"d":1028,"n":5,"t":2},"speaker":"A","text":"週に2回、火曜日と木曜日だそうです。私は、まずは5段の跳び箱を跳べるようになることが目標です。"},{"id":{"d":1028,"n":6,"t":2},"speaker":"B","text":"目標をはっきりと決めるのは良いことですね。中川先生は大学生の頃は体操の選手で、大きな大会に出たことがあると聞いたことがあります。きっと上手に教えてくださると思いますよ。"},{"id":{"d":1028,"n":7,"t":2},"speaker":"A","text":"跳び箱が跳べるようになったらマット運動も頑張って、側転やバク転もできるようになりたいです。"},{"id":{"d":1028,"n":8,"t":2},"speaker":"B","text":"良いですね。実はお母さんも体操は苦手だったのです。由美ができるようになったら教えてもらおうかと思います。"}];
INSERT INTO sentence [{"id":{"d":1029,"n":1,"t":2},"speaker":"A","text":"おはようございます。掲示板はもう見ましたか？"},{"id":{"d":1029,"n":2,"t":2},"speaker":"B","text":"いいえ、まだ確認していません。"},{"id":{"d":1029,"n":3,"t":2},"speaker":"A","text":"今週の私が担当している講義ですが、都合により休講になります。"},{"id":{"d":1029,"n":4,"t":2},"speaker":"B","text":"そうなのですね。では、来週に予定されていた試験はどうなりますか？"},{"id":{"d":1029,"n":5,"t":2},"speaker":"A","text":"申し訳ありませんが、試験も延期になります。"},{"id":{"d":1029,"n":6,"t":2},"speaker":"B","text":"承知しました。"},{"id":{"d":1029,"n":7,"t":2},"speaker":"A","text":"この旨を、クラスの皆さんに伝えてもらえますか？"},{"id":{"d":1029,"n":8,"t":2},"speaker":"B","text":"お任せください。グループLINEで皆さん宛に、休講と試験延期の件に関するメッセージを送信します。"},{"id":{"d":1029,"n":9,"t":2},"speaker":"A","text":"試験の詳細はまた後日お知らせしますね。"},{"id":{"d":1029,"n":10,"t":2},"speaker":"B","text":"分かりました。お待ちしております。"}];
INSERT INTO sentence [{"id":{"d":1030,"n":1,"t":2},"speaker":"A","text":"佐倉さん、こんにちは。先日延期した試験についての連絡です。先週に引き続きすみませんが、またクラスの人たちに連絡をお願いできますか？"},{"id":{"d":1030,"n":2,"t":2},"speaker":"B","text":"分かりました。クラスのみんなが、その情報を待っていましたよ。"},{"id":{"d":1030,"n":3,"t":2},"speaker":"A","text":"単位認定の関係で、テストではなく、レポートで成績を付けることにしました。"},{"id":{"d":1030,"n":4,"t":2},"speaker":"B","text":"本当ですか？うれしいです。"},{"id":{"d":1030,"n":5,"t":2},"speaker":"A","text":"レポートの形式については明日にも掲示板に載せるので、そちらをご確認くださいね。"},{"id":{"d":1030,"n":6,"t":2},"speaker":"B","text":"承知いたしました。ちなみに、レポート用紙は何枚分ですか？"},{"id":{"d":1030,"n":7,"t":2},"speaker":"A","text":"A4用紙に6枚と考えています。"},{"id":{"d":1030,"n":8,"t":2},"speaker":"B","text":"かなり重みのある課題ですね。"}];
INSERT INTO sentence [{"id":{"d":1031,"n":1,"t":2},"speaker":"A","text":"佐倉さんは、この授業の試験がなくなったことをもう聞きましたか？"},{"id":{"d":1031,"n":2,"t":2},"speaker":"B","text":"はい。私はテストよりレポートを書く方が得意だから、うれしいです。"},{"id":{"d":1031,"n":3,"t":2},"speaker":"A","text":"私は、テストの方が良かったです。周りと差を付けられますから。"},{"id":{"d":1031,"n":4,"t":2},"speaker":"B","text":"さすが、成績優秀者は言うことが違いますね。"},{"id":{"d":1031,"n":5,"t":2},"speaker":"A","text":"せっかく教科書の隅の隅まで覚えたのに、残念です。"}];
INSERT INTO sentence [{"id":{"d":1032,"n":1,"t":2},"speaker":"A","text":"失敗です。寝坊してしまいました。"},{"id":{"d":1032,"n":2,"t":2},"speaker":"B","text":"おはようございます。ひとまず落ち着いてください。今日の授業は何限からなのですか？"},{"id":{"d":1032,"n":3,"t":2},"speaker":"A","text":"頭が回らなくて思い出せません。今日は何曜日ですか？"},{"id":{"d":1032,"n":4,"t":2},"speaker":"B","text":"水曜日ですよ。"},{"id":{"d":1032,"n":5,"t":2},"speaker":"A","text":"それなら大丈夫です。水曜は3限まで授業を入れていませんから。"},{"id":{"d":1032,"n":6,"t":2},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":1032,"n":7,"t":2},"speaker":"A","text":"履修登録をしている授業も、昼までないことですし、二度寝します。昼ごろに起こしてくれませんか？"},{"id":{"d":1032,"n":8,"t":2},"speaker":"B","text":"良いですよ。もう課題は済んでいるということですよね？"},{"id":{"d":1032,"n":9,"t":2},"speaker":"A","text":"危ない、課題が出ていたことを忘れていました。寝るのは諦めて、課題に取りかかります。"}];
INSERT INTO sentence [{"id":{"d":1033,"n":1,"t":2},"speaker":"A","text":"瀬島さんは、大学から案内が来ていたティーチングアシスタントの募集に応募しましたか？"},{"id":{"d":1033,"n":2,"t":2},"speaker":"B","text":"私は応募していません。アルバイト代が出るわけではありませんから。"},{"id":{"d":1033,"n":3,"t":2},"speaker":"A","text":"いえ、時給2,000円もらえますよ。"},{"id":{"d":1033,"n":4,"t":2},"speaker":"B","text":"そうなのですか？破格の時給ですね。今すぐ申し込んできます。"},{"id":{"d":1033,"n":5,"t":2},"speaker":"A","text":"応募条件の中に、「9月5日から9月15日まで全日出られる人」とありましたが、予定は大丈夫ですか？"},{"id":{"d":1033,"n":6,"t":2},"speaker":"B","text":"その期間はアルバイトの予定を入れていましたが、こちらのお手当の方がはるかに良いので、アルバイトは休みます。"},{"id":{"d":1033,"n":7,"t":2},"speaker":"A","text":"教授は応募してくれる人がいたら助かると思いますが、アルバイト先から怒られませんか？"},{"id":{"d":1033,"n":8,"t":2},"speaker":"B","text":"怒られることはありませんよ。元々熱心な従業員ではありませんから。"},{"id":{"d":1033,"n":9,"t":2},"speaker":"A","text":"それはそれで問題ですね。"},{"id":{"d":1033,"n":10,"t":2},"speaker":"B","text":"店長からも、「片手間で構わない」と言われていますから。では、早速今から学務課へ行ってきます。"}];
INSERT INTO sentence [{"id":{"d":1034,"n":1,"t":2},"speaker":"A","text":"学生食堂の前に七夕の短冊が飾ってありましたね。いくつか読んでみたら、ユーモアにあふれたものばかりでした。"},{"id":{"d":1034,"n":2,"t":2},"speaker":"B","text":"私も目に留まったものを読みましたが、なかなか面白かったです。"},{"id":{"d":1034,"n":3,"t":2},"speaker":"A","text":"特に面白かった短冊にはどのようなことが書かれていましたか？"},{"id":{"d":1034,"n":4,"t":2},"speaker":"B","text":"「学生食堂の限定メニューを、売り切れ前に買えますように」というお願い事がかわいらしくて印象に残っています。"},{"id":{"d":1034,"n":5,"t":2},"speaker":"A","text":"2限後の学生食堂はかなり混雑していて、人気のメニューは即完売してしまいますよね。"},{"id":{"d":1034,"n":6,"t":2},"speaker":"B","text":"ええ。限定メニューともなると、相当な競争率かと思われます。"},{"id":{"d":1034,"n":7,"t":2},"speaker":"A","text":"織姫と彦星があの短冊を見て、願いをかなえてくれると良いですね。"},{"id":{"d":1034,"n":8,"t":2},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":1035,"n":1,"t":2},"speaker":"A","text":"ノートパソコンを起動しながら食事をすると、食べこぼしたものがキーボードの隙間に挟まりますよ。"},{"id":{"d":1035,"n":2,"t":2},"speaker":"B","text":"分かっていますが、これからオンライン授業なのです。"},{"id":{"d":1035,"n":3,"t":2},"speaker":"A","text":"授業を受けつつ昼食を食べるのですか？"},{"id":{"d":1035,"n":4,"t":2},"speaker":"B","text":"ええ。食事をしていれば眠くならないので、むしろ集中できるのですよ。"},{"id":{"d":1035,"n":5,"t":2},"speaker":"A","text":"発想の転換ですね。授業のみに集中することが一番良いことは言うまでもありませんが。"},{"id":{"d":1035,"n":6,"t":2},"speaker":"B","text":"対面授業であれば集中できますが、オンライン授業には一工夫が必要なのです。"},{"id":{"d":1035,"n":7,"t":2},"speaker":"A","text":"あなたの主張はよく分かりました。食べこぼしたものの行方にだけは気を付けていてくださいね。"},{"id":{"d":1035,"n":8,"t":2},"speaker":"B","text":"はい、気を付けます。"}];
INSERT INTO sentence [{"id":{"d":1036,"n":1,"t":2},"speaker":"A","text":"中学校に足を踏み入れたのは卒業以来です。何となく緊張します。"},{"id":{"d":1036,"n":2,"t":2},"speaker":"B","text":"私もです。母校訪問は、長らく実行したいと思っていたことの一つなのでうれしいです。"},{"id":{"d":1036,"n":3,"t":2},"speaker":"A","text":"実現できて良かったですよね。今日をずっと楽しみにしていました。"},{"id":{"d":1036,"n":4,"t":2},"speaker":"B","text":"前方の柱を見てください。あの『走るべからず』の張り紙、まだ使われていたのですね。"},{"id":{"d":1036,"n":5,"t":2},"speaker":"A","text":"習字の時間に吉田さんが書いたものですよね。今見ても、お手本のようなきれいな字ですね。"},{"id":{"d":1036,"n":6,"t":2},"speaker":"B","text":"そうですね。確か、吉田さんのお母さまは書道教室の先生だった気がします。"},{"id":{"d":1036,"n":7,"t":2},"speaker":"A","text":"初耳です。でも、この字を見る限り、なるほどと納得できますね。"},{"id":{"d":1036,"n":8,"t":2},"speaker":"B","text":"吉田さんにも久しぶりに会いたくなってきましたよ。"},{"id":{"d":1036,"n":9,"t":2},"speaker":"A","text":"そうですね。近いうちに同窓会を計画しましょう。"}];
INSERT INTO sentence [{"id":{"d":1037,"n":1,"t":2},"speaker":"A","text":"結衣ちゃん、廊下の方を何度も確認しているようですが、何かあるのですか？"},{"id":{"d":1037,"n":2,"t":2},"speaker":"B","text":"廊下ではなく、隣の美術室を見ていました。今、美術の授業を受けているのは、何組か分かりますか？"},{"id":{"d":1037,"n":3,"t":2},"speaker":"A","text":"多分、B組だと思いますよ。先ほどB組の杏奈ちゃんが絵の具を借りにきていましたから。"},{"id":{"d":1037,"n":4,"t":2},"speaker":"B","text":"やはり、B組でしたか。実はB組に彼氏がいるので、姿が見えたらうれしいなと思っていたのです。"},{"id":{"d":1037,"n":5,"t":2},"speaker":"A","text":"お相手が誰か気になります。同じ部活の相沢君ですか？"},{"id":{"d":1037,"n":6,"t":2},"speaker":"B","text":"いえ、杉山君です。"},{"id":{"d":1037,"n":7,"t":2},"speaker":"A","text":"そうだったのですね。後でなれ初めを聞かせてください。"},{"id":{"d":1037,"n":8,"t":2},"speaker":"B","text":"恥ずかしいですが、良いですよ。"}];
INSERT INTO sentence [{"id":{"d":1038,"n":1,"t":2},"speaker":"A","text":"山田さん、私たちは今週、理科室の清掃担当でしたよね？"},{"id":{"d":1038,"n":2,"t":2},"speaker":"B","text":"そうですよ。あと10分したら理科室に行こうと思います。"},{"id":{"d":1038,"n":3,"t":2},"speaker":"A","text":"では、10分ここで待っていますね。一緒に行きましょう。"},{"id":{"d":1038,"n":4,"t":2},"speaker":"B","text":"私のことは気にせず、先に向かってくれて構いませんよ。"},{"id":{"d":1038,"n":5,"t":2},"speaker":"A","text":"いえ、理科室は幽霊が出るといううわさを聞いたので、怖くて1人では行けません。"},{"id":{"d":1038,"n":6,"t":2},"speaker":"B","text":"そうですか。見えないものをそれほどに怖がっても仕方がありませんよ。"},{"id":{"d":1038,"n":7,"t":2},"speaker":"A","text":"分かっていますが、幽霊のいるところには行きたくありませんよ。山田さんは怖くありませんか？"},{"id":{"d":1038,"n":8,"t":2},"speaker":"B","text":"私は大丈夫ですよ。では、そこで待っていてください。終わったら声を掛けますね。"}];
INSERT INTO sentence [{"id":{"d":1039,"n":1,"t":2},"speaker":"A","text":"悠太君、一緒に帰りましょう。"},{"id":{"d":1039,"n":2,"t":2},"speaker":"B","text":"今日の小テストが不合格で、放課後に補習を受けなければいけなくなりました。"},{"id":{"d":1039,"n":3,"t":2},"speaker":"A","text":"つまり、一緒に帰ることはできないということですか？水曜は一緒に帰る日にしようと約束したじゃありませんか。"},{"id":{"d":1039,"n":4,"t":2},"speaker":"B","text":"そうなります。怒らないでくださいよ。先生の言い付けを破ってまで、今から帰宅しろというのですか？"},{"id":{"d":1039,"n":5,"t":2},"speaker":"A","text":"いえ、約束を果たせないことが決まった時点で教えてくれなかったことに腹を立てているのです。"},{"id":{"d":1039,"n":6,"t":2},"speaker":"B","text":"どうせ教室まで来てくれるから、その時に言えば良いと思ったのですよ。"},{"id":{"d":1039,"n":7,"t":2},"speaker":"A","text":"それはひどいです。それなら私も友達と一緒に遊びに行きたかったです。誘われたのに断ってしまいました。"},{"id":{"d":1039,"n":8,"t":2},"speaker":"B","text":"ごめんなさい。次から気を付けます。"}];
INSERT INTO sentence [{"id":{"d":1040,"n":1,"t":2},"speaker":"A","text":"中間テストがいよいよ来週から始まります。勉強は順調に進んでいますか？"},{"id":{"d":1040,"n":2,"t":2},"speaker":"B","text":"いえ、集中力が続かなくて困っています。何か良い方法はありますか？"},{"id":{"d":1040,"n":3,"t":2},"speaker":"A","text":"寝る時間を確保することです。きちんと睡眠は取っていますか？睡眠時間が少ないと集中力に影響が出ますよ。"},{"id":{"d":1040,"n":4,"t":2},"speaker":"B","text":"そうなのですね。確かに、連日のテスト勉強で十分な睡眠が取れていません。"},{"id":{"d":1040,"n":5,"t":2},"speaker":"A","text":"そうでしたか。私も寝不足の時は不用意なミスをしがちです。よく寝るようにしてくださいね。"},{"id":{"d":1040,"n":6,"t":2},"speaker":"B","text":"先生もですか。先生がミスしているところを見たことがないので意外です。"},{"id":{"d":1040,"n":7,"t":2},"speaker":"A","text":"毎週末配布される学校便りですが、私は寝る前に作成しているのですよ。そこでミスをしがちです。"},{"id":{"d":1040,"n":8,"t":2},"speaker":"B","text":"そうだったのですか。"},{"id":{"d":1040,"n":9,"t":2},"speaker":"A","text":"あの便りに含まれているミスの数は、私の睡眠バロメーターだと思ってください。"}];
INSERT INTO sentence [{"id":{"d":1041,"n":1,"t":2},"speaker":"A","text":"来年から、理系と文系に分かれて授業が行われます。ご存じでしたか？"},{"id":{"d":1041,"n":2,"t":2},"speaker":"B","text":"はい。知っています。"},{"id":{"d":1041,"n":3,"t":2},"speaker":"A","text":"では、文理コース選択の希望調査用紙の提出が昨日までだったことは知っていますか？"},{"id":{"d":1041,"n":4,"t":2},"speaker":"B","text":"いいえ。すっかり忘れていました。明日一番に先生に提出します。"},{"id":{"d":1041,"n":5,"t":2},"speaker":"A","text":"できれば今日中に出して欲しかったのですが。"},{"id":{"d":1041,"n":6,"t":2},"speaker":"B","text":"すみません。親と相談したいので、今日中に提出するのは難しいです。"},{"id":{"d":1041,"n":7,"t":2},"speaker":"A","text":"将来を決める重要な選択ですので、親御さんと相談するのは大切ですね。では、明日必ず提出してください。"},{"id":{"d":1041,"n":8,"t":2},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1042,"n":1,"t":2},"speaker":"A","text":"次は体育の授業時間ですね。"},{"id":{"d":1042,"n":2,"t":2},"speaker":"B","text":"今日から、待ち望んだプールの授業が始まります。"},{"id":{"d":1042,"n":3,"t":2},"speaker":"A","text":"楽しみにしているところ申し訳ありませんが、今日の午後は、天気予報では雨と予想されているのです。"},{"id":{"d":1042,"n":4,"t":2},"speaker":"B","text":"兄が言っていましたが、プールの授業は雨が降っても実施されるみたいですよ。"},{"id":{"d":1042,"n":5,"t":2},"speaker":"A","text":"そうなのですか。それは良かったです。"},{"id":{"d":1042,"n":6,"t":2},"speaker":"B","text":"ちなみに、雷の予報だと中止になってしまうとの情報も聞きました。"},{"id":{"d":1042,"n":7,"t":2},"speaker":"A","text":"「所により雷」と気象予報士さんが言っていました。"},{"id":{"d":1042,"n":8,"t":2},"speaker":"B","text":"もし雷が鳴れば、プール開きは延期になりそうですね。残念です。"}];
INSERT INTO sentence [{"id":{"d":1043,"n":1,"t":2},"speaker":"A","text":"この春から、警察学校へ入学することが決まりました。"},{"id":{"d":1043,"n":2,"t":2},"speaker":"B","text":"おめでとうございます。警察学校とは、どのようなことを学ぶのですか？"},{"id":{"d":1043,"n":3,"t":2},"speaker":"A","text":"座学では法律などの勉強をするそうです。また、柔道や剣道、長距離走などの体力作りもするそうです。"},{"id":{"d":1043,"n":4,"t":2},"speaker":"B","text":"そうですか。いつごろ卒業されるのですか？"},{"id":{"d":1043,"n":5,"t":2},"speaker":"A","text":"半年後です。半年間、基本的に外泊はできず、寮で生活します。"},{"id":{"d":1043,"n":6,"t":2},"speaker":"B","text":"制限が厳しいのですね。では、次にお会いできるのは10月ということでしょうか？"},{"id":{"d":1043,"n":7,"t":2},"speaker":"A","text":"はい、そうです。勉強面でも体力面でも成長して帰ってきます。"},{"id":{"d":1043,"n":8,"t":2},"speaker":"B","text":"楽しみにしています。頑張ってきてください。"}];
INSERT INTO sentence [{"id":{"d":1044,"n":1,"t":2},"speaker":"A","text":"失礼します。伊藤先生はいらっしゃいますか？"},{"id":{"d":1044,"n":2,"t":2},"speaker":"B","text":"はい、どうしましたか？"},{"id":{"d":1044,"n":3,"t":2},"speaker":"A","text":"次の期末テストの範囲について質問したいのですが、お時間よろしいでしょうか？"},{"id":{"d":1044,"n":4,"t":2},"speaker":"B","text":"大丈夫ですよ。"},{"id":{"d":1044,"n":5,"t":2},"speaker":"A","text":"先生は昨日、テスト範囲を教科書の5ページから23ページまでと仰いました。"},{"id":{"d":1044,"n":6,"t":2},"speaker":"B","text":"ええ。そうです。"},{"id":{"d":1044,"n":7,"t":2},"speaker":"A","text":"しかし、隣のクラスの子に聞いたら、「テスト範囲は、教科書の3ページから25ページまで」と言っていたのです。"},{"id":{"d":1044,"n":8,"t":2},"speaker":"B","text":"それは間違いです。あなたのクラスの生徒にお伝えした情報が正しいです。"},{"id":{"d":1044,"n":9,"t":2},"speaker":"A","text":"分かりました。教科書の5ページから23ページが正しいテスト範囲ですね？"},{"id":{"d":1044,"n":10,"t":2},"speaker":"B","text":"そうです。隣のクラスには私から訂正しておきます。教えてくれてありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":1045,"n":1,"t":2},"speaker":"A","text":"家庭科の授業と技術科の授業は、3年生から選択制になると聞きましたが、本当でしょうか？"},{"id":{"d":1045,"n":2,"t":2},"speaker":"B","text":"はい。どちらかを選択して、1年を通して学ぶことになります。"},{"id":{"d":1045,"n":3,"t":2},"speaker":"A","text":"それぞれ、どのようなものを作成する予定ですか？"},{"id":{"d":1045,"n":4,"t":2},"speaker":"B","text":"家庭科はエプロンを、技術科は懐中時計を作成します。"},{"id":{"d":1045,"n":5,"t":2},"speaker":"A","text":"もし希望者が偏ってしまった場合、どのように振り分けますか？"},{"id":{"d":1045,"n":6,"t":2},"speaker":"B","text":"抽選です。しかし、例年、希望者数に大きな偏りはありません。"},{"id":{"d":1045,"n":7,"t":2},"speaker":"A","text":"不思議ですね。教えてくださり、ありがとうございました。"},{"id":{"d":1045,"n":8,"t":2},"speaker":"B","text":"どういたしまして。クラスの人にも伝えておいてもらえますか？"},{"id":{"d":1045,"n":9,"t":2},"speaker":"A","text":"はい。分かりました。"}];
INSERT INTO sentence [{"id":{"d":1046,"n":1,"t":2},"speaker":"A","text":"もえちゃんの得意教科を教えてください。"},{"id":{"d":1046,"n":2,"t":2},"speaker":"B","text":"音楽です。幼稚園生の頃からギターを習っているので、リズム感には自信があります。"},{"id":{"d":1046,"n":3,"t":2},"speaker":"A","text":"先月のクラス会で、見事な演奏を披露していましたよね。ギターを弾けるとは格好良いです。"},{"id":{"d":1046,"n":4,"t":2},"speaker":"B","text":"ありがとうございます。あずさちゃんはどの教科が得意ですか？"},{"id":{"d":1046,"n":5,"t":2},"speaker":"A","text":"英語です。洋画が好きで日常的に英語に触れているので自然と覚えました。"},{"id":{"d":1046,"n":6,"t":2},"speaker":"B","text":"私は昨日から、来週の英語の小テストに向けて勉強を始めたのですが、理解できない箇所がいくつかあります。ぜひあずさちゃんに教えてもらいたいです。"},{"id":{"d":1046,"n":7,"t":2},"speaker":"A","text":"ええ、もちろんです。不定詞の範囲ですか？"},{"id":{"d":1046,"n":8,"t":2},"speaker":"B","text":"はい、そうです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":1047,"n":1,"t":2},"speaker":"A","text":"初めまして。岡田です。これから3年間、よろしくお願いいたします。"},{"id":{"d":1047,"n":2,"t":2},"speaker":"B","text":"上山です。私は岡田さんと初めて出会ったのではない気がします。"},{"id":{"d":1047,"n":3,"t":2},"speaker":"A","text":"以前どこかでお会いしましたか？申し訳ありませんが、思い出せません。"},{"id":{"d":1047,"n":4,"t":2},"speaker":"B","text":"入学前のオープンキャンパスで、一緒にグループワークをしませんでしたか？"},{"id":{"d":1047,"n":5,"t":2},"speaker":"A","text":"あの、プログラミング体験のグループワークですね。記憶がよみがえってきました。上山さん、お久しぶりです。"},{"id":{"d":1047,"n":6,"t":2},"speaker":"B","text":"岡田さんは、当時から高校生とは思えないプログラミング技術を持っていたので、お顔を覚えていました。"},{"id":{"d":1047,"n":7,"t":2},"speaker":"A","text":"実は、高校時代にパソコンクラブに所属していたので、プログラミングは得意なのです。"},{"id":{"d":1047,"n":8,"t":2},"speaker":"B","text":"そうだったのですね。ぜひ、コツを教えていただきたいです。"}];
INSERT INTO sentence [{"id":{"d":1048,"n":1,"t":2},"speaker":"A","text":"入学おめでとうございます。お名前を教えてください。"},{"id":{"d":1048,"n":2,"t":2},"speaker":"B","text":"中村です。"},{"id":{"d":1048,"n":3,"t":2},"speaker":"A","text":"私は2年生の斉藤です。部活動は何部に入るか既に決めていますか？"},{"id":{"d":1048,"n":4,"t":2},"speaker":"B","text":"バスケットボールに興味があるので、まずはバスケットボール部へ見学に行こうと考えていますが、どこの部に入部するかはまだ迷っています。"},{"id":{"d":1048,"n":5,"t":2},"speaker":"A","text":"そうですか。私はバレーボール部に所属しているのですが、身長が高い子にぜひ入部してほしいと考えています。中村さんの身長は何センチメートルですか？"},{"id":{"d":1048,"n":6,"t":2},"speaker":"B","text":"168センチメートルあります。"},{"id":{"d":1048,"n":7,"t":2},"speaker":"A","text":"高身長ですね。ぜひ、バレーボール部にも見学に来ませんか？レギュラー選手になれること間違いなしです。"},{"id":{"d":1048,"n":8,"t":2},"speaker":"B","text":"レギュラー選手になれるというのは、かなり魅力的です。明日、見学に行きます。"},{"id":{"d":1048,"n":9,"t":2},"speaker":"A","text":"ありがとうございます。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":1049,"n":1,"t":2},"speaker":"A","text":"部活動はどこに所属するか決めましたか？"},{"id":{"d":1049,"n":2,"t":2},"speaker":"B","text":"華道部か、吹奏楽部のどちらかに入部しようかと思い、その2択で悩んでいます。"},{"id":{"d":1049,"n":3,"t":2},"speaker":"A","text":"卓球部の活動も楽しそうでしたよ。体験入部に行きましたか？"},{"id":{"d":1049,"n":4,"t":2},"speaker":"B","text":"はい。しかし、私は体力に自信がないので運動部はやめておこうと思います。"},{"id":{"d":1049,"n":5,"t":2},"speaker":"A","text":"それでしたら、吹奏楽部も合わないかもしれません。運動部並みの有酸素運動をすると聞きました。"},{"id":{"d":1049,"n":6,"t":2},"speaker":"B","text":"そうなのですか。一体どうしてでしょうか？"},{"id":{"d":1049,"n":7,"t":2},"speaker":"A","text":"楽器を吹くのに、かなりの肺活量を必要とするためです。"},{"id":{"d":1049,"n":8,"t":2},"speaker":"B","text":"その情報を聞けて良かったです。華道部に入部することにします。"},{"id":{"d":1049,"n":9,"t":2},"speaker":"A","text":"良いですね。四季に触れることのできるすてきな部活動だと思います。"}];
INSERT INTO sentence [{"id":{"d":1050,"n":1,"t":2},"speaker":"A","text":"大変です。鍵盤ハーモニカを持ってくるのを忘れてしまいました。"},{"id":{"d":1050,"n":2,"t":2},"speaker":"B","text":"学期初めは忘れものをしてしまいますよね。"},{"id":{"d":1050,"n":3,"t":2},"speaker":"A","text":"なぜか息を吹き込むホースだけは持っているのですが、これでは意味がありませんね。"},{"id":{"d":1050,"n":4,"t":2},"speaker":"B","text":"あなたにはお姉さんがいませんでしたか？お姉さんから借りることができれば、授業が受けられますよ。"},{"id":{"d":1050,"n":5,"t":2},"speaker":"A","text":"そうですね。今から、姉に鍵盤ハーモニカを借りられないか聞きに行ってきます。"},{"id":{"d":1050,"n":6,"t":2},"speaker":"B","text":"授業開始まであと5分しかありません。急いでください。"},{"id":{"d":1050,"n":7,"t":2},"speaker":"A","text":"分かりました。もし授業に遅れたら、先生に理由を説明しておいてもらえませんか？"},{"id":{"d":1050,"n":8,"t":2},"speaker":"B","text":"分かりました。くれぐれも廊下は走らないようにしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1051,"n":1,"t":2},"speaker":"A","text":"今日の体育はサッカーの練習をする予定でしたが、あいにくの雨ですね。"},{"id":{"d":1051,"n":2,"t":2},"speaker":"B","text":"そうですね。梅雨の時期はなかなか外で体育の授業を受けられませんね。"},{"id":{"d":1051,"n":3,"t":2},"speaker":"A","text":"教室で座学をするのでしょうか？"},{"id":{"d":1051,"n":4,"t":2},"speaker":"B","text":"いえ、体育館でフットサルをするとのことです。"},{"id":{"d":1051,"n":5,"t":2},"speaker":"A","text":"そうなのですか。知りませんでした。先生から連絡がありましたか？"},{"id":{"d":1051,"n":6,"t":2},"speaker":"B","text":"はい。背面黒板に書かれていますよ。"},{"id":{"d":1051,"n":7,"t":2},"speaker":"A","text":"本当ですね。「室内シューズを忘れずに」とも書いてあります。"},{"id":{"d":1051,"n":8,"t":2},"speaker":"B","text":"気付いてない人も多そうなので、ここでアナウンスした方がいいですね。"},{"id":{"d":1051,"n":9,"t":2},"speaker":"A","text":"はい、そう思います。"}];
INSERT INTO sentence [{"id":{"d":1052,"n":1,"t":2},"speaker":"A","text":"大学は、高校と違って追試験制度がないので、良心的だと思います。"},{"id":{"d":1052,"n":2,"t":2},"speaker":"B","text":"本当にそうでしょうか。私は高校の方が良心的だと思いますよ。"},{"id":{"d":1052,"n":3,"t":2},"speaker":"A","text":"それはどうしてでしょうか？追試験がなければ、夏休みの開始が遅れることもありません。"},{"id":{"d":1052,"n":4,"t":2},"speaker":"B","text":"追試験がないということは、一発勝負だということです。もし一度でも落ちれば、単位がもらえません。"},{"id":{"d":1052,"n":5,"t":2},"speaker":"A","text":"確かに、言われてみればそうですね。"},{"id":{"d":1052,"n":6,"t":2},"speaker":"B","text":"そして、いくつも単位を落とすと、留年です。かなりシビアな制度だと思います。"},{"id":{"d":1052,"n":7,"t":2},"speaker":"A","text":"納得ができました。留年をしたくはないので、今から真面目にテスト勉強をしようと思います。"},{"id":{"d":1052,"n":8,"t":2},"speaker":"B","text":"そうしてください。一緒に進級しましょうね。"}];
INSERT INTO sentence [{"id":{"d":1053,"n":1,"t":2},"speaker":"A","text":"歴史の勉強に対して、どうしても苦手意識を持ってしまうのですが、解決策はないでしょうか？"},{"id":{"d":1053,"n":2,"t":2},"speaker":"B","text":"まずは大まかな歴史の流れを頭に入れることが大切です。土台がしっかりしていれば、たくさんの知識を積み上げられますよ。"},{"id":{"d":1053,"n":3,"t":2},"speaker":"A","text":"なるほど。具体的には、どのような勉強をすれば良いですか？"},{"id":{"d":1053,"n":4,"t":2},"speaker":"B","text":"年表を覚えましょう。一つの物語として年表を眺めてみてください。"},{"id":{"d":1053,"n":5,"t":2},"speaker":"A","text":"年数や人物名なども覚えるべきですか？"},{"id":{"d":1053,"n":6,"t":2},"speaker":"B","text":"いえ、そのような具体的なことは、流れをつかみ切った後に覚えていきましょう。"},{"id":{"d":1053,"n":7,"t":2},"speaker":"A","text":"分かりました。その方法で勉強してみます。また行き詰まったら相談させてください。"},{"id":{"d":1053,"n":8,"t":2},"speaker":"B","text":"頑張ってください。いつでも待っていますよ。"}];
INSERT INTO sentence [{"id":{"d":1054,"n":1,"t":2},"speaker":"A","text":"今年の文化祭の出し物の最優秀クラスはどこになると思いますか？"},{"id":{"d":1054,"n":2,"t":2},"speaker":"B","text":"3年C組のお化け屋敷が受賞すると思います。レベルがとても高かったです。"},{"id":{"d":1054,"n":3,"t":2},"speaker":"A","text":"確かに、C組のお化け屋敷は常に悲鳴が響いていましたからね。候補の一つだとは思います。"},{"id":{"d":1054,"n":4,"t":2},"speaker":"B","text":"あなたは、どこが優秀賞を受賞すると思いますか？"},{"id":{"d":1054,"n":5,"t":2},"speaker":"A","text":"私は2年A組のメイド喫茶が優秀賞を受賞すると思います。メイド服から全て手作りしたそうですよ。"},{"id":{"d":1054,"n":6,"t":2},"speaker":"B","text":"それは素晴らしいですね。一体準備に何時間かけたのでしょうか？"},{"id":{"d":1054,"n":7,"t":2},"speaker":"A","text":"新学期が始まった4月から、10月の文化祭でメイド喫茶を行うと決めて、地道に作成したそうです。"},{"id":{"d":1054,"n":8,"t":2},"speaker":"B","text":"それは、優秀賞の最有力候補ですね。彼らの努力が認められてほしいです。"}];
INSERT INTO sentence [{"id":{"d":1055,"n":1,"t":2},"speaker":"A","text":"これから、担任の先生の発表ですね。"},{"id":{"d":1055,"n":2,"t":2},"speaker":"B","text":"はい、とても緊張します。水木さんは、どの先生に私たちのクラスの担任を持ってもらいたいですか？"},{"id":{"d":1055,"n":3,"t":2},"speaker":"A","text":"山本先生です。山本先生は、日誌の提出を強制しないことで有名ですから。"},{"id":{"d":1055,"n":4,"t":2},"speaker":"B","text":"その代わり、教科書の暗唱を毎日行うことになりますよ。山本先生はクラスの生徒に、朝礼の際に教科書の一節を暗唱するよう求めます。"},{"id":{"d":1055,"n":5,"t":2},"speaker":"A","text":"それは少し大変そうですね。"},{"id":{"d":1055,"n":6,"t":2},"speaker":"B","text":"私は、伊藤先生が担任の先生をしてくださると良いと思います。いつも笑顔で優しい先生なので。"},{"id":{"d":1055,"n":7,"t":2},"speaker":"A","text":"伊藤先生も良い先生ですね。登校時、毎朝欠かさず校門であいさつしてくれるのは伊藤先生ですからね。"},{"id":{"d":1055,"n":8,"t":2},"speaker":"B","text":"そうですね。そろそろ発表が始まります。伊藤先生が担任になってくれますように。"}];
INSERT INTO sentence [{"id":{"d":1056,"n":1,"t":2},"speaker":"A","text":"漢字ドリルの宿題を、班ごとに回収します。"},{"id":{"d":1056,"n":2,"t":2},"speaker":"B","text":"漢字の宿題は、明日までではなかったですか？"},{"id":{"d":1056,"n":3,"t":2},"speaker":"A","text":"いえ、明日までなのは計算ドリルの方です。今日は漢字ドリルの提出日です。"},{"id":{"d":1056,"n":4,"t":2},"speaker":"B","text":"勘違いしていました。計算ドリルの方をしてきてしまいました。"},{"id":{"d":1056,"n":5,"t":2},"speaker":"A","text":"先生が来るまで、あと5分あります。今回の宿題の範囲は少なかったので、急げば先生が到着されるまでに終わると思いますよ。"},{"id":{"d":1056,"n":6,"t":2},"speaker":"B","text":"今から漢字ドリルの宿題を行います。今から急いで行うので、私の班の宿題の回収は後ろに回していただけますか？"},{"id":{"d":1056,"n":7,"t":2},"speaker":"A","text":"分かりました。可能な限り待ちますが、先生が教室に到着した時点で提出してしまいますよ。"},{"id":{"d":1056,"n":8,"t":2},"speaker":"B","text":"はい。どのようにかして間に合わせます。"}];
INSERT INTO sentence [{"id":{"d":1057,"n":1,"t":2},"speaker":"A","text":"今日の給食は何ですか？"},{"id":{"d":1057,"n":2,"t":2},"speaker":"B","text":"シチューと、鶏の照り焼きです。主食はパンです。"},{"id":{"d":1057,"n":3,"t":2},"speaker":"A","text":"良いですね。デザートは何か付いていますか？"},{"id":{"d":1057,"n":4,"t":2},"speaker":"B","text":"冷凍みかんがついています。冷凍みかんは月に1度しか出ないので、とてもうれしいです。"},{"id":{"d":1057,"n":5,"t":2},"speaker":"A","text":"今日は中島さんと木村君が欠席なので、2つ余るはずです。"},{"id":{"d":1057,"n":6,"t":2},"speaker":"B","text":"恒例の余りものじゃんけんが盛り上がりそうですね。"},{"id":{"d":1057,"n":7,"t":2},"speaker":"A","text":"そうですね。私も今日は参加しようと思います。"},{"id":{"d":1057,"n":8,"t":2},"speaker":"B","text":"ここのところずっと連敗が続いているので、今日こそは絶対に勝ちたいです。"},{"id":{"d":1057,"n":9,"t":2},"speaker":"A","text":"そうですね。私も冷凍みかんが大好きなので負けたくありません。"}];
