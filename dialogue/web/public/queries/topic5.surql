INSERT INTO sentence [{"id":{"d":168,"n":1,"t":5},"speaker":"A","text":"今までに、あなたは何か楽器を演奏したことはありますか？"},{"id":{"d":168,"n":2,"t":5},"speaker":"B","text":"私は、学生時代は吹奏楽部に入っていて、トロンボーンを吹いていました。"},{"id":{"d":168,"n":3,"t":5},"speaker":"A","text":"トランペットより少し大きい楽器ですね。でも吹奏楽部となると練習も厳しかったのではありませんか？"},{"id":{"d":168,"n":4,"t":5},"speaker":"B","text":"そうですね。コンクール前は厳しい練習もかなりありました。"},{"id":{"d":168,"n":5,"t":5},"speaker":"A","text":"コンクールの結果はどうだったのですか？"},{"id":{"d":168,"n":6,"t":5},"speaker":"B","text":"一度、県大会で銀賞をもらったことがあります。"},{"id":{"d":168,"n":7,"t":5},"speaker":"A","text":"素晴しいですね。もし機会があれば、今度演奏を聴かせてください。"},{"id":{"d":168,"n":8,"t":5},"speaker":"B","text":"はい、よいですよ。"}];
INSERT INTO sentence [{"id":{"d":169,"n":1,"t":5},"speaker":"A","text":"今日はわが家で、テレビゲームをしませんか？"},{"id":{"d":169,"n":2,"t":5},"speaker":"B","text":"良いですね。何のテレビゲームですか？"},{"id":{"d":169,"n":3,"t":5},"speaker":"A","text":"PlayStation 5を買ったので、一緒に遊びましょう。"},{"id":{"d":169,"n":4,"t":5},"speaker":"B","text":"PlayStation 5は入手困難といわれていますが、よく買うことができましたね。"},{"id":{"d":169,"n":5,"t":5},"speaker":"A","text":"はい、近くの電気屋さんに行くと、偶然1台残っていたので、すぐに買いました。"},{"id":{"d":169,"n":6,"t":5},"speaker":"B","text":"高かったのではありませんか？"},{"id":{"d":169,"n":7,"t":5},"speaker":"A","text":"5万円くらいでした。確かに、価格は高かったのですが、ずっと欲しかったので、良い買い物をしたと思っています。"},{"id":{"d":169,"n":8,"t":5},"speaker":"B","text":"ぜひ、一緒に遊びたいです。"},{"id":{"d":169,"n":9,"t":5},"speaker":"A","text":"もちろんです。一緒にゲームをしましょう。"}];
INSERT INTO sentence [{"id":{"d":170,"n":1,"t":5},"speaker":"A","text":"このアルバムの写真は小学生の時の写真です。"},{"id":{"d":170,"n":2,"t":5},"speaker":"B","text":"小学何年生のころの写真ですか？"},{"id":{"d":170,"n":3,"t":5},"speaker":"A","text":"5年生か6年生くらいだと思います。"},{"id":{"d":170,"n":4,"t":5},"speaker":"B","text":"顔つきは、今と全く変わりませんね。"},{"id":{"d":170,"n":5,"t":5},"speaker":"A","text":"そうでしょうか？それでは、この集合写真の中で私はどこに居るか分かりますか？"},{"id":{"d":170,"n":6,"t":5},"speaker":"B","text":"この方ですね。すぐに分かりました。"},{"id":{"d":170,"n":7,"t":5},"speaker":"A","text":"正解です。何か「小学校からあまり顔つきが変わっていない」と言われると、複雑な気持ちです。"},{"id":{"d":170,"n":8,"t":5},"speaker":"B","text":"私は、小学生時代から印象が変わらないことは、良いことだと思いますよ。"},{"id":{"d":170,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。"}];
