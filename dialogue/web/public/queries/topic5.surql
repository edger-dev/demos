INSERT INTO sentence [{"id":{"d":441,"n":1,"t":5},"speaker":"A","text":"これを見てください。"},{"id":{"d":441,"n":2,"t":5},"speaker":"B","text":"カセットテープですか？"},{"id":{"d":441,"n":3,"t":5},"speaker":"A","text":"わが家の父の秘蔵の品です。"},{"id":{"d":441,"n":4,"t":5},"speaker":"B","text":"カセットテープを聴く機械はあるのですか？"},{"id":{"d":441,"n":5,"t":5},"speaker":"A","text":"ラジカセはもう壊れてしまって、聴けないのです。"},{"id":{"d":441,"n":6,"t":5},"speaker":"B","text":"でも、カセットテープを大事に取ってあるのですね。"},{"id":{"d":441,"n":7,"t":5},"speaker":"A","text":"はい。カセットテープに入っている曲をYouTubeで聴いてみたら、とてもすてきでした。"},{"id":{"d":441,"n":8,"t":5},"speaker":"B","text":"お父さまの青春の1ページですね。"}];
INSERT INTO sentence [{"id":{"d":442,"n":1,"t":5},"speaker":"A","text":"花ちゃんと、雪ちゃんも、交えてトランプでババ抜きをしませんか？"},{"id":{"d":442,"n":2,"t":5},"speaker":"B","text":"良いですね。"},{"id":{"d":442,"n":3,"t":5},"speaker":"A","text":"少し変わったトランプを手に入れたのです。"},{"id":{"d":442,"n":4,"t":5},"speaker":"B","text":"そのトランプは、随分と大きいですね。"},{"id":{"d":442,"n":5,"t":5},"speaker":"A","text":"A4サイズなのですよ。"},{"id":{"d":442,"n":6,"t":5},"speaker":"B","text":"持ちにくそうですけれど、それでババ抜きをするのですか？"},{"id":{"d":442,"n":7,"t":5},"speaker":"A","text":"はい、楽しくなりそうでしょう？"},{"id":{"d":442,"n":8,"t":5},"speaker":"B","text":"はい。しかし、花ちゃんたちの手で、持てるでしょうか？"}];
INSERT INTO sentence [{"id":{"d":443,"n":1,"t":5},"speaker":"A","text":"この曲は、どこかで聴いたことがあるような気がします。"},{"id":{"d":443,"n":2,"t":5},"speaker":"B","text":"この曲は『君はロックを聴かない』ですよ。"},{"id":{"d":443,"n":3,"t":5},"speaker":"A","text":"あいみょんの曲ですか？"},{"id":{"d":443,"n":4,"t":5},"speaker":"B","text":"そうです。オルゴール曲になっているので、あいみょんの歌う原曲とは雰囲気が違いますね。"},{"id":{"d":443,"n":5,"t":5},"speaker":"A","text":"本当ですね。よく聴いたら、あいみょんの曲でした。"},{"id":{"d":443,"n":6,"t":5},"speaker":"B","text":"オルゴール曲になると、聴きながらそのまま眠りそうですね。"},{"id":{"d":443,"n":7,"t":5},"speaker":"A","text":"いつもなら、あいみょんの歌では興奮して眠れないのに、驚きました。"},{"id":{"d":443,"n":8,"t":5},"speaker":"B","text":"今夜、このオルゴール曲を聴きながら、眠ってみてはどうでしょう？"}];
INSERT INTO sentence [{"id":{"d":444,"n":1,"t":5},"speaker":"A","text":"昨日の盆踊りは、楽しかったですか？"},{"id":{"d":444,"n":2,"t":5},"speaker":"B","text":"はい。村中の人が集まってくるのですね。"},{"id":{"d":444,"n":3,"t":5},"speaker":"A","text":"腰の曲がったおばあさんが踊りの輪に入ると、腰が伸びていたでしょう？"},{"id":{"d":444,"n":4,"t":5},"speaker":"B","text":"はい。盆踊りが元気のもとなのですね。"},{"id":{"d":444,"n":5,"t":5},"speaker":"A","text":"また、来年も良かったら遊びに来てください。"},{"id":{"d":444,"n":6,"t":5},"speaker":"B","text":"また、来ても良いのですか？"},{"id":{"d":444,"n":7,"t":5},"speaker":"A","text":"はい。楽しんでくれる人は大歓迎です。"},{"id":{"d":444,"n":8,"t":5},"speaker":"B","text":"絶対にまた来年も、伺います。"}];
INSERT INTO sentence [{"id":{"d":445,"n":1,"t":5},"speaker":"A","text":"ゲームをしませんか？"},{"id":{"d":445,"n":2,"t":5},"speaker":"B","text":"それは、ジェンガというゲームですね？"},{"id":{"d":445,"n":3,"t":5},"speaker":"A","text":"そうです、知っていますか？"},{"id":{"d":445,"n":4,"t":5},"speaker":"B","text":"積み上げたブロックを交代に1本ずつ抜いて、崩した人が負けのゲームですよね。"},{"id":{"d":445,"n":5,"t":5},"speaker":"A","text":"正解です。"},{"id":{"d":445,"n":6,"t":5},"speaker":"B","text":"私はそのゲームを知っていますが、不器用なので積み上げるのが苦手です。"},{"id":{"d":445,"n":7,"t":5},"speaker":"A","text":"心配は要りません、私も不器用ですから。"},{"id":{"d":445,"n":8,"t":5},"speaker":"B","text":"良い勝負になりそうですね。"}];
INSERT INTO sentence [{"id":{"d":446,"n":1,"t":5},"speaker":"A","text":"もうすぐ、お正月ですね。"},{"id":{"d":446,"n":2,"t":5},"speaker":"B","text":"お年玉がいくらもらえるか、楽しみです。"},{"id":{"d":446,"n":3,"t":5},"speaker":"A","text":"毎年たくさんもらえるのですか？"},{"id":{"d":446,"n":4,"t":5},"speaker":"B","text":"親戚が集まるのでたくさんもらえます。"},{"id":{"d":446,"n":5,"t":5},"speaker":"A","text":"良いですね。私は、お父さんにもらう分だけです。"},{"id":{"d":446,"n":6,"t":5},"speaker":"B","text":"あなたのお父さんは、いくらくらいくれるのですか？"},{"id":{"d":446,"n":7,"t":5},"speaker":"A","text":"年齢かける1,000円だから、今年は1万2,000円だと思います。"},{"id":{"d":446,"n":8,"t":5},"speaker":"B","text":"それは、親戚中からお年玉をもらう私より、多いかもしれません。"}];
INSERT INTO sentence [{"id":{"d":447,"n":1,"t":5},"speaker":"A","text":"お年玉をもらったら、買い物に行きませんか？"},{"id":{"d":447,"n":2,"t":5},"speaker":"B","text":"はい、行きましょう。"},{"id":{"d":447,"n":3,"t":5},"speaker":"A","text":"どこに買い物に行きますか？"},{"id":{"d":447,"n":4,"t":5},"speaker":"B","text":"ペンケースが買える場所に行きましょう。私はキティちゃんのペンケースが欲しいのです。"},{"id":{"d":447,"n":5,"t":5},"speaker":"A","text":"私も欲しいと思っていました。"},{"id":{"d":447,"n":6,"t":5},"speaker":"B","text":"色違いのおそろいにしましょうか？"},{"id":{"d":447,"n":7,"t":5},"speaker":"A","text":"はい。私は、水色のペンケースがいいです。"},{"id":{"d":447,"n":8,"t":5},"speaker":"B","text":"では、私はピンクのペンケースにします。"}];
INSERT INTO sentence [{"id":{"d":448,"n":1,"t":5},"speaker":"A","text":"初売りセールは、疲れますね。"},{"id":{"d":448,"n":2,"t":5},"speaker":"B","text":"はい。すさまじい人ごみでした。"},{"id":{"d":448,"n":3,"t":5},"speaker":"A","text":"歩くだけでも大変でしたね。それでも、私は、つい福袋を買ってしまいました。"},{"id":{"d":448,"n":4,"t":5},"speaker":"B","text":"私もそうです。結局、2人とも、両手に福袋を2つずつ持っていますね。"},{"id":{"d":448,"n":5,"t":5},"speaker":"A","text":"わが家に一緒に帰って、中身を広げてみましょうか？"},{"id":{"d":448,"n":6,"t":5},"speaker":"B","text":"去年も、不用品の物々交換をしましたね。"},{"id":{"d":448,"n":7,"t":5},"speaker":"A","text":"あの時交換したマフラーは、今年の愛用品です。"},{"id":{"d":448,"n":8,"t":5},"speaker":"B","text":"私も、帽子を愛用しています。"}];
INSERT INTO sentence [{"id":{"d":449,"n":1,"t":5},"speaker":"A","text":"その棚に置いてあるぬいぐるみは、随分と古いもののようですね。"},{"id":{"d":449,"n":2,"t":5},"speaker":"B","text":"このお店が、開店した時からあるものですよ。"},{"id":{"d":449,"n":3,"t":5},"speaker":"A","text":"お店は、いつ開店したのですか？"},{"id":{"d":449,"n":4,"t":5},"speaker":"B","text":"1960年です。"},{"id":{"d":449,"n":5,"t":5},"speaker":"A","text":"もう60年以上前ですか？"},{"id":{"d":449,"n":6,"t":5},"speaker":"B","text":"あのぬいぐるみは、当時のオーナーの、子どもの頃からの宝物だったそうですよ。"},{"id":{"d":449,"n":7,"t":5},"speaker":"A","text":"お店を引き継いだ方たちにも、大切にされてきたのですね。"},{"id":{"d":449,"n":8,"t":5},"speaker":"B","text":"今では、お店の守り神です。"}];
INSERT INTO sentence [{"id":{"d":450,"n":1,"t":5},"speaker":"A","text":"漫画を、大量買い、いわゆる大人買いをしてしまいました。"},{"id":{"d":450,"n":2,"t":5},"speaker":"B","text":"大人買い、というのはまとめて買うことですよね？"},{"id":{"d":450,"n":3,"t":5},"speaker":"A","text":"はい、『鬼滅の刃』がついに完結したので、1巻から全部買いました。"},{"id":{"d":450,"n":4,"t":5},"speaker":"B","text":"全部でいくらになったのですか？"},{"id":{"d":450,"n":5,"t":5},"speaker":"A","text":"全23巻と外伝で12,144円でした。"},{"id":{"d":450,"n":6,"t":5},"speaker":"B","text":"一度に買うと結構なお値段になるのですね。"},{"id":{"d":450,"n":7,"t":5},"speaker":"A","text":"でも、漫画本は、作品によっては100巻超えもありますから、この価格は安いものです。"},{"id":{"d":450,"n":8,"t":5},"speaker":"B","text":"読み終わったら、貸してください。"}];
INSERT INTO sentence [{"id":{"d":451,"n":1,"t":5},"speaker":"A","text":"『竜とそばかすの姫』を見てきました。"},{"id":{"d":451,"n":2,"t":5},"speaker":"B","text":"話題のアニメーション映画ですね。CMで印象的な歌が流れているのを聴きました。"},{"id":{"d":451,"n":3,"t":5},"speaker":"A","text":"私も、CMで歌を聴いて見に行ったのです。アニメーションだからと侮れない内容でしたよ。"},{"id":{"d":451,"n":4,"t":5},"speaker":"B","text":"インターネットの仮想世界の話ですよね？"},{"id":{"d":451,"n":5,"t":5},"speaker":"A","text":"はい。でも、インターネットだけではなく、仮想世界と現実の問題が交錯した物語として描かれていて驚きました。"},{"id":{"d":451,"n":6,"t":5},"speaker":"B","text":"深い物語なのですね。興味がわいていきました。"}];
INSERT INTO sentence [{"id":{"d":452,"n":1,"t":5},"speaker":"A","text":"私と一緒に、文楽を見に行きませんか？"},{"id":{"d":452,"n":2,"t":5},"speaker":"B","text":"文楽とは、何ですか？"},{"id":{"d":452,"n":3,"t":5},"speaker":"A","text":"文楽とは、人形浄瑠璃のことですよ。"},{"id":{"d":452,"n":4,"t":5},"speaker":"B","text":"歌舞伎とは違うのですか？"},{"id":{"d":452,"n":5,"t":5},"speaker":"A","text":"違います。歌舞伎は人間が演じて、文楽は人形が演じるものです。"},{"id":{"d":452,"n":6,"t":5},"speaker":"B","text":"人形劇ということですか？"},{"id":{"d":452,"n":7,"t":5},"speaker":"A","text":"そうですけど、子ども向けの人形劇とは違います。"},{"id":{"d":452,"n":8,"t":5},"speaker":"B","text":"少し内容が難しそうです。"}];
INSERT INTO sentence [{"id":{"d":453,"n":1,"t":5},"speaker":"A","text":"無理を言って、文楽を見に連れてきましたけど、どうでしたか？"},{"id":{"d":453,"n":2,"t":5},"speaker":"B","text":"大興奮しています！"},{"id":{"d":453,"n":3,"t":5},"speaker":"A","text":"楽しんでくれたようで、良かったです。"},{"id":{"d":453,"n":4,"t":5},"speaker":"B","text":"古典は苦手だったのですけど、内容は面白い演劇でしたね。"},{"id":{"d":453,"n":5,"t":5},"speaker":"A","text":"難しくて理解できないということはなかったでしょう？"},{"id":{"d":453,"n":6,"t":5},"speaker":"B","text":"はい。字幕が出ていましたし、イヤホンガイドも着けていたのが良かったみたいです。"},{"id":{"d":453,"n":7,"t":5},"speaker":"A","text":"また、文楽鑑賞に付き合ってくれますか？"},{"id":{"d":453,"n":8,"t":5},"speaker":"B","text":"はい。こちらからお願いしたいくらいです。"}];
