INSERT INTO sentence [{"id":{"d":553,"n":1,"t":5},"speaker":"A","text":"お母さん、なぞなぞを出すので、答えを考えてみてください。"},{"id":{"d":553,"n":2,"t":5},"speaker":"B","text":"いいですよ。問題は自分で考えたのですか？"},{"id":{"d":553,"n":3,"t":5},"speaker":"A","text":"幼稚園で先生が出してくれたなぞなぞです。"},{"id":{"d":553,"n":4,"t":5},"speaker":"B","text":"分かりました。問題をどうぞ。"},{"id":{"d":553,"n":5,"t":5},"speaker":"A","text":"パンはパンでも、食べられないパンは何でしょう？"},{"id":{"d":553,"n":6,"t":5},"speaker":"B","text":"答えは、フライパンですね。"},{"id":{"d":553,"n":7,"t":5},"speaker":"A","text":"当たりです。どうしてすぐに分かったのですか？"},{"id":{"d":553,"n":8,"t":5},"speaker":"B","text":"有名ななぞなぞなので、問題と答えを知っていました。"},{"id":{"d":553,"n":9,"t":5},"speaker":"A","text":"そうだったのですね。"}];
INSERT INTO sentence [{"id":{"d":554,"n":1,"t":5},"speaker":"A","text":"ビンゴゲームが始まるようですよ。カードが回ってきました。"},{"id":{"d":554,"n":2,"t":5},"speaker":"B","text":"私にも1枚ください。"},{"id":{"d":554,"n":3,"t":5},"speaker":"A","text":"どうぞ。当たるといいですね。"},{"id":{"d":554,"n":4,"t":5},"speaker":"B","text":"最初の番号が発表されました。17ですね。"},{"id":{"d":554,"n":5,"t":5},"speaker":"A","text":"私のカードには17がありません。"},{"id":{"d":554,"n":6,"t":5},"speaker":"B","text":"私はありました。次の番号はあるといいですね。"},{"id":{"d":554,"n":7,"t":5},"speaker":"A","text":"次は32ですね。今度はありました。"},{"id":{"d":554,"n":8,"t":5},"speaker":"B","text":"私もありました。"},{"id":{"d":554,"n":9,"t":5},"speaker":"A","text":"連続でマスが開くとは、あなたは運がいいですね。"}];
INSERT INTO sentence [{"id":{"d":555,"n":1,"t":5},"speaker":"A","text":"おばあちゃん、これをどうぞ。"},{"id":{"d":555,"n":2,"t":5},"speaker":"B","text":"これを私にくれるのですか？"},{"id":{"d":555,"n":3,"t":5},"speaker":"A","text":"はい。今日は敬老の日なので、日々の感謝の気持ちを込めたプレゼントです。"},{"id":{"d":555,"n":4,"t":5},"speaker":"B","text":"どうもありがとう。とてもうれしいです。"},{"id":{"d":555,"n":5,"t":5},"speaker":"A","text":"開けてみてください。"},{"id":{"d":555,"n":6,"t":5},"speaker":"B","text":"まあ、すてきなハンカチですね。"},{"id":{"d":555,"n":7,"t":5},"speaker":"A","text":"気に入ってもらえましたか？"},{"id":{"d":555,"n":8,"t":5},"speaker":"B","text":"はい。私の好みにぴったりです。"}];
INSERT INTO sentence [{"id":{"d":556,"n":1,"t":5},"speaker":"A","text":"そろそろ、寝る時間ですよ。お布団に入りましょう。"},{"id":{"d":556,"n":2,"t":5},"speaker":"B","text":"お布団に入ったら、絵本を読んでください。"},{"id":{"d":556,"n":3,"t":5},"speaker":"A","text":"よいですよ。どの本がいいですか？"},{"id":{"d":556,"n":4,"t":5},"speaker":"B","text":"『シンデレラ』がいいです。"},{"id":{"d":556,"n":5,"t":5},"speaker":"A","text":"真奈は『シンデレラ』が好きですね。"},{"id":{"d":556,"n":6,"t":5},"speaker":"B","text":"はい。何度読んでもらっても飽きません。"},{"id":{"d":556,"n":7,"t":5},"speaker":"A","text":"それでは、『シンデレラ』の本を本棚から取ってきてください。"},{"id":{"d":556,"n":8,"t":5},"speaker":"B","text":"はい。取りに行ってきます。"}];
INSERT INTO sentence [{"id":{"d":557,"n":1,"t":5},"speaker":"A","text":"今日の午後は、時間がありますか？"},{"id":{"d":557,"n":2,"t":5},"speaker":"B","text":"特に予定はありませんが、どうしてですか？"},{"id":{"d":557,"n":3,"t":5},"speaker":"A","text":"ジグソーパズルを手伝ってほしいのです。"},{"id":{"d":557,"n":4,"t":5},"speaker":"B","text":"そういえば、先週、ジグソーパズルを買ったと話していましたね。"},{"id":{"d":557,"n":5,"t":5},"speaker":"A","text":"はい。買ってすぐに組み立て始めたのですが、いまだにほとんど進まないのです。"},{"id":{"d":557,"n":6,"t":5},"speaker":"B","text":"それほど難しいのですか？"},{"id":{"d":557,"n":7,"t":5},"speaker":"A","text":"はい。ピースがとても細かくて、私1人ではほとんどお手上げの状態です。"},{"id":{"d":557,"n":8,"t":5},"speaker":"B","text":"それでは今日は2人がかりで取り組みましょう。"},{"id":{"d":557,"n":9,"t":5},"speaker":"A","text":"助かります。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":558,"n":1,"t":5},"speaker":"A","text":"このプロジェクトが終わったら、打ち上げをしたいです。"},{"id":{"d":558,"n":2,"t":5},"speaker":"B","text":"私もしたいです。食事会と飲み会とカラオケとでは、どれがいいでしょうか？"},{"id":{"d":558,"n":3,"t":5},"speaker":"A","text":"飲み会はどうでしょうか？"},{"id":{"d":558,"n":4,"t":5},"speaker":"B","text":"いいですね。開催日は、プロジェクトの最終日の夜にしましょう。"},{"id":{"d":558,"n":5,"t":5},"speaker":"A","text":"はい。あとはお店を決める必要がありますね。どこか良いお店を知っていますか？"},{"id":{"d":558,"n":6,"t":5},"speaker":"B","text":"去年、友達に教えてもらったお店がとても良い雰囲気でした。"},{"id":{"d":558,"n":7,"t":5},"speaker":"A","text":"では、そのお店にしましょう。予約をお願いしてもよいですか？"},{"id":{"d":558,"n":8,"t":5},"speaker":"B","text":"はい。連絡しておきます。"}];
INSERT INTO sentence [{"id":{"d":559,"n":1,"t":5},"speaker":"A","text":"外は強風が吹いていますね。"},{"id":{"d":559,"n":2,"t":5},"speaker":"B","text":"はい。先ほど外出したら、風が強くて真っすぐ歩くのが難しいくらいでした。"},{"id":{"d":559,"n":3,"t":5},"speaker":"A","text":"そうですか。私はコンタクトレンズをしているので、目にごみが入りそうで怖いです。"},{"id":{"d":559,"n":4,"t":5},"speaker":"B","text":"もし外に出るなら、眼鏡にした方がいいですよ。"},{"id":{"d":559,"n":5,"t":5},"speaker":"A","text":"そうします。この強風は、春一番かもしれませんね。"},{"id":{"d":559,"n":6,"t":5},"speaker":"B","text":"そういえば、もう春一番が吹く時期ですね。"},{"id":{"d":559,"n":7,"t":5},"speaker":"A","text":"これからは、徐々に春らしくなっていくのでしょうね。"},{"id":{"d":559,"n":8,"t":5},"speaker":"B","text":"春は大好きな季節なので楽しみです。"}];
INSERT INTO sentence [{"id":{"d":560,"n":1,"t":5},"speaker":"A","text":"去年の大晦日に、除夜の鐘を聞きましたか？"},{"id":{"d":560,"n":2,"t":5},"speaker":"B","text":"大晦日は年越しまで起きていましたが、聞いていません。"},{"id":{"d":560,"n":3,"t":5},"speaker":"A","text":"ここ数年は、聞いていないと思いませんか？"},{"id":{"d":560,"n":4,"t":5},"speaker":"B","text":"言われてみればそうですね。そういえば、除夜の鐘を中止する寺が増えていると聞いたことがあります。"},{"id":{"d":560,"n":5,"t":5},"speaker":"A","text":"どうして中止するのでしょうか？"},{"id":{"d":560,"n":6,"t":5},"speaker":"B","text":"寺の近所の住民から、騒音として苦情が寄せられることがあるそうです。"},{"id":{"d":560,"n":7,"t":5},"speaker":"A","text":"昔からの伝統が騒音と言われてしまうとは、少し寂しい気もしますね。"},{"id":{"d":560,"n":8,"t":5},"speaker":"B","text":"時代とともに、人の意識にも変化があるのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":561,"n":1,"t":5},"speaker":"A","text":"私は趣味でレコードを集めています。"},{"id":{"d":561,"n":2,"t":5},"speaker":"B","text":"すてきなご趣味ですね。何枚くらい持っているのですか？"},{"id":{"d":561,"n":3,"t":5},"speaker":"A","text":"200枚以上あります。"},{"id":{"d":561,"n":4,"t":5},"speaker":"B","text":"それほどたくさんあると、保管するのも大変そうです。どのように管理しているのですか？"},{"id":{"d":561,"n":5,"t":5},"speaker":"A","text":"自宅の一部屋をレコード部屋にしています。"},{"id":{"d":561,"n":6,"t":5},"speaker":"B","text":"本格的ですね。レコード好きの人の中でも、そこまでしている人は珍しいのではありませんか？"},{"id":{"d":561,"n":7,"t":5},"speaker":"A","text":"友人には私の倍以上のレコードを持っている人もいますよ。彼もレコード部屋で管理していました。"},{"id":{"d":561,"n":8,"t":5},"speaker":"B","text":"上には上がいるものですね。"}];
INSERT INTO sentence [{"id":{"d":562,"n":1,"t":5},"speaker":"A","text":"日本と米国では、クリスマスの過ごし方に違いがあるのでしょうか？"},{"id":{"d":562,"n":2,"t":5},"speaker":"B","text":"いくつか違うところもあると思います。例えば、米国では、クリスマスイブのディナーにハムやターキーを食べますが、日本はどうですか？"},{"id":{"d":562,"n":3,"t":5},"speaker":"A","text":"日本では、チキンを食べる家庭が多いです。確かに、違いがありますね。"},{"id":{"d":562,"n":4,"t":5},"speaker":"B","text":"デザートは何を食べますか？"},{"id":{"d":562,"n":5,"t":5},"speaker":"A","text":"日本では、クリスマスケーキを食べます。"},{"id":{"d":562,"n":6,"t":5},"speaker":"B","text":"米国ではケーキではなく、パイやアイスクリームを食べますね。"},{"id":{"d":562,"n":7,"t":5},"speaker":"A","text":"随分と違うものですね。"},{"id":{"d":562,"n":8,"t":5},"speaker":"B","text":"クリスマス以外のイベントでも、米国と日本では違いがいろいろとありそうですね。"}];
INSERT INTO sentence [{"id":{"d":563,"n":1,"t":5},"speaker":"A","text":"子どもの頃は、どのような漫画を読みましたか？"},{"id":{"d":563,"n":2,"t":5},"speaker":"B","text":"『ドラえもん』や『ドラゴンボール』を夢中になって読みました。"},{"id":{"d":563,"n":3,"t":5},"speaker":"A","text":"私も、両方とも読みました。"},{"id":{"d":563,"n":4,"t":5},"speaker":"B","text":"どちらも大人気の漫画で、アニメーションにもなっていましたね。"},{"id":{"d":563,"n":5,"t":5},"speaker":"A","text":"そうですね。『ドラえもん』のアニメーションは今でもテレビで放送されています。"},{"id":{"d":563,"n":6,"t":5},"speaker":"B","text":"かなりの長寿番組ですよね。"},{"id":{"d":563,"n":7,"t":5},"speaker":"A","text":"そうですね。私が子どもの頃に見ていた『ドラえもん』のアニメーションを、今は私の娘も見ています。"},{"id":{"d":563,"n":8,"t":5},"speaker":"B","text":"親子で同じアニメーションを見ることになるとは、感慨深いですね。"}];
INSERT INTO sentence [{"id":{"d":564,"n":1,"t":5},"speaker":"A","text":"家の本棚がいっぱいになってしまって、新しく買った本を置くスペースがなくなってしまいました。"},{"id":{"d":564,"n":2,"t":5},"speaker":"B","text":"うちには本棚がないので、そのようなことに悩んだことがありません。"},{"id":{"d":564,"n":3,"t":5},"speaker":"A","text":"あなたは本を買わないのですか？"},{"id":{"d":564,"n":4,"t":5},"speaker":"B","text":"いいえ、電子書籍で読んでいます。"},{"id":{"d":564,"n":5,"t":5},"speaker":"A","text":"最近はそういう人が増えているようですね。"},{"id":{"d":564,"n":6,"t":5},"speaker":"B","text":"保管する場所が要らないし、持ち運びも便利なのでお勧めですよ。"},{"id":{"d":564,"n":7,"t":5},"speaker":"A","text":"便利なのは分かるのですが、昔から紙の本を読んでいるので、電子書籍にはどうしても抵抗があるのです。"},{"id":{"d":564,"n":8,"t":5},"speaker":"B","text":"私も最初はそう思っていましたが、すぐに慣れましたよ。"}];
INSERT INTO sentence [{"id":{"d":565,"n":1,"t":5},"speaker":"A","text":"おじいちゃん、将棋の指し方を教えてください。"},{"id":{"d":565,"n":2,"t":5},"speaker":"B","text":"よいですよ。では、将棋盤と駒を持ってきてください。"},{"id":{"d":565,"n":3,"t":5},"speaker":"A","text":"もう持ってきています。"},{"id":{"d":565,"n":4,"t":5},"speaker":"B","text":"それでは始めましょう。まずは駒の名前を教えますね。"},{"id":{"d":565,"n":5,"t":5},"speaker":"A","text":"たくさん種類がありますね。"},{"id":{"d":565,"n":6,"t":5},"speaker":"B","text":"この駒は知っていますか？"},{"id":{"d":565,"n":7,"t":5},"speaker":"A","text":"王将ですね。一番有名なので知っています。"},{"id":{"d":565,"n":8,"t":5},"speaker":"B","text":"そうですか。では飛車はどれだか分かりますか？"},{"id":{"d":565,"n":9,"t":5},"speaker":"A","text":"これだと思います。"}];
INSERT INTO sentence [{"id":{"d":566,"n":1,"t":5},"speaker":"A","text":"あなたのストレス解消法は何ですか？"},{"id":{"d":566,"n":2,"t":5},"speaker":"B","text":"バッティングセンターに行くことです。ボールを力いっぱい打つと、すっきりします。"},{"id":{"d":566,"n":3,"t":5},"speaker":"A","text":"野球の経験があるのですか？"},{"id":{"d":566,"n":4,"t":5},"speaker":"B","text":"ありませんが、案外打てるものですよ。"},{"id":{"d":566,"n":5,"t":5},"speaker":"A","text":"私には難しそうです。ボールが速くて、空振りしてしまいそうです。"},{"id":{"d":566,"n":6,"t":5},"speaker":"B","text":"球速を選べるので、自分に合った速さにすれば大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":567,"n":1,"t":5},"speaker":"A","text":"明日香ちゃん、一緒におままごとをしませんか？"},{"id":{"d":567,"n":2,"t":5},"speaker":"B","text":"よいですよ。私はお母さん役がやりたいです。"},{"id":{"d":567,"n":3,"t":5},"speaker":"A","text":"それでは、私は子どもの役をしますね。"},{"id":{"d":567,"n":4,"t":5},"speaker":"B","text":"ここがリビングということにしましょう。"},{"id":{"d":567,"n":5,"t":5},"speaker":"A","text":"では、その奥をキッチンということにしましょう。"},{"id":{"d":567,"n":6,"t":5},"speaker":"B","text":"お母さんはキッチンで料理をしていることにしたいと思います。"},{"id":{"d":567,"n":7,"t":5},"speaker":"A","text":"いいですね。子どもはどうしましょうか？"},{"id":{"d":567,"n":8,"t":5},"speaker":"B","text":"リビングでテレビを見ていてはどうですか？"},{"id":{"d":567,"n":9,"t":5},"speaker":"A","text":"分かりました。おままごとを始める準備ができました。"},{"id":{"d":567,"n":10,"t":5},"speaker":"B","text":"では、始めましょう。"}];
INSERT INTO sentence [{"id":{"d":568,"n":1,"t":5},"speaker":"A","text":"お母さん、お絵描きをしたので見てください。"},{"id":{"d":568,"n":2,"t":5},"speaker":"B","text":"何を描いたのですか？"},{"id":{"d":568,"n":3,"t":5},"speaker":"A","text":"お父さんとお母さんと私です。"},{"id":{"d":568,"n":4,"t":5},"speaker":"B","text":"これがお父さんですか？"},{"id":{"d":568,"n":5,"t":5},"speaker":"A","text":"はい。眼鏡が難しかったです。"},{"id":{"d":568,"n":6,"t":5},"speaker":"B","text":"お父さんの隣のこの人が、お母さんですね。"},{"id":{"d":568,"n":7,"t":5},"speaker":"A","text":"そうです。料理をしているところを描きました。"},{"id":{"d":568,"n":8,"t":5},"speaker":"B","text":"かわいらしく描いてくれてうれしいです。"}];
INSERT INTO sentence [{"id":{"d":569,"n":1,"t":5},"speaker":"A","text":"この雑誌に星座占いが載っています。"},{"id":{"d":569,"n":2,"t":5},"speaker":"B","text":"私はかに座です。運勢はどうでしょうか？"},{"id":{"d":569,"n":3,"t":5},"speaker":"A","text":"今週は金運が良いと書いてあります。"},{"id":{"d":569,"n":4,"t":5},"speaker":"B","text":"それはうれしいです。"},{"id":{"d":569,"n":5,"t":5},"speaker":"A","text":"その代わりに、健康運に要注意とあります。"},{"id":{"d":569,"n":6,"t":5},"speaker":"B","text":"気を付けて過ごします。あなたは何座ですか？"},{"id":{"d":569,"n":7,"t":5},"speaker":"A","text":"みずがめ座です。今週は恋愛運が絶好調だと書いてあります。"},{"id":{"d":569,"n":8,"t":5},"speaker":"B","text":"何か良いことがあるといいですね。"}];
INSERT INTO sentence [{"id":{"d":570,"n":1,"t":5},"speaker":"A","text":"随分昔のドラマを見ていますね。"},{"id":{"d":570,"n":2,"t":5},"speaker":"B","text":"今、再放送をしているのです。"},{"id":{"d":570,"n":3,"t":5},"speaker":"A","text":"懐かしいですね。私は当時放送されていたものを全話見ました。"},{"id":{"d":570,"n":4,"t":5},"speaker":"B","text":"ドラマの中で殺人事件が起きますが、犯人を覚えていますか？"},{"id":{"d":570,"n":5,"t":5},"speaker":"A","text":"はっきりと覚えています。"},{"id":{"d":570,"n":6,"t":5},"speaker":"B","text":"自分で予想しながら見たいので、誰が犯人かを言わないでくださいね。"},{"id":{"d":570,"n":7,"t":5},"speaker":"A","text":"分かりました。とても意外な犯人なのできっと驚くと思いますよ。"},{"id":{"d":570,"n":8,"t":5},"speaker":"B","text":"続きを見るのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":571,"n":1,"t":5},"speaker":"A","text":"すみません。キャンプで使うテントはどこにありますか？"},{"id":{"d":571,"n":2,"t":5},"speaker":"B","text":"キャンプ用はこちらにあります。何人用のテントがよろしいですか？"},{"id":{"d":571,"n":3,"t":5},"speaker":"A","text":"2人で寝られるくらいの大きさがいいです。"},{"id":{"d":571,"n":4,"t":5},"speaker":"B","text":"それでしたら、これくらいの大きさがちょうどいいと思います。"},{"id":{"d":571,"n":5,"t":5},"speaker":"A","text":"組み立てるのは難しいですか？"},{"id":{"d":571,"n":6,"t":5},"speaker":"B","text":"とても簡単です。5分くらいで組み立てられます。"},{"id":{"d":571,"n":7,"t":5},"speaker":"A","text":"いいですね。色はこの色だけですか？"},{"id":{"d":571,"n":8,"t":5},"speaker":"B","text":"他に、青と黒があります。"}];
INSERT INTO sentence [{"id":{"d":572,"n":1,"t":5},"speaker":"A","text":"何かデザートが食べたいですね。"},{"id":{"d":572,"n":2,"t":5},"speaker":"B","text":"一緒に何か作りましょうか？"},{"id":{"d":572,"n":3,"t":5},"speaker":"A","text":"そうですね。簡単に作れるものがいいですね。"},{"id":{"d":572,"n":4,"t":5},"speaker":"B","text":"クッキーはどうですか？"},{"id":{"d":572,"n":5,"t":5},"speaker":"A","text":"うれしいですが、材料はそろっているでしょうか？"},{"id":{"d":572,"n":6,"t":5},"speaker":"B","text":"はい、大丈夫です。ちょうど、チョコレートもあります。"},{"id":{"d":572,"n":7,"t":5},"speaker":"A","text":"それでは、チョコチップクッキーを作りましょう。"},{"id":{"d":572,"n":8,"t":5},"speaker":"B","text":"おいしくできるといいですね。"}];
INSERT INTO sentence [{"id":{"d":573,"n":1,"t":5},"speaker":"A","text":"雨と風が強いですね。"},{"id":{"d":573,"n":2,"t":5},"speaker":"B","text":"そうですね。この辺りには、警報も出ているようです。"},{"id":{"d":573,"n":3,"t":5},"speaker":"A","text":"何の警報ですか？"},{"id":{"d":573,"n":4,"t":5},"speaker":"B","text":"暴風警報と洪水警報です。"},{"id":{"d":573,"n":5,"t":5},"speaker":"A","text":"怖いですね。"},{"id":{"d":573,"n":6,"t":5},"speaker":"B","text":"予報では、まだしばらくこの雨と風が続くようです。"},{"id":{"d":573,"n":7,"t":5},"speaker":"A","text":"万が一に備えて、避難する準備をしておきましょう。"},{"id":{"d":573,"n":8,"t":5},"speaker":"B","text":"はい。非常用持ち出し袋を玄関に置いておきます。"}];
INSERT INTO sentence [{"id":{"d":574,"n":1,"t":5},"speaker":"A","text":"お父さんとお母さんの結婚記念日はいつですか？"},{"id":{"d":574,"n":2,"t":5},"speaker":"B","text":"4月20日です。"},{"id":{"d":574,"n":3,"t":5},"speaker":"A","text":"もうすぐですね。何回目の結婚記念日ですか？"},{"id":{"d":574,"n":4,"t":5},"speaker":"B","text":"覚えていないので、数えてみないと分かりません。"},{"id":{"d":574,"n":5,"t":5},"speaker":"A","text":"結婚した時、お母さんは何歳でしたか？"},{"id":{"d":574,"n":6,"t":5},"speaker":"B","text":"30歳です。"},{"id":{"d":574,"n":7,"t":5},"speaker":"A","text":"今は57歳なので、今年は27回目の結婚記念日になりますね。"},{"id":{"d":574,"n":8,"t":5},"speaker":"B","text":"そうですか。月日がたつのは早いですね。"},{"id":{"d":574,"n":9,"t":5},"speaker":"A","text":"当日はお祝いをしましょうね。"},{"id":{"d":574,"n":10,"t":5},"speaker":"B","text":"うれしいです。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":575,"n":1,"t":5},"speaker":"A","text":"お歳暮の注文はこちらでお願いできますか？"},{"id":{"d":575,"n":2,"t":5},"speaker":"B","text":"はい。承ります。"},{"id":{"d":575,"n":3,"t":5},"speaker":"A","text":"このカタログに載っている商品を注文したいのですが。"},{"id":{"d":575,"n":4,"t":5},"speaker":"B","text":"はい。どちらの商品でしょうか？"},{"id":{"d":575,"n":5,"t":5},"speaker":"A","text":"このハムのセットです。"},{"id":{"d":575,"n":6,"t":5},"speaker":"B","text":"かしこまりました。では、こちらの用紙に送り先の住所をご記入ください。"},{"id":{"d":575,"n":7,"t":5},"speaker":"A","text":"3カ所に送りたいので、用紙を3枚ください。"},{"id":{"d":575,"n":8,"t":5},"speaker":"B","text":"ただいま、お持ちいたします。"}];
INSERT INTO sentence [{"id":{"d":576,"n":1,"t":5},"speaker":"A","text":"これは何ですか？"},{"id":{"d":576,"n":2,"t":5},"speaker":"B","text":"ショウブという植物ですよ。"},{"id":{"d":576,"n":3,"t":5},"speaker":"A","text":"何に使うのですか？"},{"id":{"d":576,"n":4,"t":5},"speaker":"B","text":"お風呂に入れて、しょうぶ湯にするのです。"},{"id":{"d":576,"n":5,"t":5},"speaker":"A","text":"しょうぶ湯ですか。初めて聞きました。"},{"id":{"d":576,"n":6,"t":5},"speaker":"B","text":"今日5月5日は、端午の節句です。端午の節句には、しょうぶ湯に入る習慣があるのです。"},{"id":{"d":576,"n":7,"t":5},"speaker":"A","text":"日本の伝統的な習慣なのですね。"},{"id":{"d":576,"n":8,"t":5},"speaker":"B","text":"はい。しょうぶ湯に入ることには、邪気を払うという意味があるそうです。"}];
INSERT INTO sentence [{"id":{"d":577,"n":1,"t":5},"speaker":"A","text":"今日はタケノコご飯ですよ。"},{"id":{"d":577,"n":2,"t":5},"speaker":"B","text":"おいしそうですね。"},{"id":{"d":577,"n":3,"t":5},"speaker":"A","text":"旬のタケノコなので、やわらかくておいしいと思います。"},{"id":{"d":577,"n":4,"t":5},"speaker":"B","text":"タケノコは今が旬なのですか？"},{"id":{"d":577,"n":5,"t":5},"speaker":"A","text":"そうです。春が旬です。"},{"id":{"d":577,"n":6,"t":5},"speaker":"B","text":"それで、最近はよくタケノコがスーパーマーケットで売られているのですね。"},{"id":{"d":577,"n":7,"t":5},"speaker":"A","text":"そうだと思います。季節を感じられて良いですよね。"},{"id":{"d":577,"n":8,"t":5},"speaker":"B","text":"早くタケノコ飯を食べたいです。"},{"id":{"d":577,"n":9,"t":5},"speaker":"A","text":"それでは食事にしましょう。食卓に着く前に、手を洗ってきてください。"}];
INSERT INTO sentence [{"id":{"d":578,"n":1,"t":5},"speaker":"A","text":"年賀状が届きました。"},{"id":{"d":578,"n":2,"t":5},"speaker":"B","text":"たくさん届きましたね。"},{"id":{"d":578,"n":3,"t":5},"speaker":"A","text":"これはクラスメイトの春香ちゃんからです。"},{"id":{"d":578,"n":4,"t":5},"speaker":"B","text":"かわいいイラストが描いてありますね。"},{"id":{"d":578,"n":5,"t":5},"speaker":"A","text":"春香ちゃんは絵が得意なのです。"},{"id":{"d":578,"n":6,"t":5},"speaker":"B","text":"とても手間がかかるのに、イラストを手描きしてくれるとは、すてきですね。"},{"id":{"d":578,"n":7,"t":5},"speaker":"A","text":"はい。丁寧に描いてくれてうれしいです。"},{"id":{"d":578,"n":8,"t":5},"speaker":"B","text":"年賀状には、書いた人の個性が表れますね。"}];
INSERT INTO sentence [{"id":{"d":579,"n":1,"t":5},"speaker":"A","text":"このゲームはどうやって遊ぶのですか？"},{"id":{"d":579,"n":2,"t":5},"speaker":"B","text":"画面に敵のモンスターが現れるので、それに向けてビームを発射します。全てのモンスターにビームを当てて倒すと、ゲームクリアです。"},{"id":{"d":579,"n":3,"t":5},"speaker":"A","text":"ビームを出すのは、どのボタンですか？"},{"id":{"d":579,"n":4,"t":5},"speaker":"B","text":"コントローラーのAボタンです。"},{"id":{"d":579,"n":5,"t":5},"speaker":"A","text":"自分が動きたいときは、この十字キーで動かすのですよね？"},{"id":{"d":579,"n":6,"t":5},"speaker":"B","text":"そうです。Bボタンを押すと一時停止ができます。"},{"id":{"d":579,"n":7,"t":5},"speaker":"A","text":"だいたい分かりましたが、まずは、お手本を見せてください。"},{"id":{"d":579,"n":8,"t":5},"speaker":"B","text":"それでは私がプレイするので、見ていてくださいね。"}];
INSERT INTO sentence [{"id":{"d":580,"n":1,"t":5},"speaker":"A","text":"こちらの券をどうぞ。"},{"id":{"d":580,"n":2,"t":5},"speaker":"B","text":"これは何の券ですか？"},{"id":{"d":580,"n":3,"t":5},"speaker":"A","text":"福引が1回できる券です。"},{"id":{"d":580,"n":4,"t":5},"speaker":"B","text":"一等賞の商品は何でしょうか？"},{"id":{"d":580,"n":5,"t":5},"speaker":"A","text":"旅行券5万円分です。"},{"id":{"d":580,"n":6,"t":5},"speaker":"B","text":"すてきですね。福引はどこでできます？"},{"id":{"d":580,"n":7,"t":5},"speaker":"A","text":"商店街の入り口に福引コーナーが設けてありますので、そちらでどうぞ。"},{"id":{"d":580,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。行ってみます。"}];
INSERT INTO sentence [{"id":{"d":581,"n":1,"t":5},"speaker":"A","text":"新しいゲームソフトを買いました。"},{"id":{"d":581,"n":2,"t":5},"speaker":"B","text":"どのようなソフトを買ったのですか？見せてください。"},{"id":{"d":581,"n":3,"t":5},"speaker":"A","text":"これです。ずっと欲しかったものなのですが、やっと買えました。"},{"id":{"d":581,"n":4,"t":5},"speaker":"B","text":"とても人気のあるゲームですよね。"},{"id":{"d":581,"n":5,"t":5},"speaker":"A","text":"はい。学校でもこのゲームを持っている人がたくさんいます。"},{"id":{"d":581,"n":6,"t":5},"speaker":"B","text":"私の兄も持っています。"},{"id":{"d":581,"n":7,"t":5},"speaker":"A","text":"それでは、あなたもお兄さんと一緒にやったことがあるのですね。"},{"id":{"d":581,"n":8,"t":5},"speaker":"B","text":"はい。時々、兄と対戦しています。"}];
INSERT INTO sentence [{"id":{"d":582,"n":1,"t":5},"speaker":"A","text":"素敵なネックレスですね。"},{"id":{"d":582,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。夫がクリスマスにプレゼントしてくれました。"},{"id":{"d":582,"n":3,"t":5},"speaker":"A","text":"クリスマスプレゼントをくれるなんて、優しい旦那さまですね。"},{"id":{"d":582,"n":4,"t":5},"speaker":"B","text":"私たちは、毎年、クリスマスにプレゼントを贈り合うことにしているのです。"},{"id":{"d":582,"n":5,"t":5},"speaker":"A","text":"すてきですね。でも、毎年だと何を贈るか悩みませんか？"},{"id":{"d":582,"n":6,"t":5},"speaker":"B","text":"ここ数年は、事前に、お互いのリクエストを伝え合っていますので、悩むことはありません。"},{"id":{"d":582,"n":7,"t":5},"speaker":"A","text":"それはいいですね。あなたは今年は、旦那さまに何を贈ったのですか？"},{"id":{"d":582,"n":8,"t":5},"speaker":"B","text":"ベルトです。喜んでもらえたようで、毎日使ってくれています。"}];
INSERT INTO sentence [{"id":{"d":583,"n":1,"t":5},"speaker":"A","text":"花札で遊びませんか？"},{"id":{"d":583,"n":2,"t":5},"speaker":"B","text":"花札という言葉を聞いたことはありますが、実際に見たことも遊んだこともありません。"},{"id":{"d":583,"n":3,"t":5},"speaker":"A","text":"これが花札です。かるたのようなものですよ。"},{"id":{"d":583,"n":4,"t":5},"speaker":"B","text":"絵柄がきれいですね。どうやって遊ぶのですか？"},{"id":{"d":583,"n":5,"t":5},"speaker":"A","text":"たくさんの種類の遊び方があるのですが、私は「こいこい」という遊び方しか知りません。"},{"id":{"d":583,"n":6,"t":5},"speaker":"B","text":"それをしてみたいです。遊び方を教えてください。"},{"id":{"d":583,"n":7,"t":5},"speaker":"A","text":"まずは親と子を決めます。じゃんけんで決めますか？"},{"id":{"d":583,"n":8,"t":5},"speaker":"B","text":"いえ、今回はあなたが親になってください。"},{"id":{"d":583,"n":9,"t":5},"speaker":"A","text":"分かりました。次に手札を8枚ずつ配りますね。"}];
INSERT INTO sentence [{"id":{"d":584,"n":1,"t":5},"speaker":"A","text":"これは変わった形をした椅子ですね。"},{"id":{"d":584,"n":2,"t":5},"speaker":"B","text":"ゲーム用の椅子です。"},{"id":{"d":584,"n":3,"t":5},"speaker":"A","text":"そのような椅子があるのですね。"},{"id":{"d":584,"n":4,"t":5},"speaker":"B","text":"長時間プレイしても疲れにくいように作られているのです。"},{"id":{"d":584,"n":5,"t":5},"speaker":"A","text":"それほど長い時間、ゲームをするのですか？"},{"id":{"d":584,"n":6,"t":5},"speaker":"B","text":"1日に10時間以上ゲームをしていることも多いです。"},{"id":{"d":584,"n":7,"t":5},"speaker":"A","text":"座ってみてもよいですか？"},{"id":{"d":584,"n":8,"t":5},"speaker":"B","text":"もちろん、どうぞ。"},{"id":{"d":584,"n":9,"t":5},"speaker":"A","text":"確かに、座り心地が良いです。"}];
INSERT INTO sentence [{"id":{"d":585,"n":1,"t":5},"speaker":"A","text":"私は高校でかるた部に所属していました。"},{"id":{"d":585,"n":2,"t":5},"speaker":"B","text":"かるた部というと、百人一首をする部活ですよね。"},{"id":{"d":585,"n":3,"t":5},"speaker":"A","text":"はい。私の高校は結構強くて、全国大会にも出場しました。"},{"id":{"d":585,"n":4,"t":5},"speaker":"B","text":"それはすごいですね。百人一首は全て暗記していたのですか？"},{"id":{"d":585,"n":5,"t":5},"speaker":"A","text":"はい。今でも全部覚えています。"},{"id":{"d":585,"n":6,"t":5},"speaker":"B","text":"どうやって暗記するのか、こつを教えてほしいです。"},{"id":{"d":585,"n":7,"t":5},"speaker":"A","text":"百人一首が上の句と下の句に分かれているのは知っていますか？"},{"id":{"d":585,"n":8,"t":5},"speaker":"B","text":"はい。上の句を読んで、札に書いてある下の句を取るのですよね。"},{"id":{"d":585,"n":9,"t":5},"speaker":"A","text":"そうです。私は上の句と下の句を語呂合わせで覚えました。"},{"id":{"d":585,"n":10,"t":5},"speaker":"B","text":"なるほど。それなら覚えやすそうですね。"}];
INSERT INTO sentence [{"id":{"d":586,"n":1,"t":5},"speaker":"A","text":"映画の字幕が英語になっていますよ。日本語にしなくてよいのですか？"},{"id":{"d":586,"n":2,"t":5},"speaker":"B","text":"はい。英語の勉強のために、あえて英語の字幕にしています。"},{"id":{"d":586,"n":3,"t":5},"speaker":"A","text":"英語のせりふを聞きながら英語の字幕を読むのは大変ではありませんか？"},{"id":{"d":586,"n":4,"t":5},"speaker":"B","text":"最初は全くできませんが、同じ映画を何度か見ると、だんだんとできるようになってきます。"},{"id":{"d":586,"n":5,"t":5},"speaker":"A","text":"同じ映画を何度も見るのですか？私は飽きてしまいそうです。"},{"id":{"d":586,"n":6,"t":5},"speaker":"B","text":"英語の勉強のために見ているので、大丈夫ですよ。"},{"id":{"d":586,"n":7,"t":5},"speaker":"A","text":"ちなみにこの映画を見るのは、何回目ですか？"},{"id":{"d":586,"n":8,"t":5},"speaker":"B","text":"4回目です。"},{"id":{"d":586,"n":9,"t":5},"speaker":"A","text":"もうストーリーを覚えてしまっているでしょうね。"},{"id":{"d":586,"n":10,"t":5},"speaker":"B","text":"そうですね。せりふもほとんど頭に入っています。"}];
INSERT INTO sentence [{"id":{"d":587,"n":1,"t":5},"speaker":"A","text":"しばらく前から、インスタグラムに写真や動画を投稿しています。"},{"id":{"d":587,"n":2,"t":5},"speaker":"B","text":"そうなのですね。あなたのアカウントを教えてほしいです。"},{"id":{"d":587,"n":3,"t":5},"speaker":"A","text":"構いませんが、私の投稿を見られるのは少し恥ずかしいです。"},{"id":{"d":587,"n":4,"t":5},"speaker":"B","text":"どのような投稿をしているのですか？"},{"id":{"d":587,"n":5,"t":5},"speaker":"A","text":"好きな曲に合わせてダンスしている動画が多いです。"},{"id":{"d":587,"n":6,"t":5},"speaker":"B","text":"あなたはダンスが上手なので、ぴったりですね。"},{"id":{"d":587,"n":7,"t":5},"speaker":"A","text":"あなたはインスタグラムをしていないのですか？"},{"id":{"d":587,"n":8,"t":5},"speaker":"B","text":"アカウントはありますが、私は見るだけで、自分では投稿していません。"}];
INSERT INTO sentence [{"id":{"d":588,"n":1,"t":5},"speaker":"A","text":"週末に家族でサーカスを見に行きました。"},{"id":{"d":588,"n":2,"t":5},"speaker":"B","text":"私はサーカスを見たことがないのですが、どのようなことをするのですか？"},{"id":{"d":588,"n":3,"t":5},"speaker":"A","text":"クマが三輪車に乗ったり、人が空中で綱を渡ったりしていました。"},{"id":{"d":588,"n":4,"t":5},"speaker":"B","text":"一番印象に残っているものは何ですか？"},{"id":{"d":588,"n":5,"t":5},"speaker":"A","text":"ライオンが火の付いた輪をくぐるというものです。"},{"id":{"d":588,"n":6,"t":5},"speaker":"B","text":"それはすごいですね。"},{"id":{"d":588,"n":7,"t":5},"speaker":"A","text":"はい。とても迫力があって、見応えがありました。"},{"id":{"d":588,"n":8,"t":5},"speaker":"B","text":"火の輪くぐりは成功しましたか？"},{"id":{"d":588,"n":9,"t":5},"speaker":"A","text":"はい。見事に成功しました。"}];
INSERT INTO sentence [{"id":{"d":589,"n":1,"t":5},"speaker":"A","text":"ここに書道の道具が置いてありますね。"},{"id":{"d":589,"n":2,"t":5},"speaker":"B","text":"母の趣味が書道なのです。"},{"id":{"d":589,"n":3,"t":5},"speaker":"A","text":"すてきな趣味ですね。"},{"id":{"d":589,"n":4,"t":5},"speaker":"B","text":"そうですね。年齢に関係なく長く続けられる良い趣味だと思います。"},{"id":{"d":589,"n":5,"t":5},"speaker":"A","text":"あなたは書道をしないのですか？"},{"id":{"d":589,"n":6,"t":5},"speaker":"B","text":"ずっと前に挑戦したことがありますが、全く上手に書けなくて嫌になってしまいました。"},{"id":{"d":589,"n":7,"t":5},"speaker":"A","text":"上達するには時間がかかるでしょうね。"},{"id":{"d":589,"n":8,"t":5},"speaker":"B","text":"はい。私は短気なので、書道の魅力が分かる前に挫折してしまったのです。"}];
INSERT INTO sentence [{"id":{"d":590,"n":1,"t":5},"speaker":"A","text":"今度、沙羅ちゃんとテニスをする約束をしました。"},{"id":{"d":590,"n":2,"t":5},"speaker":"B","text":"それは楽しみですね。"},{"id":{"d":590,"n":3,"t":5},"speaker":"A","text":"テニスコートの予約をお願いできますか？"},{"id":{"d":590,"n":4,"t":5},"speaker":"B","text":"分かりました。北園運動公園のコートですか？"},{"id":{"d":590,"n":5,"t":5},"speaker":"A","text":"いいえ。そこは沙羅ちゃんの家から遠いので、すみれ体育広場のテニスコートがいいです。"},{"id":{"d":590,"n":6,"t":5},"speaker":"B","text":"分かりました。予約はいつにしますか？"},{"id":{"d":590,"n":7,"t":5},"speaker":"A","text":"次の土曜日の午後1時からでお願いします。"},{"id":{"d":590,"n":8,"t":5},"speaker":"B","text":"はい。空いているといいですね。"}];
INSERT INTO sentence [{"id":{"d":591,"n":1,"t":5},"speaker":"A","text":"真っ赤でおいしそうなミニトマトですね。"},{"id":{"d":591,"n":2,"t":5},"speaker":"B","text":"私が庭で育てたものです。"},{"id":{"d":591,"n":3,"t":5},"speaker":"A","text":"お母さんは本当に家庭菜園がお好きですね。"},{"id":{"d":591,"n":4,"t":5},"speaker":"B","text":"はい。私の唯一の趣味です。"},{"id":{"d":591,"n":5,"t":5},"speaker":"A","text":"このミニトマトは食べてもよいですか？"},{"id":{"d":591,"n":6,"t":5},"speaker":"B","text":"食べてもよいですが、お父さんにも食べてもらいたいので、いくつかは食べずに残しておいてください。"},{"id":{"d":591,"n":7,"t":5},"speaker":"A","text":"分かりました。みずみずしくて、とてもおいしいです。"},{"id":{"d":591,"n":8,"t":5},"speaker":"B","text":"それはよかったです。今回のミニトマトは、うまく育てられたと思います。"}];
INSERT INTO sentence [{"id":{"d":592,"n":1,"t":5},"speaker":"A","text":"このアトラクションに乗るには、どのくらい待てばよいですか？"},{"id":{"d":592,"n":2,"t":5},"speaker":"B","text":"1時間くらいだそうです。"},{"id":{"d":592,"n":3,"t":5},"speaker":"A","text":"いつも、そのくらいの待ち時間でしょうか？"},{"id":{"d":592,"n":4,"t":5},"speaker":"B","text":"人気のアトラクションなので待ち時間は長くなってしまいますが、閉園間際になると短くなります。"},{"id":{"d":592,"n":5,"t":5},"speaker":"A","text":"今日の閉園は何時ですか？"},{"id":{"d":592,"n":6,"t":5},"speaker":"B","text":"9時です。8時半近くになると、待ち時間はかなり短くなると思います。"},{"id":{"d":592,"n":7,"t":5},"speaker":"A","text":"それでは、そのくらいの時間にもう一度来ましょう。"},{"id":{"d":592,"n":8,"t":5},"speaker":"B","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":593,"n":1,"t":5},"speaker":"A","text":"最近は、小学校であやとりがはやっています。"},{"id":{"d":593,"n":2,"t":5},"speaker":"B","text":"あやとりですか？懐かしいですね。"},{"id":{"d":593,"n":3,"t":5},"speaker":"A","text":"お母さんはあやとりで何か作れますか？"},{"id":{"d":593,"n":4,"t":5},"speaker":"B","text":"簡単なものだったら作れます。ほうきとか、はしごとか、東京タワーとか。"},{"id":{"d":593,"n":5,"t":5},"speaker":"A","text":"東京タワーはどうやって作るのですか？教えてください。"},{"id":{"d":593,"n":6,"t":5},"speaker":"B","text":"いいですよ。あやとりを貸してくれますか？"},{"id":{"d":593,"n":7,"t":5},"speaker":"A","text":"はい。これでお願いします。"},{"id":{"d":593,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。では、今から作るので見ていてくださいね。"}];
INSERT INTO sentence [{"id":{"d":594,"n":1,"t":5},"speaker":"A","text":"テレビのチャンネルを変えてよいですか？"},{"id":{"d":594,"n":2,"t":5},"speaker":"B","text":"よいですよ。何を見るのですか？"},{"id":{"d":594,"n":3,"t":5},"speaker":"A","text":"サッカーのワールドカップの試合です。"},{"id":{"d":594,"n":4,"t":5},"speaker":"B","text":"そういえば、今日は試合が放送されるのでしたね。"},{"id":{"d":594,"n":5,"t":5},"speaker":"A","text":"はい。私はサッカーが大好きなので、この試合を見ることを、朝からずっと楽しみにしていました。"},{"id":{"d":594,"n":6,"t":5},"speaker":"B","text":"あなたは本当にサッカーが大好きなのですね。私はサッカーのルールもよく分かりません。"},{"id":{"d":594,"n":7,"t":5},"speaker":"A","text":"私が解説しますから、一緒に見ませんか？"},{"id":{"d":594,"n":8,"t":5},"speaker":"B","text":"ぜひお願いします。"}];
INSERT INTO sentence [{"id":{"d":595,"n":1,"t":5},"speaker":"A","text":"暑いので、今から友達とプールに行ってきます。"},{"id":{"d":595,"n":2,"t":5},"speaker":"B","text":"車で送りましょうか？"},{"id":{"d":595,"n":3,"t":5},"speaker":"A","text":"みんなと一緒に自転車で行くので大丈夫です。"},{"id":{"d":595,"n":4,"t":5},"speaker":"B","text":"分かりました。気を付けて行ってきてくださいね。プールに入るにはお金が必要ですね。利用料金はいくらですか？"},{"id":{"d":595,"n":5,"t":5},"speaker":"A","text":"500円です。"},{"id":{"d":595,"n":6,"t":5},"speaker":"B","text":"1,000円を預けますので、残ったお金は後で返してください。"},{"id":{"d":595,"n":7,"t":5},"speaker":"A","text":"プールを出た後でアイスを食べたいのですが、このお金で買ってよいですか？"},{"id":{"d":595,"n":8,"t":5},"speaker":"B","text":"よいですよ。みんなで楽しんできてくださいね。"},{"id":{"d":595,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":596,"n":1,"t":5},"speaker":"A","text":"盆踊りをやっていますよ。私たちも踊りましょう。"},{"id":{"d":596,"n":2,"t":5},"speaker":"B","text":"踊り方が分からないので、私は遠慮しておきます。"},{"id":{"d":596,"n":3,"t":5},"speaker":"A","text":"やぐらで踊っている人をまねればよいだけなので、大丈夫ですよ。"},{"id":{"d":596,"n":4,"t":5},"speaker":"B","text":"確かにそうですね。でも、人前で踊るのは恥ずかしいです。"},{"id":{"d":596,"n":5,"t":5},"speaker":"A","text":"みんな自分が踊るのに夢中で、私たちのことは見ていませんよ。きっと楽しいので、一緒に踊りましょうよ。"},{"id":{"d":596,"n":6,"t":5},"speaker":"B","text":"では、あなたが前になってください。私は後ろであなたをお手本にして踊ります。"},{"id":{"d":596,"n":7,"t":5},"speaker":"A","text":"いいですよ。行きましょう。"},{"id":{"d":596,"n":8,"t":5},"speaker":"B","text":"あなたは盆踊りが好きなのですね。"},{"id":{"d":596,"n":9,"t":5},"speaker":"A","text":"はい。子どもの頃から大好きです。"}];
INSERT INTO sentence [{"id":{"d":597,"n":1,"t":5},"speaker":"A","text":"私が詠んだ俳句が雑誌に掲載されました。"},{"id":{"d":597,"n":2,"t":5},"speaker":"B","text":"それはよかったですね。おめでとうございます。"},{"id":{"d":597,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。長年、俳句の教室に通ったかいがあります。"},{"id":{"d":597,"n":4,"t":5},"speaker":"B","text":"あなたの句が掲載された雑誌をぜひとも拝見したいです。"},{"id":{"d":597,"n":5,"t":5},"speaker":"A","text":"こちらです。83ページに載っています。"},{"id":{"d":597,"n":6,"t":5},"speaker":"B","text":"すてきな俳句ですね。"},{"id":{"d":597,"n":7,"t":5},"speaker":"A","text":"お世辞でもうれしいです。"},{"id":{"d":597,"n":8,"t":5},"speaker":"B","text":"お世辞ではありませんよ。本当に素晴らしいと思います。"}];
INSERT INTO sentence [{"id":{"d":598,"n":1,"t":5},"speaker":"A","text":"ご注文はお決まりですか？"},{"id":{"d":598,"n":2,"t":5},"speaker":"B","text":"タピオカほうじ茶ミルクを1つください。"},{"id":{"d":598,"n":3,"t":5},"speaker":"A","text":"ホットとアイスがございますが、どちらになさいますか？"},{"id":{"d":598,"n":4,"t":5},"speaker":"B","text":"アイスでお願いします。"},{"id":{"d":598,"n":5,"t":5},"speaker":"A","text":"サイズはどうされますか？"},{"id":{"d":598,"n":6,"t":5},"speaker":"B","text":"Mはどのくらいのサイズですか？"},{"id":{"d":598,"n":7,"t":5},"speaker":"A","text":"こちらにカップの見本があります。Mですと、このカップの大きさですね。"},{"id":{"d":598,"n":8,"t":5},"speaker":"B","text":"それではMでお願いします。"},{"id":{"d":598,"n":9,"t":5},"speaker":"A","text":"かしこまりました。ご準備いたしますので、少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":599,"n":1,"t":5},"speaker":"A","text":"今日は料理教室で肉じゃがを作りました。"},{"id":{"d":599,"n":2,"t":5},"speaker":"B","text":"家庭的な料理ですね。"},{"id":{"d":599,"n":3,"t":5},"speaker":"A","text":"はい。毎日の献立に活用できそうです。"},{"id":{"d":599,"n":4,"t":5},"speaker":"B","text":"おいしくできましたか？"},{"id":{"d":599,"n":5,"t":5},"speaker":"A","text":"味付けは合格点だったのですが、ニンジンが少し硬かったです。"},{"id":{"d":599,"n":6,"t":5},"speaker":"B","text":"切り方が大きかったのかもしれませんね。"},{"id":{"d":599,"n":7,"t":5},"speaker":"A","text":"火加減が弱かったようにも思います。"},{"id":{"d":599,"n":8,"t":5},"speaker":"B","text":"今度、ぜひ私にも作ってくださいね。"},{"id":{"d":599,"n":9,"t":5},"speaker":"A","text":"いつでも作って差し上げますよ。"}];
INSERT INTO sentence [{"id":{"d":600,"n":1,"t":5},"speaker":"A","text":"あなたはツイッターを使っていますか？"},{"id":{"d":600,"n":2,"t":5},"speaker":"B","text":"他の人のツイートを見ることはありますが、自分では投稿しません。"},{"id":{"d":600,"n":3,"t":5},"speaker":"A","text":"どのような人のツイートを見るのですか？"},{"id":{"d":600,"n":4,"t":5},"speaker":"B","text":"好きなタレントや芸人です。あなたもツイッターを使っているのですか？"},{"id":{"d":600,"n":5,"t":5},"speaker":"A","text":"はい。私は自分でも投稿しています。"},{"id":{"d":600,"n":6,"t":5},"speaker":"B","text":"あなたのアカウント名を教えてもらえますか？"},{"id":{"d":600,"n":7,"t":5},"speaker":"A","text":"もちろん。私のアカウントはこれです。フォローしてもらえたらうれしいです。"},{"id":{"d":600,"n":8,"t":5},"speaker":"B","text":"友達のツイートを見るのは初めてなので、楽しみです。"}];
INSERT INTO sentence [{"id":{"d":601,"n":1,"t":5},"speaker":"A","text":"時間があると、ついスマートフォンを触ってしまいます。"},{"id":{"d":601,"n":2,"t":5},"speaker":"B","text":"私もそうです。あなたはスマートフォンで何をしていることが多いですか？"},{"id":{"d":601,"n":3,"t":5},"speaker":"A","text":"インスタグラムかYouTubeを見ていることが多いです。"},{"id":{"d":601,"n":4,"t":5},"speaker":"B","text":"見始めると、つい次々と見てしまいますよね。"},{"id":{"d":601,"n":5,"t":5},"speaker":"A","text":"本当にそうです。昨夜も、外国の美しい風景の動画を見ていたらあっという間に夜中になっていました。"},{"id":{"d":601,"n":6,"t":5},"speaker":"B","text":"私は動物の動画をよく見ています。"},{"id":{"d":601,"n":7,"t":5},"speaker":"A","text":"それもいいですね。1日の中でスマートフォンを見ている時間が長過ぎるという自覚はあるのですが、なかなか止められません。"},{"id":{"d":601,"n":8,"t":5},"speaker":"B","text":"はい。いつの頃からか、隙間の時間にスマートフォンを見ることが癖になっていますね。"}];
INSERT INTO sentence [{"id":{"d":602,"n":1,"t":5},"speaker":"A","text":"ここには、いろいろな種類のウサギがいますね。"},{"id":{"d":602,"n":2,"t":5},"speaker":"B","text":"そうですね。大きさも色も、さまざまです。"},{"id":{"d":602,"n":3,"t":5},"speaker":"A","text":"あそこでウサギの餌が売られていますよ。"},{"id":{"d":602,"n":4,"t":5},"speaker":"B","text":"ウサギに餌やりができるようですね。買いましょうか？"},{"id":{"d":602,"n":5,"t":5},"speaker":"A","text":"はい、買いましょう。200円ですね。"},{"id":{"d":602,"n":6,"t":5},"speaker":"B","text":"キャベツとニンジンが入っているようです。"},{"id":{"d":602,"n":7,"t":5},"speaker":"A","text":"早速、ウサギにあげましょう。"},{"id":{"d":602,"n":8,"t":5},"speaker":"B","text":"ここから柵の中に入れるようです。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":603,"n":1,"t":5},"speaker":"A","text":"電車の写真がたくさん飾ってありますね。"},{"id":{"d":603,"n":2,"t":5},"speaker":"B","text":"電車の写真を撮ることが、私の趣味なのです。"},{"id":{"d":603,"n":3,"t":5},"speaker":"A","text":"すてきですね。この写真の電車は見たことがありません。どこを走る電車なのですか？"},{"id":{"d":603,"n":4,"t":5},"speaker":"B","text":"これは日本ではなく、スイスの電車です。"},{"id":{"d":603,"n":5,"t":5},"speaker":"A","text":"写真を撮るためにスイスまで行ったのですか？"},{"id":{"d":603,"n":6,"t":5},"speaker":"B","text":"電車の撮影が目的というわけではなく、大学の卒業旅行としてスイスに行ってきたのです。"},{"id":{"d":603,"n":7,"t":5},"speaker":"A","text":"日本の電車とはデザインが違っていて、格好良いですね。"},{"id":{"d":603,"n":8,"t":5},"speaker":"B","text":"はい。現地でカメラを構えていた時も、興奮してたくさん写真を撮ってしまいました。"}];
INSERT INTO sentence [{"id":{"d":604,"n":1,"t":5},"speaker":"A","text":"部屋に置く観葉植物を探しているのですが、どれがいいでしょうか？"},{"id":{"d":604,"n":2,"t":5},"speaker":"B","text":"観葉植物を置く予定のお部屋は、どのくらいの広さですか？"},{"id":{"d":604,"n":3,"t":5},"speaker":"A","text":"12畳くらいのリビングルームです。"},{"id":{"d":604,"n":4,"t":5},"speaker":"B","text":"それではこちらはいかがでしょうか？パキラという植物です。"},{"id":{"d":604,"n":5,"t":5},"speaker":"A","text":"よさそうです。水やりはどのくらいの頻度ですればよいのですか？"},{"id":{"d":604,"n":6,"t":5},"speaker":"B","text":"土の表面が乾燥したときに水をあげる程度で大丈夫です。"},{"id":{"d":604,"n":7,"t":5},"speaker":"A","text":"リビングは直射日光が差し込むのですが、大丈夫でしょうか？"},{"id":{"d":604,"n":8,"t":5},"speaker":"B","text":"はい。パキラはむしろ日光を好む性質がありますよ。"}];
INSERT INTO sentence [{"id":{"d":605,"n":1,"t":5},"speaker":"A","text":"何を編んでいるのですか？"},{"id":{"d":605,"n":2,"t":5},"speaker":"B","text":"孫の手袋です。"},{"id":{"d":605,"n":3,"t":5},"speaker":"A","text":"小さくてかわいい手袋ですね。"},{"id":{"d":605,"n":4,"t":5},"speaker":"B","text":"孫の4歳の誕生日がもうすぐなので、この手袋をプレゼントするつもりです。"},{"id":{"d":605,"n":5,"t":5},"speaker":"A","text":"きっと喜びますよ。"},{"id":{"d":605,"n":6,"t":5},"speaker":"B","text":"そうだとうれしいのですが、素人なので出来上がりに自信がありません。"},{"id":{"d":605,"n":7,"t":5},"speaker":"A","text":"愛情がたっぷりの手袋ですから、絶対に大丈夫ですよ。誕生日までに仕上がりそうですか？"},{"id":{"d":605,"n":8,"t":5},"speaker":"B","text":"はい。何とか間に合いそうです。"}];
INSERT INTO sentence [{"id":{"d":606,"n":1,"t":5},"speaker":"A","text":"砂の山を作っているのですか？"},{"id":{"d":606,"n":2,"t":5},"speaker":"B","text":"そうです。ママ、そこにあるスコップを貸してください。"},{"id":{"d":606,"n":3,"t":5},"speaker":"A","text":"はい、どうぞ。砂を集めるのですか？"},{"id":{"d":606,"n":4,"t":5},"speaker":"B","text":"そうです。手で集めるより、スコップを使う方が早く集められます。"},{"id":{"d":606,"n":5,"t":5},"speaker":"A","text":"なるほど。山が出来上がったらその後はどうするのですか？"},{"id":{"d":606,"n":6,"t":5},"speaker":"B","text":"山が出来上がったら、トンネルを掘ろうと思います。"},{"id":{"d":606,"n":7,"t":5},"speaker":"A","text":"いいですね。"}];
INSERT INTO sentence [{"id":{"d":607,"n":1,"t":5},"speaker":"A","text":"茜ちゃん。塗り絵の紙を持ってきたので、今から一緒にしませんか？"},{"id":{"d":607,"n":2,"t":5},"speaker":"B","text":"はい、したいです。千夏ちゃんはどれを塗りますか？"},{"id":{"d":607,"n":3,"t":5},"speaker":"A","text":"私はこの女の子の絵がいいです。茜ちゃんはどれがいいですか？"},{"id":{"d":607,"n":4,"t":5},"speaker":"B","text":"私はお花が好きのですが、お花の絵はありますか？"},{"id":{"d":607,"n":5,"t":5},"speaker":"A","text":"ありますよ。バラの絵とマリーゴールドの絵の2種類があります。どちらがいいですか？"},{"id":{"d":607,"n":6,"t":5},"speaker":"B","text":"マリーゴールドがいいです。"},{"id":{"d":607,"n":7,"t":5},"speaker":"A","text":"それでは、この紙をどうぞ。色鉛筆は1セットしかないので、一緒に使いましょうね。"},{"id":{"d":607,"n":8,"t":5},"speaker":"B","text":"はい。あの机で塗りましょう。"}];
INSERT INTO sentence [{"id":{"d":608,"n":1,"t":5},"speaker":"A","text":"やっと仕事が終わりましたね。駅まで一緒に帰りませんか？"},{"id":{"d":608,"n":2,"t":5},"speaker":"B","text":"すみません。今日は星野デパートに寄って帰ります。"},{"id":{"d":608,"n":3,"t":5},"speaker":"A","text":"買い物ですか？"},{"id":{"d":608,"n":4,"t":5},"speaker":"B","text":"いいえ、気分転換にウインドーショッピングをするつもりです。"},{"id":{"d":608,"n":5,"t":5},"speaker":"A","text":"迷惑でなければ、私も一緒に行ってよいですか？"},{"id":{"d":608,"n":6,"t":5},"speaker":"B","text":"もちろんです。お店を見て回るだけですが、大丈夫ですか？"},{"id":{"d":608,"n":7,"t":5},"speaker":"A","text":"はい。実は、私もウインドーショッピングが大好きなのです。"},{"id":{"d":608,"n":8,"t":5},"speaker":"B","text":"そうでしたか。それでは行きましょう。"}];
INSERT INTO sentence [{"id":{"d":609,"n":1,"t":5},"speaker":"A","text":"ホームパーティーに招待されました。"},{"id":{"d":609,"n":2,"t":5},"speaker":"B","text":"それは楽しみですね。"},{"id":{"d":609,"n":3,"t":5},"speaker":"A","text":"それぞれ何か一品ずつ持ち寄ることになったのですが、何がいいでしょうか？"},{"id":{"d":609,"n":4,"t":5},"speaker":"B","text":"春巻きやちらしずしなどはどうでしょう。"},{"id":{"d":609,"n":5,"t":5},"speaker":"A","text":"料理が苦手なので、おいしいものを作れる自信がありません。"},{"id":{"d":609,"n":6,"t":5},"speaker":"B","text":"それならデザートを買って持参すれば、作る必要がありませんよ。"},{"id":{"d":609,"n":7,"t":5},"speaker":"A","text":"そうですね。甘いものが好きな人が多いので、喜んでもらえそうです。"},{"id":{"d":609,"n":8,"t":5},"speaker":"B","text":"持ち運びしやすい焼き菓子は、特にいいかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":610,"n":1,"t":5},"speaker":"A","text":"お母さん、虹の色を教えてください。"},{"id":{"d":610,"n":2,"t":5},"speaker":"B","text":"どういうことですか？"},{"id":{"d":610,"n":3,"t":5},"speaker":"A","text":"虹の絵を描きたいのですが、色の順番が分かりません。"},{"id":{"d":610,"n":4,"t":5},"speaker":"B","text":"改めて聞かれると、自信がありません。インターネットで調べてみますね。"},{"id":{"d":610,"n":5,"t":5},"speaker":"A","text":"どうもありがとうございます。"},{"id":{"d":610,"n":6,"t":5},"speaker":"B","text":"外側から赤色、だいだい色、黄色、緑色、青色、藍色、紫色の順番だそうです。"},{"id":{"d":610,"n":7,"t":5},"speaker":"A","text":"だいだい色というのは初めて聞きました。どのような色ですか？"},{"id":{"d":610,"n":8,"t":5},"speaker":"B","text":"オレンジ色のことです。"}];
INSERT INTO sentence [{"id":{"d":611,"n":1,"t":5},"speaker":"A","text":"海外の映画は字幕と吹き替えのどちらで見ますか？"},{"id":{"d":611,"n":2,"t":5},"speaker":"B","text":"私は字幕で見ます。"},{"id":{"d":611,"n":3,"t":5},"speaker":"A","text":"映像を見ながら文字を読むのは疲れませんか？"},{"id":{"d":611,"n":4,"t":5},"speaker":"B","text":"初めて海外の映画を見た時から字幕で見ているせいか、大丈夫です。"},{"id":{"d":611,"n":5,"t":5},"speaker":"A","text":"私は字幕だと目が疲れてしまうので、常に吹き替えで見ます。"},{"id":{"d":611,"n":6,"t":5},"speaker":"B","text":"俳優の本当の声で映画を見たいと思いませんか？"},{"id":{"d":611,"n":7,"t":5},"speaker":"A","text":"よく聞かれますが、見ていると声優さんの声が俳優さん本人の声として感じられるようになります。"},{"id":{"d":611,"n":8,"t":5},"speaker":"B","text":"そういうものなのですね。声優さんの力も素晴らしいですものね。"}];
INSERT INTO sentence [{"id":{"d":612,"n":1,"t":5},"speaker":"A","text":"お父さん、これをどうぞ。"},{"id":{"d":612,"n":2,"t":5},"speaker":"B","text":"これは何ですか？"},{"id":{"d":612,"n":3,"t":5},"speaker":"A","text":"父の日のプレゼントです。"},{"id":{"d":612,"n":4,"t":5},"speaker":"B","text":"どうもありがとうございます。開けてよいですか？"},{"id":{"d":612,"n":5,"t":5},"speaker":"A","text":"もちろんです。開けてください。"},{"id":{"d":612,"n":6,"t":5},"speaker":"B","text":"靴下ですね。とてもうれしいです。"},{"id":{"d":612,"n":7,"t":5},"speaker":"A","text":"明日から履いてくださいね。"},{"id":{"d":612,"n":8,"t":5},"speaker":"B","text":"はい。ちょうど新しい靴下が欲しかったので、ありがたいです。"}];
INSERT INTO sentence [{"id":{"d":613,"n":1,"t":5},"speaker":"A","text":"射的の屋台がありますよ。行きましょう。"},{"id":{"d":613,"n":2,"t":5},"speaker":"B","text":"いいですね。何を狙うのですか？"},{"id":{"d":613,"n":3,"t":5},"speaker":"A","text":"あのお菓子を狙ってみます。"},{"id":{"d":613,"n":4,"t":5},"speaker":"B","text":"頑張ってください。応援しています。"},{"id":{"d":613,"n":5,"t":5},"speaker":"A","text":"外してしまいました。"},{"id":{"d":613,"n":6,"t":5},"speaker":"B","text":"惜しかったです。次はきっと当たりますよ。"},{"id":{"d":613,"n":7,"t":5},"speaker":"A","text":"当たったかと思いましたが、かすっただけですね。"},{"id":{"d":613,"n":8,"t":5},"speaker":"B","text":"本当に惜しいです。もう少しで当たりますよ。"}];
INSERT INTO sentence [{"id":{"d":614,"n":1,"t":5},"speaker":"A","text":"成人式には何を着ますか？"},{"id":{"d":614,"n":2,"t":5},"speaker":"B","text":"振り袖を着ます。"},{"id":{"d":614,"n":3,"t":5},"speaker":"A","text":"振り袖を持っているのですか？"},{"id":{"d":614,"n":4,"t":5},"speaker":"B","text":"いいえ、持っていないのでレンタルします。"},{"id":{"d":614,"n":5,"t":5},"speaker":"A","text":"私も振り袖を着ますが、母の振り袖を借ります。"},{"id":{"d":614,"n":6,"t":5},"speaker":"B","text":"お母さんが持っているのですね。"},{"id":{"d":614,"n":7,"t":5},"speaker":"A","text":"はい。母が成人式で着た振り袖です。"},{"id":{"d":614,"n":8,"t":5},"speaker":"B","text":"それはいいですね。"}];
INSERT INTO sentence [{"id":{"d":615,"n":1,"t":5},"speaker":"A","text":"今年は香澄が3歳なので、七五三のお参りに行きましょう。"},{"id":{"d":615,"n":2,"t":5},"speaker":"B","text":"そうですね。どこへお参りに行きましょうか？"},{"id":{"d":615,"n":3,"t":5},"speaker":"A","text":"氏神様がいいのではないでしょうか？"},{"id":{"d":615,"n":4,"t":5},"speaker":"B","text":"そうですね。それでは池宮神社にしましょう。"},{"id":{"d":615,"n":5,"t":5},"speaker":"A","text":"香澄には着物を着せますか？"},{"id":{"d":615,"n":6,"t":5},"speaker":"B","text":"そうですね。着物をレンタルできるお店を探しておきます。"},{"id":{"d":615,"n":7,"t":5},"speaker":"A","text":"神社はご祈祷の予約が必要になるかもしれないので、私が問い合わせをしてみます。"},{"id":{"d":615,"n":8,"t":5},"speaker":"B","text":"どうもありがとうございます。予約が必要なら、週末の午前中でお願いします。"}];
INSERT INTO sentence [{"id":{"d":616,"n":1,"t":5},"speaker":"A","text":"大学の友達が結婚することになりました。"},{"id":{"d":616,"n":2,"t":5},"speaker":"B","text":"それはおめでたいですね。"},{"id":{"d":616,"n":3,"t":5},"speaker":"A","text":"披露宴に招待されたのですが、何を着て行けばよいかが分かりません。"},{"id":{"d":616,"n":4,"t":5},"speaker":"B","text":"式の雰囲気にもよりますね。会場はどのようなところなのですか？"},{"id":{"d":616,"n":5,"t":5},"speaker":"A","text":"有名な、格式の高いホテルのようです。"},{"id":{"d":616,"n":6,"t":5},"speaker":"B","text":"それでは、正装がいいでしょうね。"},{"id":{"d":616,"n":7,"t":5},"speaker":"A","text":"なるほど。手持ちの服にはふさわしいものがないので、買うことになりそうです。"},{"id":{"d":616,"n":8,"t":5},"speaker":"B","text":"招待されている他のお友達に、どのような格好で参加するかを聞いてみてはどうでしょうか？"},{"id":{"d":616,"n":9,"t":5},"speaker":"A","text":"そうですね。試しに聞いてみます。"}];
INSERT INTO sentence [{"id":{"d":617,"n":1,"t":5},"speaker":"A","text":"はい、「居酒屋いろは」です。"},{"id":{"d":617,"n":2,"t":5},"speaker":"B","text":"もしもし、予約をお願いしたいのですが。"},{"id":{"d":617,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。お日にちはいつですか？"},{"id":{"d":617,"n":4,"t":5},"speaker":"B","text":"12月28日です。"},{"id":{"d":617,"n":5,"t":5},"speaker":"A","text":"忘年会などでしょうか？"},{"id":{"d":617,"n":6,"t":5},"speaker":"B","text":"はい。会社の忘年会です。人数が多くて、23名なのですが大丈夫でしょうか？"},{"id":{"d":617,"n":7,"t":5},"speaker":"A","text":"大丈夫です。お時間は何時からでしょうか？"},{"id":{"d":617,"n":8,"t":5},"speaker":"B","text":"19時からでお願いします。"}];
INSERT INTO sentence [{"id":{"d":618,"n":1,"t":5},"speaker":"A","text":"このドラマは面白いですね。"},{"id":{"d":618,"n":2,"t":5},"speaker":"B","text":"原作の漫画も面白いですよ。"},{"id":{"d":618,"n":3,"t":5},"speaker":"A","text":"原作があるのですか？"},{"id":{"d":618,"n":4,"t":5},"speaker":"B","text":"はい。私は原作の漫画の大ファンです。"},{"id":{"d":618,"n":5,"t":5},"speaker":"A","text":"原作の漫画を持っていますか？"},{"id":{"d":618,"n":6,"t":5},"speaker":"B","text":"はい。全巻持っています。"},{"id":{"d":618,"n":7,"t":5},"speaker":"A","text":"もしよければ、今度、貸していただけませんか？"},{"id":{"d":618,"n":8,"t":5},"speaker":"B","text":"よいですよ。あなたも原作を好きになってくれたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":619,"n":1,"t":5},"speaker":"A","text":"すてきな日傘ですね。"},{"id":{"d":619,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。母の日のプレゼントとして娘がくれました。"},{"id":{"d":619,"n":3,"t":5},"speaker":"A","text":"優しい娘さんですね。"},{"id":{"d":619,"n":4,"t":5},"speaker":"B","text":"遠くに住んでいてなかなか会えない分、母の日や私の誕生日などを気にかけてくれています。"},{"id":{"d":619,"n":5,"t":5},"speaker":"A","text":"お仕事の都合で遠くに住んでいるのですか？"},{"id":{"d":619,"n":6,"t":5},"speaker":"B","text":"いいえ、結婚した相手が地方の人で、その人の地元で暮らしているのです。"},{"id":{"d":619,"n":7,"t":5},"speaker":"A","text":"それでは、頻繁には会えませんね。"},{"id":{"d":619,"n":8,"t":5},"speaker":"B","text":"はい。この日傘も宅配便で送ってくれました。"}];
INSERT INTO sentence [{"id":{"d":620,"n":1,"t":5},"speaker":"A","text":"このスカートは着物をリメークしたものです。"},{"id":{"d":620,"n":2,"t":5},"speaker":"B","text":"あなたがリメークしたのですか？"},{"id":{"d":620,"n":3,"t":5},"speaker":"A","text":"はい。裁縫が好きなので、趣味の一環です。"},{"id":{"d":620,"n":4,"t":5},"speaker":"B","text":"とてもすてきですよ。あなたの着物を使ったのですか？"},{"id":{"d":620,"n":5,"t":5},"speaker":"A","text":"いいえ。母が昔着ていた着物です。"},{"id":{"d":620,"n":6,"t":5},"speaker":"B","text":"取っておいてくれたなんて、お母様は物持ちが良いですね。"},{"id":{"d":620,"n":7,"t":5},"speaker":"A","text":"はい。保存状態が良かったようできれいなままでした。"},{"id":{"d":620,"n":8,"t":5},"speaker":"B","text":"スカート以外にもリメークできそうですね。"}];
INSERT INTO sentence [{"id":{"d":621,"n":1,"t":5},"speaker":"A","text":"お母さん、これは何ですか？"},{"id":{"d":621,"n":2,"t":5},"speaker":"B","text":"これは、千歳あめです。"},{"id":{"d":621,"n":3,"t":5},"speaker":"A","text":"あめなのですね。"},{"id":{"d":621,"n":4,"t":5},"speaker":"B","text":"そうです。おいしいですよ。"},{"id":{"d":621,"n":5,"t":5},"speaker":"A","text":"随分と長いあめですね。"},{"id":{"d":621,"n":6,"t":5},"speaker":"B","text":"子どもが長く生きられるようにという願いが込められているそうです。"},{"id":{"d":621,"n":7,"t":5},"speaker":"A","text":"そうなのですか。千歳あめは、七五三の時にしか見ませんね。"},{"id":{"d":621,"n":8,"t":5},"speaker":"B","text":"千歳あめは、七五三の行事に結び付いたものですからね。"}];
INSERT INTO sentence [{"id":{"d":622,"n":1,"t":5},"speaker":"A","text":"ハロウィーンが近いですね。"},{"id":{"d":622,"n":2,"t":5},"speaker":"B","text":"そうですね。ハロウィーンパーティーをやりましょうか？"},{"id":{"d":622,"n":3,"t":5},"speaker":"A","text":"良いですね。どのようなパーティーにしますか？"},{"id":{"d":622,"n":4,"t":5},"speaker":"B","text":"ハロウィーンなので、仮装して集まりしょう。"},{"id":{"d":622,"n":5,"t":5},"speaker":"A","text":"そうですね。そのときに、ハロウィーンらしいお菓子を持ち寄るのはどうですか？"},{"id":{"d":622,"n":6,"t":5},"speaker":"B","text":"ハロウィーンらしいというと、カボチャを使ったお菓子などでしょうか？"},{"id":{"d":622,"n":7,"t":5},"speaker":"A","text":"はい。あとはパッケージがハロウィーン仕様のお菓子などもいいですね。"}];
INSERT INTO sentence [{"id":{"d":623,"n":1,"t":5},"speaker":"A","text":"お母さん、お菓子を買ってください。"},{"id":{"d":623,"n":2,"t":5},"speaker":"B","text":"全部はいけません。どれか一つだけならよいですよ。"},{"id":{"d":623,"n":3,"t":5},"speaker":"A","text":"チョコレートが食べたいのですが、このクッキーもおいしそうです。"},{"id":{"d":623,"n":4,"t":5},"speaker":"B","text":"どちらかを選んでください。"},{"id":{"d":623,"n":5,"t":5},"speaker":"A","text":"どちらも食べたいので、選ぶことができません。"},{"id":{"d":623,"n":6,"t":5},"speaker":"B","text":"両方は買いません。今日はどちらかにして、もう一つはまた今度にしましょう。"},{"id":{"d":623,"n":7,"t":5},"speaker":"A","text":"それでは、今日はクッキーにします。"},{"id":{"d":623,"n":8,"t":5},"speaker":"B","text":"分かりました。チョコレートは棚に戻してきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":624,"n":1,"t":5},"speaker":"A","text":"お母さん、一緒に折り紙で遊びましょう。"},{"id":{"d":624,"n":2,"t":5},"speaker":"B","text":"よいですよ。何を折りますか？"},{"id":{"d":624,"n":3,"t":5},"speaker":"A","text":"僕はお花を折ります。お母さんは好きなものを折ってください。"},{"id":{"d":624,"n":4,"t":5},"speaker":"B","text":"何を折ろうか迷います。"},{"id":{"d":624,"n":5,"t":5},"speaker":"A","text":"ここに折り紙の本がありますよ。"},{"id":{"d":624,"n":6,"t":5},"speaker":"B","text":"少し見せてください。このパンダがかわいいので、私はこれを折ります。"},{"id":{"d":624,"n":7,"t":5},"speaker":"A","text":"難しそうですね。折れるのですか？"},{"id":{"d":624,"n":8,"t":5},"speaker":"B","text":"折り方に従って挑戦してみます。"}];
INSERT INTO sentence [{"id":{"d":625,"n":1,"t":5},"speaker":"A","text":"気分転換にプールで泳いできます。"},{"id":{"d":625,"n":2,"t":5},"speaker":"B","text":"どこのプールに行くのですか？"},{"id":{"d":625,"n":3,"t":5},"speaker":"A","text":"市民プールです。"},{"id":{"d":625,"n":4,"t":5},"speaker":"B","text":"市民プールは、今日は臨時のお休みですよ。"},{"id":{"d":625,"n":5,"t":5},"speaker":"A","text":"そうでしたか。"},{"id":{"d":625,"n":6,"t":5},"speaker":"B","text":"市の広報にお知らせが出ていました。"},{"id":{"d":625,"n":7,"t":5},"speaker":"A","text":"それでは隣町の運動公園にあるプールに行くことにします。"},{"id":{"d":625,"n":8,"t":5},"speaker":"B","text":"行ってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":626,"n":1,"t":5},"speaker":"A","text":"お母さんは、今年で還暦ですね。"},{"id":{"d":626,"n":2,"t":5},"speaker":"B","text":"はい、そうですね。あまり実感はありませんが。"},{"id":{"d":626,"n":3,"t":5},"speaker":"A","text":"還暦のお祝いにプレゼントを贈りたいのですが、何か欲しいものはありますか？"},{"id":{"d":626,"n":4,"t":5},"speaker":"B","text":"気を使ってくれてありがとうございます。老眼鏡が合わなくなってきたので、新しいものが欲しいです。"},{"id":{"d":626,"n":5,"t":5},"speaker":"A","text":"分かりました。では、今度の休みに一緒に買いに行きましょう。"},{"id":{"d":626,"n":6,"t":5},"speaker":"B","text":"うれしいです。楽しみにしていますね。"}];
INSERT INTO sentence [{"id":{"d":627,"n":1,"t":5},"speaker":"A","text":"ここでは何ができるのですか？"},{"id":{"d":627,"n":2,"t":5},"speaker":"B","text":"この牧場で採れた牛乳から、バターもしくはヨーグルトを手作りすることができます。"},{"id":{"d":627,"n":3,"t":5},"speaker":"A","text":"どちらか一つを選ぶのですね？"},{"id":{"d":627,"n":4,"t":5},"speaker":"B","text":"はい。お好きな方を体験できます。"},{"id":{"d":627,"n":5,"t":5},"speaker":"A","text":"時間がかからないのはどちらですか？"},{"id":{"d":627,"n":6,"t":5},"speaker":"B","text":"ヨーグルトの方が短い時間でできます。"},{"id":{"d":627,"n":7,"t":5},"speaker":"A","text":"それではヨーグルトでお願いします。"},{"id":{"d":627,"n":8,"t":5},"speaker":"B","text":"15分後に体験コースが始まりますので、あちらの作業場にお集まりください。"}];
INSERT INTO sentence [{"id":{"d":628,"n":1,"t":5},"speaker":"A","text":"お茶をどうぞ。"},{"id":{"d":628,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。かわいい湯飲みですね。"},{"id":{"d":628,"n":3,"t":5},"speaker":"A","text":"私が作った湯飲みです。"},{"id":{"d":628,"n":4,"t":5},"speaker":"B","text":"本当ですか？とてもよく出来ていて、売り物にしか見えません。"},{"id":{"d":628,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。陶芸が趣味なのです。"},{"id":{"d":628,"n":6,"t":5},"speaker":"B","text":"絵もあなたが描いたのですか？"},{"id":{"d":628,"n":7,"t":5},"speaker":"A","text":"はい。子どもの頃から絵が好きなのです。"},{"id":{"d":628,"n":8,"t":5},"speaker":"B","text":"とてもすてきです。陶器のお店を開けますよ。"},{"id":{"d":628,"n":9,"t":5},"speaker":"A","text":"いえいえ。まだ勉強中です。"}];
INSERT INTO sentence [{"id":{"d":629,"n":1,"t":5},"speaker":"A","text":"今日の午後は友達と遊びます。"},{"id":{"d":629,"n":2,"t":5},"speaker":"B","text":"外で遊ぶつもりなら、天気を確認した方がいいかもしれません。雲行きが怪しいですよ。"},{"id":{"d":629,"n":3,"t":5},"speaker":"A","text":"本当ですね。雨が降るでしょうか？"},{"id":{"d":629,"n":4,"t":5},"speaker":"B","text":"分かりませんが、一応傘を持って行く方が安心ですね。"},{"id":{"d":629,"n":5,"t":5},"speaker":"A","text":"荷物になるので、折り畳み傘を持って行きます。"},{"id":{"d":629,"n":6,"t":5},"speaker":"B","text":"私の折り畳み傘はとても小さくて軽いですよ。使いますか？"},{"id":{"d":629,"n":7,"t":5},"speaker":"A","text":"お借りしてもよいですか？"},{"id":{"d":629,"n":8,"t":5},"speaker":"B","text":"はい。ここにありますので、どうぞ。"}];
INSERT INTO sentence [{"id":{"d":630,"n":1,"t":5},"speaker":"A","text":"大好きなアイドルグループが解散することになってしまいました。"},{"id":{"d":630,"n":2,"t":5},"speaker":"B","text":"それはショックですね。"},{"id":{"d":630,"n":3,"t":5},"speaker":"A","text":"はい。突然のことでまだ受け入れることができません。"},{"id":{"d":630,"n":4,"t":5},"speaker":"B","text":"解散の理由は何ですか？"},{"id":{"d":630,"n":5,"t":5},"speaker":"A","text":"メンバーの方向性の違いだといわれているのですが、具体的には分かりません。"},{"id":{"d":630,"n":6,"t":5},"speaker":"B","text":"そうですか。もし解散コンサートがあるのなら、チケットが取れるといいですね。"},{"id":{"d":630,"n":7,"t":5},"speaker":"A","text":"はい。何としてでもチケットを取って行きたいです。"},{"id":{"d":630,"n":8,"t":5},"speaker":"B","text":"解散コンサートがあるかどうかはなだ分からないのですか？"},{"id":{"d":630,"n":9,"t":5},"speaker":"A","text":"はい。今のところ発表はありませんが、ファンはみんな期待しています。"}];
INSERT INTO sentence [{"id":{"d":631,"n":1,"t":5},"speaker":"A","text":"学園祭のライブへ出演するバンドを募集しているようですよ。"},{"id":{"d":631,"n":2,"t":5},"speaker":"B","text":"私も募集のポスターを見ました。"},{"id":{"d":631,"n":3,"t":5},"speaker":"A","text":"私とバンドを組んで、一緒に出演しませんか？"},{"id":{"d":631,"n":4,"t":5},"speaker":"B","text":"その場合、演奏する曲はどうするのですか？"},{"id":{"d":631,"n":5,"t":5},"speaker":"A","text":"有名な曲をコピーしましょう。"},{"id":{"d":631,"n":6,"t":5},"speaker":"B","text":"どちらがボーカルをやりますか？"},{"id":{"d":631,"n":7,"t":5},"speaker":"A","text":"2人でギターとボーカルを両方するのはどうですか？"},{"id":{"d":631,"n":8,"t":5},"speaker":"B","text":"良い案です。早速、曲を決めて練習しましょう。"}];
INSERT INTO sentence [{"id":{"d":632,"n":1,"t":5},"speaker":"A","text":"子ども会からお知らせが届きました。"},{"id":{"d":632,"n":2,"t":5},"speaker":"B","text":"見せてください。餅つき大会があるそうですよ。"},{"id":{"d":632,"n":3,"t":5},"speaker":"A","text":"いつですか？"},{"id":{"d":632,"n":4,"t":5},"speaker":"B","text":"1月5日です。"},{"id":{"d":632,"n":5,"t":5},"speaker":"A","text":"新年の行事として、餅つきをするのでしょうね。"},{"id":{"d":632,"n":6,"t":5},"speaker":"B","text":"ぜひ参加したいのですが、申し込み方法は書いてありますか？。"},{"id":{"d":632,"n":7,"t":5},"speaker":"A","text":"申し込み用紙に記入して会長さんに提出するそうです。"},{"id":{"d":632,"n":8,"t":5},"speaker":"B","text":"分かりました。忘れずに提出します。"}];
INSERT INTO sentence [{"id":{"d":633,"n":1,"t":5},"speaker":"A","text":"週末にアイスショーを見に行きました。"},{"id":{"d":633,"n":2,"t":5},"speaker":"B","text":"誰と行ったのですか？"},{"id":{"d":633,"n":3,"t":5},"speaker":"A","text":"娘と息子です。"},{"id":{"d":633,"n":4,"t":5},"speaker":"B","text":"お子さんたちはおいくつですか？"},{"id":{"d":633,"n":5,"t":5},"speaker":"A","text":"娘が8歳で息子が4歳です。"},{"id":{"d":633,"n":6,"t":5},"speaker":"B","text":"お子さんたちは喜んだでしょうね。"},{"id":{"d":633,"n":7,"t":5},"speaker":"A","text":"はい。特に娘は夢中になって見ていました。"},{"id":{"d":633,"n":8,"t":5},"speaker":"B","text":"楽しんでもらえて良かったですね。"}];
INSERT INTO sentence [{"id":{"d":634,"n":1,"t":5},"speaker":"A","text":"ただいま帰りました。これをどうぞ。"},{"id":{"d":634,"n":2,"t":5},"speaker":"B","text":"お帰りなさい。まあ、この花束はどうしたのですか？"},{"id":{"d":634,"n":3,"t":5},"speaker":"A","text":"あなたへのプレゼントです。"},{"id":{"d":634,"n":4,"t":5},"speaker":"B","text":"もしかして、結婚記念日の贈り物ですか？"},{"id":{"d":634,"n":5,"t":5},"speaker":"A","text":"そうです。今日でちょうど10周年です。"},{"id":{"d":634,"n":6,"t":5},"speaker":"B","text":"そうでした。覚えていてくれたのですね。"},{"id":{"d":634,"n":7,"t":5},"speaker":"A","text":"もちろんですよ。これからもよろしくお願いしますね。"},{"id":{"d":634,"n":8,"t":5},"speaker":"B","text":"こちらこそ、よろしくお願いします。私は何も用意していませんでした。ごめんなさい。"},{"id":{"d":634,"n":9,"t":5},"speaker":"A","text":"大丈夫ですよ。気にしないでください。"}];
INSERT INTO sentence [{"id":{"d":635,"n":1,"t":5},"speaker":"A","text":"クリスマスツリーのオーナメントが売っていますよ。"},{"id":{"d":635,"n":2,"t":5},"speaker":"B","text":"かわいいものがたくさんありますね。"},{"id":{"d":635,"n":3,"t":5},"speaker":"A","text":"せっかくなので何か買っていきましょうよ。"},{"id":{"d":635,"n":4,"t":5},"speaker":"B","text":"まだ持っていないものがいいですね。"},{"id":{"d":635,"n":5,"t":5},"speaker":"A","text":"そうですね。この星が良いなと思いましたが、星は既に持っていますよね。"},{"id":{"d":635,"n":6,"t":5},"speaker":"B","text":"はい。では、このトナカイはどうですか？"},{"id":{"d":635,"n":7,"t":5},"speaker":"A","text":"かわいいですね。でもトナカイも持っています。"},{"id":{"d":635,"n":8,"t":5},"speaker":"B","text":"そうでしたか。このリボンもすてきですよ。"},{"id":{"d":635,"n":9,"t":5},"speaker":"A","text":"本当ですね。リボンは持っていないので、これを買います。"}];
INSERT INTO sentence [{"id":{"d":636,"n":1,"t":5},"speaker":"A","text":"新しく出来たスポーツセンターでは、ビリヤードができますよ。"},{"id":{"d":636,"n":2,"t":5},"speaker":"B","text":"興味はあるのですが、ルールを知りません。"},{"id":{"d":636,"n":3,"t":5},"speaker":"A","text":"私は少しだけ分かるので、一緒にやってみましょうよ。"},{"id":{"d":636,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。まず、このキューの持ち方がよく分かりません。"},{"id":{"d":636,"n":5,"t":5},"speaker":"A","text":"私が構えるので、まねしてみてください。"},{"id":{"d":636,"n":6,"t":5},"speaker":"B","text":"こういう感じですか？"},{"id":{"d":636,"n":7,"t":5},"speaker":"A","text":"いいですね。それで大丈夫です。"},{"id":{"d":636,"n":8,"t":5},"speaker":"B","text":"試しにボールを突いてみたいです。"},{"id":{"d":636,"n":9,"t":5},"speaker":"A","text":"ここにあるので、どうぞ。"}];
INSERT INTO sentence [{"id":{"d":637,"n":1,"t":5},"speaker":"A","text":"今日はこのDVDを見なければいけません。"},{"id":{"d":637,"n":2,"t":5},"speaker":"B","text":"どうしてですか？"},{"id":{"d":637,"n":3,"t":5},"speaker":"A","text":"このDVDは、今日が返却期限なのです。"},{"id":{"d":637,"n":4,"t":5},"speaker":"B","text":"でも今日は大学の課題をたくさんしなくてはいけないとも言っていましたよね。"},{"id":{"d":637,"n":5,"t":5},"speaker":"A","text":"そうなのです。課題を終わらせてからDVDを見たら、寝る時間がなくなってしまいそうです。"},{"id":{"d":637,"n":6,"t":5},"speaker":"B","text":"徹夜は体に良くないのでやめた方がいいですよ。"},{"id":{"d":637,"n":7,"t":5},"speaker":"A","text":"はい。ですので、課題をなるべく早く終わらせたいと思います。"},{"id":{"d":637,"n":8,"t":5},"speaker":"B","text":"頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":638,"n":1,"t":5},"speaker":"A","text":"今日はたくさん歌いましたね。"},{"id":{"d":638,"n":2,"t":5},"speaker":"B","text":"そろそろ終わりの時間ですが、延長しますか？"},{"id":{"d":638,"n":3,"t":5},"speaker":"A","text":"もう2時間が経ったのですか。早いですね。"},{"id":{"d":638,"n":4,"t":5},"speaker":"B","text":"はい。カラオケボックスにいると、あっという間に時間が経ってしまいます。"},{"id":{"d":638,"n":5,"t":5},"speaker":"A","text":"歌い過ぎて喉がガラガラになってきたので、延長はやめておきます。"},{"id":{"d":638,"n":6,"t":5},"speaker":"B","text":"確かに、少し声がかれていますね。"},{"id":{"d":638,"n":7,"t":5},"speaker":"A","text":"喉が弱いのか、割とすぐに声がかれてしまうのです。"},{"id":{"d":638,"n":8,"t":5},"speaker":"B","text":"それでは延長はしないでおきましょう。"}];
INSERT INTO sentence [{"id":{"d":639,"n":1,"t":5},"speaker":"A","text":"この棚にあるのは全部あなたのレコードですか？"},{"id":{"d":639,"n":2,"t":5},"speaker":"B","text":"いいえ、私の父のものです。"},{"id":{"d":639,"n":3,"t":5},"speaker":"A","text":"すごい数ですね。"},{"id":{"d":639,"n":4,"t":5},"speaker":"B","text":"趣味で集めたようです。"},{"id":{"d":639,"n":5,"t":5},"speaker":"A","text":"手に取ってみてもよいですか？"},{"id":{"d":639,"n":6,"t":5},"speaker":"B","text":"どうぞ。遠慮なく見てください。"},{"id":{"d":639,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。私の好きな歌手のレコードがたくさんあります。"},{"id":{"d":639,"n":8,"t":5},"speaker":"B","text":"どれか一枚聴いてみますか？"},{"id":{"d":639,"n":9,"t":5},"speaker":"A","text":"よいのですか？うれしいです。"}];
INSERT INTO sentence [{"id":{"d":640,"n":1,"t":5},"speaker":"A","text":"昨日の夜に、怖い映画を見ました。"},{"id":{"d":640,"n":2,"t":5},"speaker":"B","text":"ホラー映画ですか？"},{"id":{"d":640,"n":3,"t":5},"speaker":"A","text":"いいえ。ホラーではないのですが、話の展開が全く読めなくて、終始驚かされるような作品でした。"},{"id":{"d":640,"n":4,"t":5},"speaker":"B","text":"なるほど、そういう怖さでしたか。それも確かに怖いですね。"},{"id":{"d":640,"n":5,"t":5},"speaker":"A","text":"おまけに、その映画を夜に一人で見ていたので、怖さは倍増でした。"},{"id":{"d":640,"n":6,"t":5},"speaker":"B","text":"結局、最後まで見たのですか？"},{"id":{"d":640,"n":7,"t":5},"speaker":"A","text":"はい。見たのですが、その後も一人でいるのが怖かったです。"},{"id":{"d":640,"n":8,"t":5},"speaker":"B","text":"あなたの話を聞いていたら、好奇心で見たくなってきました。映画のタイトルをを教えてください。"},{"id":{"d":640,"n":9,"t":5},"speaker":"A","text":"あとでメールで送りますね。映画を見たら、あなたの感想も聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":641,"n":1,"t":5},"speaker":"A","text":"お母さん、おはようございます。僕のところにもサンタさんが来てくれましたよ。"},{"id":{"d":641,"n":2,"t":5},"speaker":"B","text":"おはようございます。どうしてサンタさんが来たと分かったのですか？"},{"id":{"d":641,"n":3,"t":5},"speaker":"A","text":"僕の枕元にプレゼントがありました。"},{"id":{"d":641,"n":4,"t":5},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":641,"n":5,"t":5},"speaker":"A","text":"はい。このプレゼントは、サンタさんが夜中に置いていってくれたものだと思います。"},{"id":{"d":641,"n":6,"t":5},"speaker":"B","text":"きっとそうですね。"},{"id":{"d":641,"n":7,"t":5},"speaker":"A","text":"サンタさんが本当に来てくれたなんて、まるで夢のようです。"},{"id":{"d":641,"n":8,"t":5},"speaker":"B","text":"プレゼントを開けてみたらどうですか？"},{"id":{"d":641,"n":9,"t":5},"speaker":"A","text":"はい、そうします。僕がお願いしたゲームソフトだとうれしいです。"}];
INSERT INTO sentence [{"id":{"d":642,"n":1,"t":5},"speaker":"A","text":"お正月といえば、お雑煮ですね。"},{"id":{"d":642,"n":2,"t":5},"speaker":"B","text":"ご実家ではどのようなお雑煮を食べていましたか？"},{"id":{"d":642,"n":3,"t":5},"speaker":"A","text":"丸餅で、白みその汁でした。"},{"id":{"d":642,"n":4,"t":5},"speaker":"B","text":"具材はどのようなものが入っていましたか？"},{"id":{"d":642,"n":5,"t":5},"speaker":"A","text":"サトイモやニンジンです。"},{"id":{"d":642,"n":6,"t":5},"speaker":"B","text":"もしかして、ご実家は京都ですか？"},{"id":{"d":642,"n":7,"t":5},"speaker":"A","text":"その通りです。よく分かりましたね。"},{"id":{"d":642,"n":8,"t":5},"speaker":"B","text":"京都出身の友達が似たようなお雑煮を食べていたと話していたことがあるのです。"},{"id":{"d":642,"n":9,"t":5},"speaker":"A","text":"お雑煮には地域性が表れるのですね。"}];
INSERT INTO sentence [{"id":{"d":643,"n":1,"t":5},"speaker":"A","text":"里田さん、これはホワイトデーのお返しです。"},{"id":{"d":643,"n":2,"t":5},"speaker":"B","text":"わざわざ用意してくださったのですか？ありがとうございます。"},{"id":{"d":643,"n":3,"t":5},"speaker":"A","text":"実は、何が良いのかが分からなかったので、妻に頼んで買ってきてもらいました。"},{"id":{"d":643,"n":4,"t":5},"speaker":"B","text":"気を使わせてしまってすみません。"},{"id":{"d":643,"n":5,"t":5},"speaker":"A","text":"中身は私もよく知らないのですが、最近人気のあるお菓子だそうです。"},{"id":{"d":643,"n":6,"t":5},"speaker":"B","text":"そうですか。とてもうれしいです。"},{"id":{"d":643,"n":7,"t":5},"speaker":"A","text":"あなたの好みに合うかどうかは分かりませんが、妻はおいしいと言っていましたよ。"},{"id":{"d":643,"n":8,"t":5},"speaker":"B","text":"甘いものは大好きなので、きっと気に入ると思います。奥様にもお礼をお伝えください。"}];
INSERT INTO sentence [{"id":{"d":644,"n":1,"t":5},"speaker":"A","text":"次はウォータースライダーを滑りましょう。"},{"id":{"d":644,"n":2,"t":5},"speaker":"B","text":"少し怖いので、私はここで待っています。"},{"id":{"d":644,"n":3,"t":5},"speaker":"A","text":"滑ったことはありますか？"},{"id":{"d":644,"n":4,"t":5},"speaker":"B","text":"前に一度だけ体験しましたが、それがとても怖かったのです。"},{"id":{"d":644,"n":5,"t":5},"speaker":"A","text":"そうですか。でも、ここのウォータースライダーは小さい子も楽しんでいるのできっと大丈夫ですよ。"},{"id":{"d":644,"n":6,"t":5},"speaker":"B","text":"確かに、小学校低学年くらいの子も楽しんでいるようです。"},{"id":{"d":644,"n":7,"t":5},"speaker":"A","text":"2人で一緒に滑る人もいるようなので、私たちもそうしましょう。"},{"id":{"d":644,"n":8,"t":5},"speaker":"B","text":"それなら大丈夫そうです。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":645,"n":1,"t":5},"speaker":"A","text":"靴をお探しですか？"},{"id":{"d":645,"n":2,"t":5},"speaker":"B","text":"はい。パンプスを探しています。"},{"id":{"d":645,"n":3,"t":5},"speaker":"A","text":"ヒールの高さはどのくらいがよろしいでしょうか？"},{"id":{"d":645,"n":4,"t":5},"speaker":"B","text":"ヒールは高くないものがいいです。3センチメートルくらいのものはありますか？"},{"id":{"d":645,"n":5,"t":5},"speaker":"A","text":"はい。こちらにはヒールが3センチメートル以下のパンプスが並んでいます。"},{"id":{"d":645,"n":6,"t":5},"speaker":"B","text":"どのような服でも合わせやすい色はどれでしょうか？"},{"id":{"d":645,"n":7,"t":5},"speaker":"A","text":"やはり、白や黒は合わせやすいですね。"},{"id":{"d":645,"n":8,"t":5},"speaker":"B","text":"そうですよね。試しに、これを履いてみてもよいでしょうか？"},{"id":{"d":645,"n":9,"t":5},"speaker":"A","text":"どうぞ。サイズはおいくつですか？"},{"id":{"d":645,"n":10,"t":5},"speaker":"B","text":"23センチメートルです。"}];
INSERT INTO sentence [{"id":{"d":646,"n":1,"t":5},"speaker":"A","text":"せっかく初詣に来たので、おみくじを引きましょう。"},{"id":{"d":646,"n":2,"t":5},"speaker":"B","text":"そうしましょう。あなたは何を引きましたか？"},{"id":{"d":646,"n":3,"t":5},"speaker":"A","text":"末吉でした。あまり良いことが書いてありません。"},{"id":{"d":646,"n":4,"t":5},"speaker":"B","text":"残念でしたね。私は大吉です。"},{"id":{"d":646,"n":5,"t":5},"speaker":"A","text":"よかったですね。何と書いてありますか？"},{"id":{"d":646,"n":6,"t":5},"speaker":"B","text":"学業成就とあります。"},{"id":{"d":646,"n":7,"t":5},"speaker":"A","text":"あなたは今年、受験があるので幸先がいいですね。"},{"id":{"d":646,"n":8,"t":5},"speaker":"B","text":"はい、うれしいです。合格できるような気がしてきました。"}];
INSERT INTO sentence [{"id":{"d":647,"n":1,"t":5},"speaker":"A","text":"今日は保育園で七夕の飾り付けをしました。"},{"id":{"d":647,"n":2,"t":5},"speaker":"B","text":"ササに飾りを付けたということですか？"},{"id":{"d":647,"n":3,"t":5},"speaker":"A","text":"はい。願い事を書いた短冊も飾りました。"},{"id":{"d":647,"n":4,"t":5},"speaker":"B","text":"あなたはどのような願い事を書きましたか？"},{"id":{"d":647,"n":5,"t":5},"speaker":"A","text":"「ピーマンが食べられるようになりたい」と書きました。"},{"id":{"d":647,"n":6,"t":5},"speaker":"B","text":"嫌いな食べ物を克服したいのですね。"},{"id":{"d":647,"n":7,"t":5},"speaker":"A","text":"はい。小学生になったら給食にピーマンが出るかもしれないので、それまでに食べられるようになりたいです。"},{"id":{"d":647,"n":8,"t":5},"speaker":"B","text":"きっと食べられるようになると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":648,"n":1,"t":5},"speaker":"A","text":"ビスク・ドールとは何ですか？"},{"id":{"d":648,"n":2,"t":5},"speaker":"B","text":"ビスクは磁器のことですから、直訳すると磁器人形のことですね。"},{"id":{"d":648,"n":3,"t":5},"speaker":"A","text":"もう少し詳しく教えていただけますか？"},{"id":{"d":648,"n":4,"t":5},"speaker":"B","text":"19世紀頃に、欧州の婦人たちに流行した磁器人形のことです。"},{"id":{"d":648,"n":5,"t":5},"speaker":"A","text":"それはアンティーク・ドールとは違うのですか？"},{"id":{"d":648,"n":6,"t":5},"speaker":"B","text":"アンティーク・ドールは価値のある人形に対して使われる名称で、必ずしも磁器製とは限りません。"},{"id":{"d":648,"n":7,"t":5},"speaker":"A","text":"アンティークという言葉の意味が骨董品であることを忘れていました。ビスク・ドールと聞くと、私はどうしてもホラー映画に出てくる古びた人形を思い浮かべてしまうので、アンティーク・ドールと混同してしまうのかもしれません。"},{"id":{"d":648,"n":8,"t":5},"speaker":"B","text":"ビスク・ドールからホラー映画を連想するのは、私も同意です。"}];
INSERT INTO sentence [{"id":{"d":649,"n":1,"t":5},"speaker":"A","text":"最近の天気をどう思いますか？"},{"id":{"d":649,"n":2,"t":5},"speaker":"B","text":"一言で言うと、安定していませんね。"},{"id":{"d":649,"n":3,"t":5},"speaker":"A","text":"一日ごとに最高気温が10度以上も違いますからね。"},{"id":{"d":649,"n":4,"t":5},"speaker":"B","text":"昨日と今日でこれだけ温度が違うと、季節感はなくなってしまいます。"},{"id":{"d":649,"n":5,"t":5},"speaker":"A","text":"そうですね。例年の今ごろの気候がどのような感じだったのか、分からなくなっています。"},{"id":{"d":649,"n":6,"t":5},"speaker":"B","text":"昨日は長袖シャツを着て、今日はアロハシャツを着るような天気では、うかつに衣替えもできなくて困っています。"},{"id":{"d":649,"n":7,"t":5},"speaker":"A","text":"あらゆる季節に対応できる服を手元に置いておけるのならいいのでしょうけど、それでは収納がいくつあっても足りませんね。"}];
INSERT INTO sentence [{"id":{"d":650,"n":1,"t":5},"speaker":"A","text":"お彼岸はいつですか？"},{"id":{"d":650,"n":2,"t":5},"speaker":"B","text":"春と秋の2回あります。"},{"id":{"d":650,"n":3,"t":5},"speaker":"A","text":"春分の日と秋分の日のころでしょうか？"},{"id":{"d":650,"n":4,"t":5},"speaker":"B","text":"春分の日と秋分の日の前後3日をお彼岸と言うようですよ。この期間は、太陽が真東から昇って真西に沈むのだそうです。"},{"id":{"d":650,"n":5,"t":5},"speaker":"A","text":"前後3日ということは、お彼岸はちょうど1週間なのですね。その期間にお墓参りに行くのはなぜですか？"},{"id":{"d":650,"n":6,"t":5},"speaker":"B","text":"この世とあの世が通じやすくなる時期だからです。仏教ではあの世は西にあり、この世は東にあるといわれています。彼岸とはあの世のことなので、太陽が真東から昇って真西に沈む春分と秋分の時期は、この世とあの世が最も通じやすくなると考えられています。"},{"id":{"d":650,"n":7,"t":5},"speaker":"A","text":"それで、お彼岸にご先祖を供養するようになったわけですね。"}];
INSERT INTO sentence [{"id":{"d":651,"n":1,"t":5},"speaker":"A","text":"それは昔のカメラですか？"},{"id":{"d":651,"n":2,"t":5},"speaker":"B","text":"よくお気付きになりましたね。これはフィルム用のカメラです。"},{"id":{"d":651,"n":3,"t":5},"speaker":"A","text":"最近は全く見なくなりました。フィルムの現像はどうされるのですか？"},{"id":{"d":651,"n":4,"t":5},"speaker":"B","text":"数は少なくなりましたが、現像をお願いできる店はまだあります。郵送で現像を受け付けているところもあります。"},{"id":{"d":651,"n":5,"t":5},"speaker":"A","text":"そうなのですね。それは、思い入れのあるカメラなのですか？"},{"id":{"d":651,"n":6,"t":5},"speaker":"B","text":"はい。これは、父の形見のカメラです。父はアマチュアカメラマンだったので、高価な機器が実家に残っていました。"},{"id":{"d":651,"n":7,"t":5},"speaker":"A","text":"すてきですね。私もフィルム用のカメラで撮影してみたいのですが、難しいでしょうか？"},{"id":{"d":651,"n":8,"t":5},"speaker":"B","text":"昔は誰もがフィルム用カメラで撮影したのですから、それほど難しくはありませんよ。"},{"id":{"d":651,"n":9,"t":5},"speaker":"A","text":"撮った写真をインスタグラムやフェイスブックに載せることはできますか？"},{"id":{"d":651,"n":10,"t":5},"speaker":"B","text":"今はフィルムをデータ化することも可能ですので、手間はかかるかもしれませんが、載せることはできると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":652,"n":1,"t":5},"speaker":"A","text":"高校生の娘がエレキギターを弾きたいと言っています。アコースティックギターとどう違うのでしょうか？"},{"id":{"d":652,"n":2,"t":5},"speaker":"B","text":"アコースティックギターはそのままでも音が出ますが、エレキギターはアンプを通さないと音が小さすぎて聞こえません。音質もかなり違うと思いますよ。"},{"id":{"d":652,"n":3,"t":5},"speaker":"A","text":"値段の違いもありますか？"},{"id":{"d":652,"n":4,"t":5},"speaker":"B","text":"一概に値段を比較することはできません。どちらのギターも、数万円のものもあれば、100万円を超えるものまであります。"},{"id":{"d":652,"n":5,"t":5},"speaker":"A","text":"弾き方に違いはあるのですか？"},{"id":{"d":652,"n":6,"t":5},"speaker":"B","text":"基本的には同じですが、エレキギターは弦が細いので弾きやすいです。エフェクターという器具で音を変えることもできます。"},{"id":{"d":652,"n":7,"t":5},"speaker":"A","text":"なるほど。勉強になります。佐々木さんはギターに詳しいのですね。"},{"id":{"d":652,"n":8,"t":5},"speaker":"B","text":"実は、私は趣味がギターなのです。お役に立てたのならよかったです。また何かあればいつでも聞いてくださいね。"},{"id":{"d":652,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。お言葉に甘えて、早速お願いがあるのですが、私たちがギターを買いに行くときに、佐々木さんに付き添ってもらえないでしょうか？"},{"id":{"d":652,"n":10,"t":5},"speaker":"B","text":"もちろんよいですよ。喜んでお供しましょう。"}];
INSERT INTO sentence [{"id":{"d":653,"n":1,"t":5},"speaker":"A","text":"オルガンとピアノはどちらも鍵盤楽器ですが、どのような違いがあるでしょうか？例えば、ピアノはふたを開ければピアノ線が見えますが、オルガンにもピアノ線のようなものはあるのでしょうか？"},{"id":{"d":653,"n":2,"t":5},"speaker":"B","text":"オルガンは空気をパイプに通すことで音を鳴らす楽器なので、ピアノ線はありません。"},{"id":{"d":653,"n":3,"t":5},"speaker":"A","text":"オルガンの鍵盤とピアノの鍵盤は、演奏方法も違うものですか？"},{"id":{"d":653,"n":4,"t":5},"speaker":"B","text":"ピアノは鍵盤をたたいた後も音が共鳴しますが、オルガンの鍵盤では指を離すと音は出なくなります。"},{"id":{"d":653,"n":5,"t":5},"speaker":"A","text":"そうなのですね。オルガンは鍵盤ハーモニカやアコーディオンに近い印象を受けました。"},{"id":{"d":653,"n":6,"t":5},"speaker":"B","text":"空気を送って音を出す点では、オルガンと鍵盤ハーモニカやアコーディオンは同じ仕組みですね。"},{"id":{"d":653,"n":7,"t":5},"speaker":"A","text":"よく分かりました。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":654,"n":1,"t":5},"speaker":"A","text":"ボサノバとは何ですか？"},{"id":{"d":654,"n":2,"t":5},"speaker":"B","text":"ブラジルで作られた音楽と言われています。有名な曲には『イパネマの娘』などがあります。"},{"id":{"d":654,"n":3,"t":5},"speaker":"A","text":"その曲は聞いたことがあります。ボサノバはブラジルの伝統的な音楽なのですか？"},{"id":{"d":654,"n":4,"t":5},"speaker":"B","text":"伝統音楽というよりは、特定の時代に流行した比較的新しい音楽です。1950年代に、アントニオ・カルロス・ジョビンがジャズの影響を受けて作ったものといわれています。"},{"id":{"d":654,"n":5,"t":5},"speaker":"A","text":"『イパネマの娘』の他にも有名なボサノバの曲はありますか？"},{"id":{"d":654,"n":6,"t":5},"speaker":"B","text":"カフェなどのBGMとしてよく使われていますので、どこかで耳にしたことのある曲は多いはずですよ。今度何枚かCDをお貸ししましょう。"},{"id":{"d":654,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":655,"n":1,"t":5},"speaker":"A","text":"スカとはどのような音楽なのでしょうか？"},{"id":{"d":655,"n":2,"t":5},"speaker":"B","text":"ジャマイカ発祥のポピュラー音楽です。レゲエはご存知ですか？"},{"id":{"d":655,"n":3,"t":5},"speaker":"A","text":"はい、ボブ・マーリーが有名ですよね。彼の音楽を聴いていてスカを知ったのです。"},{"id":{"d":655,"n":4,"t":5},"speaker":"B","text":"レゲエより前にジャマイカで流行していた音楽がスカです。レゲエはスカを遅くしたものといわれています。"},{"id":{"d":655,"n":5,"t":5},"speaker":"A","text":"レゲエは英国などでも人気ですが、スカも同じく人気があるのですか？"},{"id":{"d":655,"n":6,"t":5},"speaker":"B","text":"そのようです。英国はカリブ海地域からの移民が多く、スカやレゲエが広まりやすかったのかもしれませんね。"},{"id":{"d":655,"n":7,"t":5},"speaker":"A","text":"日本にもスカを演奏するバンドがあるのですか？"},{"id":{"d":655,"n":8,"t":5},"speaker":"B","text":"はい、ありますよ。"}];
INSERT INTO sentence [{"id":{"d":656,"n":1,"t":5},"speaker":"A","text":"YouTubeを見ることはありますか？"},{"id":{"d":656,"n":2,"t":5},"speaker":"B","text":"よく見ていますよ。必ず見る動画チャンネルがいくつかあります。"},{"id":{"d":656,"n":3,"t":5},"speaker":"A","text":"どのような動画チャンネルをよく見るのですか？"},{"id":{"d":656,"n":4,"t":5},"speaker":"B","text":"昔の名曲をカバー演奏したものを定期的にYouTubeにアップロードしている人がいて、その人のチャンネルをよく見ています。"},{"id":{"d":656,"n":5,"t":5},"speaker":"A","text":"アマチュアミュージシャンがカバー曲をアップロードしているのは知っていますが、彼らの演奏はどうですか？"},{"id":{"d":656,"n":6,"t":5},"speaker":"B","text":"驚くほど演奏が上手な人もたくさんいます。私が音楽レーベルのプロデューサーならば、すぐにでも契約したいくらいです。"},{"id":{"d":656,"n":7,"t":5},"speaker":"A","text":"そのような人もいるとは驚きました。インターネットがあれば世界中の才能を知ることもできますね。"}];
INSERT INTO sentence [{"id":{"d":657,"n":1,"t":5},"speaker":"A","text":"あなたは、動画配信サービスを利用していますか？"},{"id":{"d":657,"n":2,"t":5},"speaker":"B","text":"この間テレビを買い替えた時に、動画配信サービスに加入しました。"},{"id":{"d":657,"n":3,"t":5},"speaker":"A","text":"どのようなテレビに買い替えたのですか？"},{"id":{"d":657,"n":4,"t":5},"speaker":"B","text":"思い切って70インチの大画面テレビにしました。でも、それが動画配信サービスに加入した動機というわけではありません。"},{"id":{"d":657,"n":5,"t":5},"speaker":"A","text":"他に動画配信サービスに加入した理由があるのですか？"},{"id":{"d":657,"n":6,"t":5},"speaker":"B","text":"インターネットとテレビを接続したので、インターネット経由の映像をテレビで見られるようになりました。それがきっかけで、動画配信サービスも利用するようになったのです。"},{"id":{"d":657,"n":7,"t":5},"speaker":"A","text":"普段はどのような動画を観賞しているのですか？"},{"id":{"d":657,"n":8,"t":5},"speaker":"B","text":"気になりつつも見逃してきた映画を見ることが多いです。休日の楽しみが増えました。"}];
INSERT INTO sentence [{"id":{"d":658,"n":1,"t":5},"speaker":"A","text":"定期的に書店に行っているそうですが、どのような書籍を購入するのですか？"},{"id":{"d":658,"n":2,"t":5},"speaker":"B","text":"よく購入するのはミステリー小説です。"},{"id":{"d":658,"n":3,"t":5},"speaker":"A","text":"日本の作家の本を購入するのですか？"},{"id":{"d":658,"n":4,"t":5},"speaker":"B","text":"いいえ。海外の作家の作品を買うことが多いです。"},{"id":{"d":658,"n":5,"t":5},"speaker":"A","text":"外国と日本のミステリーに何か違いはありますか？"},{"id":{"d":658,"n":6,"t":5},"speaker":"B","text":"個人的な感想になりますが、特に物語の舞台が現代の場合は、海外のミステリーの方が現実味があるような気がしています。"},{"id":{"d":658,"n":7,"t":5},"speaker":"A","text":"なるほど。確かに、映画などでも、最新の政治問題や社会情勢を反映した作品は、海外に多い印象があります。"},{"id":{"d":658,"n":8,"t":5},"speaker":"B","text":"あとは、使用される銃器や装備も日本と海外では随分違いますね。文化が違うので、当然といえば当然ですが。"},{"id":{"d":658,"n":9,"t":5},"speaker":"A","text":"日本は文化的にも歴史的にも特徴のある国なので、日本独自の雰囲気を持つ作品はたくさんありそうです。"}];
INSERT INTO sentence [{"id":{"d":659,"n":1,"t":5},"speaker":"A","text":"テレビのバラエティー番組はお好きですか？"},{"id":{"d":659,"n":2,"t":5},"speaker":"B","text":"特段好きではありませんが、よく見ますよ。夕食時に家族がテレビでバラエティー番組を見ているので、その時に一緒に見ています。"},{"id":{"d":659,"n":3,"t":5},"speaker":"A","text":"バラエティー番組を最初から最後まで見たりはしないのですか？"},{"id":{"d":659,"n":4,"t":5},"speaker":"B","text":"ありませんね。食事が終わったら、自分の部屋に帰って勉強やゲームをします。"},{"id":{"d":659,"n":5,"t":5},"speaker":"A","text":"お笑いタレントで好きな人はいますか？"},{"id":{"d":659,"n":6,"t":5},"speaker":"B","text":"特にいません。好きなアニメーションのキャラクターやVTuberはいます。"},{"id":{"d":659,"n":7,"t":5},"speaker":"A","text":"あなたの家では、バラエティー番組は主にどなたがご覧になっているのですか？"},{"id":{"d":659,"n":8,"t":5},"speaker":"B","text":"主に母です。母はお笑いタレントが好きですよ。"},{"id":{"d":659,"n":9,"t":5},"speaker":"A","text":"テレビのチャンネルを選ぶ権利は、お母さんがお持ちなのですね。"},{"id":{"d":659,"n":10,"t":5},"speaker":"B","text":"はい。母が好きな番組を見ていれば私はそれでよいのです。"}];
INSERT INTO sentence [{"id":{"d":660,"n":1,"t":5},"speaker":"A","text":"エイプリルフールにうそをついたことはありますか？"},{"id":{"d":660,"n":2,"t":5},"speaker":"B","text":"はい。子どもの頃は、友人や家族にうそをついていました。"},{"id":{"d":660,"n":3,"t":5},"speaker":"A","text":"いたずらが好きな子どもだったのですか？"},{"id":{"d":660,"n":4,"t":5},"speaker":"B","text":"そうかもしれません。うそに引っかかった人の反応を見ては、楽しんでいました。"},{"id":{"d":660,"n":5,"t":5},"speaker":"A","text":"親に怒られたりはしませんでしたか？"},{"id":{"d":660,"n":6,"t":5},"speaker":"B","text":"怒られた記憶はありませんが、友人や弟とはそのうそが原因でよくけんかをしていました。"},{"id":{"d":660,"n":7,"t":5},"speaker":"A","text":"同年代同士の方がいたずらに敏感に反応するのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":661,"n":1,"t":5},"speaker":"A","text":"あなたはどのようなジャンルの音楽が好きですか？"},{"id":{"d":661,"n":2,"t":5},"speaker":"B","text":"私は邦楽ロックが好きです。"},{"id":{"d":661,"n":3,"t":5},"speaker":"A","text":"洋楽よりも邦楽がお好きなのですね。"},{"id":{"d":661,"n":4,"t":5},"speaker":"B","text":"はい。邦楽のロックバンドの歌詞には文学的な表現を持つものが多いので、そこが好きなのです。"},{"id":{"d":661,"n":5,"t":5},"speaker":"A","text":"邦楽のロックバンドはバンド名も凝っていますね。"},{"id":{"d":661,"n":6,"t":5},"speaker":"B","text":"はい、そこも素晴らしいところです。邦楽ロックにはこのまま文学的な歌詞を書き続けてほしいです。"}];
INSERT INTO sentence [{"id":{"d":662,"n":1,"t":5},"speaker":"A","text":"最近は、AMラジオをFMでも聞くことができるようになったと聞きました。"},{"id":{"d":662,"n":2,"t":5},"speaker":"B","text":"知りませんでした。どちらでも聞けるのは便利ですね。"},{"id":{"d":662,"n":3,"t":5},"speaker":"A","text":"はい。これを機に、新しい聴取者も増える可能性がありますね。"},{"id":{"d":662,"n":4,"t":5},"speaker":"B","text":"高齢者や家で仕事をされている方は、日中にラジオを聞くことが多いようですね。"},{"id":{"d":662,"n":5,"t":5},"speaker":"A","text":"そうなのですね。私たちが若かった頃は、深夜にラジオを聞いている人が多かったですよね。"},{"id":{"d":662,"n":6,"t":5},"speaker":"B","text":"私もよく聞いたものです。ラジオは、受験生にとってなくてはならないものでしたね。"}];
INSERT INTO sentence [{"id":{"d":663,"n":1,"t":5},"speaker":"A","text":"ラジコを利用したことがありますか？"},{"id":{"d":663,"n":2,"t":5},"speaker":"B","text":"ラジコとは何ですか？"},{"id":{"d":663,"n":3,"t":5},"speaker":"A","text":"ラジコはパソコンやスマートフォンでラジオ放送を聞くことができるサービスです。"},{"id":{"d":663,"n":4,"t":5},"speaker":"B","text":"それは便利ですね。どこのラジオ局でも聞けるのですか？"},{"id":{"d":663,"n":5,"t":5},"speaker":"A","text":"地域のラジオ局なら無料で聞くことができます。有料会員になれば、全国各地のラジオ放送も聞くことができますよ。"},{"id":{"d":663,"n":6,"t":5},"speaker":"B","text":"居住地以外の地域の放送も聞けるのはうれしいですね。聞き逃した番組を後から聞くこともできるのですか？"},{"id":{"d":663,"n":7,"t":5},"speaker":"A","text":"はい。ラジコでは、放送後1週間は過去の番組をいつでも聞くことができます。"},{"id":{"d":663,"n":8,"t":5},"speaker":"B","text":"それは大変素晴らしいサービスですね。"}];
INSERT INTO sentence [{"id":{"d":664,"n":1,"t":5},"speaker":"A","text":"寄席に行ったことがありますか？"},{"id":{"d":664,"n":2,"t":5},"speaker":"B","text":"寄席とは落語や漫才を出し物にしているところのことですよね。私は行ったことがありません。"},{"id":{"d":664,"n":3,"t":5},"speaker":"A","text":"そうです。寄席の演目には、落語や漫才の他に奇術や曲芸、漫談やコントなどもあります。"},{"id":{"d":664,"n":4,"t":5},"speaker":"B","text":"寄席というのは全国にたくさんあるのでしょうか？"},{"id":{"d":664,"n":5,"t":5},"speaker":"A","text":"それほど多くはありません。定席といわれる大きな寄席は、国内に4つしかないと言われています。"},{"id":{"d":664,"n":6,"t":5},"speaker":"B","text":"吉本興行の劇場には何度か行ったことがありますが、寄席となるとハードルが高い気がします。"},{"id":{"d":664,"n":7,"t":5},"speaker":"A","text":"お笑いがお好きなのですか？東京で活動している漫才コンビなどは、寄席で見られることもありますよ。"},{"id":{"d":664,"n":8,"t":5},"speaker":"B","text":"お笑いは大好きです。寄席でお笑いを見られるとは知りませんでしたので、今まで寄席を経験してこなかったのは損していたかもしれません。今度連れて行ってくれませんか？"},{"id":{"d":664,"n":9,"t":5},"speaker":"A","text":"もちろんです。分からないところは何でもお聞きください。"}];
INSERT INTO sentence [{"id":{"d":665,"n":1,"t":5},"speaker":"A","text":"吉井さんは、町内会の盆踊りに参加されますか？"},{"id":{"d":665,"n":2,"t":5},"speaker":"B","text":"はい、久しぶりの盆踊りなので今から楽しみです。"},{"id":{"d":665,"n":3,"t":5},"speaker":"A","text":"今年は会場も広くなり、出店も多いそうですよ。"},{"id":{"d":665,"n":4,"t":5},"speaker":"B","text":"それはにぎやかになりそうですね。"},{"id":{"d":665,"n":5,"t":5},"speaker":"A","text":"綿あめ、りんごあめ、あんずあめ、たこ焼き、イカ焼き、お好み焼き、焼きそばなど、たくさんの出店があるそうです。"},{"id":{"d":665,"n":6,"t":5},"speaker":"B","text":"それは楽しみです。私は盆踊りの曲に合わせて太鼓をたたく予定です。"},{"id":{"d":665,"n":7,"t":5},"speaker":"A","text":"以前のように太鼓で町内の皆さんを盛り上げてくださいね。"},{"id":{"d":665,"n":8,"t":5},"speaker":"B","text":"はい、頑張ります。久しぶりの盆踊りなので、事前にいろいろと確認しておく必要がありそうですね。"},{"id":{"d":665,"n":9,"t":5},"speaker":"A","text":"詳しいことは今度の会合の時に打ち合わせをしましょう。"}];
INSERT INTO sentence [{"id":{"d":666,"n":1,"t":5},"speaker":"A","text":"佐々木さんは、日本のお祭りで見てみたいものは何かありますか？"},{"id":{"d":666,"n":2,"t":5},"speaker":"B","text":"秋田の竿灯まつりを、一度現地で見てみたいです。"},{"id":{"d":666,"n":3,"t":5},"speaker":"A","text":"竿灯まつりは東北三大祭りの一つですよね。"},{"id":{"d":666,"n":4,"t":5},"speaker":"B","text":"そうです。あとの二つは、青森のねぶた祭、仙台の七夕まつりですが、その二つは行ったことがあります。"},{"id":{"d":666,"n":5,"t":5},"speaker":"A","text":"なるほど。それで、竿灯まつりを見たいわけですね。"},{"id":{"d":666,"n":6,"t":5},"speaker":"B","text":"はい、そうです。曲芸のような技は間近で見る価値があると思います。"},{"id":{"d":666,"n":7,"t":5},"speaker":"A","text":"私は竿灯を見ると消防庁の出初め式を思い出します。"},{"id":{"d":666,"n":8,"t":5},"speaker":"B","text":"確かに、大技を繰り出す出初め式は、竿灯まつりと似ているかもしれません。"}];
INSERT INTO sentence [{"id":{"d":667,"n":1,"t":5},"speaker":"A","text":"音楽のジャンル分けは難しいですね。"},{"id":{"d":667,"n":2,"t":5},"speaker":"B","text":"私もそう感じることがあります。"},{"id":{"d":667,"n":3,"t":5},"speaker":"A","text":"ファンク、ソウル、ブラックコンテンポラリーと言われても違いが分かりません。"},{"id":{"d":667,"n":4,"t":5},"speaker":"B","text":"私は、作品を表現する言葉がいろいろとあるように、ジャンルの名称も表現の一つだと考えることにしました。"},{"id":{"d":667,"n":5,"t":5},"speaker":"A","text":"なるほど。私はもうジャンルを気にせず、気に入った音楽をただ楽しもうと思っています。"},{"id":{"d":667,"n":6,"t":5},"speaker":"B","text":"それで良いと思いますよ。音楽はさまざまな要素が融合してできているので、ジャンル分けにこだわることはないと思います。"},{"id":{"d":667,"n":7,"t":5},"speaker":"A","text":"それを聞いて気が楽になりました。音楽の話をするならジャンルではなく好きな音楽の話をすればよいのですね。"}];
INSERT INTO sentence [{"id":{"d":668,"n":1,"t":5},"speaker":"A","text":"映画を見に行くとき、どのような基準で鑑賞する作品を選んでいますか？"},{"id":{"d":668,"n":2,"t":5},"speaker":"B","text":"昔は自分と趣味の合う評論家を見つけ、その人の評論を見聞きしてから映画を選んでいました。"},{"id":{"d":668,"n":3,"t":5},"speaker":"A","text":"それはいいかもしれません。その評論家の話は今も聞いているのですか？"},{"id":{"d":668,"n":4,"t":5},"speaker":"B","text":"以前ほどは聞いていません。そのうちに評論家の話を聞かなくてもなんとなく想像できるようになって、自分で映画を選んでも同じだと考えるようになったからです。"},{"id":{"d":668,"n":5,"t":5},"speaker":"A","text":"それで評論家の話に頼らなくなったということですね。"},{"id":{"d":668,"n":6,"t":5},"speaker":"B","text":"はい。でも、その映画評論家は今も好きですし、映画を見終わった後には彼の感想を読みに行くこともあります。"}];
INSERT INTO sentence [{"id":{"d":669,"n":1,"t":5},"speaker":"A","text":"先日、フォークギターを買いました。"},{"id":{"d":669,"n":2,"t":5},"speaker":"B","text":"どういった風の吹き回しですか？"},{"id":{"d":669,"n":3,"t":5},"speaker":"A","text":"ギターを弾きながら、昔のフォークソングを歌いたくなったのです。昔は、あるテレビ番組で視聴者に白いギターをプレゼントしていたこともあるほど、フォークソングが大流行していたのですよ。"},{"id":{"d":669,"n":4,"t":5},"speaker":"B","text":"それほどまでに流行していたのですね。確かに、昔のフォークソングをカバーした曲は、今も高い人気があるようです。"},{"id":{"d":669,"n":5,"t":5},"speaker":"A","text":"はい。日本のフォークソングには名曲がたくさんありますよ。今度カセットテープに入っているフォークソング名曲集をお貸ししましょう。"},{"id":{"d":669,"n":6,"t":5},"speaker":"B","text":"ありがたいのですが、あいにく私はカセットテープを再生できる機械を持っていません。"}];
INSERT INTO sentence [{"id":{"d":670,"n":1,"t":5},"speaker":"A","text":"最近の映画には、昭和時代に流行した映画のタイトルを再び使っているものがありますね。"},{"id":{"d":670,"n":2,"t":5},"speaker":"B","text":"それは私も感じていました。高齢者人口が増えているからではありませんか？"},{"id":{"d":670,"n":3,"t":5},"speaker":"A","text":"タイトルに懐かしさを感じて映画を見に来るということですか？"},{"id":{"d":670,"n":4,"t":5},"speaker":"B","text":"はい。高齢者の映画の好みは下の世代にも浸透するのではないかと思います。作品にも普遍性があるのではないでしょうか？"},{"id":{"d":670,"n":5,"t":5},"speaker":"A","text":"米国のヒーロー作品のように、いつまでも話が続いていったら面白いでしょうね。"},{"id":{"d":670,"n":6,"t":5},"speaker":"B","text":"それは面白いですね。米国のヒーロー作品が他のヒーロー作品と結び付くように、日本の作品も他の作品と結び付いたらいいと思います。"},{"id":{"d":670,"n":7,"t":5},"speaker":"A","text":"それも面白いかもしれません。"}];
INSERT INTO sentence [{"id":{"d":671,"n":1,"t":5},"speaker":"A","text":"先日、私の好きなギタリストの動画を息子に見せたら、笑われてしまいました。"},{"id":{"d":671,"n":2,"t":5},"speaker":"B","text":"なぜ、笑われたのですか？"},{"id":{"d":671,"n":3,"t":5},"speaker":"A","text":"「なぜこの人は歯でギターを弾いているの？弾きにくいだけだよね」と言われました。"},{"id":{"d":671,"n":4,"t":5},"speaker":"B","text":"なかなか鋭い視点ではありませんか。"},{"id":{"d":671,"n":5,"t":5},"speaker":"A","text":"歯でギターを弾くのが破天荒で格好良いと思っていたのですが、まさか、あそこまで笑われるとは思いませんでした。"},{"id":{"d":671,"n":6,"t":5},"speaker":"B","text":"そのギタリストの良さが伝わらなくて、残念でしたね。"},{"id":{"d":671,"n":7,"t":5},"speaker":"A","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":672,"n":1,"t":5},"speaker":"A","text":"鈴木さんは、ルービックキューブが得意だそうですね。"},{"id":{"d":672,"n":2,"t":5},"speaker":"B","text":"はい。ほんの数十秒で、全ての面をそろえることができます。"},{"id":{"d":672,"n":3,"t":5},"speaker":"A","text":"素晴らしいですね。そろえ方をぜひとも教えていただきたいのですが、そろえられるまで時間はかかるものですか？"},{"id":{"d":672,"n":4,"t":5},"speaker":"B","text":"手順がいくつかあるので、慣れるまでは時間がかかると思います。"},{"id":{"d":672,"n":5,"t":5},"speaker":"A","text":"映画やドラマではルービックキューブは頭の良い人が解いている印象があります。"},{"id":{"d":672,"n":6,"t":5},"speaker":"B","text":"そのイメージは分からなくもないですが、実際は頭の良さはあまり関係ありません。ルービックキューブを買うと、解き方のガイドも入っているはずです。"},{"id":{"d":672,"n":7,"t":5},"speaker":"A","text":"実は、私はその解き方のガイドを見ても分かりませんでした。"},{"id":{"d":672,"n":8,"t":5},"speaker":"B","text":"そうなのですね。では、まずはルービックキューブの1面をそろえてみましょう。"},{"id":{"d":672,"n":9,"t":5},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":673,"n":1,"t":5},"speaker":"A","text":"だまし絵に興味はありますか？"},{"id":{"d":673,"n":2,"t":5},"speaker":"B","text":"はい、好きです。水が下っているのか上っているのかが分からないもの、夜や昼が鳥に変化するものなどが特に好きです。"},{"id":{"d":673,"n":3,"t":5},"speaker":"A","text":"どちらもエッシャーが描いただまし絵ですね。"},{"id":{"d":673,"n":4,"t":5},"speaker":"B","text":"そうなのですね。絵を見た覚えはありますが、どこで見たのかは覚えていません。"},{"id":{"d":673,"n":5,"t":5},"speaker":"A","text":"日本でエッシャーを大きく取り上げたのは『週刊少年マガジン』だったそうですから、最初はそこで見たのかもしれませんよ。"},{"id":{"d":673,"n":6,"t":5},"speaker":"B","text":"そう言われてみれば、子どもの頃に『週刊少年マガジン』を買っていました。"},{"id":{"d":673,"n":7,"t":5},"speaker":"A","text":"エッシャーはだまし絵や無限の循環、幾何学的オブジェで有名です。西洋絵画にはエッシャーのような数学的解釈ではなく、宗教的隠喩をだまし絵のように挿入した作品もあります。"},{"id":{"d":673,"n":8,"t":5},"speaker":"B","text":"見方を変えると、どくろや悪魔が描かれているものもありますね。"},{"id":{"d":673,"n":9,"t":5},"speaker":"A","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":674,"n":1,"t":5},"speaker":"A","text":"休日に楽しみにしていることはありますか？"},{"id":{"d":674,"n":2,"t":5},"speaker":"B","text":"サーフィンです。最近始めたばかりなので、特に楽しみです。"},{"id":{"d":674,"n":3,"t":5},"speaker":"A","text":"サーフィンは子どもの頃からやっていたのですか？"},{"id":{"d":674,"n":4,"t":5},"speaker":"B","text":"いいえ、30歳になってから始めました。転勤で海沿いの家に引っ越したことがきっかけです。"},{"id":{"d":674,"n":5,"t":5},"speaker":"A","text":"サーフィンはすぐにできるものなのですか？"},{"id":{"d":674,"n":6,"t":5},"speaker":"B","text":"独学では無理だと思い、私は近所のサーフィンスクールに通いました。"},{"id":{"d":674,"n":7,"t":5},"speaker":"A","text":"サーフィンにもスクールがあるのですね。"},{"id":{"d":674,"n":8,"t":5},"speaker":"B","text":"はい。サーフボードをレンタルできますし、基本を学べますから、初心者ならば行くべきだと思います。"},{"id":{"d":674,"n":9,"t":5},"speaker":"A","text":"サーフボードは高そうですし、楽しめる場所も最初は分からないので、一人で始めるのは難しいスポーツかもしれません。"},{"id":{"d":674,"n":10,"t":5},"speaker":"B","text":"そのようなことはありませんが、もし気になるのなら、サーフィン仲間をつくると良いですよ。"}];
INSERT INTO sentence [{"id":{"d":675,"n":1,"t":5},"speaker":"A","text":"親指が腫れていますが、どうしたのですか？"},{"id":{"d":675,"n":2,"t":5},"speaker":"B","text":"金づちで自分の指をたたいてしまいました。"},{"id":{"d":675,"n":3,"t":5},"speaker":"A","text":"何かを作っていたのですか？"},{"id":{"d":675,"n":4,"t":5},"speaker":"B","text":"日曜大工で棚を作っていたのです。"},{"id":{"d":675,"n":5,"t":5},"speaker":"A","text":"日曜大工が趣味なのですか？"},{"id":{"d":675,"n":6,"t":5},"speaker":"B","text":"そうですね。庭にある物置小屋の内装を自分で改造しようとしたのが、日曜大工の始まりです。"},{"id":{"d":675,"n":7,"t":5},"speaker":"A","text":"工具をそろえるのは大変ではありませんか？"},{"id":{"d":675,"n":8,"t":5},"speaker":"B","text":"足りない工具をホームセンターに探しに行くのも楽しみの一つです。"},{"id":{"d":675,"n":9,"t":5},"speaker":"A","text":"今度からはけがのことも考えて救急箱も備えておいた方がいいですよ。"},{"id":{"d":675,"n":10,"t":5},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":676,"n":1,"t":5},"speaker":"A","text":"あなたが手に持っているのは石ですか？"},{"id":{"d":676,"n":2,"t":5},"speaker":"B","text":"はい。チャートという石です。"},{"id":{"d":676,"n":3,"t":5},"speaker":"A","text":"大事そうに持っていますが、珍しい石なのですか？"},{"id":{"d":676,"n":4,"t":5},"speaker":"B","text":"いいえ、よく見かける石ですよ。高価なものではありません。"},{"id":{"d":676,"n":5,"t":5},"speaker":"A","text":"その石には何か特別な意味でもあるのですか？"},{"id":{"d":676,"n":6,"t":5},"speaker":"B","text":"石集めが私の趣味なのです。チャートは放散虫という虫が堆積した岩石で、なぜか愛着が湧くのです。"},{"id":{"d":676,"n":7,"t":5},"speaker":"A","text":"虫が化石になっているということは、石を割ると中に虫が入っているのでしょうか？"},{"id":{"d":676,"n":8,"t":5},"speaker":"B","text":"非常に硬いので割るのは大変です。割らなくとも、ルーペを使えば表面からでも放散虫の殻を見ることができます。"},{"id":{"d":676,"n":9,"t":5},"speaker":"A","text":"石の話をしていると中学校時代を思い出します。理科の授業で火成岩や深成岩を習いました。"},{"id":{"d":676,"n":10,"t":5},"speaker":"B","text":"懐かしいですね。"}];
INSERT INTO sentence [{"id":{"d":677,"n":1,"t":5},"speaker":"A","text":"この頃はあまり飲み会に参加されていませんね。"},{"id":{"d":677,"n":2,"t":5},"speaker":"B","text":"実は、仕事帰りに学校に通っているので、しばらく飲み会には行けません。"},{"id":{"d":677,"n":3,"t":5},"speaker":"A","text":"そうだったのですね。何の学校に通っているのですか？"},{"id":{"d":677,"n":4,"t":5},"speaker":"B","text":"占いの学校です。子どもの頃から占い師に憧れていました。"},{"id":{"d":677,"n":5,"t":5},"speaker":"A","text":"占い師に憧れているとは知りませんでした。会社を辞めて占い師になるのですか？"},{"id":{"d":677,"n":6,"t":5},"speaker":"B","text":"いいえ、そのようなことは考えていません。占いを勉強しているのはほとんど趣味で、あわよくばセカンドキャリアの役に立てばいいなと思っています。"}];
INSERT INTO sentence [{"id":{"d":678,"n":1,"t":5},"speaker":"A","text":"ジャズが好きな方は、配信サービスよりもCDやアナログレコードを購入する傾向があるように思われます。"},{"id":{"d":678,"n":2,"t":5},"speaker":"B","text":"そうかもしれません。私もジャズが大好きですが、確かに、作品はCDで購入します。"},{"id":{"d":678,"n":3,"t":5},"speaker":"A","text":"なぜCDで購入するのですか？音だけの作品は購入しないということですか？"},{"id":{"d":678,"n":4,"t":5},"speaker":"B","text":"ミュージシャンがCD作品としたものは、そのままの形で購入したいのです。ミュージシャンの意向で配信サービスのみで鑑賞する作品ならば配信で購入します。"},{"id":{"d":678,"n":5,"t":5},"speaker":"A","text":"確かに、レコードやCDには芸術的価値のあるジャケットも存在しますね。"},{"id":{"d":678,"n":6,"t":5},"speaker":"B","text":"その通りです。また、ジャズはレーベルによって統一感のあるジャケットであることが多いので、つい集めたくなってしまうのです。"},{"id":{"d":678,"n":7,"t":5},"speaker":"A","text":"レコードジャケットを見るとどのレーベルか分かるのですか？"},{"id":{"d":678,"n":8,"t":5},"speaker":"B","text":"はい。大手のジャズレーベルならば、大概はジャケットで区別できますよ。"}];
INSERT INTO sentence [{"id":{"d":679,"n":1,"t":5},"speaker":"A","text":"ゲイラカイトをご存じですか？"},{"id":{"d":679,"n":2,"t":5},"speaker":"B","text":"知りませんが、カイトということは、たこのことですか？"},{"id":{"d":679,"n":3,"t":5},"speaker":"A","text":"良い読みです。ゲイラカイトは、1970年代に日本で流行した米国生まれのたこの名前です。"},{"id":{"d":679,"n":4,"t":5},"speaker":"B","text":"外国製のたこは見たことがありますが、日本のたことは形が随分と違いますよね。"},{"id":{"d":679,"n":5,"t":5},"speaker":"A","text":"はい。ゲイラカイトも、一般にはビニール製で、三角形の大きな紙飛行機に似ています。これが1970年代の日本で大流行したのです。"},{"id":{"d":679,"n":6,"t":5},"speaker":"B","text":"今はたこ揚げをする人もほとんどいませんから、たこがそれほど流行していた時期があったとはにわかに信じられません。"},{"id":{"d":679,"n":7,"t":5},"speaker":"A","text":"信じられないかもしれませんが、当時は社会現象になるほどの流行でしたよ。ゲイラカイトはよく飛んだので、電柱などに引っかける人が続出して、ニュースにもなりました。"},{"id":{"d":679,"n":8,"t":5},"speaker":"B","text":"そうだったのですね。ゲイラカイトは今も売っているのでしょうか？"},{"id":{"d":679,"n":9,"t":5},"speaker":"A","text":"売っているようですよ。米国からゲイラカイトを輸入販売している会社があるようです。"}];
INSERT INTO sentence [{"id":{"d":680,"n":1,"t":5},"speaker":"A","text":"子どもの頃、ミニカーを集めていましたか？"},{"id":{"d":680,"n":2,"t":5},"speaker":"B","text":"はい。私はチョロQを集めていました。"},{"id":{"d":680,"n":3,"t":5},"speaker":"A","text":"チョロQとは、どのようなものですか？"},{"id":{"d":680,"n":4,"t":5},"speaker":"B","text":"チョロQはプラスチック製で、ぜんまいを回して走るミニカーです。"},{"id":{"d":680,"n":5,"t":5},"speaker":"A","text":"思い出しました。昔は大変流行していましたね。当時、男の子はみんなチョロQで遊んでいました。"},{"id":{"d":680,"n":6,"t":5},"speaker":"B","text":"普通のミニカーと違って、チョロQは実物の車をデフォルメしたデザインでした。そこに改造を加えたり、色を塗り直したりして、自分だけの1台を作っていました。"},{"id":{"d":680,"n":7,"t":5},"speaker":"A","text":"懐かしいですね。当時の小学生でも購入できる値段だったので、皆が持っていたのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":681,"n":1,"t":5},"speaker":"A","text":"昔は筆入れを二つ持っている子どもをよく見かけました。"},{"id":{"d":681,"n":2,"t":5},"speaker":"B","text":"私も筆入れを二つ持って学校に行っていましたよ。一つは普通の筆入れで、もう一つはキャラクター型の消しゴムを入れるための筆入れです。"},{"id":{"d":681,"n":3,"t":5},"speaker":"A","text":"キャラクター消しゴムとは、懐かしいですね。カプセルトイで買う消しゴムのことですね。"},{"id":{"d":681,"n":4,"t":5},"speaker":"B","text":"はい。当時は20円だったので、お小遣いをもらうとカプセルトイに使っていました。"},{"id":{"d":681,"n":5,"t":5},"speaker":"A","text":"同じようなことをみんながしていましたね。"},{"id":{"d":681,"n":6,"t":5},"speaker":"B","text":"なかなか手に入らないキャラクターがあって、それを持っていると自慢できました。"},{"id":{"d":681,"n":7,"t":5},"speaker":"A","text":"持っていないものは、お互いに交換して手に入れていましたね。"},{"id":{"d":681,"n":8,"t":5},"speaker":"B","text":"そうでしたね。懐かしいです。"}];
INSERT INTO sentence [{"id":{"d":682,"n":1,"t":5},"speaker":"A","text":"テレビゲーム以前のゲームで、思い出に残っているものはありますか？"},{"id":{"d":682,"n":2,"t":5},"speaker":"B","text":"野球盤でしょうか。当時の子どもは一度は野球盤をやったことがあるでしょう。"},{"id":{"d":682,"n":3,"t":5},"speaker":"A","text":"懐かしいですね。消える魔球の機能が付いた野球盤もありました。"},{"id":{"d":682,"n":4,"t":5},"speaker":"B","text":"野球盤に消える魔球の機能が付いたのは『巨人の星』の影響ですね。"},{"id":{"d":682,"n":5,"t":5},"speaker":"A","text":"消える魔球の機能が付いていても、打とうと思えば打てる仕組みになっていましたね。"},{"id":{"d":682,"n":6,"t":5},"speaker":"B","text":"そういえば、外野の照明が本当に光る機能を備えた野球盤もありましたよ。"},{"id":{"d":682,"n":7,"t":5},"speaker":"A","text":"ありましたね。子どもながらにあまり必要のない機能だと思っていたものです。"},{"id":{"d":682,"n":8,"t":5},"speaker":"B","text":"私は部屋を暗くして照明をつけてゲームをやってみたことがあります。もっとも、よく見えなくてすぐに部屋の電気をつけましたがね。"},{"id":{"d":682,"n":9,"t":5},"speaker":"A","text":"ナイター気分を演出したかったのかもしれませんが、残念ながら光量が足りなかったようですね。"}];
INSERT INTO sentence [{"id":{"d":683,"n":1,"t":5},"speaker":"A","text":"今の子どもたちのお菓子に関する事情で気になることがあります。以前と比べておまけの付いたお菓子をあまり見なくなったような気がするのです。"},{"id":{"d":683,"n":2,"t":5},"speaker":"B","text":"確かに、おまけについて話す子どもをあまり見かけなくなったように思います。"},{"id":{"d":683,"n":3,"t":5},"speaker":"A","text":"私たちが子どもの頃は、いろいろなお菓子におまけが付いていましたよね。"},{"id":{"d":683,"n":4,"t":5},"speaker":"B","text":"確かにアイスクリームにもおまけが付いているものがありました。"},{"id":{"d":683,"n":5,"t":5},"speaker":"A","text":"今も怪獣カードや野球カードを集めている子どもはいるのでしょうか？"},{"id":{"d":683,"n":6,"t":5},"speaker":"B","text":"いるのかもしれませんが、昔ほどたくさんはいないと思います。"},{"id":{"d":683,"n":7,"t":5},"speaker":"A","text":"私は、食品玩具も日本的な文化だと思うので、廃れないでほしいと思います。"}];
INSERT INTO sentence [{"id":{"d":684,"n":1,"t":5},"speaker":"A","text":"アナログレコードの話をする人は多いですが、ソノシートのことを話題にする人は少ないですね。"},{"id":{"d":684,"n":2,"t":5},"speaker":"B","text":"そもそもソノシートは買うものではなかったような気がします。ソノシートは雑誌のおまけに付いていることが多かった印象です。"},{"id":{"d":684,"n":3,"t":5},"speaker":"A","text":"確かにそうですね。レコードと違って割れないので、ソノシート雑誌のおまけや書類扱いの郵送物にも使われていました。"},{"id":{"d":684,"n":4,"t":5},"speaker":"B","text":"ソノシートはアイドルなどのファンクラブの特典にもなっていましたね。シングルレコードより小さいタイプもありましたが、LPレコードのように大きいサイズはなかったような気がします。"},{"id":{"d":684,"n":5,"t":5},"speaker":"A","text":"今になって思えば、文化的にもソノシートは重要なアイテムだったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":685,"n":1,"t":5},"speaker":"A","text":"サントラとは何の略だったでしょうか？"},{"id":{"d":685,"n":2,"t":5},"speaker":"B","text":"サウンドトラックの略ですね。映画やドラマ作品などで使われた音の素材を集めたものです。"},{"id":{"d":685,"n":3,"t":5},"speaker":"A","text":"そうでした。昔はサウンドトラックなどなかったように思いますが、いつ頃から定着したのでしょうか？"},{"id":{"d":685,"n":4,"t":5},"speaker":"B","text":"よく分かりませんが、かつて『サタデー・ナイト・フィーバー』のサウンドトラックが大ヒットしたのは覚えています。オリジナルサウンドトラックとも言いますから、オリジナルではない作品との差別化を図ったのではないでしょうか？"},{"id":{"d":685,"n":5,"t":5},"speaker":"A","text":"オリジナルではない曲も売られていたのですか？"},{"id":{"d":685,"n":6,"t":5},"speaker":"B","text":"昔はイージーリスニングというジャンルがあって、編曲した映画音楽などが作品化されていました。"},{"id":{"d":685,"n":7,"t":5},"speaker":"A","text":"映画音楽そのものの人気が高まったことも、サウンドトラックが一般化したことの要因だと思います。"}];
INSERT INTO sentence [{"id":{"d":686,"n":1,"t":5},"speaker":"A","text":"ライブハウスに行ったことがありますか？"},{"id":{"d":686,"n":2,"t":5},"speaker":"B","text":"はい。学生の時はバンドを組んでいたので、必然的にライブハウスに行っていました。"},{"id":{"d":686,"n":3,"t":5},"speaker":"A","text":"格好良いです。まさか、ライブハウスで演奏する側とは思いませんでした。"},{"id":{"d":686,"n":4,"t":5},"speaker":"B","text":"もちろん、演奏をするだけではなく、他のバンドのライブを見に行くこともありましたよ。"},{"id":{"d":686,"n":5,"t":5},"speaker":"A","text":"ライブハウスには1人で入る人もいるのですか？"},{"id":{"d":686,"n":6,"t":5},"speaker":"B","text":"私は慣れているので1人でライブハウスに入ります。そういう人も多かったと思いますよ。"},{"id":{"d":686,"n":7,"t":5},"speaker":"A","text":"そうなのですか。"},{"id":{"d":686,"n":8,"t":5},"speaker":"B","text":"近いうちにライブハウスへ行く予定があるのですか？"},{"id":{"d":686,"n":9,"t":5},"speaker":"A","text":"昔好きだったバンドが今度ライブハウスで再結成ライブを行うので、ぜひ行きたいと思っています。でも、私はライブハウスに行ったことがないので、不安だったのです。"},{"id":{"d":686,"n":10,"t":5},"speaker":"B","text":"それでライブハウスについていろいろと質問なさっていたのですね。1人でライブハウスに行っても大丈夫ですから、ぜひライブを楽しんで来てくださいね。"},{"id":{"d":686,"n":11,"t":5},"speaker":"A","text":"ありがとうございます。あなたのおかげで勇気が出ました。今回はライブ体験を満喫したいと思います。"}];
INSERT INTO sentence [{"id":{"d":687,"n":1,"t":5},"speaker":"A","text":"スーパーボールを知っていますか？"},{"id":{"d":687,"n":2,"t":5},"speaker":"B","text":"それは、アメリカンフットボールの大会のことですか？"},{"id":{"d":687,"n":3,"t":5},"speaker":"A","text":"違います。そちらのことではなく、500円玉くらいの大きさのゴムでできたボールのことです。"},{"id":{"d":687,"n":4,"t":5},"speaker":"B","text":"ああ、あの極端によく弾むゴムのボールのことですね。もちろん知っていますよ。"},{"id":{"d":687,"n":5,"t":5},"speaker":"A","text":"あれは今でも売られているのでしょうか？最近はあれで遊んでいる子どもをほとんど見かけません。"},{"id":{"d":687,"n":6,"t":5},"speaker":"B","text":"私は祭りの屋台で見ましたよ。「スーパーボールすくい」という名前の屋台でした。"},{"id":{"d":687,"n":7,"t":5},"speaker":"A","text":"金魚すくいのようなものでしょうか？"},{"id":{"d":687,"n":8,"t":5},"speaker":"B","text":"そうです。金魚の代わりに、スーパーボールをすくうのです。"},{"id":{"d":687,"n":9,"t":5},"speaker":"A","text":"小さいスーパーボールなら、金魚よりも簡単に取れそうですね。"},{"id":{"d":687,"n":10,"t":5},"speaker":"B","text":"はい。祭りの屋台以外では、スーパーボールを売っているお店を見たことはありませんね。"},{"id":{"d":687,"n":11,"t":5},"speaker":"A","text":"そうですか。昔は駄菓子屋でもスーパーボールが売られていたものですが、今でも売られているのですかね。"}];
INSERT INTO sentence [{"id":{"d":688,"n":1,"t":5},"speaker":"A","text":"あなたはペットを飼っていますか？"},{"id":{"d":688,"n":2,"t":5},"speaker":"B","text":"私はヤモリを飼っています。"},{"id":{"d":688,"n":3,"t":5},"speaker":"A","text":"ヤモリをペットとして買うことは、珍しいのではありませんか？"},{"id":{"d":688,"n":4,"t":5},"speaker":"B","text":"そのようなことはないと思います。アパートでの1人暮らしですから、ケースで飼えるペットに目が行くのは自然なことだと思います。"},{"id":{"d":688,"n":5,"t":5},"speaker":"A","text":"なるほど。ヤモリは、トカゲやイモリとの区別がつくのですか？"},{"id":{"d":688,"n":6,"t":5},"speaker":"B","text":"つきますよ。トカゲは爪が大きく、ヤモリのようには壁を上りません。日本の一般的なイモリのおなかはヤモリと違って赤いです。"},{"id":{"d":688,"n":7,"t":5},"speaker":"A","text":"ペットとしてヤモリを選んだ理由は何ですか？"},{"id":{"d":688,"n":8,"t":5},"speaker":"B","text":"ヤモリは壁を上る姿や手足が丸まっているところがかわいいのですが、その姿に心をつかまれてしまいました。きゅっと鳴くところも魅力的です。"},{"id":{"d":688,"n":9,"t":5},"speaker":"A","text":"本当にヤモリをかわいがっているのですね。"},{"id":{"d":688,"n":10,"t":5},"speaker":"B","text":"はい。部屋を暗くして、小さな明かりでヤモリを眺めている時が、至福の瞬間です。"}];
INSERT INTO sentence [{"id":{"d":689,"n":1,"t":5},"speaker":"A","text":"プラバンを知っていますか？"},{"id":{"d":689,"n":2,"t":5},"speaker":"B","text":"もちろん知っています。懐かしいですね。私の母がよくプラバンで小物を作っていました。"},{"id":{"d":689,"n":3,"t":5},"speaker":"A","text":"お母さんはプラバンに興味があったのですか？"},{"id":{"d":689,"n":4,"t":5},"speaker":"B","text":"母の本職はデザイナーで、絵を描くことが好きだったので、ブラバンに興味があったのでしょう。"},{"id":{"d":689,"n":5,"t":5},"speaker":"A","text":"プラバンは作る工程の楽しさもありますが、やはり絵はうまい方がいいでしょうね。"},{"id":{"d":689,"n":6,"t":5},"speaker":"B","text":"小学生くらいまでは、私のネームプレートやキーホルダーを母がプラバンで作ってくれていました。母は絵がとても上手だったので、出来上がりはまるで購入品のようでした。"},{"id":{"d":689,"n":7,"t":5},"speaker":"A","text":"ご自分では何も作らなかったのですか？"},{"id":{"d":689,"n":8,"t":5},"speaker":"B","text":"もちろん自分でも作ったことがあります。母の作品を見ると、どうしても母に作ってもらう方がいいと思ってしまい、途中でやめてしまっていました。"},{"id":{"d":689,"n":9,"t":5},"speaker":"A","text":"本職のデザイナーですから、売り物になるくらいの出来だったのでしょうね。"},{"id":{"d":689,"n":10,"t":5},"speaker":"B","text":"はい、素晴らしい出来でした。"}];
INSERT INTO sentence [{"id":{"d":690,"n":1,"t":5},"speaker":"A","text":"この間の日曜日にホームセンターでお会いしましたね。あの日は、何を買ったのですか？"},{"id":{"d":690,"n":2,"t":5},"speaker":"B","text":"版画用ゴム版と両面テープを購入しました。"},{"id":{"d":690,"n":3,"t":5},"speaker":"A","text":"版画が趣味なのですか？"},{"id":{"d":690,"n":4,"t":5},"speaker":"B","text":"子どもたちにゴム版はんこの作り方を教えるために買いました。"},{"id":{"d":690,"n":5,"t":5},"speaker":"A","text":"消しゴムはんこなら作ったことがありますが、それよりも難しいですか？"},{"id":{"d":690,"n":6,"t":5},"speaker":"B","text":"ゴム板は消しゴムよりも大きいので、作業はしやすいかもしれません。"},{"id":{"d":690,"n":7,"t":5},"speaker":"A","text":"版画用のゴム板ですから、自由に切ることができて便利ですね。"},{"id":{"d":690,"n":8,"t":5},"speaker":"B","text":"はい。版画用のゴム板は、消しゴムはんこにはない繊細な表現ができます。"},{"id":{"d":690,"n":9,"t":5},"speaker":"A","text":"ところで、なぜ子どもたちにゴム版はんこを教えようと思ったのですか？"},{"id":{"d":690,"n":10,"t":5},"speaker":"B","text":"冬休みの自由課題について相談されたので、昔作ったことがあったゴム版はんこを教えることにしたのです。"}];
INSERT INTO sentence [{"id":{"d":691,"n":1,"t":5},"speaker":"A","text":"レタリングシートを孫に譲ろうとしたら断られました。今の子どもはレタリングなどはしないのですか？"},{"id":{"d":691,"n":2,"t":5},"speaker":"B","text":"私もレタリングシートは使いません。お孫さんは見たこともないかもしれませんね。"},{"id":{"d":691,"n":3,"t":5},"speaker":"A","text":"そうなのですか。昔は、カセットテープやVHS、CD、DVDのケースに貼るラベルを作るために重宝されておりましたのに。"},{"id":{"d":691,"n":4,"t":5},"speaker":"B","text":"カセットテープやVHS、CD、DVDをお孫さんは使っていないのではないでしょうか？"},{"id":{"d":691,"n":5,"t":5},"speaker":"A","text":"そうなのですね。昔はお気に入りの曲を録音し、きれいにレタリングしたケースに入れていたものです。"},{"id":{"d":691,"n":6,"t":5},"speaker":"B","text":"それを友人にプレゼントしていましたよね。"},{"id":{"d":691,"n":7,"t":5},"speaker":"A","text":"私もしていました。今から考えると結構恥ずかしい行為ですね。"},{"id":{"d":691,"n":8,"t":5},"speaker":"B","text":"プリンターを置く家が増えた頃から、レタリングシートは廃れていったと思います。"}];
INSERT INTO sentence [{"id":{"d":692,"n":1,"t":5},"speaker":"A","text":"先月のお盆の時に実家に帰ったら、ゲームボーイを見つけました。"},{"id":{"d":692,"n":2,"t":5},"speaker":"B","text":"それは懐かしいですね。いつのゲームボーイでしょうか？"},{"id":{"d":692,"n":3,"t":5},"speaker":"A","text":"初代のゲームボーイとゲームボーイカラーです。"},{"id":{"d":692,"n":4,"t":5},"speaker":"B","text":"初代のゲームボーイは30年以上前に発売したものですね。まだ動くのですか？"},{"id":{"d":692,"n":5,"t":5},"speaker":"A","text":"驚くことに、まだ動きました。"},{"id":{"d":692,"n":6,"t":5},"speaker":"B","text":"ゲームボーイのソフトはまだありますか？"},{"id":{"d":692,"n":7,"t":5},"speaker":"A","text":"はい。ゲームボーイとゲームボーイカラーのどちらとも20タイトルほどのゲームソフトがあります。"},{"id":{"d":692,"n":8,"t":5},"speaker":"B","text":"かなりの数のゲームソフトを持っているのですね。全部のソフトを遊んだのですか？"},{"id":{"d":692,"n":9,"t":5},"speaker":"A","text":"そのはずですが、遊んだ記憶のないゲームソフトもありました。"}];
INSERT INTO sentence [{"id":{"d":693,"n":1,"t":5},"speaker":"A","text":"釣りが趣味とお聞きしました。どのような釣りが特にお好きですか？"},{"id":{"d":693,"n":2,"t":5},"speaker":"B","text":"実家が渓流の近くだったので、アユの友釣りを子どもの頃から楽しんできました。"},{"id":{"d":693,"n":3,"t":5},"speaker":"A","text":"珍しいですね。アユの友釣りをされている方に初めてお会いしました。"},{"id":{"d":693,"n":4,"t":5},"speaker":"B","text":"装備が大がかりになりがちで、おとりのアユも購入しなければいけないので、一般の川釣りよりも費用はかかるかもしれません。"},{"id":{"d":693,"n":5,"t":5},"speaker":"A","text":"友釣りの大がかりな装置とはどのようなものでしょうか？"},{"id":{"d":693,"n":6,"t":5},"speaker":"B","text":"さおは長くて高感度でなければならないので高価になりがちで、おとりのアユを入れる缶もエアポンプなどの機能が付いたものが好まれます。"},{"id":{"d":693,"n":7,"t":5},"speaker":"A","text":"なるほど。川の中に入るので、そのための衣服も必要ですね。"},{"id":{"d":693,"n":8,"t":5},"speaker":"B","text":"そうです。ウエットタイツ、専用の足袋、釣り用ベスト、偏光眼鏡、玉網、携帯用のいけすが友釣りには必要です。"},{"id":{"d":693,"n":9,"t":5},"speaker":"A","text":"友釣りは技術的にも難しいのですか？"},{"id":{"d":693,"n":10,"t":5},"speaker":"B","text":"慣れるまでは経験者と一緒に行く方がいいですが、釣り自体はそれほど難しくはないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":694,"n":1,"t":5},"speaker":"A","text":"盆栽を趣味になさっているそうですね。きっかけは何だったのでしょうか？"},{"id":{"d":694,"n":2,"t":5},"speaker":"B","text":"祖父が亡くなった時に遺していった盆栽を、私が世話するようになったことがきっかけです。"},{"id":{"d":694,"n":3,"t":5},"speaker":"A","text":"盆栽の世話は大変ではありませんか？"},{"id":{"d":694,"n":4,"t":5},"speaker":"B","text":"水をやって日に当てるだけですので、作業自体は簡単です。難しいのはそれをこまめに継続することです。"},{"id":{"d":694,"n":5,"t":5},"speaker":"A","text":"なるほど。こまめなお世話が必要なのですね。"},{"id":{"d":694,"n":6,"t":5},"speaker":"B","text":"盆栽を趣味とする方に高齢者が多いのは、日中に家にいなければならないからだと思います。会社勤務の方が盆栽の世話をすることは難しいかもしれません。"},{"id":{"d":694,"n":7,"t":5},"speaker":"A","text":"あなたは自宅で仕事をされているので、それで、盆栽の世話を頼まれたのですか？"},{"id":{"d":694,"n":8,"t":5},"speaker":"B","text":"そうです。祖父が大切にしていた盆栽なので、世話をすることができる私が引き受けました。"},{"id":{"d":694,"n":9,"t":5},"speaker":"A","text":"盆栽には剪定が必要ですよね？あなたは剪定もしているのですか？"},{"id":{"d":694,"n":10,"t":5},"speaker":"B","text":"いいえ、今はまだ剪定をしていません。盆栽は切ってしまったら取り返しがつきませんから、もう少し知識と技術を身に着けてから挑戦したいと思っています。"}];
INSERT INTO sentence [{"id":{"d":695,"n":1,"t":5},"speaker":"A","text":"映画鑑賞が趣味とのことですが、見る映画を選ぶときの基準はありますか？"},{"id":{"d":695,"n":2,"t":5},"speaker":"B","text":"主演俳優や監督で見る映画を選んでいますが、基準というほどではありません。"},{"id":{"d":695,"n":3,"t":5},"speaker":"A","text":"アカデミー賞受賞作品は必ず見ますか？"},{"id":{"d":695,"n":4,"t":5},"speaker":"B","text":"そうですね。大体は見るようにしていますが、必ず見るのはラズベリー賞受賞作品です。"},{"id":{"d":695,"n":5,"t":5},"speaker":"A","text":"ラズベリー賞とは何でしょうか？"},{"id":{"d":695,"n":6,"t":5},"speaker":"B","text":"毎年のアカデミー賞授賞式の前日に、前年度公開作品の中から最低の映画を選出して贈る賞です。"},{"id":{"d":695,"n":7,"t":5},"speaker":"A","text":"変わった賞があるのですね。そのような映画が日本で公開されるのですか？"},{"id":{"d":695,"n":8,"t":5},"speaker":"B","text":"最低の映画といっても前評判とのギャップが大きい作品のことで、大作映画が受賞することが多いです。"},{"id":{"d":695,"n":9,"t":5},"speaker":"A","text":"大作映画が選ばれるのであれば、大物スターが受賞することもあるわけですね。"},{"id":{"d":695,"n":10,"t":5},"speaker":"B","text":"そうです。名のある監督や俳優でなければ、ラズベリー賞は受賞できないとも言えます。"}];
INSERT INTO sentence [{"id":{"d":696,"n":1,"t":5},"speaker":"A","text":"ホラー映画がお好きだそうですが、特にどういったホラー映画がお好きなのですか？"},{"id":{"d":696,"n":2,"t":5},"speaker":"B","text":"一通りホラー映画は見ていますが、特に好きなのはジャパニーズホラーです。"},{"id":{"d":696,"n":3,"t":5},"speaker":"A","text":"ジャパニーズホラーは人気がありますね。米国でリメイクもされている作品も多いと聞きます。"},{"id":{"d":696,"n":4,"t":5},"speaker":"B","text":"はい。日本はアニメーション作品だけではなく、ホラー映画の分野でも世界に通用する作品を作ることができると思います。"},{"id":{"d":696,"n":5,"t":5},"speaker":"A","text":"海外でリメイクされたジャパニーズホラーについてはどう思いますか？"},{"id":{"d":696,"n":6,"t":5},"speaker":"B","text":"映画として面白かった作品はありますが、やはりジャパニーズホラーは日本のものが一番好みです。リメイク作品では、サスペンス要素やスプラッター要素が追加されることもしばしばあり、それがあまり好みではありません。"},{"id":{"d":696,"n":7,"t":5},"speaker":"A","text":"確かに、ジャパニーズホラーは、サスペンス映画やスリラー映画、スプラッター映画とも違う、独特の雰囲気を持っていますね。"},{"id":{"d":696,"n":8,"t":5},"speaker":"B","text":"そう思います。ジャパニーズホラーならではの怖さは、昔の日本映画においても見ることができます。それはこれからも受け継がれていくでしょう。"},{"id":{"d":696,"n":9,"t":5},"speaker":"A","text":"日本では、ジャパニーズホラーに特化した映画監督や脚本家もいますね。"}];
INSERT INTO sentence [{"id":{"d":697,"n":1,"t":5},"speaker":"A","text":"大人になってから始めた趣味はありますか？"},{"id":{"d":697,"n":2,"t":5},"speaker":"B","text":"オルゴール作りを5年ほど前から続けています。"},{"id":{"d":697,"n":3,"t":5},"speaker":"A","text":"すてきですね。オルゴール作りといえば、中学校の美術の授業でオルゴールの箱を作った記憶があります。"},{"id":{"d":697,"n":4,"t":5},"speaker":"B","text":"私はオルゴールの箱だけでなく、曲も自分で作っています。"},{"id":{"d":697,"n":5,"t":5},"speaker":"A","text":"オルゴールの曲も作ることができるのですか？私がオルゴールの箱を作った時は、曲は既成のものを選んでいました。"},{"id":{"d":697,"n":6,"t":5},"speaker":"B","text":"今はオルゴールの手作りキットがあって、曲も自分で作ることができます。"},{"id":{"d":697,"n":7,"t":5},"speaker":"A","text":"自分で好きな曲を作って、オルゴールにすることができるのですか？"},{"id":{"d":697,"n":8,"t":5},"speaker":"B","text":"はい。そのためには、多少は音楽の素養も必要かもしれません。"}];
INSERT INTO sentence [{"id":{"d":698,"n":1,"t":5},"speaker":"A","text":"大人になってから新しく始める趣味としては、どのようなものがお薦めですか？"},{"id":{"d":698,"n":2,"t":5},"speaker":"B","text":"城巡りはどうでしょうか。移動に時間と費用はかかりますが、だからこそ経済力のある大人が楽しめるのではないかと思います。私も大人になってから、城巡りをするようになりました。"},{"id":{"d":698,"n":3,"t":5},"speaker":"A","text":"なるほど。城巡りの面白さはどこにありますか？"},{"id":{"d":698,"n":4,"t":5},"speaker":"B","text":"建造物としての素晴らしさを楽しむのはもちろんですが、その城にまつわる歴史を知ることも面白いと感じます。"},{"id":{"d":698,"n":5,"t":5},"speaker":"A","text":"城巡りは歴史を学ぶいいきっかけにもなるのですね。興味深いです。"},{"id":{"d":698,"n":6,"t":5},"speaker":"B","text":"城内だけなく、城の周辺にも歴史的価値のあるスポットが数多く存在しているので、城を訪れる際には、あらかじめ見るべき箇所に目星を付けておくといいです。"},{"id":{"d":698,"n":7,"t":5},"speaker":"A","text":"城には見どころも満載なのですね。城を訪れたときに、必ずしていることはありますか？"},{"id":{"d":698,"n":8,"t":5},"speaker":"B","text":"私は、必ず写真を撮るようにしています。城はしばしば改修工事が行われるので、自分が訪れたときの景色を写真に残すようにしています。"}];
INSERT INTO sentence [{"id":{"d":699,"n":1,"t":5},"speaker":"A","text":"日曜日はいつもどのように過ごされていますか？"},{"id":{"d":699,"n":2,"t":5},"speaker":"B","text":"月に一度は、近所の行きつけのそば屋に行きます。"},{"id":{"d":699,"n":3,"t":5},"speaker":"A","text":"そばを食べるのですね。なかなか粋な趣味だと思います。"},{"id":{"d":699,"n":4,"t":5},"speaker":"B","text":"それほど大したものではありません。そば屋のテレビで競馬中継を見ながらビールを飲むのが、私にとっても至福のひとときです。"},{"id":{"d":699,"n":5,"t":5},"speaker":"A","text":"そのそば屋にはどれくらい通っているのですか？"},{"id":{"d":699,"n":6,"t":5},"speaker":"B","text":"そば屋の主人が先代からそば屋を引き継いだのが20年くらい前でしたが、その頃から日曜日にはそこに行くようになりました。"},{"id":{"d":699,"n":7,"t":5},"speaker":"A","text":"長いこと通っているのですね。そば屋の主人は知り合いなのですか？"},{"id":{"d":699,"n":8,"t":5},"speaker":"B","text":"はい。小学校時代からの友人なので、店にいると落ち着きます。"},{"id":{"d":699,"n":9,"t":5},"speaker":"A","text":"それでは昔からの知り合いが集まってきそうですね。"},{"id":{"d":699,"n":10,"t":5},"speaker":"B","text":"ええ、それもまた楽しいです。幼なじみが偶然店に集まり、まるで同窓会のようになるときもあります。"}];
INSERT INTO sentence [{"id":{"d":700,"n":1,"t":5},"speaker":"A","text":"全くの素人でもこの年齢から始められる楽器はありますか？"},{"id":{"d":700,"n":2,"t":5},"speaker":"B","text":"どの楽器でも始められると思いますが、大正琴はどうですか？左手で鍵盤を押さえ、右手のピックで演奏するものです。誰でも正確に音を出すことができます。"},{"id":{"d":700,"n":3,"t":5},"speaker":"A","text":"面白そうですね。大正琴を買うとしたら、おいくらくらいするのでしょうか？"},{"id":{"d":700,"n":4,"t":5},"speaker":"B","text":"数万円でかなり良い大正琴が買えますよ。"},{"id":{"d":700,"n":5,"t":5},"speaker":"A","text":"そのくらいなら手が出せそうです。ところで、なぜ大正琴というのでしょうね。大正時代と関係があるのでしょうか？"},{"id":{"d":700,"n":6,"t":5},"speaker":"B","text":"そのようです。大正時代に、ある人がタイプライターのキーを付けた琴を発明して、それが大流行したそうです。"},{"id":{"d":700,"n":7,"t":5},"speaker":"A","text":"そのような由来があるのですね。私は五線譜などまるで分かりませんが、初心者用の教則本などはあるのでしょうか？"},{"id":{"d":700,"n":8,"t":5},"speaker":"B","text":"そもそも楽器自体が音楽初心者用なので、その教則本も分かりやすいものになっています。"},{"id":{"d":700,"n":9,"t":5},"speaker":"A","text":"それなら安心です。大正琴の音楽教室はありますか？"},{"id":{"d":700,"n":10,"t":5},"speaker":"B","text":"大手のピアノ教室では大正琴も教えているところが多いです。カルチャーセンターでも教えているところがあるようですよ。"},{"id":{"d":700,"n":11,"t":5},"speaker":"A","text":"そうなのですね。それなら、私でも気軽に始められそうです。"}];
INSERT INTO sentence [{"id":{"d":701,"n":1,"t":5},"speaker":"A","text":"スマートフォンの無料ゲームに長時間を費やしてしまうことがあり、反省しています。"},{"id":{"d":701,"n":2,"t":5},"speaker":"B","text":"ゲームに没頭してしまう人は多いようですね。かつて、有料ゲームに大金を使ってしまう若者がいるというニュースを見たことがあります。"},{"id":{"d":701,"n":3,"t":5},"speaker":"A","text":"はい。確か10年くらい前に課金制度が一躍社会問題になりましたね。"},{"id":{"d":701,"n":4,"t":5},"speaker":"B","text":"あまりゲームに詳しくないのですが、課金制度とはどういうものでしょうか？"},{"id":{"d":701,"n":5,"t":5},"speaker":"A","text":"私も無料の範囲でしかゲームをしないのでそれほど詳しくは知りませんが、お金を払うとゲームの攻略に有利なアイテムが手に入って、ゲームを優位に進められるそうですよ。"},{"id":{"d":701,"n":6,"t":5},"speaker":"B","text":"基本的に無料で遊べるゲームも、お金を払うと攻略が容易になるのですね。"},{"id":{"d":701,"n":7,"t":5},"speaker":"A","text":"そうです。課金制度自体が悪いのではなく、利用の仕方の問題なのでしょうね。節度を持って利用することが大事です。"}];
INSERT INTO sentence [{"id":{"d":702,"n":1,"t":5},"speaker":"A","text":"オンラインゲームはどういうゲームなのですか？"},{"id":{"d":702,"n":2,"t":5},"speaker":"B","text":"インターネットに接続して、画面の向こうのプレイヤーとプレイをするゲームのことです。"},{"id":{"d":702,"n":3,"t":5},"speaker":"A","text":"なぜインターネットに接続するのですか？"},{"id":{"d":702,"n":4,"t":5},"speaker":"B","text":"他のプレイヤーと対戦を楽しむことができるからではないでしょうか？"},{"id":{"d":702,"n":5,"t":5},"speaker":"A","text":"オンラインゲームは、常に対戦相手と1対1なのですか？"},{"id":{"d":702,"n":6,"t":5},"speaker":"B","text":"いいえ。多人数参加型のオンラインゲームもあります。対戦するだけではなく、グループをつくって一緒に目的を達成するゲームもあります。"},{"id":{"d":702,"n":7,"t":5},"speaker":"A","text":"ゲームが好きな人にはコミュニケーションが苦手な人が多いと聞きましたが、オンラインゲームは十分に社交的ですね。"},{"id":{"d":702,"n":8,"t":5},"speaker":"B","text":"そう思います。社交的でなければ、会ったこともない人とゲームはしませんよね。"},{"id":{"d":702,"n":9,"t":5},"speaker":"A","text":"いろいろな楽しみ方もあるようですし、私もオンラインゲームをやってみたいと思いました。"}];
INSERT INTO sentence [{"id":{"d":703,"n":1,"t":5},"speaker":"A","text":"子どものおもちゃの中には、大人の職業に関係したものがありますよね。"},{"id":{"d":703,"n":2,"t":5},"speaker":"B","text":"おもちゃの聴診器やスーパーマーケットのレジなどのことですか？"},{"id":{"d":703,"n":3,"t":5},"speaker":"A","text":"そうです。聴診器はいまだに人気があるようですね。"},{"id":{"d":703,"n":4,"t":5},"speaker":"B","text":"注射器とおもちゃの薬が入ったお医者さんセットも人気があったような気がします。"},{"id":{"d":703,"n":5,"t":5},"speaker":"A","text":"今はおもちゃの聴診器でも本当に心音を聴くことができるそうですよ。"},{"id":{"d":703,"n":6,"t":5},"speaker":"B","text":"それは本格的ですね。私が子どもの頃は、お気に入りの人形を患者に見立てていました。"},{"id":{"d":703,"n":7,"t":5},"speaker":"A","text":"患者用の人形や子ども用の白衣までもが用意されているようです。"},{"id":{"d":703,"n":8,"t":5},"speaker":"B","text":"そこまで用意されているのですね。そのうち自動体外式除細動器、つまりAEDのおもちゃも売られるようになるかもしれません。"},{"id":{"d":703,"n":9,"t":5},"speaker":"A","text":"AEDのおもちゃは、実はもうあるのですよ。"},{"id":{"d":703,"n":10,"t":5},"speaker":"B","text":"それは驚きました。"}];
INSERT INTO sentence [{"id":{"d":704,"n":1,"t":5},"speaker":"A","text":"かごめかごめという遊びをしたことがありますか？"},{"id":{"d":704,"n":2,"t":5},"speaker":"B","text":"遊び方は何となく知っていますが、私自身は遊んだ記憶はありません。"},{"id":{"d":704,"n":3,"t":5},"speaker":"A","text":"そうですよね。よく知られた遊びだと思いますが、実際に遊んだことのある人はあまりいないように思うのです。"},{"id":{"d":704,"n":4,"t":5},"speaker":"B","text":"鬼が真ん中で自分の目をふさぎ、その周りを子どもたちが輪になって回る遊びですよね。"},{"id":{"d":704,"n":5,"t":5},"speaker":"A","text":"そうです。ホラー映画や推理小説では、かごめかごめがよく登場しますね。"},{"id":{"d":704,"n":6,"t":5},"speaker":"B","text":"私も映画でかごめかごめが使われていたのを見た記憶があります。だから、どのような遊びかを覚えているのかもしれません。"}];
INSERT INTO sentence [{"id":{"d":705,"n":1,"t":5},"speaker":"A","text":"10年くらい前に、子どもたちの間では一輪車が流行していましたね。"},{"id":{"d":705,"n":2,"t":5},"speaker":"B","text":"私も公園で一輪車に乗っている子どもたちを何度も見たことがあります。ローラースケートよりも人気があったように思います。"},{"id":{"d":705,"n":3,"t":5},"speaker":"A","text":"当時は、玄関先に一輪車が置いてある家をよく見かけたものです。そういえば、息子の小学校では、一輪車を使った授業もありました。"},{"id":{"d":705,"n":4,"t":5},"speaker":"B","text":"小学校の授業で使われていたのなら、ローラースケートやスケートボードより流行するのは当然ですね。しかし、なぜ小学校の授業で一輪車が使われたのでしょうか？"},{"id":{"d":705,"n":5,"t":5},"speaker":"A","text":"体幹やバランス感覚を育てるためではないでしょうか？"},{"id":{"d":705,"n":6,"t":5},"speaker":"B","text":"なるほど、納得です。"}];
INSERT INTO sentence [{"id":{"d":706,"n":1,"t":5},"speaker":"A","text":"昔から竹馬について、不思議に思っていることがあります。"},{"id":{"d":706,"n":2,"t":5},"speaker":"B","text":"竹馬のどこが不思議なのでしょうか？"},{"id":{"d":706,"n":3,"t":5},"speaker":"A","text":"名前です。竹馬と言いますが、馬に乗っているようにはとても見えませんよね。"},{"id":{"d":706,"n":4,"t":5},"speaker":"B","text":"そうでしょうか？例えば、背筋を伸ばして乗るところや棒を握るところは、馬に乗って手綱を握るところに似ていると思います。"},{"id":{"d":706,"n":5,"t":5},"speaker":"A","text":"そう言われてみれば、上下に動いたり、ぎこちなく乗ったりするところは似ているかもしれません。"},{"id":{"d":706,"n":6,"t":5},"speaker":"B","text":"竹馬に乗って歩く音も、馬の足音のように聞こえませんか？"},{"id":{"d":706,"n":7,"t":5},"speaker":"A","text":"なるほど。音は考えたことがありませんでした。"},{"id":{"d":706,"n":8,"t":5},"speaker":"B","text":"竹馬といえば、一時期はパイプ製の竹馬もよく見られましたね。"},{"id":{"d":706,"n":9,"t":5},"speaker":"A","text":"私の小学校にもパイプ製の竹馬が置いてありました。とても高い竹馬を器用に乗りこなしている人もいました。"}];
INSERT INTO sentence [{"id":{"d":707,"n":1,"t":5},"speaker":"A","text":"さすがにここまで暑いと外出する気になりません。"},{"id":{"d":707,"n":2,"t":5},"speaker":"B","text":"そうですね。建物の中は涼しいのですが、外が暑いので寒暖差で頭がくらくらするときがあります。"},{"id":{"d":707,"n":3,"t":5},"speaker":"A","text":"今日は外出をやめて、エアコンの効いた部屋で読書を楽しむことにします。"},{"id":{"d":707,"n":4,"t":5},"speaker":"B","text":"自宅での読書はいいですね。"},{"id":{"d":707,"n":5,"t":5},"speaker":"A","text":"自宅でもくつろげるハンモックを購入したので、そこで読書をします。"},{"id":{"d":707,"n":6,"t":5},"speaker":"B","text":"読書のためにわざわざハンモックを買ったのですか？"},{"id":{"d":707,"n":7,"t":5},"speaker":"A","text":"はい。簡易ベッドの代わりに買ったのですが、居心地が良いのです。休日の午後はそこで読書と昼寝をします。"},{"id":{"d":707,"n":8,"t":5},"speaker":"B","text":"普通のベッドやソファよりもハンモックの方がいいですか？"},{"id":{"d":707,"n":9,"t":5},"speaker":"A","text":"はい。ベッドやソファは背中に熱がたまりますが、ハンモックは下が空間なので熱がたまりにくく、とても快適です。"},{"id":{"d":707,"n":10,"t":5},"speaker":"B","text":"なるほど。そのような利点もあるのですね。"},{"id":{"d":707,"n":11,"t":5},"speaker":"A","text":"それに場所も取りませんしすぐに片づけられるので、自立式のハンモックは大変便利です。"}];
INSERT INTO sentence [{"id":{"d":708,"n":1,"t":5},"speaker":"A","text":"ライトノベルを読む人の多くは、電子書籍で購入するのでしょうか？"},{"id":{"d":708,"n":2,"t":5},"speaker":"B","text":"どうでしょうか。私は紙媒体でライトノベルを買っていますよ。私の周りでも、紙で購入する人が多い印象です。"},{"id":{"d":708,"n":3,"t":5},"speaker":"A","text":"そうなのですね。何巻も続く作品が多いので、電子書籍の方が保管しやすいのかなと思っていました。"},{"id":{"d":708,"n":4,"t":5},"speaker":"B","text":"ライトノベル愛好者の中には挿絵に引かれている人もいます。それもあって、紙で保管したいと考える人が多いのかもしれません。"},{"id":{"d":708,"n":5,"t":5},"speaker":"A","text":"確かに、ライトノベルの表紙には、著者の名前とイラストレーターの名前が併記してあるものが多く見られますね。本で購入していると、本棚がすぐにいっぱいになってしまうのではありませんか？"},{"id":{"d":708,"n":6,"t":5},"speaker":"B","text":"その通りです。すぐに本棚がいっぱいになってしまいます。"},{"id":{"d":708,"n":7,"t":5},"speaker":"A","text":"イラストにも価値を覚えるとのことでしたので、たとえ本棚がいっぱいになっても、購入したライトノベルを捨てたり売ったりすることはないのでしょうか？"},{"id":{"d":708,"n":8,"t":5},"speaker":"B","text":"もちろんです。ライトノベルを捨てるなど考えられません。"}];
INSERT INTO sentence [{"id":{"d":709,"n":1,"t":5},"speaker":"A","text":"趣味でバンド活動をされているそうですが、楽器は何を担当されているのですか？"},{"id":{"d":709,"n":2,"t":5},"speaker":"B","text":"ドラムとパーカッションです。ラテン音楽のバンドなので、パーカッションではさまざま楽器を担当しています。"},{"id":{"d":709,"n":3,"t":5},"speaker":"A","text":"ラテン音楽ではどのような打楽器を演奏しているのですか？"},{"id":{"d":709,"n":4,"t":5},"speaker":"B","text":"スティールパン、クィーカ、ティンバレス、コンガ、マラカスなどです。"},{"id":{"d":709,"n":5,"t":5},"speaker":"A","text":"コンガとマラカスしか分かりませんでした。スティールパンとは何ですか？"},{"id":{"d":709,"n":6,"t":5},"speaker":"B","text":"ドラム缶の底部をたたいてへこませ、音階を出せるようにした楽器です。"},{"id":{"d":709,"n":7,"t":5},"speaker":"A","text":"クィーカやティンバレスも一般的なラテン楽器なのですか？"},{"id":{"d":709,"n":8,"t":5},"speaker":"B","text":"どちらも南米では一般的な楽器です。「クィーカ」という音が出る楽器がクィーカ、小太鼓が二つ並ぶのがティンバレスです。"},{"id":{"d":709,"n":9,"t":5},"speaker":"A","text":"演奏の時に、それだけたくさんの打楽器を運ぶのは大変そうですね。"},{"id":{"d":709,"n":10,"t":5},"speaker":"B","text":"そうですね。でも、練習場所は近所ですし、演奏も公民館や町内会の夏祭りくらいなので自分の車で運べます。"}];
INSERT INTO sentence [{"id":{"d":710,"n":1,"t":5},"speaker":"A","text":"以前にテレビ番組でスティールドラムを演奏している人を見ました。スティールパンは、スティールドラムと同じものですか？"},{"id":{"d":710,"n":2,"t":5},"speaker":"B","text":"はい。スティールパンはスティールドラムと同じものです。"},{"id":{"d":710,"n":3,"t":5},"speaker":"A","text":"スティールドラムは手作業で作られるので、量産が難しいと聞きました。"},{"id":{"d":710,"n":4,"t":5},"speaker":"B","text":"そうなのですね。実は、スティールパンを自分で作っている人と出会って、その人の作品を買ったことがあります。"},{"id":{"d":710,"n":5,"t":5},"speaker":"A","text":"スティールドラムを自作している人がいるのですか？"},{"id":{"d":710,"n":6,"t":5},"speaker":"B","text":"都内の公園を歩いていたら、スティールパンを演奏する大道芸人がいたのです。話しかけたら、自作の楽器を売って生活しているとのことでした。"},{"id":{"d":710,"n":7,"t":5},"speaker":"A","text":"スティールドラムの製作で暮らしを立てていけるのですか？"},{"id":{"d":710,"n":8,"t":5},"speaker":"B","text":"それは分かりませんが、スティールドラム以外にも他の民族楽器をいろいろと作っているそうでした。大道芸人としての活動は、楽器のデモンストレーションを兼ねているそうです。"},{"id":{"d":710,"n":9,"t":5},"speaker":"A","text":"そのデモンストレーションを見て、民族楽器を買っていく人がいるのですね。"},{"id":{"d":710,"n":10,"t":5},"speaker":"B","text":"はい。音楽大学の学生などの専門の方は、民族楽器を物珍しがって高値で買ってくれるそうです。"}];
INSERT INTO sentence [{"id":{"d":711,"n":1,"t":5},"speaker":"A","text":"今度、パラグライダーに乗ってこようと思っています。"},{"id":{"d":711,"n":2,"t":5},"speaker":"B","text":"パラグライダーとはどのようなものですか？"},{"id":{"d":711,"n":3,"t":5},"speaker":"A","text":"四角いパラシュート状のもので空を滑空する、スカイスポーツの一種です。"},{"id":{"d":711,"n":4,"t":5},"speaker":"B","text":"危険に思えるスポーツですが、あなたは何度かパラグライダーを経験しているのですか？"},{"id":{"d":711,"n":5,"t":5},"speaker":"A","text":"いいえ、今回が初めてです。今回私が行く所は、パラグライダーの基本操作から練習できるので、安全にパラグライダーを体験できるそうです。"},{"id":{"d":711,"n":6,"t":5},"speaker":"B","text":"私は高所恐怖症なのでスカイスポーツの魅力が分からないのですが、なぜあなたはパラグライダーをしたいのですか？"},{"id":{"d":711,"n":7,"t":5},"speaker":"A","text":"スカイスポーツ全般に興味があるのですが、他のスカイスポーツと比べて、パラグライダーは比較的安価かつ簡単に体験できるからです。"},{"id":{"d":711,"n":8,"t":5},"speaker":"B","text":"確かに、スカイスポーツはお金がかかりそうなスポーツですね。体験できる場所は近くにあるのですか？"},{"id":{"d":711,"n":9,"t":5},"speaker":"A","text":"はい。郊外の丘陵地帯で行われているので、車があればすぐに行くことができます。"}];
INSERT INTO sentence [{"id":{"d":712,"n":1,"t":5},"speaker":"A","text":"映画でビルから人が飛んでいるシーンを見たのですが、あのようなことが本当にできるのですか？"},{"id":{"d":712,"n":2,"t":5},"speaker":"B","text":"そのようなシーンを見たことがないのでイメージが湧かないのですが、ハンググライダーやパラグライダーを使って飛ぶのですか？"},{"id":{"d":712,"n":3,"t":5},"speaker":"A","text":"いいえ、手と足にひれのようなものを着けてムササビのように飛びます。"},{"id":{"d":712,"n":4,"t":5},"speaker":"B","text":"なるほど、ウイングスーツのことですね。新しいスカイダイビングとも言われています。実際には飛行許可は出ないでしょうが、技術的にはウイングスーツでビルから飛ぶことは可能です。"},{"id":{"d":712,"n":5,"t":5},"speaker":"A","text":"とても危険な行為に思えるのですが、ウイングスーツで飛ぶためのライセンスなどはあるのでしょうか？"},{"id":{"d":712,"n":6,"t":5},"speaker":"B","text":"ライセンスがあるかどうかは分かりませんが、恐らくウイングスーツを使用できる人はベテランのスカイダイバーだけだと思います。"},{"id":{"d":712,"n":7,"t":5},"speaker":"A","text":"スピードもかなり出ているように見えましたが、どうやって着陸するのでしょう？"},{"id":{"d":712,"n":8,"t":5},"speaker":"B","text":"小型のパラシュートを装着しているので、それを使えば減速して着陸できると思いますよ。"},{"id":{"d":712,"n":9,"t":5},"speaker":"A","text":"そういえば、映画でも着陸の時はパラシュートを使っていたかもしれません。"}];
INSERT INTO sentence [{"id":{"d":713,"n":1,"t":5},"speaker":"A","text":"学生時代は美術部だったそうですね。銅版画とリトグラフの違いについてご存じですか？"},{"id":{"d":713,"n":2,"t":5},"speaker":"B","text":"はい、知っていますよ。銅版画は銅板に傷をつけ、そこにインクを垂らして紙に写し取るものです。"},{"id":{"d":713,"n":3,"t":5},"speaker":"A","text":"それは木版画と工程が似ていますね。リトグラフはどのような方法ですか？"},{"id":{"d":713,"n":4,"t":5},"speaker":"B","text":"リトグラフでは吸湿性の石や金属に油性のクレヨンで絵を描き、そこにローラーで水を付け、インクを塗ります。"},{"id":{"d":713,"n":5,"t":5},"speaker":"A","text":"なるほど。クレヨンの部分にはインクが付いて、水の部分には付かないのですね。"},{"id":{"d":713,"n":6,"t":5},"speaker":"B","text":"そうです。そこに紙を置いてプレスをかけて、印刷をします。"},{"id":{"d":713,"n":7,"t":5},"speaker":"A","text":"銅版画はプレスをかけないのですか？"},{"id":{"d":713,"n":8,"t":5},"speaker":"B","text":"いいえ、銅版画もリトグラフもプレス機を使いますよ。"},{"id":{"d":713,"n":9,"t":5},"speaker":"A","text":"では、どちらも家で作業するにはプレス機が必要ということですね。"},{"id":{"d":713,"n":10,"t":5},"speaker":"B","text":"はい。銅版画やリトグラフを趣味にするなら、まず工房のカルチャースクールに通ってみてはいかがでしょうか？"},{"id":{"d":713,"n":11,"t":5},"speaker":"A","text":"そうですね。調べてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":714,"n":1,"t":5},"speaker":"A","text":"パステル画を描くことが趣味だとうかがいました。どのようなところに魅力を感じていますか？"},{"id":{"d":714,"n":2,"t":5},"speaker":"B","text":"道具や材料を比較的に安価でそろえることができ、特別な技術も必要なく気軽に楽しめるところです。"},{"id":{"d":714,"n":3,"t":5},"speaker":"A","text":"パステルとは、クレヨンのようなものですか？"},{"id":{"d":714,"n":4,"t":5},"speaker":"B","text":"オイルパステルはクレヨンに似ていますね。それ以外にも、チョークのようなソフトパステル、それよりも硬いハードパステルなどの種類があります。"},{"id":{"d":714,"n":5,"t":5},"speaker":"A","text":"パステルにも種類があるのですね。パステル画を描くために、他に必要なものはありますか？"},{"id":{"d":714,"n":6,"t":5},"speaker":"B","text":"消しゴム、カッター、マスキングテープなどの日用品を使います。定着材は購入する必要があります。"},{"id":{"d":714,"n":7,"t":5},"speaker":"A","text":"定着材はいくらくらいするのですか？"},{"id":{"d":714,"n":8,"t":5},"speaker":"B","text":"定着剤は、1,000円前後で購入できます。"},{"id":{"d":714,"n":9,"t":5},"speaker":"A","text":"パステル画で使う紙は、どのようなものがいいのですか？"},{"id":{"d":714,"n":10,"t":5},"speaker":"B","text":"専門のパステル用紙もありますが、水彩紙や木炭紙、画用紙などもパステル画に使うことができます。"}];
INSERT INTO sentence [{"id":{"d":715,"n":1,"t":5},"speaker":"A","text":"最近、ハープの練習をされているそうですね。"},{"id":{"d":715,"n":2,"t":5},"speaker":"B","text":"はい。でも、ハープといっても、座って演奏するあのハープではありませんよ。"},{"id":{"d":715,"n":3,"t":5},"speaker":"A","text":"クラシック音楽で使われる、弦を指で弾くあの楽器ではないのですか？"},{"id":{"d":715,"n":4,"t":5},"speaker":"B","text":"はい。私が練習しているのはブルースハープといって、ブルース音楽で使うハーモニカです。"},{"id":{"d":715,"n":5,"t":5},"speaker":"A","text":"ハーモニカのこともハープというのですね。ブルースが好きなのですか？"},{"id":{"d":715,"n":6,"t":5},"speaker":"B","text":"元々はブルースが好きだったわけではありませんが、ブルースハープを演奏している人を見て興味を持つようになりました。"},{"id":{"d":715,"n":7,"t":5},"speaker":"A","text":"ブルースハープは簡単に演奏できるものなのですか？"},{"id":{"d":715,"n":8,"t":5},"speaker":"B","text":"吹けば音は出ますから管楽器よりは簡単ですが、奥は深いと思います。"},{"id":{"d":715,"n":9,"t":5},"speaker":"A","text":"ブルースハープの奥深さとは何でしょうか？"},{"id":{"d":715,"n":10,"t":5},"speaker":"B","text":"ブルースは個性を出すことが必要なので、ただ吹いて、音を出すだけで満足してはいけないというところです。"}];
INSERT INTO sentence [{"id":{"d":716,"n":1,"t":5},"speaker":"A","text":"音楽教室に通っているそうですね。"},{"id":{"d":716,"n":2,"t":5},"speaker":"B","text":"はい、ゴスペルを歌っています。"},{"id":{"d":716,"n":3,"t":5},"speaker":"A","text":"ゴスペルという言葉は聞いたことがあるのですが、どういうものですか？"},{"id":{"d":716,"n":4,"t":5},"speaker":"B","text":"アメリカの教会で歌われる音楽のジャンルです。"},{"id":{"d":716,"n":5,"t":5},"speaker":"A","text":"教室があるほどにゴスペルは人気のある音楽なのですか？"},{"id":{"d":716,"n":6,"t":5},"speaker":"B","text":"ポピュラー音楽と同じくらい人気があると思いますよ。アメリカのポピュラー音楽はゴスペルから発展したともいわれています。"},{"id":{"d":716,"n":7,"t":5},"speaker":"A","text":"そうなのですね。教会音楽では、大勢で歌うのでしょうか？"},{"id":{"d":716,"n":8,"t":5},"speaker":"B","text":"はい。音楽教室では10人ほどでゴスペルを歌っています。"},{"id":{"d":716,"n":9,"t":5},"speaker":"A","text":"英語の歌を歌うのですか？"},{"id":{"d":716,"n":10,"t":5},"speaker":"B","text":"英語の歌が中心ですが、日本語の歌も歌います。英語で歌えるようになることも夢でしたから、教室は楽しいですよ。"}];
INSERT INTO sentence [{"id":{"d":717,"n":1,"t":5},"speaker":"A","text":"田島さんは良い買い物をしたと感じるときはありますか？"},{"id":{"d":717,"n":2,"t":5},"speaker":"B","text":"私は昔の音楽作品がボックス仕様で再販売されるとつい購入してしまうのですが、今まで買ったものはどれも満足できるものでした。"},{"id":{"d":717,"n":3,"t":5},"speaker":"A","text":"ボックスにはその作品以外にも、同じ曲の別録音やデモテープ、当時のライブ音源などが入っていることが多いですね。それは確かに良い買い物といえそうです。"},{"id":{"d":717,"n":4,"t":5},"speaker":"B","text":"過去のライブのロゴ入りタオルやシャツなど、今では手に入らない貴重なグッズが入っていることもあります。"},{"id":{"d":717,"n":5,"t":5},"speaker":"A","text":"再販売品だけあって特色がありますね。そこまで凝っていたら、とても手放す気にはなれませんね。"},{"id":{"d":717,"n":6,"t":5},"speaker":"B","text":"もちろんです。ずっと大事にしたいと思っています。"},{"id":{"d":717,"n":7,"t":5},"speaker":"A","text":"別録音やミックス違いの音源を聴いても私には同じ曲としか思えず、今まではボックス仕様の再販売の意義が分かりませんでしたが、アーティストの軌跡を知ることができるわけですね。。"},{"id":{"d":717,"n":8,"t":5},"speaker":"B","text":"別録音が多いと、デモテープからどのようにヒット曲まで作り上げられていくのかが分かり、興味深いです。"},{"id":{"d":717,"n":9,"t":5},"speaker":"A","text":"それは興味深いですね。"},{"id":{"d":717,"n":10,"t":5},"speaker":"B","text":"もう解散したメンバーたちが楽しそうに演奏している音源を聴くと、私は涙が出そうになります。"}];
INSERT INTO sentence [{"id":{"d":718,"n":1,"t":5},"speaker":"A","text":"最近、電動キックボードを購入したそうですね。"},{"id":{"d":718,"n":2,"t":5},"speaker":"B","text":"はい。歩くには少し長い距離も電動キックボードで移動できるようになり、とても楽です。"},{"id":{"d":718,"n":3,"t":5},"speaker":"A","text":"キックボードであちこちに行けるようになれば、楽しみが増えますね。"},{"id":{"d":718,"n":4,"t":5},"speaker":"B","text":"でも、私はよく知らない一般道をキックボードで走ることはお勧めできません。"},{"id":{"d":718,"n":5,"t":5},"speaker":"A","text":"どうしてですか？"},{"id":{"d":718,"n":6,"t":5},"speaker":"B","text":"キックボードは車輪が小さく、道路に凹凸があると転びやすいのです。特に初めての道を走るときには普段以上に注意が必要です。"},{"id":{"d":718,"n":7,"t":5},"speaker":"A","text":"そうなのですね。あなたは普段キックボードをどのように使っているのですか？"},{"id":{"d":718,"n":8,"t":5},"speaker":"B","text":"私はキックボードを車に積んでいます。目的地の近くに駐車スペースを確保できないとき、少し離れた駐車場所から目的地までの移動にキックボードを使います。"},{"id":{"d":718,"n":9,"t":5},"speaker":"A","text":"なるほど。キックボードは確かに自転車より持ち運びが楽ですし、便利な使い方ですね。"}];
INSERT INTO sentence [{"id":{"d":719,"n":1,"t":5},"speaker":"A","text":"民俗音楽を演奏するサークルに参加されているそうですね。"},{"id":{"d":719,"n":2,"t":5},"speaker":"B","text":"はい。フォルクローレが好きでCDを集めていましたが、私も演奏してみたいと思いサークルに加入しました。"},{"id":{"d":719,"n":3,"t":5},"speaker":"A","text":"楽器は何を担当しているのですか？"},{"id":{"d":719,"n":4,"t":5},"speaker":"B","text":"フォルクローレを演奏するときは、ケーナを演奏します。"},{"id":{"d":719,"n":5,"t":5},"speaker":"A","text":"ケーナはフォルクローレで使われる縦笛ですね。ケーナの材質は竹ですか？"},{"id":{"d":719,"n":6,"t":5},"speaker":"B","text":"はい。元々はアシで作っていたようですが、今は竹や木で作ります。"},{"id":{"d":719,"n":7,"t":5},"speaker":"A","text":"普段はどのように楽器を練習しているのですか？"},{"id":{"d":719,"n":8,"t":5},"speaker":"B","text":"私は譜面が読めないので、耳で聞いて曲の音階に合っていそうなケーナを選んでいます。"},{"id":{"d":719,"n":9,"t":5},"speaker":"A","text":"音階によってケーナを使い分けるのですか？"},{"id":{"d":719,"n":10,"t":5},"speaker":"B","text":"はい。高音専用をケニージャ、低音専用をケナーチョと言います。通常サイズのケーナが一般的に使われますが、曲によっては他の二つを使う場合もあります。"}];
INSERT INTO sentence [{"id":{"d":720,"n":1,"t":5},"speaker":"A","text":"民俗音楽といえば、私はケルト音楽を思い出します。"},{"id":{"d":720,"n":2,"t":5},"speaker":"B","text":"私の民俗音楽サークルではケルト音楽も演奏しますよ。"},{"id":{"d":720,"n":3,"t":5},"speaker":"A","text":"あなたは何の楽器を担当されているのですか？"},{"id":{"d":720,"n":4,"t":5},"speaker":"B","text":"ティン・ホイッスルという金属製の小さな縦笛を担当しています。"},{"id":{"d":720,"n":5,"t":5},"speaker":"A","text":"ケルト音楽の高音の笛の音といえば、ティン・ホイッスルですね。ちなみにサークルの中でバグパイプを演奏する人はいるのですか？"},{"id":{"d":720,"n":6,"t":5},"speaker":"B","text":"ケルト音楽にバグパイプは欠かせません。私のサークルにももちろんバグパイプ奏者はいます。"},{"id":{"d":720,"n":7,"t":5},"speaker":"A","text":"確かアコーディオンもよく使われていますよね。演奏が難しそうな楽器がケルト音楽には数多く登場するように思います。"},{"id":{"d":720,"n":8,"t":5},"speaker":"B","text":"私は音楽経験がありませんが、他のメンバーは音楽的な素養のある方ばかりです。難しい曲もありますが楽しみながら合奏しています。"},{"id":{"d":720,"n":9,"t":5},"speaker":"A","text":"すてきですね。普段、練習はどこでされているのですか？"},{"id":{"d":720,"n":10,"t":5},"speaker":"B","text":"合奏の時はスタジオを借ります。個人練習ではカラオケ店を使っている人が多いようです。"}];
INSERT INTO sentence [{"id":{"d":721,"n":1,"t":5},"speaker":"A","text":"今年の夏は、星の観察会に参加しようと思っています。"},{"id":{"d":721,"n":2,"t":5},"speaker":"B","text":"どこまで行かれるのですか？"},{"id":{"d":721,"n":3,"t":5},"speaker":"A","text":"長野の上高地です。"},{"id":{"d":721,"n":4,"t":5},"speaker":"B","text":"上高地ですか、涼しそうでいいですね。"},{"id":{"d":721,"n":5,"t":5},"speaker":"A","text":"夜は真夏でも防寒具が要るほどだと聞きました。"},{"id":{"d":721,"n":6,"t":5},"speaker":"B","text":"もしかして、先日に私が買ったヤッケを貸してほしいのですか？"},{"id":{"d":721,"n":7,"t":5},"speaker":"A","text":"ご名答です。貸していただけますか？"},{"id":{"d":721,"n":8,"t":5},"speaker":"B","text":"はい、どうぞ。風邪をひかないように楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":722,"n":1,"t":5},"speaker":"A","text":"エコバッグをたくさん作ったので、あなたにもお一つ差し上げます。どうぞ。"},{"id":{"d":722,"n":2,"t":5},"speaker":"B","text":"うれしいです。ありがとうございます。"},{"id":{"d":722,"n":3,"t":5},"speaker":"A","text":"ミシンの講習会で習ったのですが、熱中してしまってつい作り過ぎました。"},{"id":{"d":722,"n":4,"t":5},"speaker":"B","text":"とてもきれいにできていますね。"},{"id":{"d":722,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。講習会で紹介されて、つい勢いで買ってしまった新品のミシンで作ったかいがありました。"},{"id":{"d":722,"n":6,"t":5},"speaker":"B","text":"それは大丈夫なのですか？ミシンの詐欺ではありませんよね？"},{"id":{"d":722,"n":7,"t":5},"speaker":"A","text":"一般的な値段でしたので、心配ありません。"},{"id":{"d":722,"n":8,"t":5},"speaker":"B","text":"それなら良かったです。また何か作ったら見せてくださいね。"}];
INSERT INTO sentence [{"id":{"d":723,"n":1,"t":5},"speaker":"A","text":"今度、私の弟がテレビ番組に出演するのでよければぜひ見てください。"},{"id":{"d":723,"n":2,"t":5},"speaker":"B","text":"何の番組に出演されるのですか？"},{"id":{"d":723,"n":3,"t":5},"speaker":"A","text":"確か『捜査一課長』というドラマだったと思います。"},{"id":{"d":723,"n":4,"t":5},"speaker":"B","text":"木曜日にやっているドラマですよね。どのような役で出演されるのですか？"},{"id":{"d":723,"n":5,"t":5},"speaker":"A","text":"死体役の人の友達の友達の役だそうです。"},{"id":{"d":723,"n":6,"t":5},"speaker":"B","text":"せりふはあるのですか？"},{"id":{"d":723,"n":7,"t":5},"speaker":"A","text":"一言だけあるそうです。"},{"id":{"d":723,"n":8,"t":5},"speaker":"B","text":"一言でもせりふがあるなんて、素晴らしいです。絶対に見ますね。"}];
INSERT INTO sentence [{"id":{"d":724,"n":1,"t":5},"speaker":"A","text":"今日は雨が降っているのに、出かけるのですか？"},{"id":{"d":724,"n":2,"t":5},"speaker":"B","text":"雨が降っているといっても、小雨で風もそれほど強くないので大丈夫ですよ。"},{"id":{"d":724,"n":3,"t":5},"speaker":"A","text":"植物園に行くのでしたっけ？"},{"id":{"d":724,"n":4,"t":5},"speaker":"B","text":"はい。ショウブが見頃らしいので、それを見てきます。"},{"id":{"d":724,"n":5,"t":5},"speaker":"A","text":"雨にぬれたショウブも良いかもしれませんね。"},{"id":{"d":724,"n":6,"t":5},"speaker":"B","text":"写真をたくさん撮ってきますね。"},{"id":{"d":724,"n":7,"t":5},"speaker":"A","text":"楽しみにしています。もし雨にぬれたら拭けるように、タオルを忘れないでくださいね。"},{"id":{"d":724,"n":8,"t":5},"speaker":"B","text":"忘れていました。部屋から取ってきます。"}];
INSERT INTO sentence [{"id":{"d":725,"n":1,"t":5},"speaker":"A","text":"この間、テレビ番組を見ていたら、海外の家庭でクリスマスツリーの飾りを作っていました。"},{"id":{"d":725,"n":2,"t":5},"speaker":"B","text":"ツリーの飾りを手作りしていたのですか？"},{"id":{"d":725,"n":3,"t":5},"speaker":"A","text":"はい。驚いたのは、ツリーの飾りとしてクッキーを焼いていたことです。"},{"id":{"d":725,"n":4,"t":5},"speaker":"B","text":"食べるためではなく、飾るためにクッキーを作っていたということですか？"},{"id":{"d":725,"n":5,"t":5},"speaker":"A","text":"はい。硬めにクッキーを焼いて、穴を開けてひもを通して飾っていました。"},{"id":{"d":725,"n":6,"t":5},"speaker":"B","text":"そのクッキーをクリスマスに食べるのですか？"},{"id":{"d":725,"n":7,"t":5},"speaker":"A","text":"私もそれが気になっています。"},{"id":{"d":725,"n":8,"t":5},"speaker":"B","text":"そこまでは映像がなかったのですね。"}];
INSERT INTO sentence [{"id":{"d":726,"n":1,"t":5},"speaker":"A","text":"今年の敬老の日は何日でしたか？"},{"id":{"d":726,"n":2,"t":5},"speaker":"B","text":"敬老の日は、毎年9月15日ではないのですか？"},{"id":{"d":726,"n":3,"t":5},"speaker":"A","text":"いつだったかに法律が変わって、毎年違う日に移動するようになったのではありませんか？"},{"id":{"d":726,"n":4,"t":5},"speaker":"B","text":"そうなのですか？"},{"id":{"d":726,"n":5,"t":5},"speaker":"A","text":"カレンダーはどこにありますか？"},{"id":{"d":726,"n":6,"t":5},"speaker":"B","text":"ここにあります。"},{"id":{"d":726,"n":7,"t":5},"speaker":"A","text":"ほら、今年の敬老の日は9月19日です。"},{"id":{"d":726,"n":8,"t":5},"speaker":"B","text":"本当ですね。うっかりしていました。"}];
INSERT INTO sentence [{"id":{"d":727,"n":1,"t":5},"speaker":"A","text":"今年の敬老の日には何かするのですか？"},{"id":{"d":727,"n":2,"t":5},"speaker":"B","text":"老人ホームに入っている祖父に贈り物をしようと思っています。"},{"id":{"d":727,"n":3,"t":5},"speaker":"A","text":"いいですね、何を贈るのですか？"},{"id":{"d":727,"n":4,"t":5},"speaker":"B","text":"それが頭の痛い問題なのです。何にしたらよいと思いますか？"},{"id":{"d":727,"n":5,"t":5},"speaker":"A","text":"おじいさまのご趣味に合わせたものはいかがですか？"},{"id":{"d":727,"n":6,"t":5},"speaker":"B","text":"以前は囲碁が趣味だったのですが、老人ホームでは対戦相手がいないそうです。"},{"id":{"d":727,"n":7,"t":5},"speaker":"A","text":"それは残念ですね。"},{"id":{"d":727,"n":8,"t":5},"speaker":"B","text":"敬老の日まではあと3週間あるので、もうしばらく考えます。"}];
INSERT INTO sentence [{"id":{"d":728,"n":1,"t":5},"speaker":"A","text":"明日は、明治神宮に初詣に行きませんか？"},{"id":{"d":728,"n":2,"t":5},"speaker":"B","text":"私は初詣にはもう行ってしまいました。"},{"id":{"d":728,"n":3,"t":5},"speaker":"A","text":"明治神宮に行かれたのですか？"},{"id":{"d":728,"n":4,"t":5},"speaker":"B","text":"いいえ、近所の小さな神社に行きました。"},{"id":{"d":728,"n":5,"t":5},"speaker":"A","text":"もう一度、大きな神社へも行きませんか？あなたと一緒に初詣に行きたいのです。"},{"id":{"d":728,"n":6,"t":5},"speaker":"B","text":"分かりました、それではお付き合いします。それにしても随分遅い初詣ですね。"},{"id":{"d":728,"n":7,"t":5},"speaker":"A","text":"明治神宮は混みますから、私はいつも1月中旬に行くのです。"},{"id":{"d":728,"n":8,"t":5},"speaker":"B","text":"なるほど。明日は良いお天気だといいですね。"}];
INSERT INTO sentence [{"id":{"d":729,"n":1,"t":5},"speaker":"A","text":"お母さん、ふみちゃんと「あつまれどうぶつの森」をしていたら、お誕生日会に呼ばれました。"},{"id":{"d":729,"n":2,"t":5},"speaker":"B","text":"それは楽しみですね。お誕生日会はいつですか？"},{"id":{"d":729,"n":3,"t":5},"speaker":"A","text":"「日曜日におうちに来てください」と言われました。"},{"id":{"d":729,"n":4,"t":5},"speaker":"B","text":"ところで、おうちというのは「あつまれどうぶつの森」のおうちのことですか？それともふみちゃんの本当のおうちですか？"},{"id":{"d":729,"n":5,"t":5},"speaker":"A","text":"分かりません。"},{"id":{"d":729,"n":6,"t":5},"speaker":"B","text":"ふみちゃんにもう一度聞いてみてもらえますか？"},{"id":{"d":729,"n":7,"t":5},"speaker":"A","text":"はい。ゲームの中のおうちなのか、ふみちゃんのおうちなのか聞いてみます。"},{"id":{"d":729,"n":8,"t":5},"speaker":"B","text":"あとでふみちゃんのお母さんにも確認しておきましょう。"}];
INSERT INTO sentence [{"id":{"d":730,"n":1,"t":5},"speaker":"A","text":"ふみちゃん、お誕生日おめでとうございます。"},{"id":{"d":730,"n":2,"t":5},"speaker":"B","text":"かなちゃん、ありがとうございます。"},{"id":{"d":730,"n":3,"t":5},"speaker":"A","text":"これをどうぞ。お母さんと一緒にクッキーを作りました。"},{"id":{"d":730,"n":4,"t":5},"speaker":"B","text":"箱を開けてもよいですか？"},{"id":{"d":730,"n":5,"t":5},"speaker":"A","text":"もちろんです。開けてください。"},{"id":{"d":730,"n":6,"t":5},"speaker":"B","text":"「あつまれどうぶつの森」のキャラクターのクッキーですね。かわいいです。"},{"id":{"d":730,"n":7,"t":5},"speaker":"A","text":"そうです。"},{"id":{"d":730,"n":8,"t":5},"speaker":"B","text":"これはしずえですね？"},{"id":{"d":730,"n":9,"t":5},"speaker":"A","text":"はい。しずえは難しかったのでお母さんが作りました。私はたぬきちを作りました。"},{"id":{"d":730,"n":10,"t":5},"speaker":"B","text":"たぬきちもかわいいです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":731,"n":1,"t":5},"speaker":"A","text":"先日は、娘の誕生日にかわいいクッキーをありがとうございました。"},{"id":{"d":731,"n":2,"t":5},"speaker":"B","text":"急なお呼ばれでしたから、クッキーくらいしか用意できなくてごめんなさい。"},{"id":{"d":731,"n":3,"t":5},"speaker":"A","text":"とんでもありません。あのようにかわいらしいものを作っていただいて娘も大喜びしていました。"},{"id":{"d":731,"n":4,"t":5},"speaker":"B","text":"趣味でクッキーをよく作っておりますので、喜んでいただけて何よりです。"},{"id":{"d":731,"n":5,"t":5},"speaker":"A","text":"かなちゃんも一緒に作られたのですか？"},{"id":{"d":731,"n":6,"t":5},"speaker":"B","text":"そうです。かなと一緒に作りました。"},{"id":{"d":731,"n":7,"t":5},"speaker":"A","text":"とても上手にできていて驚きました。お店で売ってほしいくらいです。"},{"id":{"d":731,"n":8,"t":5},"speaker":"B","text":"それは言い過ぎですよ。でも、うれしいお言葉をどうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":732,"n":1,"t":5},"speaker":"A","text":"来週の火曜日に、サークルでカラオケ大会をするのです。楽しみで仕方がありません。"},{"id":{"d":732,"n":2,"t":5},"speaker":"B","text":"麗華さんはカラオケが好きですものね。"},{"id":{"d":732,"n":3,"t":5},"speaker":"A","text":"はい、大好きです。"},{"id":{"d":732,"n":4,"t":5},"speaker":"B","text":"大会ということは優劣をつけるのですか？"},{"id":{"d":732,"n":5,"t":5},"speaker":"A","text":"そうです。カラオケの機械で点数が出るでしょう？"},{"id":{"d":732,"n":6,"t":5},"speaker":"B","text":"はい。そういう機械もありますね。"},{"id":{"d":732,"n":7,"t":5},"speaker":"A","text":"あれで順位をつけるようです。"},{"id":{"d":732,"n":8,"t":5},"speaker":"B","text":"なるほど。「目指せ100点！」ですね。"}];
INSERT INTO sentence [{"id":{"d":733,"n":1,"t":5},"speaker":"A","text":"サークルのカラオケ大会で3位でした。"},{"id":{"d":733,"n":2,"t":5},"speaker":"B","text":"おめでとうございます。何人が参加したのですか？"},{"id":{"d":733,"n":3,"t":5},"speaker":"A","text":"全部で21人でした。"},{"id":{"d":733,"n":4,"t":5},"speaker":"B","text":"21人中で3位とはすごいですね。"},{"id":{"d":733,"n":5,"t":5},"speaker":"A","text":"1位が2人いて、私はその次の3位でした。"},{"id":{"d":733,"n":6,"t":5},"speaker":"B","text":"同率1位だったのですか？"},{"id":{"d":733,"n":7,"t":5},"speaker":"A","text":"はい。2人とも100点だったのです。"},{"id":{"d":733,"n":8,"t":5},"speaker":"B","text":"その次で3位なら本当にすばらしいですよ。おめでとうございます。"}];
INSERT INTO sentence [{"id":{"d":734,"n":1,"t":5},"speaker":"A","text":"ケーキをごちそうしてくれて、ありがとうございます。"},{"id":{"d":734,"n":2,"t":5},"speaker":"B","text":"カラオケ大会入賞のお祝いですよ。"},{"id":{"d":734,"n":3,"t":5},"speaker":"A","text":"本当は私が落ち込んでいたから慰めてくれたのでしょう？"},{"id":{"d":734,"n":4,"t":5},"speaker":"B","text":"はい。100点が取れなかった、と言ってしょんぼりしていましたからね。"},{"id":{"d":734,"n":5,"t":5},"speaker":"A","text":"気を使わせてしまってすみません。100点を取るのは本当に難しいですね。"},{"id":{"d":734,"n":6,"t":5},"speaker":"B","text":"そうですよ、98点も胸を張れる点数です。"},{"id":{"d":734,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。ケーキとあなたの言葉で元気が出てきました。"},{"id":{"d":734,"n":8,"t":5},"speaker":"B","text":"それはよかったです。"}];
INSERT INTO sentence [{"id":{"d":735,"n":1,"t":5},"speaker":"A","text":"来月は久しぶりに映画館へ映画を見に行こうと思います。"},{"id":{"d":735,"n":2,"t":5},"speaker":"B","text":"最近はDVDばかり見ていたのに、どういう風の吹き回しですか？"},{"id":{"d":735,"n":3,"t":5},"speaker":"A","text":"大好きな漫画が映画化されるのです。"},{"id":{"d":735,"n":4,"t":5},"speaker":"B","text":"DVDになるのを待たないのですか？"},{"id":{"d":735,"n":5,"t":5},"speaker":"A","text":"今回は待ちきれません。"},{"id":{"d":735,"n":6,"t":5},"speaker":"B","text":"漫画の実写化は自分のイメージと違ってがっかりすることもありますよ。"},{"id":{"d":735,"n":7,"t":5},"speaker":"A","text":"このキャスティングを見てください。これならがっかりすることはあり得ません。"},{"id":{"d":735,"n":8,"t":5},"speaker":"B","text":"これはすばらしいですね。これなら私も見たいです。"},{"id":{"d":735,"n":9,"t":5},"speaker":"A","text":"そうでしょう。一緒に見に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":736,"n":1,"t":5},"speaker":"A","text":"歌舞伎教室のチケットがあるのですが、一緒に行きませんか？"},{"id":{"d":736,"n":2,"t":5},"speaker":"B","text":"歌舞伎教室ですか？何だか難しそうです。"},{"id":{"d":736,"n":3,"t":5},"speaker":"A","text":"中学生向けの分かりやすい解説もあるそうですよ。"},{"id":{"d":736,"n":4,"t":5},"speaker":"B","text":"そうですか。でも、歌舞伎にはあまり興味がないので、行くかどうか迷います。"},{"id":{"d":736,"n":5,"t":5},"speaker":"A","text":"そうなのですか？あなたはこのタレントのファンではありませんでしたか？この方のファンなら、歌舞伎に興味があると思ってお誘いしたのですが。"},{"id":{"d":736,"n":6,"t":5},"speaker":"B","text":"この方は、歌舞伎俳優なのですか？"},{"id":{"d":736,"n":7,"t":5},"speaker":"A","text":"そうですよ。知りませんでしたか？"},{"id":{"d":736,"n":8,"t":5},"speaker":"B","text":"知りませんでした。それなら歌舞伎教室に行きます。誘ってくれてありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":737,"n":1,"t":5},"speaker":"A","text":"歌舞伎教室はどうでしたか？"},{"id":{"d":737,"n":2,"t":5},"speaker":"B","text":"とても楽しかったです。"},{"id":{"d":737,"n":3,"t":5},"speaker":"A","text":"お芝居の言葉遣いもそれほど難しくなかったでしょう？"},{"id":{"d":737,"n":4,"t":5},"speaker":"B","text":"はい。もっと難しいと思っていました。"},{"id":{"d":737,"n":5,"t":5},"speaker":"A","text":"今度は歌舞伎座に鑑賞に行きたいですね。"},{"id":{"d":737,"n":6,"t":5},"speaker":"B","text":"ええ、ぜひ一度行ってみたいです。"},{"id":{"d":737,"n":7,"t":5},"speaker":"A","text":"あなたの好きな俳優が出演されているときもあるようなので、せっかくならそのときに行きましょう。"},{"id":{"d":737,"n":8,"t":5},"speaker":"B","text":"それは楽しみです。"}];
INSERT INTO sentence [{"id":{"d":738,"n":1,"t":5},"speaker":"A","text":"梅雨が明けましたね。"},{"id":{"d":738,"n":2,"t":5},"speaker":"B","text":"これから暑くなると思うと、うんざりしてしまいます。"},{"id":{"d":738,"n":3,"t":5},"speaker":"A","text":"後ろ向きに考えないで、夏を楽しみましょうよ。"},{"id":{"d":738,"n":4,"t":5},"speaker":"B","text":"今年の夏は何か予定があるのですか？"},{"id":{"d":738,"n":5,"t":5},"speaker":"A","text":"今年の夏は恋人と沢登りに行く予定です。"},{"id":{"d":738,"n":6,"t":5},"speaker":"B","text":"沢ですか？去年は海水浴によく行っていましたよね？"},{"id":{"d":738,"n":7,"t":5},"speaker":"A","text":"海好きの彼とはもう別れました。今お付き合いしている人は山好きなのです。"},{"id":{"d":738,"n":8,"t":5},"speaker":"B","text":"そうでしたか。楽しんできてくださいね。"}];
INSERT INTO sentence [{"id":{"d":739,"n":1,"t":5},"speaker":"A","text":"今年は暖冬ですね。"},{"id":{"d":739,"n":2,"t":5},"speaker":"B","text":"そうですね。今年の冬は、まだ一度もコートを着ていません。"},{"id":{"d":739,"n":3,"t":5},"speaker":"A","text":"私も短いジャケットで済んでしまっています。"},{"id":{"d":739,"n":4,"t":5},"speaker":"B","text":"そういえば、この間、かわいいコートを見つけたのですよ。"},{"id":{"d":739,"n":5,"t":5},"speaker":"A","text":"いいですね。買ってきたのですか？"},{"id":{"d":739,"n":6,"t":5},"speaker":"B","text":"これだけ暖かいと買っても着る機会がなさそうで、保留中です。"},{"id":{"d":739,"n":7,"t":5},"speaker":"A","text":"このまま暖かくて着られないともったいないですからね。"},{"id":{"d":739,"n":8,"t":5},"speaker":"B","text":"はい。でも、売り切れたらすぐに買わなかったことを後悔しそうで、ずっと悩んでいます。"}];
INSERT INTO sentence [{"id":{"d":740,"n":1,"t":5},"speaker":"A","text":"この近くにCDを売っている場所はありますか？"},{"id":{"d":740,"n":2,"t":5},"speaker":"B","text":"駅前のビルにタワーレコードが入っていたような気がします。"},{"id":{"d":740,"n":3,"t":5},"speaker":"A","text":"駅前にあるのですね。良かったです。"},{"id":{"d":740,"n":4,"t":5},"speaker":"B","text":"あなたの家の近くにもCDショップがあったと思いますが、そこでは買わないのですか？"},{"id":{"d":740,"n":5,"t":5},"speaker":"A","text":"あそこは閉店してしまいました。よく利用していた店なので、とても残念です。"},{"id":{"d":740,"n":6,"t":5},"speaker":"B","text":"最近は音楽がインターネットで手に入りますから、CDが売れなくて閉店する店も増えているようですね。"},{"id":{"d":740,"n":7,"t":5},"speaker":"A","text":"そうですね。私も最近はダウンロードで音楽を購入することが増えました。でも、今回は母に渡すものなので、CDでなければいけません。"},{"id":{"d":740,"n":8,"t":5},"speaker":"B","text":"なるほど、CDはお母様へのプレゼントなのですね。納得です。"}];
INSERT INTO sentence [{"id":{"d":741,"n":1,"t":5},"speaker":"A","text":"おかげさまでCDを買いに行けました。"},{"id":{"d":741,"n":2,"t":5},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":741,"n":3,"t":5},"speaker":"A","text":"あれからずっと母はプレゼントしたCDを繰り返し聞いていて、一緒に聞いていたら私もすっかりその曲を覚えてしまいました。"},{"id":{"d":741,"n":4,"t":5},"speaker":"B","text":"以前、あなたはその歌手には興味がないと言っていましたよね？"},{"id":{"d":741,"n":5,"t":5},"speaker":"A","text":"ええ、でも彼らの曲を聞いているうちに、少し好きになってきたかもしれません。"},{"id":{"d":741,"n":6,"t":5},"speaker":"B","text":"そうだったのですね。先ほどから鼻歌でずっと彼らの曲を歌っていたので、不思議に思っていたのです。"}];
INSERT INTO sentence [{"id":{"d":742,"n":1,"t":5},"speaker":"A","text":"トランプのカードを1枚取って、絵柄と数字を覚えてください。"},{"id":{"d":742,"n":2,"t":5},"speaker":"B","text":"はい。覚えました。"},{"id":{"d":742,"n":3,"t":5},"speaker":"A","text":"では、そのカードを好きな場所に戻して、好きなだけカードを切ってください。"},{"id":{"d":742,"n":4,"t":5},"speaker":"B","text":"はい。切りました。"},{"id":{"d":742,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。さて、私のパワーであなたが先ほど選んだカードを当てましょう。"},{"id":{"d":742,"n":6,"t":5},"speaker":"B","text":"はい。"},{"id":{"d":742,"n":7,"t":5},"speaker":"A","text":"あなたが選んだカードは、ハートの5ですね？"},{"id":{"d":742,"n":8,"t":5},"speaker":"B","text":"違います。"},{"id":{"d":742,"n":9,"t":5},"speaker":"A","text":"違うのですか？"},{"id":{"d":742,"n":10,"t":5},"speaker":"B","text":"はい、全然違います。やはり一日マジック教室に通っただけでマジックをマスターするのは難しかったようですね。"}];
INSERT INTO sentence [{"id":{"d":743,"n":1,"t":5},"speaker":"A","text":"あそこを見てください。"},{"id":{"d":743,"n":2,"t":5},"speaker":"B","text":"きれいな虹が出ていますね。"},{"id":{"d":743,"n":3,"t":5},"speaker":"A","text":"先ほどまで雨が降っていましたからね。"},{"id":{"d":743,"n":4,"t":5},"speaker":"B","text":"とてもすてきな景色です。この景色を写真に撮ったら、虹も写るでしょうか？"},{"id":{"d":743,"n":5,"t":5},"speaker":"A","text":"どうでしょうか。撮ってみたらいいと思いますよ。"},{"id":{"d":743,"n":6,"t":5},"speaker":"B","text":"スマートフォンはあちらで充電中なので、取ってきますね。"},{"id":{"d":743,"n":7,"t":5},"speaker":"A","text":"早くしないと虹が消えてしまいそうですよ。念のために私のスマートフォンで撮っておきますね。"},{"id":{"d":743,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。間に合わなかったら、後でその写真をくださいね。"}];
INSERT INTO sentence [{"id":{"d":744,"n":1,"t":5},"speaker":"A","text":"由香ちゃんはお絵描きをしているのですか？"},{"id":{"d":744,"n":2,"t":5},"speaker":"B","text":"はい、お絵描きをしています。"},{"id":{"d":744,"n":3,"t":5},"speaker":"A","text":"かわいいワンちゃんの絵ですね。"},{"id":{"d":744,"n":4,"t":5},"speaker":"B","text":"お姉さん、これはワンちゃんではありません。"},{"id":{"d":744,"n":5,"t":5},"speaker":"A","text":"猫ちゃんですか？"},{"id":{"d":744,"n":6,"t":5},"speaker":"B","text":"猫ちゃんでもありません。"},{"id":{"d":744,"n":7,"t":5},"speaker":"A","text":"では、何を描いているのですか？"},{"id":{"d":744,"n":8,"t":5},"speaker":"B","text":"私が描いているのはパパの絵です。"}];
INSERT INTO sentence [{"id":{"d":745,"n":1,"t":5},"speaker":"A","text":"この小説をご存じですか？剣と魔法の世界が舞台の冒険物で、とても面白いのですよ。"},{"id":{"d":745,"n":2,"t":5},"speaker":"B","text":"初めて見るタイトルです。あいにく私はファンタジー小説は読みません。"},{"id":{"d":745,"n":3,"t":5},"speaker":"A","text":"実は、これは優香さんが書いた小説です。"},{"id":{"d":745,"n":4,"t":5},"speaker":"B","text":"同級生の、あの優香さんですか？"},{"id":{"d":745,"n":5,"t":5},"speaker":"A","text":"そうです。彼女の才能には驚きですよ。彼女が書いたファンタジー小説でも読みませんか？"},{"id":{"d":745,"n":6,"t":5},"speaker":"B","text":"そういうことなら話は別です。その小説が読めるURLを教えてください。"}];
INSERT INTO sentence [{"id":{"d":746,"n":1,"t":5},"speaker":"A","text":"インターネット小説には面白い作品がたくさんあるのですね。"},{"id":{"d":746,"n":2,"t":5},"speaker":"B","text":"「インターネット小説は小説ではない」と言ってたあなたからそのような発言が出るとは、驚きました。何があったのですか？"},{"id":{"d":746,"n":3,"t":5},"speaker":"A","text":"食わず嫌いもよくないと思い、試しに一つ読んでみたらとても面白くて、すっかりとりこになってしまいました。"},{"id":{"d":746,"n":4,"t":5},"speaker":"B","text":"インターネット小説も良いものでしょう？インターネット小説として連載されていた作品の中には、その後、書籍化されているものもありますからね。"},{"id":{"d":746,"n":5,"t":5},"speaker":"A","text":"私が最初に読んだ作品もそのタイプでした。15巻もあったのですが、あまりの面白さに思わず全巻まとめて購入してしまいました。"},{"id":{"d":746,"n":6,"t":5},"speaker":"B","text":"それはかなりの出費ですね。"},{"id":{"d":746,"n":7,"t":5},"speaker":"A","text":"はい、高額の出費でした。しかし、後悔はしていません。すてきな作品に出会えてよかったです。"}];
INSERT INTO sentence [{"id":{"d":747,"n":1,"t":5},"speaker":"A","text":"今日は成人式なのですね。"},{"id":{"d":747,"n":2,"t":5},"speaker":"B","text":"そのようですね。振り袖を着た女性が、先ほどから窓の外を通っていきますね。"},{"id":{"d":747,"n":3,"t":5},"speaker":"A","text":"式はこの先の公民館で行われるのですか？"},{"id":{"d":747,"n":4,"t":5},"speaker":"B","text":"はい。毎年公民館で行われているようです。"},{"id":{"d":747,"n":5,"t":5},"speaker":"A","text":"式の後には、このお店でもお食事会などをするのでしょうか？"},{"id":{"d":747,"n":6,"t":5},"speaker":"B","text":"うちはお祝い事に使われるようなお店ではありませんから、そのようなことはほとんどありませんよ。"},{"id":{"d":747,"n":7,"t":5},"speaker":"A","text":"では、ゆっくりご飯を食べていても大丈夫ですね。"},{"id":{"d":747,"n":8,"t":5},"speaker":"B","text":"はい、お構いなく。着慣れない着物に疲れ果てた子がお茶を飲みに来ることはたまにありますけれどね。"}];
INSERT INTO sentence [{"id":{"d":748,"n":1,"t":5},"speaker":"A","text":"梅雨はじめじめしていて、嫌いです。"},{"id":{"d":748,"n":2,"t":5},"speaker":"B","text":"私もこの季節は好きではありません。早く梅雨が明けてほしいですね。"},{"id":{"d":748,"n":3,"t":5},"speaker":"A","text":"この間、テレビで気象予報士が言っていたのですが、梅雨が明けない年もあるそうですよ。"},{"id":{"d":748,"n":4,"t":5},"speaker":"B","text":"そのようなことがあるのですか？"},{"id":{"d":748,"n":5,"t":5},"speaker":"A","text":"はい。今までにも数回あったのだそうです。"},{"id":{"d":748,"n":6,"t":5},"speaker":"B","text":"梅雨が明けないとどうなるのですか？"},{"id":{"d":748,"n":7,"t":5},"speaker":"A","text":"夏の間はずっとじめじめして、そのまま秋になるそうです。"},{"id":{"d":748,"n":8,"t":5},"speaker":"B","text":"そのような夏は本当に嫌ですね。"}];
INSERT INTO sentence [{"id":{"d":749,"n":1,"t":5},"speaker":"A","text":"今日は寒いですね。"},{"id":{"d":749,"n":2,"t":5},"speaker":"B","text":"ええ。真冬日になるのではありませんか？"},{"id":{"d":749,"n":3,"t":5},"speaker":"A","text":"さすがに真冬日にはなりませんよ。"},{"id":{"d":749,"n":4,"t":5},"speaker":"B","text":"真冬日は最低気温が0度未満でしょう？"},{"id":{"d":749,"n":5,"t":5},"speaker":"A","text":"違いますよ。確か、最高気温が0度未満の日を真冬日というのですよ。"},{"id":{"d":749,"n":6,"t":5},"speaker":"B","text":"それは雪国に行かないと出ない数字ですね。"},{"id":{"d":749,"n":7,"t":5},"speaker":"A","text":"東京では近年はないのではないでしょうか。"}];
INSERT INTO sentence [{"id":{"d":750,"n":1,"t":5},"speaker":"A","text":"今日は何の日でしょうか？"},{"id":{"d":750,"n":2,"t":5},"speaker":"B","text":"6月1日ですから、衣替えの日です。"},{"id":{"d":750,"n":3,"t":5},"speaker":"A","text":"それも正解の一つですが、他にも答えがあります。"},{"id":{"d":750,"n":4,"t":5},"speaker":"B","text":"1日という日付ですし、さまざまな記念日が設定されていそうですね。"},{"id":{"d":750,"n":5,"t":5},"speaker":"A","text":"その通りです。私が調べただけで20以上ありました。"},{"id":{"d":750,"n":6,"t":5},"speaker":"B","text":"それほどたくさんあるのですか？"},{"id":{"d":750,"n":7,"t":5},"speaker":"A","text":"はい。その中で私のお気に入りはチューインガムの日です。はい、これをどうぞ。"},{"id":{"d":750,"n":8,"t":5},"speaker":"B","text":"チューインガムですね。大好きなのでうれしいです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":751,"n":1,"t":5},"speaker":"A","text":"昨日のドラマを見逃してしまいました。"},{"id":{"d":751,"n":2,"t":5},"speaker":"B","text":"動画配信サービスでは見られませんか？"},{"id":{"d":751,"n":3,"t":5},"speaker":"A","text":"動画配信サービスですか？"},{"id":{"d":751,"n":4,"t":5},"speaker":"B","text":"はい。見逃した番組をパソコンやスマートフォンで無料で見られるサービスです。"},{"id":{"d":751,"n":5,"t":5},"speaker":"A","text":"そのようなものがあるのですね。"},{"id":{"d":751,"n":6,"t":5},"speaker":"B","text":"全ての番組が見られるわけではありませんが、放送中のドラマは見られる場合が多いです。"},{"id":{"d":751,"n":7,"t":5},"speaker":"A","text":"そうなのですね。今すぐにでも見たいのですが、見るためには何をしたらよいでしょうか？"},{"id":{"d":751,"n":8,"t":5},"speaker":"B","text":"まずはアプリケーションを入れて、目当てのドラマが配信されているか調べてみるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":752,"n":1,"t":5},"speaker":"A","text":"動画配信サービスで、見逃した番組を見ることができました。"},{"id":{"d":752,"n":2,"t":5},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":752,"n":3,"t":5},"speaker":"A","text":"世の中は便利になっているのですね。"},{"id":{"d":752,"n":4,"t":5},"speaker":"B","text":"皆がインターネットを使う時代ですからね。"},{"id":{"d":752,"n":5,"t":5},"speaker":"A","text":"今回はどうしても見たかったので利用しましたが、データ通信量が心配です。"},{"id":{"d":752,"n":6,"t":5},"speaker":"B","text":"Wi-Fiを使わなかったのですか？"},{"id":{"d":752,"n":7,"t":5},"speaker":"A","text":"はい。Wi-Fiがない場所で見てしまいました。"},{"id":{"d":752,"n":8,"t":5},"speaker":"B","text":"動画はデータ通信量が多いので、今度からはWi-Fiが利用できる場所で見ることをお勧めします。"},{"id":{"d":752,"n":9,"t":5},"speaker":"A","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":753,"n":1,"t":5},"speaker":"A","text":"祖母が入所している老人ホームから、節分の行事食の写真が届きました。"},{"id":{"d":753,"n":2,"t":5},"speaker":"B","text":"節分と言えば恵方巻きですね。"},{"id":{"d":753,"n":3,"t":5},"speaker":"A","text":"はい。でも、私の祖母は歯が弱いので、のり巻きを食べることができません。"},{"id":{"d":753,"n":4,"t":5},"speaker":"B","text":"それではどのような食事が用意されたのですか？"},{"id":{"d":753,"n":5,"t":5},"speaker":"A","text":"ちらしずしと、恵方巻きのようなロールケーキを出してくれたようです。"},{"id":{"d":753,"n":6,"t":5},"speaker":"B","text":"季節感もありながら、食べやすく工夫されていて、すばらしいですね。"},{"id":{"d":753,"n":7,"t":5},"speaker":"A","text":"鬼のお面を着けた職員の方と一緒に豆まきもしたそうですよ。祖母も楽しめたそうです。"},{"id":{"d":753,"n":8,"t":5},"speaker":"B","text":"おばあさまが元気に過ごせているようで良かったですね。"}];
INSERT INTO sentence [{"id":{"d":754,"n":1,"t":5},"speaker":"A","text":"七夕飾りを作って、この笹に飾りましょう。"},{"id":{"d":754,"n":2,"t":5},"speaker":"B","text":"大きな笹ですね。"},{"id":{"d":754,"n":3,"t":5},"speaker":"A","text":"先生の家の裏山にあったものを持ってきました。"},{"id":{"d":754,"n":4,"t":5},"speaker":"B","text":"これだけ大きければたくさん飾りが付けられそうです。"},{"id":{"d":754,"n":5,"t":5},"speaker":"A","text":"まずは、短冊に願い事を書きましょうか。"},{"id":{"d":754,"n":6,"t":5},"speaker":"B","text":"先生、願い事は一つでなくてもよいのですか？"},{"id":{"d":754,"n":7,"t":5},"speaker":"A","text":"短冊を飾る場所はたくさんあるので、いくつ書いてもよいですよ。"},{"id":{"d":754,"n":8,"t":5},"speaker":"B","text":"分かりました。何を書こうか悩みます。"}];
INSERT INTO sentence [{"id":{"d":755,"n":1,"t":5},"speaker":"A","text":"お気に入りの曲をCDに録音しました。"},{"id":{"d":755,"n":2,"t":5},"speaker":"B","text":"CDのラベルには何も書かないのですか？"},{"id":{"d":755,"n":3,"t":5},"speaker":"A","text":"後で曲名を書くつもりです。"},{"id":{"d":755,"n":4,"t":5},"speaker":"B","text":"そのCDなら、プリンターを使って曲名を印刷できますよ。"},{"id":{"d":755,"n":5,"t":5},"speaker":"A","text":"入力した曲名をCDに印刷できるのですか？"},{"id":{"d":755,"n":6,"t":5},"speaker":"B","text":"はい。それだけではなく、イラストや写真も印刷することができます。"},{"id":{"d":755,"n":7,"t":5},"speaker":"A","text":"その方法を試してみたいです。どのようにすればよいか教えていただけますか？"},{"id":{"d":755,"n":8,"t":5},"speaker":"B","text":"分かりました。パソコンに専用ソフトをダウンロードするところから始めましょう。"}];
INSERT INTO sentence [{"id":{"d":756,"n":1,"t":5},"speaker":"A","text":"すみません。この座席番号は、こちらの席でよろしいでしょうか？"},{"id":{"d":756,"n":2,"t":5},"speaker":"B","text":"チケットを見せていただけますか？"},{"id":{"d":756,"n":3,"t":5},"speaker":"A","text":"はい、これです。"},{"id":{"d":756,"n":4,"t":5},"speaker":"B","text":"この座席番号は、この一つ前の座席ですね。"},{"id":{"d":756,"n":5,"t":5},"speaker":"A","text":"そうでしたか。ありがとうございます。"},{"id":{"d":756,"n":6,"t":5},"speaker":"B","text":"こちらの映画館は初めていらしたのですか？"},{"id":{"d":756,"n":7,"t":5},"speaker":"A","text":"ええ、初めてです。映画館で映画を見ること自体が久しぶりです。"},{"id":{"d":756,"n":8,"t":5},"speaker":"B","text":"この映画館は、映像も音も素晴らしいですよ。ぜひ楽しんでいってください。"}];
INSERT INTO sentence [{"id":{"d":757,"n":1,"t":5},"speaker":"A","text":"秋の夜長には読書がいいですね。"},{"id":{"d":757,"n":2,"t":5},"speaker":"B","text":"あなたが今読んでいるのは、先週僕が買ってきた本ではありませんか？"},{"id":{"d":757,"n":3,"t":5},"speaker":"A","text":"はい、そうです。楽しく読ませていただいていますよ。"},{"id":{"d":757,"n":4,"t":5},"speaker":"B","text":"それは良かったです。でも、先に読んだからといって結末を語らないでくださいね。"},{"id":{"d":757,"n":5,"t":5},"speaker":"A","text":"分かりました、3日だけは話すのを我慢しますから、その間にあなたも読み終えてください。"},{"id":{"d":757,"n":6,"t":5},"speaker":"B","text":"それは難しいことです。せめて1週間は我慢してください。"}];
INSERT INTO sentence [{"id":{"d":758,"n":1,"t":5},"speaker":"A","text":"心置きなく読書の感想を言い合えるなんて良いですね。"},{"id":{"d":758,"n":2,"t":5},"speaker":"B","text":"そうですね。僕が選んだ本は面白かったでしょう？"},{"id":{"d":758,"n":3,"t":5},"speaker":"A","text":"はい、あなたの選ぶ本にはほとんど外れがありません。どうやって選んでいるのですか？"},{"id":{"d":758,"n":4,"t":5},"speaker":"B","text":"実は、表紙だけで選んでいるのですよ。"},{"id":{"d":758,"n":5,"t":5},"speaker":"A","text":"そうなのですか。てっきり裏表紙のあらすじを読んで選んでいるのかと思いました。"},{"id":{"d":758,"n":6,"t":5},"speaker":"B","text":"そこには話の核心に関わる情報が書かれているときもあるので、読まないようにしています。"},{"id":{"d":758,"n":7,"t":5},"speaker":"A","text":"つまり、ほとんど直感で本を選んでいるということですか？"},{"id":{"d":758,"n":8,"t":5},"speaker":"B","text":"そうなりますね。"}];
INSERT INTO sentence [{"id":{"d":759,"n":1,"t":5},"speaker":"A","text":"夜中のテレビアニメーションに夢中になっていて、最近はずっと寝不足です。"},{"id":{"d":759,"n":2,"t":5},"speaker":"B","text":"最近のテレビアニメーションは夜遅くに放送していますからね。"},{"id":{"d":759,"n":3,"t":5},"speaker":"A","text":"内容が子ども向けではないのですよ。"},{"id":{"d":759,"n":4,"t":5},"speaker":"B","text":"18歳未満は視聴できないのですか？"},{"id":{"d":759,"n":5,"t":5},"speaker":"A","text":"そこまでではありませんが、子どもには解釈が難しすぎる作品も多いと思います。"},{"id":{"d":759,"n":6,"t":5},"speaker":"B","text":"大人はそれに夢中になってしまうのですね。"},{"id":{"d":759,"n":7,"t":5},"speaker":"A","text":"はい。見応えのある作品がたくさんあり、とても面白いです。"},{"id":{"d":759,"n":8,"t":5},"speaker":"B","text":"録画をして、後から見ることはしませんか？"},{"id":{"d":759,"n":9,"t":5},"speaker":"A","text":"面白い作品が毎日何本もあるので、録画をしてしまうと溜まって大変なことになるのです。"},{"id":{"d":759,"n":10,"t":5},"speaker":"B","text":"それで寝不足なのですね。仕事中は寝ないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":760,"n":1,"t":5},"speaker":"A","text":"お父さん、もう少し左に移動してください。"},{"id":{"d":760,"n":2,"t":5},"speaker":"B","text":"狭い台所でこれ以上左に行くのは無理です。"},{"id":{"d":760,"n":3,"t":5},"speaker":"A","text":"では、私の代わりにこのニンジンを切ってください。"},{"id":{"d":760,"n":4,"t":5},"speaker":"B","text":"分かりました。どのように切ればよいのですか？"},{"id":{"d":760,"n":5,"t":5},"speaker":"A","text":"お花の形です。"},{"id":{"d":760,"n":6,"t":5},"speaker":"B","text":"お花の形に切ることは、お父さんには難しいです。"},{"id":{"d":760,"n":7,"t":5},"speaker":"A","text":"分かりました。お父さんはもう何もしなくていいので、台所から出て休んでいてください。"},{"id":{"d":760,"n":8,"t":5},"speaker":"B","text":"すみません。母の日のカレーライス作りは大変ですね。"}];
INSERT INTO sentence [{"id":{"d":761,"n":1,"t":5},"speaker":"A","text":"美玖ちゃんが作ってくれたカレーライスは、とてもおいしかったですよ。"},{"id":{"d":761,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。母の日なので、お母さんのために一生懸命に作りました。"},{"id":{"d":761,"n":3,"t":5},"speaker":"A","text":"ニンジンがお花の形になっているのもすてきでした。"},{"id":{"d":761,"n":4,"t":5},"speaker":"B","text":"頑張ってお花の形にしたので、喜んでもらえてうれしいです。お父さんにニンジンを切ってくださいと頼んだら「できない」と言われたので、私が一人で切りました。"},{"id":{"d":761,"n":5,"t":5},"speaker":"A","text":"お父さんもお手伝いをしてくれたのですか？"},{"id":{"d":761,"n":6,"t":5},"speaker":"B","text":"はい。お米を研いでくれました。"},{"id":{"d":761,"n":7,"t":5},"speaker":"A","text":"では、このカレーライスは美玖ちゃんとお父さんの合作ですね。"},{"id":{"d":761,"n":8,"t":5},"speaker":"B","text":"そうですね。でも全体の90パーセントは私が作りましたよ。"}];
INSERT INTO sentence [{"id":{"d":762,"n":1,"t":5},"speaker":"A","text":"定年退職したら、ギターを習おうと思います。"},{"id":{"d":762,"n":2,"t":5},"speaker":"B","text":"楽器を習うのはいいことですね。"},{"id":{"d":762,"n":3,"t":5},"speaker":"A","text":"昔からギターには憧れていたのです。"},{"id":{"d":762,"n":4,"t":5},"speaker":"B","text":"どのような音楽を習うつもりですか？"},{"id":{"d":762,"n":5,"t":5},"speaker":"A","text":"クラシックギターから始めてみるつもりです。近所のギター教室に聞いてみたら、そこではクラシックギターを教えているそうでしたので。"},{"id":{"d":762,"n":6,"t":5},"speaker":"B","text":"まずは基本を学ぶためにも、クラシックギターを習うのは良いことですね。"},{"id":{"d":762,"n":7,"t":5},"speaker":"A","text":"実は、私が憧れていたのはエレキギターの方なので、いずれはエレキギターにも挑戦したいと思っています。"},{"id":{"d":762,"n":8,"t":5},"speaker":"B","text":"すてきな目標ですね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":763,"n":1,"t":5},"speaker":"A","text":"年末にピアノの発表会があります。"},{"id":{"d":763,"n":2,"t":5},"speaker":"B","text":"そのようですね。先生からのお便りにもそのことが書いてありました。"},{"id":{"d":763,"n":3,"t":5},"speaker":"A","text":"里香ちゃんは発表会でドレスを着るそうです。"},{"id":{"d":763,"n":4,"t":5},"speaker":"B","text":"和香子もドレスを着たいのですか？"},{"id":{"d":763,"n":5,"t":5},"speaker":"A","text":"はい、私もドレスが着たいです。"},{"id":{"d":763,"n":6,"t":5},"speaker":"B","text":"分かりました。では、ピアノの練習を頑張ったら、好きなドレスを着てもよいですよ。"},{"id":{"d":763,"n":7,"t":5},"speaker":"A","text":"本当ですか？私はピンク色のドレスが着たいので、練習を頑張ります。"},{"id":{"d":763,"n":8,"t":5},"speaker":"B","text":"練習を頑張って、間違えずに弾けるようになりましょうね。"}];
INSERT INTO sentence [{"id":{"d":764,"n":1,"t":5},"speaker":"A","text":"新年おめでとうございます。今年もよろしくお願いします。"},{"id":{"d":764,"n":2,"t":5},"speaker":"B","text":"明けましておめでとうございます。こちらこそよろしくお願いします。"},{"id":{"d":764,"n":3,"t":5},"speaker":"A","text":"新年早々にすみませんが、来週の新年会への参加者は何人になりそうですか？"},{"id":{"d":764,"n":4,"t":5},"speaker":"B","text":"恐らく、8人になると思います。"},{"id":{"d":764,"n":5,"t":5},"speaker":"A","text":"あら、1人足りませんね。"},{"id":{"d":764,"n":6,"t":5},"speaker":"B","text":"戸田さんは海外旅行中です。"},{"id":{"d":764,"n":7,"t":5},"speaker":"A","text":"そうでしたか。もう少し新年会の開催日を遅くして差し上げれば戸田さんも参加できたのでしょうか？"},{"id":{"d":764,"n":8,"t":5},"speaker":"B","text":"彼女が帰ってきたら、また別の名目で集まればよいのですよ。"}];
INSERT INTO sentence [{"id":{"d":765,"n":1,"t":5},"speaker":"A","text":"明日はお天気が崩れそうですね。"},{"id":{"d":765,"n":2,"t":5},"speaker":"B","text":"そのようです。なので、今日のうちに洗濯をたくさんしておこうと思っています。"},{"id":{"d":765,"n":3,"t":5},"speaker":"A","text":"天気が悪いと洗濯物を干せませんからね。"},{"id":{"d":765,"n":4,"t":5},"speaker":"B","text":"はい。掃除や家事も、できるだけ今日のうちに済ませておこうと思います。"},{"id":{"d":765,"n":5,"t":5},"speaker":"A","text":"張り切っていますね。"},{"id":{"d":765,"n":6,"t":5},"speaker":"B","text":"明日は大雨の予報ですから、ゲーム三昧の一日にしたいのです。昔から、雨の日はゲームをしたくなるのです。"},{"id":{"d":765,"n":7,"t":5},"speaker":"A","text":"子どもたちやご主人を送り出してからゲームをするのですか？"},{"id":{"d":765,"n":8,"t":5},"speaker":"B","text":"はい、そのつもりです。"}];
INSERT INTO sentence [{"id":{"d":766,"n":1,"t":5},"speaker":"A","text":"どちらへお出かけですか？"},{"id":{"d":766,"n":2,"t":5},"speaker":"B","text":"老人施設に絵手紙を教えに行きます。"},{"id":{"d":766,"n":3,"t":5},"speaker":"A","text":"絵手紙の先生をなさるのですか？"},{"id":{"d":766,"n":4,"t":5},"speaker":"B","text":"はい。趣味でずっと続けているものです。"},{"id":{"d":766,"n":5,"t":5},"speaker":"A","text":"すてきですね。ボランティアで教えているのですか？"},{"id":{"d":766,"n":6,"t":5},"speaker":"B","text":"用具代に少々色がついた金額を頂いています。"},{"id":{"d":766,"n":7,"t":5},"speaker":"A","text":"楽しそうですね。"},{"id":{"d":766,"n":8,"t":5},"speaker":"B","text":"はい。おじいさんやおばあさんからも元気を頂けますから、とても楽しいです。"}];
INSERT INTO sentence [{"id":{"d":767,"n":1,"t":5},"speaker":"A","text":"本気で絵を描こうとすると、お金がかかるのですね。"},{"id":{"d":767,"n":2,"t":5},"speaker":"B","text":"そうですか？紙と鉛筆があれば簡単に描けそうな気がします。"},{"id":{"d":767,"n":3,"t":5},"speaker":"A","text":"私の娘は、パソコンとイラスト用のソフトとペンタブレットなどがなければ描けないと言うのです。"},{"id":{"d":767,"n":4,"t":5},"speaker":"B","text":"なるほど。デジタルのイラストを描きたがっているのですね。"},{"id":{"d":767,"n":5,"t":5},"speaker":"A","text":"学校の部活の友達はみんな持っているそうなのです。"},{"id":{"d":767,"n":6,"t":5},"speaker":"B","text":"家のパソコンを使うことにして、ソフトとペンタブレットだけを買ってあげるというのでは駄目なのですか？"},{"id":{"d":767,"n":7,"t":5},"speaker":"A","text":"家のパソコンは古くて、最新のソフトを使おうとすると動作が重くなってしまうそうです。"},{"id":{"d":767,"n":8,"t":5},"speaker":"B","text":"それでは高額な出費になってしまいそうですね。"}];
INSERT INTO sentence [{"id":{"d":768,"n":1,"t":5},"speaker":"A","text":"よかったらこのお皿を使っていただけませんか？"},{"id":{"d":768,"n":2,"t":5},"speaker":"B","text":"すてきなお皿ですね。どうされたのですか？"},{"id":{"d":768,"n":3,"t":5},"speaker":"A","text":"私が陶芸教室で作ったのです。"},{"id":{"d":768,"n":4,"t":5},"speaker":"B","text":"これはあなたの手作りなのですか？"},{"id":{"d":768,"n":5,"t":5},"speaker":"A","text":"そうです。ぜひ使ってください。"},{"id":{"d":768,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。でも、とてもよくできていますから、ご自分でお使いになればよろしいのに。"},{"id":{"d":768,"n":7,"t":5},"speaker":"A","text":"たくさん作り過ぎて、もうわが家の食器棚には入らないのです。"},{"id":{"d":768,"n":8,"t":5},"speaker":"B","text":"これだけ完成度の高いお皿なら買ってでも欲しいと思う方がいると思いますよ。ありがたく使わせていただきます。"}];
INSERT INTO sentence [{"id":{"d":769,"n":1,"t":5},"speaker":"A","text":"今度、陶芸教室のバザーがあるのですが、いらっしゃいませんか？"},{"id":{"d":769,"n":2,"t":5},"speaker":"B","text":"先日に頂いたお皿のようなものが出品されるのですか？"},{"id":{"d":769,"n":3,"t":5},"speaker":"A","text":"はい。でも他の方の作品はもっと素晴らしいですよ。"},{"id":{"d":769,"n":4,"t":5},"speaker":"B","text":"それは見てみたいですね。"},{"id":{"d":769,"n":5,"t":5},"speaker":"A","text":"皆さんお上手ですが、プロの作品ではありませんからお求めやすいと思います。"},{"id":{"d":769,"n":6,"t":5},"speaker":"B","text":"とても興味があります。ぜひ伺いたいと思います。"},{"id":{"d":769,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。バザーの開催は、今度の土曜日と日曜日です。"},{"id":{"d":769,"n":8,"t":5},"speaker":"B","text":"私は新しいマグカップが欲しいのです。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":770,"n":1,"t":5},"speaker":"A","text":"バザーですてきなマグカップを手に入れました。"},{"id":{"d":770,"n":2,"t":5},"speaker":"B","text":"そのマグカップは、陶芸教室の中でも一番上手な生徒さんの作品です。"},{"id":{"d":770,"n":3,"t":5},"speaker":"A","text":"これが300円だなんて、申し訳ないくらいです。"},{"id":{"d":770,"n":4,"t":5},"speaker":"B","text":"その生徒さんは、そろそろ自分でお店を始めたいそうですから、バザーに出品するのは今回が最後かもしれません。"},{"id":{"d":770,"n":5,"t":5},"speaker":"A","text":"そうなのですか。今回は貴重な機会だったのですね。"},{"id":{"d":770,"n":6,"t":5},"speaker":"B","text":"はい。ぜひ、そのマグカップを大切にしてあげてくださいね。"},{"id":{"d":770,"n":7,"t":5},"speaker":"A","text":"もちろんです。とっておきの飲み物を飲むときに使います。"},{"id":{"d":770,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。ところで、次のバザーでは私の作品を買ってくださるとうれしいです。"}];
INSERT INTO sentence [{"id":{"d":771,"n":1,"t":5},"speaker":"A","text":"ショッピングモールでクリスマスソングのコンサートがあるそうです。"},{"id":{"d":771,"n":2,"t":5},"speaker":"B","text":"真ん中の広場で行われるのですか？"},{"id":{"d":771,"n":3,"t":5},"speaker":"A","text":"そうです。特設ステージが造られるようです。"},{"id":{"d":771,"n":4,"t":5},"speaker":"B","text":"クリスマスの買い物のついでに寄りましょうか？"},{"id":{"d":771,"n":5,"t":5},"speaker":"A","text":"いいですね。"},{"id":{"d":771,"n":6,"t":5},"speaker":"B","text":"入場料などは要るのですか？"},{"id":{"d":771,"n":7,"t":5},"speaker":"A","text":"無料だそうですよ。"},{"id":{"d":771,"n":8,"t":5},"speaker":"B","text":"クリスマス気分が盛り上がりそうですね。"}];
INSERT INTO sentence [{"id":{"d":772,"n":1,"t":5},"speaker":"A","text":"すてきなコンサートでしたね。"},{"id":{"d":772,"n":2,"t":5},"speaker":"B","text":"そうですね。歌っていたのは、教会の合唱団の方たちだったのでしょうか？"},{"id":{"d":772,"n":3,"t":5},"speaker":"A","text":"このショッピングモールにある文化センターで活動しているゴスペル愛好会だそうです。"},{"id":{"d":772,"n":4,"t":5},"speaker":"B","text":"そのような教室もあるのですね。"},{"id":{"d":772,"n":5,"t":5},"speaker":"A","text":"すてきな歌で、クリスマスの気分が盛り上がりました。"},{"id":{"d":772,"n":6,"t":5},"speaker":"B","text":"私もです。小さいツリーを買おうと思っていたのですが、一回り大きいものにしようかと思います。"},{"id":{"d":772,"n":7,"t":5},"speaker":"A","text":"私もケーキの予約を1号大きいものに変更しようと思います。"},{"id":{"d":772,"n":8,"t":5},"speaker":"B","text":"今年のクリスマスは豪華になりそうですね。"}];
INSERT INTO sentence [{"id":{"d":773,"n":1,"t":5},"speaker":"A","text":"かしわ餅を五つください。"},{"id":{"d":773,"n":2,"t":5},"speaker":"B","text":"こしあんと粒あんとみそあんがありますが、どれにいたしましょうか？"},{"id":{"d":773,"n":3,"t":5},"speaker":"A","text":"こしあんを二つと粒あんを三つでお願いします。"},{"id":{"d":773,"n":4,"t":5},"speaker":"B","text":"はい。少々お待ちください。"},{"id":{"d":773,"n":5,"t":5},"speaker":"A","text":"粒あんの餅は、よもぎ餅なのですね。"},{"id":{"d":773,"n":6,"t":5},"speaker":"B","text":"割って確かめなくても、どちらのあんかが分かるようにしています。"},{"id":{"d":773,"n":7,"t":5},"speaker":"A","text":"子どもたちは粒あんの方が好きなので助かります。"},{"id":{"d":773,"n":8,"t":5},"speaker":"B","text":"おまけにこいのぼりのシールをつけました。"}];
INSERT INTO sentence [{"id":{"d":774,"n":1,"t":5},"speaker":"A","text":"ギターがお上手ですね。"},{"id":{"d":774,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。先ほどのあなたのピアノもすてきでしたよ。"},{"id":{"d":774,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":774,"n":4,"t":5},"speaker":"B","text":"ここのカフェは演奏ができるのがいいですね。"},{"id":{"d":774,"n":5,"t":5},"speaker":"A","text":"はい。いろいろな方と音楽の話ができるのもいいです。"},{"id":{"d":774,"n":6,"t":5},"speaker":"B","text":"もしよければ、今度は一緒に演奏しませんか？"},{"id":{"d":774,"n":7,"t":5},"speaker":"A","text":"いいですね。マスターにも歌手として参加してほしいと思います。"},{"id":{"d":774,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。早速、曲を決めましょう。"}];
INSERT INTO sentence [{"id":{"d":775,"n":1,"t":5},"speaker":"A","text":"今度の週末はお暇ですか？"},{"id":{"d":775,"n":2,"t":5},"speaker":"B","text":"暇ですけれど、何かあるのですか？"},{"id":{"d":775,"n":3,"t":5},"speaker":"A","text":"実はコミックマーケットに出店するのですが、売り子を手伝ってくれる人を探しているのです。"},{"id":{"d":775,"n":4,"t":5},"speaker":"B","text":"新作の漫画が描けたのですか？"},{"id":{"d":775,"n":5,"t":5},"speaker":"A","text":"はい、そうです。"},{"id":{"d":775,"n":6,"t":5},"speaker":"B","text":"一番に読ませてくれたら、喜んで売り子をします。"},{"id":{"d":775,"n":7,"t":5},"speaker":"A","text":"お安いご用です。明日には印刷が出来上がる予定なので、一番にお見せしますよ。"},{"id":{"d":775,"n":8,"t":5},"speaker":"B","text":"楽しみにしています。土日は張り切ってお手伝いにいきますよ。"}];
INSERT INTO sentence [{"id":{"d":776,"n":1,"t":5},"speaker":"A","text":"高級なスピーカーから聞こえてくる音色は格別ですね。"},{"id":{"d":776,"n":2,"t":5},"speaker":"B","text":"気に入っていただけましたか？"},{"id":{"d":776,"n":3,"t":5},"speaker":"A","text":"はい。視聴させていただき、良い体験ができました。"},{"id":{"d":776,"n":4,"t":5},"speaker":"B","text":"他のスピーカーでも視聴されますか？"},{"id":{"d":776,"n":5,"t":5},"speaker":"A","text":"いいえ。これが一番良さそうなので、他のものの視聴はやめておきます。"},{"id":{"d":776,"n":6,"t":5},"speaker":"B","text":"承知しました。こちらの商品は、本日、ご購入されますか？"},{"id":{"d":776,"n":7,"t":5},"speaker":"A","text":"妻と相談してから決めます。また参ります。"},{"id":{"d":776,"n":8,"t":5},"speaker":"B","text":"かしこまりました。またのご来店をお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":777,"n":1,"t":5},"speaker":"A","text":"玄関先にかわいいものが置いてありますね。"},{"id":{"d":777,"n":2,"t":5},"speaker":"B","text":"これのことですか？"},{"id":{"d":777,"n":3,"t":5},"speaker":"A","text":"そうです。"},{"id":{"d":777,"n":4,"t":5},"speaker":"B","text":"これはこコケ玉です。先日、コケ玉教室に行って作ってきたのです。"},{"id":{"d":777,"n":5,"t":5},"speaker":"A","text":"なるほど、植木鉢がなくてもこうすればコケを育てられるのですね。"},{"id":{"d":777,"n":6,"t":5},"speaker":"B","text":"そのようです。お皿は100円ショップのもので済みました。"},{"id":{"d":777,"n":7,"t":5},"speaker":"A","text":"小さなお人形も置いたのですね。"},{"id":{"d":777,"n":8,"t":5},"speaker":"B","text":"かわいいでしょう？「ウエルカム」としているように見せたくて置いてみました。"},{"id":{"d":777,"n":9,"t":5},"speaker":"A","text":"これもすてきですね。十分にウエルカムをしてもらいましたよ。"},{"id":{"d":777,"n":10,"t":5},"speaker":"B","text":"さあ、どうぞそろそろお入りください。ようこそわが家へ。"}];
INSERT INTO sentence [{"id":{"d":778,"n":1,"t":5},"speaker":"A","text":"お宅の犬のサイズを測らせてもらってもよいですか？"},{"id":{"d":778,"n":2,"t":5},"speaker":"B","text":"構いませんが、なぜですか？"},{"id":{"d":778,"n":3,"t":5},"speaker":"A","text":"犬の洋服を作りたいのです。"},{"id":{"d":778,"n":4,"t":5},"speaker":"B","text":"わが家の犬には洋服は着せていませんよ。"},{"id":{"d":778,"n":5,"t":5},"speaker":"A","text":"勘違いさせてごめんなさい。同じくらいの大きさの犬を義母が飼っているので、その子用の洋服を作ろうと思っているのです。それでサイズを測らせていただけたらありがたいのですが。"},{"id":{"d":778,"n":6,"t":5},"speaker":"B","text":"なるほど、そういうことですか。"},{"id":{"d":778,"n":7,"t":5},"speaker":"A","text":"義母に私の趣味が洋裁だと言ったら、ぜひ作ってほしいと言われたのです。"},{"id":{"d":778,"n":8,"t":5},"speaker":"B","text":"測っていただいて結構ですよ。頑張って洋服を作ってください。"}];
INSERT INTO sentence [{"id":{"d":779,"n":1,"t":5},"speaker":"A","text":"明日の敬老の日には何をする予定ですか？"},{"id":{"d":779,"n":2,"t":5},"speaker":"B","text":"保育園の子どもたちが来て、一緒に縁日遊びをする予定です。"},{"id":{"d":779,"n":3,"t":5},"speaker":"A","text":"子どもたちも入所者の皆さんも喜びますね。"},{"id":{"d":779,"n":4,"t":5},"speaker":"B","text":"そうだといいのですが。ところで、山本さん、ヨーヨー釣りの道具を作るのを手伝っていただけますか？"},{"id":{"d":779,"n":5,"t":5},"speaker":"A","text":"はい、いいですよ。これは、縁日遊びに使うための小道具ですか？"},{"id":{"d":779,"n":6,"t":5},"speaker":"B","text":"そのつもりです。風船は私が作るので、あなたは金具にこよりを付けてください。"},{"id":{"d":779,"n":7,"t":5},"speaker":"A","text":"分かりました。こよりを付ける作業は意外と難しいですね。"},{"id":{"d":779,"n":8,"t":5},"speaker":"B","text":"あまりきつくすると切れなくなって、ゲームになりませんから気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":780,"n":1,"t":5},"speaker":"A","text":"子どもたちのお遊戯は良かったですね。"},{"id":{"d":780,"n":2,"t":5},"speaker":"B","text":"良かったですね。敬老の日に合わせて、保育園の先生も準備してくださったそうです。"},{"id":{"d":780,"n":3,"t":5},"speaker":"A","text":"入所者の皆さんも手をたたいて喜んでおられたのがうれしかったです。"},{"id":{"d":780,"n":4,"t":5},"speaker":"B","text":"おやつのチョコバナナも皆喜んでくれていましたが、後のお世話が大変でしたね。チョコスプレーが床に散らばってしまいましたし、子どもたちも入所者も口の周りがチョコレートだらけになってしまいました。"},{"id":{"d":780,"n":5,"t":5},"speaker":"A","text":"すみません、あれは私の提案です。"},{"id":{"d":780,"n":6,"t":5},"speaker":"B","text":"そうだったのですか？でも、盛り上がったのはよかったですね。来年のおやつはいちごあめにしましょうか？"},{"id":{"d":780,"n":7,"t":5},"speaker":"A","text":"それがいいですね。"}];
INSERT INTO sentence [{"id":{"d":781,"n":1,"t":5},"speaker":"A","text":"聞いてください。昨日、私はとうとう行けました！"},{"id":{"d":781,"n":2,"t":5},"speaker":"B","text":"どこにですか？"},{"id":{"d":781,"n":3,"t":5},"speaker":"A","text":"お笑いライブです！"},{"id":{"d":781,"n":4,"t":5},"speaker":"B","text":"お笑いがお好きだったのですか？"},{"id":{"d":781,"n":5,"t":5},"speaker":"A","text":"はい。動画を見て好きになりました。"},{"id":{"d":781,"n":6,"t":5},"speaker":"B","text":"それでライブに行かれたのですね？"},{"id":{"d":781,"n":7,"t":5},"speaker":"A","text":"ええ。生でコントや漫才を見て大笑いし、とても気持ちがよかったです。そこで初めて知ったお笑いコンビも好きになってしまいましたよ。"},{"id":{"d":781,"n":8,"t":5},"speaker":"B","text":"楽しめたようでよかったですね。"}];
INSERT INTO sentence [{"id":{"d":782,"n":1,"t":5},"speaker":"A","text":"この小説を読んだことはありますか？"},{"id":{"d":782,"n":2,"t":5},"speaker":"B","text":"あります。犯人が意外な人ですよね。"},{"id":{"d":782,"n":3,"t":5},"speaker":"A","text":"結論を言わないでいてくれるとは、松本さんは優しいですね。"},{"id":{"d":782,"n":4,"t":5},"speaker":"B","text":"はい。一応は、気を使ったつもりです。"},{"id":{"d":782,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。一応、私も読み終えましたので犯人が誰かは知っていますよ。"},{"id":{"d":782,"n":6,"t":5},"speaker":"B","text":"そうでしたか。ところで、この小説を原作とした実写映画が公開中なのですが、作品を見たら全く違う人が犯人だったのでとても驚きました。"},{"id":{"d":782,"n":7,"t":5},"speaker":"A","text":"そうなのですか？小説はとても良くできていたので、あの犯人以外は考えられません。"},{"id":{"d":782,"n":8,"t":5},"speaker":"B","text":"そうですよね。おかげで映画は陳腐な物語に見えてしまいました。"},{"id":{"d":782,"n":9,"t":5},"speaker":"A","text":"教えてくださってありがとうございます。それなら私は映画は見ないようにします。"}];
INSERT INTO sentence [{"id":{"d":783,"n":1,"t":5},"speaker":"A","text":"この前植えた種から芽が出ました。"},{"id":{"d":783,"n":2,"t":5},"speaker":"B","text":"本当ですね。これは朝顔の赤ちゃんですよ。"},{"id":{"d":783,"n":3,"t":5},"speaker":"A","text":"絵日記に描きます。"},{"id":{"d":783,"n":4,"t":5},"speaker":"B","text":"いいですね。これから少しずつ大きくなっていく様子を描くといいですよ。"},{"id":{"d":783,"n":5,"t":5},"speaker":"A","text":"いつお花が咲きますか？"},{"id":{"d":783,"n":6,"t":5},"speaker":"B","text":"まだ芽が出たばかりですから、あと1カ月くらいはかかると思います。"},{"id":{"d":783,"n":7,"t":5},"speaker":"A","text":"毎日描けば、絵日記の宿題がそれだけで埋まりそうです。"},{"id":{"d":783,"n":8,"t":5},"speaker":"B","text":"いっそのこと自由研究にしてもいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":784,"n":1,"t":5},"speaker":"A","text":"記念硬貨のコレクションがいっぱいになってしまいました。"},{"id":{"d":784,"n":2,"t":5},"speaker":"B","text":"はい。いつの間にか、きれいな箱が閉まらないくらい、たくさんになってしまいましたね。"},{"id":{"d":784,"n":3,"t":5},"speaker":"A","text":"もうこれ以上は増やせないくらいです。"},{"id":{"d":784,"n":4,"t":5},"speaker":"B","text":"それなら、コレクションケースを作ればよいのではありませんか？"},{"id":{"d":784,"n":5,"t":5},"speaker":"A","text":"私が自分で作るのですか？"},{"id":{"d":784,"n":6,"t":5},"speaker":"B","text":"はい。ホームセンターでDIY教室を開いていますよ。"},{"id":{"d":784,"n":7,"t":5},"speaker":"A","text":"好きな形のものを作れるのですか？"},{"id":{"d":784,"n":8,"t":5},"speaker":"B","text":"講師の先生に相談すれば、自分の好きなものを作れるようですよ。"}];
INSERT INTO sentence [{"id":{"d":785,"n":1,"t":5},"speaker":"A","text":"クレーンゲームでぬいぐるみをたくさん取りました。"},{"id":{"d":785,"n":2,"t":5},"speaker":"B","text":"あなたはクレーンゲームが上手なのですね。"},{"id":{"d":785,"n":3,"t":5},"speaker":"A","text":"はい。得意ですし、大好きです。家に持って帰ると母に叱られるのですが、なかなかやめられません。"},{"id":{"d":785,"n":4,"t":5},"speaker":"B","text":"どうして叱られるのですか？お小遣いはあまり使っていないでしょう？"},{"id":{"d":785,"n":5,"t":5},"speaker":"A","text":"もう部屋中がぬいぐるみだらけだからです。"},{"id":{"d":785,"n":6,"t":5},"speaker":"B","text":"いくつあるのですか？"},{"id":{"d":785,"n":7,"t":5},"speaker":"A","text":"数えたことがないので分かりませんが、ぬいぐるみで部屋の窓から外が見えません。"},{"id":{"d":785,"n":8,"t":5},"speaker":"B","text":"それほどまでにたくさんあるのなら、さすがに叱られてしまいますね。"}];
INSERT INTO sentence [{"id":{"d":786,"n":1,"t":5},"speaker":"A","text":"クレーンゲームでたくさんぬいぐるみを取りました。よかったらもらってくれませんか？"},{"id":{"d":786,"n":2,"t":5},"speaker":"B","text":"たくさんは頂けないので2つだけください。"},{"id":{"d":786,"n":3,"t":5},"speaker":"A","text":"2つだけでいいのですか？あと2つくらいいかがですか？"},{"id":{"d":786,"n":4,"t":5},"speaker":"B","text":"結構です。決して遠慮しているのではなく、家に置き場所がないのですよ。"},{"id":{"d":786,"n":5,"t":5},"speaker":"A","text":"そうですか。たくさん取り過ぎてしまったので、困りました。"},{"id":{"d":786,"n":6,"t":5},"speaker":"B","text":"先ほど、じっと見ていた子どもたちにあげたらどうでしょう？"},{"id":{"d":786,"n":7,"t":5},"speaker":"A","text":"それはいいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":787,"n":1,"t":5},"speaker":"A","text":"ジェンガゲームを譲りたいのですが、もらってくれませんか？"},{"id":{"d":787,"n":2,"t":5},"speaker":"B","text":"これはかなり高いおもちゃですよね？本当にいただいてよいのですか？"},{"id":{"d":787,"n":3,"t":5},"speaker":"A","text":"はい。お正月に家族で遊ぼうと思って買ったのですが、いざやってみたら全然ゲームにならなかったのです。"},{"id":{"d":787,"n":4,"t":5},"speaker":"B","text":"どうしてですか？"},{"id":{"d":787,"n":5,"t":5},"speaker":"A","text":"せっかく積み上げても、実家の猫が怪獣のように破壊してしまうのです。"},{"id":{"d":787,"n":6,"t":5},"speaker":"B","text":"部屋から猫を追い出して遊べばよかったのではありませんか？"},{"id":{"d":787,"n":7,"t":5},"speaker":"A","text":"そのようなかわいそうなことはできませんよ。"},{"id":{"d":787,"n":8,"t":5},"speaker":"B","text":"では、そのうち私の家でしましょう。そのうちホームパーティーを開きますから来てください。"}];
INSERT INTO sentence [{"id":{"d":788,"n":1,"t":5},"speaker":"A","text":"どこかに将棋道場はありませんか？"},{"id":{"d":788,"n":2,"t":5},"speaker":"B","text":"将棋を習うのですか？"},{"id":{"d":788,"n":3,"t":5},"speaker":"A","text":"私でありませんよ。息子を通わせたいのです。"},{"id":{"d":788,"n":4,"t":5},"speaker":"B","text":"息子さんは将棋がお強いのですか？"},{"id":{"d":788,"n":5,"t":5},"speaker":"A","text":"はい、そうなのです。私はあっという間に勝てなくなりました。"},{"id":{"d":788,"n":6,"t":5},"speaker":"B","text":"息子さんの相手を探されているのですね。"},{"id":{"d":788,"n":7,"t":5},"speaker":"A","text":"ええ、そうです。"},{"id":{"d":788,"n":8,"t":5},"speaker":"B","text":"駅前のビルに良い将棋教室がありますよ。"}];
INSERT INTO sentence [{"id":{"d":789,"n":1,"t":5},"speaker":"A","text":"これらの絵の中で、どれが一番良いと思いますか？"},{"id":{"d":789,"n":2,"t":5},"speaker":"B","text":"どれもすてきですね。"},{"id":{"d":789,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。でも、どれか一つを選んでください。"},{"id":{"d":789,"n":4,"t":5},"speaker":"B","text":"選んだらその絵を私にくださるのですか？"},{"id":{"d":789,"n":5,"t":5},"speaker":"A","text":"ごめんなさい、そうではありません。今度、絵画教室の展示会に出す作品を選ぶときの参考にさせていただこうと思って聞いてみたのです。"},{"id":{"d":789,"n":6,"t":5},"speaker":"B","text":"なるほど、そうでしたか。展示が終わった後にも頂けませんか？"},{"id":{"d":789,"n":7,"t":5},"speaker":"A","text":"大事にしていただけるのでしたら、よいですよ。"},{"id":{"d":789,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。それではこちらの森の絵がいいです。"}];
INSERT INTO sentence [{"id":{"d":790,"n":1,"t":5},"speaker":"A","text":"すてきなピアスを着けていますね。"},{"id":{"d":790,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。これは自分で作ったのです。"},{"id":{"d":790,"n":3,"t":5},"speaker":"A","text":"てっきり買ったものだと思いました。上手ですね。"},{"id":{"d":790,"n":4,"t":5},"speaker":"B","text":"最近は、ビーズアクセサリー作りに夢中になっているのです。"},{"id":{"d":790,"n":5,"t":5},"speaker":"A","text":"これはビーズなのですか？七宝焼きのようです。"},{"id":{"d":790,"n":6,"t":5},"speaker":"B","text":"ビーズですよ。このような豪華なパーツも売っているのです。"},{"id":{"d":790,"n":7,"t":5},"speaker":"A","text":"私が持つビーズのイメージとはだいぶ違うのですね。"},{"id":{"d":790,"n":8,"t":5},"speaker":"B","text":"そうかもしれません。ビーズアクセサリー作りは楽しいですよ。よかったら一緒に挑戦しませんか？"}];
INSERT INTO sentence [{"id":{"d":791,"n":1,"t":5},"speaker":"A","text":"これは何のケースですか？"},{"id":{"d":791,"n":2,"t":5},"speaker":"B","text":"フルートが入っているケースです。"},{"id":{"d":791,"n":3,"t":5},"speaker":"A","text":"フルートを演奏されるのですか？"},{"id":{"d":791,"n":4,"t":5},"speaker":"B","text":"先月から習い始めました。まだ演奏するというほどではなく、時々奇跡的に音が出るというレベルです。"},{"id":{"d":791,"n":5,"t":5},"speaker":"A","text":"音を出すのも難しそうですね。"},{"id":{"d":791,"n":6,"t":5},"speaker":"B","text":"はい。でも、憧れの楽器を持っているだけで心が浮き立ちます。"},{"id":{"d":791,"n":7,"t":5},"speaker":"A","text":"フルートに憧れていたのですか？"},{"id":{"d":791,"n":8,"t":5},"speaker":"B","text":"はい。学生時代から憧れていました。"}];
INSERT INTO sentence [{"id":{"d":792,"n":1,"t":5},"speaker":"A","text":"茶道教室に行きませんか？"},{"id":{"d":792,"n":2,"t":5},"speaker":"B","text":"茶道ですか？正座が苦手なので私は遠慮したいです。"},{"id":{"d":792,"n":3,"t":5},"speaker":"A","text":"お茶はお嫌いですか？"},{"id":{"d":792,"n":4,"t":5},"speaker":"B","text":"お茶は大好きですよ。観光地にあるお店ではお茶菓子付きのセットを頼むこともあります。"},{"id":{"d":792,"n":5,"t":5},"speaker":"A","text":"正座だけが問題なのでしたら、それは気にしなくても大丈夫ですよ。"},{"id":{"d":792,"n":6,"t":5},"speaker":"B","text":"正座をしなくてもよいのですか？"},{"id":{"d":792,"n":7,"t":5},"speaker":"A","text":"はい。私の行っている教室は椅子に座るコースがあるのです。"},{"id":{"d":792,"n":8,"t":5},"speaker":"B","text":"それなら行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":793,"n":1,"t":5},"speaker":"A","text":"かわいらしい表札ですね。"},{"id":{"d":793,"n":2,"t":5},"speaker":"B","text":"それは母がトールペイントで作ったものです。"},{"id":{"d":793,"n":3,"t":5},"speaker":"A","text":"玄関にもウエルカムボードがありますね。"},{"id":{"d":793,"n":4,"t":5},"speaker":"B","text":"家に入るともっと作品がありますよ。"},{"id":{"d":793,"n":5,"t":5},"speaker":"A","text":"お母様はそれほどいろいろなものを作られたのですか？"},{"id":{"d":793,"n":6,"t":5},"speaker":"B","text":"はい。母は、木にも布にもプラスチックにもガラスにも絵を描いてしまうのです。"},{"id":{"d":793,"n":7,"t":5},"speaker":"A","text":"家中がお母様の作品だらけなのですね。"},{"id":{"d":793,"n":8,"t":5},"speaker":"B","text":"もう置き場所がないのでそろそろやめてほしい、というのが本音です。"}];
INSERT INTO sentence [{"id":{"d":794,"n":1,"t":5},"speaker":"A","text":"折り紙で何を折ろうとしているのですか？"},{"id":{"d":794,"n":2,"t":5},"speaker":"B","text":"バラの花です。"},{"id":{"d":794,"n":3,"t":5},"speaker":"A","text":"何枚も失敗してしまったのですね。"},{"id":{"d":794,"n":4,"t":5},"speaker":"B","text":"そうなのです。折り方を見ても途中で分からなくなってしまうのです。"},{"id":{"d":794,"n":5,"t":5},"speaker":"A","text":"どのような折り方なのですか？"},{"id":{"d":794,"n":6,"t":5},"speaker":"B","text":"この動画のような折り方です。これを見ながら折っているのです。"},{"id":{"d":794,"n":7,"t":5},"speaker":"A","text":"完成品はとてもきれいですね。"},{"id":{"d":794,"n":8,"t":5},"speaker":"B","text":"よかったらお母さんも折ってみてください。"}];
INSERT INTO sentence [{"id":{"d":795,"n":1,"t":5},"speaker":"A","text":"年越しそばを食べに行きませんか？"},{"id":{"d":795,"n":2,"t":5},"speaker":"B","text":"まだお昼ですよ。そばを食べるには早いのではありませんか？"},{"id":{"d":795,"n":3,"t":5},"speaker":"A","text":"今日は大晦日ですから、別に早くありませんよ。"},{"id":{"d":795,"n":4,"t":5},"speaker":"B","text":"大晦日なら年越しそばをいつ食べてもよいのですか？"},{"id":{"d":795,"n":5,"t":5},"speaker":"A","text":"よいのではありませんか？私はいつも夕ご飯に食べています。"},{"id":{"d":795,"n":6,"t":5},"speaker":"B","text":"私は除夜の鐘を聞きながら食べないと、年越しという感じがしません。"},{"id":{"d":795,"n":7,"t":5},"speaker":"A","text":"それほど遅い時間では、私は寝てしまっています。"},{"id":{"d":795,"n":8,"t":5},"speaker":"B","text":"それでも私は夜中に食べたいので、他の方と行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":796,"n":1,"t":5},"speaker":"A","text":"お月見も良いものですね。"},{"id":{"d":796,"n":2,"t":5},"speaker":"B","text":"きちんとお団子やススキを用意してお月見をしたのは初めてですね。"},{"id":{"d":796,"n":3,"t":5},"speaker":"A","text":"夜になって涼しくなってきましたし、きれいな月が見られて良い気分です。"},{"id":{"d":796,"n":4,"t":5},"speaker":"B","text":"お団子は後でいただくとして、先に月見そばを食べましょう。"},{"id":{"d":796,"n":5,"t":5},"speaker":"A","text":"おいしそうですね。冷やしそばにしたのですか？"},{"id":{"d":796,"n":6,"t":5},"speaker":"B","text":"はい。温かいそばを食べると汗をかきそうだったので、冷たいそばにしました。"},{"id":{"d":796,"n":7,"t":5},"speaker":"A","text":"私はビールを持ってきましたよ。"},{"id":{"d":796,"n":8,"t":5},"speaker":"B","text":"冷えているうちに飲みましょう。"}];
INSERT INTO sentence [{"id":{"d":797,"n":1,"t":5},"speaker":"A","text":"暑中見舞いを出し忘れてしまいました。"},{"id":{"d":797,"n":2,"t":5},"speaker":"B","text":"もう立秋ですよ。これから出すのであれば、残暑見舞いになってしまいますね。"},{"id":{"d":797,"n":3,"t":5},"speaker":"A","text":"そうですよね。"},{"id":{"d":797,"n":4,"t":5},"speaker":"B","text":"忘れないように、残暑見舞いも早めに出した方がいいですよ。"},{"id":{"d":797,"n":5,"t":5},"speaker":"A","text":"そうしたいのですが、暑中見舞い用に印刷されているはがきを買ってしまいました。"},{"id":{"d":797,"n":6,"t":5},"speaker":"B","text":"来年の暑中見舞いには使えませんか？"},{"id":{"d":797,"n":7,"t":5},"speaker":"A","text":"はい。しっかりと今年の年号が書いてあるのです。"},{"id":{"d":797,"n":8,"t":5},"speaker":"B","text":"削って書き直すわけにもいきませんね。それは残念でしたね。"}];
INSERT INTO sentence [{"id":{"d":798,"n":1,"t":5},"speaker":"A","text":"お盆ですから、今日は横浜の叔母さまとお墓参りに行きますよ。"},{"id":{"d":798,"n":2,"t":5},"speaker":"B","text":"お母さん、お盆は8月ではないのですか？"},{"id":{"d":798,"n":3,"t":5},"speaker":"A","text":"横浜のお盆は7月なのですよ。"},{"id":{"d":798,"n":4,"t":5},"speaker":"B","text":"場所によって違うのですか？"},{"id":{"d":798,"n":5,"t":5},"speaker":"A","text":"理由は私も知りませんが、地域によって違うようですよ。"},{"id":{"d":798,"n":6,"t":5},"speaker":"B","text":"そうなのですね。お墓に持っていくお花を買っていきますか？"},{"id":{"d":798,"n":7,"t":5},"speaker":"A","text":"そうですね。お花とお線香を買っていきましょう。"},{"id":{"d":798,"n":8,"t":5},"speaker":"B","text":"墓地には日影がありませんので、暑くならないといいですね。"}];
INSERT INTO sentence [{"id":{"d":799,"n":1,"t":5},"speaker":"A","text":"もうすぐ海開きですね。"},{"id":{"d":799,"n":2,"t":5},"speaker":"B","text":"はい。夏の到来という感じがしてワクワクします。"},{"id":{"d":799,"n":3,"t":5},"speaker":"A","text":"今年の水着は買いましたか？"},{"id":{"d":799,"n":4,"t":5},"speaker":"B","text":"買いました。ビキニとセパレート型水着です。"},{"id":{"d":799,"n":5,"t":5},"speaker":"A","text":"両方を用意したのですか？"},{"id":{"d":799,"n":6,"t":5},"speaker":"B","text":"ビーチに行って周りを見渡して、その場の雰囲気に合う方を着ようと思っています。"},{"id":{"d":799,"n":7,"t":5},"speaker":"A","text":"私はビキニしか用意していません。"},{"id":{"d":799,"n":8,"t":5},"speaker":"B","text":"それなら私もビキニにします。"}];
INSERT INTO sentence [{"id":{"d":800,"n":1,"t":5},"speaker":"A","text":"豪華なお弁当ですね。"},{"id":{"d":800,"n":2,"t":5},"speaker":"B","text":"今日は土用の丑の日ですから、うなぎ弁当を買いました。"},{"id":{"d":800,"n":3,"t":5},"speaker":"A","text":"お弁当がもう冷めてしまっているのではありませんか？"},{"id":{"d":800,"n":4,"t":5},"speaker":"B","text":"いいえ。先ほど、給湯室の電子レンジで温めてきました。"},{"id":{"d":800,"n":5,"t":5},"speaker":"A","text":"用意がいいですね。私もうなぎ弁当にすればよかったです。"},{"id":{"d":800,"n":6,"t":5},"speaker":"B","text":"佐藤さんはコンビニエンスストアの弁当ですか？"},{"id":{"d":800,"n":7,"t":5},"speaker":"A","text":"さっぱりしたものが食べたかったので、冷やしうどんを買いました。"},{"id":{"d":800,"n":8,"t":5},"speaker":"B","text":"土用の丑の日は「う」が付く食べ物を食べればよいのですよ。「うどん」も正解です。"}];
INSERT INTO sentence [{"id":{"d":801,"n":1,"t":5},"speaker":"A","text":"今日、お店で流れている曲は雨宮さんが持ってきたCDだそうですね。"},{"id":{"d":801,"n":2,"t":5},"speaker":"B","text":"はい、そうです。"},{"id":{"d":801,"n":3,"t":5},"speaker":"A","text":"どのような基準で選曲されたのですか？"},{"id":{"d":801,"n":4,"t":5},"speaker":"B","text":"今年は空梅雨なので、雨が降るように雨の歌ばかりを集めてみました。"},{"id":{"d":801,"n":5,"t":5},"speaker":"A","text":"そう言われてみると、確かに、全て雨の歌ですね。"},{"id":{"d":801,"n":6,"t":5},"speaker":"B","text":"雨が降らないと、私たちの店も困るでしょう？"},{"id":{"d":801,"n":7,"t":5},"speaker":"A","text":"このお店は傘屋ですからね。"},{"id":{"d":801,"n":8,"t":5},"speaker":"B","text":"この雨乞いが少しは効いてくれるといいのですが。"}];
INSERT INTO sentence [{"id":{"d":802,"n":1,"t":5},"speaker":"A","text":"今度の日曜日に、幼稚園のお友達同士で集まって、ハロウィンパーティーを開きます。"},{"id":{"d":802,"n":2,"t":5},"speaker":"B","text":"仮装をして行けばよいのですか？"},{"id":{"d":802,"n":3,"t":5},"speaker":"A","text":"はい。お子さんと一緒に、魔女やお化けや黒猫の仮装をして来てください。"},{"id":{"d":802,"n":4,"t":5},"speaker":"B","text":"お菓子も用意した方がいいのでしょうか？"},{"id":{"d":802,"n":5,"t":5},"speaker":"A","text":"いいえ、要りません。事前に会費として1,000円を集めて、お菓子と飲み物はこちらで用意します。"},{"id":{"d":802,"n":6,"t":5},"speaker":"B","text":"お料理はどうしますか？"},{"id":{"d":802,"n":7,"t":5},"speaker":"A","text":"お茶会のようなものなので、軽食の差し入れは歓迎しますが、何も持ってこなくても構いませんよ。"},{"id":{"d":802,"n":8,"t":5},"speaker":"B","text":"分かりました。当日を楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":803,"n":1,"t":5},"speaker":"A","text":"重そうな荷物ですね。"},{"id":{"d":803,"n":2,"t":5},"speaker":"B","text":"年末年始は実家にも帰らず一人で過ごす、と言ったら、ある人が大量に漫画を貸してくれたのです。"},{"id":{"d":803,"n":3,"t":5},"speaker":"A","text":"もしかしてその人は手塚さんですか？"},{"id":{"d":803,"n":4,"t":5},"speaker":"B","text":"その通りです。同じ苗字の漫画家の作品をたくさん持っているのだそうです。"},{"id":{"d":803,"n":5,"t":5},"speaker":"A","text":"私も去年の年末に同じように漫画を借りました。"},{"id":{"d":803,"n":6,"t":5},"speaker":"B","text":"年末年始で読み切れましたか？"},{"id":{"d":803,"n":7,"t":5},"speaker":"A","text":"いいえ、読み切れませんでした。一人でいてもずっと漫画を読んで過ごすわけにはいきませんからね。"},{"id":{"d":803,"n":8,"t":5},"speaker":"B","text":"そうですよね。お酒を飲む時間の方が長くなりそうです。"}];
INSERT INTO sentence [{"id":{"d":804,"n":1,"t":5},"speaker":"A","text":"お父さん、お正月に見るDVDを買ってもらえませんか？"},{"id":{"d":804,"n":2,"t":5},"speaker":"B","text":"構いませんが、DVDを見る時間はありそうですか？冬休みには宿題はないのですか？"},{"id":{"d":804,"n":3,"t":5},"speaker":"A","text":"はい、宿題はありません。"},{"id":{"d":804,"n":4,"t":5},"speaker":"B","text":"そうですか。何のDVDを買ってほしいのですか？"},{"id":{"d":804,"n":5,"t":5},"speaker":"A","text":"『クレヨンしんちゃん』です。"},{"id":{"d":804,"n":6,"t":5},"speaker":"B","text":"テレビ版と映画版があると思いますが、どちらが欲しいですか？"},{"id":{"d":804,"n":7,"t":5},"speaker":"A","text":"映画版のDVDをお願いします。できたら二つか三つ買ってほしいです。"},{"id":{"d":804,"n":8,"t":5},"speaker":"B","text":"今回はせめて二つにしておきましょう。"},{"id":{"d":804,"n":9,"t":5},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":805,"n":1,"t":5},"speaker":"A","text":"珍しく東京に雪が積もりましたね。"},{"id":{"d":805,"n":2,"t":5},"speaker":"B","text":"電車が止まってしまったので、今日は出社しなくてよいそうです。"},{"id":{"d":805,"n":3,"t":5},"speaker":"A","text":"子どもたちも休みだそうです。"},{"id":{"d":805,"n":4,"t":5},"speaker":"B","text":"それなら子どもたちと雪遊びでもしようと思います。"},{"id":{"d":805,"n":5,"t":5},"speaker":"A","text":"この程度の雪なら、雪だるまも小さなものしかできそうにありませんよ。"},{"id":{"d":805,"n":6,"t":5},"speaker":"B","text":"雪うさぎくらいのものなら作れませんか？"},{"id":{"d":805,"n":7,"t":5},"speaker":"A","text":"それくらいなら作れそうです。お隣の家にお願いして、センリョウの実を頂いてきましょうか？"},{"id":{"d":805,"n":8,"t":5},"speaker":"B","text":"うさぎの目にするのですね。耳はうちの木の葉でよいですね。"}];
INSERT INTO sentence [{"id":{"d":806,"n":1,"t":5},"speaker":"A","text":"今晩のお祭りには浴衣を着ていきますか？"},{"id":{"d":806,"n":2,"t":5},"speaker":"B","text":"いいえ、歩きにくいので私は甚平にします。"},{"id":{"d":806,"n":3,"t":5},"speaker":"A","text":"金魚すくいやヨーヨー釣りをするのであればしゃがみますから、甚平の方が動きやすそうですね。"},{"id":{"d":806,"n":4,"t":5},"speaker":"B","text":"そう思います。"},{"id":{"d":806,"n":5,"t":5},"speaker":"A","text":"浴衣より情緒は薄れるように思いますが、仕方ありませんね。"},{"id":{"d":806,"n":6,"t":5},"speaker":"B","text":"私にとっては、お祭りでは情緒よりも夜店での戦利品が重要です。"},{"id":{"d":806,"n":7,"t":5},"speaker":"A","text":"あなたにとってお祭りは戦いなのですね。"}];
INSERT INTO sentence [{"id":{"d":807,"n":1,"t":5},"speaker":"A","text":"お渡しする約束をしていたドラマのDVDです。どうぞ。"},{"id":{"d":807,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":807,"n":3,"t":5},"speaker":"A","text":"私のハードディスクに元の映像が残っているので、返さなくてよいですよ。"},{"id":{"d":807,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。では、ゆっくり拝見します。"},{"id":{"d":807,"n":5,"t":5},"speaker":"A","text":"私も大好きなドラマなので、あなたが最終回まで見終わったら、感想を語り合いましょうね。"},{"id":{"d":807,"n":6,"t":5},"speaker":"B","text":"分かりました。私は初回を見逃して、ブームに乗り遅れてしまったのですよね。"},{"id":{"d":807,"n":7,"t":5},"speaker":"A","text":"本当に面白いのでぜひ見てください。私は何度も見返していて、まだブームの最中です。"},{"id":{"d":807,"n":8,"t":5},"speaker":"B","text":"熱が冷めないうちに見ようと思います。"}];
INSERT INTO sentence [{"id":{"d":808,"n":1,"t":5},"speaker":"A","text":"お母さん、温かいお汁粉が食べたいです。"},{"id":{"d":808,"n":2,"t":5},"speaker":"B","text":"あさってが鏡開きなのですから、今日は我慢しましょう。"},{"id":{"d":808,"n":3,"t":5},"speaker":"A","text":"鏡開きにはお汁粉を作ってくれますか？"},{"id":{"d":808,"n":4,"t":5},"speaker":"B","text":"はい。おばあちゃんに頂いた小豆がありますから作る予定ですよ。"},{"id":{"d":808,"n":5,"t":5},"speaker":"A","text":"私は甘めのお汁粉がいいです。"},{"id":{"d":808,"n":6,"t":5},"speaker":"B","text":"去年は、お砂糖を控え気味にしたのを覚えているのですね。"},{"id":{"d":808,"n":7,"t":5},"speaker":"A","text":"はい。せっかくのお汁粉なのに私には甘さが足りなくて残念でした。"},{"id":{"d":808,"n":8,"t":5},"speaker":"B","text":"分かりました。今年はあなたのご要望に従って甘くしますよ。"}];
INSERT INTO sentence [{"id":{"d":809,"n":1,"t":5},"speaker":"A","text":"父の日のプレゼントを探しています。"},{"id":{"d":809,"n":2,"t":5},"speaker":"B","text":"こちらのネクタイなどはいかがでしょうか？"},{"id":{"d":809,"n":3,"t":5},"speaker":"A","text":"去年まではネクタイをプレゼントしていたのですが、父は3月に定年退職をしたので今年は違うものがいいと思っています。"},{"id":{"d":809,"n":4,"t":5},"speaker":"B","text":"では、食べ物やお酒はいかがですか？"},{"id":{"d":809,"n":5,"t":5},"speaker":"A","text":"父はおしゃれが好きなので、ファッションに関係するものをプレゼントしたいのです。"},{"id":{"d":809,"n":6,"t":5},"speaker":"B","text":"それではこちらのリラクシングウエアはいかがでしょうか？"},{"id":{"d":809,"n":7,"t":5},"speaker":"A","text":"これはゆったりしているのに、程よくきちんとした感じもあって良いですね。"},{"id":{"d":809,"n":8,"t":5},"speaker":"B","text":"ええ、それがこのウエアのポイントです。これを着たまま、ご近所への買い物にも出かけられますよ。"}];
INSERT INTO sentence [{"id":{"d":810,"n":1,"t":5},"speaker":"A","text":"米寿のお祝いに、孫からビデオメッセージが届きました。"},{"id":{"d":810,"n":2,"t":5},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":810,"n":3,"t":5},"speaker":"A","text":"息子がスマートフォンで見せてくれました。"},{"id":{"d":810,"n":4,"t":5},"speaker":"B","text":"息子さんのスマートフォンに送られてきたのですね。"},{"id":{"d":810,"n":5,"t":5},"speaker":"A","text":"はい。近頃は、ビデオとお話ができるのですね。"},{"id":{"d":810,"n":6,"t":5},"speaker":"B","text":"お孫さんとお話しされたのですか？"},{"id":{"d":810,"n":7,"t":5},"speaker":"A","text":"しました。ビデオメッセージなのに、話しかけるときちんと返事をしてくれるのです。"},{"id":{"d":810,"n":8,"t":5},"speaker":"B","text":"それはきっとビデオメッセージではなく、テレビ電話ですよ。"}];
INSERT INTO sentence [{"id":{"d":811,"n":1,"t":5},"speaker":"A","text":"みゆさんとお誕生日が一日違いでうれしいです。"},{"id":{"d":811,"n":2,"t":5},"speaker":"B","text":"私もうれしいです。早速、プレゼント交換をしましょう。"},{"id":{"d":811,"n":3,"t":5},"speaker":"A","text":"はい。私からは髪留めをプレゼントします。"},{"id":{"d":811,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。私からはマーカーペンです。"},{"id":{"d":811,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":811,"n":6,"t":5},"speaker":"B","text":"ひよりさん、11歳おめでとうございます。"},{"id":{"d":811,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。みゆさんも、明日、11歳おめでとうございます。"},{"id":{"d":811,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。では、お母さんたちが用意してくれたケーキをいただきましょう。"}];
INSERT INTO sentence [{"id":{"d":812,"n":1,"t":5},"speaker":"A","text":"今年のクリスマスプレゼントはピザでした。"},{"id":{"d":812,"n":2,"t":5},"speaker":"B","text":"宅配のピザを注文したのですか？"},{"id":{"d":812,"n":3,"t":5},"speaker":"A","text":"そうです。そうしたら、サンタさんがバイクに乗ってピザを持ってきてくれました。"},{"id":{"d":812,"n":4,"t":5},"speaker":"B","text":"サンタさんは他にプレゼントはくれなかったのですか？"},{"id":{"d":812,"n":5,"t":5},"speaker":"A","text":"枕元にはありませんでした。お父さんに聞いたら「ピザをもらったでしょう？」と言われました。"},{"id":{"d":812,"n":6,"t":5},"speaker":"B","text":"サンタさんは、ピザが好きな子にピザをプレゼントしてくれたのでしょうね。"},{"id":{"d":812,"n":7,"t":5},"speaker":"A","text":"ピザはとてもおいしかったです。"},{"id":{"d":812,"n":8,"t":5},"speaker":"B","text":"それは良かったです。来年のプレゼントもピザかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":813,"n":1,"t":5},"speaker":"A","text":"バレンタインデーには誰かにチョコレートをあげましたか？"},{"id":{"d":813,"n":2,"t":5},"speaker":"B","text":"私は誰にもあげませんでした。でも、たくさんチョコレートをもらいました。"},{"id":{"d":813,"n":3,"t":5},"speaker":"A","text":"チョコレートをもらったのですか？"},{"id":{"d":813,"n":4,"t":5},"speaker":"B","text":"はい。後輩や先輩、クラスの友達からももらいました。"},{"id":{"d":813,"n":5,"t":5},"speaker":"A","text":"最近は友達にチョコをあげるのが流行しているそうですね。"},{"id":{"d":813,"n":6,"t":5},"speaker":"B","text":"そのようです。チョコをくれた人にはホワイトデーにお返しをしなければいけないでしょうか？"},{"id":{"d":813,"n":7,"t":5},"speaker":"A","text":"女子同士だったらホワイトデーに合わせてお返しをしなくてもよいのではありませんか？でも、一方的に頂いたままというのも申し訳ないので、何かの機会にお返しするのもいいかもしれませんね。"},{"id":{"d":813,"n":8,"t":5},"speaker":"B","text":"そうですね。考えてみます。"}];
INSERT INTO sentence [{"id":{"d":814,"n":1,"t":5},"speaker":"A","text":"きれいなお花の写真ですね。"},{"id":{"d":814,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。スーパーの展示コーナーで展示会があるのですが、この写真はそこで飾ってもらうパネルにするためのものです。"},{"id":{"d":814,"n":3,"t":5},"speaker":"A","text":"これは一眼レフカメラで撮ったのですか？"},{"id":{"d":814,"n":4,"t":5},"speaker":"B","text":"いいえ、スマートフォンで撮りました。"},{"id":{"d":814,"n":5,"t":5},"speaker":"A","text":"スマートフォンでもこのようにきれいに撮れるのですね。"},{"id":{"d":814,"n":6,"t":5},"speaker":"B","text":"カルチャーセンターのスマートフォン写真教室に通っているのです。"},{"id":{"d":814,"n":7,"t":5},"speaker":"A","text":"きちんと教わると上手に撮影ができるのですね。"},{"id":{"d":814,"n":8,"t":5},"speaker":"B","text":"学ぶことが多くて、楽しいです。次からは人物撮影のコースが始まります。それも楽しみです。"}];
INSERT INTO sentence [{"id":{"d":815,"n":1,"t":5},"speaker":"A","text":"和菓子屋さんに人がたくさんいますね。"},{"id":{"d":815,"n":2,"t":5},"speaker":"B","text":"今日からお彼岸なので、おはぎを買っているのでしょう。"},{"id":{"d":815,"n":3,"t":5},"speaker":"A","text":"私はおはぎが大好きです。"},{"id":{"d":815,"n":4,"t":5},"speaker":"B","text":"では、あなたも買っていかれたらいかがですか？"},{"id":{"d":815,"n":5,"t":5},"speaker":"A","text":"私の家には仏壇はありませんが、買ってもいいのでしょうか？"},{"id":{"d":815,"n":6,"t":5},"speaker":"B","text":"仏壇の有無に関係なく、普通に和菓子として食べればよいのではありませんか？"},{"id":{"d":815,"n":7,"t":5},"speaker":"A","text":"そうですよね。今日のおはぎは特においしそうに見えます。あなたも何か買っていきますか？"},{"id":{"d":815,"n":8,"t":5},"speaker":"B","text":"私はもなかを買おうと思います。"}];
INSERT INTO sentence [{"id":{"d":816,"n":1,"t":5},"speaker":"A","text":"河原にナズナが生えていたので採ってきました。"},{"id":{"d":816,"n":2,"t":5},"speaker":"B","text":"ナズナを採ってどうするのですか？"},{"id":{"d":816,"n":3,"t":5},"speaker":"A","text":"七草がゆに入れようと思います。"},{"id":{"d":816,"n":4,"t":5},"speaker":"B","text":"この辺りは犬の散歩コースですから、よく洗わないといけませんよ。"},{"id":{"d":816,"n":5,"t":5},"speaker":"A","text":"分かりました。よく洗って使います。"},{"id":{"d":816,"n":6,"t":5},"speaker":"B","text":"私はよく洗っても料理に使う気にはなりませんけれどね。"},{"id":{"d":816,"n":7,"t":5},"speaker":"A","text":"七草がゆは食べないのですか？"},{"id":{"d":816,"n":8,"t":5},"speaker":"B","text":"食べますよ。私はいつもスーパーの七草セットを買って作っています。"}];
INSERT INTO sentence [{"id":{"d":817,"n":1,"t":5},"speaker":"A","text":"今度の月曜日はスポーツの日でお休みです。"},{"id":{"d":817,"n":2,"t":5},"speaker":"B","text":"その日は地区運動会があるので、今から楽しみです。"},{"id":{"d":817,"n":3,"t":5},"speaker":"A","text":"ところで、以前は「スポーツの日」が「体育の日」と言われていたのを知っていますか？"},{"id":{"d":817,"n":4,"t":5},"speaker":"B","text":"いいえ、知りませんでした。"},{"id":{"d":817,"n":5,"t":5},"speaker":"A","text":"皆さんが1年生の時から「スポーツの日」になったのですよ。"},{"id":{"d":817,"n":6,"t":5},"speaker":"B","text":"「体育の日」は体育の授業をする日だったのですか？"},{"id":{"d":817,"n":7,"t":5},"speaker":"A","text":"いいえ。1964年に開催された東京オリンピックの開会式を記念してつくられた祝日です。"}];
INSERT INTO sentence [{"id":{"d":818,"n":1,"t":5},"speaker":"A","text":"彰君はずっと積み木で遊んでいるのですね。"},{"id":{"d":818,"n":2,"t":5},"speaker":"B","text":"あれは積み木のように見えますが、違うものですよ。"},{"id":{"d":818,"n":3,"t":5},"speaker":"A","text":"そう言われれば、ところどころに穴が開いていますね。あれは何ですか？"},{"id":{"d":818,"n":4,"t":5},"speaker":"B","text":"あれはゲームの一種で、うまく組むと穴を通って球が下まで落ちるのです。"},{"id":{"d":818,"n":5,"t":5},"speaker":"A","text":"頭を使って考えるゲームなのですね。"},{"id":{"d":818,"n":6,"t":5},"speaker":"B","text":"そうです。義母が頭の良い子に育つようにと買い与えてくれたのです。"},{"id":{"d":818,"n":7,"t":5},"speaker":"A","text":"夢中になっている様子を見ると、順調に頭の良い子に育っているように見えますよ。"},{"id":{"d":818,"n":8,"t":5},"speaker":"B","text":"買ってくれた義母には悪いのですが、私は彰には頭の良い子よりも優しい子になってほしいと思っています。"}];
INSERT INTO sentence [{"id":{"d":819,"n":1,"t":5},"speaker":"A","text":"明日は晴れますか？"},{"id":{"d":819,"n":2,"t":5},"speaker":"B","text":"天気予報では晴れると言っていましたよ。"},{"id":{"d":819,"n":3,"t":5},"speaker":"A","text":"それは良かったです。今日、学校で日食観測グラスをもらったので、明日それを使うことが楽しみです。"},{"id":{"d":819,"n":4,"t":5},"speaker":"B","text":"そういえば明日は日食ですね。見られるといいですね。"},{"id":{"d":819,"n":5,"t":5},"speaker":"A","text":"はい。日食の様子を観察して、時間ごとの太陽の絵を描いていくという宿題が出ています。"},{"id":{"d":819,"n":6,"t":5},"speaker":"B","text":"そうなのですね。明日は土曜日ですが、お出かけする子はどうするのですか？"},{"id":{"d":819,"n":7,"t":5},"speaker":"A","text":"お出かけ先で見るのだそうです。"},{"id":{"d":819,"n":8,"t":5},"speaker":"B","text":"全国的に晴れるといいですね。"}];
INSERT INTO sentence [{"id":{"d":820,"n":1,"t":5},"speaker":"A","text":"今日はどこに行くのですか？"},{"id":{"d":820,"n":2,"t":5},"speaker":"B","text":"海岸まで行こうと思っています。"},{"id":{"d":820,"n":3,"t":5},"speaker":"A","text":"海岸で散歩をするのですか？"},{"id":{"d":820,"n":4,"t":5},"speaker":"B","text":"はい。お散歩がてらにビーチコーミングをしようと思っています。"},{"id":{"d":820,"n":5,"t":5},"speaker":"A","text":"何か目的はあるのですか？"},{"id":{"d":820,"n":6,"t":5},"speaker":"B","text":"流木でインテリアを作りたいのです。"},{"id":{"d":820,"n":7,"t":5},"speaker":"A","text":"もしかして、車庫のところに置いてある木も流木なのですか？"},{"id":{"d":820,"n":8,"t":5},"speaker":"B","text":"そうです。もう一つくらいあると良い作品ができそうなのです。"}];
INSERT INTO sentence [{"id":{"d":821,"n":1,"t":5},"speaker":"A","text":"居間の照明を作りましょう。"},{"id":{"d":821,"n":2,"t":5},"speaker":"B","text":"お父さん、私の拾ってきた貝殻も付けてよいですか？"},{"id":{"d":821,"n":3,"t":5},"speaker":"A","text":"よいですが、どこに付けたいのですか？"},{"id":{"d":821,"n":4,"t":5},"speaker":"B","text":"ここです。"},{"id":{"d":821,"n":5,"t":5},"speaker":"A","text":"いいですね。でも、1カ所だと寂しい気がするので、こことそこの2カ所に付けると良さそうです。"},{"id":{"d":821,"n":6,"t":5},"speaker":"B","text":"大切な貝殻なので惜しい気もしますが、3つまででしたら使っても構いません。"},{"id":{"d":821,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。貝殻のおかげでかわいい照明器具ができますね。"},{"id":{"d":821,"n":8,"t":5},"speaker":"B","text":"とてもすてきです。もっと貝殻が欲しいので、また貝拾いに連れて行ってください。"}];
INSERT INTO sentence [{"id":{"d":822,"n":1,"t":5},"speaker":"A","text":"それは小さいスケッチブックですか？"},{"id":{"d":822,"n":2,"t":5},"speaker":"B","text":"いいえ、これはスケッチブックにそっくりのポーチです。"},{"id":{"d":822,"n":3,"t":5},"speaker":"A","text":"表紙のデザインがそのままポーチになっているのですね。"},{"id":{"d":822,"n":4,"t":5},"speaker":"B","text":"そうです。レトロ文具シリーズとして売られているのですよ。"},{"id":{"d":822,"n":5,"t":5},"speaker":"A","text":"すてきですね。かわいいし、使いやすそうです。"},{"id":{"d":822,"n":6,"t":5},"speaker":"B","text":"そうでしょう？このシリーズはどれもかわいいし使いやすいのです。"},{"id":{"d":822,"n":7,"t":5},"speaker":"A","text":"さては他にも買いましたね。"},{"id":{"d":822,"n":8,"t":5},"speaker":"B","text":"はい。文具マニアになってしまいそうです。"}];
INSERT INTO sentence [{"id":{"d":823,"n":1,"t":5},"speaker":"A","text":"今日は夕飯の後に楽器の練習をするのですか？"},{"id":{"d":823,"n":2,"t":5},"speaker":"B","text":"はい。お祭りが近いので、夜も太鼓と笛の練習があります。"},{"id":{"d":823,"n":3,"t":5},"speaker":"A","text":"食事を済ませたら学校まで車で送っていきますよ。"},{"id":{"d":823,"n":4,"t":5},"speaker":"B","text":"行きは健君のお母さんが送ってくれるので大丈夫です。"},{"id":{"d":823,"n":5,"t":5},"speaker":"A","text":"それでは、帰りは私が迎えに行きましょう。"},{"id":{"d":823,"n":6,"t":5},"speaker":"B","text":"健君のお母さんにそう言っておきます。"},{"id":{"d":823,"n":7,"t":5},"speaker":"A","text":"練習が終了するのは8時30分でしたか？"},{"id":{"d":823,"n":8,"t":5},"speaker":"B","text":"はい、そうです。"}];
INSERT INTO sentence [{"id":{"d":824,"n":1,"t":5},"speaker":"A","text":"このところはずっとピアノを弾いていますね。"},{"id":{"d":824,"n":2,"t":5},"speaker":"B","text":"はい。動画を撮ってインターネットに上げようと思っているのです。"},{"id":{"d":824,"n":3,"t":5},"speaker":"A","text":"誰に見せたいのですか？"},{"id":{"d":824,"n":4,"t":5},"speaker":"B","text":"世界中の人と言いたいところですが、私が前に習っていたピアノの先生に見せたいのです。"},{"id":{"d":824,"n":5,"t":5},"speaker":"A","text":"引っ越す前にお世話になっていた城山先生ですね。"},{"id":{"d":824,"n":6,"t":5},"speaker":"B","text":"そうです。遠くに離れていても頑張っていますと伝えたいと思っています。"},{"id":{"d":824,"n":7,"t":5},"speaker":"A","text":"すてきですね。撮影がうまくいくように、私に手伝えることがあれば言ってください。"},{"id":{"d":824,"n":8,"t":5},"speaker":"B","text":"ありがとうございます、お母さん。"}];
INSERT INTO sentence [{"id":{"d":825,"n":1,"t":5},"speaker":"A","text":"お中元にそうめんを頂きました。"},{"id":{"d":825,"n":2,"t":5},"speaker":"B","text":"たくさん頂いたのですか？"},{"id":{"d":825,"n":3,"t":5},"speaker":"A","text":"はい、たくさんありますよ。"},{"id":{"d":825,"n":4,"t":5},"speaker":"B","text":"では、今度の日曜日に流しそうめんをしませんか？"},{"id":{"d":825,"n":5,"t":5},"speaker":"A","text":"いいですね。でも道具はどうするのですか？"},{"id":{"d":825,"n":6,"t":5},"speaker":"B","text":"おもちゃ屋さんに大きな流しそうめんの装置があったので、それを買おうと思います。"},{"id":{"d":825,"n":7,"t":5},"speaker":"A","text":"なるほど。子どもたちが喜びそうですね。"},{"id":{"d":825,"n":8,"t":5},"speaker":"B","text":"明日、内緒で買ってきます。"}];
INSERT INTO sentence [{"id":{"d":826,"n":1,"t":5},"speaker":"A","text":"私たちのマンションはベランダが広くて良かったですね。"},{"id":{"d":826,"n":2,"t":5},"speaker":"B","text":"はい。おかげで子ども用のプールも大きいサイズのものが置けました。"},{"id":{"d":826,"n":3,"t":5},"speaker":"A","text":"子どもたちも楽しそうですね。"},{"id":{"d":826,"n":4,"t":5},"speaker":"B","text":"暑くなり過ぎたら、すぐ家に逃げ込めるのも良いです。"},{"id":{"d":826,"n":5,"t":5},"speaker":"A","text":"そうですね。でも、すぐに逃げ込んで、ビールを飲んでばかりはいないでくださいね。"},{"id":{"d":826,"n":6,"t":5},"speaker":"B","text":"分かりました。せめてお昼までは我慢します。"},{"id":{"d":826,"n":7,"t":5},"speaker":"A","text":"そうしてください。もう少ししたら、かき氷を作りましょうか？"},{"id":{"d":826,"n":8,"t":5},"speaker":"B","text":"良いですね。私が作りましょう。"}];
INSERT INTO sentence [{"id":{"d":827,"n":1,"t":5},"speaker":"A","text":"おじいちゃんの家で遊ぶのは楽しいです。"},{"id":{"d":827,"n":2,"t":5},"speaker":"B","text":"そうですか？それは良かったです。"},{"id":{"d":827,"n":3,"t":5},"speaker":"A","text":"明日の朝もカブトムシを採りに連れて行ってくれますか？"},{"id":{"d":827,"n":4,"t":5},"speaker":"B","text":"もちろんよいですよ。早起きできますか？"},{"id":{"d":827,"n":5,"t":5},"speaker":"A","text":"今日は早起きできました。明日もきっと大丈夫です。"},{"id":{"d":827,"n":6,"t":5},"speaker":"B","text":"それでは虫かごと網を用意しておいてください。"},{"id":{"d":827,"n":7,"t":5},"speaker":"A","text":"はい。楽しみです。"},{"id":{"d":827,"n":8,"t":5},"speaker":"B","text":"明日はクワガタも採れるといいですね。"}];
INSERT INTO sentence [{"id":{"d":828,"n":1,"t":5},"speaker":"A","text":"ここ数日はお天気が急変することが多いですね。"},{"id":{"d":828,"n":2,"t":5},"speaker":"B","text":"そうですね。先日、いとこの車がひょうでへこんでしまいました。"},{"id":{"d":828,"n":3,"t":5},"speaker":"A","text":"ひょうの被害をニュースで見ました。いとこはあの地域に住んでいるのですか？"},{"id":{"d":828,"n":4,"t":5},"speaker":"B","text":"そうです。ひょうで庭が真っ白になったと言っていました。"},{"id":{"d":828,"n":5,"t":5},"speaker":"A","text":"大変でしたね。"},{"id":{"d":828,"n":6,"t":5},"speaker":"B","text":"わが家の雨による被害は、洗濯物がぬれただけでよかったです。"},{"id":{"d":828,"n":7,"t":5},"speaker":"A","text":"今の時期は外干しは避けた方がいいですね。"},{"id":{"d":828,"n":8,"t":5},"speaker":"B","text":"はい。早くカラッと洗濯物が乾く天気になってほしいですね。"}];
INSERT INTO sentence [{"id":{"d":829,"n":1,"t":5},"speaker":"A","text":"私の消しゴムのコレクションが2,000個を超えました。今までは適当に段ボールに入れていたのですが、整理したいと思っています。"},{"id":{"d":829,"n":2,"t":5},"speaker":"B","text":"そうするべきです。100円ショップで箱を買ってくるといいですよ。"},{"id":{"d":829,"n":3,"t":5},"speaker":"A","text":"100円ショップには使えそうな箱があるのですか？"},{"id":{"d":829,"n":4,"t":5},"speaker":"B","text":"2,000個全てを入れられる箱は恐らくないでしょうね。でも、種類ごとにまとめる箱をいくつか買えばよいのではありませんか？"},{"id":{"d":829,"n":5,"t":5},"speaker":"A","text":"それは良い考えですね。私はガラスケースのようなものを買おうとしていました。"},{"id":{"d":829,"n":6,"t":5},"speaker":"B","text":"それも良さそうですが、100円ショップよりは値段が高そうですね。"}];
INSERT INTO sentence [{"id":{"d":830,"n":1,"t":5},"speaker":"A","text":"庭で育てたラベンダーでラベンダースティックを作りました。"},{"id":{"d":830,"n":2,"t":5},"speaker":"B","text":"ラベンダースティックは北海道のお土産に頂いたことがあります。"},{"id":{"d":830,"n":3,"t":5},"speaker":"A","text":"私が作ったものももらってくれますか？"},{"id":{"d":830,"n":4,"t":5},"speaker":"B","text":"くださるのですか？前に頂いたものはもう香りがなくなってしまったので、うれしいです。"},{"id":{"d":830,"n":5,"t":5},"speaker":"A","text":"好きな色のリボンを3本選んでください。"},{"id":{"d":830,"n":6,"t":5},"speaker":"B","text":"では、赤とピンク白でお願いします。これを作るのは難しくありませんか？"},{"id":{"d":830,"n":7,"t":5},"speaker":"A","text":"意外と簡単ですよ。はい、どうぞ。"},{"id":{"d":830,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。早速、玄関に飾らせていただきます。"}];
INSERT INTO sentence [{"id":{"d":831,"n":1,"t":5},"speaker":"A","text":"今、パンッという音がしませんでしたか？"},{"id":{"d":831,"n":2,"t":5},"speaker":"B","text":"驚かせてしまってすみません。風船が割れた音です。"},{"id":{"d":831,"n":3,"t":5},"speaker":"A","text":"風船で何をしているのですか？"},{"id":{"d":831,"n":4,"t":5},"speaker":"B","text":"夏美の誕生日にバルーンアートを作ってあげようと思って練習しているのです。"},{"id":{"d":831,"n":5,"t":5},"speaker":"A","text":"上手に作ることができれば夏美は喜ぶと思います。"},{"id":{"d":831,"n":6,"t":5},"speaker":"B","text":"「割れた風船の数を見ると、それはなかなか難しそうだ」という顔をしていますね。"},{"id":{"d":831,"n":7,"t":5},"speaker":"A","text":"そのようなことは思っていませんよ。頑張ってくださいね。"},{"id":{"d":831,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。あと1週間でかわいいプードルを作ってみせます。"}];
INSERT INTO sentence [{"id":{"d":832,"n":1,"t":5},"speaker":"A","text":"ポップコーンをどうぞ。"},{"id":{"d":832,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。まるで、映画館のポップコーンバケツのようですね。"},{"id":{"d":832,"n":3,"t":5},"speaker":"A","text":"今日のためにインターネットでお取り寄せをしたのです。コーラや涙を拭くためのタオルの用意しました。"},{"id":{"d":832,"n":4,"t":5},"speaker":"B","text":"準備万端ですね。"},{"id":{"d":832,"n":5,"t":5},"speaker":"A","text":"はい。早速、電気を消して、DVDを見ましょう。"},{"id":{"d":832,"n":6,"t":5},"speaker":"B","text":"映画館のようでワクワクします。"},{"id":{"d":832,"n":7,"t":5},"speaker":"A","text":"よかったです。でも、椅子の座り心地だけはどうすることもできませんでした。"},{"id":{"d":832,"n":8,"t":5},"speaker":"B","text":"全く気になりませんよ。それに、映画館とは違って寝そべることもできるので最高です。"}];
INSERT INTO sentence [{"id":{"d":833,"n":1,"t":5},"speaker":"A","text":"読書の秋ですね。"},{"id":{"d":833,"n":2,"t":5},"speaker":"B","text":"そうですね。でも、私が読んでいるのは漫画ですよ。"},{"id":{"d":833,"n":3,"t":5},"speaker":"A","text":"てっきり文庫本かと思いました。"},{"id":{"d":833,"n":4,"t":5},"speaker":"B","text":"文庫本のサイズの漫画です。"},{"id":{"d":833,"n":5,"t":5},"speaker":"A","text":"そのようなものがあるのですね。"},{"id":{"d":833,"n":6,"t":5},"speaker":"B","text":"はい。これは昔の漫画が文庫本サイズで再編されたものです。"},{"id":{"d":833,"n":7,"t":5},"speaker":"A","text":"たくさん積んでありますが、何巻も続く話なのですか？"},{"id":{"d":833,"n":8,"t":5},"speaker":"B","text":"はい。全部で15巻あります。秋の夜は長いですからまとめて読むことができますよ。"}];
INSERT INTO sentence [{"id":{"d":834,"n":1,"t":5},"speaker":"A","text":"夏休みになってから、毎日ピアノの音がします。"},{"id":{"d":834,"n":2,"t":5},"speaker":"B","text":"お隣の息子さんがピアノを習い始めたと聞きましたよ。"},{"id":{"d":834,"n":3,"t":5},"speaker":"A","text":"そうなのですね。始めたばかりなのに上手です。先生の教え方が素晴らしいのでしょうか？"},{"id":{"d":834,"n":4,"t":5},"speaker":"B","text":"お子さんに音楽の才能があるのかもしれませんよ。"},{"id":{"d":834,"n":5,"t":5},"speaker":"A","text":"そうかもしれませんね。もし、先生が素晴らしいのでしたら、その方を紹介していただいて、娘の美智香にも習わせたいと思ったのですが。"},{"id":{"d":834,"n":6,"t":5},"speaker":"B","text":"お隣の奥さんに先生を紹介していただけないか聞いてみましょうか？"},{"id":{"d":834,"n":7,"t":5},"speaker":"A","text":"よかったら聞いてみてください。"}];
INSERT INTO sentence [{"id":{"d":835,"n":1,"t":5},"speaker":"A","text":"プラモデルの部品をなくしてしまいました。"},{"id":{"d":835,"n":2,"t":5},"speaker":"B","text":"困りましたね。それがないと完成しないのですか？"},{"id":{"d":835,"n":3,"t":5},"speaker":"A","text":"そうなのです。部屋のどこかにはあると思うのですが、見つかりません。"},{"id":{"d":835,"n":4,"t":5},"speaker":"B","text":"プラモデルの部品の一つ一つは小さいものですからね。"},{"id":{"d":835,"n":5,"t":5},"speaker":"A","text":"もしかしたら、母が掃除機で吸い取ってしまったのかもしれません。"},{"id":{"d":835,"n":6,"t":5},"speaker":"B","text":"そうかもしれませんね。もし部品が見つからなければ、粘土などを使って自分で作ってみてはどうですか？"},{"id":{"d":835,"n":7,"t":5},"speaker":"A","text":"それは格好が悪いですよ。"},{"id":{"d":835,"n":8,"t":5},"speaker":"B","text":"では、なくした部品だけを買えるかどうかメーカーに問い合わせてみてはどうですか？"}];
INSERT INTO sentence [{"id":{"d":836,"n":1,"t":5},"speaker":"A","text":"文化センターで行われているフラダンス教室に入りました。"},{"id":{"d":836,"n":2,"t":5},"speaker":"B","text":"それは楽しそうですね。"},{"id":{"d":836,"n":3,"t":5},"speaker":"A","text":"楽しいです。それに。フラダンスを始めてから足腰が丈夫になったような気がします。"},{"id":{"d":836,"n":4,"t":5},"speaker":"B","text":"窓のところのハンガーに掛けてあるのは、フラダンスの衣装ですか？"},{"id":{"d":836,"n":5,"t":5},"speaker":"A","text":"そうですよ。今度の発表会のために皆とおそろいで作りました。"},{"id":{"d":836,"n":6,"t":5},"speaker":"B","text":"明るい色でとてもすてきですね。"},{"id":{"d":836,"n":7,"t":5},"speaker":"A","text":"少し前まではこのような色のドレスを着るなんて思いもしませんでしたよ。"},{"id":{"d":836,"n":8,"t":5},"speaker":"B","text":"きっとお似合いですよ。発表会には応援に行きますね。"}];
INSERT INTO sentence [{"id":{"d":837,"n":1,"t":5},"speaker":"A","text":"おじいちゃん、プレゼントがあります。"},{"id":{"d":837,"n":2,"t":5},"speaker":"B","text":"何ですか？"},{"id":{"d":837,"n":3,"t":5},"speaker":"A","text":"これをどうぞ。さあ、開けてみてください。"},{"id":{"d":837,"n":4,"t":5},"speaker":"B","text":"これは板ですか？"},{"id":{"d":837,"n":5,"t":5},"speaker":"A","text":"板のようなテーブルです。ジグソーパズルを組み立てるときに使うのですよ。"},{"id":{"d":837,"n":6,"t":5},"speaker":"B","text":"最近は私がジグソーパズルをしているから買ってくれたのですか？"},{"id":{"d":837,"n":7,"t":5},"speaker":"A","text":"はい、いつも新聞紙の上で作っているでしょう？それが大変そうだと思っていました。"},{"id":{"d":837,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。とてもうれしいです。良い孫を持って私は幸せです。"}];
INSERT INTO sentence [{"id":{"d":838,"n":1,"t":5},"speaker":"A","text":"何だかピーピーという音がしますね。"},{"id":{"d":838,"n":2,"t":5},"speaker":"B","text":"にぎやかですみません。"},{"id":{"d":838,"n":3,"t":5},"speaker":"A","text":"子どもたちは何をしているのですか？"},{"id":{"d":838,"n":4,"t":5},"speaker":"B","text":"お祭りで吹き戻しを買ってきたのです。"},{"id":{"d":838,"n":5,"t":5},"speaker":"A","text":"吹き戻しというのは何ですか？"},{"id":{"d":838,"n":6,"t":5},"speaker":"B","text":"息を吹き込むと伸びて、口を離すと縮む笛のことですよ。"},{"id":{"d":838,"n":7,"t":5},"speaker":"A","text":"あのおもちゃのことですね。ピーピーと聞こえていたのは笛だったのですね。"},{"id":{"d":838,"n":8,"t":5},"speaker":"B","text":"そうです。もう時間も遅いですからそろそろやめるよう注意してきます。"}];
INSERT INTO sentence [{"id":{"d":839,"n":1,"t":5},"speaker":"A","text":"今日はこどもの日ですから、銭湯に行きましょう。"},{"id":{"d":839,"n":2,"t":5},"speaker":"B","text":"どうしてに行くのですか？"},{"id":{"d":839,"n":3,"t":5},"speaker":"A","text":"しょうぶ湯に入るためです。今日はこどもの日ですからね。"},{"id":{"d":839,"n":4,"t":5},"speaker":"B","text":"銭湯に行かないとしょうぶ湯には入れないのですか？"},{"id":{"d":839,"n":5,"t":5},"speaker":"A","text":"家でもできますが、わが家のお風呂はしょうぶ湯にしません。"},{"id":{"d":839,"n":6,"t":5},"speaker":"B","text":"しょうぶ湯にするとお風呂が壊れてしまうのですか？"},{"id":{"d":839,"n":7,"t":5},"speaker":"A","text":"そういうわけではありませんが、残り湯を洗濯に使いたいのでしょうぶを入れたくありません。"},{"id":{"d":839,"n":8,"t":5},"speaker":"B","text":"そうであれば銭湯に行きます。お風呂上がりにはコーヒー牛乳を買ってください。"},{"id":{"d":839,"n":9,"t":5},"speaker":"A","text":"いいですよ。"}];
INSERT INTO sentence [{"id":{"d":840,"n":1,"t":5},"speaker":"A","text":"たくさん雪が降りましたね。"},{"id":{"d":840,"n":2,"t":5},"speaker":"B","text":"そうですね。子どもたちは庭と駐車場で雪だるまを作るそうです。"},{"id":{"d":840,"n":3,"t":5},"speaker":"A","text":"お宅は広いから良いですね。"},{"id":{"d":840,"n":4,"t":5},"speaker":"B","text":"よかったら、浩二君と仁香ちゃんもいらしてください。"},{"id":{"d":840,"n":5,"t":5},"speaker":"A","text":"ご一緒してもよいのですか？"},{"id":{"d":840,"n":6,"t":5},"speaker":"B","text":"もちろんです。体のいい雪かきのようなものですから。"},{"id":{"d":840,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。きっと喜びます。"},{"id":{"d":840,"n":8,"t":5},"speaker":"B","text":"お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":841,"n":1,"t":5},"speaker":"A","text":"24日はクリスマスパーティーをします。"},{"id":{"d":841,"n":2,"t":5},"speaker":"B","text":"パーティーは家でするのですか？"},{"id":{"d":841,"n":3,"t":5},"speaker":"A","text":"はい。よかったらいらっしゃいませんか？"},{"id":{"d":841,"n":4,"t":5},"speaker":"B","text":"よいのですか？ご家族で楽しまれるのではないのですか？"},{"id":{"d":841,"n":5,"t":5},"speaker":"A","text":"山口さんなら大歓迎です。もしよければ、鶏肉を持参していただけるとうれしいです。"},{"id":{"d":841,"n":6,"t":5},"speaker":"B","text":"ケーキではなくて鶏肉を持って行くのですか？"},{"id":{"d":841,"n":7,"t":5},"speaker":"A","text":"24日は鶏すき焼きにする予定なのです。白菜とネギもあるとうれしいです。"},{"id":{"d":841,"n":8,"t":5},"speaker":"B","text":"それはクリスマスパーティーではなく、鍋を囲む忘年会ですね。"}];
INSERT INTO sentence [{"id":{"d":842,"n":1,"t":5},"speaker":"A","text":"暑い暑いと思っていたら、暦に大暑とありました。"},{"id":{"d":842,"n":2,"t":5},"speaker":"B","text":"今日は本当に暑いですね。暑気払いにうなぎでも食べに行きませんか？"},{"id":{"d":842,"n":3,"t":5},"speaker":"A","text":"いいですね。"},{"id":{"d":842,"n":4,"t":5},"speaker":"B","text":"大塚に良いお店があるのですよ。"},{"id":{"d":842,"n":5,"t":5},"speaker":"A","text":"うな重が食べられればどこでもよいですよ。"},{"id":{"d":842,"n":6,"t":5},"speaker":"B","text":"うな重の他にもいろいろなうなぎ料理があるので、あなたもきっと気に入ると思います。"},{"id":{"d":842,"n":7,"t":5},"speaker":"A","text":"お酒も飲めますか？"},{"id":{"d":842,"n":8,"t":5},"speaker":"B","text":"はい、飲めます。"}];
INSERT INTO sentence [{"id":{"d":843,"n":1,"t":5},"speaker":"A","text":"線香花火を買ってきました。"},{"id":{"d":843,"n":2,"t":5},"speaker":"B","text":"お母さん、線香花火以外の花火はありませんか？"},{"id":{"d":843,"n":3,"t":5},"speaker":"A","text":"今回はありません。"},{"id":{"d":843,"n":4,"t":5},"speaker":"B","text":"ねずみ花火などの派手な花火もしたかったです。"},{"id":{"d":843,"n":5,"t":5},"speaker":"A","text":"手作りの線香花火のお店に買いに行ったのですから、他の花火はありません。"},{"id":{"d":843,"n":6,"t":5},"speaker":"B","text":"専門店の線香花火なら、特別感がありますね。ろうそくとバケツを用意します。"},{"id":{"d":843,"n":7,"t":5},"speaker":"A","text":"お願いします。とても長く楽しめる花火ですから、一緒に火を付けて誰が一番になるかを競争しましょう。"},{"id":{"d":843,"n":8,"t":5},"speaker":"B","text":"楽しそうです。せっかくなら、一番長い時間持った人が一番大きいスイカを食べられるということにしませんか？"}];
INSERT INTO sentence [{"id":{"d":844,"n":1,"t":5},"speaker":"A","text":"海の日に、砂の彫刻コンテストがあります。"},{"id":{"d":844,"n":2,"t":5},"speaker":"B","text":"参加するのですか？"},{"id":{"d":844,"n":3,"t":5},"speaker":"A","text":"はい、高校時代の美術部の仲間に誘われました。"},{"id":{"d":844,"n":4,"t":5},"speaker":"B","text":"楽しそうですね。"},{"id":{"d":844,"n":5,"t":5},"speaker":"A","text":"同窓会も兼ねているので楽しみです。"},{"id":{"d":844,"n":6,"t":5},"speaker":"B","text":"何の彫刻を作るつもりなのですか？"},{"id":{"d":844,"n":7,"t":5},"speaker":"A","text":"ポセイドンを作るつもりです。"},{"id":{"d":844,"n":8,"t":5},"speaker":"B","text":"コンテストの結果が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":845,"n":1,"t":5},"speaker":"A","text":"圭太君、どうしてびしょびしょなのですか？"},{"id":{"d":845,"n":2,"t":5},"speaker":"B","text":"拓君と遊んでいたらぬれました。"},{"id":{"d":845,"n":3,"t":5},"speaker":"A","text":"何をして遊んだのですか？"},{"id":{"d":845,"n":4,"t":5},"speaker":"B","text":"水鉄砲で怪獣ごっこをしました。"},{"id":{"d":845,"n":5,"t":5},"speaker":"A","text":"圭太君が怪獣になって撃たれたのですか？"},{"id":{"d":845,"n":6,"t":5},"speaker":"B","text":"交代で怪獣役を担当しました。拓君もびしょびしょになっていました。"},{"id":{"d":845,"n":7,"t":5},"speaker":"A","text":"そうでしたか。風邪をひかないようにまずはお風呂に入りなさい。"},{"id":{"d":845,"n":8,"t":5},"speaker":"B","text":"はい。とても楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":846,"n":1,"t":5},"speaker":"A","text":"これほどまでにたくさんのサクランボを買ってきたのですか？"},{"id":{"d":846,"n":2,"t":5},"speaker":"B","text":"これは買ったものではありません。お父さんと悠里と紗里で出場した大会の景品です。"},{"id":{"d":846,"n":3,"t":5},"speaker":"A","text":"何の大会に出場したのですか？"},{"id":{"d":846,"n":4,"t":5},"speaker":"B","text":"サクランボの種を飛ばす大会に出場したのです。"},{"id":{"d":846,"n":5,"t":5},"speaker":"A","text":"3人とも出たのですか？"},{"id":{"d":846,"n":6,"t":5},"speaker":"B","text":"はい。参加賞と飛ばした距離によってもらえる賞で、一人100グラムずつもらいました。"},{"id":{"d":846,"n":7,"t":5},"speaker":"A","text":"全部で300グラムもあるとは素晴らしいですね。"},{"id":{"d":846,"n":8,"t":5},"speaker":"B","text":"お母さんに喜んでもらえてよかったですが、優勝を逃してしまって悔しいです。"},{"id":{"d":846,"n":9,"t":5},"speaker":"A","text":"優勝できなくてもいいのですよ。サクランボは夕飯後のデザートにしましょうね。"}];
INSERT INTO sentence [{"id":{"d":847,"n":1,"t":5},"speaker":"A","text":"大切なぬいぐるみの手が取れてしまいました。"},{"id":{"d":847,"n":2,"t":5},"speaker":"B","text":"それは大変ですね。そのぬいぐるみは、あなたが赤ちゃんの時からかわいがってきたものですからね。"},{"id":{"d":847,"n":3,"t":5},"speaker":"A","text":"お母さん、ぬいぐるみの手を直せますか？"},{"id":{"d":847,"n":4,"t":5},"speaker":"B","text":"もちろんできますよ。"},{"id":{"d":847,"n":5,"t":5},"speaker":"A","text":"今、直せますか？"},{"id":{"d":847,"n":6,"t":5},"speaker":"B","text":"ごめんなさい。今は夕飯の支度をしなくてはいけません。"},{"id":{"d":847,"n":7,"t":5},"speaker":"A","text":"では、明日だったら直せますか？"},{"id":{"d":847,"n":8,"t":5},"speaker":"B","text":"今日の夜に直してみましょう。きっと明日の朝には元気になって、また一緒に寝てくれますよ。"}];
INSERT INTO sentence [{"id":{"d":848,"n":1,"t":5},"speaker":"A","text":"庭に段ボールで秘密基地を造りました。"},{"id":{"d":848,"n":2,"t":5},"speaker":"B","text":"健太君が一人で作ったのですか？"},{"id":{"d":848,"n":3,"t":5},"speaker":"A","text":"そうです。直君だけは秘密基地にご招待します。"},{"id":{"d":848,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。うれしいです。"},{"id":{"d":848,"n":5,"t":5},"speaker":"A","text":"ようこそ、僕の秘密基地へ。ジュースを飲みますか？"},{"id":{"d":848,"n":6,"t":5},"speaker":"B","text":"それは健太君のおやつでしょう？"},{"id":{"d":848,"n":7,"t":5},"speaker":"A","text":"直君をおもてなししたいのです。"},{"id":{"d":848,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。では、ジュースを頂きます。"}];
INSERT INTO sentence [{"id":{"d":849,"n":1,"t":5},"speaker":"A","text":"新聞広告で紙鉄砲を作りましたよ。"},{"id":{"d":849,"n":2,"t":5},"speaker":"B","text":"お父さん、それは鉄砲の形をしていませんよ。"},{"id":{"d":849,"n":3,"t":5},"speaker":"A","text":"そう言われればそうですね。"},{"id":{"d":849,"n":4,"t":5},"speaker":"B","text":"どう見ても三角形です。"},{"id":{"d":849,"n":5,"t":5},"speaker":"A","text":"でも、この三角形が良い音を出すのですよ。"},{"id":{"d":849,"n":6,"t":5},"speaker":"B","text":"どうやって撃つのですか？"},{"id":{"d":849,"n":7,"t":5},"speaker":"A","text":"この端を持って、上から下へ振るのです。ほら、このように。"},{"id":{"d":849,"n":8,"t":5},"speaker":"B","text":"パンッという音がしました。まるで鉄砲のようです。"}];
INSERT INTO sentence [{"id":{"d":850,"n":1,"t":5},"speaker":"A","text":"詩を書いてみたのですが、見ていただけませんか？"},{"id":{"d":850,"n":2,"t":5},"speaker":"B","text":"いいですよ。でも、どうして私に頼むのですか？"},{"id":{"d":850,"n":3,"t":5},"speaker":"A","text":"学園祭であなたが作った歌が素晴らしかったからです。もしよかったら、この詩に曲をつけていただきたいのです。"},{"id":{"d":850,"n":4,"t":5},"speaker":"B","text":"私の歌を気に入ってくれたのはうれしいのですが、どのような詩でも曲を作るとは限りませんよ。"},{"id":{"d":850,"n":5,"t":5},"speaker":"A","text":"それでも構いません。詩を見ていただけますか？"},{"id":{"d":850,"n":6,"t":5},"speaker":"B","text":"分かりました。すてきな詩ですね。曲をつけるならリズムが合わないところがあるので、手直しをしてもよいでしょうか？"},{"id":{"d":850,"n":7,"t":5},"speaker":"A","text":"もちろんよいですが、それはつまり曲をつけてくれるということですか？"},{"id":{"d":850,"n":8,"t":5},"speaker":"B","text":"はい。"},{"id":{"d":850,"n":9,"t":5},"speaker":"A","text":"本当ですか？"},{"id":{"d":850,"n":10,"t":5},"speaker":"B","text":"うそは言いませんよ。曲が完成したら、軽音楽部のクリスマスコンサートで一緒に歌いましょう。"},{"id":{"d":850,"n":11,"t":5},"speaker":"A","text":"うれしいです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":851,"n":1,"t":5},"speaker":"A","text":"クリスマスコンサートは大盛況でしたね。"},{"id":{"d":851,"n":2,"t":5},"speaker":"B","text":"ええ、よかったです。毎年、チケットの争奪戦ですが、今年は特に激しかったですね。"},{"id":{"d":851,"n":3,"t":5},"speaker":"A","text":"音楽室で練習している音が漏れていたから、事前の期待も高かったのでしょうね。"},{"id":{"d":851,"n":4,"t":5},"speaker":"B","text":"期待を裏切ることなく、素晴らしいコンサートでしたね。"},{"id":{"d":851,"n":5,"t":5},"speaker":"A","text":"はい。特に、軽音楽部のバンドは素晴らしかったです。あれは、プロでも通用しそうな演奏でした。"},{"id":{"d":851,"n":6,"t":5},"speaker":"B","text":"あの演奏には驚きましたね。あまりの迫力に鳥肌が立ってしまいました。"},{"id":{"d":851,"n":7,"t":5},"speaker":"A","text":"彼女がもしデビューしたら私は真っ先にCDを買います。"},{"id":{"d":851,"n":8,"t":5},"speaker":"B","text":"私もです。"}];
INSERT INTO sentence [{"id":{"d":852,"n":1,"t":5},"speaker":"A","text":"今回は大作に挑戦しようと思います。"},{"id":{"d":852,"n":2,"t":5},"speaker":"B","text":"いいですね。何号くらいのキャンバスに描くつもりですか？"},{"id":{"d":852,"n":3,"t":5},"speaker":"A","text":"まだ決めていませんが、部屋に入るのは100号が限界だと思います。"},{"id":{"d":852,"n":4,"t":5},"speaker":"B","text":"100号だと美紀さんの身長より高いですね。"},{"id":{"d":852,"n":5,"t":5},"speaker":"A","text":"手を伸ばせば届かない高さではありませんよ。"},{"id":{"d":852,"n":6,"t":5},"speaker":"B","text":"描いた絵は展覧会に出すのですか？"},{"id":{"d":852,"n":7,"t":5},"speaker":"A","text":"今はそのつもりで描き始めようと思っています。"},{"id":{"d":852,"n":8,"t":5},"speaker":"B","text":"期待しています。出来上がりが楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":853,"n":1,"t":5},"speaker":"A","text":"最近、あなたは電車の中でずっとスマートフォンを触っていますね。"},{"id":{"d":853,"n":2,"t":5},"speaker":"B","text":"世の中の人は、ほとんどがそうでしょう？私だけが変なのでしょうか？"},{"id":{"d":853,"n":3,"t":5},"speaker":"A","text":"変わっていると思います。他の人は動画を見たり、ニュースやメールを確認したりしているようなのに、あなたはすごい勢いで文章を打っていますよね。"},{"id":{"d":853,"n":4,"t":5},"speaker":"B","text":"実は、インターネット小説を書いているのです。"},{"id":{"d":853,"n":5,"t":5},"speaker":"A","text":"スマートフォンでインターネット小説を書いているのですか？"},{"id":{"d":853,"n":6,"t":5},"speaker":"B","text":"はい、それほど珍しいことではありませんよ。投稿サイトも多いです。"},{"id":{"d":853,"n":7,"t":5},"speaker":"A","text":"投稿したことがあるのですか？"},{"id":{"d":853,"n":8,"t":5},"speaker":"B","text":"まだありませんが、満足のいく作品ができたら投稿しようと思っています。"}];
INSERT INTO sentence [{"id":{"d":854,"n":1,"t":5},"speaker":"A","text":"お母さん、虹は6色なのですか？"},{"id":{"d":854,"n":2,"t":5},"speaker":"B","text":"いいえ、虹は7色だと思いますが、なぜですか？"},{"id":{"d":854,"n":3,"t":5},"speaker":"A","text":"英語塾にある絵本では、6色しかなかったのです。"},{"id":{"d":854,"n":4,"t":5},"speaker":"B","text":"そうなのですか？何色がなかったのでしょう？"},{"id":{"d":854,"n":5,"t":5},"speaker":"A","text":"分かりません。教わったのはレッド、オレンジ、イエロー、グリーン、ブルー、パープルです。"},{"id":{"d":854,"n":6,"t":5},"speaker":"B","text":"そうすると、足りないのは藍色ですね。"},{"id":{"d":854,"n":7,"t":5},"speaker":"A","text":"藍色は英語で何というのですか？"},{"id":{"d":854,"n":8,"t":5},"speaker":"B","text":"インディゴですね。どうして藍色がないのかは、お母さんも分かりません。"}];
INSERT INTO sentence [{"id":{"d":855,"n":1,"t":5},"speaker":"A","text":"お母さん、英語では虹は6色しかないそうですよ。"},{"id":{"d":855,"n":2,"t":5},"speaker":"B","text":"そうなのですか？"},{"id":{"d":855,"n":3,"t":5},"speaker":"A","text":"以前、お母さんは「どうして英語だと虹に藍色がないのか分からない」と言いましたよね。"},{"id":{"d":855,"n":4,"t":5},"speaker":"B","text":"はい、言いました。"},{"id":{"d":855,"n":5,"t":5},"speaker":"A","text":"英語塾の先生に聞いてみたら、英語圏では虹は6色なのだと教えてくれました。"},{"id":{"d":855,"n":6,"t":5},"speaker":"B","text":"色数の差は、日本と英語圏の文化の違いなのでしょうか？"},{"id":{"d":855,"n":7,"t":5},"speaker":"A","text":"はい、そうです。先生も文化の違いだとおっしゃっていました。"},{"id":{"d":855,"n":8,"t":5},"speaker":"B","text":"なるほど、勉強になりました。"}];
INSERT INTO sentence [{"id":{"d":856,"n":1,"t":5},"speaker":"A","text":"祖父が書いた日記が本になることになりました。"},{"id":{"d":856,"n":2,"t":5},"speaker":"B","text":"日記は自費で出版するのですか？"},{"id":{"d":856,"n":3,"t":5},"speaker":"A","text":"自費出版ではなく、大手の出版社が本にして出版してくれるそうです。"},{"id":{"d":856,"n":4,"t":5},"speaker":"B","text":"それはすごいですね。でも、どうしてですか？"},{"id":{"d":856,"n":5,"t":5},"speaker":"A","text":"僕が祖父の日記をインターネットにアップロードしていたのですが、それを見て声を掛けてくれたようです。"},{"id":{"d":856,"n":6,"t":5},"speaker":"B","text":"どのような日記なのですか？"},{"id":{"d":856,"n":7,"t":5},"speaker":"A","text":"内容は日常のたわいもない出来事ですが、絵日記にして約50年分あります。"},{"id":{"d":856,"n":8,"t":5},"speaker":"B","text":"50年間の絵日記ですか。素晴らしいですね。よかったら私にも原本を見せてください。"}];
INSERT INTO sentence [{"id":{"d":857,"n":1,"t":5},"speaker":"A","text":"新しいショッピングモールに小劇場が入ったのを知っていますか？"},{"id":{"d":857,"n":2,"t":5},"speaker":"B","text":"知りませんでした。"},{"id":{"d":857,"n":3,"t":5},"speaker":"A","text":"こけら落としは、人気のある役者さんが所属している大衆演劇だと聞きました。"},{"id":{"d":857,"n":4,"t":5},"speaker":"B","text":"面白そうですが、子どもを連れて行ける公演ではなさそうであることが少し残念ですね。"},{"id":{"d":857,"n":5,"t":5},"speaker":"A","text":"でも、その後の公演予定を見たら、子ども劇団や人形劇団が目白押しでした。"},{"id":{"d":857,"n":6,"t":5},"speaker":"B","text":"本当ですか？それなら、子どもたちも喜びますね。"},{"id":{"d":857,"n":7,"t":5},"speaker":"A","text":"よかったら子ども向けの劇を一緒に鑑賞しませんか？"},{"id":{"d":857,"n":8,"t":5},"speaker":"B","text":"ぜひご一緒したいです。チケットはショッピングモールのサービスカウンターで買えるのでしょうか？"},{"id":{"d":857,"n":9,"t":5},"speaker":"A","text":"はい、そうだと思います。明日一緒に買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":858,"n":1,"t":5},"speaker":"A","text":"今日は一日中プラレールに付き合うことになって大変でした。"},{"id":{"d":858,"n":2,"t":5},"speaker":"B","text":"疲れましたね。"},{"id":{"d":858,"n":3,"t":5},"speaker":"A","text":"あなたが大き過ぎるプラレールを買ってくるからですよ。"},{"id":{"d":858,"n":4,"t":5},"speaker":"B","text":"大きなプラレールは僕の子どもの頃の夢でもあったのです。剛には夢で終わらせてほしくなかったのですよ。"},{"id":{"d":858,"n":5,"t":5},"speaker":"A","text":"おかげでリビングの家具を全部動かして、プラレールを組み立てる羽目になりました。"},{"id":{"d":858,"n":6,"t":5},"speaker":"B","text":"ごめんなさい。君まで巻き込むことになるとは思っていませんでした。"},{"id":{"d":858,"n":7,"t":5},"speaker":"A","text":"あなた一人ではソファーも動かせなかったでしょう？それにしても、あのプラレールの線路はいつまであのままにしておくつもりですか？"},{"id":{"d":858,"n":8,"t":5},"speaker":"B","text":"せっかく作ったので、もうしばらくはそのままにしておきたいです。"}];
INSERT INTO sentence [{"id":{"d":859,"n":1,"t":5},"speaker":"A","text":"こちらの商品は、クリスマスプレゼント用にお包みしますか？"},{"id":{"d":859,"n":2,"t":5},"speaker":"B","text":"はい。包んでください。"},{"id":{"d":859,"n":3,"t":5},"speaker":"A","text":"コスメティックセットは女の子のおもちゃの中でも人気があるのですよ。"},{"id":{"d":859,"n":4,"t":5},"speaker":"B","text":"そうなのですね。"},{"id":{"d":859,"n":5,"t":5},"speaker":"A","text":"お嬢様も喜ばれると思います。"},{"id":{"d":859,"n":6,"t":5},"speaker":"B","text":"実は、これは子どもの希望ではなくて妻の希望なのですよ。"},{"id":{"d":859,"n":7,"t":5},"speaker":"A","text":"奥様がお選びになったのですか？"},{"id":{"d":859,"n":8,"t":5},"speaker":"B","text":"はい。娘が妻の化粧品を勝手に使ってしまったので、自分のものを触らせないためだと言っていました。"}];
INSERT INTO sentence [{"id":{"d":860,"n":1,"t":5},"speaker":"A","text":"すみません、今月号はもう入荷していますか？"},{"id":{"d":860,"n":2,"t":5},"speaker":"B","text":"いつものパズル雑誌のことですね？"},{"id":{"d":860,"n":3,"t":5},"speaker":"A","text":"はい。その雑誌の8月号です。"},{"id":{"d":860,"n":4,"t":5},"speaker":"B","text":"ちょうど今朝、入ってきましたよ。今日はこれだけでよいですか？"},{"id":{"d":860,"n":5,"t":5},"speaker":"A","text":"いいえ、他にも欲しいものがあります。こちらでは切手も扱っていましたよね？"},{"id":{"d":860,"n":6,"t":5},"speaker":"B","text":"はい、ございますよ。いくらの切手にしますか？"},{"id":{"d":860,"n":7,"t":5},"speaker":"A","text":"その雑誌の懸賞応募はがき用の切手が欲しいのですが、値段が分かりません。"},{"id":{"d":860,"n":8,"t":5},"speaker":"B","text":"はがきでしたら63円ですね。何枚に要りますか？"},{"id":{"d":860,"n":9,"t":5},"speaker":"A","text":"1枚ください。"}];
INSERT INTO sentence [{"id":{"d":861,"n":1,"t":5},"speaker":"A","text":"お母さん、今日は武蔵君の家にお泊まりをしてもよいですか？"},{"id":{"d":861,"n":2,"t":5},"speaker":"B","text":"構いませんが、急にお泊まりをさせてもらうのは武蔵君の家のご迷惑にならないでしょうか？"},{"id":{"d":861,"n":3,"t":5},"speaker":"A","text":"武蔵君のお母さんもよいと言ってくれました。"},{"id":{"d":861,"n":4,"t":5},"speaker":"B","text":"本当ですか？電話して確認してもよいですか？"},{"id":{"d":861,"n":5,"t":5},"speaker":"A","text":"はい、武蔵君の家に電話してください。"},{"id":{"d":861,"n":6,"t":5},"speaker":"B","text":"泊まって何をするつもりなのですか？"},{"id":{"d":861,"n":7,"t":5},"speaker":"A","text":"武蔵君と新しいゲームをする約束をしました。"},{"id":{"d":861,"n":8,"t":5},"speaker":"B","text":"分かりました。武蔵君のお母さんに「ゲームで遊ぶ時間は2時間までにしてください」とお願いしておきますね。"}];
INSERT INTO sentence [{"id":{"d":862,"n":1,"t":5},"speaker":"A","text":"金田さん、この湿布を貼っていただけませんか？貼りたいところに手が届かなくて困っているのです。"},{"id":{"d":862,"n":2,"t":5},"speaker":"B","text":"よいですよ。ご主人には頼めなかったのですか？"},{"id":{"d":862,"n":3,"t":5},"speaker":"A","text":"夫には内緒にしておきたいのです。"},{"id":{"d":862,"n":4,"t":5},"speaker":"B","text":"すごく大きなあざができていますよ。内緒にしたままでいいのですか？"},{"id":{"d":862,"n":5,"t":5},"speaker":"A","text":"はい。夫が趣味の日曜大工で作った椅子に座った途端に、椅子が壊れてしまってできたあざなのです。"},{"id":{"d":862,"n":6,"t":5},"speaker":"B","text":"それで、転げ落ちてしまったのですか？"},{"id":{"d":862,"n":7,"t":5},"speaker":"A","text":"そうなのです。椅子はホームセンターの講師の方に補強して直してもらいましたが、あざはまだ消えていません。"},{"id":{"d":862,"n":8,"t":5},"speaker":"B","text":"今もご主人は上手に椅子を作ることができたと思っているわけですね。それは正直に伝えた方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":863,"n":1,"t":5},"speaker":"A","text":"ついに、カメラの望遠レンズを買ってしまいました。"},{"id":{"d":863,"n":2,"t":5},"speaker":"B","text":"高そうなレンズですね。"},{"id":{"d":863,"n":3,"t":5},"speaker":"A","text":"はい。でも、一眼レフカメラを買ったら、このレンズがなければいけませんよ。"},{"id":{"d":863,"n":4,"t":5},"speaker":"B","text":"確かに望遠レンズがあれば、スマートフォンでは撮れない写真が撮れますね。"},{"id":{"d":863,"n":5,"t":5},"speaker":"A","text":"そうなのです。早速。次の休みに野球場に行って試し撮りをする予定です。"},{"id":{"d":863,"n":6,"t":5},"speaker":"B","text":"メジャーリーグから帰ってきた選手がいるチームの試合ですか？"},{"id":{"d":863,"n":7,"t":5},"speaker":"A","text":"そうです。お目当てはその選手です。"},{"id":{"d":863,"n":8,"t":5},"speaker":"B","text":"良い写真を撮ってきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":864,"n":1,"t":5},"speaker":"A","text":"祖母が趣味で集めていたアンティークの雑貨をもらいました。"},{"id":{"d":864,"n":2,"t":5},"speaker":"B","text":"どのようなものなのですか？"},{"id":{"d":864,"n":3,"t":5},"speaker":"A","text":"食器や引き出しなどの小さな家具です。写真を見ますか？"},{"id":{"d":864,"n":4,"t":5},"speaker":"B","text":"はい、見せてください。"},{"id":{"d":864,"n":5,"t":5},"speaker":"A","text":"これです。"},{"id":{"d":864,"n":6,"t":5},"speaker":"B","text":"とてもすてきです。おばあさまは良い趣味をお持ちなのですね。"},{"id":{"d":864,"n":7,"t":5},"speaker":"A","text":"とてもすてきで私も気に入っているのですが、自分の部屋に置いてみたらその空間だけが浮いている感じがします。"},{"id":{"d":864,"n":8,"t":5},"speaker":"B","text":"今まではアンティークとは無縁の生活をしていたのですね。"},{"id":{"d":864,"n":9,"t":5},"speaker":"A","text":"はい。ですから、これからは私もアンティークの雑貨を集めることにしました。"},{"id":{"d":864,"n":10,"t":5},"speaker":"B","text":"アンティークを探しに骨董市巡りに行くのでしたら付き合いますよ。"}];
INSERT INTO sentence [{"id":{"d":865,"n":1,"t":5},"speaker":"A","text":"七五三のお参りに来ている子どもたちはかわいいですね。"},{"id":{"d":865,"n":2,"t":5},"speaker":"B","text":"機嫌良く神社にお参りしているうちはいいのですけれどね。"},{"id":{"d":865,"n":3,"t":5},"speaker":"A","text":"先輩のお子さんは去年が七五三だったのでしたね。"},{"id":{"d":865,"n":4,"t":5},"speaker":"B","text":"はい。娘が3歳でしたので、一応お祝いしました。"},{"id":{"d":865,"n":5,"t":5},"speaker":"A","text":"お祝いの話題なのに、声が沈んでいますよ。"},{"id":{"d":865,"n":6,"t":5},"speaker":"B","text":"去年のことを思い出して、気分が沈んでいます。"},{"id":{"d":865,"n":7,"t":5},"speaker":"A","text":"何があったのですか？"},{"id":{"d":865,"n":8,"t":5},"speaker":"B","text":"娘は最初こそ大喜びではしゃいでいたのですが、だんだんと疲れて大泣きをしてしまったのです。泣きつかれたのか、写真を撮る時には寝ていました。私もぐったりでしたよ。"}];
INSERT INTO sentence [{"id":{"d":866,"n":1,"t":5},"speaker":"A","text":"お父さん、たこ揚げをしましょう。"},{"id":{"d":866,"n":2,"t":5},"speaker":"B","text":"あいにく、家のそばにはたこを揚げられる場所がありません。"},{"id":{"d":866,"n":3,"t":5},"speaker":"A","text":"では、こま回しにしましょう。"},{"id":{"d":866,"n":4,"t":5},"speaker":"B","text":"お父さんはこま回しが上手ではありませんよ。"},{"id":{"d":866,"n":5,"t":5},"speaker":"A","text":"上手でなくてもよいです。歌のようなお正月の遊びをしたいです。"},{"id":{"d":866,"n":6,"t":5},"speaker":"B","text":"どのような歌ですか？"},{"id":{"d":866,"n":7,"t":5},"speaker":"A","text":"「お正月にはたこ揚げて　こまを回して遊びましょう」という歌です。"},{"id":{"d":866,"n":8,"t":5},"speaker":"B","text":"その歌は聞いたことがあります。分かりました、一緒にこま回しをしましょう。"}];
INSERT INTO sentence [{"id":{"d":867,"n":1,"t":5},"speaker":"A","text":"何だかすごいバッグを持っていますね。"},{"id":{"d":867,"n":2,"t":5},"speaker":"B","text":"持ち方を間違えました。こちらは裏面です。"},{"id":{"d":867,"n":3,"t":5},"speaker":"A","text":"透明のポケットに缶バッジがたくさん入っていましたよ。その絵はアニメーションのキャラクターですか？"},{"id":{"d":867,"n":4,"t":5},"speaker":"B","text":"はい。今日は、仕事の後に好きなアニメーションのイベントがあるのです。その時に持っていくための痛バッグです。"},{"id":{"d":867,"n":5,"t":5},"speaker":"A","text":"「痛バッグ」とは何ですか？初めて聞く言葉です。"},{"id":{"d":867,"n":6,"t":5},"speaker":"B","text":"アニメーションが好きな人が、好きなキャラクターの缶バッジやキーホルダーなどをたくさん付けて、そのキャラクターへの愛を表現したかばんをそう呼びます。このかばんは趣味で作ったものです。"},{"id":{"d":867,"n":7,"t":5},"speaker":"A","text":"裏返せば普通のバッグのように見えるのですね。よく考えてありますね。"},{"id":{"d":867,"n":8,"t":5},"speaker":"B","text":"今日は仕事ですから、一応おとなしめのバッグにしたのです。"}];
INSERT INTO sentence [{"id":{"d":868,"n":1,"t":5},"speaker":"A","text":"先日、「痛バッグ」というものを見せてもらったでしょう？"},{"id":{"d":868,"n":2,"t":5},"speaker":"B","text":"はい。"},{"id":{"d":868,"n":3,"t":5},"speaker":"A","text":"アニメーションが好きないとこに聞いたら、彼女も痛バッグをたくさん持っているそうです。"},{"id":{"d":868,"n":4,"t":5},"speaker":"B","text":"そうでしたか。痛バッグは、アニメーションが好きな人には必携のアイテムですね。"},{"id":{"d":868,"n":5,"t":5},"speaker":"A","text":"どこで買ったのかを聞いたら、有名なファッションブランドの店だということにも驚きました。"},{"id":{"d":868,"n":6,"t":5},"speaker":"B","text":"そうなのです。意外かもしれませんが、痛バッグファッションアイテムとして認知されていようなのです。"},{"id":{"d":868,"n":7,"t":5},"speaker":"A","text":"高田さんも普段からおしゃれですものね。洋服はファッションブランドの店で買うこともあるのですか？"},{"id":{"d":868,"n":8,"t":5},"speaker":"B","text":"はい、時々ですがブランド店にも買いに行きます。"},{"id":{"d":868,"n":9,"t":5},"speaker":"A","text":"高田さんがブランド店で痛バッグを買っていたとは意外でした。"},{"id":{"d":868,"n":10,"t":5},"speaker":"B","text":"アニメーションが好きなことは隠していたので、驚かせてしまいましたね。"}];
INSERT INTO sentence [{"id":{"d":869,"n":1,"t":5},"speaker":"A","text":"クリスマスケーキを一緒に作りませんか？"},{"id":{"d":869,"n":2,"t":5},"speaker":"B","text":"はい、作りたいです。ケーキにはイチゴをたくさん載せたいです。"},{"id":{"d":869,"n":3,"t":5},"speaker":"A","text":"いいですよ。では、始めましょう。"},{"id":{"d":869,"n":4,"t":5},"speaker":"B","text":"まずは何をするのですか？"},{"id":{"d":869,"n":5,"t":5},"speaker":"A","text":"私はスポンジケーキを焼きますから、風香はイチゴを洗ってください。"},{"id":{"d":869,"n":6,"t":5},"speaker":"B","text":"分かりました。"},{"id":{"d":869,"n":7,"t":5},"speaker":"A","text":"洗い終わったら、生クリームを泡立ててくださいね。"},{"id":{"d":869,"n":8,"t":5},"speaker":"B","text":"はい。早く飾り付けをしたいです。"}];
INSERT INTO sentence [{"id":{"d":870,"n":1,"t":5},"speaker":"A","text":"今年のゴールデンウィークはどこにも出かけない予定です。"},{"id":{"d":870,"n":2,"t":5},"speaker":"B","text":"連休といっても飛び石連休ですからね。"},{"id":{"d":870,"n":3,"t":5},"speaker":"A","text":"鈴川さんはどこかに行く予定ですか？"},{"id":{"d":870,"n":4,"t":5},"speaker":"B","text":"いいえ、私も家にいます。庭の夏ミカンの収穫をします。"},{"id":{"d":870,"n":5,"t":5},"speaker":"A","text":"夏ミカンの木が庭にあるのですか？"},{"id":{"d":870,"n":6,"t":5},"speaker":"B","text":"はい。収穫した後は、砂糖漬けやピールやジャムを作ろうと思います。"},{"id":{"d":870,"n":7,"t":5},"speaker":"A","text":"何だか楽しそうですね。"},{"id":{"d":870,"n":8,"t":5},"speaker":"B","text":"お出かけをしないのなら、一緒に作りませんか？ミカンもジャムもお分けしますよ。"}];
INSERT INTO sentence [{"id":{"d":871,"n":1,"t":5},"speaker":"A","text":"俊樹君にスイカ割りをしようと誘われました。"},{"id":{"d":871,"n":2,"t":5},"speaker":"B","text":"俊樹君のおうちでするのですか？"},{"id":{"d":871,"n":3,"t":5},"speaker":"A","text":"いいえ、俊樹君のおじいさんの畑でするそうです。"},{"id":{"d":871,"n":4,"t":5},"speaker":"B","text":"隣町におじいさんがおられると聞いたことがあります。そこに行くのですね？"},{"id":{"d":871,"n":5,"t":5},"speaker":"A","text":"そうです。行ってもよいですか？"},{"id":{"d":871,"n":6,"t":5},"speaker":"B","text":"俊樹君のお母さんに一応聞いてみますね。少し待っていてください。"},{"id":{"d":871,"n":7,"t":5},"speaker":"A","text":"電話は終わりましたか？俊樹君のお母さんは何と言っていましたか？"},{"id":{"d":871,"n":8,"t":5},"speaker":"B","text":"「お母さんも一緒に来てください」と誘っていただきました。一緒に行ってスイカをごちそうになりましょう。"}];
INSERT INTO sentence [{"id":{"d":872,"n":1,"t":5},"speaker":"A","text":"文也は、あなたがクリスマスにくれたおもちゃのピアノでずっと遊んでいますよ。"},{"id":{"d":872,"n":2,"t":5},"speaker":"B","text":"気に入ってくれたのですね。それはよかったです。"},{"id":{"d":872,"n":3,"t":5},"speaker":"A","text":"あなたの感想は、それだけですか？"},{"id":{"d":872,"n":4,"t":5},"speaker":"B","text":"気に入ってくれてうれしいということ以外に、どのような感想がありますか？"},{"id":{"d":872,"n":5,"t":5},"speaker":"A","text":"息子の音楽の才能が感じられるということはありませんか？"},{"id":{"d":872,"n":6,"t":5},"speaker":"B","text":"冗談でしょう。私には楽しく鍵盤をたたいているだけにしか見えません。"},{"id":{"d":872,"n":7,"t":5},"speaker":"A","text":"あれほどまでに夢中になることができるのも、きっと才能の片鱗ですよ。"},{"id":{"d":872,"n":8,"t":5},"speaker":"B","text":"私にはそうは思えません。"}];
INSERT INTO sentence [{"id":{"d":873,"n":1,"t":5},"speaker":"A","text":"今年もデパートの屋上にビアガーデンが開きましたよ。"},{"id":{"d":873,"n":2,"t":5},"speaker":"B","text":"仕事の終わりが待ち遠しい季節になりましたね。"},{"id":{"d":873,"n":3,"t":5},"speaker":"A","text":"昨日、母がお中元をデパートで送ったら、このようなクーポン券をもらってきました。"},{"id":{"d":873,"n":4,"t":5},"speaker":"B","text":"ビアガーデンのお会計から20パーセントを割り引いてくれるのですか。うれしいですね。"},{"id":{"d":873,"n":5,"t":5},"speaker":"A","text":"クーポン券には期限がありますから、期限前に一緒に行きましょう。"},{"id":{"d":873,"n":6,"t":5},"speaker":"B","text":"期限は今月中ですか。月末は忙しくなりますから、今週のどこかで行きませんか？"},{"id":{"d":873,"n":7,"t":5},"speaker":"A","text":"明日はいかがですか？"},{"id":{"d":873,"n":8,"t":5},"speaker":"B","text":"いいですね。明日はビールを楽しみに仕事をします。"}];
INSERT INTO sentence [{"id":{"d":874,"n":1,"t":5},"speaker":"A","text":"おばあちゃんが将司のために木目込みでこいのぼりを作ってくれましたよ。"},{"id":{"d":874,"n":2,"t":5},"speaker":"B","text":"これは良いですね。これならテーブルや棚の上に飾っておくことができます。"},{"id":{"d":874,"n":3,"t":5},"speaker":"A","text":"普通のこいのぼりは外で飾らなくてはいけませんからね。"},{"id":{"d":874,"n":4,"t":5},"speaker":"B","text":"ええ、残念ながらマンションには飾るところがありません。"},{"id":{"d":874,"n":5,"t":5},"speaker":"A","text":"おばあちゃんは昔から木目込み人形を作っていたので、このようにかわいい人形も作れるのですね。"},{"id":{"d":874,"n":6,"t":5},"speaker":"B","text":"そ大切にしないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":875,"n":1,"t":5},"speaker":"A","text":"今日は文化の日ですから、みんなでお出かけしましょう。"},{"id":{"d":875,"n":2,"t":5},"speaker":"B","text":"どこに行くのですか？"},{"id":{"d":875,"n":3,"t":5},"speaker":"A","text":"文化の日ということで美術館や博物館の展示が無料で見られるところがあるので、そこへ行こうと思います。"},{"id":{"d":875,"n":4,"t":5},"speaker":"B","text":"私は博物館がいいです。"},{"id":{"d":875,"n":5,"t":5},"speaker":"A","text":"分かりました。でも、お母さんのために美術館にも付き合ってくださいね。"},{"id":{"d":875,"n":6,"t":5},"speaker":"B","text":"美術館のカフェでパフェを食べてもよいですか？"},{"id":{"d":875,"n":7,"t":5},"speaker":"A","text":"カフェにたくさんの人が並んでいなかったら、よいですよ。"},{"id":{"d":875,"n":8,"t":5},"speaker":"B","text":"絶対に混んでいると思うので、そのときは近所のアイスで手を打ちます。"}];
INSERT INTO sentence [{"id":{"d":876,"n":1,"t":5},"speaker":"A","text":"100円ショップでこれを買ってきました。"},{"id":{"d":876,"n":2,"t":5},"speaker":"B","text":"これは何ですか？"},{"id":{"d":876,"n":3,"t":5},"speaker":"A","text":"スマートフォンに付けるレンズです。これを付けると接写撮影ができるようになるのです。"},{"id":{"d":876,"n":4,"t":5},"speaker":"B","text":"これで何を撮るのですか？"},{"id":{"d":876,"n":5,"t":5},"speaker":"A","text":"雪の結晶です。明日は雪の予報でしょう？"},{"id":{"d":876,"n":6,"t":5},"speaker":"B","text":"そういえば天気予報でそのように言っていましたね。"},{"id":{"d":876,"n":7,"t":5},"speaker":"A","text":"このレンズを使うと雪の結晶がきれいに撮れるとテレビの番組で言っていたのです。"},{"id":{"d":876,"n":8,"t":5},"speaker":"B","text":"そうなのですか。きれいに撮れたら見せてくださいね。"}];
INSERT INTO sentence [{"id":{"d":877,"n":1,"t":5},"speaker":"A","text":"スマートフォンで雪の結晶の写真が撮れました。"},{"id":{"d":877,"n":2,"t":5},"speaker":"B","text":"きれいですね。何枚あるのですか？"},{"id":{"d":877,"n":3,"t":5},"speaker":"A","text":"50枚以上撮ったのですが、うまく撮れたのは5枚です。"},{"id":{"d":877,"n":4,"t":5},"speaker":"B","text":"難しいのですね。でも、これは図鑑に載せたいくらいきれいに撮れていますよ。"},{"id":{"d":877,"n":5,"t":5},"speaker":"A","text":"そうですよね。私も結構きれいに撮れたと思います。"},{"id":{"d":877,"n":6,"t":5},"speaker":"B","text":"また雪が降ったら、私も撮ってみたいです。"},{"id":{"d":877,"n":7,"t":5},"speaker":"A","text":"そのときに備えて、接写レンズを用意しておくといいですよ。"},{"id":{"d":877,"n":8,"t":5},"speaker":"B","text":"そうですね。もし雪が降らなかったら、お花の写真でも撮ります。"}];
INSERT INTO sentence [{"id":{"d":878,"n":1,"t":5},"speaker":"A","text":"雪の中なのに、ワンちゃんのお散歩ですか？"},{"id":{"d":878,"n":2,"t":5},"speaker":"B","text":"はい。いつもの時間に散歩しないと機嫌が悪くなってしまうのです。"},{"id":{"d":878,"n":3,"t":5},"speaker":"A","text":"ひと晩で1メートル近く雪が積もりましたから、お散歩も大変ですね。"},{"id":{"d":878,"n":4,"t":5},"speaker":"B","text":"積もった雪に飛び込みたいようで困っています。"},{"id":{"d":878,"n":5,"t":5},"speaker":"A","text":"立ち話をしている間に、ワンちゃんが雪まみれになってきましたね。"},{"id":{"d":878,"n":6,"t":5},"speaker":"B","text":"毎年、雪の季節は雪玉が体にたくさん付くのですよ。"},{"id":{"d":878,"n":7,"t":5},"speaker":"A","text":"帰ってからもお世話が大変そうですね。"},{"id":{"d":878,"n":8,"t":5},"speaker":"B","text":"はい。家に帰ったらすぐにシャワーで雪玉を落とします。"}];
INSERT INTO sentence [{"id":{"d":879,"n":1,"t":5},"speaker":"A","text":"なぜ床に寝ているのですか？"},{"id":{"d":879,"n":2,"t":5},"speaker":"B","text":"タマが寝ているからです。"},{"id":{"d":879,"n":3,"t":5},"speaker":"A","text":"猫と一緒に寝ていないでください。"},{"id":{"d":879,"n":4,"t":5},"speaker":"B","text":"でも、猫は一番涼しい場所を知っているのですよ。"},{"id":{"d":879,"n":5,"t":5},"speaker":"A","text":"猫は寝ていてもよいですが、人間は寝てばかりではいけませんよ。"},{"id":{"d":879,"n":6,"t":5},"speaker":"B","text":"今日は休みなので何もしたくありません。ここで寝ていたいです。"},{"id":{"d":879,"n":7,"t":5},"speaker":"A","text":"もうそろそろ子どものお風呂の時間です。寝てばかりいないで、一緒にお風呂に入ってきてください。"}];
INSERT INTO sentence [{"id":{"d":880,"n":1,"t":5},"speaker":"A","text":"もう梅雨入りですね。"},{"id":{"d":880,"n":2,"t":5},"speaker":"B","text":"そうですね。毎日雨が降っていて憂鬱です。"},{"id":{"d":880,"n":3,"t":5},"speaker":"A","text":"そういえば、最近、私は新しい傘を買いました。"},{"id":{"d":880,"n":4,"t":5},"speaker":"B","text":"それですか？黒くて渋い傘ですね。"},{"id":{"d":880,"n":5,"t":5},"speaker":"A","text":"はい。これを開くと、青空の絵が広がるのです。"},{"id":{"d":880,"n":6,"t":5},"speaker":"B","text":"内側に青空の絵が描かれているのですね。雨なのに青空の下を歩いているようです。"},{"id":{"d":880,"n":7,"t":5},"speaker":"A","text":"この傘のおかげで、雨降りの日でも気分が明るくなります。"},{"id":{"d":880,"n":8,"t":5},"speaker":"B","text":"それはすてきです。私もこの傘のような気分が盛り上がるレイングッズを探してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":881,"n":1,"t":5},"speaker":"A","text":"仁志、どうしてこのような古い曲を知っているのですか？"},{"id":{"d":881,"n":2,"t":5},"speaker":"B","text":"TikTokで知りました。今は、昭和歌謡が流行しているのですよ。"},{"id":{"d":881,"n":3,"t":5},"speaker":"A","text":"若者の間で流行しているのですか？"},{"id":{"d":881,"n":4,"t":5},"speaker":"B","text":"そうです。交流サイトを見ていると、同世代の人が昭和の歌をたくさん歌っています。"},{"id":{"d":881,"n":5,"t":5},"speaker":"A","text":"このような古い歌を息子が振り付きで歌うとは思ってもいませんでした。"},{"id":{"d":881,"n":6,"t":5},"speaker":"B","text":"動画で昔の歌手を見ることもできるので、それを見て覚えました。"},{"id":{"d":881,"n":7,"t":5},"speaker":"A","text":"そうでしたか。その曲は、私が子どもの頃におじいちゃんが歌っていた歌ですよ。"},{"id":{"d":881,"n":8,"t":5},"speaker":"B","text":"おじいちゃんの孫だけあって、格好良かったでしょう？"}];
INSERT INTO sentence [{"id":{"d":882,"n":1,"t":5},"speaker":"A","text":"今日、幼稚園でいつもおばあちゃんが作っている絵を作りました。"},{"id":{"d":882,"n":2,"t":5},"speaker":"B","text":"私が作っている絵というのは何ですか？"},{"id":{"d":882,"n":3,"t":5},"speaker":"A","text":"紙をちぎって貼り付ける絵です。"},{"id":{"d":882,"n":4,"t":5},"speaker":"B","text":"ちぎり絵のことですね。上手にできましたか？"},{"id":{"d":882,"n":5,"t":5},"speaker":"A","text":"はい、見てください。これです。"},{"id":{"d":882,"n":6,"t":5},"speaker":"B","text":"これは雪だるまと、私が飼っている犬のミルクですね。"},{"id":{"d":882,"n":7,"t":5},"speaker":"A","text":"はい、その通りです。"},{"id":{"d":882,"n":8,"t":5},"speaker":"B","text":"色のついた紙に白い雪だるまと白いミルクが映えて、とても上手にできていますね。"}];
INSERT INTO sentence [{"id":{"d":883,"n":1,"t":5},"speaker":"A","text":"もうすぐ定年退職になりますが、その後の生活を考えると頭が痛いです。"},{"id":{"d":883,"n":2,"t":5},"speaker":"B","text":"退職したら、趣味に没頭するなどして優雅に暮らせばよいのですよ。"},{"id":{"d":883,"n":3,"t":5},"speaker":"A","text":"私は長年、無趣味であることが悩みの種なのです。"},{"id":{"d":883,"n":4,"t":5},"speaker":"B","text":"今から趣味を作っていけばよいのですよ。焦ることはありません。"},{"id":{"d":883,"n":5,"t":5},"speaker":"A","text":"私でも始められそうな趣味はないでしょうか？"},{"id":{"d":883,"n":6,"t":5},"speaker":"B","text":"佐藤さんは社員旅行で写真を撮っていましたよね。カメラはどうですか？"},{"id":{"d":883,"n":7,"t":5},"speaker":"A","text":"カメラは嫌いではありませんが、写真の出来栄えはそれほど良いものではありませんでしたよ。"},{"id":{"d":883,"n":8,"t":5},"speaker":"B","text":"それは謙遜ですよ。時間ができたら、カメラ教室に通ってみるといいと思います。"}];
INSERT INTO sentence [{"id":{"d":884,"n":1,"t":5},"speaker":"A","text":"今日のオムレツは大きいですね。うれしいです。"},{"id":{"d":884,"n":2,"t":5},"speaker":"B","text":"今日はイースターですから大サービスですよ。"},{"id":{"d":884,"n":3,"t":5},"speaker":"A","text":"今日のご飯がオムレツになったのはイースターだからなのですか？"},{"id":{"d":884,"n":4,"t":5},"speaker":"B","text":"そうです。イースターエッグを作って、エッグハントというものをやろうと思ったのですよ。"},{"id":{"d":884,"n":5,"t":5},"speaker":"A","text":"エッグハントというゲームは知っています。ご飯の後にやりますか？"},{"id":{"d":884,"n":6,"t":5},"speaker":"B","text":"いいえ、イースターエッグを作るのに失敗したので、残念ながらゲームはできません。"},{"id":{"d":884,"n":7,"t":5},"speaker":"A","text":"それで卵がたくさん割れてしまったので、オムレツを作ったのですね。"},{"id":{"d":884,"n":8,"t":5},"speaker":"B","text":"はい。では、おいしくオムレツをいただきましょう。"}];
INSERT INTO sentence [{"id":{"d":885,"n":1,"t":5},"speaker":"A","text":"自動車販売店の夏祭りのお知らせが来ていますよ。"},{"id":{"d":885,"n":2,"t":5},"speaker":"B","text":"「ご家族でどうぞ」と書いてありますね。"},{"id":{"d":885,"n":3,"t":5},"speaker":"A","text":"子どもたちに輪投げやヨーヨー釣りをさせてくれるようです。"},{"id":{"d":885,"n":4,"t":5},"speaker":"B","text":"皆で行きますか？"},{"id":{"d":885,"n":5,"t":5},"speaker":"A","text":"そうですね。ちょうど自動車継続検査の前で、車の買い替えを考えていたところですし、行きましょう。"},{"id":{"d":885,"n":6,"t":5},"speaker":"B","text":"新車の下調べもするのですね。"},{"id":{"d":885,"n":7,"t":5},"speaker":"A","text":"はい。このタイミングで見ておきたいのです。"},{"id":{"d":885,"n":8,"t":5},"speaker":"B","text":"分かりました。でも、パンフレットをもらうだけにしてくださいね。契約するつもりはありませんよ。"}];
INSERT INTO sentence [{"id":{"d":886,"n":1,"t":5},"speaker":"A","text":"クリスマスにアルバイトをしませんか？"},{"id":{"d":886,"n":2,"t":5},"speaker":"B","text":"構いませんが、何のアルバイトですか？"},{"id":{"d":886,"n":3,"t":5},"speaker":"A","text":"サンタクロースの格好をしてクリスマスケーキを売る仕事です。"},{"id":{"d":886,"n":4,"t":5},"speaker":"B","text":"以前、ケーキが売り切れるまでサンタクロースの格好をし続けなくてはならないというアルバイトをしたことがありますが、あれは大変でした。そのようなものであれば、遠慮したいです。"},{"id":{"d":886,"n":5,"t":5},"speaker":"A","text":"ケーキが売り切れなかったとしても、閉店時間には売るのをやめてよいそうですよ。"},{"id":{"d":886,"n":6,"t":5},"speaker":"B","text":"本当ですか？"},{"id":{"d":886,"n":7,"t":5},"speaker":"A","text":"はい。人気のお店ですから、閉店時間前に売り切れてしまう可能性も高いです。"},{"id":{"d":886,"n":8,"t":5},"speaker":"B","text":"それならお引き受けします。"}];
INSERT INTO sentence [{"id":{"d":887,"n":1,"t":5},"speaker":"A","text":"お父さん、ウサギさんに風船をもらいました。"},{"id":{"d":887,"n":2,"t":5},"speaker":"B","text":"どこにウサギさんがいたのですか？"},{"id":{"d":887,"n":3,"t":5},"speaker":"A","text":"階段の上です。"},{"id":{"d":887,"n":4,"t":5},"speaker":"B","text":"あのウサギさんからもらったのですね。"},{"id":{"d":887,"n":5,"t":5},"speaker":"A","text":"そうです。あ、風船に紙が付いています。"},{"id":{"d":887,"n":6,"t":5},"speaker":"B","text":"「イースターのイベントのお知らせ」と書いてありますね。"},{"id":{"d":887,"n":7,"t":5},"speaker":"A","text":"イベントでは何をするのですか？"},{"id":{"d":887,"n":8,"t":5},"speaker":"B","text":"今度の土曜日と日曜日に、卵を探すイベントを開催するらしいです。行ってみましょうか？"}];
INSERT INTO sentence [{"id":{"d":888,"n":1,"t":5},"speaker":"A","text":"鏡餅を買ってきましたので、神棚に飾ってください。"},{"id":{"d":888,"n":2,"t":5},"speaker":"B","text":"鏡餅とは、これのことですか？"},{"id":{"d":888,"n":3,"t":5},"speaker":"A","text":"そうです。毎年、飾っているでしょう？"},{"id":{"d":888,"n":4,"t":5},"speaker":"B","text":"そうですが、私が飾るのは初めてです。これは工作をしなければなりませんか？"},{"id":{"d":888,"n":5,"t":5},"speaker":"A","text":"はい、紙やプラスチックでできた飾り物を作るのです。この餅の形をしたプラスチック容器の中に、四角い餅が入っているのです。"},{"id":{"d":888,"n":6,"t":5},"speaker":"B","text":"知りませんでした。毎年、これで鏡割りをしていたのですか？"},{"id":{"d":888,"n":7,"t":5},"speaker":"A","text":"そうですよ。鏡開きのお汁粉に入っているお餅はいつもこれです。"},{"id":{"d":888,"n":8,"t":5},"speaker":"B","text":"この年になっても知らないことがあるものですね。"}];
INSERT INTO sentence [{"id":{"d":889,"n":1,"t":5},"speaker":"A","text":"今日のマルシェは強風のために中止だそうです。"},{"id":{"d":889,"n":2,"t":5},"speaker":"B","text":"楽しみにしていたのに残念です。"},{"id":{"d":889,"n":3,"t":5},"speaker":"A","text":"同感です。でも、確かにこの強風ではテントが飛んでしまいますからね。"},{"id":{"d":889,"n":4,"t":5},"speaker":"B","text":"参加するはずだった農家の方はもう帰られたのでしょうか？"},{"id":{"d":889,"n":5,"t":5},"speaker":"A","text":"天気予報を見れば中止となることが分かっていたと思うので、会場には来ていないのではないでしょうか。"},{"id":{"d":889,"n":6,"t":5},"speaker":"B","text":"そうですか。では、毎年買っているトウモロコシの農家さんには会えないのですね。"},{"id":{"d":889,"n":7,"t":5},"speaker":"A","text":"残念でしたね。せっかくここまで来たので、この先のカフェでコーンスープ付きのランチを食べませんか？"},{"id":{"d":889,"n":8,"t":5},"speaker":"B","text":"いいですね、行きましょう。風が強くて立っているのも大変なので、早くお店に入りたいです。"}];
INSERT INTO sentence [{"id":{"d":890,"n":1,"t":5},"speaker":"A","text":"このスタンプはかわいいですね。"},{"id":{"d":890,"n":2,"t":5},"speaker":"B","text":"これは、先輩が作って売っているスタンプなのです。"},{"id":{"d":890,"n":3,"t":5},"speaker":"A","text":"このスタンプを自分で作って売っているのですか？"},{"id":{"d":890,"n":4,"t":5},"speaker":"B","text":"はい。私でも作ることができるそうですが、絵心がないと駄目ですよね。"},{"id":{"d":890,"n":5,"t":5},"speaker":"A","text":"先輩は美術部や漫画研究会に入っていたのですか？"},{"id":{"d":890,"n":6,"t":5},"speaker":"B","text":"いいえ。私と同じ柔道部でしたが、イラストを描くのが趣味だったようです。"},{"id":{"d":890,"n":7,"t":5},"speaker":"A","text":"趣味の延長でスタンプを作ることができるのですね。"},{"id":{"d":890,"n":8,"t":5},"speaker":"B","text":"今では立派なイラストレーターですよ。"}];
INSERT INTO sentence [{"id":{"d":891,"n":1,"t":5},"speaker":"A","text":"お花を習いに行きたいのです。どこかに良い教室はありませんか？"},{"id":{"d":891,"n":2,"t":5},"speaker":"B","text":"あいにく心当たりはありませんね。カルチャーセンターに問い合わせてみたらいかがでしょう？"},{"id":{"d":891,"n":3,"t":5},"speaker":"A","text":"既に問い合わせてみましたが、近くのカルチャーセンターにはなかったのです。"},{"id":{"d":891,"n":4,"t":5},"speaker":"B","text":"では、お花屋さんに聞いてみたらどうでしょうか？個人で教えている先生が見つかるかもしれませんよ。"},{"id":{"d":891,"n":5,"t":5},"speaker":"A","text":"あなたは頭が良いですね。お花屋さんに聞くことは思いつきませんでした。"},{"id":{"d":891,"n":6,"t":5},"speaker":"B","text":"花束より、生け花に使いそうなお花が多いお店に聞いてみるといいと思います。"},{"id":{"d":891,"n":7,"t":5},"speaker":"A","text":"なるほど。それはとても良い考えです。あなたは本当に頼りになりますね。"},{"id":{"d":891,"n":8,"t":5},"speaker":"B","text":"褒めすぎですよ。"}];
INSERT INTO sentence [{"id":{"d":892,"n":1,"t":5},"speaker":"A","text":"ようやく華道教室を見つけることができました。"},{"id":{"d":892,"n":2,"t":5},"speaker":"B","text":"ずっと探していらっしゃるようでしたので、見つかって良かったですね。"},{"id":{"d":892,"n":3,"t":5},"speaker":"A","text":"はい。とても素敵な奥様が教えてくださる小さな教室です。"},{"id":{"d":892,"n":4,"t":5},"speaker":"B","text":"どのようにして見つけたのですか？"},{"id":{"d":892,"n":5,"t":5},"speaker":"A","text":"お花屋さんに、納品先の華道教室を教えていただきました。"},{"id":{"d":892,"n":6,"t":5},"speaker":"B","text":"華道の先生は何人もいるのですか？"},{"id":{"d":892,"n":7,"t":5},"speaker":"A","text":"はい。何人もの先生を教えていただいたので、その中から選びました。"},{"id":{"d":892,"n":8,"t":5},"speaker":"B","text":"この町にはそれほど多くの華道の先生がいるのですね。少し驚きました。"}];
INSERT INTO sentence [{"id":{"d":893,"n":1,"t":5},"speaker":"A","text":"暖かくなってきたら虫が出てきましたね。"},{"id":{"d":893,"n":2,"t":5},"speaker":"B","text":"そうですね。私の家では、昨日、アリの行列が台所に入ってきて大変でした。"},{"id":{"d":893,"n":3,"t":5},"speaker":"A","text":"退治したのですか？"},{"id":{"d":893,"n":4,"t":5},"speaker":"B","text":"はい。殺虫剤があったので、食品にかからないようにそれをまきました。"},{"id":{"d":893,"n":5,"t":5},"speaker":"A","text":"何がアリを呼び寄せたのでしょうね。"},{"id":{"d":893,"n":6,"t":5},"speaker":"B","text":"息子が棚の隙間にこぼしたお菓子のかけらが原因だったようです。"},{"id":{"d":893,"n":7,"t":5},"speaker":"A","text":"棚の隙間とは掃除がしづらいところですね。お気の毒でした。"},{"id":{"d":893,"n":8,"t":5},"speaker":"B","text":"はい。気を付けていたのですが、棚の隙間にあるお菓子のかけらは見つけられませんでした。"}];
INSERT INTO sentence [{"id":{"d":894,"n":1,"t":5},"speaker":"A","text":"こんにちは。今日はご主人と一緒ではないのですか？"},{"id":{"d":894,"n":2,"t":5},"speaker":"B","text":"主人は趣味のバイク整備に精を出しています。"},{"id":{"d":894,"n":3,"t":5},"speaker":"A","text":"いつも格好良いバイクに乗られていますものね。"},{"id":{"d":894,"n":4,"t":5},"speaker":"B","text":"格好良いのかもしれませんが、お金はかかりますし、一人の世界に入ってしまうのは困ったものです。"},{"id":{"d":894,"n":5,"t":5},"speaker":"A","text":"そのような人は、褒めたたえてあげると良いですよ。"},{"id":{"d":894,"n":6,"t":5},"speaker":"B","text":"褒めたたえるとどうなりますか？"},{"id":{"d":894,"n":7,"t":5},"speaker":"A","text":"満足して、それ以上はお金をかけなくなるかもしれませんよ。うちの主人がそうでした。"},{"id":{"d":894,"n":8,"t":5},"speaker":"B","text":"最近、あなたのご主人がガレージにいないのは、そのようなことがあったのですね。"}];
INSERT INTO sentence [{"id":{"d":895,"n":1,"t":5},"speaker":"A","text":"今日もおそばを食べるのですか？"},{"id":{"d":895,"n":2,"t":5},"speaker":"B","text":"そうですよ。"},{"id":{"d":895,"n":3,"t":5},"speaker":"A","text":"たまにはご飯が食べたいです。僕はお昼も係長に連れられておそばを食べたのです。"},{"id":{"d":895,"n":4,"t":5},"speaker":"B","text":"お父さんが定年退職後の趣味でそば打ちを始めてしまったので、当分はおそばになると思いますよ。"},{"id":{"d":895,"n":5,"t":5},"speaker":"A","text":"夕飯にまで残さないで、お昼に全部食べてしまってもよいのですよ。"},{"id":{"d":895,"n":6,"t":5},"speaker":"B","text":"お父さんと2人でこの量を食べきれるはずがないでしょう。"},{"id":{"d":895,"n":7,"t":5},"speaker":"A","text":"それはそうかもしれませんが。"},{"id":{"d":895,"n":8,"t":5},"speaker":"B","text":"趣味がなかったお父さんがやる気になっているのです。食べて応援してあげてください。"}];
INSERT INTO sentence [{"id":{"d":896,"n":1,"t":5},"speaker":"A","text":"ゴールデンウィークはどこかに行きましたか？"},{"id":{"d":896,"n":2,"t":5},"speaker":"B","text":"インターネットカフェで2泊3日の宿泊をし、漫画の読み放題を満喫しました。"},{"id":{"d":896,"n":3,"t":5},"speaker":"A","text":"充実したお休みだったのですね。"},{"id":{"d":896,"n":4,"t":5},"speaker":"B","text":"はい。インターネットカフェにはマッサージチェアがありましたし、安くておいしいご飯も食べられたので最高でした。"},{"id":{"d":896,"n":5,"t":5},"speaker":"A","text":"インターネットカフェにはシャワーもありますものね。"},{"id":{"d":896,"n":6,"t":5},"speaker":"B","text":"そうです。しかも、ドリンクも飲み放題で、ソフトクリームも食べ放題なのですよ。"},{"id":{"d":896,"n":7,"t":5},"speaker":"A","text":"それはうれしいですね。つい食べ過ぎてしまいそうです。"},{"id":{"d":896,"n":8,"t":5},"speaker":"B","text":"ええ、そうなのです。"}];
INSERT INTO sentence [{"id":{"d":897,"n":1,"t":5},"speaker":"A","text":"日曜日に荷物が届きます。"},{"id":{"d":897,"n":2,"t":5},"speaker":"B","text":"何の荷物ですか？"},{"id":{"d":897,"n":3,"t":5},"speaker":"A","text":"水槽と金魚の飼育セットです。"},{"id":{"d":897,"n":4,"t":5},"speaker":"B","text":"お祭りで瑛太が採ってきた金魚用に買ったのですね。"},{"id":{"d":897,"n":5,"t":5},"speaker":"A","text":"はい。いつまでもバケツの中にいるのはかわいそうですから、インターネットの通信販売で買いました。"},{"id":{"d":897,"n":6,"t":5},"speaker":"B","text":"お祭りの金魚はどのくらいの期間飼えるものなのでしょうか？"},{"id":{"d":897,"n":7,"t":5},"speaker":"A","text":"何年も飼って、コイのように大きくなったという話も聞いたことがあります。"},{"id":{"d":897,"n":8,"t":5},"speaker":"B","text":"そうなのですね。ぜひ長く飼ってあげてくださいね。"},{"id":{"d":897,"n":9,"t":5},"speaker":"A","text":"はい。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":898,"n":1,"t":5},"speaker":"A","text":"のど自慢大会に由紀ちゃんが出るそうです。"},{"id":{"d":898,"n":2,"t":5},"speaker":"B","text":"由紀ちゃんは民謡を習っているから、歌が上手ですものね。"},{"id":{"d":898,"n":3,"t":5},"speaker":"A","text":"私も出たかったです。"},{"id":{"d":898,"n":4,"t":5},"speaker":"B","text":"大会に出る方法も分からなかったのですから、仕方がありませんよ。"},{"id":{"d":898,"n":5,"t":5},"speaker":"A","text":"それでは由紀ちゃんの応援に行きたいです。"},{"id":{"d":898,"n":6,"t":5},"speaker":"B","text":"観覧席の申し込みがまだ間に合うのかは調べてみましたか？"},{"id":{"d":898,"n":7,"t":5},"speaker":"A","text":"調べました。明日までに往復はがきを出せばよいそうです。"},{"id":{"d":898,"n":8,"t":5},"speaker":"B","text":"往復はがきですか？"},{"id":{"d":898,"n":9,"t":5},"speaker":"A","text":"はい。家にありますか？"},{"id":{"d":898,"n":10,"t":5},"speaker":"B","text":"往復はがきは家にありませんので、郵便局で買ってくる必要があります。"}];
INSERT INTO sentence [{"id":{"d":899,"n":1,"t":5},"speaker":"A","text":"昨日は、海に行ってきました。"},{"id":{"d":899,"n":2,"t":5},"speaker":"B","text":"もう秋なのに海に行ったのですか？"},{"id":{"d":899,"n":3,"t":5},"speaker":"A","text":"はい、秋だからこそ行ったのですよ。人が少なくて波の音がよく聞こえました。"},{"id":{"d":899,"n":4,"t":5},"speaker":"B","text":"そのような時期に海辺を歩くのも良いですね。"},{"id":{"d":899,"n":5,"t":5},"speaker":"A","text":"写真も撮ってきましたよ。富士山と海の写真です。"},{"id":{"d":899,"n":6,"t":5},"speaker":"B","text":"きれいに撮れていますね。とてもすてきな写真です。"},{"id":{"d":899,"n":7,"t":5},"speaker":"A","text":"もしよければ、この写真をあなたに送りますか？"},{"id":{"d":899,"n":8,"t":5},"speaker":"B","text":"うれしいです。ぜひメールで送ってください。"}];
INSERT INTO sentence [{"id":{"d":900,"n":1,"t":5},"speaker":"A","text":"朝から大雨ですね。"},{"id":{"d":900,"n":2,"t":5},"speaker":"B","text":"今日の花火大会は中止でしょうか？"},{"id":{"d":900,"n":3,"t":5},"speaker":"A","text":"その可能性は高いですね。天気予報では雨は深夜までやまないそうです。"},{"id":{"d":900,"n":4,"t":5},"speaker":"B","text":"せっかく新しい浴衣を着て行こうと思っていたのに、残念です。"},{"id":{"d":900,"n":5,"t":5},"speaker":"A","text":"熱海まで行けば、別の花火大会がありますよ。"},{"id":{"d":900,"n":6,"t":5},"speaker":"B","text":"思い切って熱海まで旅行に行ってしまいましょうか？"},{"id":{"d":900,"n":7,"t":5},"speaker":"A","text":"それも良いかもしれません。花火が目当てでは日帰り旅行は無理ですから、温泉宿に泊まりたいです。"}];
INSERT INTO sentence [{"id":{"d":901,"n":1,"t":5},"speaker":"A","text":"今年の暮れまでには、この絵を仕上げてしまいたいです。"},{"id":{"d":901,"n":2,"t":5},"speaker":"B","text":"これは卒業制作なのですか？"},{"id":{"d":901,"n":3,"t":5},"speaker":"A","text":"いいえ、卒業制作の絵は別に描いています。これはコンクール用です。"},{"id":{"d":901,"n":4,"t":5},"speaker":"B","text":"並行して描いているのですね。大変そうです。"},{"id":{"d":901,"n":5,"t":5},"speaker":"A","text":"本当は去年のコンクールで最優秀賞を取って、今年は卒業制作に没頭する予定でした。"},{"id":{"d":901,"n":6,"t":5},"speaker":"B","text":"ということは、去年のコンクールは駄目だったのですか？"},{"id":{"d":901,"n":7,"t":5},"speaker":"A","text":"はい。去年は自転車で転倒して利き腕を骨折し、コンクール用の絵を完成させられなかったのです。"},{"id":{"d":901,"n":8,"t":5},"speaker":"B","text":"そう言われれば、去年の今頃はギプスをしていましたね。今年こそ最優秀賞を取ってください。応援しています。"},{"id":{"d":901,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":902,"n":1,"t":5},"speaker":"A","text":"お誕生日おめでとうございます。これはあなたへのプレゼントです。"},{"id":{"d":902,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。これはブタの置物ですか？"},{"id":{"d":902,"n":3,"t":5},"speaker":"A","text":"これは蚊遣り豚ですよ。"},{"id":{"d":902,"n":4,"t":5},"speaker":"B","text":"穴が開いていますね。この中に蚊取り線香を入れるのですか？"},{"id":{"d":902,"n":5,"t":5},"speaker":"A","text":"そうです。この間、蚊に悩まされていると言っていたでしょう？"},{"id":{"d":902,"n":6,"t":5},"speaker":"B","text":"はい、暑くなってきたら蚊が増えて困っていました。覚えていてくれたのですね。"},{"id":{"d":902,"n":7,"t":5},"speaker":"A","text":"はい。蚊取り線香は自分で好みのものを用意してくださいね。"},{"id":{"d":902,"n":8,"t":5},"speaker":"B","text":"帰りにホームセンターで買って帰ります。早速今日から使いますね。"}];
INSERT INTO sentence [{"id":{"d":903,"n":1,"t":5},"speaker":"A","text":"母の日に娘がカーネーションを1本くれました。"},{"id":{"d":903,"n":2,"t":5},"speaker":"B","text":"かわいらしいですね。きっとお小遣いを節約して買ってきてくれたのですよ。"},{"id":{"d":903,"n":3,"t":5},"speaker":"A","text":"もう少し多くお小遣いをあげておけばよかったでしょうか？"},{"id":{"d":903,"n":4,"t":5},"speaker":"B","text":"そのようなことはないと思いますよ。少ないお小遣いで工夫することも勉強です。"},{"id":{"d":903,"n":5,"t":5},"speaker":"A","text":"私は母にバッグをあげました。かなり高級でしたので生活費を切り詰めましたよ。"},{"id":{"d":903,"n":6,"t":5},"speaker":"B","text":"娘共々、母の日のプレゼント選びで勉強したのですね。"},{"id":{"d":903,"n":7,"t":5},"speaker":"A","text":"そうなりますね。"},{"id":{"d":903,"n":8,"t":5},"speaker":"B","text":"私は母に何もあげていないし、子どもからももらっていないので勉強になりませんでした。"}];
INSERT INTO sentence [{"id":{"d":904,"n":1,"t":5},"speaker":"A","text":"もうご飯の時間ですから、おもちゃ箱におもちゃを片付けてください。"},{"id":{"d":904,"n":2,"t":5},"speaker":"B","text":"まだもう少し遊びたいです。"},{"id":{"d":904,"n":3,"t":5},"speaker":"A","text":"ご飯が冷めてしまいますよ。"},{"id":{"d":904,"n":4,"t":5},"speaker":"B","text":"後でまた遊びますから、このままでよいですか？"},{"id":{"d":904,"n":5,"t":5},"speaker":"A","text":"駄目です。ご飯の前には、一度片付けてください。"},{"id":{"d":904,"n":6,"t":5},"speaker":"B","text":"分かりました。片付けます。"},{"id":{"d":904,"n":7,"t":5},"speaker":"A","text":"ふてくされて、雑に片付けてはいけませんよ。"},{"id":{"d":904,"n":8,"t":5},"speaker":"B","text":"分かっています。"}];
INSERT INTO sentence [{"id":{"d":905,"n":1,"t":5},"speaker":"A","text":"6月10日はお父さんとお母さんの結婚記念日でしたよね。"},{"id":{"d":905,"n":2,"t":5},"speaker":"B","text":"そうですけれど、お父さんは何もしてくれたことがありません。"},{"id":{"d":905,"n":3,"t":5},"speaker":"A","text":"今年は銀婚式でもありますし、きっと何かしてくれますよ。"},{"id":{"d":905,"n":4,"t":5},"speaker":"B","text":"そうでしょうか？でも、期待はしないようにしておきます。"},{"id":{"d":905,"n":5,"t":5},"speaker":"A","text":"お母さん、期待しましょうよ。"},{"id":{"d":905,"n":6,"t":5},"speaker":"B","text":"さては、あなたたちがお父さんに入れ知恵をして、何かさせるつもりですね。"},{"id":{"d":905,"n":7,"t":5},"speaker":"A","text":"それは秘密です。でも、大いに期待していてください。"},{"id":{"d":905,"n":8,"t":5},"speaker":"B","text":"分かりました。ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":906,"n":1,"t":5},"speaker":"A","text":"結婚記念日に腕時計を頂けるなんて思ってもいませんでした。"},{"id":{"d":906,"n":2,"t":5},"speaker":"B","text":"気に入ってくれましたか？"},{"id":{"d":906,"n":3,"t":5},"speaker":"A","text":"とても気に入りました。私の腕時計はもう30年も使っていましたから、新しいものが頂けてうれしいです。"},{"id":{"d":906,"n":4,"t":5},"speaker":"B","text":"娘たちに言われて買ってきて良かったですよ。"},{"id":{"d":906,"n":5,"t":5},"speaker":"A","text":"娘たちに言われないと買ってくれなかったのですか？"},{"id":{"d":906,"n":6,"t":5},"speaker":"B","text":"いいえ、何か用意しようとは思っていました。でも何にするかが決まらなくて困っていたのです。"},{"id":{"d":906,"n":7,"t":5},"speaker":"A","text":"そうだったのですね。でも、どうして腕時計にしたのですか？"},{"id":{"d":906,"n":8,"t":5},"speaker":"B","text":"記念日というものは時間に関係しているので、時間に深く関わる腕時計が良いのではないかと娘たちが助言をしてくれたからです。"}];
INSERT INTO sentence [{"id":{"d":907,"n":1,"t":5},"speaker":"A","text":"今日は平日なのに、どうして子どもたちが遊びに来ているのですか？"},{"id":{"d":907,"n":2,"t":5},"speaker":"B","text":"千葉県の県民の日だからですよ。"},{"id":{"d":907,"n":3,"t":5},"speaker":"A","text":"県民の日は学校がお休みなのですか？"},{"id":{"d":907,"n":4,"t":5},"speaker":"B","text":"そうです。この日は、千葉県内の観光施設の入場料が安くなるそうです。場所によっては無料で利用できるところもあるそうですよ。"},{"id":{"d":907,"n":5,"t":5},"speaker":"A","text":"そのような特典があるのですね。うらやましいです。"},{"id":{"d":907,"n":6,"t":5},"speaker":"B","text":"神奈川県にも県民の日があるでしょう？"},{"id":{"d":907,"n":7,"t":5},"speaker":"A","text":"いいえ、神奈川県にはありませんよ。東京都にはあるのですか？"},{"id":{"d":907,"n":8,"t":5},"speaker":"B","text":"はい。東京都には都民の日があります。"}];
INSERT INTO sentence [{"id":{"d":908,"n":1,"t":5},"speaker":"A","text":"今日は少し怖い目に遭いました。"},{"id":{"d":908,"n":2,"t":5},"speaker":"B","text":"何があったのですか？"},{"id":{"d":908,"n":3,"t":5},"speaker":"A","text":"会社の帰りにスマートフォンで音楽を聴きながら歩いていたら、車にひかれそうになりました。"},{"id":{"d":908,"n":4,"t":5},"speaker":"B","text":"危ないですね。大丈夫だったのですか？"},{"id":{"d":908,"n":5,"t":5},"speaker":"A","text":"幸いに何もありませんでした。私が少し車道にはみ出していたのです。クラクションの音で気が付きました。"},{"id":{"d":908,"n":6,"t":5},"speaker":"B","text":"暗い夜道でスマートフォンに集中するのはあまりにも危ないですよ。なぜそのようなことをしたのですか？"},{"id":{"d":908,"n":7,"t":5},"speaker":"A","text":"好きなアーティストのアルバムが出たばかりだったので、どうしても聴きたかったのです。"},{"id":{"d":908,"n":8,"t":5},"speaker":"B","text":"それでも道を歩くときはやめた方がいいです。本当にひかれなくて良かったです。"}];
INSERT INTO sentence [{"id":{"d":909,"n":1,"t":5},"speaker":"A","text":"ウクレレを習うことができる教室はありますか？"},{"id":{"d":909,"n":2,"t":5},"speaker":"B","text":"三つ先の駅に「ギター・ウクレレ」という看板が出ていたように思います。"},{"id":{"d":909,"n":3,"t":5},"speaker":"A","text":"駅のベンチに付いている広告ですか？"},{"id":{"d":909,"n":4,"t":5},"speaker":"B","text":"あやふやな記憶ですが、多分、ベンチに付いていた広告だったと思います。"},{"id":{"d":909,"n":5,"t":5},"speaker":"A","text":"今度行って見てみます。情報をありがとうございます。"},{"id":{"d":909,"n":6,"t":5},"speaker":"B","text":"どうしてギターではなくウクレレなのですか？"},{"id":{"d":909,"n":7,"t":5},"speaker":"A","text":"ハワイに行ってウクレレを買ってきたのです。弾けなければただの置物になってしまうので、習ってみようと思いまして。"},{"id":{"d":909,"n":8,"t":5},"speaker":"B","text":"そうでしたか。教室が見つかるといいですね。"}];
INSERT INTO sentence [{"id":{"d":910,"n":1,"t":5},"speaker":"A","text":"今年は雪が少ないですね。"},{"id":{"d":910,"n":2,"t":5},"speaker":"B","text":"去年の大雪を思うと助かりますよね。"},{"id":{"d":910,"n":3,"t":5},"speaker":"A","text":"あまり雪下ろしをしなくてもよいということが一番うれしいです。"},{"id":{"d":910,"n":4,"t":5},"speaker":"B","text":"毎年、雪下ろしは大変ですからね。"},{"id":{"d":910,"n":5,"t":5},"speaker":"A","text":"今年は雪下ろしの時間が少ないので、編み物に精を出しています。"},{"id":{"d":910,"n":6,"t":5},"speaker":"B","text":"今、着けているマフラーと帽子も今年に作ったのですか？"},{"id":{"d":910,"n":7,"t":5},"speaker":"A","text":"そうです。たくさん編んで、婦人会のバザーに出そうと思っています。"},{"id":{"d":910,"n":8,"t":5},"speaker":"B","text":"買いに行きます。"}];
INSERT INTO sentence [{"id":{"d":911,"n":1,"t":5},"speaker":"A","text":"五目豆ですか？"},{"id":{"d":911,"n":2,"t":5},"speaker":"B","text":"そうです。豆まきの豆を煮たのですよ。"},{"id":{"d":911,"n":3,"t":5},"speaker":"A","text":"豆まきの豆は毎年、食べ切れませんからね。"},{"id":{"d":911,"n":4,"t":5},"speaker":"B","text":"そのような理由もありますけれど、おじいちゃんがそのままの豆を食べられないでしょう？"},{"id":{"d":911,"n":5,"t":5},"speaker":"A","text":"はい、豆が硬いので、歯のないおじいちゃんには食べられませんね。"},{"id":{"d":911,"n":6,"t":5},"speaker":"B","text":"柔らかく煮れば食べられると思って、作りました。"},{"id":{"d":911,"n":7,"t":5},"speaker":"A","text":"手間がかかるのに、大変でしたね。"},{"id":{"d":911,"n":8,"t":5},"speaker":"B","text":"おじいちゃんにも年の数だけ豆を食べてもらいたいですからね。"}];
INSERT INTO sentence [{"id":{"d":912,"n":1,"t":5},"speaker":"A","text":"おみくじを引きたいのですが、こちらで引けますか？"},{"id":{"d":912,"n":2,"t":5},"speaker":"B","text":"はい。そちらの箱を逆さにして振って出た番号を教えてください。"},{"id":{"d":912,"n":3,"t":5},"speaker":"A","text":"分かりました。18番です。"},{"id":{"d":912,"n":4,"t":5},"speaker":"B","text":"初穂料は100円です。"},{"id":{"d":912,"n":5,"t":5},"speaker":"A","text":"はい。"},{"id":{"d":912,"n":6,"t":5},"speaker":"B","text":"こちらが18番のおみくじです。"},{"id":{"d":912,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。中吉でした。まずまずですね。"}];
INSERT INTO sentence [{"id":{"d":913,"n":1,"t":5},"speaker":"A","text":"もうすぐ年末ジャンボ宝くじの抽選です。"},{"id":{"d":913,"n":2,"t":5},"speaker":"B","text":"楽しみですね。"},{"id":{"d":913,"n":3,"t":5},"speaker":"A","text":"はい。今年は気張って9万円分も買ってしまいました。"},{"id":{"d":913,"n":4,"t":5},"speaker":"B","text":"それはまた随分つぎ込みましたね。"},{"id":{"d":913,"n":5,"t":5},"speaker":"A","text":"今年こそ当選してみせますよ。"},{"id":{"d":913,"n":6,"t":5},"speaker":"B","text":"宝くじは神棚に置いたのですか？"},{"id":{"d":913,"n":7,"t":5},"speaker":"A","text":"いいえ。神棚の掃除がまだ済んでいないので、そこには置いていません。"},{"id":{"d":913,"n":8,"t":5},"speaker":"B","text":"早く掃除をして神様に預けた方がいいですよ。"}];
INSERT INTO sentence [{"id":{"d":914,"n":1,"t":5},"speaker":"A","text":"元日から3日まではスーパーマーケットがお休みになりますから、おせち料理以外も用意しなくてはいけませんね。"},{"id":{"d":914,"n":2,"t":5},"speaker":"B","text":"そうですね。買ったおせち料理は元日のうちに食べ終わってしまいます。"},{"id":{"d":914,"n":3,"t":5},"speaker":"A","text":"2日からのご飯は何にしますか？"},{"id":{"d":914,"n":4,"t":5},"speaker":"B","text":"すき焼きはどうですか？"},{"id":{"d":914,"n":5,"t":5},"speaker":"A","text":"いいですね。2日はすき焼きにしましょう。"},{"id":{"d":914,"n":6,"t":5},"speaker":"B","text":"3日は普通に焼き魚とご飯でもよいですね。"},{"id":{"d":914,"n":7,"t":5},"speaker":"A","text":"お正月なのに、焼き魚でよいのですか？"},{"id":{"d":914,"n":8,"t":5},"speaker":"B","text":"いいですよ。お正月というなら、いつもより高級な魚にすればよいのではありませんか？"}];
INSERT INTO sentence [{"id":{"d":915,"n":1,"t":5},"speaker":"A","text":"暑いときには辛いものが食べたくなりませんか？"},{"id":{"d":915,"n":2,"t":5},"speaker":"B","text":"いいえ。このように暑いと、私は食欲がなくなります。"},{"id":{"d":915,"n":3,"t":5},"speaker":"A","text":"そうなのですね。どなたか一緒に辛いものを食べに行ってくれそうな人はいないでしょうか？"},{"id":{"d":915,"n":4,"t":5},"speaker":"B","text":"すぐには思い当たりません。どこのお店に行くつもりだったのですか？"},{"id":{"d":915,"n":5,"t":5},"speaker":"A","text":"インド料理かタイ料理か韓国料理です。"},{"id":{"d":915,"n":6,"t":5},"speaker":"B","text":"韓国料理でしたら付き合ってもよいですよ。"},{"id":{"d":915,"n":7,"t":5},"speaker":"A","text":"よいのですか？食べられるものはありますか？"},{"id":{"d":915,"n":8,"t":5},"speaker":"B","text":"冷麺なら大丈夫だと思います。"}];
INSERT INTO sentence [{"id":{"d":916,"n":1,"t":5},"speaker":"A","text":"トマトがたくさん採れたので食べてください。"},{"id":{"d":916,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。家庭菜園で採れたのですか？"},{"id":{"d":916,"n":3,"t":5},"speaker":"A","text":"そうです。この春から熱中しています。"},{"id":{"d":916,"n":4,"t":5},"speaker":"B","text":"初めて作ったのですか？立派なトマトですね。"},{"id":{"d":916,"n":5,"t":5},"speaker":"A","text":"初めてですが、われながら上手にできました。"},{"id":{"d":916,"n":6,"t":5},"speaker":"B","text":"今年の天気はトマト栽培に向いていたのでしょうか？"},{"id":{"d":916,"n":7,"t":5},"speaker":"A","text":"そうかもしれません。"}];
INSERT INTO sentence [{"id":{"d":917,"n":1,"t":5},"speaker":"A","text":"木島さん、こけしを集めていると聞いたのですが、本当ですか？"},{"id":{"d":917,"n":2,"t":5},"speaker":"B","text":"はい。昔からこけしが大好きで、家にたくさんありますよ。"},{"id":{"d":917,"n":3,"t":5},"speaker":"A","text":"実は、私の祖父母の家が解体される前に見に行ったら、こけしがたくさんあったのです。"},{"id":{"d":917,"n":4,"t":5},"speaker":"B","text":"それらはおばあさんのコレクションなのでしょうか？"},{"id":{"d":917,"n":5,"t":5},"speaker":"A","text":"はい。亡くなった祖母のものですけれど、もしよかったらもらっていただけませんか？"},{"id":{"d":917,"n":6,"t":5},"speaker":"B","text":"昔のこけしにはとても価値のあるものがあることもあります。本当に頂いてしまってよいのですか？"},{"id":{"d":917,"n":7,"t":5},"speaker":"A","text":"はい。このままでは家と一緒にごみになってしまいますから、もらっていただけたらうれしいです。"},{"id":{"d":917,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。今度の休みに取りに行かせてもらいます。"}];
INSERT INTO sentence [{"id":{"d":918,"n":1,"t":5},"speaker":"A","text":"寛太、こちらに来てくれますか？"},{"id":{"d":918,"n":2,"t":5},"speaker":"B","text":"はい、おじいさん。"},{"id":{"d":918,"n":3,"t":5},"speaker":"A","text":"もうすぐあなたの誕生日ですから、これをあげます。"},{"id":{"d":918,"n":4,"t":5},"speaker":"B","text":"これは碁石ですか？高級品ですよね？"},{"id":{"d":918,"n":5,"t":5},"speaker":"A","text":"はい。小さな頃から私の相手をして囲碁が上手になって、今は囲碁部でも強い方なのでしょう？"},{"id":{"d":918,"n":6,"t":5},"speaker":"B","text":"はい。囲碁部の中では強い、という程度ですけれどね。"},{"id":{"d":918,"n":7,"t":5},"speaker":"A","text":"これからも精進するように、私からの贈り物です。"},{"id":{"d":918,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。大事にします。"}];
INSERT INTO sentence [{"id":{"d":919,"n":1,"t":5},"speaker":"A","text":"みんなでカラオケに行きましょう。"},{"id":{"d":919,"n":2,"t":5},"speaker":"B","text":"この間行った時は理衣は飽きて寝てしまいましたが、また連れて行くのですか？"},{"id":{"d":919,"n":3,"t":5},"speaker":"A","text":"今日は大丈夫ですよ。"},{"id":{"d":919,"n":4,"t":5},"speaker":"B","text":"どうしてそう思うのですか？"},{"id":{"d":919,"n":5,"t":5},"speaker":"A","text":"今日はしっかりお昼寝もしましたし、これを持っていきますから。"},{"id":{"d":919,"n":6,"t":5},"speaker":"B","text":"これはペットボトルですか？"},{"id":{"d":919,"n":7,"t":5},"speaker":"A","text":"ペットボトルで作ったマラカスです。これを振っていればきっと眠くはなりませんよ。"},{"id":{"d":919,"n":8,"t":5},"speaker":"B","text":"それはいいですね。理衣も楽しめそうです。"}];
INSERT INTO sentence [{"id":{"d":920,"n":1,"t":5},"speaker":"A","text":"夏の京都は暑いですね。"},{"id":{"d":920,"n":2,"t":5},"speaker":"B","text":"京都は盆地ですからね。"},{"id":{"d":920,"n":3,"t":5},"speaker":"A","text":"暑いのに、これほど多くの人がいるとは思っていませんでした。"},{"id":{"d":920,"n":4,"t":5},"speaker":"B","text":"祇園祭ですからね。"},{"id":{"d":920,"n":5,"t":5},"speaker":"A","text":"山鉾巡行というものはまだ先でしょう？"},{"id":{"d":920,"n":6,"t":5},"speaker":"B","text":"はい。でもその前にお祭りはもう始まっているのですよ。"},{"id":{"d":920,"n":7,"t":5},"speaker":"A","text":"これほど暑くて人も多いのなら、秋に来れば良かったです。"},{"id":{"d":920,"n":8,"t":5},"speaker":"B","text":"お祭りの雰囲気を感じながらかき氷を食べるのも、風情があって良いですよ。"}];
INSERT INTO sentence [{"id":{"d":921,"n":1,"t":5},"speaker":"A","text":"昨日はあなたのお誕生日でしたよね？"},{"id":{"d":921,"n":2,"t":5},"speaker":"B","text":"はい、昨日は私の誕生日でした。"},{"id":{"d":921,"n":3,"t":5},"speaker":"A","text":"一日遅れですけれど、これをどうぞ。"},{"id":{"d":921,"n":4,"t":5},"speaker":"B","text":"ケーキですか？ありがとうございます。"},{"id":{"d":921,"n":5,"t":5},"speaker":"A","text":"コンビニエンスストアのケーキなのですが、お誕生日おめでとうございます。"},{"id":{"d":921,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。この年になったら誰もお祝いしてくれませんから、うれしいです。"},{"id":{"d":921,"n":7,"t":5},"speaker":"A","text":"昨日はどのように過ごしていましたか？"},{"id":{"d":921,"n":8,"t":5},"speaker":"B","text":"一人でファミリーレストランに行って、アプリケーションの誕生日特典でパンケーキを食べてきました。"}];
INSERT INTO sentence [{"id":{"d":922,"n":1,"t":5},"speaker":"A","text":"今日は豪太を遊びに連れて行ってくれてありがとうございました。"},{"id":{"d":922,"n":2,"t":5},"speaker":"B","text":"どういたしまして。たまにはおいっ子と遊ぶのも楽しいものでした。"},{"id":{"d":922,"n":3,"t":5},"speaker":"A","text":"豪太「お兄ちゃんと映画を見てきた」と言っていましたよ。"},{"id":{"d":922,"n":4,"t":5},"speaker":"B","text":"はい、怪獣映画を見てきました。"},{"id":{"d":922,"n":5,"t":5},"speaker":"A","text":"豪太は怪獣ものが大好きですから、きっと楽しめたと思います。"},{"id":{"d":922,"n":6,"t":5},"speaker":"B","text":"おいのためと言いながら、自分も楽しめる映画でした。"},{"id":{"d":922,"n":7,"t":5},"speaker":"A","text":"そういえば、あなたも子どもの頃は怪獣が大好きでしたね。"},{"id":{"d":922,"n":8,"t":5},"speaker":"B","text":"はい。姉さんのお人形と怪獣のフィギュアを戦わせて遊んでいましたね。"}];
INSERT INTO sentence [{"id":{"d":923,"n":1,"t":5},"speaker":"A","text":"その三角形の袋には何が入っているのですか？"},{"id":{"d":923,"n":2,"t":5},"speaker":"B","text":"この中にはハープが入っています。"},{"id":{"d":923,"n":3,"t":5},"speaker":"A","text":"楽器のハープですか？持ち歩けるほど小さいのですか？"},{"id":{"d":923,"n":4,"t":5},"speaker":"B","text":"はい、これは小型のハープです。教室には大きいものがありますよ。"},{"id":{"d":923,"n":5,"t":5},"speaker":"A","text":"教室でハープを習っているのですか？"},{"id":{"d":923,"n":6,"t":5},"speaker":"B","text":"はい。最近習い始めたばかりなので、まだ『きらきら星』しか弾けません。"},{"id":{"d":923,"n":7,"t":5},"speaker":"A","text":"習い始めたばかりでも1曲弾けるのですね。"},{"id":{"d":923,"n":8,"t":5},"speaker":"B","text":"練習すれば誰でもできますよ。"}];
INSERT INTO sentence [{"id":{"d":924,"n":1,"t":5},"speaker":"A","text":"最近は、時代物の作品にはまっています。"},{"id":{"d":924,"n":2,"t":5},"speaker":"B","text":"以前はSF小説ばかりを読んでいたのにどうしてですか？"},{"id":{"d":924,"n":3,"t":5},"speaker":"A","text":"今月は、懐事情が苦しくて本が買えなかったのですが、青空文庫ならただで読めると気が付いたからです。"},{"id":{"d":924,"n":4,"t":5},"speaker":"B","text":"時代物の作品も青空文庫で読めるのですか？"},{"id":{"d":924,"n":5,"t":5},"speaker":"A","text":"最近は読めるようになってきたようです。"},{"id":{"d":924,"n":6,"t":5},"speaker":"B","text":"時代物は面白いですか？"},{"id":{"d":924,"n":7,"t":5},"speaker":"A","text":"はい。過去にタイムスリップした気分が味わえて、面白いです。"},{"id":{"d":924,"n":8,"t":5},"speaker":"B","text":"そのような意味では時代物もSFなのですね。"},{"id":{"d":924,"n":9,"t":5},"speaker":"A","text":"そうかもしれません。"}];
INSERT INTO sentence [{"id":{"d":925,"n":1,"t":5},"speaker":"A","text":"これは何ですか？"},{"id":{"d":925,"n":2,"t":5},"speaker":"B","text":"青汁ですよ。以前から「健康に良さそうなので試したい」と言っていたでしょう？"},{"id":{"d":925,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。甘くてとてもおいしいですね。"},{"id":{"d":925,"n":4,"t":5},"speaker":"B","text":"気に入ってくれてよかったです。"},{"id":{"d":925,"n":5,"t":5},"speaker":"A","text":"これなら毎日でも飲める気がします。"},{"id":{"d":925,"n":6,"t":5},"speaker":"B","text":"言いづらいのですが、今日は4月1日ですよ。"},{"id":{"d":925,"n":7,"t":5},"speaker":"A","text":"つまり、エイプリルフールのうそということですか？"},{"id":{"d":925,"n":8,"t":5},"speaker":"B","text":"そうです。"},{"id":{"d":925,"n":9,"t":5},"speaker":"A","text":"では、この飲み物は何ですか？"},{"id":{"d":925,"n":10,"t":5},"speaker":"B","text":"抹茶ラテですよ。"}];
INSERT INTO sentence [{"id":{"d":926,"n":1,"t":5},"speaker":"A","text":"今日の夕飯はカレーライスです。子どもたちと一緒に作ります。"},{"id":{"d":926,"n":2,"t":5},"speaker":"B","text":"子どもたちがお手伝いしてくれるのですね。"},{"id":{"d":926,"n":3,"t":5},"speaker":"A","text":"はい。昼間に、料理がテーマのカードゲームで遊んだら興味を持ってくれたようです。"},{"id":{"d":926,"n":4,"t":5},"speaker":"B","text":"そのカードゲームはどういうものなのですか？"},{"id":{"d":926,"n":5,"t":5},"speaker":"A","text":"メニューの食材が描かれているカードを最初にそろえた人が勝ちというゲームです。"},{"id":{"d":926,"n":6,"t":5},"speaker":"B","text":"小さい子でも分かりやすそうですね。"},{"id":{"d":926,"n":7,"t":5},"speaker":"A","text":"ええ。私が「カレーライスの材料は家に全部ある」と言ったら、子どもたちが「作ってみたい」と言ったので、作ることにしました。"},{"id":{"d":926,"n":8,"t":5},"speaker":"B","text":"食育にもなる素晴らしいゲームですね。"}];
INSERT INTO sentence [{"id":{"d":927,"n":1,"t":5},"speaker":"A","text":"今日は随分とにぎやかですね。"},{"id":{"d":927,"n":2,"t":5},"speaker":"B","text":"弟が、動物が話すおもちゃをたくさん置いていったのです。"},{"id":{"d":927,"n":3,"t":5},"speaker":"A","text":"この犬がしゃべるのですか？"},{"id":{"d":927,"n":4,"t":5},"speaker":"B","text":"そうです。ほら、「この犬がしゃべるのですか」と言ったでしょう？"},{"id":{"d":927,"n":5,"t":5},"speaker":"A","text":"なるほど、そのままオウム返しをするのですね。"},{"id":{"d":927,"n":6,"t":5},"speaker":"B","text":"このようなものがたくさんあって少し騒がしいのですが、子どもは大喜びなのです。"},{"id":{"d":927,"n":7,"t":5},"speaker":"A","text":"ずっと遊んでいるのですか？"},{"id":{"d":927,"n":8,"t":5},"speaker":"B","text":"はい。でも、そろそろ夕飯の時間なのでやめさせます。"}];
INSERT INTO sentence [{"id":{"d":928,"n":1,"t":5},"speaker":"A","text":"お歳暮にハムを頂きました。"},{"id":{"d":928,"n":2,"t":5},"speaker":"B","text":"高級そうなハムですね。"},{"id":{"d":928,"n":3,"t":5},"speaker":"A","text":"ええ。どのようにして食べましょうか？"},{"id":{"d":928,"n":4,"t":5},"speaker":"B","text":"厚く切ってハムステーキにするのが良いのではありませんか？"},{"id":{"d":928,"n":5,"t":5},"speaker":"A","text":"いいですね。せっかくなら、サラダも作って、ワインやチーズも用意したいです。"},{"id":{"d":928,"n":6,"t":5},"speaker":"B","text":"そうですね。でも、残念ながらワインとチーズは家にありませんよ。"},{"id":{"d":928,"n":7,"t":5},"speaker":"A","text":"明日は買い物に行きますから、明日の夕飯にしましょう。"},{"id":{"d":928,"n":8,"t":5},"speaker":"B","text":"明日が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":929,"n":1,"t":5},"speaker":"A","text":"家を片付けていたら、子どもの頃のお人形が出てきました。"},{"id":{"d":929,"n":2,"t":5},"speaker":"B","text":"かわいらしいですね。でも着ている服がボロボロです。"},{"id":{"d":929,"n":3,"t":5},"speaker":"A","text":"はい。少しかわいそうです。"},{"id":{"d":929,"n":4,"t":5},"speaker":"B","text":"お人形用に売っている服を買ってきてあげましょうか？"},{"id":{"d":929,"n":5,"t":5},"speaker":"A","text":"いいえ。もう私も大人になったので、この子に服くらいは作ってあげられますよ。"},{"id":{"d":929,"n":6,"t":5},"speaker":"B","text":"では、自分で作りますか？"},{"id":{"d":929,"n":7,"t":5},"speaker":"A","text":"はい。小物を作った時のレースや残り布もありますから、作ってみます。"},{"id":{"d":929,"n":8,"t":5},"speaker":"B","text":"すてきですね。出来上がったら見せてください。"}];
INSERT INTO sentence [{"id":{"d":930,"n":1,"t":5},"speaker":"A","text":"元日に、着物を着て初詣に行きたいです。"},{"id":{"d":930,"n":2,"t":5},"speaker":"B","text":"着付けをしてくれる美容院はあるのですか？"},{"id":{"d":930,"n":3,"t":5},"speaker":"A","text":"いいえ、いつも利用しているところはお休みです。どこかに開いているところはないでしょうか？"},{"id":{"d":930,"n":4,"t":5},"speaker":"B","text":"分かりません。髪のセットも必要なのですか？"},{"id":{"d":930,"n":5,"t":5},"speaker":"A","text":"いいえ、髪は自分で結うので大丈夫です。"},{"id":{"d":930,"n":6,"t":5},"speaker":"B","text":"着物だけなら、私が着せてあげましょうか？"},{"id":{"d":930,"n":7,"t":5},"speaker":"A","text":"それはありがたいですが、着付けができるのですか？"},{"id":{"d":930,"n":8,"t":5},"speaker":"B","text":"はい。昔、師範の資格を取ったことがあります。"}];
INSERT INTO sentence [{"id":{"d":931,"n":1,"t":5},"speaker":"A","text":"今日は冬至です。"},{"id":{"d":931,"n":2,"t":5},"speaker":"B","text":"かぼちゃを食べる日ですね。"},{"id":{"d":931,"n":3,"t":5},"speaker":"A","text":"毎年、煮つけにするのも芸がないので今年はこれを作ってみました。"},{"id":{"d":931,"n":4,"t":5},"speaker":"B","text":"パンプキンパイですか？"},{"id":{"d":931,"n":5,"t":5},"speaker":"A","text":"はい。おいしそうにできたでしょう？"},{"id":{"d":931,"n":6,"t":5},"speaker":"B","text":"そうですね。このパイのレシピは、ハロウィーンの時に私がもらってきたものではありませんか？"},{"id":{"d":931,"n":7,"t":5},"speaker":"A","text":"その通りです。よく分かりましたね。"},{"id":{"d":931,"n":8,"t":5},"speaker":"B","text":"パイの模様がレシピの写真とそっくりだったので分かりました。"}];
INSERT INTO sentence [{"id":{"d":932,"n":1,"t":5},"speaker":"A","text":"おばあちゃん、一緒に塗り絵をしましょう。"},{"id":{"d":932,"n":2,"t":5},"speaker":"B","text":"いいですよ。どれを塗りますか？"},{"id":{"d":932,"n":3,"t":5},"speaker":"A","text":"奈々はこの女の子の絵を塗ります。"},{"id":{"d":932,"n":4,"t":5},"speaker":"B","text":"おばあちゃんはどれを塗りましょうか？"},{"id":{"d":932,"n":5,"t":5},"speaker":"A","text":"この本をどうぞ。"},{"id":{"d":932,"n":6,"t":5},"speaker":"B","text":"大人の塗り絵の本ですね。この本はどうしたのですか？"},{"id":{"d":932,"n":7,"t":5},"speaker":"A","text":"お母さんが「おばあちゃんに持っていきなさい」と言ったので、持ってきたものです。"},{"id":{"d":932,"n":8,"t":5},"speaker":"B","text":"あら、プレゼントなのですね。ありがとうございます。お母さんにも伝えておいてください。"}];
INSERT INTO sentence [{"id":{"d":933,"n":1,"t":5},"speaker":"A","text":"何を一生懸命に踊っているのですか？"},{"id":{"d":933,"n":2,"t":5},"speaker":"B","text":"アイドルのダンスです。"},{"id":{"d":933,"n":3,"t":5},"speaker":"A","text":"道理で見たことがあると思いました。流行していますよね。"},{"id":{"d":933,"n":4,"t":5},"speaker":"B","text":"はい。今、とても流行しています。"},{"id":{"d":933,"n":5,"t":5},"speaker":"A","text":"とても上手でしたけれど、どのようにして覚えたのですか？"},{"id":{"d":933,"n":6,"t":5},"speaker":"B","text":"動画を見て覚えました。明日、学校のみんなで踊ります。"},{"id":{"d":933,"n":7,"t":5},"speaker":"A","text":"昼休みにみんなで踊るのですか？"},{"id":{"d":933,"n":8,"t":5},"speaker":"B","text":"はい。お昼の校内放送でアイドル特集をするので、友だちと皆で踊る約束をしました。"}];
INSERT INTO sentence [{"id":{"d":934,"n":1,"t":5},"speaker":"A","text":"炎天下で草刈りをしたら倒れそうになりました。"},{"id":{"d":934,"n":2,"t":5},"speaker":"B","text":"ボランティアも良いですけれど、無理はしないでくださいね。"},{"id":{"d":934,"n":3,"t":5},"speaker":"A","text":"あの道は放っておくと草だらけになってしまうのですよ。"},{"id":{"d":934,"n":4,"t":5},"speaker":"B","text":"もっと熱中症対策をしなければいけませんね。"},{"id":{"d":934,"n":5,"t":5},"speaker":"A","text":"はい。今日は飲み物が少し足りませんでした。"},{"id":{"d":934,"n":6,"t":5},"speaker":"B","text":"帽子もキャップではなくて、首の後ろも隠れるものにした方がいいですよ。"},{"id":{"d":934,"n":7,"t":5},"speaker":"A","text":"そうですね、今日は反省点がたくさんあります。"},{"id":{"d":934,"n":8,"t":5},"speaker":"B","text":"来週は、塩飴や首の後ろを冷やす保冷材も用意しましょう。"}];
INSERT INTO sentence [{"id":{"d":935,"n":1,"t":5},"speaker":"A","text":"おばさん、アニメーションの映画を見に連れて行ってください。"},{"id":{"d":935,"n":2,"t":5},"speaker":"B","text":"お父さんは忙し過ぎて連れて行ってくれないのですね。"},{"id":{"d":935,"n":3,"t":5},"speaker":"A","text":"パパは日曜日もお仕事です。"},{"id":{"d":935,"n":4,"t":5},"speaker":"B","text":"分かりました。では、一緒にアニメーションの映画を見に行きましょう。"},{"id":{"d":935,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。かばんを持ってきます。"},{"id":{"d":935,"n":6,"t":5},"speaker":"B","text":"今すぐ行くのですか？まずは映画の時間を調べなくてはなりませんよ。"},{"id":{"d":935,"n":7,"t":5},"speaker":"A","text":"とりあえず支度ができました。いつでも出かけられます。"},{"id":{"d":935,"n":8,"t":5},"speaker":"B","text":"ちょうど今出れば、お昼前の回に間に合いますね。早速出かけましょう。"}];
INSERT INTO sentence [{"id":{"d":936,"n":1,"t":5},"speaker":"A","text":"お父さん、オケラとは何ですか？"},{"id":{"d":936,"n":2,"t":5},"speaker":"B","text":"一文無しのことですよ。"},{"id":{"d":936,"n":3,"t":5},"speaker":"A","text":"一文無しがどうして、ミミズとアメンボと一緒に生きているのですか？"},{"id":{"d":936,"n":4,"t":5},"speaker":"B","text":"ミミズとアメンボですか？もしかして『手のひらを太陽に』の歌詞に出てくるオケラですか？"},{"id":{"d":936,"n":5,"t":5},"speaker":"A","text":"そうです。一文無しはお金がない人のことでしょう？"},{"id":{"d":936,"n":6,"t":5},"speaker":"B","text":"なるほど。その歌詞のオケラは田んぼにいる虫のことですよ。"},{"id":{"d":936,"n":7,"t":5},"speaker":"A","text":"虫ですか。僕は見たことがありません。"},{"id":{"d":936,"n":8,"t":5},"speaker":"B","text":"今は少なくなってしまった虫なのです。その歌が作られたときは身近な虫だったのだと思います。"}];
INSERT INTO sentence [{"id":{"d":937,"n":1,"t":5},"speaker":"A","text":"今日は旗日ですね。"},{"id":{"d":937,"n":2,"t":5},"speaker":"B","text":"おばあちゃん、旗日とは何ですか？"},{"id":{"d":937,"n":3,"t":5},"speaker":"A","text":"美久ちゃんは旗日を知らないのですね。"},{"id":{"d":937,"n":4,"t":5},"speaker":"B","text":"初めて聞きました。今日は祝日でしょう？"},{"id":{"d":937,"n":5,"t":5},"speaker":"A","text":"はい、昔は祝日に国旗を飾っていた家が多かったのです。それもあって旗日と言ったのだと思います。"},{"id":{"d":937,"n":6,"t":5},"speaker":"B","text":"今は旗を飾っていませんよね？"},{"id":{"d":937,"n":7,"t":5},"speaker":"A","text":"はい、わが家ではいつからかやらなくなってしまいましたけれど、角の木田さんは今日も飾っていますよ。"},{"id":{"d":937,"n":8,"t":5},"speaker":"B","text":"そうなのですか？今から見てきます。"}];
INSERT INTO sentence [{"id":{"d":938,"n":1,"t":5},"speaker":"A","text":"冬でも花壇に花が咲いていますね。"},{"id":{"d":938,"n":2,"t":5},"speaker":"B","text":"これはクリスマスローズですよ。"},{"id":{"d":938,"n":3,"t":5},"speaker":"A","text":"これがバラなのですか？"},{"id":{"d":938,"n":4,"t":5},"speaker":"B","text":"いいえ。「ローズ」という名前が付いていますが、バラではありません。"},{"id":{"d":938,"n":5,"t":5},"speaker":"A","text":"そうなのですね。下を向いて咲いていて少し内気な感じがしますね。"},{"id":{"d":938,"n":6,"t":5},"speaker":"B","text":"そこがかわいいので、育てています。"},{"id":{"d":938,"n":7,"t":5},"speaker":"A","text":"寒い中で育てるのは難しいですか？"},{"id":{"d":938,"n":8,"t":5},"speaker":"B","text":"そうでもありませんよ。来年はチャレンジしてみてください。"}];
INSERT INTO sentence [{"id":{"d":939,"n":1,"t":5},"speaker":"A","text":"昨日は盛大に豆まきをしていたようですね。"},{"id":{"d":939,"n":2,"t":5},"speaker":"B","text":"大きな声で「鬼は外、福は内」と言っていたのが聞こえましたか？"},{"id":{"d":939,"n":3,"t":5},"speaker":"A","text":"はい。わが家の邪気まではらってもらえた気がして、よかったです。"},{"id":{"d":939,"n":4,"t":5},"speaker":"B","text":"お恥ずかしいです。"},{"id":{"d":939,"n":5,"t":5},"speaker":"A","text":"庭の境にラッカセイが落ちていましたが、豆まきでラッカセイを投げたのですか？"},{"id":{"d":939,"n":6,"t":5},"speaker":"B","text":"はい。故郷の北海道ではいつもラッカセイを投げます。"},{"id":{"d":939,"n":7,"t":5},"speaker":"A","text":"北海道の方は、大豆をまかないのですか？"},{"id":{"d":939,"n":8,"t":5},"speaker":"B","text":"大豆をまくところはあまりないかもしれませんね。ラッカセイの方が拾いやすいですし、後から食べやすいからだと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":940,"n":1,"t":5},"speaker":"A","text":"今度、お正月飾りを作る講習会があるそうです。"},{"id":{"d":940,"n":2,"t":5},"speaker":"B","text":"門松を作るのですか？"},{"id":{"d":940,"n":3,"t":5},"speaker":"A","text":"フラワーアレンジ教室でするそうですから、リースの形の飾りだと思いますよ。"},{"id":{"d":940,"n":4,"t":5},"speaker":"B","text":"私の家では、父が門松以外のお正月飾りを置かないのですよ。"},{"id":{"d":940,"n":5,"t":5},"speaker":"A","text":"そうですか。私はマンションなのでリースの方がいいです。"},{"id":{"d":940,"n":6,"t":5},"speaker":"B","text":"リースの大きさは選べるのでしょうか？"},{"id":{"d":940,"n":7,"t":5},"speaker":"A","text":"先生に相談すれば、好きな大きさで作ることができると思いますよ。"},{"id":{"d":940,"n":8,"t":5},"speaker":"B","text":"車用に小さいものが作れるのなら、参加したいです。"}];
INSERT INTO sentence [{"id":{"d":941,"n":1,"t":5},"speaker":"A","text":"初夢は「一富士、二鷹、三茄子」が良いと言いますね。"},{"id":{"d":941,"n":2,"t":5},"speaker":"B","text":"はい。初夢に出てくると縁起が良いとされるもののことですね。"},{"id":{"d":941,"n":3,"t":5},"speaker":"A","text":"その三つのいずれかが出てくる夢を見たことがありますか？"},{"id":{"d":941,"n":4,"t":5},"speaker":"B","text":"私はありません。あなたはありますか？"},{"id":{"d":941,"n":5,"t":5},"speaker":"A","text":"私もありません。運が向いていないということなのでしょうか？"},{"id":{"d":941,"n":6,"t":5},"speaker":"B","text":"そのようなことはありませんよ。"},{"id":{"d":941,"n":7,"t":5},"speaker":"A","text":"来年こそは縁起の良い夢を見て良い年にしたいです。"},{"id":{"d":941,"n":8,"t":5},"speaker":"B","text":"私もです。枕の下にお守りでも入れますかね。"}];
INSERT INTO sentence [{"id":{"d":942,"n":1,"t":5},"speaker":"A","text":"今からパーティーをしましょう。"},{"id":{"d":942,"n":2,"t":5},"speaker":"B","text":"帰ってきた途端にどうしたのですか？"},{"id":{"d":942,"n":3,"t":5},"speaker":"A","text":"デパートの地下の食品売り場をうろついていたら、お星さまのついたお菓子がたくさん売られていたのです。"},{"id":{"d":942,"n":4,"t":5},"speaker":"B","text":"そのお菓子を買ってきたのですか？"},{"id":{"d":942,"n":5,"t":5},"speaker":"A","text":"はい。その中から、飛び切りにかわいいものを買ってきました。"},{"id":{"d":942,"n":6,"t":5},"speaker":"B","text":"今日は何かパーティーをするような日だったでしょうか？"},{"id":{"d":942,"n":7,"t":5},"speaker":"A","text":"今日は七夕ですよ。"},{"id":{"d":942,"n":8,"t":5},"speaker":"B","text":"なるほど、だからお星さまなのですね。パーティーの準備を手伝います。"}];
INSERT INTO sentence [{"id":{"d":943,"n":1,"t":5},"speaker":"A","text":"お花屋さんでホオズキを買ってきました。"},{"id":{"d":943,"n":2,"t":5},"speaker":"B","text":"かわいい鉢植えですね。"},{"id":{"d":943,"n":3,"t":5},"speaker":"A","text":"これから実が赤くなっていくのを楽しめますよ。"},{"id":{"d":943,"n":4,"t":5},"speaker":"B","text":"来年は、ほおずき市に行きたいですね。"},{"id":{"d":943,"n":5,"t":5},"speaker":"A","text":"いいですね。以前ほおずき市に行った時には、近くにおいしい小倉アイスのお店がありましたよね。"},{"id":{"d":943,"n":6,"t":5},"speaker":"B","text":"はい。とてもおいしかったですね。また食べに行きたいです。"},{"id":{"d":943,"n":7,"t":5},"speaker":"A","text":"来年にでも行きましょう。"},{"id":{"d":943,"n":8,"t":5},"speaker":"B","text":"ほおずき市へ行く目的がホオズキかアイスクリームかよく分からなくなっていますが、ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":944,"n":1,"t":5},"speaker":"A","text":"「2.5次元俳優」とは何ですか？"},{"id":{"d":944,"n":2,"t":5},"speaker":"B","text":"漫画を舞台化した作品に出ている俳優のことをそう呼ぶのだそうです。"},{"id":{"d":944,"n":3,"t":5},"speaker":"A","text":"漫画が2次元で、現実が3次元で、舞台がその中間の2.5次元ということですか？"},{"id":{"d":944,"n":4,"t":5},"speaker":"B","text":"そういうことです。若者が多数出演する作品が多いので、2.5次元俳優には若手の俳優が多いですよ。"},{"id":{"d":944,"n":5,"t":5},"speaker":"A","text":"なるほど。私がテレビで見た方も若い方でした。"},{"id":{"d":944,"n":6,"t":5},"speaker":"B","text":"観客は若い子だらけだと思いますけれど、一度舞台を見てみたいですね。"},{"id":{"d":944,"n":7,"t":5},"speaker":"A","text":"そのようなことは気にせず、今度、行きましょうよ。"},{"id":{"d":944,"n":8,"t":5},"speaker":"B","text":"そうですね。チケットを探してみましょう。"}];
INSERT INTO sentence [{"id":{"d":945,"n":1,"t":5},"speaker":"A","text":"十五夜なのに曇っていますね。"},{"id":{"d":945,"n":2,"t":5},"speaker":"B","text":"今日はお月見ができないのでしょうか？"},{"id":{"d":945,"n":3,"t":5},"speaker":"A","text":"分かりません。天気予報を見ましたか？"},{"id":{"d":945,"n":4,"t":5},"speaker":"B","text":"いいえ。見ていません。"},{"id":{"d":945,"n":5,"t":5},"speaker":"A","text":"今、スマートフォンで見てみますね。雨は降らないようですよ。"},{"id":{"d":945,"n":6,"t":5},"speaker":"B","text":"では、厚い雲の切れ間から月が見られるのを期待するしかありませんね。"},{"id":{"d":945,"n":7,"t":5},"speaker":"A","text":"そうですね。一応、お団子の用意はしましたよ。"},{"id":{"d":945,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。いざとなったらパソコンで月の写真を表示してお月見をしましょう。"}];
INSERT INTO sentence [{"id":{"d":946,"n":1,"t":5},"speaker":"A","text":"タクシーで帰ってきたのですか？"},{"id":{"d":946,"n":2,"t":5},"speaker":"B","text":"はい。たくさん買い物をしたので、思わずタクシーに乗ってしまいました。"},{"id":{"d":946,"n":3,"t":5},"speaker":"A","text":"本当にたくさんの荷物ですね。"},{"id":{"d":946,"n":4,"t":5},"speaker":"B","text":"今日はお盆休みで息子一家が帰ってくるので、そのための準備です。"},{"id":{"d":946,"n":5,"t":5},"speaker":"A","text":"それは楽しみですね。"},{"id":{"d":946,"n":6,"t":5},"speaker":"B","text":"はい。孫が食べ盛りなので、食料品をたくさん買ってきました。"},{"id":{"d":946,"n":7,"t":5},"speaker":"A","text":"どのようなごちそうを作るのですか？"},{"id":{"d":946,"n":8,"t":5},"speaker":"B","text":"それほど大したものではありませんよ。息子の好物のから揚げと肉じゃがを作ります。"}];
INSERT INTO sentence [{"id":{"d":947,"n":1,"t":5},"speaker":"A","text":"誕生日のプレゼントは何がいいですか？"},{"id":{"d":947,"n":2,"t":5},"speaker":"B","text":"悩ましいです。お母さん、少し値段が高いものでもよいですか？"},{"id":{"d":947,"n":3,"t":5},"speaker":"A","text":"遠慮がちに言いますね。何ですか？"},{"id":{"d":947,"n":4,"t":5},"speaker":"B","text":"キーボードが欲しいです。"},{"id":{"d":947,"n":5,"t":5},"speaker":"A","text":"パソコンのキーボードが壊れたのですか？"},{"id":{"d":947,"n":6,"t":5},"speaker":"B","text":"違います。楽器のキーボードです。友達とバンド演奏をしたいのです。"},{"id":{"d":947,"n":7,"t":5},"speaker":"A","text":"いくらくらいするのですか？"},{"id":{"d":947,"n":8,"t":5},"speaker":"B","text":"ピンからキリまでありますけれど、僕が欲しいものは9万円くらいです。"}];
INSERT INTO sentence [{"id":{"d":948,"n":1,"t":5},"speaker":"A","text":"ぼた餅を作ったのでおすそ分けです。"},{"id":{"d":948,"n":2,"t":5},"speaker":"B","text":"これはおはぎではありませんか？"},{"id":{"d":948,"n":3,"t":5},"speaker":"A","text":"いいえ。春分の日に作るものは、ぼた餅というのですよ。"},{"id":{"d":948,"n":4,"t":5},"speaker":"B","text":"そうなのですか。知りませんでした。"},{"id":{"d":948,"n":5,"t":5},"speaker":"A","text":"秋はおはぎというのです。"},{"id":{"d":948,"n":6,"t":5},"speaker":"B","text":"同じものなのに名前が違うのですね。"},{"id":{"d":948,"n":7,"t":5},"speaker":"A","text":"細かく言えば、ぼた餅はこしあんで作るそうですが、私はこしあんが苦手なのです。"},{"id":{"d":948,"n":8,"t":5},"speaker":"B","text":"それでおはぎと全く同じものになったのですね。"}];
INSERT INTO sentence [{"id":{"d":949,"n":1,"t":5},"speaker":"A","text":"少し前までは、テレビ番組を録画して、それをビデオテープに焼いたものを、米国に住んでいる娘の家へ送っていました。"},{"id":{"d":949,"n":2,"t":5},"speaker":"B","text":"今は送っていないのですか？"},{"id":{"d":949,"n":3,"t":5},"speaker":"A","text":"はい。今はインターネットで大体見られます。便利な世の中になりました。"},{"id":{"d":949,"n":4,"t":5},"speaker":"B","text":"世界中のどこにいても見られるのでしょうか？"},{"id":{"d":949,"n":5,"t":5},"speaker":"A","text":"少なくとも、米国で日本のテレビ番組を見ることはできるようです。ビデオテープが要らない分、孫にお菓子や日本食や小物をいろいろと送っています。"},{"id":{"d":949,"n":6,"t":5},"speaker":"B","text":"すてきですね。娘さんもお孫さんも、楽しみに待っているでしょうね。"}];
INSERT INTO sentence [{"id":{"d":950,"n":1,"t":5},"speaker":"A","text":"やっと暖かくなってきたと思っていたのに、今日は寒いです。"},{"id":{"d":950,"n":2,"t":5},"speaker":"B","text":"寒の戻りですね。"},{"id":{"d":950,"n":3,"t":5},"speaker":"A","text":"またしばらくは寒いのでしょうか？"},{"id":{"d":950,"n":4,"t":5},"speaker":"B","text":"そのようです。天気予報ではあさってくらいまで寒いと言っていましたよ。"},{"id":{"d":950,"n":5,"t":5},"speaker":"A","text":"これだけ寒いと、桜の開花が遅れそうですね。"},{"id":{"d":950,"n":6,"t":5},"speaker":"B","text":"お花見の日を考え直さなくてはならないでしょうか？"},{"id":{"d":950,"n":7,"t":5},"speaker":"A","text":"そうかもしれません。3日くらい遅らせますか？"},{"id":{"d":950,"n":8,"t":5},"speaker":"B","text":"そうですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":951,"n":1,"t":5},"speaker":"A","text":"ヒロ君、今日は何のお稽古ですか？"},{"id":{"d":951,"n":2,"t":5},"speaker":"B","text":"ダンスのお稽古です。"},{"id":{"d":951,"n":3,"t":5},"speaker":"A","text":"ダンスを習っているのですか？"},{"id":{"d":951,"n":4,"t":5},"speaker":"B","text":"はい。身体が丈夫になるからと、お母さんが教室を探してきました。"},{"id":{"d":951,"n":5,"t":5},"speaker":"A","text":"ダンスは楽しいですか？"},{"id":{"d":951,"n":6,"t":5},"speaker":"B","text":"はい。お母さんに言われて入りましたが、今はとても楽しいです。"},{"id":{"d":951,"n":7,"t":5},"speaker":"A","text":"それは良かったですね。"},{"id":{"d":951,"n":8,"t":5},"speaker":"B","text":"今度、発表会がありますから、おばちゃんも見に来てください。"}];
INSERT INTO sentence [{"id":{"d":952,"n":1,"t":5},"speaker":"A","text":"加奈ちゃん、何の絵を描いているのですか？"},{"id":{"d":952,"n":2,"t":5},"speaker":"B","text":"アニメーションのキャラクターの絵です。"},{"id":{"d":952,"n":3,"t":5},"speaker":"A","text":"上手に描けていますね。"},{"id":{"d":952,"n":4,"t":5},"speaker":"B","text":"私がペンで絵を描いて、由奈がこれに色を塗ります。"},{"id":{"d":952,"n":5,"t":5},"speaker":"A","text":"由奈のために塗り絵を作ってあげているのですか？"},{"id":{"d":952,"n":6,"t":5},"speaker":"B","text":"はい、そうです。由奈の塗り絵もこの通りに上手ですよ。"},{"id":{"d":952,"n":7,"t":5},"speaker":"A","text":"本当ですね。きちんとお顔は白色で塗ってあります。"},{"id":{"d":952,"n":8,"t":5},"speaker":"B","text":"身体も赤くきれいに塗ってあります。とても上手ですよね。"}];
INSERT INTO sentence [{"id":{"d":953,"n":1,"t":5},"speaker":"A","text":"今日はいつもと違うキューピー人形を付けていますね。"},{"id":{"d":953,"n":2,"t":5},"speaker":"B","text":"昨日まで名古屋に行っていたので、そこで新しく買ってきました。"},{"id":{"d":953,"n":3,"t":5},"speaker":"A","text":"これは野球チームのマスコットですね。"},{"id":{"d":953,"n":4,"t":5},"speaker":"B","text":"そうです。野球チームのユニフォームを着たキューピーちゃんです。"},{"id":{"d":953,"n":5,"t":5},"speaker":"A","text":"日本中にいろいろなコスチューム姿のキューピー人形が売っているのですか？"},{"id":{"d":953,"n":6,"t":5},"speaker":"B","text":"そうですね。観光地ではどこでも売っていますよ。"},{"id":{"d":953,"n":7,"t":5},"speaker":"A","text":"たくさん持っているのですか？"},{"id":{"d":953,"n":8,"t":5},"speaker":"B","text":"はい。私はコレクターというほどではありませんけれど、地方に行くとつい買ってしまいます。"}];
INSERT INTO sentence [{"id":{"d":954,"n":1,"t":5},"speaker":"A","text":"この曲はどこかで聞いたことがあります。"},{"id":{"d":954,"n":2,"t":5},"speaker":"B","text":"私もどこかで聞いたことがあります。"},{"id":{"d":954,"n":3,"t":5},"speaker":"A","text":"何の曲か、思い出せませんか？"},{"id":{"d":954,"n":4,"t":5},"speaker":"B","text":"思い出せませんが、良いものを知っていますよ。そのようなときは鼻歌検索アプリケーションが便利です。"},{"id":{"d":954,"n":5,"t":5},"speaker":"A","text":"鼻歌で検索してくれるのですか？"},{"id":{"d":954,"n":6,"t":5},"speaker":"B","text":"そうです。スマートフォンに向かって歌ってみてください。"},{"id":{"d":954,"n":7,"t":5},"speaker":"A","text":"恥ずかしいですよ。"},{"id":{"d":954,"n":8,"t":5},"speaker":"B","text":"一緒に歌えば恥ずかしくありませんよ。さあ、歌いましょう。"}];
INSERT INTO sentence [{"id":{"d":955,"n":1,"t":5},"speaker":"A","text":"そろそろ梅雨入りですね。"},{"id":{"d":955,"n":2,"t":5},"speaker":"B","text":"そうですね。今年は早いのでしょうか？それとも遅いのでしょうか？"},{"id":{"d":955,"n":3,"t":5},"speaker":"A","text":"気象庁の予報によると、平年並みだそうです。"},{"id":{"d":955,"n":4,"t":5},"speaker":"B","text":"平年並みというと、梅雨入りはいつごろになるのですか？"},{"id":{"d":955,"n":5,"t":5},"speaker":"A","text":"6月初旬の7日ごろになりそうです。"},{"id":{"d":955,"n":6,"t":5},"speaker":"B","text":"あと1週間もありませんね。それで、梅雨明けはいつごろなのですか？"},{"id":{"d":955,"n":7,"t":5},"speaker":"A","text":"7月の19日ごろだそうです。"},{"id":{"d":955,"n":8,"t":5},"speaker":"B","text":"40日以上になるのですか。長いですね。"}];
INSERT INTO sentence [{"id":{"d":956,"n":1,"t":5},"speaker":"A","text":"『カメラを止めるな！』という映画を見たことがありますか？"},{"id":{"d":956,"n":2,"t":5},"speaker":"B","text":"いいえ、ありません。どのような映画ですか？"},{"id":{"d":956,"n":3,"t":5},"speaker":"A","text":"一種のホラー映画なのですが、面白かったです。"},{"id":{"d":956,"n":4,"t":5},"speaker":"B","text":"私はホラー映画があまり好きではありません。怖がりなのです。"},{"id":{"d":956,"n":5,"t":5},"speaker":"A","text":"その映画はホラー映画そのものではなくて、パロディーなのです。"},{"id":{"d":956,"n":6,"t":5},"speaker":"B","text":"それは興味深いですね。どのような映画なのですか？"},{"id":{"d":956,"n":7,"t":5},"speaker":"A","text":"ゾンビ映画を作ろうとしているチームのドタバタ劇とでも言うべき映画です。"},{"id":{"d":956,"n":8,"t":5},"speaker":"B","text":"それはなかなか面白そうですね。"},{"id":{"d":956,"n":9,"t":5},"speaker":"A","text":"大ヒットした映画で、最近、フランスでリメイクをしているらしいです。"},{"id":{"d":956,"n":10,"t":5},"speaker":"B","text":"今度、私も見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":957,"n":1,"t":5},"speaker":"A","text":"最近は、テレビで海外ドラマを見かけませんね。"},{"id":{"d":957,"n":2,"t":5},"speaker":"B","text":"そのようなことはありませんよ。BS放送では多くの海外ドラマを放送しています。"},{"id":{"d":957,"n":3,"t":5},"speaker":"A","text":"なるほど、私は地上波デジタル放送しか見ていないのでそのように感じたのですね。"},{"id":{"d":957,"n":4,"t":5},"speaker":"B","text":"確かに地上波デジタル放送では、今はほとんど海外ドラマを放送していませんね。"},{"id":{"d":957,"n":5,"t":5},"speaker":"A","text":"私の若い頃は、米国のテレビドラマがたくさん放送されていました。"},{"id":{"d":957,"n":6,"t":5},"speaker":"B","text":"例えばどのようなドラマですか？"},{"id":{"d":957,"n":7,"t":5},"speaker":"A","text":"『名犬ラッシー』や『ララミー牧場』など、上げればきりがありません。ほとんど毎日、どこかのテレビ局で放送されていたように思います。"},{"id":{"d":957,"n":8,"t":5},"speaker":"B","text":"そうですか。今はBS放送で韓国や中国のドラマが放送されていますよ。"},{"id":{"d":957,"n":9,"t":5},"speaker":"A","text":"そうなのですね。"},{"id":{"d":957,"n":10,"t":5},"speaker":"B","text":"一度ご覧になるといいですよ。なかなか面白いです。"},{"id":{"d":957,"n":11,"t":5},"speaker":"A","text":"そうですか。今度見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":958,"n":1,"t":5},"speaker":"A","text":"『ブラームスはお好き』を読んだことはありますか？"},{"id":{"d":958,"n":2,"t":5},"speaker":"B","text":"はい、フランソワーズ・サガンの小説ですね。映画も見ました。"},{"id":{"d":958,"n":3,"t":5},"speaker":"A","text":"映画は『さよならをもう一度』というタイトルでしたね。イブ・モンタンとイングリッド・バーグマンが主演していました。"},{"id":{"d":958,"n":4,"t":5},"speaker":"B","text":"それと、アンソニー・パーキンスも出ていました。"},{"id":{"d":958,"n":5,"t":5},"speaker":"A","text":"ブラームスの交響曲第3番の第3楽章が効果的でしたね。"},{"id":{"d":958,"n":6,"t":5},"speaker":"B","text":"あの映画にしては少しメランコリック過ぎる感じもしましたが。"},{"id":{"d":958,"n":7,"t":5},"speaker":"A","text":"確かにそのような感じがしないでもありませんね。"},{"id":{"d":958,"n":8,"t":5},"speaker":"B","text":"でも、良い映画で良い音楽でした。"},{"id":{"d":958,"n":9,"t":5},"speaker":"A","text":"同感です。"}];
INSERT INTO sentence [{"id":{"d":959,"n":1,"t":5},"speaker":"A","text":"エイプリルフールの由来を知っていますか？"},{"id":{"d":959,"n":2,"t":5},"speaker":"B","text":"いいえ、知りません。教えてください。"},{"id":{"d":959,"n":3,"t":5},"speaker":"A","text":"国によっていろいろな説があるようですが、例えば、フランスでは、国王が年の始まりを1月1日にすると宣言したことに国民が反発して、それまで新年だった4月1日を「うその新年」と言って騒動を起こしたのが始まりだったと言われています。"},{"id":{"d":959,"n":4,"t":5},"speaker":"B","text":"面白いですね。他にはどのような説がありますか？"},{"id":{"d":959,"n":5,"t":5},"speaker":"A","text":"インドでは、3月末まで苦しい修行をしていた仏教の修行僧が4月1日になるとすぐに俗人に戻るとからかわれていたのが起源だと言われています。"},{"id":{"d":959,"n":6,"t":5},"speaker":"B","text":"日本ではいつごろに始まったのですか？"},{"id":{"d":959,"n":7,"t":5},"speaker":"A","text":"江戸時代には4月1日が「不義理の日」といって、義理を欠いている人にわびる日だったそうです。それが後に「うそをついてもいい日」になったそうです。"},{"id":{"d":959,"n":8,"t":5},"speaker":"B","text":"どの説も面白いですね。私たちも、何か面白いうそを考えましょうか？"}];
INSERT INTO sentence [{"id":{"d":960,"n":1,"t":5},"speaker":"A","text":"カメラを買おうと思っているのですが、どのメーカーが良いでしょうか？"},{"id":{"d":960,"n":2,"t":5},"speaker":"B","text":"日本のメーカーは性能の点ではどこも遜色ありません。あとは、好みの問題だと思います。"},{"id":{"d":960,"n":3,"t":5},"speaker":"A","text":"それぞれのメーカーの特色があれば教えてくれませんか？"},{"id":{"d":960,"n":4,"t":5},"speaker":"B","text":"いいですよ。例えば、日本カメラは鮮やかな発色が特徴だといわれています。"},{"id":{"d":960,"n":5,"t":5},"speaker":"A","text":"私はどちらかというと落ち着いた感じの写真が好きです。"},{"id":{"d":960,"n":6,"t":5},"speaker":"B","text":"それなら富士光学の方がいいかもしれませんね。ナチュラルな落ち着いた発色を得意としています。"},{"id":{"d":960,"n":7,"t":5},"speaker":"A","text":"そのように聞くと、気持ちが動きますね。横浜写真社はどうでしょうか？"},{"id":{"d":960,"n":8,"t":5},"speaker":"B","text":"横浜写真社は技術の進化が目覚ましく、絶えず性能が向上しています。"},{"id":{"d":960,"n":9,"t":5},"speaker":"A","text":"そうですか。でも、私はそれほど技術にこだわりません。他にはどのようなメーカーがありますか？"},{"id":{"d":960,"n":10,"t":5},"speaker":"B","text":"アルファフィルムはあまり機種は多くありませんが、フィルムカメラ時代の伝統を引き継いで、独自の色表現を追求しています。"},{"id":{"d":960,"n":11,"t":5},"speaker":"A","text":"それも魅力がありますね。いろいろと教えてくださってありがとうございます。参考にして選びたいと思います。"}];
INSERT INTO sentence [{"id":{"d":961,"n":1,"t":5},"speaker":"A","text":"久しぶりに良い天気になりましたね。"},{"id":{"d":961,"n":2,"t":5},"speaker":"B","text":"そうですね。この一週間はずっと雨が降っていたので、憂鬱でした。"},{"id":{"d":961,"n":3,"t":5},"speaker":"A","text":"気晴らしにどこかへドライブに行きませんか？"},{"id":{"d":961,"n":4,"t":5},"speaker":"B","text":"賛成です。どこに行きましょうか？"},{"id":{"d":961,"n":5,"t":5},"speaker":"A","text":"伊豆はどうでしょう？スカイラインを走るのはとても気持ちが良いですよ。"},{"id":{"d":961,"n":6,"t":5},"speaker":"B","text":"それはいいですね。伊豆高原で何かおいしいものを食べましょう。"},{"id":{"d":961,"n":7,"t":5},"speaker":"A","text":"下田あたりまで足を延ばしてもいいですね。"},{"id":{"d":961,"n":8,"t":5},"speaker":"B","text":"いいですね。早速出発しましょう。"}];
INSERT INTO sentence [{"id":{"d":962,"n":1,"t":5},"speaker":"A","text":"昔は名曲喫茶というものがあって、よく通ったものです。"},{"id":{"d":962,"n":2,"t":5},"speaker":"B","text":"確か渋谷にありましたね。テレビで放送していました。とても大きなスピーカーがありました。"},{"id":{"d":962,"n":3,"t":5},"speaker":"A","text":"それは、タイガーという喫茶店ですね。今でも残っている数少ない名曲喫茶の一つです。私は一度だけ行ったことがあります。"},{"id":{"d":962,"n":4,"t":5},"speaker":"B","text":"あなたが通っていた喫茶店はどこにあったのですか？"},{"id":{"d":962,"n":5,"t":5},"speaker":"A","text":"吉祥寺にコンチェルトという喫茶店があって、そこに通っていました。"},{"id":{"d":962,"n":6,"t":5},"speaker":"B","text":"タイガーのように大きなスピーカーがあったのですか？"},{"id":{"d":962,"n":7,"t":5},"speaker":"A","text":"いいえ、小さな喫茶店で、スピーカーも小さかったのですが性能の良いものでした。"},{"id":{"d":962,"n":8,"t":5},"speaker":"B","text":"もちろん、クラシックの曲でしょう？"},{"id":{"d":962,"n":9,"t":5},"speaker":"A","text":"そうです。客がリクエストしたものをかけるのです。皆、耳を傾けて静かに聴いていました。"},{"id":{"d":962,"n":10,"t":5},"speaker":"B","text":"あなたはどのような曲をリクエストしたのですか？"},{"id":{"d":962,"n":11,"t":5},"speaker":"A","text":"私はあまり詳しくなかったので、他の人がリクエストした曲を聴いていました。それでいろいろな作曲家のいろいろな曲を知ったのです。"},{"id":{"d":962,"n":12,"t":5},"speaker":"B","text":"古き良き時代ですね。"}];
INSERT INTO sentence [{"id":{"d":963,"n":1,"t":5},"speaker":"A","text":"熱帯夜が続いていて、最近はあまりよく眠れていません。"},{"id":{"d":963,"n":2,"t":5},"speaker":"B","text":"連日、30度近い温度ですからね。クーラーはつけていないのですか？"},{"id":{"d":963,"n":3,"t":5},"speaker":"A","text":"はい。つけっ放しにして眠ると、朝起きたときにぐったりしてしまうものですから、つけていません。"},{"id":{"d":963,"n":4,"t":5},"speaker":"B","text":"体温を下げるとよく眠れるようになりますよ。"},{"id":{"d":963,"n":5,"t":5},"speaker":"A","text":"とはいえ、何も掛けずに寝ると風邪をひいてしまいますし、どうすればよいのでしょうか？"},{"id":{"d":963,"n":6,"t":5},"speaker":"B","text":"保冷剤を使って頭を冷やすといいですよ。体温が下がると眠りやすくなります。"},{"id":{"d":963,"n":7,"t":5},"speaker":"A","text":"試してみます。他に、何か良い方法はありますか？"},{"id":{"d":963,"n":8,"t":5},"speaker":"B","text":"麻の寝具ですと放熱や放湿がうまくいきますので、体温が下がります。"},{"id":{"d":963,"n":9,"t":5},"speaker":"A","text":"なるほど、早速試してみます。"}];
INSERT INTO sentence [{"id":{"d":964,"n":1,"t":5},"speaker":"A","text":"正月に一般参賀に行ってきました。"},{"id":{"d":964,"n":2,"t":5},"speaker":"B","text":"そうですか。私はテレビで見たことはありますが、行ったことはありません。"},{"id":{"d":964,"n":3,"t":5},"speaker":"A","text":"皇居前で整列して、警察の誘導に従って正門から入るのですが、大勢の人がいて長い列ができていました。"},{"id":{"d":964,"n":4,"t":5},"speaker":"B","text":"長和殿というのでしたか、その前の広場に大勢の人がひしめいて日の丸の旗を振っていますよね。"},{"id":{"d":964,"n":5,"t":5},"speaker":"A","text":"私も日の丸の旗を振りましたよ。しばらくすると、天皇ご一家がベランダに現れて、天皇陛下がお言葉を述べられました。"},{"id":{"d":964,"n":6,"t":5},"speaker":"B","text":"どのような気分がするのですか？"},{"id":{"d":964,"n":7,"t":5},"speaker":"A","text":"厳粛な気持ちになるのかと思っていましたが、意外とそうでもありませんでしたね。"},{"id":{"d":964,"n":8,"t":5},"speaker":"B","text":"昔とは違ってきているのでしょうか？"},{"id":{"d":964,"n":9,"t":5},"speaker":"A","text":"昔のことは知りませんが、皆さんは割と自然な雰囲気でしたね。"}];
INSERT INTO sentence [{"id":{"d":965,"n":1,"t":5},"speaker":"A","text":"梅雨が明けたら、どこかの海に行きましょう。"},{"id":{"d":965,"n":2,"t":5},"speaker":"B","text":"いいですね。どこに行きましょうか？"},{"id":{"d":965,"n":3,"t":5},"speaker":"A","text":"湘南の海がいいです。サーフィンをしたいのです。"},{"id":{"d":965,"n":4,"t":5},"speaker":"B","text":"湘南ですか。それもいいですけれど、道路が混みますよ。海岸通りなどは渋滞がひどくて、自動車がなかなか前に進みません。"},{"id":{"d":965,"n":5,"t":5},"speaker":"A","text":"湘南だけではなく、この時期はどこも混みますよ。朝早く出かければ大丈夫でしょう。"},{"id":{"d":965,"n":6,"t":5},"speaker":"B","text":"それもそうですね。それでは湘南にしましょう。"}];
INSERT INTO sentence [{"id":{"d":966,"n":1,"t":5},"speaker":"A","text":"クリスマスの時期は、イルミネーションが楽しみです。"},{"id":{"d":966,"n":2,"t":5},"speaker":"B","text":"そうですね。いろいろなところで大掛かりなイルミネーションが行われますね。"},{"id":{"d":966,"n":3,"t":5},"speaker":"A","text":"恵比寿ガーデンプレイスの大シャンデリアは有名ですね。世界最大級のシャンデリアだそうです。"},{"id":{"d":966,"n":4,"t":5},"speaker":"B","text":"お台場の生木を使った台場メモリアルツリーも人気があります。高さが20メートル、幅が10メートルあるそうです。"},{"id":{"d":966,"n":5,"t":5},"speaker":"A","text":"多摩センターのイルミネーションは約40万球あるそうですから、壮観でしょうね。"},{"id":{"d":966,"n":6,"t":5},"speaker":"B","text":"どれもすばらしいですね。毎年、家族で見に行くことにしていますが、今年は東京スカイツリーのイルミネーションを見るつもりです。"}];
INSERT INTO sentence [{"id":{"d":967,"n":1,"t":5},"speaker":"A","text":"雲行きが怪しくなってきましたね。"},{"id":{"d":967,"n":2,"t":5},"speaker":"B","text":"これは、ひと雨来そうです。"},{"id":{"d":967,"n":3,"t":5},"speaker":"A","text":"どうしましょうか？このまま登り続けますか？"},{"id":{"d":967,"n":4,"t":5},"speaker":"B","text":"山の天気は変わりやすいですから、あまり無理をしない方がいいかもしれません。"},{"id":{"d":967,"n":5,"t":5},"speaker":"A","text":"もう少しで頂上なのに、残念ですね。"},{"id":{"d":967,"n":6,"t":5},"speaker":"B","text":"そうですね。でも、ここは思い切って引き返す方がいいでしょう。"},{"id":{"d":967,"n":7,"t":5},"speaker":"A","text":"麓までどのくらいかかるでしょうか？"},{"id":{"d":967,"n":8,"t":5},"speaker":"B","text":"3時間もあれば下りられるでしょう。"},{"id":{"d":967,"n":9,"t":5},"speaker":"A","text":"それでは、麓の温泉にでも入ってゆっくり休みましょう。"}];
INSERT INTO sentence [{"id":{"d":968,"n":1,"t":5},"speaker":"A","text":"敬老の日におじいさんに何を贈りましょうか？"},{"id":{"d":968,"n":2,"t":5},"speaker":"B","text":"最近、おじいさんは盆栽に凝っていますから、盆栽が良いのではありませんか？"},{"id":{"d":968,"n":3,"t":5},"speaker":"A","text":"そういえば、おじいさんは桜の盆栽が欲しいと言っていました。"},{"id":{"d":968,"n":4,"t":5},"speaker":"B","text":"インターネットで調べたのですが、富士吉田市に盆栽を扱っている園芸店があります。"},{"id":{"d":968,"n":5,"t":5},"speaker":"A","text":"そこに桜の盆栽がありますか？"},{"id":{"d":968,"n":6,"t":5},"speaker":"B","text":"はい、ウェブサイトを見ると、富士桜の盆栽を扱っているようです。"},{"id":{"d":968,"n":7,"t":5},"speaker":"A","text":"それは良かったですね。では次の日曜日に行きましょう。"},{"id":{"d":968,"n":8,"t":5},"speaker":"B","text":"ついでに山梨名物のほうとうでも食べましょう。"}];
INSERT INTO sentence [{"id":{"d":969,"n":1,"t":5},"speaker":"A","text":"サッカーの日本対ブラジル戦を見ましたか？"},{"id":{"d":969,"n":2,"t":5},"speaker":"B","text":"いいえ、見ていません。どうでしたか？"},{"id":{"d":969,"n":3,"t":5},"speaker":"A","text":"3対0で負けました。完敗です。"},{"id":{"d":969,"n":4,"t":5},"speaker":"B","text":"無得点だったのですね。"},{"id":{"d":969,"n":5,"t":5},"speaker":"A","text":"ペナルティーキックを与えてしまったのが駄目でした。"},{"id":{"d":969,"n":6,"t":5},"speaker":"B","text":"ブラジル選手のスピードを抑えるにはファウルで止めるしかなかったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":970,"n":1,"t":5},"speaker":"A","text":"娘にぬいぐるみを買いたいのですが、今一番人気があるぬいぐるみは何ですか？"},{"id":{"d":970,"n":2,"t":5},"speaker":"B","text":"やはりテディベアですね。小さいお子様から大人まで人気があります。"},{"id":{"d":970,"n":3,"t":5},"speaker":"A","text":"いくつか見せてください。"},{"id":{"d":970,"n":4,"t":5},"speaker":"B","text":"かしこまりました。これなどはいかがでしょうか？"},{"id":{"d":970,"n":5,"t":5},"speaker":"A","text":"随分と大きいですね。娘よりも大きいくらいです。"},{"id":{"d":970,"n":6,"t":5},"speaker":"B","text":"今は、このくらい大きなテディベアが人気です。"},{"id":{"d":970,"n":7,"t":5},"speaker":"A","text":"確かに、抱いて寝ると安心できそうですね。これにしましょう。"},{"id":{"d":970,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。ただ今、お包みいたします。"}];
INSERT INTO sentence [{"id":{"d":971,"n":1,"t":5},"speaker":"A","text":"真夏日は知っていましたが、真冬日というものがあることは知りませんでした。"},{"id":{"d":971,"n":2,"t":5},"speaker":"B","text":"それは私も知りませんでした。どのように定義されているのですか？"},{"id":{"d":971,"n":3,"t":5},"speaker":"A","text":"最高気温が0度未満の日を真冬日というのだそうです。"},{"id":{"d":971,"n":4,"t":5},"speaker":"B","text":"この辺りではそのような日はめったにありませんね。"},{"id":{"d":971,"n":5,"t":5},"speaker":"A","text":"北国や山間部では、1月下旬から2月上旬ごろに、真冬日になることがあります。"},{"id":{"d":971,"n":6,"t":5},"speaker":"B","text":"真冬日があるのなら冬日というものもあるのでしょうね。"},{"id":{"d":971,"n":7,"t":5},"speaker":"A","text":"はい、最低気温が0度未満の日を冬日といいます。"},{"id":{"d":971,"n":8,"t":5},"speaker":"B","text":"それなら、この辺りでもたまにあります。"}];
INSERT INTO sentence [{"id":{"d":972,"n":1,"t":5},"speaker":"A","text":"十五夜にお月見をしましょう。"},{"id":{"d":972,"n":2,"t":5},"speaker":"B","text":"今年の十五夜はいつですか？"},{"id":{"d":972,"n":3,"t":5},"speaker":"A","text":"今年は9月10日だそうです。"},{"id":{"d":972,"n":4,"t":5},"speaker":"B","text":"土曜日ですね。天気はどうでしょうか？"},{"id":{"d":972,"n":5,"t":5},"speaker":"A","text":"天気予報では晴れると言っていました。"},{"id":{"d":972,"n":6,"t":5},"speaker":"B","text":"それなら、きれいな満月を見ることができそうですね。"},{"id":{"d":972,"n":7,"t":5},"speaker":"A","text":"はい。そういえば、先日、スーパーマーケットでススキと月見団子とクリのセットが売られていました。"},{"id":{"d":972,"n":8,"t":5},"speaker":"B","text":"お月見セットですね。それでは、今日の買い物でそれを買ってきましょう。"}];
INSERT INTO sentence [{"id":{"d":973,"n":1,"t":5},"speaker":"A","text":"あなたは、毎日欠かさずに日記をつけているのですか？"},{"id":{"d":973,"n":2,"t":5},"speaker":"B","text":"毎日とはいきませんが、できるだけ書いています。書かないと気持ちが悪いのです。"},{"id":{"d":973,"n":3,"t":5},"speaker":"A","text":"いつからつけているのですか？"},{"id":{"d":973,"n":4,"t":5},"speaker":"B","text":"子どもの頃からつけています。1年で大学ノート1冊分ですから、もう30冊になります。"},{"id":{"d":973,"n":5,"t":5},"speaker":"A","text":"よく続きますね。私も日記をつけたいと思うのですが、なかなか続きません。続けるこつはありますか？"},{"id":{"d":973,"n":6,"t":5},"speaker":"B","text":"こつというようなものはありません。ただ、10分程度で書けるくらいに、ごく簡単に書きます。"},{"id":{"d":973,"n":7,"t":5},"speaker":"A","text":"なるほど。日記は、やはり昔のことを思い出すきっかけになりますか？"},{"id":{"d":973,"n":8,"t":5},"speaker":"B","text":"はい、日記を読むと、すっかり忘れていたことがよみがえってきます。"},{"id":{"d":973,"n":9,"t":5},"speaker":"A","text":"良いですね。私も諦めずに書いておけばよかったです。"},{"id":{"d":973,"n":10,"t":5},"speaker":"B","text":"今からでも遅くありません。今日から始めるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":974,"n":1,"t":5},"speaker":"A","text":"中込環さんはヒット曲がなかなか出ませんね。"},{"id":{"d":974,"n":2,"t":5},"speaker":"B","text":"10年前に10万枚を売るヒット曲が出ましたけれど、その後は鳴かず飛ばずです。"},{"id":{"d":974,"n":3,"t":5},"speaker":"A","text":"歌唱力のある歌い手なのですが、良い曲に恵まれません。"},{"id":{"d":974,"n":4,"t":5},"speaker":"B","text":"作詞家と作曲家を変えてみてはどうでしょうか？"},{"id":{"d":974,"n":5,"t":5},"speaker":"A","text":"そうですね。誰か当てはありますか？"},{"id":{"d":974,"n":6,"t":5},"speaker":"B","text":"作詞を松山尋さんに頼んだらどうでしょう。中込さんの持つ情感をうまく引き出せると思います。"},{"id":{"d":974,"n":7,"t":5},"speaker":"A","text":"作曲家は誰が良いでしょうね。"},{"id":{"d":974,"n":8,"t":5},"speaker":"B","text":"藤本タローさんが良いと思います。中込さんの声質に合う曲を書いてくれると思います。"},{"id":{"d":974,"n":9,"t":5},"speaker":"A","text":"それでは、お二人に相談してみましょう。"}];
INSERT INTO sentence [{"id":{"d":975,"n":1,"t":5},"speaker":"A","text":"海の日というものがあるとは知りませんでした。"},{"id":{"d":975,"n":2,"t":5},"speaker":"B","text":"海の日は祝日でもありますよ。"},{"id":{"d":975,"n":3,"t":5},"speaker":"A","text":"何月何日ですか？"},{"id":{"d":975,"n":4,"t":5},"speaker":"B","text":"7月の第3月曜日です。"},{"id":{"d":975,"n":5,"t":5},"speaker":"A","text":"確かに、会社も休みになっています。でも、それが海の日だとは少しも気が付きませんでした。"},{"id":{"d":975,"n":6,"t":5},"speaker":"B","text":"平成8年に制定されましたから、もう20年以上になるのですよ。"},{"id":{"d":975,"n":7,"t":5},"speaker":"A","text":"それほど早くに始まったのですか。海の日があるなら、山の日もありますか？"},{"id":{"d":975,"n":8,"t":5},"speaker":"B","text":"はい、山の日もあります。これは8月11日です。"},{"id":{"d":975,"n":9,"t":5},"speaker":"A","text":"海の日と同時に制定されたのですか？"},{"id":{"d":975,"n":10,"t":5},"speaker":"B","text":"いいえ、山の日は平成28年に制定されました。"},{"id":{"d":975,"n":11,"t":5},"speaker":"A","text":"最近ですね。それも知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":976,"n":1,"t":5},"speaker":"A","text":"昨日はひょうが降りましたね。あれほど大きなひょうを見たのは初めてです。"},{"id":{"d":976,"n":2,"t":5},"speaker":"B","text":"空が暗くなったと思ったら、突然大粒のひょうが降ってきて、とても驚きました。"},{"id":{"d":976,"n":3,"t":5},"speaker":"A","text":"私はちょうど自動車の中にいたのですが、屋根に当たる音がそれはすさまじかったです。"},{"id":{"d":976,"n":4,"t":5},"speaker":"B","text":"私は外を歩いていました。直撃されたらたまらないと思って、慌ててビルの中に駆け込みました。"},{"id":{"d":976,"n":5,"t":5},"speaker":"A","text":"ニュースを見たら、ゴルフボールくらいの大きさのひょうが降ったところもあったそうです。"},{"id":{"d":976,"n":6,"t":5},"speaker":"B","text":"それは大変ですね。温室ハウスのガラスが割れて、農作物に被害が出たところもあったそうですね。"},{"id":{"d":976,"n":7,"t":5},"speaker":"A","text":"それは困りますね。最近は異常気象が増えてきたような気がします。"},{"id":{"d":976,"n":8,"t":5},"speaker":"B","text":"そうですね。ここにも地球温暖化の影響が出てきているのでしょうか。"}];
INSERT INTO sentence [{"id":{"d":977,"n":1,"t":5},"speaker":"A","text":"広場にやぐらが組み立てられていますね。何のためのやぐらですか？"},{"id":{"d":977,"n":2,"t":5},"speaker":"B","text":"明日は、盆踊りがあるのです。そのためのやぐらです。"},{"id":{"d":977,"n":3,"t":5},"speaker":"A","text":"そういえば、旅行ガイドに盆踊りのことが載っていました。実際に見るのは初めてです。"},{"id":{"d":977,"n":4,"t":5},"speaker":"B","text":"良い機会ですから、あなたも参加してみてはどうですか？"},{"id":{"d":977,"n":5,"t":5},"speaker":"A","text":"外国人の私でも参加できるのですか？"},{"id":{"d":977,"n":6,"t":5},"speaker":"B","text":"もちろんです。国籍は関係ありません。外国の方もよく参加して踊っていますよ。"},{"id":{"d":977,"n":7,"t":5},"speaker":"A","text":"浴衣というものを着るのでしょう？"},{"id":{"d":977,"n":8,"t":5},"speaker":"B","text":"はい。そのままの服でもよいですが、レンタルの浴衣もあります。"},{"id":{"d":977,"n":9,"t":5},"speaker":"A","text":"せっかくですから、私も浴衣を着てみたいです。"}];
INSERT INTO sentence [{"id":{"d":978,"n":1,"t":5},"speaker":"A","text":"今年は初詣はどうしますか？"},{"id":{"d":978,"n":2,"t":5},"speaker":"B","text":"それは行かなければいけませんが、三が日に行くのは考えものですね。"},{"id":{"d":978,"n":3,"t":5},"speaker":"A","text":"去年は大勢の参拝客で大変な目に遭いましたからね。"},{"id":{"d":978,"n":4,"t":5},"speaker":"B","text":"ええ。本殿にたどり着くまでに3時間近くかかりました。"},{"id":{"d":978,"n":5,"t":5},"speaker":"A","text":"あのようなことは二度とごめんです。すっかりくたびれてしまいました。"},{"id":{"d":978,"n":6,"t":5},"speaker":"B","text":"三が日を過ぎてからなら、もう少し空いているのではないでしょうか？"},{"id":{"d":978,"n":7,"t":5},"speaker":"A","text":"そうですね。それでは、5日に行くことにしましょう。"},{"id":{"d":978,"n":8,"t":5},"speaker":"B","text":"5日は何曜日ですか？"},{"id":{"d":978,"n":9,"t":5},"speaker":"A","text":"日曜日です。"},{"id":{"d":978,"n":10,"t":5},"speaker":"B","text":"日曜日も混むのではないでしょうか？月曜日にしましょう。"},{"id":{"d":978,"n":11,"t":5},"speaker":"A","text":"それでは、6日月曜日に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":979,"n":1,"t":5},"speaker":"A","text":"今年の夏は暑そうですね。"},{"id":{"d":979,"n":2,"t":5},"speaker":"B","text":"そうですね。6月だというのに、もう35度を超えるところが出ています。"},{"id":{"d":979,"n":3,"t":5},"speaker":"A","text":"昨日は熊谷で38度、甲府で36度だったそうですよ。"},{"id":{"d":979,"n":4,"t":5},"speaker":"B","text":"どちらも内陸の盆地ですから、夏は暑くて冬は寒いでしょうね。"},{"id":{"d":979,"n":5,"t":5},"speaker":"A","text":"甲府は盆地ですが、熊谷は盆地ではありませんよ。"},{"id":{"d":979,"n":6,"t":5},"speaker":"B","text":"そうなのですか？では、どうして熊谷は暑いのでしょうか？"},{"id":{"d":979,"n":7,"t":5},"speaker":"A","text":"二つの説があると言われています。一つはフェーン現象、もう一つはヒートアイランド現象によるものです。"},{"id":{"d":979,"n":8,"t":5},"speaker":"B","text":"熊谷はそれほど大きな都市でもないのに、ヒートアイランド現象が起こるのですか？"},{"id":{"d":979,"n":9,"t":5},"speaker":"A","text":"東京の都心から流れてくる熱風が熊谷のあたりで滞留するのだそうです。"},{"id":{"d":979,"n":10,"t":5},"speaker":"B","text":"そうだったのですか。それは知りませんでした。"}];
INSERT INTO sentence [{"id":{"d":980,"n":1,"t":5},"speaker":"A","text":"除夜の鐘を聞くと、しみじみとした気持ちになります。"},{"id":{"d":980,"n":2,"t":5},"speaker":"B","text":"そうですね。私は毎年、テレビで「ゆく年くる年」を見ながら除夜の鐘を聞いています。"},{"id":{"d":980,"n":3,"t":5},"speaker":"A","text":"全国の除夜の鐘を聞くことができますね。それも感慨深いものがあります。"},{"id":{"d":980,"n":4,"t":5},"speaker":"B","text":"ところが、最近は必ずしもそうではなくなっているそうです。"},{"id":{"d":980,"n":5,"t":5},"speaker":"A","text":"どういうことですか？"},{"id":{"d":980,"n":6,"t":5},"speaker":"B","text":"除夜の鐘を騒音だと感じる人が増えているのだそうです。"},{"id":{"d":980,"n":7,"t":5},"speaker":"A","text":"そういえば、騒音に関するトラブルは年々増えているそうですね。"},{"id":{"d":980,"n":8,"t":5},"speaker":"B","text":"お寺によっては鐘をつくこと自体を中止したり、鐘をつく時間を昼間にずらしたりしているところもあるそうです。"},{"id":{"d":980,"n":9,"t":5},"speaker":"A","text":"そうなのですか。昔からの伝統が消えてゆくのは寂しいですね。"}];
INSERT INTO sentence [{"id":{"d":981,"n":1,"t":5},"speaker":"A","text":"父が定年退職をしてギターを始めました。"},{"id":{"d":981,"n":2,"t":5},"speaker":"B","text":"それは良いことですね。"},{"id":{"d":981,"n":3,"t":5},"speaker":"A","text":"父は大学時代にロックバンドを組んでいたのです。"},{"id":{"d":981,"n":4,"t":5},"speaker":"B","text":"そうだったのですか。"},{"id":{"d":981,"n":5,"t":5},"speaker":"A","text":"就職を機にバンドも解散して、その後は仕事一筋でした。"},{"id":{"d":981,"n":6,"t":5},"speaker":"B","text":"退職を機にまた若い頃の趣味に戻ったわけですね。"},{"id":{"d":981,"n":7,"t":5},"speaker":"A","text":"はい。昔のバンド仲間にも声を掛けて、また新しいバンドを組むそうです。バンド名はオールド・ボーイズです。"},{"id":{"d":981,"n":8,"t":5},"speaker":"B","text":"すてきですね。コンサートをすることがあったら、ぜひ聞きに行きます。"}];
INSERT INTO sentence [{"id":{"d":982,"n":1,"t":5},"speaker":"A","text":"あちらにとても目立つスポーツカーが止まっていますね。"},{"id":{"d":982,"n":2,"t":5},"speaker":"B","text":"あのエンブレムはマーキュリーですね。"},{"id":{"d":982,"n":3,"t":5},"speaker":"A","text":"あまり見かけない車ですね。"},{"id":{"d":982,"n":4,"t":5},"speaker":"B","text":"米国の車です。日本にはあまり輸入されていません。"},{"id":{"d":982,"n":5,"t":5},"speaker":"A","text":"フロントが精強な感じがしますね。"},{"id":{"d":982,"n":6,"t":5},"speaker":"B","text":"そうですね。迫力があります。"},{"id":{"d":982,"n":7,"t":5},"speaker":"A","text":"でも、ダッシュボードの上にはぬいぐるみがたくさん置いてありますよ。"},{"id":{"d":982,"n":8,"t":5},"speaker":"B","text":"本当ですね。車の外観とは正反対のかわいらしい車内ですね。"}];
INSERT INTO sentence [{"id":{"d":983,"n":1,"t":5},"speaker":"A","text":"ホラー小説と言えばスティーブン・キングですね。"},{"id":{"d":983,"n":2,"t":5},"speaker":"B","text":"彼は「ホラーの帝王」といわれていますね。私もよく読みました。"},{"id":{"d":983,"n":3,"t":5},"speaker":"A","text":"キングの小説の中で何が一番好きですか？"},{"id":{"d":983,"n":4,"t":5},"speaker":"B","text":"最初に読んだのが『シャイニング』で、今でもそれが一番好きです。"},{"id":{"d":983,"n":5,"t":5},"speaker":"A","text":"映画にもなりましたね。ジャック・ニコルソンが狂気に陥る主人公を見事に演じていました。"},{"id":{"d":983,"n":6,"t":5},"speaker":"B","text":"あなたはどの小説が一番好きですか？"},{"id":{"d":983,"n":7,"t":5},"speaker":"A","text":"私は『グリーンマイル』が好きです。"},{"id":{"d":983,"n":8,"t":5},"speaker":"B","text":"それも映画になりましたね。トム・ハンクスが看守の役を演じました。"},{"id":{"d":983,"n":9,"t":5},"speaker":"A","text":"映画も素晴らしかったです。やはり、スティーブン・キングは目を離せない作家です。"}];
INSERT INTO sentence [{"id":{"d":984,"n":1,"t":5},"speaker":"A","text":"テニスをしましょう。"},{"id":{"d":984,"n":2,"t":5},"speaker":"B","text":"この炎天下でするのですか？"},{"id":{"d":984,"n":3,"t":5},"speaker":"A","text":"もちろん、そのようなことはしません。私も熱中症は怖いですから。"},{"id":{"d":984,"n":4,"t":5},"speaker":"B","text":"それならどうしようというのですか？"},{"id":{"d":984,"n":5,"t":5},"speaker":"A","text":"インドアテニスです。最近そのクラブに入りました。"},{"id":{"d":984,"n":6,"t":5},"speaker":"B","text":"私はクラブには入るつもりはありませんよ。"},{"id":{"d":984,"n":7,"t":5},"speaker":"A","text":"大丈夫です。会員と一緒だと格安でプレーできます。"},{"id":{"d":984,"n":8,"t":5},"speaker":"B","text":"それはうれしいですね。ぜひお相手をお願いします。"}];
INSERT INTO sentence [{"id":{"d":985,"n":1,"t":5},"speaker":"A","text":"今年は梅雨が明けるのが早かったですね。"},{"id":{"d":985,"n":2,"t":5},"speaker":"B","text":"そうですね。まだ6月ですから、例年より1カ月も早いです。"},{"id":{"d":985,"n":3,"t":5},"speaker":"A","text":"このようなことは、私の記憶では初めてです。"},{"id":{"d":985,"n":4,"t":5},"speaker":"B","text":"気象庁の発表では、これまでで最も短い梅雨だったようですよ。"},{"id":{"d":985,"n":5,"t":5},"speaker":"A","text":"その分、暑い夏が続くのでしょうか？"},{"id":{"d":985,"n":6,"t":5},"speaker":"B","text":"それは何とも言えません。意外と涼しい夏になるかもしれません。"},{"id":{"d":985,"n":7,"t":5},"speaker":"A","text":"そうだといいのですが。私は夏にからきし弱いのです。"},{"id":{"d":985,"n":8,"t":5},"speaker":"B","text":"私も同じです。夏に強い人はめったにいないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":986,"n":1,"t":5},"speaker":"A","text":"息子がお笑い芸人になりたいと言っているのです。"},{"id":{"d":986,"n":2,"t":5},"speaker":"B","text":"すてきな将来の夢ですね。"},{"id":{"d":986,"n":3,"t":5},"speaker":"A","text":"ところが、親の欲目で見ても、とてもお笑いの才能があるとは思えません。"},{"id":{"d":986,"n":4,"t":5},"speaker":"B","text":"そのようなことは分かりませんよ。隠れた才能を持っているかもしれません。"},{"id":{"d":986,"n":5,"t":5},"speaker":"A","text":"それに、どうしてお笑い芸人になりたいと思うようになったのかもさっぱり分かりません。"},{"id":{"d":986,"n":6,"t":5},"speaker":"B","text":"憧れのお笑い芸人でもいるのでしょうか？"},{"id":{"d":986,"n":7,"t":5},"speaker":"A","text":"そうかもしれませんね。本人はよくダウンタウンのようになりたいと言っていますが。"},{"id":{"d":986,"n":8,"t":5},"speaker":"B","text":"なかなか高い理想を持っていますね。でも、そのくらいが良いですよ。"},{"id":{"d":986,"n":9,"t":5},"speaker":"A","text":"心配ですが、親としては黙って見守るしかありません。"}];
INSERT INTO sentence [{"id":{"d":987,"n":1,"t":5},"speaker":"A","text":"8月にペルセウス座流星群が見られるそうです。"},{"id":{"d":987,"n":2,"t":5},"speaker":"B","text":"流れ星がたくさん見られますね。楽しみです。"},{"id":{"d":987,"n":3,"t":5},"speaker":"A","text":"国立天文台の予想では、13日の4時ごろに極大になるそうです。"},{"id":{"d":987,"n":4,"t":5},"speaker":"B","text":"その頃、月の状態はどうなっているのでしょうか？"},{"id":{"d":987,"n":5,"t":5},"speaker":"A","text":"8日が新月ですから、13日は月明りの影響はないでしょう。"},{"id":{"d":987,"n":6,"t":5},"speaker":"B","text":"では、問題は天気ですね。晴れていればよいですが、曇りだと全く見えません。"},{"id":{"d":987,"n":7,"t":5},"speaker":"A","text":"長期予報では晴れるそうですが、天気は変わることがありますからね。"},{"id":{"d":987,"n":8,"t":5},"speaker":"B","text":"もし晴れていれば、一緒に見ましょう。"}];
INSERT INTO sentence [{"id":{"d":988,"n":1,"t":5},"speaker":"A","text":"写真はお好きですか？"},{"id":{"d":988,"n":2,"t":5},"speaker":"B","text":"はい、大好きです。自分でも撮っています。"},{"id":{"d":988,"n":3,"t":5},"speaker":"A","text":"どのような写真を撮っているのですか？"},{"id":{"d":988,"n":4,"t":5},"speaker":"B","text":"主に風景写真を撮っています。休みの日はほとんど撮影に行きます。"},{"id":{"d":988,"n":5,"t":5},"speaker":"A","text":"お気に入りの場所があるのですか？それとも、いろいろなところに行きますか？"},{"id":{"d":988,"n":6,"t":5},"speaker":"B","text":"以前はいろいろなところに行きましたが、最近はもっぱら富士山を撮っています。"},{"id":{"d":988,"n":7,"t":5},"speaker":"A","text":"富士山はいろいろな表情を見せるのでしょうね。"},{"id":{"d":988,"n":8,"t":5},"speaker":"B","text":"はい。時刻や天候によって表情が変わりますから、飽きることがありません。"},{"id":{"d":988,"n":9,"t":5},"speaker":"A","text":"あなたの写真をぜひ見てみたいものです。個展はなさるのですか？"},{"id":{"d":988,"n":10,"t":5},"speaker":"B","text":"実は、ちょうど来月に朝日ギャラリーで個展を開催します。"},{"id":{"d":988,"n":11,"t":5},"speaker":"A","text":"そうなのですね。ぜひ見に行きたいです。"},{"id":{"d":988,"n":12,"t":5},"speaker":"B","text":"ありがとうございます。ぜひ来てください。"}];
INSERT INTO sentence [{"id":{"d":989,"n":1,"t":5},"speaker":"A","text":"満月の夜にちなむ言い伝えがありますね。"},{"id":{"d":989,"n":2,"t":5},"speaker":"B","text":"オオカミ男の話などですか？満月の夜に半獣半人のオオカミ男に変身するというものです。"},{"id":{"d":989,"n":3,"t":5},"speaker":"A","text":"それもありますね。昔から、満月には不思議なパワーがあると思われていました。"},{"id":{"d":989,"n":4,"t":5},"speaker":"B","text":"月の満ち欠けは確かにパワーの盛衰と関係があるように見えますね。"},{"id":{"d":989,"n":5,"t":5},"speaker":"A","text":"はい。満月の夜と運気を絡めて、金運が良くなるともいわれているようです。"},{"id":{"d":989,"n":6,"t":5},"speaker":"B","text":"金運とも関係があるのですか？"},{"id":{"d":989,"n":7,"t":5},"speaker":"A","text":"そういう説もあるそうです。満月の夜に財布を振ると金運が良くなるともいわれています。"},{"id":{"d":989,"n":8,"t":5},"speaker":"B","text":"面白いですね。今度、満月の夜に財布を振ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":990,"n":1,"t":5},"speaker":"A","text":"今日は波が高いですね。"},{"id":{"d":990,"n":2,"t":5},"speaker":"B","text":"絶好のサーフィン日和です。"},{"id":{"d":990,"n":3,"t":5},"speaker":"A","text":"サーフィンをしていらっしゃるのですか？"},{"id":{"d":990,"n":4,"t":5},"speaker":"B","text":"はい、もう30年近くしています。"},{"id":{"d":990,"n":5,"t":5},"speaker":"A","text":"失礼ですが、おいくつですか？"},{"id":{"d":990,"n":6,"t":5},"speaker":"B","text":"今年で50歳になります。"},{"id":{"d":990,"n":7,"t":5},"speaker":"A","text":"驚きました。とてもそうは見えません。これからサーフィンに行かれるのですか？"},{"id":{"d":990,"n":8,"t":5},"speaker":"B","text":"はい、そのつもりです。この波を見ていると、波に乗りたくて体がむずむずしてきます。"},{"id":{"d":990,"n":9,"t":5},"speaker":"A","text":"気持ちもお若いですね。"}];
INSERT INTO sentence [{"id":{"d":991,"n":1,"t":5},"speaker":"A","text":"昨日は河口湖で打ち上げ花火がありました。"},{"id":{"d":991,"n":2,"t":5},"speaker":"B","text":"コロナ禍でしばらく開かれていなかったイベントですか？"},{"id":{"d":991,"n":3,"t":5},"speaker":"A","text":"そうです。3年ぶりでしたから、大勢の観光客が来ていました。"},{"id":{"d":991,"n":4,"t":5},"speaker":"B","text":"渋滞がひどかったのではありませんか？"},{"id":{"d":991,"n":5,"t":5},"speaker":"A","text":"はい。湖畔の駐車場が満車で、空いている駐車場を見つけるのが大変でした。"},{"id":{"d":991,"n":6,"t":5},"speaker":"B","text":"花火はどうでしたか？"},{"id":{"d":991,"n":7,"t":5},"speaker":"A","text":"楽しめました。3歳の子どもは花火を見るのが初めてだったので、大喜びしていました。"},{"id":{"d":991,"n":8,"t":5},"speaker":"B","text":"それは良かったですね。お子さんにも良い思い出になるでしょう。"}];
INSERT INTO sentence [{"id":{"d":992,"n":1,"t":5},"speaker":"A","text":"今日は幼稚園で何をしましたか？"},{"id":{"d":992,"n":2,"t":5},"speaker":"B","text":"今日はお絵描きをしました。"},{"id":{"d":992,"n":3,"t":5},"speaker":"A","text":"絵里ちゃんは何を描きましたか？"},{"id":{"d":992,"n":4,"t":5},"speaker":"B","text":"お母さんの似顔絵を描きました。"},{"id":{"d":992,"n":5,"t":5},"speaker":"A","text":"お母さんに見せてくれますか？"},{"id":{"d":992,"n":6,"t":5},"speaker":"B","text":"はい、これです。"},{"id":{"d":992,"n":7,"t":5},"speaker":"A","text":"よく描けていますね。"},{"id":{"d":992,"n":8,"t":5},"speaker":"B","text":"先生にも褒められました。お母さんにそっくりだと言われました。"},{"id":{"d":992,"n":9,"t":5},"speaker":"A","text":"そうですか。絵里ちゃんが褒められて、お母さんもうれしいです。"}];
INSERT INTO sentence [{"id":{"d":993,"n":1,"t":5},"speaker":"A","text":"季節外れの猛暑が続いたと思ったら、もう台風がやって来ますね。"},{"id":{"d":993,"n":2,"t":5},"speaker":"B","text":"今年は何だか異常な天候になりそうですね。"},{"id":{"d":993,"n":3,"t":5},"speaker":"A","text":"ハザードマップによれば、この辺りは大雨が降ると洪水の危険があります。"},{"id":{"d":993,"n":4,"t":5},"speaker":"B","text":"ハザードマップにそのように書いてあるのですか？"},{"id":{"d":993,"n":5,"t":5},"speaker":"A","text":"はい。大雨が降ると釜無川がすぐに氾濫してしまいます。昔から何度も氾濫しました。"},{"id":{"d":993,"n":6,"t":5},"speaker":"B","text":"今は水も少なくて、とても氾濫しそうもありませんけれどね。"},{"id":{"d":993,"n":7,"t":5},"speaker":"A","text":"普段はそうですね。でも、雨が降ると上流からの水であっという間に水かさが増してきます。"},{"id":{"d":993,"n":8,"t":5},"speaker":"B","text":"今のうちに避難経路などを調べて、備えておく方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":994,"n":1,"t":5},"speaker":"A","text":"おじいさん、敬老の日のプレゼントです。"},{"id":{"d":994,"n":2,"t":5},"speaker":"B","text":"俊君がプレゼントしてくれるとは、どのような風の吹き回しですか？"},{"id":{"d":994,"n":3,"t":5},"speaker":"A","text":"おじいさんにはかわいがってもらっているので、いつかお礼をしたいと思っていたのです。"},{"id":{"d":994,"n":4,"t":5},"speaker":"B","text":"そのように思っていたとは知りませんでした。"},{"id":{"d":994,"n":5,"t":5},"speaker":"A","text":"さあ、開けてみてください。"},{"id":{"d":994,"n":6,"t":5},"speaker":"B","text":"マフラーですか。ちょうどほしいと思っていたところなので、うれしいです。"},{"id":{"d":994,"n":7,"t":5},"speaker":"A","text":"はい。せっかくなら身に着けてもらえるものがよくて、選びました。"},{"id":{"d":994,"n":8,"t":5},"speaker":"B","text":"高かったのでしょう？出費をさせてすみませんでしたね。"},{"id":{"d":994,"n":9,"t":5},"speaker":"A","text":"気にしないでください。夏のボーナスがありますから、大丈夫ですよ。"},{"id":{"d":994,"n":10,"t":5},"speaker":"B","text":"ありがたく頂きます。この冬が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":995,"n":1,"t":5},"speaker":"A","text":"カラオケに行きましょう。"},{"id":{"d":995,"n":2,"t":5},"speaker":"B","text":"カラオケにはもう何年も行っていません。久しぶりに行くので楽しみです。"},{"id":{"d":995,"n":3,"t":5},"speaker":"A","text":"それなら、きっと驚きますよ。最近のカラオケの進歩は素晴らしいですから。"},{"id":{"d":995,"n":4,"t":5},"speaker":"B","text":"何が変わったのですか？"},{"id":{"d":995,"n":5,"t":5},"speaker":"A","text":"例えば、人工知能を活用した採点システムが搭載されているのです。"},{"id":{"d":995,"n":6,"t":5},"speaker":"B","text":"カラオケ機器が歌の上手や下手を採点するのですか？"},{"id":{"d":995,"n":7,"t":5},"speaker":"A","text":"そうです。音程やリズム、抑揚などを細かく分析して採点するのです。"},{"id":{"d":995,"n":8,"t":5},"speaker":"B","text":"それは少し嫌ですね。上手か下手かにかかわらず、気持ちよく歌いたいです。"},{"id":{"d":995,"n":9,"t":5},"speaker":"A","text":"そのようなことを言わずに一度体験してみてください。ゲーム感覚で楽しめますよ。"}];
INSERT INTO sentence [{"id":{"d":996,"n":1,"t":5},"speaker":"A","text":"フェルメールの展覧会がありますね。"},{"id":{"d":996,"n":2,"t":5},"speaker":"B","text":"それは知りませんでした。いつからですか？"},{"id":{"d":996,"n":3,"t":5},"speaker":"A","text":"5月から上野の西洋美術館であります。"},{"id":{"d":996,"n":4,"t":5},"speaker":"B","text":"それはぜひ行かなくてはいけませんね。何点くらいの作品が来るのですか？"},{"id":{"d":996,"n":5,"t":5},"speaker":"A","text":"正確には知りませんが大規模な展覧会で、世界中からフェルメールの作品を集めてくるそうですよ。"},{"id":{"d":996,"n":6,"t":5},"speaker":"B","text":"そのような機会は二度とありませんね。きっと大勢の人が見に行くことでしょう。"},{"id":{"d":996,"n":7,"t":5},"speaker":"A","text":"私もそう思います。きっと長い行列ができることでしょうね。"},{"id":{"d":996,"n":8,"t":5},"speaker":"B","text":"『モナ・リザ』や『ミロのヴィーナス』が来たの時も、長い行列ができましたね。"},{"id":{"d":996,"n":9,"t":5},"speaker":"A","text":"そうでした。私は途中で諦めて、結局見ることができませんでした。"}];
INSERT INTO sentence [{"id":{"d":997,"n":1,"t":5},"speaker":"A","text":"フジロックフェスティバルのシーズンがやって来ましたね。"},{"id":{"d":997,"n":2,"t":5},"speaker":"B","text":"2020年は中止、2021年は規模を縮小しての開催でしたから、今年こそは通常の開催を期待したいですね。"},{"id":{"d":997,"n":3,"t":5},"speaker":"A","text":"そうですね。でも、ここに来てまた感染者が急速に増えてきましたから、どうなるでしょうか。"},{"id":{"d":997,"n":4,"t":5},"speaker":"B","text":"昨年は国内だけでしたが、今年は世界中からロックシンガーが参加する予定ですので、主催者は気が気ではないでしょうね。"},{"id":{"d":997,"n":5,"t":5},"speaker":"A","text":"そうですね。"},{"id":{"d":997,"n":6,"t":5},"speaker":"B","text":"ところで、前から気になっていたのですが、苗場で開かれるのになぜフジロックフェスティバルというのでしょうか？"},{"id":{"d":997,"n":7,"t":5},"speaker":"A","text":"第1回が富士天神山スキー場で開かれたからです。その後はずっと苗場で開かれているのですが、名前はそのままなのです。"},{"id":{"d":997,"n":8,"t":5},"speaker":"B","text":"なるほど、そうだったのですか。富士の方が世界的にも通りがいいですからね。"}];
INSERT INTO sentence [{"id":{"d":998,"n":1,"t":5},"speaker":"A","text":"クラシック音楽がお好きなのですね。あなたの棚にはCDがたくさんあります。"},{"id":{"d":998,"n":2,"t":5},"speaker":"B","text":"はい、若い頃からクラシック音楽を聴いてきました。"},{"id":{"d":998,"n":3,"t":5},"speaker":"A","text":"ジャズやポップスはお聴きになりませんか？"},{"id":{"d":998,"n":4,"t":5},"speaker":"B","text":"聴かないわけではありませんが、クラシック音楽ほどではありません。"},{"id":{"d":998,"n":5,"t":5},"speaker":"A","text":"私はどちらかというとジャズが好きです。マイルス・デイビスやジョン・コルトレーンが特に好きです。"},{"id":{"d":998,"n":6,"t":5},"speaker":"B","text":"私はバッハが好きですが、バッハの音楽はジャズにアレンジされることもありますね。"},{"id":{"d":998,"n":7,"t":5},"speaker":"A","text":"そういえば、ジャック・ルーシェの『プレイ・バッハ』というレコードがありますね。"},{"id":{"d":998,"n":8,"t":5},"speaker":"B","text":"ええ、私もあのレコードを持っています。"},{"id":{"d":998,"n":9,"t":5},"speaker":"A","text":"あれは名盤ですね。素晴らしい演奏です。"}];
INSERT INTO sentence [{"id":{"d":999,"n":1,"t":5},"speaker":"A","text":"雲行きが怪しくなりましたね。最終回まで天気が持つでしょうか？"},{"id":{"d":999,"n":2,"t":5},"speaker":"B","text":"どうでしょうか。良い試合なので、何とか最後まで戦えるといいのですが。"},{"id":{"d":999,"n":3,"t":5},"speaker":"A","text":"ぽつぽつ降ってきました。"},{"id":{"d":999,"n":4,"t":5},"speaker":"B","text":"この程度なら試合を続行できます。"},{"id":{"d":999,"n":5,"t":5},"speaker":"A","text":"雨脚が強くなってきました。これは中止ですね。"},{"id":{"d":999,"n":6,"t":5},"speaker":"B","text":"残念ですね。両チームとも死力を尽くした良い試合でしたのに。"},{"id":{"d":999,"n":7,"t":5},"speaker":"A","text":"一時試合を中断するという放送がありました。しばらく様子を見るようです。"},{"id":{"d":999,"n":8,"t":5},"speaker":"B","text":"雨が上がってくれることを祈ります。"}];
INSERT INTO sentence [{"id":{"d":1000,"n":1,"t":5},"speaker":"A","text":"もう年の瀬ですか。"},{"id":{"d":1000,"n":2,"t":5},"speaker":"B","text":"早いものですね。つい最近、正月になったばかりだと思っていました。"},{"id":{"d":1000,"n":3,"t":5},"speaker":"A","text":"年を取ると時間が経つのが早くなるといいますが、本当ですね。"},{"id":{"d":1000,"n":4,"t":5},"speaker":"B","text":"子どもの頃は一年が長かったですね。"},{"id":{"d":1000,"n":5,"t":5},"speaker":"A","text":"ところで、年末はどのように過ごされますか？"},{"id":{"d":1000,"n":6,"t":5},"speaker":"B","text":"孫たちが東京からやって来るので、その相手をします。"},{"id":{"d":1000,"n":7,"t":5},"speaker":"A","text":"にぎやかでいいですね。私には孫がいないのでうらやましいです。"},{"id":{"d":1000,"n":8,"t":5},"speaker":"B","text":"そうでもありませんよ。孫たちを相手にするのはとにかく疲れます。帰ったらぐったりして、2、3日は寝込みます。"},{"id":{"d":1000,"n":9,"t":5},"speaker":"A","text":"想像はつきます。でも年末にお孫さんたちに囲まれて過ごせるのはお幸せですよ。"},{"id":{"d":1000,"n":10,"t":5},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":1001,"n":1,"t":5},"speaker":"A","text":"SF映画は好きですか？"},{"id":{"d":1001,"n":2,"t":5},"speaker":"B","text":"はい、好きでよく見ます。"},{"id":{"d":1001,"n":3,"t":5},"speaker":"A","text":"今まで見た中で何が一番良かったですか？"},{"id":{"d":1001,"n":4,"t":5},"speaker":"B","text":"やはり、スタンリー・キューブリックの『2001年宇宙の旅』ですね。"},{"id":{"d":1001,"n":5,"t":5},"speaker":"A","text":"あれは面白い作品でしたね。始めから終わりまで謎めいていました。"},{"id":{"d":1001,"n":6,"t":5},"speaker":"B","text":"しかも、その謎が解けないのがまた面白いのです。あなたは何が一番好きですか？"},{"id":{"d":1001,"n":7,"t":5},"speaker":"A","text":"私は、アンドレイ・タルコフスキーの『惑星ソラリス』が好きです。"},{"id":{"d":1001,"n":8,"t":5},"speaker":"B","text":"あれも良かったですね。何だか切ない映画でした。"},{"id":{"d":1001,"n":9,"t":5},"speaker":"A","text":"SF映画でそのような気持ちになるものは少ないです。"},{"id":{"d":1001,"n":10,"t":5},"speaker":"B","text":"はい。まさにタルコフスキーらしい映画です。"}];
INSERT INTO sentence [{"id":{"d":1002,"n":1,"t":5},"speaker":"A","text":"雨上がりの空気が爽やかですね。"},{"id":{"d":1002,"n":2,"t":5},"speaker":"B","text":"涼しい風が吹いてきます。"},{"id":{"d":1002,"n":3,"t":5},"speaker":"A","text":"虹も出ていますよ。"},{"id":{"d":1002,"n":4,"t":5},"speaker":"B","text":"どこにですか？"},{"id":{"d":1002,"n":5,"t":5},"speaker":"A","text":"あなたの後ろの方に出ています。"},{"id":{"d":1002,"n":6,"t":5},"speaker":"B","text":"本当ですね。湖の上に懸かっていて、きれいですね。"},{"id":{"d":1002,"n":7,"t":5},"speaker":"A","text":"その上の方にも、もう一つの虹がうっすらと懸かっています。"},{"id":{"d":1002,"n":8,"t":5},"speaker":"B","text":"二重の虹とは、珍しいですね。"},{"id":{"d":1002,"n":9,"t":5},"speaker":"A","text":"私も初めて見ます。"}];
INSERT INTO sentence [{"id":{"d":1003,"n":1,"t":5},"speaker":"A","text":"今年の母の日はいつですか？"},{"id":{"d":1003,"n":2,"t":5},"speaker":"B","text":"今年は5月8日の日曜日です。"},{"id":{"d":1003,"n":3,"t":5},"speaker":"A","text":"何を贈りましょうか？お母さんが喜んでくれるものは何でしょうね？"},{"id":{"d":1003,"n":4,"t":5},"speaker":"B","text":"何が良いですかね。去年は何をプレゼントしたかを覚えていますか？"},{"id":{"d":1003,"n":5,"t":5},"speaker":"A","text":"はい。去年はスカーフを贈ったところ、とても喜んでくれました。"},{"id":{"d":1003,"n":6,"t":5},"speaker":"B","text":"では、日傘はどうでしょうか？5月にはだんだん日差しが強くなってきますから。"},{"id":{"d":1003,"n":7,"t":5},"speaker":"A","text":"それは良いですね。一緒に探しに行きましょう。"},{"id":{"d":1003,"n":8,"t":5},"speaker":"B","text":"それでは、これから銀座に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1004,"n":1,"t":5},"speaker":"A","text":"ミステリー小説は読みますか？"},{"id":{"d":1004,"n":2,"t":5},"speaker":"B","text":"はい、よく読みます。特に、出張で地方に行くときは必ず持って行きます。"},{"id":{"d":1004,"n":3,"t":5},"speaker":"A","text":"その気持ちはよく分かります。列車や飛行機で読むのによく合いますよね。"},{"id":{"d":1004,"n":4,"t":5},"speaker":"B","text":"はい。帰りの列車の中でビールを飲みながらミステリー小説を読むのは良いものです。"},{"id":{"d":1004,"n":5,"t":5},"speaker":"A","text":"今まで読んだ中で印象に残っているものはありますか？"},{"id":{"d":1004,"n":6,"t":5},"speaker":"B","text":"『チーム・バチスタの栄光』が印象に残っています。とても面白かったです。"},{"id":{"d":1004,"n":7,"t":5},"speaker":"A","text":"「このミステリーがすごい！」の大賞を受賞しましたね。"},{"id":{"d":1004,"n":8,"t":5},"speaker":"B","text":"テレビドラマにもなりました。これも面白かったです。"}];
INSERT INTO sentence [{"id":{"d":1005,"n":1,"t":5},"speaker":"A","text":"この野外コンサートにはよく来られるのですか？"},{"id":{"d":1005,"n":2,"t":5},"speaker":"B","text":"はい、去年のコンサートにも来ました。"},{"id":{"d":1005,"n":3,"t":5},"speaker":"A","text":"去年は途中で雨が降り出して、大変なことになりましたね。"},{"id":{"d":1005,"n":4,"t":5},"speaker":"B","text":"そうでしたね。雨が降っても出演者たちは中止せずに最後まで演奏しました。"},{"id":{"d":1005,"n":5,"t":5},"speaker":"A","text":"多くの観客も、ずぶぬれになりながらも、退出せずに最後まで聴いていました。"},{"id":{"d":1005,"n":6,"t":5},"speaker":"B","text":"それがかえって一体感を生んで、とても良いコンサートになりましたね。"},{"id":{"d":1005,"n":7,"t":5},"speaker":"A","text":"ええ。終わった時の熱狂はすごかったですね。出演者も観客も一体になって、歓声を上げていました。"},{"id":{"d":1005,"n":8,"t":5},"speaker":"B","text":"忘れられないコンサートになりました。"}];
INSERT INTO sentence [{"id":{"d":1006,"n":1,"t":5},"speaker":"A","text":"そろそろ節分ですね。節分豆を買っておきましょうか？"},{"id":{"d":1006,"n":2,"t":5},"speaker":"B","text":"そうですね。多めに買いましょう。豆まきの時にまき切らずに残っても、僕が食べるから心配はありませんよ。"},{"id":{"d":1006,"n":3,"t":5},"speaker":"A","text":"あなたは豆菓子が好きですものね。分かりました、多めに買うことにします。"},{"id":{"d":1006,"n":4,"t":5},"speaker":"B","text":"ありがとう。この辺りの家は子どもが大きくなり、豆まきをする家も少なくなりましたね。"},{"id":{"d":1006,"n":5,"t":5},"speaker":"A","text":"そうですね。最近は「鬼は外、福は内」の声はわが家からしか聞こえないような気がします。"},{"id":{"d":1006,"n":6,"t":5},"speaker":"B","text":"日本古来の行事は大事にしたいので、わが家はこれからもずっと続けていきましょう。"},{"id":{"d":1006,"n":7,"t":5},"speaker":"A","text":"そうですね。それに、あなたにとってはいり大豆をたくさん食べることのできる大事な行事ですものね。"},{"id":{"d":1006,"n":8,"t":5},"speaker":"B","text":"その通りです。節分は大事な行事です。"}];
INSERT INTO sentence [{"id":{"d":1007,"n":1,"t":5},"speaker":"A","text":"最近はどのような音楽を聴いていますか？"},{"id":{"d":1007,"n":2,"t":5},"speaker":"B","text":"いろいろなジャンルの曲を聴きますが、今気に入っているのはあいみょんです。"},{"id":{"d":1007,"n":3,"t":5},"speaker":"A","text":"若い人に人気のアーティストですよね。どのようにして新しい曲を知るのですか？"},{"id":{"d":1007,"n":4,"t":5},"speaker":"B","text":"スマートスピーカーに「流行している曲をかけて」と話し掛けると、最近のヒット曲をスピーカーから流してくれます。"},{"id":{"d":1007,"n":5,"t":5},"speaker":"A","text":"それは良いですね。昔はテレビやラジオの「全国歌謡ベストテン」などの番組から新しい曲を知りましたが、今はスマートスピーカーがヒット曲を教えてくれるのですね。"},{"id":{"d":1007,"n":6,"t":5},"speaker":"B","text":"はい。新しい曲だけでなく、「静かな夜に聴くジャズをかけて」などといった注文にも応えてくれます。"},{"id":{"d":1007,"n":7,"t":5},"speaker":"A","text":"良いですね。私は聴く曲がワンパターンになっているので、スマートスピーカーが欲しくなりました。"},{"id":{"d":1007,"n":8,"t":5},"speaker":"B","text":"スマートスピーカーは便利ですよ。それに、さまざまな呼びかけに応えてくれるので愛着が湧きます。ぜひ使ってみてください。"}];
INSERT INTO sentence [{"id":{"d":1008,"n":1,"t":5},"speaker":"A","text":"来月の花火大会の桟敷席の抽選に当たりました。美津子さん、予定が空いていたら一緒に行きませんか？"},{"id":{"d":1008,"n":2,"t":5},"speaker":"B","text":"印旛沼の花火大会ですか？桟敷席はプラチナチケットだと聞いていますがよく当たりましたね。開催日はいつでしょうか？"},{"id":{"d":1008,"n":3,"t":5},"speaker":"A","text":"10月25日です。"},{"id":{"d":1008,"n":4,"t":5},"speaker":"B","text":"予定は入っていません。順子さんのご家族は花火大会に行かなくていいのですか？"},{"id":{"d":1008,"n":5,"t":5},"speaker":"A","text":"両親はその日に他の用事が入っているので、花火大会に行くことができません。"},{"id":{"d":1008,"n":6,"t":5},"speaker":"B","text":"それでしたら、ぜひご一緒させてください。確か音楽に合わせて打ち上げられるスターマインが有名でしたよね。二尺玉も何発か打ち上げられるのですよね？"},{"id":{"d":1008,"n":7,"t":5},"speaker":"A","text":"そうです。私も桟敷席で見るのは初めてなので、今からとても楽しみです。秋も深まった日の夜なので、防寒着を用意してきてくださいね。チケットはもう一席分あるので、由美さんも誘いましょうか？"},{"id":{"d":1008,"n":8,"t":5},"speaker":"B","text":"いいですね。3人で集まるのは久しぶりですからうれしく思います。誘ってくださってありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1009,"n":1,"t":5},"speaker":"A","text":"あさっての仕事終わりに予定がなければ映画を見に行きませんか？"},{"id":{"d":1009,"n":2,"t":5},"speaker":"B","text":"よいですよ。しばらく映画を見に行っていません。今は何が公開されているのでしょうか？"},{"id":{"d":1009,"n":3,"t":5},"speaker":"A","text":"半年にわたって往年の名作が公開されていて、12月は『スティング』が上映されています。『スティング』を見たことはありますか？"},{"id":{"d":1009,"n":4,"t":5},"speaker":"B","text":"いいえ。ハンサムな俳優がダブル主演を務めている映画でしたよね。コメディーだったでしょうか？"},{"id":{"d":1009,"n":5,"t":5},"speaker":"A","text":"はい。ポール・ニューマンとロバート・レッドフォードが2人で大掛かりな詐欺を仕掛ける痛快なコメディーです。本田さんもきっと気に入ることと思います。"},{"id":{"d":1009,"n":6,"t":5},"speaker":"B","text":"そうえいば、あさっては12月1日で映画の日ですから、料金が割引価格になるはずですね。"},{"id":{"d":1009,"n":7,"t":5},"speaker":"A","text":"映画の日だとは気付きませんでした。この日に行かない手はありませんよ。本田さん、行きましょう。"},{"id":{"d":1009,"n":8,"t":5},"speaker":"B","text":"はい。あさっては仕事を定時に終わらせられるように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":1010,"n":1,"t":5},"speaker":"A","text":"明日は7月7日で七夕です。七夕はお月見や節分と違って、その日だけ特別に食べるものはありませんよね。"},{"id":{"d":1010,"n":2,"t":5},"speaker":"B","text":"地方によってはそうめんを食べることがあるようですよ。"},{"id":{"d":1010,"n":3,"t":5},"speaker":"A","text":"どうしてそうめんなのでしょうね。そうめんが川の流れのように見えて、天の川を連想させるからでしょうか？"},{"id":{"d":1010,"n":4,"t":5},"speaker":"B","text":"中国の小麦粉でできたお菓子に由来すると聞いたことがあります。"},{"id":{"d":1010,"n":5,"t":5},"speaker":"A","text":"そうなのですね。わが家では七夕だからといってそうめんを食べることはありませんが、何か特別な行事食があれば七夕がもっと楽しくなると思います。"},{"id":{"d":1010,"n":6,"t":5},"speaker":"B","text":"あなたが小学生の頃は七夕の日の給食に星形のゼリーが出ていて、あなたもお兄ちゃんもそれを楽しみにしていましたよ。覚えていませんか？"},{"id":{"d":1010,"n":7,"t":5},"speaker":"A","text":"思い出しました。七夕には青いゼリーの中に黄色い星形のゼリーが浮かんでいるかわいらしいデザートが出ました。"},{"id":{"d":1010,"n":8,"t":5},"speaker":"B","text":"学校の栄養士さんも、生徒たちが喜ぶようにいろいろと工夫をしてくださっていたのですね。星形のゼリーも立派な行事食ですね。"}];
INSERT INTO sentence [{"id":{"d":1011,"n":1,"t":5},"speaker":"A","text":"昨日の大河ドラマを録画するのを忘れてしまいました。中川さんは大河ドラマを欠かさずに見ていると言っていましたよね。昨日の放送は録画してありますか？"},{"id":{"d":1011,"n":2,"t":5},"speaker":"B","text":"いいえ、大河ドラマはリアルタイムで見ることにしているので、録画はしていません。"},{"id":{"d":1011,"n":3,"t":5},"speaker":"A","text":"そうですか。大河ドラマの再放送があるかどうかはご存じですか？"},{"id":{"d":1011,"n":4,"t":5},"speaker":"B","text":"土曜日の午後に再放送があるようですよ。時間は知らないので、番組表で調べてみてください。"},{"id":{"d":1011,"n":5,"t":5},"speaker":"A","text":"再放送があるのですね。よかったです。これまで大河ドラマを見る習慣がなかったものですから、うっかり見るのを忘れてしまいました。"},{"id":{"d":1011,"n":6,"t":5},"speaker":"B","text":"今回の大河ドラマは前評判が高かったので、これまで大河ドラマを敬遠していた人たちも見ているようですね。"},{"id":{"d":1011,"n":7,"t":5},"speaker":"A","text":"初回を見てみたら一気に引き込まれて、すっかりとりこになっています。物語の舞台になっている鎌倉に行ってみようかなと思っているほどです。"},{"id":{"d":1011,"n":8,"t":5},"speaker":"B","text":"三谷幸喜さんの脚本は見る人を引きつけますよね。コメディーの中にも史実に基づいたシリアスなストーリーを巧みに織り交ぜていて、飽きさせません。"}];
INSERT INTO sentence [{"id":{"d":1012,"n":1,"t":5},"speaker":"A","text":"金井さん、今年の大みそかも岡野先生の家に集まろうという話があります。一緒に行きませんか？"},{"id":{"d":1012,"n":2,"t":5},"speaker":"B","text":"ぜひ行きたいです。皆で一緒に年越しができるのですね。"},{"id":{"d":1012,"n":3,"t":5},"speaker":"A","text":"去年は先生のお宅で紅白歌合戦を見てから、八幡宮へ皆で初詣に行きました。八幡宮へ向かう途中に、近所のお寺から除夜の鐘が聞こえて、年越しの風情がたっぷりでしたよ。"},{"id":{"d":1012,"n":4,"t":5},"speaker":"B","text":"そうですか、それはいいですね。昨年は家庭の用事があってお誘いを断らざるを得ず、とても残念でした。八幡宮の人出はどうでしたか？"},{"id":{"d":1012,"n":5,"t":5},"speaker":"A","text":"大勢の人が初詣に来ていたので、参道で長時間並びました。今年も覚悟しておいてくださいね。"},{"id":{"d":1012,"n":6,"t":5},"speaker":"B","text":"分かりました。暖かい服装で行きます。初詣の後は、皆はどうするのでしょうか？"},{"id":{"d":1012,"n":7,"t":5},"speaker":"A","text":"皆で一緒に最寄り駅まで行き、そこで解散です。真夜中ですがお正月なので特別ダイヤで電車が動いているため、家に帰れなくなる心配はありません。"},{"id":{"d":1012,"n":8,"t":5},"speaker":"B","text":"分かりました。集合時間や場所などが決まったら教えてください。"}];
INSERT INTO sentence [{"id":{"d":1013,"n":1,"t":5},"speaker":"A","text":"西の方から黒い雲が迫ってきています。急に雲行きが怪しくなってきました。"},{"id":{"d":1013,"n":2,"t":5},"speaker":"B","text":"冷たい風も吹いてきましたね。間もなく雨が降ってきそうです。大久保さん、洗濯物は大丈夫ですか？"},{"id":{"d":1013,"n":3,"t":5},"speaker":"A","text":"はい。今日はにわか雨が降ると天気予報で言っていたので、洗濯物は取り込んできました。天気予報ではひょうも降るかもしれないと言っていました。"},{"id":{"d":1013,"n":4,"t":5},"speaker":"B","text":"ひょうは嫌ですね。数年前に自宅の辺りでゴルフボールほどの大きさのひょうが降ったことがあります。氷の塊が打ち付けて激しい音がするので、家の中にいても恐怖を感じました。"},{"id":{"d":1013,"n":5,"t":5},"speaker":"A","text":"自然の猛威ですね。ひょうも怖いですが、私は雷も苦手です。"},{"id":{"d":1013,"n":6,"t":5},"speaker":"B","text":"雷も怖いですね。近くに雷が落ちると、何かが割けるようなものすごい音がして恐ろしいです。"},{"id":{"d":1013,"n":7,"t":5},"speaker":"A","text":"そうですね。そのような話をしているうちに、雷鳴が聞こえてきましたよ。"},{"id":{"d":1013,"n":8,"t":5},"speaker":"B","text":"空が稲光で光ったようです。雷が遠くに行くまでここから出ない方がいいですね。"},{"id":{"d":1013,"n":9,"t":5},"speaker":"A","text":"はい。もうしばらく室内にいましょう。"},{"id":{"d":1013,"n":10,"t":5},"speaker":"B","text":"そこのカフェでコーヒーでも飲みながら待ちませんか？"},{"id":{"d":1013,"n":11,"t":5},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":1014,"n":1,"t":5},"speaker":"A","text":"中条さんはスマートフォンにラジコを入れていますか？"},{"id":{"d":1014,"n":2,"t":5},"speaker":"B","text":"ラジコとは何ですか？アプリケーションですか？"},{"id":{"d":1014,"n":3,"t":5},"speaker":"A","text":"そうです。スマートフォンでラジオを聞くためのアプリケーションです。放送後1週間は聞き逃し配信がありますので、お気に入りの番組を逃さず聞くことができます。"},{"id":{"d":1014,"n":4,"t":5},"speaker":"B","text":"私にはラジオを聞く習慣がありません。そもそも、ラジオという言葉自体を、久しぶりに耳にしたように思います。野口さんは、ラジオをよく聞かれるのですか？"},{"id":{"d":1014,"n":5,"t":5},"speaker":"A","text":"はい、家事の手を止めずに楽しむことができるので、私はテレビよりもラジオの方が気に入っています。ひとたび聞き始めると、番組が変わっても局を変えずに聞き続けるので、期せずしてさまざまなジャンルの情報を得ることができます。"},{"id":{"d":1014,"n":6,"t":5},"speaker":"B","text":"なるほど。情報だけではなく、音楽も自分が日頃は聴かないようなジャンルの曲に触れることができそうですね。"},{"id":{"d":1014,"n":7,"t":5},"speaker":"A","text":"そうなのです。ラジオのおかげで、私は案外新しい曲を知っています。時には曲名も忘れてしまったような昔の音楽に遭遇することもあり、楽しいですよ。"},{"id":{"d":1014,"n":8,"t":5},"speaker":"B","text":"私もラジオを聞きたくなってきました。ラジコをスマートフォンに入れようと思います。野口さんのお薦めのラジオ番組を教えていただけますか？"}];
INSERT INTO sentence [{"id":{"d":1015,"n":1,"t":5},"speaker":"A","text":"こいのぼりが泳いでいますね。もう端午の節句が近いのですね。"},{"id":{"d":1015,"n":2,"t":5},"speaker":"B","text":"おばあちゃん、端午の節句とは何ですか？"},{"id":{"d":1015,"n":3,"t":5},"speaker":"A","text":"端午の節句とは5月5日のこどもの日のことですよ。季節の節目の日のことを節句と言います。"},{"id":{"d":1015,"n":4,"t":5},"speaker":"B","text":"端午の節句の他に、どのような節句があるのですか？"},{"id":{"d":1015,"n":5,"t":5},"speaker":"A","text":"七草の節句、桃の節句、七夕の節句、菊の節句があります。七草の節句は1月7日で、わが家では七草がゆを食べますね。桃の節句は3月3日のひな祭りのことです。七夕は知っていますね。菊の節句は9月9日で重陽の節句とも言います。"},{"id":{"d":1015,"n":6,"t":5},"speaker":"B","text":"菊の節句以外は、特別なものを食べたり、特別なものを飾ったりするから知っていました。うちでは菊の節句に何もしていませんよね？"},{"id":{"d":1015,"n":7,"t":5},"speaker":"A","text":"そうですね。私が子どもの頃は菊湯に入ることもありましたが、今は特別なことは何もしませんね。菊の節句は栗の節句とも呼ばれるので、栗ご飯を食べることもありました。"},{"id":{"d":1015,"n":8,"t":5},"speaker":"B","text":"私は栗ご飯が大好きです。今年から9月9日に栗ご飯を食べることをうちのルールにしたいです。"},{"id":{"d":1015,"n":9,"t":5},"speaker":"A","text":"いいですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":1016,"n":1,"t":5},"speaker":"A","text":"東の空を見ましたか？驚くほど大きなお月さまが見えていましたよ。"},{"id":{"d":1016,"n":2,"t":5},"speaker":"B","text":"そういえば、今日はスーパームーンだと朝のニュースで言っていました。今日は良い天気ですし、相当大きくはっきりと見えたのではありませんか？"},{"id":{"d":1016,"n":3,"t":5},"speaker":"A","text":"はい。まるで、演劇の舞台背景で描かれるような大きなお月さまでした。私が子どもの頃にはスーパームーンという言葉はありませんでした。何らかの原因でだんだんと月が大きく見えるようになってきているのでしょうか？"},{"id":{"d":1016,"n":4,"t":5},"speaker":"B","text":"いいえ。月が地球に近づいてきているなどということは考えにくいので、きっとスーパームーンという新しい呼び名が生まれたのだと思います。"},{"id":{"d":1016,"n":5,"t":5},"speaker":"A","text":"子どもの頃に見た月よりもずっと大きいような気がします。それは、子どもの時分は夕方の時間にゆっくりと空を見上げるようなことがなかったからかもしれませんね。"},{"id":{"d":1016,"n":6,"t":5},"speaker":"B","text":"そうですね。子どもの頃は夕方は家路を急いでいたり、習い事で忙しかったりしましたものね。"},{"id":{"d":1016,"n":7,"t":5},"speaker":"A","text":"大人になってからも、家事や育児や仕事に追われて毎日が慌ただしく過ぎていきました。"},{"id":{"d":1016,"n":8,"t":5},"speaker":"B","text":"ようやく空を見上げて、ゆっくりと月を愛でる余裕が出てきたということですね。"}];
INSERT INTO sentence [{"id":{"d":1017,"n":1,"t":5},"speaker":"A","text":"12月になりましたね。年賀状の準備はしていますか？私はまだ年賀状を買ってもいません。"},{"id":{"d":1017,"n":2,"t":5},"speaker":"B","text":"私もまだ年賀状には何も手を付けていません。年末はやらなくてはならないことが多くて気持ちが急きますよね。"},{"id":{"d":1017,"n":3,"t":5},"speaker":"A","text":"そうですね。年賀状の作成、大掃除、お歳暮の手配、そしてクライマックスはおせち料理作りです。"},{"id":{"d":1017,"n":4,"t":5},"speaker":"B","text":"佐々木さんはきちんとおせち料理を作られるのですね。私はここ数年は、市販のおせちセットで済ませています。"},{"id":{"d":1017,"n":5,"t":5},"speaker":"A","text":"2段重や3段重になっているものですね。私も気になってはいるのですが、おいしいかどうかが心配で、一度も試したことはありません。"},{"id":{"d":1017,"n":6,"t":5},"speaker":"B","text":"おせちセットは年々おいしくなっていますよ。食材も自分では調理が難しいようなものも入っていて、特別感がありますので、お正月の食卓が華やかになります。"},{"id":{"d":1017,"n":7,"t":5},"speaker":"A","text":"いいですね。お煮しめだけ自分で作って、あとはおせちセットにしてしまおうかしら。そう考えたら気が楽になりました。"},{"id":{"d":1017,"n":8,"t":5},"speaker":"B","text":"おせち料理の負担がなくなった分、年末の大掃除に力を入れることができます。掃除が行き届いた家で新年を迎えることができて気持ちが良いですよ。"}];
INSERT INTO sentence [{"id":{"d":1018,"n":1,"t":5},"speaker":"A","text":"松下さんのお子さんの誕生祝いに何をプレゼントしましょうか？"},{"id":{"d":1018,"n":2,"t":5},"speaker":"B","text":"女の子とのことですよね。肌触りの良いぬいぐるみはいかがでしょうか？"},{"id":{"d":1018,"n":3,"t":5},"speaker":"A","text":"私はベビー服やスタイなど、身に着けるものをイメージしていましたが、ぬいぐるみも良いですね。"},{"id":{"d":1018,"n":4,"t":5},"speaker":"B","text":"少し大きくなったら遊べるような、知育玩具も良いのではないかと思います。他の人と重ならないものやいくつあっても重宝するものを選びたいですね。"},{"id":{"d":1018,"n":5,"t":5},"speaker":"A","text":"そうですね。迷ってしまいますね。お子さんのいる方にアドバイスを頂きましょうか？総務課の青木さんに聞いてみましょう。"},{"id":{"d":1018,"n":6,"t":5},"speaker":"B","text":"そうですね。ある程度候補を絞ってから買いに行きましょう。"},{"id":{"d":1018,"n":7,"t":5},"speaker":"A","text":"今週の土曜日は空いていますか？横浜の伊勢佐木町にあるベビー用品店がとてもかわいいので、そこへ行きたいです。"},{"id":{"d":1018,"n":8,"t":5},"speaker":"B","text":"土曜日は特に予定がないので、ぜひ行きましょう。ベビー用品はどれもかわいらしくて見ていると幸せな気分になりますから、楽しみです。"}];
INSERT INTO sentence [{"id":{"d":1019,"n":1,"t":5},"speaker":"A","text":"早苗ちゃんは図工の成績がいつも良いですね。小さい頃からお絵描きが上手でしたから、絵を描く才能があるのかもしれませんよ。"},{"id":{"d":1019,"n":2,"t":5},"speaker":"B","text":"私もそのような気がしています。絵画教室に通わせようかと思っているところです。"},{"id":{"d":1019,"n":3,"t":5},"speaker":"A","text":"早苗ちゃんの描いたこのイラストもとても味があって良いですよね。本人がしたいと言うのであれば、ぜひさせてあげてください。"},{"id":{"d":1019,"n":4,"t":5},"speaker":"B","text":"そういえば、お母さんも自治会の回覧に挿絵を描いていましたよね。早苗はそのようなおばあちゃんのまねをしていたのかもしれませんね。"},{"id":{"d":1019,"n":5,"t":5},"speaker":"A","text":"それはうれしいことです。絵画だけではなく、粘土などの造形に挑戦するのもいいかもしれませんね。"},{"id":{"d":1019,"n":6,"t":5},"speaker":"B","text":"そうですね。工作やアクセサリー作りも好きですから、造形を習わせるのもいいですね。"},{"id":{"d":1019,"n":7,"t":5},"speaker":"A","text":"美術館に行ったり、良い景色の場所に連れて行ったりして、美しいものにもたくさん触れさせてあげてください。"},{"id":{"d":1019,"n":8,"t":5},"speaker":"B","text":"はい、そうします。早苗の興味の幅が広がる経験をさせてあげられたらと思います。"}];
INSERT INTO sentence [{"id":{"d":1020,"n":1,"t":5},"speaker":"A","text":"全員、活動をやめていったん教室に入りましょう。水分を取って身体を休めてください。"},{"id":{"d":1020,"n":2,"t":5},"speaker":"B","text":"先生がエアコンを入れて教室を冷やしておいてくださったのですね。ありがとうございます。"},{"id":{"d":1020,"n":3,"t":5},"speaker":"A","text":"この炎天下では、いくら君たちが外での活動に慣れているといっても体調を悪くしてしまいますからね。頭が痛かったり、気分が悪かったりはしませんか？"},{"id":{"d":1020,"n":4,"t":5},"speaker":"B","text":"大丈夫です。涼しい部屋で冷たい飲み物を飲んだら、だいぶ元気になりました。"},{"id":{"d":1020,"n":5,"t":5},"speaker":"A","text":"明日は猛暑日になると天気予報で言っていました。明日の部活は、教室で戦術の研究をすることにします。"},{"id":{"d":1020,"n":6,"t":5},"speaker":"B","text":"戦術を練るのも大事ですが、やはり外でボールを蹴りたいです。いっそのこと雨が降る方がいいです。雨が降れば、外で部活ができますか？"},{"id":{"d":1020,"n":7,"t":5},"speaker":"A","text":"雷さえ鳴らなければ、雨でもサッカーはできますよ。"},{"id":{"d":1020,"n":8,"t":5},"speaker":"B","text":"では、やはり雨がいいですね。涼しい部屋で休憩をして、身体が十分に休まりました。また外に出てよいですか？"},{"id":{"d":1020,"n":9,"t":5},"speaker":"A","text":"よいですよ。30分後にまた休憩を入れましょう。"}];
INSERT INTO sentence [{"id":{"d":1021,"n":1,"t":5},"speaker":"A","text":"柏木さん、涼しげなネックレスですね。今日のファッションとよく合っています。"},{"id":{"d":1021,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。実はこれは手作りなのです。手芸屋で買ったビーズアクセサリーのキットで作ったネックレスです。"},{"id":{"d":1021,"n":3,"t":5},"speaker":"A","text":"とても手作りには見えません。かなり手が込んでいますよね。難しかったでしょう。"},{"id":{"d":1021,"n":4,"t":5},"speaker":"B","text":"いいえ、それが案外簡単だったのです。作り方も詳しく書かれていて、誰にでもできると思います。"},{"id":{"d":1021,"n":5,"t":5},"speaker":"A","text":"私も以前、ビーズのアクセサリーを作ろうとしたことがあります。テキストの通りに作ろうとしたのですが、指定のビーズをお店で見つけることができず、諦めてしまいました。"},{"id":{"d":1021,"n":6,"t":5},"speaker":"B","text":"確かに、テキストに書いてある通りの材料をそろえるのは大変ですね。その点、キットはお手軽です。割高感はあるかもしれませんが、無駄が出ないので、むしろ安く済むかもしれません。"},{"id":{"d":1021,"n":7,"t":5},"speaker":"A","text":"そうですね。もう一度私もビーズアクセサリー作りにチャレンジしようかしら。キットはどこの手芸屋さんにも売っているでしょうか？"},{"id":{"d":1021,"n":8,"t":5},"speaker":"B","text":"どこでも手に入ると思いますが、隣駅のホームセンターの2階の手芸コーナーですと品ぞろえが充実していましたよ。"}];
INSERT INTO sentence [{"id":{"d":1022,"n":1,"t":5},"speaker":"A","text":"今かかっている曲は、何という曲名ですか？私が若い頃に流行した曲です。"},{"id":{"d":1022,"n":2,"t":5},"speaker":"B","text":"荒井由実さんの曲で、曲名は確か『ひこうき雲』だったと思います。最近公開されたスタジオジブリのアニメーション映画の主題歌になっています。"},{"id":{"d":1022,"n":3,"t":5},"speaker":"A","text":"道理で近頃よく耳にするわけですね。その映画のタイトルは何ですか？"},{"id":{"d":1022,"n":4,"t":5},"speaker":"B","text":"「風立ちぬ」です。"},{"id":{"d":1022,"n":5,"t":5},"speaker":"A","text":"飛行機に関係する物語ですね。そういえば映画のポスターをよく見かけます。ユーミンの曲は映画やドラマの主題歌になることが多いですね。"},{"id":{"d":1022,"n":6,"t":5},"speaker":"B","text":"そうですね。私たちの青春時代のドラマはユーミンの曲がよく使われていましたよね。音楽とともにいろいろな思い出がよみがえります。"},{"id":{"d":1022,"n":7,"t":5},"speaker":"A","text":"『ひこうき雲』はそのようなユーミンの曲の中でも初期の部類だと思いますが、今時の若い子たちに受け入れられるのでしょうか？古くさいと思われることはなかったのでしょうか？"},{"id":{"d":1022,"n":8,"t":5},"speaker":"B","text":"すんなりと受け入れられたようですよ。うちの子も気に入っていて、よく鼻歌を歌っています。"}];
INSERT INTO sentence [{"id":{"d":1023,"n":1,"t":5},"speaker":"A","text":"おはようございます。ライブチケットの当選結果が、今朝、発表されましたね。"},{"id":{"d":1023,"n":2,"t":5},"speaker":"B","text":"そうですね。もうチェックしましたか？"},{"id":{"d":1023,"n":3,"t":5},"speaker":"A","text":"いいえ、まだ心の準備ができておらず、確認できていません。"},{"id":{"d":1023,"n":4,"t":5},"speaker":"B","text":"もしよければ、今から一緒に確認しませんか？"},{"id":{"d":1023,"n":5,"t":5},"speaker":"A","text":"そうしましょう。緊張してきました。"},{"id":{"d":1023,"n":6,"t":5},"speaker":"B","text":"「3、2、1」の掛け声で同時に確認しましょう。"},{"id":{"d":1023,"n":7,"t":5},"speaker":"A","text":"分かりました。3、2、1。"},{"id":{"d":1023,"n":8,"t":5},"speaker":"B","text":"当選しました！しかも、A席だそうです。うそのようですが、前回に引き続き2回連続で良い席が当たりました。"},{"id":{"d":1023,"n":9,"t":5},"speaker":"A","text":"おめでとうございます。私は当選すらしませんでした。"},{"id":{"d":1023,"n":10,"t":5},"speaker":"B","text":"そうなのですね。何だか、申し訳ない気持ちです。"},{"id":{"d":1023,"n":11,"t":5},"speaker":"A","text":"久しぶりの参加になるはずが、残念な結果になってしまいました。"},{"id":{"d":1023,"n":12,"t":5},"speaker":"B","text":"第2公演に期待しましょう。"}];
INSERT INTO sentence [{"id":{"d":1024,"n":1,"t":5},"speaker":"A","text":"今週末は何をしていましたか？"},{"id":{"d":1024,"n":2,"t":5},"speaker":"B","text":"京セラドーム大阪に行って、野球の試合を見てきました。"},{"id":{"d":1024,"n":3,"t":5},"speaker":"A","text":"いいですね。どこ対どこの試合を観戦したのですか？"},{"id":{"d":1024,"n":4,"t":5},"speaker":"B","text":"阪神対ヤクルトです。阪神のグッズをたくさん持って行きましたよ。"},{"id":{"d":1024,"n":5,"t":5},"speaker":"A","text":"阪神ファンだったのですね。ご出身は大阪ですか？"},{"id":{"d":1024,"n":6,"t":5},"speaker":"B","text":"はい、そうです。よくご存じですね。"},{"id":{"d":1024,"n":7,"t":5},"speaker":"A","text":"いいえ、推測です。"},{"id":{"d":1024,"n":8,"t":5},"speaker":"B","text":"なぜ、そう推測したのですか？"},{"id":{"d":1024,"n":9,"t":5},"speaker":"A","text":"グッズをたくさん持つほどの熱心な阪神ファンの9割は、大阪出身だと聞いたことがあります。"},{"id":{"d":1024,"n":10,"t":5},"speaker":"B","text":"あながち間違いではないと思います。"}];
INSERT INTO sentence [{"id":{"d":1025,"n":1,"t":5},"speaker":"A","text":"昨日放送されたお笑いグランプリを見ましたか？"},{"id":{"d":1025,"n":2,"t":5},"speaker":"B","text":"いいえ、見ていません。なぜならば、家にテレビがないからです。"},{"id":{"d":1025,"n":3,"t":5},"speaker":"A","text":"そうなのですね。残念です。今回の優勝者は誰だったかを教えましょうか？"},{"id":{"d":1025,"n":4,"t":5},"speaker":"B","text":"待ってください。インターネットの見逃し配信で見る予定なので、結末を話すのは厳禁でお願いします。"},{"id":{"d":1025,"n":5,"t":5},"speaker":"A","text":"危なかったです。あなたの楽しみを奪ってしまうところでした。"},{"id":{"d":1025,"n":6,"t":5},"speaker":"B","text":"今晩にでも見る予定なので、また明日、この話の続きをしましょう。"},{"id":{"d":1025,"n":7,"t":5},"speaker":"A","text":"いいですね。早くお互いの感想を語り合いたいです。"},{"id":{"d":1025,"n":8,"t":5},"speaker":"B","text":"あなたは、かなり本格的なお笑い好きなのですね。"},{"id":{"d":1025,"n":9,"t":5},"speaker":"A","text":"ええ。私もいつかこのグランプリに出場するので、その時はリアルタイムで見てくださいね。"},{"id":{"d":1025,"n":10,"t":5},"speaker":"B","text":"応援していますよ。昨日の放送も未来のあなたの出演も、楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":1026,"n":1,"t":5},"speaker":"A","text":"見てください、流れ星の動画を撮ることができました。"},{"id":{"d":1026,"n":2,"t":5},"speaker":"B","text":"動画に収めることができるほどの大きさの流れ星だったのですね。ぜひ見せてください。"},{"id":{"d":1026,"n":3,"t":5},"speaker":"A","text":"これです。ここに小さく写っているのが見えますか？"},{"id":{"d":1026,"n":4,"t":5},"speaker":"B","text":"見えました。動画でこれほどしっかり写っているということは、実物はより迫力満点だったことでしょう。"},{"id":{"d":1026,"n":5,"t":5},"speaker":"A","text":"ええ。UFOなのではないかと思ってしまうほどの迫力でした。"},{"id":{"d":1026,"n":6,"t":5},"speaker":"B","text":"そういえば、「UFOの動画や写真を撮ったときはおはらいをしないと連れ去られる」と先日のテレビ番組で言っていましたよ。"},{"id":{"d":1026,"n":7,"t":5},"speaker":"A","text":"本当ですか？そのような映画のようなことがあるでしょうか？"},{"id":{"d":1026,"n":8,"t":5},"speaker":"B","text":"本当かどうかはよく分かりませんが、手遅れになってはいけませんし、気になるのであればおはらいをするに越したことはありませんよ。"},{"id":{"d":1026,"n":9,"t":5},"speaker":"A","text":"そうですね。今週末に行ってきます。"}];
INSERT INTO sentence [{"id":{"d":1027,"n":1,"t":5},"speaker":"A","text":"お久しぶりです。3年ぶりにあかねちゃんとお食事をすることができてうれしいです。"},{"id":{"d":1027,"n":2,"t":5},"speaker":"B","text":"私もです。待ち遠しくて、予定の3時間前に渋谷に着いていましたよ。"},{"id":{"d":1027,"n":3,"t":5},"speaker":"A","text":"それほど前からいたのですか？どうやって時間をつぶしていたのですか？"},{"id":{"d":1027,"n":4,"t":5},"speaker":"B","text":"少し離れたところにある、映画館に行っていました。"},{"id":{"d":1027,"n":5,"t":5},"speaker":"A","text":"あそこの映画館ですね。何の映画を見たのですか？"},{"id":{"d":1027,"n":6,"t":5},"speaker":"B","text":"『ハリー・ポッター』シリーズの新作を見てきました。展開が複雑でとても興奮しました。"},{"id":{"d":1027,"n":7,"t":5},"speaker":"A","text":"いいですね。私も先週に見てきましたよ。お店で感想を語りましょう。"},{"id":{"d":1027,"n":8,"t":5},"speaker":"B","text":"賛成です。趣味の合う友達とは一緒にいても話題が尽きることがなくて、とても楽しいですね。"}];
INSERT INTO sentence [{"id":{"d":1028,"n":1,"t":5},"speaker":"A","text":"こんばんは。もう12月ですね。今月は楽しいイベントが多くて、今から心が躍ります。"},{"id":{"d":1028,"n":2,"t":5},"speaker":"B","text":"そうなのですか？それはうらやましいです。"},{"id":{"d":1028,"n":3,"t":5},"speaker":"A","text":"椎名君は、何か予定はないのですか？"},{"id":{"d":1028,"n":4,"t":5},"speaker":"B","text":"ありませんよ。これまでの月と何ら変わりはありません。"},{"id":{"d":1028,"n":5,"t":5},"speaker":"A","text":"クリスマスも大晦日も予定がないのですか？"},{"id":{"d":1028,"n":6,"t":5},"speaker":"B","text":"はい、情けないのですが特にありません。"},{"id":{"d":1028,"n":7,"t":5},"speaker":"A","text":"そうなのですね。それは良かったです。ちょうどクリスマスに水族館へ誘おうと思っていたところです。"},{"id":{"d":1028,"n":8,"t":5},"speaker":"B","text":"冗談はやめてください。憧れの和田さんと僕がクリスマスを一緒に過ごすなんて考えられません。"},{"id":{"d":1028,"n":9,"t":5},"speaker":"A","text":"椎名君が何と言おうと、私は一緒に過ごしたいのです。一緒に新しくできた水族館に行きませんか？"},{"id":{"d":1028,"n":10,"t":5},"speaker":"B","text":"そのようなことを言ってもらえるとは、とてもうれしいです。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1029,"n":1,"t":5},"speaker":"A","text":"南君が歌っているところを見たことがありますか？"},{"id":{"d":1029,"n":2,"t":5},"speaker":"B","text":"いいえ。でも、今度の皆でカラオケに行く予定なので、そこで彼の歌っている姿を見られるかもしれません。"},{"id":{"d":1029,"n":3,"t":5},"speaker":"A","text":"ぜひ、期待しておいてください。彼はプロ並みに上手ですよ。"},{"id":{"d":1029,"n":4,"t":5},"speaker":"B","text":"あの南君がそれほど上手なのですか？彼は普段はおとなしい印象ですから、歌っているところがイメージできません。"},{"id":{"d":1029,"n":5,"t":5},"speaker":"A","text":"それが、一度マイクを握ると別人のように甘い声で歌うのですよ。"},{"id":{"d":1029,"n":6,"t":5},"speaker":"B","text":"ギャップがあるのですね。すてきではありませんか。"},{"id":{"d":1029,"n":7,"t":5},"speaker":"A","text":"ええ。カラオケに行った次の日から彼が人気者になるのは間違いありません。"},{"id":{"d":1029,"n":8,"t":5},"speaker":"B","text":"来月の文化祭の出し物がまだ決まっていないのですが、南君に一曲歌ってもらうのもいいかもしれませんね。"},{"id":{"d":1029,"n":9,"t":5},"speaker":"A","text":"それは素晴らしい案です。彼なら、全校生徒の心をつかむことができますよ。"}];
INSERT INTO sentence [{"id":{"d":1030,"n":1,"t":5},"speaker":"A","text":"今日は久しぶりにお会いできてよかったです。佐藤さんとの思い出といえば、小学校の頃に交換日記をしたことです。"},{"id":{"d":1030,"n":2,"t":5},"speaker":"B","text":"懐かしいですね。毎日学校で会っていたのにそれでもなお話したいことであふれていたなんて、すてきな年頃でしたね。"},{"id":{"d":1030,"n":3,"t":5},"speaker":"A","text":"そうですね。小さな出来事でも当時の私たちからすればビッグニュースでしたね。"},{"id":{"d":1030,"n":4,"t":5},"speaker":"B","text":"今となっては、自身の目の前のことをこなすのに精いっぱいで、誰かに報告する時間すらないように思います。"},{"id":{"d":1030,"n":5,"t":5},"speaker":"A","text":"私なんて、殺伐とした世の中に慣れてしまい、小さな感動にすら気付けていません。"},{"id":{"d":1030,"n":6,"t":5},"speaker":"B","text":"少し日常から離れて童心を取り戻すように心掛けた方が良さそうですね。"},{"id":{"d":1030,"n":7,"t":5},"speaker":"A","text":"そうですね。今週末に、久しぶりに思い出の河原まで足を運んでみませんか？"},{"id":{"d":1030,"n":8,"t":5},"speaker":"B","text":"賛成です。あの河原に行くのは15年ぶりなのでとても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":1031,"n":1,"t":5},"speaker":"A","text":"濱川君、就職活動は落ち着きましたか？"},{"id":{"d":1031,"n":2,"t":5},"speaker":"B","text":"はい、山場は越えました。まだ少しすることは残っていますが、負担にならない程度です。"},{"id":{"d":1031,"n":3,"t":5},"speaker":"A","text":"それは良かったですね。去年から就職活動に取り組む姿を見て、ずっと応援していました。"},{"id":{"d":1031,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。あなたはよく気遣いのメッセージをくれましたよね。とても励みになりました。"},{"id":{"d":1031,"n":5,"t":5},"speaker":"A","text":"全て終わったら、一緒にどこかへ出かけましょう。車は私が出しますよ。"},{"id":{"d":1031,"n":6,"t":5},"speaker":"B","text":"うれしいです。去年は勉強に打ち込んでいて行き損ねた、アジサイ鑑賞に行きたいです。"},{"id":{"d":1031,"n":7,"t":5},"speaker":"A","text":"長谷寺のアジサイですね。ぜひ行きましょう。今年は雨量が少なかったため、開花時期が遅れているようです。"},{"id":{"d":1031,"n":8,"t":5},"speaker":"B","text":"ちょうど良かったです。最終面接が来週末にあるので、それが終わった頃には、ちょうど満開になっているのではないでしょうか。"},{"id":{"d":1031,"n":9,"t":5},"speaker":"A","text":"そうですね。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":1032,"n":1,"t":5},"speaker":"A","text":"休日の過ごし方を教えてください。"},{"id":{"d":1032,"n":2,"t":5},"speaker":"B","text":"私はもっぱら散歩に出かけます。早朝に出発して、朝日を見に行くこともありますよ。"},{"id":{"d":1032,"n":3,"t":5},"speaker":"A","text":"休日にも早起きするなど、私には考えられません。"},{"id":{"d":1032,"n":4,"t":5},"speaker":"B","text":"早起きが習慣化しているので、そこまで大変なことではありませんよ。"},{"id":{"d":1032,"n":5,"t":5},"speaker":"A","text":"朝日を見た後は何をするのですか？"},{"id":{"d":1032,"n":6,"t":5},"speaker":"B","text":"散歩の際は必ず一眼レフカメラを持って行くので、自然の写真を撮りながらあちこち歩き回ります。"},{"id":{"d":1032,"n":7,"t":5},"speaker":"A","text":"私も一眼レフカメラを持っています。今度一緒に風景写真を撮りながら旅をしませんか？"},{"id":{"d":1032,"n":8,"t":5},"speaker":"B","text":"いいですね。朝早くから行動することになりますから、覚悟しておいてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1033,"n":1,"t":5},"speaker":"A","text":"来月から、菅田さんと小松さんが出演する映画が公開されますね。"},{"id":{"d":1033,"n":2,"t":5},"speaker":"B","text":"ようやくの公開ですね。去年に映画化が決定してからずっと楽しみにしていました。"},{"id":{"d":1033,"n":3,"t":5},"speaker":"A","text":"あの2人が主演の映画は、良作に間違いありませんからね。"},{"id":{"d":1033,"n":4,"t":5},"speaker":"B","text":"そうですね。これまでも2本の映画で共演されていましたが、どちらの作品でも演技力に圧倒されました。"},{"id":{"d":1033,"n":5,"t":5},"speaker":"A","text":"2人の芝居に引き込まれ過ぎて、号泣したのを覚えています。"},{"id":{"d":1033,"n":6,"t":5},"speaker":"B","text":"そうでしたね。前回なんて、映画館から出る時に目が真っ赤に充血していました。"},{"id":{"d":1033,"n":7,"t":5},"speaker":"A","text":"今回は、ハンカチを2、3枚は持っていこうと思います。"},{"id":{"d":1033,"n":8,"t":5},"speaker":"B","text":"バスタオルの方がいいかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":1034,"n":1,"t":5},"speaker":"A","text":"さえちゃん、学校の試験はいつまでですか？"},{"id":{"d":1034,"n":2,"t":5},"speaker":"B","text":"17日が最終日です。つまり、来週から夏休みです。"},{"id":{"d":1034,"n":3,"t":5},"speaker":"A","text":"実は、夏休みにさえちゃんと行きたいところがあるのです。"},{"id":{"d":1034,"n":4,"t":5},"speaker":"B","text":"どこでしょうか？"},{"id":{"d":1034,"n":5,"t":5},"speaker":"A","text":"スタジオジブリ展です。東京で開催されるので、ぜひ一緒に行きたいと思っています。"},{"id":{"d":1034,"n":6,"t":5},"speaker":"B","text":"私も同じことを思っていました。あかねちゃんはスタジオジブリの大ファンですものね。"},{"id":{"d":1034,"n":7,"t":5},"speaker":"A","text":"はい。しかもその展示会のメインが、私の大好きな『千と千尋の神隠し』の世界だそうです。"},{"id":{"d":1034,"n":8,"t":5},"speaker":"B","text":"それはたまりませんね。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1035,"n":1,"t":5},"speaker":"A","text":"なぜそれほどうまく絵が描けるのですか？"},{"id":{"d":1035,"n":2,"t":5},"speaker":"B","text":"幼少期から、よく絵を描いていたと親から聞いています。そのおかげでしょう。"},{"id":{"d":1035,"n":3,"t":5},"speaker":"A","text":"そうでしたか。もし、今からでも実践できる絵の描き方のこつがあれば、教えてください。"},{"id":{"d":1035,"n":4,"t":5},"speaker":"B","text":"輪郭をはっきりと描き過ぎないことが肝だと思います。"},{"id":{"d":1035,"n":5,"t":5},"speaker":"A","text":"なるほど。そればどうしてでしょうか？"},{"id":{"d":1035,"n":6,"t":5},"speaker":"B","text":"実物で、くっきりとした輪郭線があるものなど、ほぼないからです。リアリティがなくなってしまいます。"},{"id":{"d":1035,"n":7,"t":5},"speaker":"A","text":"言われてみればそうですね。その場合、背景との境界線はどのように表現するのですか？"},{"id":{"d":1035,"n":8,"t":5},"speaker":"B","text":"影を有効に使うといいですよ。"}];
INSERT INTO sentence [{"id":{"d":1036,"n":1,"t":5},"speaker":"A","text":"最近、料理教室に通い始めました。"},{"id":{"d":1036,"n":2,"t":5},"speaker":"B","text":"いいですね。通おうと思ったきっかけは何ですか？"},{"id":{"d":1036,"n":3,"t":5},"speaker":"A","text":"料理をすることはもともと好きなのですが、作るレシピがワンパターンになってしまっていたので、それを解決したかったのです。"},{"id":{"d":1036,"n":4,"t":5},"speaker":"B","text":"すてきな試みですね。教室では、どのような料理を作るのですか？"},{"id":{"d":1036,"n":5,"t":5},"speaker":"A","text":"和食がメインです。肉じゃがや煮付けといった一般的なものだけなく、郷土料理も習います。"},{"id":{"d":1036,"n":6,"t":5},"speaker":"B","text":"とても良いですね。それは確かに、料理のレパートリーが増えそうです。"},{"id":{"d":1036,"n":7,"t":5},"speaker":"A","text":"はい。料理の腕が上がるといった利点のみならず、全国を旅行した気分になれるのも気に入っています。"},{"id":{"d":1036,"n":8,"t":5},"speaker":"B","text":"私も通いたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":1037,"n":1,"t":5},"speaker":"A","text":"大阪出身の人は、「じゃんけんぽん」のことを、「いんじゃんほい」と言うと聞きましたが、本当ですか？"},{"id":{"d":1037,"n":2,"t":5},"speaker":"B","text":"はい。大阪の友達とじゃんけんをするときは「いんじゃんほい」という掛け声を使います。"},{"id":{"d":1037,"n":3,"t":5},"speaker":"A","text":"そうなのですね。じゃんけんの掛け声に地域性があるとは知りませんでした。"},{"id":{"d":1037,"n":4,"t":5},"speaker":"B","text":"その他にも、「ちーけった」という掛け声もあるそうですよ。"},{"id":{"d":1037,"n":5,"t":5},"speaker":"A","text":"それは、どこの地域で言われているものですか？"},{"id":{"d":1037,"n":6,"t":5},"speaker":"B","text":"千葉県です。"},{"id":{"d":1037,"n":7,"t":5},"speaker":"A","text":"そうなのですね。千葉出身の友達は何人かいますが、じゃんけんでその掛け声を使っていることがなかったので驚きです。"},{"id":{"d":1037,"n":8,"t":5},"speaker":"B","text":"今では、あまり使われなくなっているのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":1038,"n":1,"t":5},"speaker":"A","text":"ゆいかちゃんは、本の虫ですね。いつ会っても本を抱えている印象です。"},{"id":{"d":1038,"n":2,"t":5},"speaker":"B","text":"はい。小さい頃から読書が何よりも好きで、暇さえあれば本を読んでいます。"},{"id":{"d":1038,"n":3,"t":5},"speaker":"A","text":"すてきな時間の過ごし方ですね。お薦めの本を教えてほしいです。"},{"id":{"d":1038,"n":4,"t":5},"speaker":"B","text":"もちろんよいですよ。どのようなジャンルが好きですか？"},{"id":{"d":1038,"n":5,"t":5},"speaker":"A","text":"私はSFやミステリーが好きです。特に、最後にどんでん返しがある物語が大好きです。"},{"id":{"d":1038,"n":6,"t":5},"speaker":"B","text":"それなら、あきちゃんにぴったりの物語がありますよ。田代裕彦さんの『シナオシ』です。"},{"id":{"d":1038,"n":7,"t":5},"speaker":"A","text":"聞いたことがない題名です。ゆいかちゃんはマイナーな本まで知っているのですね。"},{"id":{"d":1038,"n":8,"t":5},"speaker":"B","text":"ええ。今は絶版になってしまいましたし、貸し出している図書館も少ないので注意してください。"},{"id":{"d":1038,"n":9,"t":5},"speaker":"A","text":"ゆいかちゃんはどのようにして読んだのですか？"},{"id":{"d":1038,"n":10,"t":5},"speaker":"B","text":"隣町の図書館まで行きました。あそこなら確実に置いてありますよ。"}];
INSERT INTO sentence [{"id":{"d":1039,"n":1,"t":5},"speaker":"A","text":"お久しぶりです。半年前にお会いした時よりも体形がすっきりしたように思います。"},{"id":{"d":1039,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。最近は、自分磨きに没頭しているので、その成果が出ているのかもしれません。"},{"id":{"d":1039,"n":3,"t":5},"speaker":"A","text":"具体的には、どのようなことをしているのですか？"},{"id":{"d":1039,"n":4,"t":5},"speaker":"B","text":"ピラティスやエステティックサロンに通っています。"},{"id":{"d":1039,"n":5,"t":5},"speaker":"A","text":"ピラティスは、最近は韓国出身のアイドルの間で流行している印象があります。"},{"id":{"d":1039,"n":6,"t":5},"speaker":"B","text":"はい。私も、応援している韓国のアイドルがピラティスを習っているということで、影響を受けて始めました。"},{"id":{"d":1039,"n":7,"t":5},"speaker":"A","text":"自分磨きのために月にどれくらいのお金をかけているのですか？"},{"id":{"d":1039,"n":8,"t":5},"speaker":"B","text":"5万円から10万円はかけています。お給料の3分の1は美容にかけています。"},{"id":{"d":1039,"n":9,"t":5},"speaker":"A","text":"きれいになったのが納得できる金額ですね。"}];
INSERT INTO sentence [{"id":{"d":1040,"n":1,"t":5},"speaker":"A","text":"しおりちゃんは、子どもの頃は何をして遊ぶことが多かったですか？"},{"id":{"d":1040,"n":2,"t":5},"speaker":"B","text":"私はトランプで遊ぶことが多かったです。たくさんの遊び方があるので、飽きずに楽しめていました。"},{"id":{"d":1040,"n":3,"t":5},"speaker":"A","text":"すぐに思いつく遊び方としては、ババ抜きや神経衰弱、スピードなどが思い浮かびますが、他にもありますか？"},{"id":{"d":1040,"n":4,"t":5},"speaker":"B","text":"有名なところだと、大富豪、ブラックジャック、七並べなどがあります。"},{"id":{"d":1040,"n":5,"t":5},"speaker":"A","text":"どれも一度は遊んだことがあります。トランプ遊びの中で最もお気に入りの遊び方を教えてください。"},{"id":{"d":1040,"n":6,"t":5},"speaker":"B","text":"マイナーな遊び方ですが、クリベッジというゲームがお気に入りです。"},{"id":{"d":1040,"n":7,"t":5},"speaker":"A","text":"聞いたことがありません。ルールを教えてもらえますか？"},{"id":{"d":1040,"n":8,"t":5},"speaker":"B","text":"ダーツのルールにポーカーを混ぜたようなゲームです。今から実践して説明しますね。"}];
INSERT INTO sentence [{"id":{"d":1041,"n":1,"t":5},"speaker":"A","text":"寒い日は、どのように過ごしていますか？"},{"id":{"d":1041,"n":2,"t":5},"speaker":"B","text":"可能な限り、こたつに入って過ごしています。温もりを感じながらのんびりとテレビの流し見をする時間が至福です。"},{"id":{"d":1041,"n":3,"t":5},"speaker":"A","text":"すてきですね。そこに、ミカンがあったら、なお良いですね。"},{"id":{"d":1041,"n":4,"t":5},"speaker":"B","text":"風情がありますね。あなたにとってはどのような過ごし方が理想的ですか？"},{"id":{"d":1041,"n":5,"t":5},"speaker":"A","text":"私はランニングをしに外へ出ます。だんだんと体温が上昇するとともに自己肯定感も上がっていくのが癖になります。"},{"id":{"d":1041,"n":6,"t":5},"speaker":"B","text":"それは尊敬できます。私は走るのが得意ではないので、こたつの中から応援しています。"},{"id":{"d":1041,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。もし気が向いたら、ランニングとは言わずともウォーキングを一緒にしてみませんか？"},{"id":{"d":1041,"n":8,"t":5},"speaker":"B","text":"歩くだけならできそうです。ぜひ、寒空ウォーキングに繰り出しましょう。"}];
INSERT INTO sentence [{"id":{"d":1042,"n":1,"t":5},"speaker":"A","text":"夏はさまざまなレジャースポットが盛り上がりを見せますよね。"},{"id":{"d":1042,"n":2,"t":5},"speaker":"B","text":"そうですね。先週は鴨川沿いでバーべキューをしましたし、来週は泉南で海水浴をする予定です。"},{"id":{"d":1042,"n":3,"t":5},"speaker":"A","text":"満喫していますね。"},{"id":{"d":1042,"n":4,"t":5},"speaker":"B","text":"阿曽君はこの夏にどこかへ出かける予定はありますか？"},{"id":{"d":1042,"n":5,"t":5},"speaker":"A","text":"明日は、映画の舞台挨拶を見に行く予定がありますが、夏らしいことはまだしていません。"},{"id":{"d":1042,"n":6,"t":5},"speaker":"B","text":"それでしたら、僕の仲間たちと一緒に海水浴に行きましょう。友達なら誰でも誘っていいことになっているのです。"},{"id":{"d":1042,"n":7,"t":5},"speaker":"A","text":"うれしいです。早速、明日は浮き輪を買って帰ろうと思います。"},{"id":{"d":1042,"n":8,"t":5},"speaker":"B","text":"水着も忘れずに持ってきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":1043,"n":1,"t":5},"speaker":"A","text":"一年のイベントで、最も好きなものは何ですか？"},{"id":{"d":1043,"n":2,"t":5},"speaker":"B","text":"クリスマスです。イルミネーションの輝きや、クリスマスソングのベルの音が気分を高揚させてくれます。"},{"id":{"d":1043,"n":3,"t":5},"speaker":"A","text":"とてもよく分かります。冬の澄んだ空気に、光や音が映えますよね。"},{"id":{"d":1043,"n":4,"t":5},"speaker":"B","text":"はい。あの時期は、いろいろなところへ出かけたくなります。"},{"id":{"d":1043,"n":5,"t":5},"speaker":"A","text":"去年は、どのようなところへ行きましたか？"},{"id":{"d":1043,"n":6,"t":5},"speaker":"B","text":"イルミネーションを見るために、神戸まで行きました。"},{"id":{"d":1043,"n":7,"t":5},"speaker":"A","text":"神戸ルミナリエですね。私も気になっていたのですが、まだ一度も行ったことがありません。どうでしたか？"},{"id":{"d":1043,"n":8,"t":5},"speaker":"B","text":"圧巻の一言です。一帯が現実世界ではないような雰囲気になり、まるで物語の世界に入り込んだかのような気分になります。"},{"id":{"d":1043,"n":9,"t":5},"speaker":"A","text":"それは素晴らしいですね。今年は必ず行こうと思います。"}];
INSERT INTO sentence [{"id":{"d":1044,"n":1,"t":5},"speaker":"A","text":"日本の年中行事には、どのようなものがありますか？"},{"id":{"d":1044,"n":2,"t":5},"speaker":"B","text":"ひな祭り、端午の節句、七夕などがあります。"},{"id":{"d":1044,"n":3,"t":5},"speaker":"A","text":"あなたは、どれが一番好きですか？"},{"id":{"d":1044,"n":4,"t":5},"speaker":"B","text":"ひな祭りです。ひな壇を眺める時間がとても好きだからです。お人形たちの精巧な作りに毎回感動します。"},{"id":{"d":1044,"n":5,"t":5},"speaker":"A","text":"私は5カ月前に初めて日本に来たので、まだひな祭りを経験していません。今から楽しみです。"},{"id":{"d":1044,"n":6,"t":5},"speaker":"B","text":"そうでしたか。ひな祭りの日は、ちらしずしや手まりずしを食べてお祝いすることが多いです。"},{"id":{"d":1044,"n":7,"t":5},"speaker":"A","text":"ちらしずしは聞いたことがありますが、手まりずしは初耳です。どのような食べ物なのですか？"},{"id":{"d":1044,"n":8,"t":5},"speaker":"B","text":"さまざまな食材で装飾が施された、小さな丸いおすしです。見て楽しいだけではなく食べてもおいしい、愛らしさ満点の食べ物です。"},{"id":{"d":1044,"n":9,"t":5},"speaker":"A","text":"3月3日がますます楽しみになってきました。"}];
INSERT INTO sentence [{"id":{"d":1045,"n":1,"t":5},"speaker":"A","text":"先日、友達のバイオリン演奏会に行ってきました。"},{"id":{"d":1045,"n":2,"t":5},"speaker":"B","text":"いかがでしたか？私は完全に素人ですので、素人にも分かる説明をしていただきたいです。"},{"id":{"d":1045,"n":3,"t":5},"speaker":"A","text":"楽器の奥深さに触れることのできる時間でした。バイオリンは、同じ型でも作られた年代によって音色に差が出るのですよ。"},{"id":{"d":1045,"n":4,"t":5},"speaker":"B","text":"そうなのですか。不思議ですね。木の材質にも音を左右する要素があるということですか？"},{"id":{"d":1045,"n":5,"t":5},"speaker":"A","text":"はい。木の種類に加えて、年輪の数でも響きが変わるため、この世界に二つとして同じものは存在しないのです。"},{"id":{"d":1045,"n":6,"t":5},"speaker":"B","text":"それを加味して演奏会を仕上げるのは、並大抵の努力ではかないませんね。"},{"id":{"d":1045,"n":7,"t":5},"speaker":"A","text":"はい。演奏した友人も、幼少期からバイオリンを続けていて、休日を返上して打ち込んでいました。"},{"id":{"d":1045,"n":8,"t":5},"speaker":"B","text":"アーティストの鏡ですね。"},{"id":{"d":1045,"n":9,"t":5},"speaker":"A","text":"はい。友人は、小さい頃から「何よりもバイオリンを弾くのが好きだ」と言っていました。"}];
INSERT INTO sentence [{"id":{"d":1046,"n":1,"t":5},"speaker":"A","text":"今年は猛暑が続いていて、そろそろ体が参ってしまいそうです。"},{"id":{"d":1046,"n":2,"t":5},"speaker":"B","text":"そうですね。梅雨も極端に短かったですし、異常気象が続きますね。"},{"id":{"d":1046,"n":3,"t":5},"speaker":"A","text":"はい。この猛暑を乗り切るため、最近、私は冷感グッズを集めることに尽力しています。"},{"id":{"d":1046,"n":4,"t":5},"speaker":"B","text":"それはいいですね。私は昔ながらの打ち水でこの猛暑を乗り切ろうとしています。"},{"id":{"d":1046,"n":5,"t":5},"speaker":"A","text":"打ち水は、気化熱で周囲の温度を下げてくれるのですよね。風流ですてきな対策ですね。"}];
INSERT INTO sentence [{"id":{"d":1047,"n":1,"t":5},"speaker":"A","text":"すみません、レンタルしているCDの交換をお願いしたいのですが。"},{"id":{"d":1047,"n":2,"t":5},"speaker":"B","text":"はい、理由をお伺いしてもよろしいでしょうか？"},{"id":{"d":1047,"n":3,"t":5},"speaker":"A","text":"先日こちらでレンタルしたCDの、中身とパッケージに相違がありました。"},{"id":{"d":1047,"n":4,"t":5},"speaker":"B","text":"相違と言いますと、具体的にどのようなことでしょうか？"},{"id":{"d":1047,"n":5,"t":5},"speaker":"A","text":"パッケージはアルバム曲のものですが、CD自体はシングル曲のみが収録されたものでした。"},{"id":{"d":1047,"n":6,"t":5},"speaker":"B","text":"それは大変失礼いたしました。こちらの手違いでございます。ただ今、別のものと交換いたします。"},{"id":{"d":1047,"n":7,"t":5},"speaker":"A","text":"お願いします。ちなみに、レンタル期間の延長などの対応は受けられますか？"},{"id":{"d":1047,"n":8,"t":5},"speaker":"B","text":"はい。レンタル期間は1週間延長させていただきます。また、次回からお使いいただけるクーポン券をお付けします。"},{"id":{"d":1047,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1048,"n":1,"t":5},"speaker":"A","text":"とてもかわいい絵柄のお洋服ですね。どこで購入しましたか？"},{"id":{"d":1048,"n":2,"t":5},"speaker":"B","text":"このTシャツの絵は私の姉が手描きしたものなので、非売品です。"},{"id":{"d":1048,"n":3,"t":5},"speaker":"A","text":"お姉さまはとても絵がお上手なのですね。イラストレーターのお仕事をされているのですか？"},{"id":{"d":1048,"n":4,"t":5},"speaker":"B","text":"いいえ。絵を描くことは彼女にとってただの趣味です。家の至るところに彼女の作品が転がっています。"},{"id":{"d":1048,"n":5,"t":5},"speaker":"A","text":"趣味にとどめておくにはもったいないほどの実力の持ち主ですね。もし販売しているようなら購入したいほどです。"},{"id":{"d":1048,"n":6,"t":5},"speaker":"B","text":"姉に言えば作品の一つや二つは、プレゼントしてくれるはずですよ。"},{"id":{"d":1048,"n":7,"t":5},"speaker":"A","text":"それは恐れ多いです。"},{"id":{"d":1048,"n":8,"t":5},"speaker":"B","text":"いいえ、彼女は逆に助かると思います。彼女の部屋は作品であふれ返っていて寝るスペースもないほどですから、数を減らしたいはずです。"},{"id":{"d":1048,"n":9,"t":5},"speaker":"A","text":"そのような事情でしたら喜んで頂きます。"}];
INSERT INTO sentence [{"id":{"d":1049,"n":1,"t":5},"speaker":"A","text":"こんにちは。店員さんに一つお伺いしたいのですが。"},{"id":{"d":1049,"n":2,"t":5},"speaker":"B","text":"はい、どうされましたか？"},{"id":{"d":1049,"n":3,"t":5},"speaker":"A","text":"こちらのホームセンターには、花火はもう置いていますか？"},{"id":{"d":1049,"n":4,"t":5},"speaker":"B","text":"いいえ、まだ置いておりません。例年6月中は入荷しないと思われます。"},{"id":{"d":1049,"n":5,"t":5},"speaker":"A","text":"そうですか。梅雨も明けたことですし公園で花火をする計画を立てていましたが、また今度にしようと思います。"},{"id":{"d":1049,"n":6,"t":5},"speaker":"B","text":"申し訳ありません。例年と比較して異常なほど早い梅雨明けでしたので、対応が遅れてしまいました。"},{"id":{"d":1049,"n":7,"t":5},"speaker":"A","text":"何か花火の代わりになるような、夏らしい玩具は販売していますか？"},{"id":{"d":1049,"n":8,"t":5},"speaker":"B","text":"水鉄砲はいかがでしょうか？こちらも、夏の楽しみの一つではないかと思います。"},{"id":{"d":1049,"n":9,"t":5},"speaker":"A","text":"では、水鉄砲を購入したいと思います。"}];
INSERT INTO sentence [{"id":{"d":1050,"n":1,"t":5},"speaker":"A","text":"お父さん、今日は月曜日ですが、なぜ会社に行かないのですか？"},{"id":{"d":1050,"n":2,"t":5},"speaker":"B","text":"今日は会社はお休みです。なぜなら創立記念日だからです。"},{"id":{"d":1050,"n":3,"t":5},"speaker":"A","text":"そうでしたか。"},{"id":{"d":1050,"n":4,"t":5},"speaker":"B","text":"稔の方こそ学校に行かなくてよいのですか？そろそろいつも稔が家を出ている時間になりますよ。"},{"id":{"d":1050,"n":5,"t":5},"speaker":"A","text":"今日から私は夏休みです。学校が再開するのは1カ月半後で、それまでは登校の必要がありません。"},{"id":{"d":1050,"n":6,"t":5},"speaker":"B","text":"もう夏休みなのですね。とても長期の休みでうらやましいです。"},{"id":{"d":1050,"n":7,"t":5},"speaker":"A","text":"お父さんの夏休みは、どれくらいの期間なのですか？"},{"id":{"d":1050,"n":8,"t":5},"speaker":"B","text":"たったの5日間です。小学生がとてもうらやましいです。"}];
