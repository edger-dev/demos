INSERT INTO sentence [{"id":{"d":177,"n":1,"t":5},"speaker":"A","text":"今日は、キャンプに行きましょうか？"},{"id":{"d":177,"n":2,"t":5},"speaker":"B","text":"良いですね。道具はそろっていますか？"},{"id":{"d":177,"n":3,"t":5},"speaker":"A","text":"はい、先週の日曜日に、お店で買いそろえました。"},{"id":{"d":177,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。キャンプ場での食事は、何を作りますか？"},{"id":{"d":177,"n":5,"t":5},"speaker":"A","text":"カレーにしようと思います。食材は、もう買ってあります。"},{"id":{"d":177,"n":6,"t":5},"speaker":"B","text":"準備が良いですね。それでは、キャンプに行きましょう。"}];
