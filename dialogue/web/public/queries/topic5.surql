INSERT INTO sentence [{"id":{"d":9,"n":1,"t":5},"speaker":"A","text":"学生時代は何のスポーツをしていましたか？"},{"id":{"d":9,"n":2,"t":5},"speaker":"B","text":"柔道をしていました。"},{"id":{"d":9,"n":3,"t":5},"speaker":"A","text":"柔道ですか。段は何段を持っているのですか？"},{"id":{"d":9,"n":4,"t":5},"speaker":"B","text":"初段です。小学生の時から柔道を始めて国士舘高校の柔道部に所属していました。"},{"id":{"d":9,"n":5,"t":5},"speaker":"A","text":"強いと思う人はいましたか？"},{"id":{"d":9,"n":6,"t":5},"speaker":"B","text":"先輩の鈴木桂治さんです。オリンピックで金メダルを取りましたが、彼は学生の時から強かったです。"}];
INSERT INTO sentence [{"id":{"d":10,"n":1,"t":5},"speaker":"A","text":"DVDで好きな映画はありますか？"},{"id":{"d":10,"n":2,"t":5},"speaker":"B","text":"『13日の金曜日』という映画が好きです。"},{"id":{"d":10,"n":3,"t":5},"speaker":"A","text":"どのような映画ですか？"},{"id":{"d":10,"n":4,"t":5},"speaker":"B","text":"すごくハラハラするホラー映画です。"},{"id":{"d":10,"n":5,"t":5},"speaker":"A","text":"どのような人が出てきますか？"},{"id":{"d":10,"n":6,"t":5},"speaker":"B","text":"ジェイソンという仮面をかぶった人が出てきます。"},{"id":{"d":10,"n":7,"t":5},"speaker":"A","text":"仮面ですか？それは怖いですね。"},{"id":{"d":10,"n":8,"t":5},"speaker":"B","text":"マスクの中の素顔が知りたくなります。"}];
INSERT INTO sentence [{"id":{"d":11,"n":1,"t":5},"speaker":"A","text":"映画を見に行きたいです。"},{"id":{"d":11,"n":2,"t":5},"speaker":"B","text":"映画ですか。それは良いですね。何の映画が見たいですか？"},{"id":{"d":11,"n":3,"t":5},"speaker":"A","text":"恋愛系が良いですね。最後にハッピーエンドで終わる映画が好きですね。"},{"id":{"d":11,"n":4,"t":5},"speaker":"B","text":"確かに、ハッピーエンドで終わると、見ていてとても気持ちが良いですよね。"},{"id":{"d":11,"n":5,"t":5},"speaker":"A","text":"そうなのです。今度一緒に映画を見に行きませんか？"},{"id":{"d":11,"n":6,"t":5},"speaker":"B","text":"それは良いですね。水曜日はレディースデーなので、少しお得に映画が見られます。"},{"id":{"d":11,"n":7,"t":5},"speaker":"A","text":"そうしたら、水曜日に行きましょう。"}];
