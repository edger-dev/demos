INSERT INTO sentence [{"id":{"d":462,"n":1,"t":5},"speaker":"A","text":"塗り絵は、楽しいですね。"},{"id":{"d":462,"n":2,"t":5},"speaker":"B","text":"ええ。親子で、すっかり夢中になってしまいましたね。"},{"id":{"d":462,"n":3,"t":5},"speaker":"A","text":"ママの塗り絵は難しそうです。"},{"id":{"d":462,"n":4,"t":5},"speaker":"B","text":"これは「大人の塗り絵」という本ですからね。"},{"id":{"d":462,"n":5,"t":5},"speaker":"A","text":"私の塗り絵はかわいいでしょう？"},{"id":{"d":462,"n":6,"t":5},"speaker":"B","text":"はい。かわいい女の子の絵ですね。"},{"id":{"d":462,"n":7,"t":5},"speaker":"A","text":"ピンクのドレスにしました。"},{"id":{"d":462,"n":8,"t":5},"speaker":"B","text":"タンスに入っているあなたのドレスに似ていますね。"}];
INSERT INTO sentence [{"id":{"d":463,"n":1,"t":5},"speaker":"A","text":"最近、数独にはまっています。"},{"id":{"d":463,"n":2,"t":5},"speaker":"B","text":"数独とは何ですか？"},{"id":{"d":463,"n":3,"t":5},"speaker":"A","text":"数独とは、「数字は独身に限る」の略で、1から9の数字を重ならないように埋めるパズルです。"},{"id":{"d":463,"n":4,"t":5},"speaker":"B","text":"それはナンバープレイスというのではありませんか？"},{"id":{"d":463,"n":5,"t":5},"speaker":"A","text":"はい、そういう名前もあります。"},{"id":{"d":463,"n":6,"t":5},"speaker":"B","text":"同じものなのですね？"},{"id":{"d":463,"n":7,"t":5},"speaker":"A","text":"はい。私は「パズル通信ニコリ」という雑誌でこのパズルを覚えたので、「数独」の方がしっくりくるのです。"},{"id":{"d":463,"n":8,"t":5},"speaker":"B","text":"なるほど。名前はともかく、楽しいパズルですよね。"}];
INSERT INTO sentence [{"id":{"d":464,"n":1,"t":5},"speaker":"A","text":"映画を見に行きませんか？"},{"id":{"d":464,"n":2,"t":5},"speaker":"B","text":"良いですね。何か、良い映画がありますか？"},{"id":{"d":464,"n":3,"t":5},"speaker":"A","text":"はい、『ドライブ・マイ・カー』はいかがですか？"},{"id":{"d":464,"n":4,"t":5},"speaker":"B","text":"はい。今、評判の映画ですね。私も見たいと思っていました。"},{"id":{"d":464,"n":5,"t":5},"speaker":"A","text":"そうですか。お誘いして良かったです。今度の日曜日はいかがですか？"},{"id":{"d":464,"n":6,"t":5},"speaker":"B","text":"はい、その日は何も予定がないので大丈夫です。どこで待ち合わせましょうか？"},{"id":{"d":464,"n":7,"t":5},"speaker":"A","text":"渋谷駅のハチ公前で待ち合わせましょう。"},{"id":{"d":464,"n":8,"t":5},"speaker":"B","text":"分かりました。映画が始まるのは何時からですか？"},{"id":{"d":464,"n":9,"t":5},"speaker":"A","text":"午前9時からです。"},{"id":{"d":464,"n":10,"t":5},"speaker":"B","text":"随分早いですね。もっと遅い時間帯はないのですか？"},{"id":{"d":464,"n":11,"t":5},"speaker":"A","text":"はい、その1回だけなのです。12時ごろに終わるので、一緒にランチを食べましょう。"},{"id":{"d":464,"n":12,"t":5},"speaker":"B","text":"それは良いですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":465,"n":1,"t":5},"speaker":"A","text":"今年の冬は、随分と雪が多いですね。"},{"id":{"d":465,"n":2,"t":5},"speaker":"B","text":"そうですね。この数年は雪が少なくて、温暖化の影響だろうと思っていたのですが、今年は違いますね。"},{"id":{"d":465,"n":3,"t":5},"speaker":"A","text":"10年ほど前に、非常に多くの雪が降ったことがありましたよね。"},{"id":{"d":465,"n":4,"t":5},"speaker":"B","text":"そう、あの時は驚きました。一晩で1メートルを超える雪が降りましたものね。"},{"id":{"d":465,"n":5,"t":5},"speaker":"A","text":"除雪するのに2、3日かかり、その間、何軒か孤立した家がありました。"},{"id":{"d":465,"n":6,"t":5},"speaker":"B","text":"自衛隊に出動してもらいましたね。"},{"id":{"d":465,"n":7,"t":5},"speaker":"A","text":"屋根が落ちた体育館がいくつかありましたね。"},{"id":{"d":465,"n":8,"t":5},"speaker":"B","text":"そうでした。小学校か中学校の体育館でした。"},{"id":{"d":465,"n":9,"t":5},"speaker":"A","text":"わが家の周りの状況もひどかったと思います。除雪した跡が、壁のようにそそり立って、その間を自動車が恐る恐る動いていました。"},{"id":{"d":465,"n":10,"t":5},"speaker":"B","text":"後にも先にも、あれほどの雪を経験したことがありません。"}];
INSERT INTO sentence [{"id":{"d":466,"n":1,"t":5},"speaker":"A","text":"山田さんは、クラシック音楽はお好きですか？"},{"id":{"d":466,"n":2,"t":5},"speaker":"B","text":"はい、とても好きで、よくコンサートに行きます。"},{"id":{"d":466,"n":3,"t":5},"speaker":"A","text":"それはちょうど良かった。実はチケットが2枚手に入ったのですが、一緒に行きませんか？"},{"id":{"d":466,"n":4,"t":5},"speaker":"B","text":"それはうれしいですね。演目は何ですか？"},{"id":{"d":466,"n":5,"t":5},"speaker":"A","text":"バッハのピアノ曲がいくつかと、現代音楽のやはりピアノ曲がいくつかです。"},{"id":{"d":466,"n":6,"t":5},"speaker":"B","text":"ほう、なかなか凝った構成ですね。私はバッハが好きなので、興味があります。"},{"id":{"d":466,"n":7,"t":5},"speaker":"A","text":"それは良かったです。現代音楽は、もしかすると敬遠なさるかと思っていたのですが。"},{"id":{"d":466,"n":8,"t":5},"speaker":"B","text":"とんでもない、面白そうではありませんか。ぜひ、行ってみたいです。いつですか？"},{"id":{"d":466,"n":9,"t":5},"speaker":"A","text":"今度の日曜日の午後7時からです。その前に軽く夕食を取って、それから行きましょう。"},{"id":{"d":466,"n":10,"t":5},"speaker":"B","text":"良いですね。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":467,"n":1,"t":5},"speaker":"A","text":"山中湖や河口湖では、冬になると毎週土曜日に打ち上げ花火をしていました。"},{"id":{"d":467,"n":2,"t":5},"speaker":"B","text":"湖上での打ち上げ花火は、壮観でしょうね。"},{"id":{"d":467,"n":3,"t":5},"speaker":"A","text":"はい、なかなかの見ものですよ。これを見るために大勢の観光客が来ていました。"},{"id":{"d":467,"n":4,"t":5},"speaker":"B","text":"過去形でおっしゃるのは、今はしていないからですか？"},{"id":{"d":467,"n":5,"t":5},"speaker":"A","text":"そうなのです。新型コロナウイルス感染症流行の影響で、この2年間は中止せざるを得なくなりました。"},{"id":{"d":467,"n":6,"t":5},"speaker":"B","text":"それは残念ですね。ホテル業界や飲食業には大打撃ですね。"},{"id":{"d":467,"n":7,"t":5},"speaker":"A","text":"そうです。新型コロナウイルス感染症パンデミックの前は、大勢のインバウンドでごった返していましたが、今ではほとんど見かけなくなりました。"},{"id":{"d":467,"n":8,"t":5},"speaker":"B","text":"新型コロナウイルス感染症パンデミックが早く収束して、また花火大会が再開するといいですね。"}];
INSERT INTO sentence [{"id":{"d":468,"n":1,"t":5},"speaker":"A","text":"川本さんは、ドライブするときにBGMとして音楽を聴きますか？"},{"id":{"d":468,"n":2,"t":5},"speaker":"B","text":"はい、もちろん聴きます。"},{"id":{"d":468,"n":3,"t":5},"speaker":"A","text":"どういう媒体で聴いていますか？CDですか？"},{"id":{"d":468,"n":4,"t":5},"speaker":"B","text":"はい、CDでももちろん聴きます。木下さんは、何で聴いているのですか？"},{"id":{"d":468,"n":5,"t":5},"speaker":"A","text":"私もCDで聴いています。でも、オートチェンジャーが付いていないので、いちいちCDを取り替えるのが面倒で、困っているのです。"},{"id":{"d":468,"n":6,"t":5},"speaker":"B","text":"それなら、CD内のデータをパソコンでコピーしてUSBなどに入れればよいですよ。"},{"id":{"d":468,"n":7,"t":5},"speaker":"A","text":"それは、著作権法に違反するのではありませんか？"},{"id":{"d":468,"n":8,"t":5},"speaker":"B","text":"自分だけのためにするのであれば、違反にはなりませんよ。"},{"id":{"d":468,"n":9,"t":5},"speaker":"A","text":"そうなのですね。それでは、自分で試します。"}];
INSERT INTO sentence [{"id":{"d":469,"n":1,"t":5},"speaker":"A","text":"岸本さんのお宅では、正月のおせち料理はどうしていますか？"},{"id":{"d":469,"n":2,"t":5},"speaker":"B","text":"わが家では、大みそかに家族総出でおせち料理を作っています。"},{"id":{"d":469,"n":3,"t":5},"speaker":"A","text":"ええ！全部、自分たちで作るのですか？"},{"id":{"d":469,"n":4,"t":5},"speaker":"B","text":"はい、そうです。1年に1回のことですから。家族でにぎやかに作ります。なかなか楽しいものですよ。"},{"id":{"d":469,"n":5,"t":5},"speaker":"A","text":"両親が生きていた頃は、私の家でも自分たちで作っていましたが、今はとてもできません。"},{"id":{"d":469,"n":6,"t":5},"speaker":"B","text":"それでは、スーパーマーケットや通販で、出来合いのものを買うのですか？"},{"id":{"d":469,"n":7,"t":5},"speaker":"A","text":"はい。手作りもしてみたいのですが、難しそうな気がするのです。"},{"id":{"d":469,"n":8,"t":5},"speaker":"B","text":"簡単なものから、少しずつ作ったらいかがですか？"},{"id":{"d":469,"n":9,"t":5},"speaker":"A","text":"そうですね。岸本さんのお宅は楽しそうですので、わが家でも一度挑戦してみます。"},{"id":{"d":469,"n":10,"t":5},"speaker":"B","text":"ぜひ、そうするといいですよ。"}];
INSERT INTO sentence [{"id":{"d":470,"n":1,"t":5},"speaker":"A","text":"2月も終わりに近づいて、ようやく冬も終わりそうですね。"},{"id":{"d":470,"n":2,"t":5},"speaker":"B","text":"そうですね。今年は寒波が何度も来て、雪も多かったから、春が待ち遠しいですね。"},{"id":{"d":470,"n":3,"t":5},"speaker":"A","text":"ええ。そろそろキャンディーズの「もうすぐ春ですね」という歌詞の歌が聴こえてくる時節ですね。"},{"id":{"d":470,"n":4,"t":5},"speaker":"B","text":"自分でも自然に口ずさんでしまいます。"},{"id":{"d":470,"n":5,"t":5},"speaker":"A","text":"それと、クリスマスのときの「きっと君は来ない」という歌詞の歌もそうですね。"},{"id":{"d":470,"n":6,"t":5},"speaker":"B","text":"もはや、定番になりましたね。"},{"id":{"d":470,"n":7,"t":5},"speaker":"A","text":"歌を聴くことで、春やクリスマスの気分が、いやが上にも高まりますね。"}];
INSERT INTO sentence [{"id":{"d":471,"n":1,"t":5},"speaker":"A","text":"佐伯さんは、ハンドパンという楽器を知っていますか？"},{"id":{"d":471,"n":2,"t":5},"speaker":"B","text":"いえ、知りません。初めて聞きました。どのような楽器ですか？"},{"id":{"d":471,"n":3,"t":5},"speaker":"A","text":"鉄でできた打楽器で、直径が50センチメートルくらいの、空飛ぶ円盤のような形をした物です。"},{"id":{"d":471,"n":4,"t":5},"speaker":"B","text":"どのような音が出るのですか？"},{"id":{"d":471,"n":5,"t":5},"speaker":"A","text":"何とも不思議な音色で、説明するのが難しいです。分かりやすく言えば、フライパンの裏を叩いたような音ですが、もっと底深くて神秘的な感じがします。"},{"id":{"d":471,"n":6,"t":5},"speaker":"B","text":"アフリカかどこかの民族音楽の楽器の音のようにも聴こえますが、違いますか？"},{"id":{"d":471,"n":7,"t":5},"speaker":"A","text":"いえ、20年ほど前にスイスで初めて作られた楽器なのです。もしかすると、どこかの民族音楽に影響を受けたのかもしれません。"},{"id":{"d":471,"n":8,"t":5},"speaker":"B","text":"面白そうですね。木村さん、その楽器を持っていらっしゃるのですか？"},{"id":{"d":471,"n":9,"t":5},"speaker":"A","text":"いえ、まだ持っていませんが、手頃な値段な物があれば買いたいと思っています。"},{"id":{"d":471,"n":10,"t":5},"speaker":"B","text":"もし買われたら、ぜひ、音色を聴かせてください。"}];
INSERT INTO sentence [{"id":{"d":472,"n":1,"t":5},"speaker":"A","text":"「駅ピアノ・空港・街角ピアノ」というテレビ番組があるでしょう？"},{"id":{"d":472,"n":2,"t":5},"speaker":"B","text":"鉄道の駅や空港、街角にピアノが置いてあって、通りがかりの人が自由に演奏する番組ですね。"},{"id":{"d":472,"n":3,"t":5},"speaker":"A","text":"そうです。私はあの番組が好きで、よく見ています。"},{"id":{"d":472,"n":4,"t":5},"speaker":"B","text":"いろいろな年齢、いろいろな職業の人がピアノを演奏していますね。"},{"id":{"d":472,"n":5,"t":5},"speaker":"A","text":"さまざまな人生経験をした人が、それぞれにピアノに託した思いを述べていて感動します。"},{"id":{"d":472,"n":6,"t":5},"speaker":"B","text":"独学で習得したという人が多くて、感心します。"},{"id":{"d":472,"n":7,"t":5},"speaker":"A","text":"この間は、ドイツでピアノ教師をしているフィリピン人の女性が、ほろ酔い気分で演奏しながら、なかなか味わいのあることを述べていました。"},{"id":{"d":472,"n":8,"t":5},"speaker":"B","text":"それは私も見ました。風格のある女性で、見ていて気持ちが良かったです。"}];
INSERT INTO sentence [{"id":{"d":473,"n":1,"t":5},"speaker":"A","text":"『007』の映画がテレビで再放送されていますね。池田君は、見ていますか？"},{"id":{"d":473,"n":2,"t":5},"speaker":"B","text":"はい、見ています。僕はダニエル・クレイグのジェームズ・ボンドしか知りませんが、その前のジェームズ・ボンドが見られて面白いです。"},{"id":{"d":473,"n":3,"t":5},"speaker":"A","text":"池田君は、どの俳優の演じたジェームズ・ボンドが好きですか？"},{"id":{"d":473,"n":4,"t":5},"speaker":"B","text":"僕はショーン・コネリーが好きですね。歴代のジェームズ・ボンド役の中では一番渋いと思います。野田君は誰が好きですか？"},{"id":{"d":473,"n":5,"t":5},"speaker":"A","text":"僕はピアース・ブロスナンが好きです。クールで格好良いです。"},{"id":{"d":473,"n":6,"t":5},"speaker":"B","text":"ロジャー・ムーアも良いですね。何となくユーモラスなところがいいです。"},{"id":{"d":473,"n":7,"t":5},"speaker":"A","text":"俳優によって、ジェームズ・ボンド役のイメージが、がらりと変わるのも面白いですね。"},{"id":{"d":473,"n":8,"t":5},"speaker":"B","text":"それにしても、『007』は息の長いシリーズですね。"}];
INSERT INTO sentence [{"id":{"d":474,"n":1,"t":5},"speaker":"A","text":"梅雨入りには、2種類あるということを知っていましたか？"},{"id":{"d":474,"n":2,"t":5},"speaker":"B","text":"いえ、知りませんでした。どう違うのですか？"},{"id":{"d":474,"n":3,"t":5},"speaker":"A","text":"1つは気象庁が使用している用語で、実際に梅雨の期間に入ることです。"},{"id":{"d":474,"n":4,"t":5},"speaker":"B","text":"私たちが普通に使っている梅雨入りですね。もう1つは何ですか？"},{"id":{"d":474,"n":5,"t":5},"speaker":"A","text":"暦の上の梅雨入りのことで、「入梅」といいます。農家にとっては、田植えの日取りを決める目安になっていました。"},{"id":{"d":474,"n":6,"t":5},"speaker":"B","text":"なるほど。私たちにとっては、梅雨入りというのは憂鬱なものですが、農家にとっては大事なものだったのですね。"},{"id":{"d":474,"n":7,"t":5},"speaker":"A","text":"今は気象データで、梅雨入りの時期を予想することができますが、昔はそれがなかったので、暦が重要だったのです。"}];
INSERT INTO sentence [{"id":{"d":475,"n":1,"t":5},"speaker":"A","text":"節分の豆まきのとき、「鬼は外、福は内」と言うでしょう？"},{"id":{"d":475,"n":2,"t":5},"speaker":"B","text":"はい、わが家でもそう言います。"},{"id":{"d":475,"n":3,"t":5},"speaker":"A","text":"それが普通だと思っていたのですが、そうでないところもあるのです。"},{"id":{"d":475,"n":4,"t":5},"speaker":"B","text":"それは知りませんでした。そのようなところもあるのですか？"},{"id":{"d":475,"n":5,"t":5},"speaker":"A","text":"例えば、弘法大師が鬼を追い出したという伝承のある村では、鬼を敵対視しなくなって「鬼は内」と言うそうです。"},{"id":{"d":475,"n":6,"t":5},"speaker":"B","text":"なるほど、われわれは当たり前に、鬼は怖いものと思い、「鬼は外」と言っていますけれどね。"},{"id":{"d":475,"n":7,"t":5},"speaker":"A","text":"他にも、いろいろな理由で「鬼は内」と言うところがあるようです。"},{"id":{"d":475,"n":8,"t":5},"speaker":"B","text":"必ずしも、鬼は怖いものではないわけですね。"}];
INSERT INTO sentence [{"id":{"d":476,"n":1,"t":5},"speaker":"A","text":"しばらく、カラオケに行っていませんね。"},{"id":{"d":476,"n":2,"t":5},"speaker":"B","text":"新型コロナウイルス感染症の流行が始まった頃、カラオケスナックで集団感染が起こりました。"},{"id":{"d":476,"n":3,"t":5},"speaker":"A","text":"そうでした。テレビのニュース番組で放送されて、「カラオケは悪者」のように言われましたね。"},{"id":{"d":476,"n":4,"t":5},"speaker":"B","text":"私たちカラオケファンは、いたたまれない気分になりました。"},{"id":{"d":476,"n":5,"t":5},"speaker":"A","text":"カラオケが悪いのではないのですけれどね。"},{"id":{"d":476,"n":6,"t":5},"speaker":"B","text":"先日、カラオケ・カラヤンのオーナーに会いましたが、「閉鎖しようかどうか迷っている」と言っていました。"},{"id":{"d":476,"n":7,"t":5},"speaker":"A","text":"それはお気の毒ですね。気の置けないカラオケスナックですのに、残念ですね。"},{"id":{"d":476,"n":8,"t":5},"speaker":"B","text":"何か元気づけるようなことをしてあげたいです。"},{"id":{"d":476,"n":9,"t":5},"speaker":"A","text":"お客として行くのが一番なのですが、このような状況では、それもままならなりません。"},{"id":{"d":476,"n":10,"t":5},"speaker":"B","text":"どのようにしてか、持ちこたえてほしいものです。"}];
INSERT INTO sentence [{"id":{"d":477,"n":1,"t":5},"speaker":"A","text":"気象予報士の丹波さん、週末は大雪になりそうですね。"},{"id":{"d":477,"n":2,"t":5},"speaker":"B","text":"はい、典型的な冬型の気圧配置で、北海道や東北では猛烈な吹雪になりそうです。"},{"id":{"d":477,"n":3,"t":5},"speaker":"A","text":"雪は、どのくらい降りそうですか？"},{"id":{"d":477,"n":4,"t":5},"speaker":"B","text":"今夜から降り始めて、24時間の降雪量が、多いところで80センチメートルになります。"},{"id":{"d":477,"n":5,"t":5},"speaker":"A","text":"吹雪になるところもあると聞いていますが、どのくらいの風速になるのですか？"},{"id":{"d":477,"n":6,"t":5},"speaker":"B","text":"襟裳岬では風速35メートルくらいになります。"},{"id":{"d":477,"n":7,"t":5},"speaker":"A","text":"それは台風並みではありませんか。いわゆるホワイトアウトで目の前が見えなくなるそうですが、交通事故が心配ですね。"},{"id":{"d":477,"n":8,"t":5},"speaker":"B","text":"車の運転は、慎重の上にも慎重を期していただきたいと思います。不要不急の用事でもない限り、自動車の運転は控えた方が良さそうです。"},{"id":{"d":477,"n":9,"t":5},"speaker":"A","text":"この大雪はいつまで続きそうですか？"},{"id":{"d":477,"n":10,"t":5},"speaker":"B","text":"あさってには、冬型の気圧配置も緩むので、そうなったら雪も収まると思います。"}];
INSERT INTO sentence [{"id":{"d":478,"n":1,"t":5},"speaker":"A","text":"クリスマスイブは、どのように過ごしますか？"},{"id":{"d":478,"n":2,"t":5},"speaker":"B","text":"今年はコロナ禍ですし、自宅で過ごします。"},{"id":{"d":478,"n":3,"t":5},"speaker":"A","text":"去年もそうでした。2年続けて自宅でのクリスマスイブですね。"},{"id":{"d":478,"n":4,"t":5},"speaker":"B","text":"毎年、回り持ちでパーティーを開いていた頃が懐かしいです。"},{"id":{"d":478,"n":5,"t":5},"speaker":"A","text":"あの頃の仲間と、連絡を取っていますか？"},{"id":{"d":478,"n":6,"t":5},"speaker":"B","text":"はい。最近、深町さんと電話で話しました。"},{"id":{"d":478,"n":7,"t":5},"speaker":"A","text":"どのような様子でしたか？"},{"id":{"d":478,"n":8,"t":5},"speaker":"B","text":"やはり懐かしがっていて、「また、みんなでパーティーを開きたい」と言っていました。"},{"id":{"d":478,"n":9,"t":5},"speaker":"A","text":"来年こそはコロナウイルス感染症の流行が終息して、また、パーティーを開けるようになると良いのですが。"},{"id":{"d":478,"n":10,"t":5},"speaker":"B","text":"そう願いたいですね。"}];
INSERT INTO sentence [{"id":{"d":479,"n":1,"t":5},"speaker":"A","text":"露天風呂に漬かりながら、眺める夕焼けは格別ですね。"},{"id":{"d":479,"n":2,"t":5},"speaker":"B","text":"海が穏やかで、水面に夕焼けが映っています。"},{"id":{"d":479,"n":3,"t":5},"speaker":"A","text":"いろいろ温泉に行きましたが、これほど美しいところは初めてです。"},{"id":{"d":479,"n":4,"t":5},"speaker":"B","text":"そうでしょう。前々から一度お誘いしたかったのです。"},{"id":{"d":479,"n":5,"t":5},"speaker":"A","text":"久々にゆったりした温泉気分に浸れました。"},{"id":{"d":479,"n":6,"t":5},"speaker":"B","text":"この1年間は仕事、仕事で休む暇もなかったですから。"},{"id":{"d":479,"n":7,"t":5},"speaker":"A","text":"そのかいがあって、プロジェクトが成功して良かったです。"},{"id":{"d":479,"n":8,"t":5},"speaker":"B","text":"この後は、この地方の地酒と新鮮な海鮮料理が待っています。"},{"id":{"d":479,"n":9,"t":5},"speaker":"A","text":"そろそろ、夕焼けも終わりそうですね。"},{"id":{"d":479,"n":10,"t":5},"speaker":"B","text":"それでは、宴会といきましょうか。"}];
INSERT INTO sentence [{"id":{"d":480,"n":1,"t":5},"speaker":"A","text":"天気が荒れ模様ですね。明日は大丈夫でしょうか？"},{"id":{"d":480,"n":2,"t":5},"speaker":"B","text":"テレビ番組の天気予報のキャスターが、「明日は晴れる」と言っていました。"},{"id":{"d":480,"n":3,"t":5},"speaker":"A","text":"でも、山の天気は変わりやすいですから、当てにはできません。"},{"id":{"d":480,"n":4,"t":5},"speaker":"B","text":"山の天気は変わりやすいからこそ、この荒れ模様も明日は治まるとも考えられます。"},{"id":{"d":480,"n":5,"t":5},"speaker":"A","text":"なるほど、そういう考え方もありますね。"},{"id":{"d":480,"n":6,"t":5},"speaker":"B","text":"いずれにせよ、山頂を目指すかどうかは、明日になってみないと判断できません。"},{"id":{"d":480,"n":7,"t":5},"speaker":"A","text":"山小屋のご主人の意見も聞いてみる方がいいでしょう。"},{"id":{"d":480,"n":8,"t":5},"speaker":"B","text":"そうですね。山小屋のご主人なら、この辺の天気には詳しいでしょうから。"}];
INSERT INTO sentence [{"id":{"d":481,"n":1,"t":5},"speaker":"A","text":"もうすぐ母の日です。今年は何を贈りましょうか？"},{"id":{"d":481,"n":2,"t":5},"speaker":"B","text":"今年の贈り物を考える前に、去年の贈り物について確認したいと思います。去年は何を贈ったか覚えていますか？"},{"id":{"d":481,"n":3,"t":5},"speaker":"A","text":"もう忘れたのですか？去年は、花とスイーツのセットを贈りました。"},{"id":{"d":481,"n":4,"t":5},"speaker":"B","text":"思い出しました。とても喜んでくれましたね。"},{"id":{"d":481,"n":5,"t":5},"speaker":"A","text":"今年は、何か別のものを贈りたいです。"},{"id":{"d":481,"n":6,"t":5},"speaker":"B","text":"お母さんが欲しがっている物といったら何でしょうか？"},{"id":{"d":481,"n":7,"t":5},"speaker":"A","text":"腰痛対策グッズでしょう。最近、「腰が痛い」とよく言っています。"},{"id":{"d":481,"n":8,"t":5},"speaker":"B","text":"それなら、マッサージ器はいかがでしょう？"},{"id":{"d":481,"n":9,"t":5},"speaker":"A","text":"それは良い考えです。何か心当たりがありますか？"},{"id":{"d":481,"n":10,"t":5},"speaker":"B","text":"クッションタイプのマッサージ器があります。使いやすいし、腰痛にちょうど良いと思います。"},{"id":{"d":481,"n":11,"t":5},"speaker":"A","text":"では、今度の休みに2人で、その商品を探しに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":482,"n":1,"t":5},"speaker":"A","text":"下北沢の劇場で、新しい芝居をしているのを知っていますか？"},{"id":{"d":482,"n":2,"t":5},"speaker":"B","text":"いいえ、知りません。コロナ禍で芝居も制限されていると聞きましたが、下北沢では芝居をしているのですか？"},{"id":{"d":482,"n":3,"t":5},"speaker":"A","text":"そうです。もちろん、コロナウイルス感染症対策は十分にしていると思います。"},{"id":{"d":482,"n":4,"t":5},"speaker":"B","text":"何という芝居ですか？"},{"id":{"d":482,"n":5,"t":5},"speaker":"A","text":"『蜃気楼をめざせ』というタイトルのブラック・コメディーです。"},{"id":{"d":482,"n":6,"t":5},"speaker":"B","text":"興味が湧くタイトルですね。どのような芝居ですか？"},{"id":{"d":482,"n":7,"t":5},"speaker":"A","text":"貧しい若者たちが、力を合わせて巨悪に立ち向かうという芝居です。"},{"id":{"d":482,"n":8,"t":5},"speaker":"B","text":"面白そうなストーリーですね。ますます興味が湧いてきます。もう見たのですか？"},{"id":{"d":482,"n":9,"t":5},"speaker":"A","text":"いえ、まだ見ていません。あなたと、一緒に見に行こうと思っていました。"},{"id":{"d":482,"n":10,"t":5},"speaker":"B","text":"良いですね。芝居はこの2年の間に見る機会がありませんでしたから、久しぶりに見たいです。"},{"id":{"d":482,"n":11,"t":5},"speaker":"A","text":"それでは、チケットを2枚予約しておきます。"}];
INSERT INTO sentence [{"id":{"d":483,"n":1,"t":5},"speaker":"A","text":"11月にアブラゼミを捕まえました。"},{"id":{"d":483,"n":2,"t":5},"speaker":"B","text":"健一君は昆虫に興味があるのですか？"},{"id":{"d":483,"n":3,"t":5},"speaker":"A","text":"はい、昆虫採集をしています。"},{"id":{"d":483,"n":4,"t":5},"speaker":"B","text":"感心ですね。どのような状況でアブラゼミを見つけたのですか？"},{"id":{"d":483,"n":5,"t":5},"speaker":"A","text":"アブラゼミが、ベランダに転がっていたので死んでいるかと思って覗いてみたら、急に動いたので驚きました。"},{"id":{"d":483,"n":6,"t":5},"speaker":"B","text":"アブラゼミは夏の生き物ですよね？それが、冬も近い11月に生きていたとは不思議ですね。"},{"id":{"d":483,"n":7,"t":5},"speaker":"A","text":"はい、僕も不思議に思って電話しました。"},{"id":{"d":483,"n":8,"t":5},"speaker":"B","text":"健一君は何が原因だと思いますか？"},{"id":{"d":483,"n":9,"t":5},"speaker":"A","text":"僕は、原因がよく分かりません。お父さんは、「地球温暖化の影響ではないか」と言っていました。"},{"id":{"d":483,"n":10,"t":5},"speaker":"B","text":"都市のヒートアイランド現象の影響という説もあるのです。健一君は、ヒートアイランド現象を知っていますか？"},{"id":{"d":483,"n":11,"t":5},"speaker":"A","text":"はい、知っています。都市部で緑が減少して地面や空気の熱が奪われにくくなり、熱がこもった状態になることです。"},{"id":{"d":483,"n":12,"t":5},"speaker":"B","text":"その通りです。冬にアブラゼミが生きている理由は、まだよく分かっていないのです。健一君が研究してくれるとうれしいです。"},{"id":{"d":483,"n":13,"t":5},"speaker":"A","text":"分かりました。もっと研究してみます。"}];
INSERT INTO sentence [{"id":{"d":484,"n":1,"t":5},"speaker":"A","text":"この悪天候では、全便欠航になるでしょうね。"},{"id":{"d":484,"n":2,"t":5},"speaker":"B","text":"困りました。私は、今日中に大阪に行かなければいけないのです。"},{"id":{"d":484,"n":3,"t":5},"speaker":"A","text":"私も得意先と約束があり、どうしても行かなければいけません。"},{"id":{"d":484,"n":4,"t":5},"speaker":"B","text":"飛行機が駄目だとなると、新幹線を使うしか方法がありませんね。"},{"id":{"d":484,"n":5,"t":5},"speaker":"A","text":"新幹線が動いていれば良いのですが、動いていなければお手上げです。"},{"id":{"d":484,"n":6,"t":5},"speaker":"B","text":"インターネットで調べてみましょう。"},{"id":{"d":484,"n":7,"t":5},"speaker":"A","text":"どうですか？動いていますか？"},{"id":{"d":484,"n":8,"t":5},"speaker":"B","text":"どうやら動いているようです。"},{"id":{"d":484,"n":9,"t":5},"speaker":"A","text":"良かった。では品川まで大急ぎで戻って、新幹線に乗ることにしましょう。"},{"id":{"d":484,"n":10,"t":5},"speaker":"B","text":"得意先に不義理をしないで済みそうです。助かりました。"}];
INSERT INTO sentence [{"id":{"d":485,"n":1,"t":5},"speaker":"A","text":"梅雨になると、頭が痛くなったり目まいがしたりします。"},{"id":{"d":485,"n":2,"t":5},"speaker":"B","text":"体が、むくんだりしませんか？"},{"id":{"d":485,"n":3,"t":5},"speaker":"A","text":"確かに、体がむくんできます。"},{"id":{"d":485,"n":4,"t":5},"speaker":"B","text":"梅雨時の典型的な症状で、湿気が原因で体に水分がたまっているのです。"},{"id":{"d":485,"n":5,"t":5},"speaker":"A","text":"それなら、その水分を排泄すればよいわけですね。"},{"id":{"d":485,"n":6,"t":5},"speaker":"B","text":"そういうことになります。"},{"id":{"d":485,"n":7,"t":5},"speaker":"A","text":"漢方で何かよく効く薬がありますか？"},{"id":{"d":485,"n":8,"t":5},"speaker":"B","text":"「五苓散」という薬があります。全身の水分代謝をよくします。"},{"id":{"d":485,"n":9,"t":5},"speaker":"A","text":"それでは、その「五苓散」を処方してください。"}];
INSERT INTO sentence [{"id":{"d":486,"n":1,"t":5},"speaker":"A","text":"これが椅子ですか？変わったデザインですね。何だか座り心地が悪そうです。"},{"id":{"d":486,"n":2,"t":5},"speaker":"B","text":"岡本太郎の作った椅子で、作品名は「坐ることを拒否する椅子」です。"},{"id":{"d":486,"n":3,"t":5},"speaker":"A","text":"椅子は座るものですよね？「坐ることを拒否する椅子」とは、椅子の役割に矛盾しているのではありませんか？"},{"id":{"d":486,"n":4,"t":5},"speaker":"B","text":"これは、岡本太郎流の考え方によるものです。"},{"id":{"d":486,"n":5,"t":5},"speaker":"A","text":"どのような考え方ですか？"},{"id":{"d":486,"n":6,"t":5},"speaker":"B","text":"生活を生き生きとしたものにするには、座り心地の良い安楽な椅子よりも、切り株のような椅子の方がいいという考え方のようです。"},{"id":{"d":486,"n":7,"t":5},"speaker":"A","text":"常識の逆を行っているようですね。"},{"id":{"d":486,"n":8,"t":5},"speaker":"B","text":"まさにその通りです。岡本太郎はいつも常識に逆らっていました。"},{"id":{"d":486,"n":9,"t":5},"speaker":"A","text":"そう思ってこの椅子を見ると、かえって座ってみたくなります。"},{"id":{"d":486,"n":10,"t":5},"speaker":"B","text":"どうぞ、ご自由にお座りください。"}];
INSERT INTO sentence [{"id":{"d":487,"n":1,"t":5},"speaker":"A","text":"雨上がりの空がきれいですね。"},{"id":{"d":487,"n":2,"t":5},"speaker":"B","text":"虹も出ていますよ。"},{"id":{"d":487,"n":3,"t":5},"speaker":"A","text":"どこに出ているのですか？"},{"id":{"d":487,"n":4,"t":5},"speaker":"B","text":"あなたの後ろ、ちょうど神社の鳥居の辺りから虹が懸っています。"},{"id":{"d":487,"n":5,"t":5},"speaker":"A","text":"本当ですね。鳥居から背後の森にかけてくっきりと虹が見えますね。"},{"id":{"d":487,"n":6,"t":5},"speaker":"B","text":"まるで、太古の景色を眺めているようです。"},{"id":{"d":487,"n":7,"t":5},"speaker":"A","text":"古代の人々も同じように感動したのでしょうね。"},{"id":{"d":487,"n":8,"t":5},"speaker":"B","text":"たぶん、私たちよりも、もっと大きな感銘を受けたのでしょう。"},{"id":{"d":487,"n":9,"t":5},"speaker":"A","text":"そう考えると、信心深い気持ちになります。"}];
INSERT INTO sentence [{"id":{"d":488,"n":1,"t":5},"speaker":"A","text":"お正月は帰省しましたか？"},{"id":{"d":488,"n":2,"t":5},"speaker":"B","text":"年末から年始にかけて、北海道の実家で過ごしました。"},{"id":{"d":488,"n":3,"t":5},"speaker":"A","text":"年末から帰省したのですか？"},{"id":{"d":488,"n":4,"t":5},"speaker":"B","text":"はい。私の実家では、大みそかにごちそうを食べるのです。"},{"id":{"d":488,"n":5,"t":5},"speaker":"A","text":"そういえば、北海道では、大みそかにおせち料理を食べる習慣があると聞いたことがあります。"},{"id":{"d":488,"n":6,"t":5},"speaker":"B","text":"はい。北海道でも元日におせち料理を食べる家もありますが。私の実家では、大みそかにおせち料理を食べて、元旦にお雑煮を食べます。"},{"id":{"d":488,"n":7,"t":5},"speaker":"A","text":"年越しそばは、食べないのですか？"},{"id":{"d":488,"n":8,"t":5},"speaker":"B","text":"もちろん、食べますよ。大みそかの夕方6時ごろからおせちを食べ始めて、ゆっくり飲んだり食べたりして、11時ごろに年越しそばを食べます。"}];
INSERT INTO sentence [{"id":{"d":489,"n":1,"t":5},"speaker":"A","text":"三連休はどこかに出かけましたか？"},{"id":{"d":489,"n":2,"t":5},"speaker":"B","text":"いいえ。このところ仕事が忙しかったので、家でのんびり過ごしました。"},{"id":{"d":489,"n":3,"t":5},"speaker":"A","text":"ゆっくり休めましたか？"},{"id":{"d":489,"n":4,"t":5},"speaker":"B","text":"はい。録画しておいた海外ドラマを一気に見ました。"},{"id":{"d":489,"n":5,"t":5},"speaker":"A","text":"どのようなドラマが好きですか？"},{"id":{"d":489,"n":6,"t":5},"speaker":"B","text":"特にこだわりはないのですが、そういえば、ミステリードラマをよく見ます。"},{"id":{"d":489,"n":7,"t":5},"speaker":"A","text":"ミステリードラマは続きが気になるから、見始めると止まらなくなりますよね。"},{"id":{"d":489,"n":8,"t":5},"speaker":"B","text":"そうなのです。この週末もシーズン1だけのつもりが、シーズン2まで見てしまいました。"}];
INSERT INTO sentence [{"id":{"d":490,"n":1,"t":5},"speaker":"A","text":"かわいいイラストですね。自分で描いたのですか？"},{"id":{"d":490,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。自分で描きました。"},{"id":{"d":490,"n":3,"t":5},"speaker":"A","text":"絵は、得意なのですか？"},{"id":{"d":490,"n":4,"t":5},"speaker":"B","text":"はい。子どもの頃からお絵描きが好きで、高校では漫画部に入っていました。"},{"id":{"d":490,"n":5,"t":5},"speaker":"A","text":"道理で上手なわけですね。"},{"id":{"d":490,"n":6,"t":5},"speaker":"B","text":"絵を描いていると、楽しくて時間を忘れてしまいます。"},{"id":{"d":490,"n":7,"t":5},"speaker":"A","text":"分かります。好きなことをしていると、あっという今に時間が過ぎてしまいますよね。私も、漫画を読んでいると時間を忘れてしまいます。"},{"id":{"d":490,"n":8,"t":5},"speaker":"B","text":"小山さんも漫画が好きですか？それなら、今度一緒に漫画喫茶に行ってみませんか？"}];
INSERT INTO sentence [{"id":{"d":491,"n":1,"t":5},"speaker":"A","text":"きれいな夕焼けですね。"},{"id":{"d":491,"n":2,"t":5},"speaker":"B","text":"本当ですね。今日は、特別にきれいですね。"},{"id":{"d":491,"n":3,"t":5},"speaker":"A","text":"明日の天気は、きっと晴れですね。"},{"id":{"d":491,"n":4,"t":5},"speaker":"B","text":"夕焼けがきれいだと、次の日は晴れるのですか？"},{"id":{"d":491,"n":5,"t":5},"speaker":"A","text":"昔からの言い伝えです。"},{"id":{"d":491,"n":6,"t":5},"speaker":"B","text":"初めて聞きました。"},{"id":{"d":491,"n":7,"t":5},"speaker":"A","text":"外れることもありますが、当たることが多いのですよ。"},{"id":{"d":491,"n":8,"t":5},"speaker":"B","text":"それならきっと、何らかの理由があるのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":492,"n":1,"t":5},"speaker":"A","text":"雨が降りそうな空模様ですね。"},{"id":{"d":492,"n":2,"t":5},"speaker":"B","text":"そうですね。昨日のテレビ番組の天気予報コーナーでも、今日は「夕方から雨が降る」と言っていました。"},{"id":{"d":492,"n":3,"t":5},"speaker":"A","text":"傘を持ってきましたか？"},{"id":{"d":492,"n":4,"t":5},"speaker":"B","text":"いいえ。それが寝過ごしてしまい、慌てて家を出たので、傘を持ってくるのを忘れてしまいました。"},{"id":{"d":492,"n":5,"t":5},"speaker":"A","text":"傘をお貸ししましょうか？"},{"id":{"d":492,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。でも、急いで家へ傘を取りに帰れば、どうにかなるでしょう。"},{"id":{"d":492,"n":7,"t":5},"speaker":"A","text":"話しているうちに、雨が降り始めました。"},{"id":{"d":492,"n":8,"t":5},"speaker":"B","text":"本当ですね。すみませんが、傘を貸してください。"}];
INSERT INTO sentence [{"id":{"d":493,"n":1,"t":5},"speaker":"A","text":"いつもイヤホンをつけていますね。あなたは、どのような曲を聴いているのですか？"},{"id":{"d":493,"n":2,"t":5},"speaker":"B","text":"大抵はクラシック音楽を聴いています。作曲家の中では、バッハが好きです。"},{"id":{"d":493,"n":3,"t":5},"speaker":"A","text":"私も、バッハの曲はよく聴きます。"},{"id":{"d":493,"n":4,"t":5},"speaker":"B","text":"鈴木さんも、バッハがお好きですか？"},{"id":{"d":493,"n":5,"t":5},"speaker":"A","text":"バッハがお好きな方には申し訳ないのですが、私は、もっぱら仕事中にBGMとして聴いています。"},{"id":{"d":493,"n":6,"t":5},"speaker":"B","text":"確かに、バッハの曲を聴いていると、仕事がはかどりますね。"},{"id":{"d":493,"n":7,"t":5},"speaker":"A","text":"バッハの曲の中でも、仕事中のBGMになるような曲を集めたアルバムがあるのですが、それをダウンロードして聴いています。"},{"id":{"d":493,"n":8,"t":5},"speaker":"B","text":"そのアルバムでしたら、私もCDを持っています。"}];
INSERT INTO sentence [{"id":{"d":494,"n":1,"t":5},"speaker":"A","text":"今日は3月3日、桃の節句ですね。山田さんのお嬢さんは初節句ですか？"},{"id":{"d":494,"n":2,"t":5},"speaker":"B","text":"はい。おかげさまで、無事初節句を迎えることができました。"},{"id":{"d":494,"n":3,"t":5},"speaker":"A","text":"おめでとうございます。生まれてから1歳までは、お祝い事がめじろ押しですね。"},{"id":{"d":494,"n":4,"t":5},"speaker":"B","text":"そうなのです。生後7日目のお七夜に始まって、1カ月目のお宮参り、100日目のお食い初めと大忙しでした。"},{"id":{"d":494,"n":5,"t":5},"speaker":"A","text":"そして今日は初節句、初めてのひな祭りですね。"},{"id":{"d":494,"n":6,"t":5},"speaker":"B","text":"2月の初めにひな人形を飾りました。今夜は、家族でひな祭りのお祝いをします。"},{"id":{"d":494,"n":7,"t":5},"speaker":"A","text":"わが家も今夜はひな祭りのごちそうです。ちらしずしと、はまぐりのお吸い物を作ります。"},{"id":{"d":494,"n":8,"t":5},"speaker":"B","text":"あら、全く同じです。ひな祭りといえばちらしずしですね。"}];
INSERT INTO sentence [{"id":{"d":495,"n":1,"t":5},"speaker":"A","text":"キャンプの最後の晩は、みんなで花火をしませんか？"},{"id":{"d":495,"n":2,"t":5},"speaker":"B","text":"良いですね。でも、このキャンプ場では、打ち上げ花火やロケット花火は禁止されているみたいです。"},{"id":{"d":495,"n":3,"t":5},"speaker":"A","text":"手持ち花火はどうですか？"},{"id":{"d":495,"n":4,"t":5},"speaker":"B","text":"手持ち花火ならできるそうです。"},{"id":{"d":495,"n":5,"t":5},"speaker":"A","text":"それでは、手持ち花火を買って行きましょう。私は、実は線香花火が一番好きなのです。"},{"id":{"d":495,"n":6,"t":5},"speaker":"B","text":"私も線香花火が一番好きです。色取り取りの花火もきれいですが、線香花火は飽きませんよね。"},{"id":{"d":495,"n":7,"t":5},"speaker":"A","text":"火花がきれいに出るように、火玉が落ちないように、持ち方を工夫するのが楽しいですね。"},{"id":{"d":495,"n":8,"t":5},"speaker":"B","text":"同感です。いろいろな線香花火を買って、線香花火大会をしましょう。"}];
INSERT INTO sentence [{"id":{"d":496,"n":1,"t":5},"speaker":"A","text":"もうすぐ、母の日ですね。プレゼントは、用意しましたか？"},{"id":{"d":496,"n":2,"t":5},"speaker":"B","text":"今年は実家に行けないので、カーネーションの鉢植えを送りました。"},{"id":{"d":496,"n":3,"t":5},"speaker":"A","text":"もう送ったのですか？早いですね。"},{"id":{"d":496,"n":4,"t":5},"speaker":"B","text":"最近は、「母の日」ではなく「母の月」にしようという呼びかけがあるようです。"},{"id":{"d":496,"n":5,"t":5},"speaker":"A","text":"そうなのですか。確かに「母の日」というと、「母の日必着」の注文が集中してしまいますからね。"},{"id":{"d":496,"n":6,"t":5},"speaker":"B","text":"なかなか良い考えだと思って、早めにカーネーションの鉢植えを送りました。"},{"id":{"d":496,"n":7,"t":5},"speaker":"A","text":"そういうことなら、私も早めに「母の月」の贈り物を送ることにします。"},{"id":{"d":496,"n":8,"t":5},"speaker":"B","text":"カーネーションですか？"},{"id":{"d":496,"n":9,"t":5},"speaker":"A","text":"いいえ。私の母は「花より団子」なので、お菓子を送ります。"}];
INSERT INTO sentence [{"id":{"d":497,"n":1,"t":5},"speaker":"A","text":"今度の月曜日は、何の祝日ですか？"},{"id":{"d":497,"n":2,"t":5},"speaker":"B","text":"7月の第3月曜日は、海の日です。"},{"id":{"d":497,"n":3,"t":5},"speaker":"A","text":"海の日とは、どういう日なのですか？"},{"id":{"d":497,"n":4,"t":5},"speaker":"B","text":"海の恩恵に感謝するとともに、海洋国日本の繁栄を願う日です。"},{"id":{"d":497,"n":5,"t":5},"speaker":"A","text":"海に囲まれた国ならではの祝日ですね。どのようなことをしますか？"},{"id":{"d":497,"n":6,"t":5},"speaker":"B","text":"特に決まってはいませんが、いろいろな所で海に関するイベントが開かれます。"},{"id":{"d":497,"n":7,"t":5},"speaker":"A","text":"小学生の子どもが喜ぶような過ごし方はありますか？"},{"id":{"d":497,"n":8,"t":5},"speaker":"B","text":"はい。わが家は去年、水族館に連れて行きました。今年は家で海のDVDを見たり、魚料理を食べたりして過ごそうと思っています。"},{"id":{"d":497,"n":9,"t":5},"speaker":"A","text":"水族館はいつもより混みそうですね。テーマを決めて家で楽しむのも良いですね。"}];
INSERT INTO sentence [{"id":{"d":498,"n":1,"t":5},"speaker":"A","text":"おや、七夕のササ飾りですね。"},{"id":{"d":498,"n":2,"t":5},"speaker":"B","text":"はい。今日、図工の時間に作りました。"},{"id":{"d":498,"n":3,"t":5},"speaker":"A","text":"きれいですね。短冊には願い事が書いてあるのですか？"},{"id":{"d":498,"n":4,"t":5},"speaker":"B","text":"はい、「ピアノが上手になりますように」と書きました。"},{"id":{"d":498,"n":5,"t":5},"speaker":"A","text":"ゆみさんは、いつも熱心にピアノの練習をしていますから、きっと上手になりますよ。"},{"id":{"d":498,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":498,"n":7,"t":5},"speaker":"A","text":"それにしても、立派なササですね。"},{"id":{"d":498,"n":8,"t":5},"speaker":"B","text":"はい。学校の近くに住んでいるおじいさんが持って来てくれました。"},{"id":{"d":498,"n":9,"t":5},"speaker":"A","text":"全員分を持ってきてくれたのですか。それは素晴らしいですね。"},{"id":{"d":498,"n":10,"t":5},"speaker":"B","text":"おじいさんが山を持っていて、そこからササを取ってきてくれたそうです。"}];
INSERT INTO sentence [{"id":{"d":499,"n":1,"t":5},"speaker":"A","text":"楽器販売店に寄ってもよいですか？ギターを見たいのです。"},{"id":{"d":499,"n":2,"t":5},"speaker":"B","text":"よいですよ。楽器販売店の商品は見ていて飽きませんし。それに、私もちょうどCDを買いに行きたいと思っていたのです。"},{"id":{"d":499,"n":3,"t":5},"speaker":"A","text":"CDですか？"},{"id":{"d":499,"n":4,"t":5},"speaker":"B","text":"昨日、私が好きなピアニストのアルバムが出たのです。"},{"id":{"d":499,"n":5,"t":5},"speaker":"A","text":"そういえば、鈴木さんはピアノを弾くのでしたね。僕らのバンドでキーボードメンバーを募集中なのですが、入りませんか？"},{"id":{"d":499,"n":6,"t":5},"speaker":"B","text":"お誘いはありがたいのですが、クラシック音楽以外は演奏するのが難しいと思います。"},{"id":{"d":499,"n":7,"t":5},"speaker":"A","text":"そうですか？でもポップスやジャズも得意ですよね。ネットで動画を見ましたよ。"},{"id":{"d":499,"n":8,"t":5},"speaker":"B","text":"見つかってしまいました。実はポップスも好きです。"},{"id":{"d":499,"n":9,"t":5},"speaker":"A","text":"良かったら一度、僕らのバンドの練習に顔を出してください。"}];
INSERT INTO sentence [{"id":{"d":500,"n":1,"t":5},"speaker":"A","text":"今年のバレンタインデーは、土曜日ですね。"},{"id":{"d":500,"n":2,"t":5},"speaker":"B","text":"義理チョコを買わなくて済むから、私は助かります。"},{"id":{"d":500,"n":3,"t":5},"speaker":"A","text":"チョコレートをもらう方も、お返しを考えなくて済むから助かります。"},{"id":{"d":500,"n":4,"t":5},"speaker":"B","text":"アメリカ合衆国でも、バレンタインデーに女性から男性にチョコレートを贈るのですか？"},{"id":{"d":500,"n":5,"t":5},"speaker":"A","text":"いいえ。アメリカ合衆国では、カップルがプレゼントを贈り合います。"},{"id":{"d":500,"n":6,"t":5},"speaker":"B","text":"男性から女性に贈ることもあるのですか？"},{"id":{"d":500,"n":7,"t":5},"speaker":"A","text":"はい、そうです。それに贈るものはチョコレートに限らず、お花やお菓子などいろいろです。"},{"id":{"d":500,"n":8,"t":5},"speaker":"B","text":"日本では、どうしてチョコレートなのでしょう？"},{"id":{"d":500,"n":9,"t":5},"speaker":"A","text":"チョコレート会社の人が提案したから、という説があります。"}];
INSERT INTO sentence [{"id":{"d":501,"n":1,"t":5},"speaker":"A","text":"私の地元では、今週末に七夕祭りがあります。"},{"id":{"d":501,"n":2,"t":5},"speaker":"B","text":"どのようなお祭りですか？"},{"id":{"d":501,"n":3,"t":5},"speaker":"A","text":"駅前の商店街に、色取り取りのササ飾りが飾られます。"},{"id":{"d":501,"n":4,"t":5},"speaker":"B","text":"ササ飾りは誰が作るのですか？"},{"id":{"d":501,"n":5,"t":5},"speaker":"A","text":"地域の学生や団体が出品するのです。今年も、200本ほど集まったそうです。"},{"id":{"d":501,"n":6,"t":5},"speaker":"B","text":"それは壮観でしょうね。私の地元では、8月の末に納涼花火大会があります。"},{"id":{"d":501,"n":7,"t":5},"speaker":"A","text":"花火はどこで見るのですか？"},{"id":{"d":501,"n":8,"t":5},"speaker":"B","text":"わが家のベランダから見ます。"},{"id":{"d":501,"n":9,"t":5},"speaker":"A","text":"そう言えば、お住まいはマンションの8階でしたね。自宅から花火が見えるとはうらやましいです。"},{"id":{"d":501,"n":10,"t":5},"speaker":"B","text":"年に一度だけですが。良かったら遊びにいらしてください。ベランダでビールを飲みながら、花火を見ましょう。"}];
INSERT INTO sentence [{"id":{"d":502,"n":1,"t":5},"speaker":"A","text":"明日は、何の祝日ですか？"},{"id":{"d":502,"n":2,"t":5},"speaker":"B","text":"明日は、春分の日です。"},{"id":{"d":502,"n":3,"t":5},"speaker":"A","text":"春分の日は、毎年3月21日なのですか？"},{"id":{"d":502,"n":4,"t":5},"speaker":"B","text":"いいえ。春分の日は、太陽の動きを基準にして決められるので、毎年同じ日になるわけではありません。例年、3月20日から21日頃です。"},{"id":{"d":502,"n":5,"t":5},"speaker":"A","text":"春分に日にはどのようなことをしますか？"},{"id":{"d":502,"n":6,"t":5},"speaker":"B","text":"お墓参りに行く習慣があります。"},{"id":{"d":502,"n":7,"t":5},"speaker":"A","text":"日本には祝日が多いですね。1年に何日あるのですか？"},{"id":{"d":502,"n":8,"t":5},"speaker":"B","text":"1年に祝日は、16日あります。"}];
INSERT INTO sentence [{"id":{"d":503,"n":1,"t":5},"speaker":"A","text":"大みそかの予定はありますか？"},{"id":{"d":503,"n":2,"t":5},"speaker":"B","text":"特にありません。家でのんびり過ごすと思います。いつものようにテレビで除夜の鐘を聴きながら、年越しをします。"},{"id":{"d":503,"n":3,"t":5},"speaker":"A","text":"除夜の鐘は風情があって、良いですね。"},{"id":{"d":503,"n":4,"t":5},"speaker":"B","text":"108回つくのですよね。一度、お寺で聴いてみたいものだと思っています。"},{"id":{"d":503,"n":5,"t":5},"speaker":"A","text":"それなら、除夜の鐘をつきに行きませんか？"},{"id":{"d":503,"n":6,"t":5},"speaker":"B","text":"お寺の鐘はお坊さんがつくのかと思っていました。つかせてもらえるのですか？"},{"id":{"d":503,"n":7,"t":5},"speaker":"A","text":"はい。わが家の近くのお寺では一般の人も鐘をつきます。"},{"id":{"d":503,"n":8,"t":5},"speaker":"B","text":"それはいいですね。ぜひ、行きたいです。"}];
INSERT INTO sentence [{"id":{"d":504,"n":1,"t":5},"speaker":"A","text":"お正月は楽しかったですか？"},{"id":{"d":504,"n":2,"t":5},"speaker":"B","text":"はい。親戚の家に行って、いとこたちと一緒に遊びました。"},{"id":{"d":504,"n":3,"t":5},"speaker":"A","text":"どのような遊びをしましたか？"},{"id":{"d":504,"n":4,"t":5},"speaker":"B","text":"元日はお天気が良かったので、河原でたこ揚げをしました。"},{"id":{"d":504,"n":5,"t":5},"speaker":"A","text":"河原だと電線もないし、思い切りたこ揚げを楽しめますね。"},{"id":{"d":504,"n":6,"t":5},"speaker":"B","text":"風もちょうど良く吹いていて、とても高くたこが揚がりました。"},{"id":{"d":504,"n":7,"t":5},"speaker":"A","text":"それは良かったですね。家の中ではどのような遊びをしましたか？"},{"id":{"d":504,"n":8,"t":5},"speaker":"B","text":"かるた取りとトランプ、それにテレビゲームもしました。"},{"id":{"d":504,"n":9,"t":5},"speaker":"A","text":"楽しいお正月だったのですね？"},{"id":{"d":504,"n":10,"t":5},"speaker":"B","text":"はい。三が日は、あっという間に終わってしまいました。"}];
INSERT INTO sentence [{"id":{"d":505,"n":1,"t":5},"speaker":"A","text":"何を作っているのですか？"},{"id":{"d":505,"n":2,"t":5},"speaker":"B","text":"鬼のお面を作っています。"},{"id":{"d":505,"n":3,"t":5},"speaker":"A","text":"明日は2月3日ですから、節分ですね。豆まきのお面が必要ですね。"},{"id":{"d":505,"n":4,"t":5},"speaker":"B","text":"はい。今年は私が鬼の役をします。"},{"id":{"d":505,"n":5,"t":5},"speaker":"A","text":"あなたの弟さんと、妹さんが喜ぶでしょうね。"},{"id":{"d":505,"n":6,"t":5},"speaker":"B","text":"はい。今から張り切って「鬼は外、福は内」と、練習しています。"},{"id":{"d":505,"n":7,"t":5},"speaker":"A","text":"楽しい豆まきになりそうですね。それにしても、このお面はよくできていますね。"},{"id":{"d":505,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。お面だけでなく衣装も作りました。"},{"id":{"d":505,"n":9,"t":5},"speaker":"A","text":"準備万端ですね。"}];
INSERT INTO sentence [{"id":{"d":506,"n":1,"t":5},"speaker":"A","text":"今年は、梅雨入りが早いですね。"},{"id":{"d":506,"n":2,"t":5},"speaker":"B","text":"そうですね。平年では、関東甲信越の梅雨入りは6月7日ごろですよね。"},{"id":{"d":506,"n":3,"t":5},"speaker":"A","text":"これから梅雨明けまで、1カ月半くらいは雨が続くのですね。"},{"id":{"d":506,"n":4,"t":5},"speaker":"B","text":"梅雨時は洗濯物が乾きにくいし、憂鬱ですね。"},{"id":{"d":506,"n":5,"t":5},"speaker":"A","text":"私は自転車通学なので、雨の日の登校は大変です。"},{"id":{"d":506,"n":6,"t":5},"speaker":"B","text":"雨の日はどうするのですか？"},{"id":{"d":506,"n":7,"t":5},"speaker":"A","text":"小雨なら、雨ガッパを着て自転車に乗ります。大雨の時は、危ないのでバスに乗ります。家族に車で学校まで送ってもらうこともあります。"},{"id":{"d":506,"n":8,"t":5},"speaker":"B","text":"いずれにしても、朝早くから準備が大変ですね。"},{"id":{"d":506,"n":9,"t":5},"speaker":"A","text":"はい。早く梅雨が明けると良いのですが。"}];
INSERT INTO sentence [{"id":{"d":507,"n":1,"t":5},"speaker":"A","text":"あなたは、いつも電車の中で本を読んでいますね。"},{"id":{"d":507,"n":2,"t":5},"speaker":"B","text":"子どもの頃は人から、「本の虫」と言われていました。今でも出かける時は、本を持っていないと落ち着きません。"},{"id":{"d":507,"n":3,"t":5},"speaker":"A","text":"読書家なのですね。"},{"id":{"d":507,"n":4,"t":5},"speaker":"B","text":"読書家というより、乱読家です。いろいろな本を手当たり次第に読んでいます。"},{"id":{"d":507,"n":5,"t":5},"speaker":"A","text":"あなたは、乱読でも本を読むのですから良いですよ。私の場合は、積読ですから。家には、いつか読もうと積んである本がたくさんあります。"},{"id":{"d":507,"n":6,"t":5},"speaker":"B","text":"どのような本ですか？"},{"id":{"d":507,"n":7,"t":5},"speaker":"A","text":"主に小説です。なかでもミステリー小説が多いです。"},{"id":{"d":507,"n":8,"t":5},"speaker":"B","text":"ミステリー小説は、読み始めると止まらなくなります。"},{"id":{"d":507,"n":9,"t":5},"speaker":"A","text":"そうなのです。うっかりすると一晩中読んでしまいます。そうした理由があるので、なおさら読み始められないのです。"}];
INSERT INTO sentence [{"id":{"d":508,"n":1,"t":5},"speaker":"A","text":"何か音楽をかけましょうか？"},{"id":{"d":508,"n":2,"t":5},"speaker":"B","text":"お願いします。"},{"id":{"d":508,"n":3,"t":5},"speaker":"A","text":"どのような音楽が好きですか？"},{"id":{"d":508,"n":4,"t":5},"speaker":"B","text":"最近は韓国の歌手の曲をよく聴いています。"},{"id":{"d":508,"n":5,"t":5},"speaker":"A","text":"私も聴きます。男性と女性ではどちらが良いでしょうか？"},{"id":{"d":508,"n":6,"t":5},"speaker":"B","text":"どちらも好きですが、男性の歌手の曲を聴くことが多いですね。"},{"id":{"d":508,"n":7,"t":5},"speaker":"A","text":"それでは男性の歌手の曲で、私が好きなものにしましょう。"},{"id":{"d":508,"n":8,"t":5},"speaker":"B","text":"良いですね。あなたがどのような曲が好きなのか楽しみです。"}];
INSERT INTO sentence [{"id":{"d":509,"n":1,"t":5},"speaker":"A","text":"朝から雨が降っていますね。"},{"id":{"d":509,"n":2,"t":5},"speaker":"B","text":"昨日も雨でしたし、そろそろ梅雨入りかもしれません。"},{"id":{"d":509,"n":3,"t":5},"speaker":"A","text":"そういえば沖縄が梅雨入りしたというニュースを見ました。"},{"id":{"d":509,"n":4,"t":5},"speaker":"B","text":"梅雨は嫌いなので、憂鬱です。"},{"id":{"d":509,"n":5,"t":5},"speaker":"A","text":"雨が多くなるからですか？"},{"id":{"d":509,"n":6,"t":5},"speaker":"B","text":"はい。おまけに湿気が多くてカビが生えやすくなるところも困ります。"},{"id":{"d":509,"n":7,"t":5},"speaker":"A","text":"確かにそうですね。"},{"id":{"d":509,"n":8,"t":5},"speaker":"B","text":"食べ物も傷みやすくなるので、気を付けないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":510,"n":1,"t":5},"speaker":"A","text":"月がきれいですよ。"},{"id":{"d":510,"n":2,"t":5},"speaker":"B","text":"どこに見えますか？"},{"id":{"d":510,"n":3,"t":5},"speaker":"A","text":"あのビルの上です。"},{"id":{"d":510,"n":4,"t":5},"speaker":"B","text":"本当にきれいですね。"},{"id":{"d":510,"n":5,"t":5},"speaker":"A","text":"満月ですね。"},{"id":{"d":510,"n":6,"t":5},"speaker":"B","text":"そうですね。オレンジ色でとても大きいです。"},{"id":{"d":510,"n":7,"t":5},"speaker":"A","text":"いつか月に行ってみたいです。"},{"id":{"d":510,"n":8,"t":5},"speaker":"B","text":"行ける日が来るといいですね。"}];
INSERT INTO sentence [{"id":{"d":511,"n":1,"t":5},"speaker":"A","text":"お祭りの屋台では何が好きですか？"},{"id":{"d":511,"n":2,"t":5},"speaker":"B","text":"金魚すくいです。"},{"id":{"d":511,"n":3,"t":5},"speaker":"A","text":"得意なのですか？"},{"id":{"d":511,"n":4,"t":5},"speaker":"B","text":"割と得意です。5匹すくったことがあります。"},{"id":{"d":511,"n":5,"t":5},"speaker":"A","text":"それはすごいですね。私は1匹もすくえたことがありません。"},{"id":{"d":511,"n":6,"t":5},"speaker":"B","text":"あなたはどの屋台が好きですか？"},{"id":{"d":511,"n":7,"t":5},"speaker":"A","text":"焼きそばの屋台が好きです。"},{"id":{"d":511,"n":8,"t":5},"speaker":"B","text":"屋台の焼きそばはおいしいですよね。"}];
INSERT INTO sentence [{"id":{"d":512,"n":1,"t":5},"speaker":"A","text":"お花を買ってきました。"},{"id":{"d":512,"n":2,"t":5},"speaker":"B","text":"きれいですね。どこで買ったのですか？"},{"id":{"d":512,"n":3,"t":5},"speaker":"A","text":"駅前の花屋です。"},{"id":{"d":512,"n":4,"t":5},"speaker":"B","text":"新しくできた花屋ですか？"},{"id":{"d":512,"n":5,"t":5},"speaker":"A","text":"そうです。きれいな花がたくさんあって、思わず買ってしまいました。"},{"id":{"d":512,"n":6,"t":5},"speaker":"B","text":"花瓶に入れて飾っておきましょう。"},{"id":{"d":512,"n":7,"t":5},"speaker":"A","text":"花瓶はありますか？"},{"id":{"d":512,"n":8,"t":5},"speaker":"B","text":"はい。今持ってきますね。"}];
INSERT INTO sentence [{"id":{"d":513,"n":1,"t":5},"speaker":"A","text":"次の日曜日に、友達と遊びに行ってきてもよいですか？"},{"id":{"d":513,"n":2,"t":5},"speaker":"B","text":"どこへ行くのですか？"},{"id":{"d":513,"n":3,"t":5},"speaker":"A","text":"原宿です。"},{"id":{"d":513,"n":4,"t":5},"speaker":"B","text":"行き方は分かりますか？"},{"id":{"d":513,"n":5,"t":5},"speaker":"A","text":"友達が何度も行ったことがあるそうなので、大丈夫です。"},{"id":{"d":513,"n":6,"t":5},"speaker":"B","text":"そうですか。待ち合わせはどこに何時ですか？"},{"id":{"d":513,"n":7,"t":5},"speaker":"A","text":"10時に駅前です。"},{"id":{"d":513,"n":8,"t":5},"speaker":"B","text":"分かりました。当日は、駅まで送りましょうか？"},{"id":{"d":513,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":514,"n":1,"t":5},"speaker":"A","text":"あなた宛てに手紙が届いていますよ。"},{"id":{"d":514,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":514,"n":3,"t":5},"speaker":"A","text":"誰からの手紙ですか？"},{"id":{"d":514,"n":4,"t":5},"speaker":"B","text":"幼なじみからです。バースデーカードを送ってくれました。"},{"id":{"d":514,"n":5,"t":5},"speaker":"A","text":"見せてもらってもよいですか？"},{"id":{"d":514,"n":6,"t":5},"speaker":"B","text":"これです。"},{"id":{"d":514,"n":7,"t":5},"speaker":"A","text":"かわいいカードですね。"},{"id":{"d":514,"n":8,"t":5},"speaker":"B","text":"毎年、お互いの誕生日にカードを送り合っているのです。"},{"id":{"d":514,"n":9,"t":5},"speaker":"A","text":"それは良いですね。"}];
INSERT INTO sentence [{"id":{"d":515,"n":1,"t":5},"speaker":"A","text":"良い匂いがしますね。"},{"id":{"d":515,"n":2,"t":5},"speaker":"B","text":"クッキーを焼いています。"},{"id":{"d":515,"n":3,"t":5},"speaker":"A","text":"お菓子作りが好きなのですね。"},{"id":{"d":515,"n":4,"t":5},"speaker":"B","text":"昔から好きです。趣味のようなものです。"},{"id":{"d":515,"n":5,"t":5},"speaker":"A","text":"今日はどのようなクッキーを作っているのですか？"},{"id":{"d":515,"n":6,"t":5},"speaker":"B","text":"チョコチップクッキーです。焼き上がったら、試食してください。"},{"id":{"d":515,"n":7,"t":5},"speaker":"A","text":"クッキーは大好きなので楽しみです。"},{"id":{"d":515,"n":8,"t":5},"speaker":"B","text":"ぜひ、率直な感想を聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":516,"n":1,"t":5},"speaker":"A","text":"もしもし、おばあちゃんですか？雄介です。"},{"id":{"d":516,"n":2,"t":5},"speaker":"B","text":"ああ、雄介。元気にしていますか？"},{"id":{"d":516,"n":3,"t":5},"speaker":"A","text":"元気です。おばあちゃんも変わりありませんか？"},{"id":{"d":516,"n":4,"t":5},"speaker":"B","text":"ええ。変わりなく過ごしていますよ。今日はどうしたのですか？"},{"id":{"d":516,"n":5,"t":5},"speaker":"A","text":"ゴールデンウィークにおばあちゃんの家に遊びに行きたいのですが、いいですか？"},{"id":{"d":516,"n":6,"t":5},"speaker":"B","text":"もちろん、いいですよ。お父さんかお母さんと一緒に来るのですか？"},{"id":{"d":516,"n":7,"t":5},"speaker":"A","text":"いいえ、今回は一人で行く予定です。"},{"id":{"d":516,"n":8,"t":5},"speaker":"B","text":"それでは、駅まで迎えに行きますね。"},{"id":{"d":516,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。電車の時間が決まったら、また連絡します。"}];
INSERT INTO sentence [{"id":{"d":517,"n":1,"t":5},"speaker":"A","text":"お父さん、勉強しているのですか？"},{"id":{"d":517,"n":2,"t":5},"speaker":"B","text":"勉強ではなく、クロスワードパズルを解いているのです。"},{"id":{"d":517,"n":3,"t":5},"speaker":"A","text":"面白そうですね。"},{"id":{"d":517,"n":4,"t":5},"speaker":"B","text":"一緒に解きますか？"},{"id":{"d":517,"n":5,"t":5},"speaker":"A","text":"はい、解いてみたいです。"},{"id":{"d":517,"n":6,"t":5},"speaker":"B","text":"今は、この問題を考えているところです。"},{"id":{"d":517,"n":7,"t":5},"speaker":"A","text":"難しいですね。"},{"id":{"d":517,"n":8,"t":5},"speaker":"B","text":"そうなのです。時間はあるので、ゆっくりと考えましょう。"}];
INSERT INTO sentence [{"id":{"d":518,"n":1,"t":5},"speaker":"A","text":"江藤さんは、バイオリンを習っているそうですね？"},{"id":{"d":518,"n":2,"t":5},"speaker":"B","text":"はい。週に1回だけですが、バイオリン教室に通っています。"},{"id":{"d":518,"n":3,"t":5},"speaker":"A","text":"長く習っているのですか？"},{"id":{"d":518,"n":4,"t":5},"speaker":"B","text":"2年くらい続けています。"},{"id":{"d":518,"n":5,"t":5},"speaker":"A","text":"何か弾ける曲はありますか？"},{"id":{"d":518,"n":6,"t":5},"speaker":"B","text":"いいえ、まだありません。曲の一部を繰り返し練習している段階です。"},{"id":{"d":518,"n":7,"t":5},"speaker":"A","text":"発表会などに出場することになったら、招待していただきたいです。"},{"id":{"d":518,"n":8,"t":5},"speaker":"B","text":"もしそのような日が来たら、ぜひ聴きに来てください。"}];
INSERT INTO sentence [{"id":{"d":519,"n":1,"t":5},"speaker":"A","text":"立春はいつかを知っていますか？"},{"id":{"d":519,"n":2,"t":5},"speaker":"B","text":"確か、節分の翌日だったと思います。"},{"id":{"d":519,"n":3,"t":5},"speaker":"A","text":"そうなのですね。どのような意味がある日なのですか？"},{"id":{"d":519,"n":4,"t":5},"speaker":"B","text":"春の始まりを意味する日です。"},{"id":{"d":519,"n":5,"t":5},"speaker":"A","text":"詳しいですね。"},{"id":{"d":519,"n":6,"t":5},"speaker":"B","text":"一緒に暮らしている祖母が、よく暦の話をしてくれるのです。"},{"id":{"d":519,"n":7,"t":5},"speaker":"A","text":"自然と身に付いたという感じなのですね。"},{"id":{"d":519,"n":8,"t":5},"speaker":"B","text":"はい。季節感を感じられるので、ありがたく思っています。"}];
INSERT INTO sentence [{"id":{"d":520,"n":1,"t":5},"speaker":"A","text":"スーパーに行ったら、花火が売られていましたよ。"},{"id":{"d":520,"n":2,"t":5},"speaker":"B","text":"花火は大好きです。お母さん、花火を買ってください。"},{"id":{"d":520,"n":3,"t":5},"speaker":"A","text":"買うのはよいですが、どこでやりましょう？"},{"id":{"d":520,"n":4,"t":5},"speaker":"B","text":"公園はどうですか？"},{"id":{"d":520,"n":5,"t":5},"speaker":"A","text":"この近所の公園では、花火は禁止されているようです。"},{"id":{"d":520,"n":6,"t":5},"speaker":"B","text":"そうなのですか。それは残念です。"},{"id":{"d":520,"n":7,"t":5},"speaker":"A","text":"どこかに、広くて花火ができるところがあるといいのですが。"},{"id":{"d":520,"n":8,"t":5},"speaker":"B","text":"海岸はどうでしょうか？"},{"id":{"d":520,"n":9,"t":5},"speaker":"A","text":"海岸はいいですね。近くはありませんが、車でなら行くことができます。"}];
INSERT INTO sentence [{"id":{"d":521,"n":1,"t":5},"speaker":"A","text":"大事な話があるのですが、聞いてもらえますか？"},{"id":{"d":521,"n":2,"t":5},"speaker":"B","text":"急にどうしたのですか？"},{"id":{"d":521,"n":3,"t":5},"speaker":"A","text":"驚かないで聞いてくださいね。"},{"id":{"d":521,"n":4,"t":5},"speaker":"B","text":"そう言われると緊張してしまいます。どのような話ですか？"},{"id":{"d":521,"n":5,"t":5},"speaker":"A","text":"実は、家族の都合で転校することになって、今日がこの学校に通う最後の日なのです。"},{"id":{"d":521,"n":6,"t":5},"speaker":"B","text":"それは本当ですか？"},{"id":{"d":521,"n":7,"t":5},"speaker":"A","text":"ごめんなさい、うそです。今日はエイプリルフールです。"},{"id":{"d":521,"n":8,"t":5},"speaker":"B","text":"そうでした。驚きましたよ。でも、うそで安心しました。"}];
INSERT INTO sentence [{"id":{"d":522,"n":1,"t":5},"speaker":"A","text":"週末は何をしていましたか？"},{"id":{"d":522,"n":2,"t":5},"speaker":"B","text":"ずっとゲームをしていました。"},{"id":{"d":522,"n":3,"t":5},"speaker":"A","text":"どのようなゲームですか？"},{"id":{"d":522,"n":4,"t":5},"speaker":"B","text":"ロールプレイングゲームです。"},{"id":{"d":522,"n":5,"t":5},"speaker":"A","text":"私もロールプレイングゲームが大好きです。"},{"id":{"d":522,"n":6,"t":5},"speaker":"B","text":"『ドラゴンクエスト』シリーズは好きですか？"},{"id":{"d":522,"n":7,"t":5},"speaker":"A","text":"もちろんです。時間を忘れてプレイしていました。"},{"id":{"d":522,"n":8,"t":5},"speaker":"B","text":"私もそうです。この週末も、ゲームであっという間に時間が過ぎました。"}];
INSERT INTO sentence [{"id":{"d":523,"n":1,"t":5},"speaker":"A","text":"あなたは日記を書いていますか？"},{"id":{"d":523,"n":2,"t":5},"speaker":"B","text":"はい。若い頃から毎日書いています。"},{"id":{"d":523,"n":3,"t":5},"speaker":"A","text":"素晴らしいですね。毎日だと、書くことがない日もあるのではありませんか？"},{"id":{"d":523,"n":4,"t":5},"speaker":"B","text":"特に何もない日は、起きた時間や食事内容などを記録しています。"},{"id":{"d":523,"n":5,"t":5},"speaker":"A","text":"それなら、書くことに困りませんね。"},{"id":{"d":523,"n":6,"t":5},"speaker":"B","text":"あなたも日記を書いているのですか？"},{"id":{"d":523,"n":7,"t":5},"speaker":"A","text":"いいえ、何度か挑戦しましたがいつも三日坊主です。"},{"id":{"d":523,"n":8,"t":5},"speaker":"B","text":"書く時間を決めると、習慣になりやすいですよ。"},{"id":{"d":523,"n":9,"t":5},"speaker":"A","text":"そうなのですね。今度試してみます。"}];
INSERT INTO sentence [{"id":{"d":524,"n":1,"t":5},"speaker":"A","text":"カラオケに行きたいです。"},{"id":{"d":524,"n":2,"t":5},"speaker":"B","text":"次の休みの日に一緒に行きましょうか？"},{"id":{"d":524,"n":3,"t":5},"speaker":"A","text":"ぜひ行きましょう。私たち2人でカラオケに行くのは初めてですね。"},{"id":{"d":524,"n":4,"t":5},"speaker":"B","text":"そういえば、いつもは大勢で行っていましたね。"},{"id":{"d":524,"n":5,"t":5},"speaker":"A","text":"2人だとたくさん歌えそうです。"},{"id":{"d":524,"n":6,"t":5},"speaker":"B","text":"そうですね、交代で歌えますからね。"},{"id":{"d":524,"n":7,"t":5},"speaker":"A","text":"次の休みが楽しみです。"},{"id":{"d":524,"n":8,"t":5},"speaker":"B","text":"私も楽しみです。何を歌うかを考えておきます。"}];
INSERT INTO sentence [{"id":{"d":525,"n":1,"t":5},"speaker":"A","text":"最後に映画を見たのはいつですか？"},{"id":{"d":525,"n":2,"t":5},"speaker":"B","text":"去年の夏休みです。"},{"id":{"d":525,"n":3,"t":5},"speaker":"A","text":"随分前ですね。"},{"id":{"d":525,"n":4,"t":5},"speaker":"B","text":"家の近くに映画館がないので、頻繁には見ません。"},{"id":{"d":525,"n":5,"t":5},"speaker":"A","text":"一番近い映画館までは、どのくらいかかるのですか？"},{"id":{"d":525,"n":6,"t":5},"speaker":"B","text":"車で1時間以上かかります。"},{"id":{"d":525,"n":7,"t":5},"speaker":"A","text":"かなり遠いですね。"},{"id":{"d":525,"n":8,"t":5},"speaker":"B","text":"ええ、そうなのです。もっと近くに映画館があれば、もっと頻繁に映画を見に行けるのですが。"}];
INSERT INTO sentence [{"id":{"d":526,"n":1,"t":5},"speaker":"A","text":"今週は寒くなるようですね。"},{"id":{"d":526,"n":2,"t":5},"speaker":"B","text":"天気予報を見たのですか？"},{"id":{"d":526,"n":3,"t":5},"speaker":"A","text":"はい。雪が降るかもしれないと報じられていました。"},{"id":{"d":526,"n":4,"t":5},"speaker":"B","text":"寒いのは苦手ですが、雪は好きです。"},{"id":{"d":526,"n":5,"t":5},"speaker":"A","text":"私も同じです。"},{"id":{"d":526,"n":6,"t":5},"speaker":"B","text":"雪が降ったら、雪だるまを作りましょうか？"},{"id":{"d":526,"n":7,"t":5},"speaker":"A","text":"いいですね。楽しみになってきました。"},{"id":{"d":526,"n":8,"t":5},"speaker":"B","text":"雪だるまが作れるくらい、たくさん降るといいですね。"}];
INSERT INTO sentence [{"id":{"d":527,"n":1,"t":5},"speaker":"A","text":"梓さんはイラストを描くことが上手ですね。"},{"id":{"d":527,"n":2,"t":5},"speaker":"B","text":"小さい頃から、絵を描くことが好きなのです。"},{"id":{"d":527,"n":3,"t":5},"speaker":"A","text":"絵を習ったことはありますか？"},{"id":{"d":527,"n":4,"t":5},"speaker":"B","text":"いいえ、自己流です。"},{"id":{"d":527,"n":5,"t":5},"speaker":"A","text":"習ったことがないのに、そのように上手に描けるものなのですね。"},{"id":{"d":527,"n":6,"t":5},"speaker":"B","text":"好きだからたくさん描いているだけですよ。"},{"id":{"d":527,"n":7,"t":5},"speaker":"A","text":"将来はイラストレーターになるのですか？"},{"id":{"d":527,"n":8,"t":5},"speaker":"B","text":"そうなれたらいいなと思っています。"}];
INSERT INTO sentence [{"id":{"d":528,"n":1,"t":5},"speaker":"A","text":"明日は何か予定がありますか？"},{"id":{"d":528,"n":2,"t":5},"speaker":"B","text":"特にありません。"},{"id":{"d":528,"n":3,"t":5},"speaker":"A","text":"ボウリングに行きませんか？"},{"id":{"d":528,"n":4,"t":5},"speaker":"B","text":"いいですね。行きたいです。"},{"id":{"d":528,"n":5,"t":5},"speaker":"A","text":"午前と午後では、どちらがいいですか？"},{"id":{"d":528,"n":6,"t":5},"speaker":"B","text":"起きるのが遅いので、午後がいいです。"},{"id":{"d":528,"n":7,"t":5},"speaker":"A","text":"それでは午後にしましょう。待ち合わせは1時に駅前でよいですか？"},{"id":{"d":528,"n":8,"t":5},"speaker":"B","text":"はい。大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":529,"n":1,"t":5},"speaker":"A","text":"今日は風が強いですね。"},{"id":{"d":529,"n":2,"t":5},"speaker":"B","text":"はい。髪が乱れてしまいました。"},{"id":{"d":529,"n":3,"t":5},"speaker":"A","text":"私はコンタクトレンズなので、大変でした。"},{"id":{"d":529,"n":4,"t":5},"speaker":"B","text":"目にごみが入ってしまったのですか？"},{"id":{"d":529,"n":5,"t":5},"speaker":"A","text":"そうです。痛くて目が開けられなくなってしまいました。"},{"id":{"d":529,"n":6,"t":5},"speaker":"B","text":"災難でしたね。大丈夫でしたか？"},{"id":{"d":529,"n":7,"t":5},"speaker":"A","text":"先ほど、コンタクトレンズを外して眼鏡に替えました。"},{"id":{"d":529,"n":8,"t":5},"speaker":"B","text":"今日は眼鏡で過ごした方がよさそうですね。"}];
INSERT INTO sentence [{"id":{"d":530,"n":1,"t":5},"speaker":"A","text":"久しぶりにトランプをしませんか？"},{"id":{"d":530,"n":2,"t":5},"speaker":"B","text":"ぜひしましょう。何のゲームをしますか？"},{"id":{"d":530,"n":3,"t":5},"speaker":"A","text":"ポーカーのやり方は知っていますか？"},{"id":{"d":530,"n":4,"t":5},"speaker":"B","text":"もちろん、知っています。"},{"id":{"d":530,"n":5,"t":5},"speaker":"A","text":"では、ポーカーにしましょう。"},{"id":{"d":530,"n":6,"t":5},"speaker":"B","text":"少し待ってください。せっかくなら、お父さんとお母さんも入れて4人でしませんか？"},{"id":{"d":530,"n":7,"t":5},"speaker":"A","text":"いいですね。その方が楽しそうです。"},{"id":{"d":530,"n":8,"t":5},"speaker":"B","text":"今、2人を呼んできます。"},{"id":{"d":530,"n":9,"t":5},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":531,"n":1,"t":5},"speaker":"A","text":"時間があるときはどのようなことをしていますか？"},{"id":{"d":531,"n":2,"t":5},"speaker":"B","text":"だいたいは、本を読んでいます。"},{"id":{"d":531,"n":3,"t":5},"speaker":"A","text":"どのようなジャンルの本が好きですか？"},{"id":{"d":531,"n":4,"t":5},"speaker":"B","text":"SFやファンタジーが好きです。"},{"id":{"d":531,"n":5,"t":5},"speaker":"A","text":"1カ月に何冊くらい読みますか？"},{"id":{"d":531,"n":6,"t":5},"speaker":"B","text":"2、3冊くらいです。"},{"id":{"d":531,"n":7,"t":5},"speaker":"A","text":"それだけ読むと、部屋が本だらけになってしまいませんか？"},{"id":{"d":531,"n":8,"t":5},"speaker":"B","text":"本は図書館で借りているので、大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":532,"n":1,"t":5},"speaker":"A","text":"あなたが好きな韓国のアイドルが日本でコンサートをするそうです。"},{"id":{"d":532,"n":2,"t":5},"speaker":"B","text":"そうなのですか？ぜひ行きたいです。"},{"id":{"d":532,"n":3,"t":5},"speaker":"A","text":"人気があるので、チケットを手に入れるのは難しそうですよ。"},{"id":{"d":532,"n":4,"t":5},"speaker":"B","text":"そうですよね。何か良い方法はないでしょうか？"},{"id":{"d":532,"n":5,"t":5},"speaker":"A","text":"ファンクラブの会員になっていますか？"},{"id":{"d":532,"n":6,"t":5},"speaker":"B","text":"いいえ、会員ではありません。"},{"id":{"d":532,"n":7,"t":5},"speaker":"A","text":"ファンクラブの会員だと、優先的にチケットを購入できることが多いと聞いたことがあります。"},{"id":{"d":532,"n":8,"t":5},"speaker":"B","text":"私もそれは聞いたことがあります。入会方法を調べてみます。"}];
INSERT INTO sentence [{"id":{"d":533,"n":1,"t":5},"speaker":"A","text":"それは何のチラシですか？"},{"id":{"d":533,"n":2,"t":5},"speaker":"B","text":"花火大会のお知らせです。"},{"id":{"d":533,"n":3,"t":5},"speaker":"A","text":"見せていただけますか？"},{"id":{"d":533,"n":4,"t":5},"speaker":"B","text":"はい、どうぞ。"},{"id":{"d":533,"n":5,"t":5},"speaker":"A","text":"隅田川の花火大会ですね。行ってみたいです。"},{"id":{"d":533,"n":6,"t":5},"speaker":"B","text":"私は行ったことがありますが、大変な混雑でしたよ。"},{"id":{"d":533,"n":7,"t":5},"speaker":"A","text":"そうですか。人混みは苦手なので、その花火大会へ行くのは諦めます。"},{"id":{"d":533,"n":8,"t":5},"speaker":"B","text":"他にもう少し規模の小さい花火大会があるかを調べてみましょう。"}];
INSERT INTO sentence [{"id":{"d":534,"n":1,"t":5},"speaker":"A","text":"音楽がかかっていますね。"},{"id":{"d":534,"n":2,"t":5},"speaker":"B","text":"はい。うるさいですか？"},{"id":{"d":534,"n":3,"t":5},"speaker":"A","text":"いいえ、逆です。とても素晴らしいので、誰の曲かを教えてもらおうと思ったのです。"},{"id":{"d":534,"n":4,"t":5},"speaker":"B","text":"父が持っていたCDを適当にかけたので、あいにく誰の曲かはよく分かりません。"},{"id":{"d":534,"n":5,"t":5},"speaker":"A","text":"そうでしたか。"},{"id":{"d":534,"n":6,"t":5},"speaker":"B","text":"CDのジャケットを見ますか？"},{"id":{"d":534,"n":7,"t":5},"speaker":"A","text":"はい。見せてください。"},{"id":{"d":534,"n":8,"t":5},"speaker":"B","text":"これです。今、3曲目がかかっています。"},{"id":{"d":534,"n":9,"t":5},"speaker":"A","text":"知らないアーティスト名です。インターネットで調べてみることにします。"}];
