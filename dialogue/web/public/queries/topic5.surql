INSERT INTO sentence [{"id":{"d":183,"n":1,"t":5},"speaker":"A","text":"私の趣味は、コントを見て、ホームページにレビューを掲載する事です。"},{"id":{"d":183,"n":2,"t":5},"speaker":"B","text":"どのようなレビューを掲載しているのか、見せてもらえますか？"},{"id":{"d":183,"n":3,"t":5},"speaker":"A","text":"良いですよ。私のホームページにレビューを載せていますので、ぜひ見てください。"},{"id":{"d":183,"n":4,"t":5},"speaker":"B","text":"自宅でゆっくり見てみます。明日は、それぞれのレビューを元にディベートをしましょう。"},{"id":{"d":183,"n":5,"t":5},"speaker":"A","text":"良いですね。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":184,"n":1,"t":5},"speaker":"A","text":"何十年も耳に残るこの曲こそ、名曲にふさわしいと思います。"},{"id":{"d":184,"n":2,"t":5},"speaker":"B","text":"そうですね。誰もが聴いたことがある曲ですものね。"},{"id":{"d":184,"n":3,"t":5},"speaker":"A","text":"はい。曲名や誰が歌っているかは分からなくても、一度は聴いたことがあるはずです。"},{"id":{"d":184,"n":4,"t":5},"speaker":"B","text":"そうですね。まさに名曲です。"},{"id":{"d":184,"n":5,"t":5},"speaker":"A","text":"こういう曲が、何十年何百年と歌い継がれていけば良いですね。"}];
INSERT INTO sentence [{"id":{"d":185,"n":1,"t":5},"speaker":"A","text":"今日は節分です。"},{"id":{"d":185,"n":2,"t":5},"speaker":"B","text":"豆まきをしましょう。"},{"id":{"d":185,"n":3,"t":5},"speaker":"A","text":"鬼役はお父さんにお願いします。お面をかぶって逃げてもらいましょう。"},{"id":{"d":185,"n":4,"t":5},"speaker":"B","text":"私は豆を用意します。思い切り豆を投げて、鬼を退治しましょう。"},{"id":{"d":185,"n":5,"t":5},"speaker":"A","text":"そうですね。そして、わが家に福を呼び込みましょう。"}];
INSERT INTO sentence [{"id":{"d":186,"n":1,"t":5},"speaker":"A","text":"鮮やかな色のコートを探しておりまして、知人にこちらのお店を教えてもらいました。このお店のお洋服はカラーバリエーションが豊富ですね。"},{"id":{"d":186,"n":2,"t":5},"speaker":"B","text":"はい、当店は色鮮やかなお洋服をたくさん取りそろえております。どのような色がお好みですか？"},{"id":{"d":186,"n":3,"t":5},"speaker":"A","text":"私は青系統のお洋服が好きです。"},{"id":{"d":186,"n":4,"t":5},"speaker":"B","text":"そうなのですね。それでは、こちらのコートはいかがですか？"},{"id":{"d":186,"n":5,"t":5},"speaker":"A","text":"きれいな紺色ですね。試着してもよろしいですか？"},{"id":{"d":186,"n":6,"t":5},"speaker":"B","text":"もちろんです。あちらが試着室です。"},{"id":{"d":186,"n":7,"t":5},"speaker":"A","text":"サイズもちょうど良く、とても気に入ったので、購入いたします。"},{"id":{"d":186,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":187,"n":1,"t":5},"speaker":"A","text":"最近、映画鑑賞に夢中です。"},{"id":{"d":187,"n":2,"t":5},"speaker":"B","text":"それは、すてきですね。私も、映画鑑賞がとても好きです。"},{"id":{"d":187,"n":3,"t":5},"speaker":"A","text":"おお、どのような映画を見られますか？"},{"id":{"d":187,"n":4,"t":5},"speaker":"B","text":"主に日本映画とフランス映画、昔のハリウッド映画を見ます。"},{"id":{"d":187,"n":5,"t":5},"speaker":"A","text":"また通好み、という感じがしますね。具体的には、どのような映画を見られるのですか？"},{"id":{"d":187,"n":6,"t":5},"speaker":"B","text":"日本映画だと成瀬巳喜男監督の映画が好きで、フランス映画はフランソワ・トリュフォーの関わった作品が好きです。昔のハリウッド映画は、オードリー・ヘプバーンが出演しているものが好きです。"},{"id":{"d":187,"n":7,"t":5},"speaker":"A","text":"なるほど、すごく趣味の良いセレクトですね。私もちょうど、その辺のものを見たいと思っていたところでした。"},{"id":{"d":187,"n":8,"t":5},"speaker":"B","text":"はい、お薦めの作品なのでぜひ見てください！"}];
INSERT INTO sentence [{"id":{"d":188,"n":1,"t":5},"speaker":"A","text":"太陽は東から上がり、西に沈みます。さて、月の動きはどうでしょうか？"},{"id":{"d":188,"n":2,"t":5},"speaker":"B","text":"月も太陽と同じ動きだと思います。"},{"id":{"d":188,"n":3,"t":5},"speaker":"A","text":"正解です。ただ少しずつ上がる方位が変わるらしいです。"},{"id":{"d":188,"n":4,"t":5},"speaker":"B","text":"そうなのですね。どうしてですか？"},{"id":{"d":188,"n":5,"t":5},"speaker":"A","text":"そこまで詳しく分かりませんが、自転と公転の関係だと思います。"},{"id":{"d":188,"n":6,"t":5},"speaker":"B","text":"宇宙の出来事は、未知のことが多いですね。"}];
INSERT INTO sentence [{"id":{"d":189,"n":1,"t":5},"speaker":"A","text":"今年の母の日のプレゼントは、買いましたか？"},{"id":{"d":189,"n":2,"t":5},"speaker":"B","text":"はい、買いました。母の好きな香水をプレゼントしようと思います。"},{"id":{"d":189,"n":3,"t":5},"speaker":"A","text":"それは、いいアイデアですね。ちなみにどちらの香水をプレゼントする予定ですか？"},{"id":{"d":189,"n":4,"t":5},"speaker":"B","text":"DIORの「ミス ディオール ローズ エッセンス」です。"},{"id":{"d":189,"n":5,"t":5},"speaker":"A","text":"あら、お母さまはとてもおしゃれな方なのですね。"},{"id":{"d":189,"n":6,"t":5},"speaker":"B","text":"はい、母は昔からおしゃれをするのが好きです。出かける時にきれいな格好をして、良い香りをつけるのが好きなのです。"},{"id":{"d":189,"n":7,"t":5},"speaker":"A","text":"きっと、香水のプレゼントは、あなたのお母さまに喜んでいただけますね！"},{"id":{"d":189,"n":8,"t":5},"speaker":"B","text":"はい、喜んでくれると良いなと思っています。"}];
INSERT INTO sentence [{"id":{"d":190,"n":1,"t":5},"speaker":"A","text":"明日のクリスマスイブを、一緒に過ごしませんか？"},{"id":{"d":190,"n":2,"t":5},"speaker":"B","text":"仕事があるので、難しいです。"},{"id":{"d":190,"n":3,"t":5},"speaker":"A","text":"そうなのですね。残念です。"},{"id":{"d":190,"n":4,"t":5},"speaker":"B","text":"でも次の日のクリスマスは、一緒に過ごすことができると思います。"},{"id":{"d":190,"n":5,"t":5},"speaker":"A","text":"それはうれしいです。それではクリスマスパーティーは、25日にしましょう。"}];
INSERT INTO sentence [{"id":{"d":191,"n":1,"t":5},"speaker":"A","text":"布団から出たくない季節になりました。真冬に突入ですね。"},{"id":{"d":191,"n":2,"t":5},"speaker":"B","text":"そうですね。本当に寒い季節になり、起きることに苦労をしています。"},{"id":{"d":191,"n":3,"t":5},"speaker":"A","text":"何か気持ち良く起きられる良い方法はありませんか？"},{"id":{"d":191,"n":4,"t":5},"speaker":"B","text":"湯たんぽや電気毛布で布団を暖かくしたら良いと思います。"},{"id":{"d":191,"n":5,"t":5},"speaker":"A","text":"なるほど、試してみます。"}];
INSERT INTO sentence [{"id":{"d":192,"n":1,"t":5},"speaker":"A","text":"砂むし温泉をご存じですか？とても健康に良いそうです。"},{"id":{"d":192,"n":2,"t":5},"speaker":"B","text":"どういう温泉なのですか？"},{"id":{"d":192,"n":3,"t":5},"speaker":"A","text":"砂浜に体を埋めて、地下からの蒸気熱で温まるという温泉です。"},{"id":{"d":192,"n":4,"t":5},"speaker":"B","text":"それはとても温まりそうですね。"},{"id":{"d":192,"n":5,"t":5},"speaker":"A","text":"確かにそうですね。汗をたくさんかくそうです。"},{"id":{"d":192,"n":6,"t":5},"speaker":"B","text":"寒い冬には良さそうですね。"},{"id":{"d":192,"n":7,"t":5},"speaker":"A","text":"冬はもちろんのこと、1年を通して、とても気持ちが良いとのことです。温泉を出た後の水風呂が、また格別です。"},{"id":{"d":192,"n":8,"t":5},"speaker":"B","text":"それは気持ちが良さそうです。私も興味が湧いてきました。"}];
INSERT INTO sentence [{"id":{"d":193,"n":1,"t":5},"speaker":"A","text":"昨日行った夏祭りは、とても楽しかったですね。"},{"id":{"d":193,"n":2,"t":5},"speaker":"B","text":"はい、とても楽しかったです。浴衣を着て出店で金魚すくいをしましたね。今年は夏の風物詩を堪能できて、本当に幸せです。"},{"id":{"d":193,"n":3,"t":5},"speaker":"A","text":"私も同じ気持ちです。久しぶりに夏祭りに行きましたが、やはり、すごく楽しいなと思いました。"},{"id":{"d":193,"n":4,"t":5},"speaker":"B","text":"こういう時、日本人に生まれて良かったと思いますよね。"},{"id":{"d":193,"n":5,"t":5},"speaker":"A","text":"はい、本当にそう思います。日本文化って良いなと、改めて思いました。"}];
INSERT INTO sentence [{"id":{"d":194,"n":1,"t":5},"speaker":"A","text":"今年のクリスマスは、どのように過ごされるご予定ですか？"},{"id":{"d":194,"n":2,"t":5},"speaker":"B","text":"今年は恋人と、おいしいフランス料理を恵比寿に食べに行く予定です。"},{"id":{"d":194,"n":3,"t":5},"speaker":"A","text":"それは、楽しみですね。ちなみにどちらのお店ですか？"},{"id":{"d":194,"n":4,"t":5},"speaker":"B","text":"「ジョエル・ロブション」というお店です。"},{"id":{"d":194,"n":5,"t":5},"speaker":"A","text":"あの有名店ですね。私も以前行ったことがありますが、内装もすてきで食事もおいしく、すてきな時間が過ごせました。"},{"id":{"d":194,"n":6,"t":5},"speaker":"B","text":"はい、私も前から行ってみたいと思っていたので、念願がかなったという感じです。"},{"id":{"d":194,"n":7,"t":5},"speaker":"A","text":"それは、良いですね。ぜひ、楽しんでいらしてください。"},{"id":{"d":194,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":195,"n":1,"t":5},"speaker":"A","text":"最近、娘がお絵描きに夢中です。ずっとクレヨンとスケッチブックを手離しません。"},{"id":{"d":195,"n":2,"t":5},"speaker":"B","text":"それは、興味深いです。将来、画家になるかもしれませんね。"},{"id":{"d":195,"n":3,"t":5},"speaker":"A","text":"はい、すっかりその気のようで、将来の夢は画家だと言っています。"},{"id":{"d":195,"n":4,"t":5},"speaker":"B","text":"子どもは夢があって良いですね。私も、小さい頃の夢を思い出しました。"},{"id":{"d":195,"n":5,"t":5},"speaker":"A","text":"そうですね。わが子ながら、今後も、夢は大きく持ってほしいと思います。"},{"id":{"d":195,"n":6,"t":5},"speaker":"B","text":"私は、あなたの娘さんのことを応援しています！"}];
INSERT INTO sentence [{"id":{"d":196,"n":1,"t":5},"speaker":"A","text":"私と一緒に、交換日記をしませんか？"},{"id":{"d":196,"n":2,"t":5},"speaker":"B","text":"良いですよ。私は、交換日記にどのようなことを書けば良いですか？"},{"id":{"d":196,"n":3,"t":5},"speaker":"A","text":"その日にあったことを、例えば授業のことやうれしかったことなど、何でも好きなことを書いてください。"},{"id":{"d":196,"n":4,"t":5},"speaker":"B","text":"分かりました。2人だけの日記というのが緊張しますね。"},{"id":{"d":196,"n":5,"t":5},"speaker":"A","text":"そうですね。2人だけの秘密を共有しているみたいで緊張しますね。"}];
INSERT INTO sentence [{"id":{"d":197,"n":1,"t":5},"speaker":"A","text":"この歌は、キーが高すぎて歌えません。"},{"id":{"d":197,"n":2,"t":5},"speaker":"B","text":"それでは、もう少し下げて歌ってみましょう。どうですか？"},{"id":{"d":197,"n":3,"t":5},"speaker":"A","text":"とても歌いやすくなりました。聴いていて変ではありませんか？"},{"id":{"d":197,"n":4,"t":5},"speaker":"B","text":"全く変ではありません。とてもお上手ですよ。"},{"id":{"d":197,"n":5,"t":5},"speaker":"A","text":"そうですか。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":198,"n":1,"t":5},"speaker":"A","text":"今日の月は、きれいな満月です。"},{"id":{"d":198,"n":2,"t":5},"speaker":"B","text":"そうですね。お月見には良い夜ですね。"},{"id":{"d":198,"n":3,"t":5},"speaker":"A","text":"お団子でも、買ってきましょうか？"},{"id":{"d":198,"n":4,"t":5},"speaker":"B","text":"良いアイデアです。そうしましょう。"},{"id":{"d":198,"n":5,"t":5},"speaker":"A","text":"昔から、月ではウサギが餅つきをしている、といういわれがあります。"},{"id":{"d":198,"n":6,"t":5},"speaker":"B","text":"はい、知っています。とてもすてきで、幻想的なお話ですね。"}];
INSERT INTO sentence [{"id":{"d":199,"n":1,"t":5},"speaker":"A","text":"今年は良い年でした。来年もどうぞよろしくお願いします。"},{"id":{"d":199,"n":2,"t":5},"speaker":"B","text":"本当に1年お世話になりました。こちらこそ、来年もどうぞよろしくお願いします。"},{"id":{"d":199,"n":3,"t":5},"speaker":"A","text":"来年も良い年になるように、お正月はゆっくり休みましょうね。"},{"id":{"d":199,"n":4,"t":5},"speaker":"B","text":"そうですね。また正月休み明けにお会いしましょう。"},{"id":{"d":199,"n":5,"t":5},"speaker":"A","text":"はい。良い年をお迎えください。"}];
INSERT INTO sentence [{"id":{"d":200,"n":1,"t":5},"speaker":"A","text":"ようやく雨が上がったようなので、洗濯物が干せます。"},{"id":{"d":200,"n":2,"t":5},"speaker":"B","text":"長らく雨が降り続いていたので、なかなか洗濯物を外に干せなかったですものね。"},{"id":{"d":200,"n":3,"t":5},"speaker":"A","text":"ええ、本当に困っていました。部屋干しは臭いが気になりますので、洗濯物はたまる一方でした。"},{"id":{"d":200,"n":4,"t":5},"speaker":"B","text":"そうですね。でも最近は部屋干しでも、とても良い香りのする洗剤や柔軟剤が売っています。"},{"id":{"d":200,"n":5,"t":5},"speaker":"A","text":"良いことを聞きました。今度買い物に行ったときに、探してみます。"}];
INSERT INTO sentence [{"id":{"d":201,"n":1,"t":5},"speaker":"A","text":"あなたは、手品が上手ですね。"},{"id":{"d":201,"n":2,"t":5},"speaker":"B","text":"私は、随分前から練習していたのです。"},{"id":{"d":201,"n":3,"t":5},"speaker":"A","text":"手先が器用なのですね。"},{"id":{"d":201,"n":4,"t":5},"speaker":"B","text":"それほどでもありませんよ。"},{"id":{"d":201,"n":5,"t":5},"speaker":"A","text":"タネ明かしをしてもらえますか？"},{"id":{"d":201,"n":6,"t":5},"speaker":"B","text":"それはできませんよ。秘密です。"},{"id":{"d":201,"n":7,"t":5},"speaker":"A","text":"では、自分でトリックを見破るのでもう一度、手品をしてください。"},{"id":{"d":201,"n":8,"t":5},"speaker":"B","text":"よいですよ。よく見ていてくださいね。"}];
INSERT INTO sentence [{"id":{"d":202,"n":1,"t":5},"speaker":"A","text":"すてきなひな人形ですね。"},{"id":{"d":202,"n":2,"t":5},"speaker":"B","text":"私が子どもの頃に、両親から買ってもらったものです。"},{"id":{"d":202,"n":3,"t":5},"speaker":"A","text":"思い出の品ですね。"},{"id":{"d":202,"n":4,"t":5},"speaker":"B","text":"はい。毎年欠かさずに飾っています。"},{"id":{"d":202,"n":5,"t":5},"speaker":"A","text":"私も娘が生まれたらひな人形を買ってあげたいです。"},{"id":{"d":202,"n":6,"t":5},"speaker":"B","text":"どのひな人形にするのかを、選ぶのも楽しいでしょうね。"},{"id":{"d":202,"n":7,"t":5},"speaker":"A","text":"娘が大人になっても、大事に飾ってくれたらうれしいです。"},{"id":{"d":202,"n":8,"t":5},"speaker":"B","text":"きっと大切にしてくれると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":203,"n":1,"t":5},"speaker":"A","text":"そろそろ年賀状を書きましょうか？"},{"id":{"d":203,"n":2,"t":5},"speaker":"B","text":"来年の干支は何ですか？"},{"id":{"d":203,"n":3,"t":5},"speaker":"A","text":"未です。"},{"id":{"d":203,"n":4,"t":5},"speaker":"B","text":"では年賀状にヒツジのイラストを描こうと思います。"},{"id":{"d":203,"n":5,"t":5},"speaker":"A","text":"パソコンでイラストを描いて、年賀状に印刷してはどうでしょう？"},{"id":{"d":203,"n":6,"t":5},"speaker":"B","text":"そうですね。パソコンでイラストを描くのは慣れています。"},{"id":{"d":203,"n":7,"t":5},"speaker":"A","text":"宛名や住所は手書きしますか？"},{"id":{"d":203,"n":8,"t":5},"speaker":"B","text":"はい。毎年手書きしているので、今年もそうします。"}];
INSERT INTO sentence [{"id":{"d":204,"n":1,"t":5},"speaker":"A","text":"おお、素晴らしいです！ストライクですね。"},{"id":{"d":204,"n":2,"t":5},"speaker":"B","text":"ボウリングは、ある程度得意なのです。"},{"id":{"d":204,"n":3,"t":5},"speaker":"A","text":"誰かに教わったのですか？"},{"id":{"d":204,"n":4,"t":5},"speaker":"B","text":"はい、父親から教わりました。小さい時から、父親がよくボウリングに連れて行ってくれたのです。"},{"id":{"d":204,"n":5,"t":5},"speaker":"A","text":"そうだったのですね。さて、次は、私の投げる番ですね。上手に投げるコツはありますか？"},{"id":{"d":204,"n":6,"t":5},"speaker":"B","text":"力を抜いて、リラックスしてボールを投げるといいですよ。頑張ってください。"},{"id":{"d":204,"n":7,"t":5},"speaker":"A","text":"頑張ります。"},{"id":{"d":204,"n":8,"t":5},"speaker":"B","text":"8本のピンが倒れましたね。"},{"id":{"d":204,"n":9,"t":5},"speaker":"A","text":"うれしいです。あなたのアドバイスのおかげです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":205,"n":1,"t":5},"speaker":"A","text":"夏と冬では、どちらが好きですか？"},{"id":{"d":205,"n":2,"t":5},"speaker":"B","text":"冬の方が、好きです。"},{"id":{"d":205,"n":3,"t":5},"speaker":"A","text":"なぜですか？"},{"id":{"d":205,"n":4,"t":5},"speaker":"B","text":"北海道出身のせいか、暑いのが苦手です。"},{"id":{"d":205,"n":5,"t":5},"speaker":"A","text":"私も寒い地方で育ちましたが、夏の方が好きです。"},{"id":{"d":205,"n":6,"t":5},"speaker":"B","text":"夏のどのようなところが好きですか？"},{"id":{"d":205,"n":7,"t":5},"speaker":"A","text":"夏は、明るい雰囲気があるところと、マリンスポーツができるところが大好きなのです。"},{"id":{"d":205,"n":8,"t":5},"speaker":"B","text":"あなたは、夏の海で行うスポーツがお好きなのですね。私は反対に、冬のスポーツである、スキーやスノーボードが好きです。"},{"id":{"d":205,"n":9,"t":5},"speaker":"A","text":"育った地域の気候が似ていても、好きな季節や好きなスポーツは人それぞれ違うものですね。"}];
INSERT INTO sentence [{"id":{"d":206,"n":1,"t":5},"speaker":"A","text":"バレンタインデーに友達へチョコレートをあげたいと思います。"},{"id":{"d":206,"n":2,"t":5},"speaker":"B","text":"一緒にチョコレートを買いに行きましょうか？"},{"id":{"d":206,"n":3,"t":5},"speaker":"A","text":"はい。手作りをしてみたいのですが、難しいでしょうか？"},{"id":{"d":206,"n":4,"t":5},"speaker":"B","text":"手作りチョコレートには、簡単に作れるものもあるし、難しいものもありますね。"},{"id":{"d":206,"n":5,"t":5},"speaker":"A","text":"簡単に作れるのは、どのようなものですか？"},{"id":{"d":206,"n":6,"t":5},"speaker":"B","text":"チョコレートを溶かして、型に入れて冷やし固めるものなら簡単です。"},{"id":{"d":206,"n":7,"t":5},"speaker":"A","text":"それに挑戦してみたいです。"},{"id":{"d":206,"n":8,"t":5},"speaker":"B","text":"それでは必要な材料を買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":207,"n":1,"t":5},"speaker":"A","text":"韓国のドラマを見たことはありますか？"},{"id":{"d":207,"n":2,"t":5},"speaker":"B","text":"はい。私は、たくさん見たことがあります。"},{"id":{"d":207,"n":3,"t":5},"speaker":"A","text":"私は、まだ見たことがありません。たくさんの作品があるので、どれを見ればよいか迷ってしまいます。"},{"id":{"d":207,"n":4,"t":5},"speaker":"B","text":"どのようなジャンルのドラマが好きですか？"},{"id":{"d":207,"n":5,"t":5},"speaker":"A","text":"日本のドラマでは、恋愛ドラマをよく見ます。"},{"id":{"d":207,"n":6,"t":5},"speaker":"B","text":"韓国ドラマでも、面白い恋愛ドラマがいろいろありますよ。"},{"id":{"d":207,"n":7,"t":5},"speaker":"A","text":"何か1つ、お薦めの恋愛ドラマを教えてください。"},{"id":{"d":207,"n":8,"t":5},"speaker":"B","text":"はい。私の最近の一番のお薦めは『彼女はキレイだった』という作品です。この作品をきっかけに、韓国ドラマを好きになってもらえたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":208,"n":1,"t":5},"speaker":"A","text":"ギターを習い始めました。"},{"id":{"d":208,"n":2,"t":5},"speaker":"B","text":"何か、ギターを習い始めたきっかけがあるのですか？"},{"id":{"d":208,"n":3,"t":5},"speaker":"A","text":"好きな歌手がギターを弾きながら歌っているところを見て、自分も弾きたいと思ったのです。"},{"id":{"d":208,"n":4,"t":5},"speaker":"B","text":"ギターは買ったのですか？"},{"id":{"d":208,"n":5,"t":5},"speaker":"A","text":"いいえ。家に、父が使っていたギターがありましたので、それを借りています。"},{"id":{"d":208,"n":6,"t":5},"speaker":"B","text":"それなら、お父さまにギターの弾き方を教えていただくのはどうですか？"},{"id":{"d":208,"n":7,"t":5},"speaker":"A","text":"それは、難しいと思います。父はギターを買ったものの、ほとんど弾けないまま止めてしまったそうです。"},{"id":{"d":208,"n":8,"t":5},"speaker":"B","text":"あなたは、目標があるから続けられそうですね。"},{"id":{"d":208,"n":9,"t":5},"speaker":"A","text":"ギターを弾けるようになりたいので、練習を頑張ります。"}];
INSERT INTO sentence [{"id":{"d":209,"n":1,"t":5},"speaker":"A","text":"明日は、祝日ですね。"},{"id":{"d":209,"n":2,"t":5},"speaker":"B","text":"何の日ですか？"},{"id":{"d":209,"n":3,"t":5},"speaker":"A","text":"春分の日です。"},{"id":{"d":209,"n":4,"t":5},"speaker":"B","text":"そうでしたね。これから、少しずつ春らしくなっていきますね。"},{"id":{"d":209,"n":5,"t":5},"speaker":"A","text":"春は大好きな季節なので、待ち遠しいです。"},{"id":{"d":209,"n":6,"t":5},"speaker":"B","text":"春分の日は、どのようなことをする日か、ご存じですか？"},{"id":{"d":209,"n":7,"t":5},"speaker":"A","text":"春彼岸でお墓参りに行く習慣がありますね。"},{"id":{"d":209,"n":8,"t":5},"speaker":"B","text":"それでは私たちも明日お墓参りに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":210,"n":1,"t":5},"speaker":"A","text":"次の週末は何をしましょうか？"},{"id":{"d":210,"n":2,"t":5},"speaker":"B","text":"私は、映画を見に行きたいです。"},{"id":{"d":210,"n":3,"t":5},"speaker":"A","text":"そうですか。映画を見に行くのは久しぶりですね。"},{"id":{"d":210,"n":4,"t":5},"speaker":"B","text":"ちょうど、次の週末から公開される映画があるのです。『キングスマン：ファースト・エージェント』という作品なのですが、知っていますか？"},{"id":{"d":210,"n":5,"t":5},"speaker":"A","text":"最近、テレビなどで広告されている映画ですね。"},{"id":{"d":210,"n":6,"t":5},"speaker":"B","text":"はい。アクション映画なので、ぜひ映画館で見たいです。"},{"id":{"d":210,"n":7,"t":5},"speaker":"A","text":"前売り券を買っておきましょうか？"},{"id":{"d":210,"n":8,"t":5},"speaker":"B","text":"はい。話題作のようなので、その方が安心ですね。"}];
INSERT INTO sentence [{"id":{"d":211,"n":1,"t":5},"speaker":"A","text":"その本は面白いですか？"},{"id":{"d":211,"n":2,"t":5},"speaker":"B","text":"はい。とても面白くて、読んでいるとつい時間を忘れてしまいます。"},{"id":{"d":211,"n":3,"t":5},"speaker":"A","text":"どのようなジャンルの本ですか？"},{"id":{"d":211,"n":4,"t":5},"speaker":"B","text":"推理小説です。"},{"id":{"d":211,"n":5,"t":5},"speaker":"A","text":"私も推理小説をよく読みます。"},{"id":{"d":211,"n":6,"t":5},"speaker":"B","text":"では、この本もぜひ読んでみてください。読み終わったら貸しますから。"},{"id":{"d":211,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。でも犯人を言わないでくださいね。"},{"id":{"d":211,"n":8,"t":5},"speaker":"B","text":"もちろん言いません。推理小説は謎解きが面白いですよね。"}];
INSERT INTO sentence [{"id":{"d":212,"n":1,"t":5},"speaker":"A","text":"由佳さんの誕生会に招待されました。"},{"id":{"d":212,"n":2,"t":5},"speaker":"B","text":"それは良かったですね。誕生会はいつですか？"},{"id":{"d":212,"n":3,"t":5},"speaker":"A","text":"18日の日曜日です。"},{"id":{"d":212,"n":4,"t":5},"speaker":"B","text":"どこで誕生会をするのですか？"},{"id":{"d":212,"n":5,"t":5},"speaker":"A","text":"由佳さんの家です。"},{"id":{"d":212,"n":6,"t":5},"speaker":"B","text":"それまでにプレゼントを用意したいですね。"},{"id":{"d":212,"n":7,"t":5},"speaker":"A","text":"どのようなプレゼントがいいと思いますか？"},{"id":{"d":212,"n":8,"t":5},"speaker":"B","text":"すぐには、思い浮かびませんね。一緒に誕生会に参加する友達と相談してみてはどうですか？"},{"id":{"d":212,"n":9,"t":5},"speaker":"A","text":"それは良い案です。明日、学校で友達に聞いてみます。"}];
INSERT INTO sentence [{"id":{"d":213,"n":1,"t":5},"speaker":"A","text":"昨日はカラオケに行きました。"},{"id":{"d":213,"n":2,"t":5},"speaker":"B","text":"誰と行ったのですか？"},{"id":{"d":213,"n":3,"t":5},"speaker":"A","text":"1人で行きました。"},{"id":{"d":213,"n":4,"t":5},"speaker":"B","text":"そうなのですね。私は1人でカラオケに行ったことがありません。"},{"id":{"d":213,"n":5,"t":5},"speaker":"A","text":"1人でカラオケに行き歌うと、上手に歌えなくても気になりませんから、気楽でいいですよ。"},{"id":{"d":213,"n":6,"t":5},"speaker":"B","text":"歌う練習をしたい時には、良さそうですね。"},{"id":{"d":213,"n":7,"t":5},"speaker":"A","text":"はい。思いきり歌えるので、リフレッシュできます。"},{"id":{"d":213,"n":8,"t":5},"speaker":"B","text":"何時間くらい歌ったのですか？"},{"id":{"d":213,"n":9,"t":5},"speaker":"A","text":"2時間です。1人なのでたくさんの曲を歌うことができました。"}];
INSERT INTO sentence [{"id":{"d":214,"n":1,"t":5},"speaker":"A","text":"ジェットコースターとお化け屋敷では、どちらが苦手ですか？"},{"id":{"d":214,"n":2,"t":5},"speaker":"B","text":"お化け屋敷の方が苦手です。"},{"id":{"d":214,"n":3,"t":5},"speaker":"A","text":"友達と一緒でも怖いですか？"},{"id":{"d":214,"n":4,"t":5},"speaker":"B","text":"怖いです。遊園地に行ってもお化け屋敷には絶対に行きません。"},{"id":{"d":214,"n":5,"t":5},"speaker":"A","text":"私も得意ではありませんが、目をつぶっていればお化け屋敷に行けます。"},{"id":{"d":214,"n":6,"t":5},"speaker":"B","text":"私は、お化け屋敷の音だけでも怖いので、目をつぶっていても我慢できません。"},{"id":{"d":214,"n":7,"t":5},"speaker":"A","text":"本当に苦手なのですね。"},{"id":{"d":214,"n":8,"t":5},"speaker":"B","text":"生まれて初めて行ったお化け屋敷がとても怖くて、それ以来苦手です。"},{"id":{"d":214,"n":9,"t":5},"speaker":"A","text":"トラウマになってしまったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":215,"n":1,"t":5},"speaker":"A","text":"もしもし、武志です。おばあちゃんですか？"},{"id":{"d":215,"n":2,"t":5},"speaker":"B","text":"はい。武志君は、元気にしていますか？"},{"id":{"d":215,"n":3,"t":5},"speaker":"A","text":"はい、元気です。春休みにおばあちゃんの家に泊まりに行ってもよいですか？"},{"id":{"d":215,"n":4,"t":5},"speaker":"B","text":"よいですよ。お父さんかお母さんと一緒に来ますか？"},{"id":{"d":215,"n":5,"t":5},"speaker":"A","text":"僕が1人で行きます。"},{"id":{"d":215,"n":6,"t":5},"speaker":"B","text":"今まで1人で来たことはありませんが、私の家までの交通手段は分かりますか？"},{"id":{"d":215,"n":7,"t":5},"speaker":"A","text":"はい。行き方をお母さんに教えてもらうので大丈夫です。"},{"id":{"d":215,"n":8,"t":5},"speaker":"B","text":"楽しみにしているので、気を付けて来てくださいね。"},{"id":{"d":215,"n":9,"t":5},"speaker":"A","text":"日にちが決まったら、また連絡します。"}];
INSERT INTO sentence [{"id":{"d":216,"n":1,"t":5},"speaker":"A","text":"今日は節分ですね。"},{"id":{"d":216,"n":2,"t":5},"speaker":"B","text":"豆はありますか？"},{"id":{"d":216,"n":3,"t":5},"speaker":"A","text":"はい。昨日買っておきました。"},{"id":{"d":216,"n":4,"t":5},"speaker":"B","text":"豆まきをするのが楽しみです。"},{"id":{"d":216,"n":5,"t":5},"speaker":"A","text":"鬼のお面が欲しいですね。"},{"id":{"d":216,"n":6,"t":5},"speaker":"B","text":"先ほど作ったものが、ここにあります。"},{"id":{"d":216,"n":7,"t":5},"speaker":"A","text":"上手にできていますね。"},{"id":{"d":216,"n":8,"t":5},"speaker":"B","text":"誰が鬼になりますか？"},{"id":{"d":216,"n":9,"t":5},"speaker":"A","text":"お父さんにお願いしましょう。"}];
INSERT INTO sentence [{"id":{"d":217,"n":1,"t":5},"speaker":"A","text":"何を書いているのですか？"},{"id":{"d":217,"n":2,"t":5},"speaker":"B","text":"サンタクロースへの手紙です。"},{"id":{"d":217,"n":3,"t":5},"speaker":"A","text":"クリスマスプレゼントをお願いする手紙ですね。"},{"id":{"d":217,"n":4,"t":5},"speaker":"B","text":"はい。毎年この手紙を書くのが楽しみです。"},{"id":{"d":217,"n":5,"t":5},"speaker":"A","text":"サンタクロースには何をお願いするのですか？"},{"id":{"d":217,"n":6,"t":5},"speaker":"B","text":"欲しいものがあれこれあって、お願いすることを決められません。"},{"id":{"d":217,"n":7,"t":5},"speaker":"A","text":"去年も同じように悩んでいましたね。"},{"id":{"d":217,"n":8,"t":5},"speaker":"B","text":"はい。クリスマスまでまだ日にちがあるのでじっくり考えます。"}];
INSERT INTO sentence [{"id":{"d":218,"n":1,"t":5},"speaker":"A","text":"勤労感謝の日に、お父さんに贈り物をしたいです。"},{"id":{"d":218,"n":2,"t":5},"speaker":"B","text":"そうですね。何を贈りましょうか？"},{"id":{"d":218,"n":3,"t":5},"speaker":"A","text":"勤労感謝の日なので、仕事に関する物がいいと思います。"},{"id":{"d":218,"n":4,"t":5},"speaker":"B","text":"お父さんが仕事で使う物で、プレゼントにふさわしい物を考えましょう。"},{"id":{"d":218,"n":5,"t":5},"speaker":"A","text":"仕事で使う物だと、ボールペン、靴下、ネクタイなどはいかがでしょう？"},{"id":{"d":218,"n":6,"t":5},"speaker":"B","text":"確か、お父さんはネクタイを少ししか持っていなかったはずです。"},{"id":{"d":218,"n":7,"t":5},"speaker":"A","text":"ではネクタイを贈りましょう。"},{"id":{"d":218,"n":8,"t":5},"speaker":"B","text":"お父さんに似合いそうなネクタイが、見つかるといいですね。"}];
INSERT INTO sentence [{"id":{"d":219,"n":1,"t":5},"speaker":"A","text":"ミュージカルは好きですか？"},{"id":{"d":219,"n":2,"t":5},"speaker":"B","text":"大好きですが、どうかしたのですか？"},{"id":{"d":219,"n":3,"t":5},"speaker":"A","text":"友達とミュージカルに行く予定でチケットを買ったのですが、友達に急用ができて行けなくなってしまったのです。良かったら、一緒に行きませんか？"},{"id":{"d":219,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。どのようなミュージカルですか？"},{"id":{"d":219,"n":5,"t":5},"speaker":"A","text":"ブロードウェイでも上演されていて、話題になっている『ウィキッド』という作品です。"},{"id":{"d":219,"n":6,"t":5},"speaker":"B","text":"ぜひ、見たいです。"},{"id":{"d":219,"n":7,"t":5},"speaker":"A","text":"25日の公演チケットなのですが、ご都合はいかがですか？"},{"id":{"d":219,"n":8,"t":5},"speaker":"B","text":"その日は空いています。お誘いいただきありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":220,"n":1,"t":5},"speaker":"A","text":"今年の夏祭りはいつですか？"},{"id":{"d":220,"n":2,"t":5},"speaker":"B","text":"8月の最初の週末です。"},{"id":{"d":220,"n":3,"t":5},"speaker":"A","text":"夏祭りには、浴衣を着て参加したいです。"},{"id":{"d":220,"n":4,"t":5},"speaker":"B","text":"あなたの浴衣は、子どもの頃に買ったものなので、サイズが小さいと思いますよ。"},{"id":{"d":220,"n":5,"t":5},"speaker":"A","text":"そうですね。他に浴衣はないでしょうか？"},{"id":{"d":220,"n":6,"t":5},"speaker":"B","text":"去年、お姉さんが買った浴衣があります。"},{"id":{"d":220,"n":7,"t":5},"speaker":"A","text":"花火大会に行く時に買ったものですね？"},{"id":{"d":220,"n":8,"t":5},"speaker":"B","text":"はい。借りてもよいかどうか、お姉さんに聞いてみてください。"}];
INSERT INTO sentence [{"id":{"d":221,"n":1,"t":5},"speaker":"A","text":"あなたのご趣味は何ですか？"},{"id":{"d":221,"n":2,"t":5},"speaker":"B","text":"絵を描くことです。"},{"id":{"d":221,"n":3,"t":5},"speaker":"A","text":"すてきな趣味ですね。油絵ですか？"},{"id":{"d":221,"n":4,"t":5},"speaker":"B","text":"はい。油絵も水彩画も描きます。"},{"id":{"d":221,"n":5,"t":5},"speaker":"A","text":"どのようなものを描くのですか？"},{"id":{"d":221,"n":6,"t":5},"speaker":"B","text":"私は、風景画を描くことが多いです。"},{"id":{"d":221,"n":7,"t":5},"speaker":"A","text":"写生などをするのですか？"},{"id":{"d":221,"n":8,"t":5},"speaker":"B","text":"はい。休みの日は遠出して写生することがあります。"}];
INSERT INTO sentence [{"id":{"d":222,"n":1,"t":5},"speaker":"A","text":"あちらにこいのぼりが見えます。"},{"id":{"d":222,"n":2,"t":5},"speaker":"B","text":"立派なこいのぼりですね。"},{"id":{"d":222,"n":3,"t":5},"speaker":"A","text":"本当に泳いでいるようですね。"},{"id":{"d":222,"n":4,"t":5},"speaker":"B","text":"こどもの日というと、やはりこいのぼりですね。"},{"id":{"d":222,"n":5,"t":5},"speaker":"A","text":"他にこどもの日らしいものはあるのでしょうか？"},{"id":{"d":222,"n":6,"t":5},"speaker":"B","text":"かぶとを飾る習慣がありますね。"},{"id":{"d":222,"n":7,"t":5},"speaker":"A","text":"こどもの日に食べる物もありますよね？"},{"id":{"d":222,"n":8,"t":5},"speaker":"B","text":"それはかしわ餅のことですね。"},{"id":{"d":222,"n":9,"t":5},"speaker":"A","text":"そうです。話していたらかしわ餅を食べたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":223,"n":1,"t":5},"speaker":"A","text":"本棚に漫画本がたくさんありますね。"},{"id":{"d":223,"n":2,"t":5},"speaker":"B","text":"漫画が好きで、漫画本を集めているのです。"},{"id":{"d":223,"n":3,"t":5},"speaker":"A","text":"全部で何冊くらいありますか？"},{"id":{"d":223,"n":4,"t":5},"speaker":"B","text":"300冊以上あると思います。"},{"id":{"d":223,"n":5,"t":5},"speaker":"A","text":"それは多いですね。驚きました。"},{"id":{"d":223,"n":6,"t":5},"speaker":"B","text":"この家に引っ越す時、漫画本を運ぶのに苦労しました。"},{"id":{"d":223,"n":7,"t":5},"speaker":"A","text":"段ボール箱で何箱分でしたか？"},{"id":{"d":223,"n":8,"t":5},"speaker":"B","text":"20箱以上だったと思います。"},{"id":{"d":223,"n":9,"t":5},"speaker":"A","text":"段ボールの数が多いことに加えて重いので、さぞ大変だったでしょうね。"}];
INSERT INTO sentence [{"id":{"d":224,"n":1,"t":5},"speaker":"A","text":"七夕のササをもらってきました。"},{"id":{"d":224,"n":2,"t":5},"speaker":"B","text":"どなたからいただいたのですか？"},{"id":{"d":224,"n":3,"t":5},"speaker":"A","text":"近所の人から分けてもらいました。"},{"id":{"d":224,"n":4,"t":5},"speaker":"B","text":"七夕飾りを作るのに最適ですね。"},{"id":{"d":224,"n":5,"t":5},"speaker":"A","text":"早速、ササに飾りつけをしましょう。"},{"id":{"d":224,"n":6,"t":5},"speaker":"B","text":"私は折り紙で星を作ります。"},{"id":{"d":224,"n":7,"t":5},"speaker":"A","text":"短冊も必要ですね。"},{"id":{"d":224,"n":8,"t":5},"speaker":"B","text":"去年用意した短冊が残っているはずなので、持ってきます。"},{"id":{"d":224,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":225,"n":1,"t":5},"speaker":"A","text":"高野さんから、お歳暮が届きました。"},{"id":{"d":225,"n":2,"t":5},"speaker":"B","text":"毎年送っていただいて、ありがたいですね。"},{"id":{"d":225,"n":3,"t":5},"speaker":"A","text":"後で、お礼の電話をかけておきます。"},{"id":{"d":225,"n":4,"t":5},"speaker":"B","text":"お願いします。何を頂いたのですか？"},{"id":{"d":225,"n":5,"t":5},"speaker":"A","text":"ハムとソーセージの詰め合わせです。"},{"id":{"d":225,"n":6,"t":5},"speaker":"B","text":"私の大好物です。"},{"id":{"d":225,"n":7,"t":5},"speaker":"A","text":"どのような調理法がいいですか？"},{"id":{"d":225,"n":8,"t":5},"speaker":"B","text":"焼いてください。焼いただけで十分おいしいと思います。"},{"id":{"d":225,"n":9,"t":5},"speaker":"A","text":"そうですね、夕食でいただきましょう。"}];
