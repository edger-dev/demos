INSERT INTO sentence [{"id":{"d":242,"n":1,"t":5},"speaker":"A","text":"佐々木さんは、お笑いが好きでしたよね？"},{"id":{"d":242,"n":2,"t":5},"speaker":"B","text":"はい、好きですよ。どうしてですか？"},{"id":{"d":242,"n":3,"t":5},"speaker":"A","text":"テレビで見た芸人さんが面白かったので、その方のライブに行ってみたくなりました。でも、ライブへ行くことは初めてなので、まずはお笑いについて詳しい人に、お話を聞きたいと思いました。"},{"id":{"d":242,"n":4,"t":5},"speaker":"B","text":"良いですね。私は、お笑いライブにもよく行きますよ。チケットは、買ったのですか？"},{"id":{"d":242,"n":5,"t":5},"speaker":"A","text":"まだです。ライブの日程は調べて、チケットがまだ余っていることは確認しているのですが、なかなかチケットの購入に踏み切れずにいます。"},{"id":{"d":242,"n":6,"t":5},"speaker":"B","text":"そうなのですね。もし良かったら一緒に行きますか？"},{"id":{"d":242,"n":7,"t":5},"speaker":"A","text":"はい。良いのですか？ご一緒していただけると安心です。"},{"id":{"d":242,"n":8,"t":5},"speaker":"B","text":"いくつか候補日を教えてください。お互いに都合が良い日のチケットを買いましょう。"},{"id":{"d":242,"n":9,"t":5},"speaker":"A","text":"うれしいです。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":243,"n":1,"t":5},"speaker":"A","text":"休日の楽しみは何ですか？"},{"id":{"d":243,"n":2,"t":5},"speaker":"B","text":"音楽を聴くことです。"},{"id":{"d":243,"n":3,"t":5},"speaker":"A","text":"どういう媒体で聴くのですか？"},{"id":{"d":243,"n":4,"t":5},"speaker":"B","text":"CDやレコードで聴くこともありますが、YouTubeやサブスクリプションも活用しています。"},{"id":{"d":243,"n":5,"t":5},"speaker":"A","text":"便利な世の中になったものです。音楽を購入するときはどうしているのですか？"},{"id":{"d":243,"n":6,"t":5},"speaker":"B","text":"買うのはCDが多いです。配信も利用しますが、やはりジャケットがあった方が落ち着きます。"},{"id":{"d":243,"n":7,"t":5},"speaker":"A","text":"その感覚はよく分かります。音楽ではありませんが、私も本を購入するときは、電子書籍よりも紙媒体で欲しいと思ってしまいます。"},{"id":{"d":243,"n":8,"t":5},"speaker":"B","text":"ジャケットや装丁も作品の一部ですし、手に取って楽しみたいと思ってしまいますよね。"},{"id":{"d":243,"n":9,"t":5},"speaker":"A","text":"まさにそうなのです。置き場所にさえ困らなければ、全て物理媒体で購入したいところです。"},{"id":{"d":243,"n":10,"t":5},"speaker":"B","text":"電子媒体の良さは場所を取らないところと、アクセスのしやすさにありますよね。今の時代は、両方の良さを享受できるので幸せです。"}];
INSERT INTO sentence [{"id":{"d":244,"n":1,"t":5},"speaker":"A","text":"映画館には、よく行きますか？"},{"id":{"d":244,"n":2,"t":5},"speaker":"B","text":"はい、映画が好きなので結構行きますよ。"},{"id":{"d":244,"n":3,"t":5},"speaker":"A","text":"そうですか。どのくらいの頻度で映画館へ行きますか？"},{"id":{"d":244,"n":4,"t":5},"speaker":"B","text":"月に1日を私だけの映画の日にしています。その日は1日に2～3本の映画を見ますね。"},{"id":{"d":244,"n":5,"t":5},"speaker":"A","text":"それは素晴らしいですね。同じ映画館で、何本も見るのですか？"},{"id":{"d":244,"n":6,"t":5},"speaker":"B","text":"そういうときもありますが、映画館をいくつか、はしごをするときもあります。"},{"id":{"d":244,"n":7,"t":5},"speaker":"A","text":"見る映画は、事前に決めておくのですか？それとも当日に選ぶのですか？"},{"id":{"d":244,"n":8,"t":5},"speaker":"B","text":"どちらの場合もありますよ。でも、基本的には見たい映画と、上映スケジュールを調べて、あらかじめ複数本見る計画を立てることが多いです。"}];
INSERT INTO sentence [{"id":{"d":245,"n":1,"t":5},"speaker":"A","text":"あなたは、テレビドラマをよく見ますか？"},{"id":{"d":245,"n":2,"t":5},"speaker":"B","text":"私は、海外のドラマを、よく見ています。"},{"id":{"d":245,"n":3,"t":5},"speaker":"A","text":"日本のドラマは、見ませんか？"},{"id":{"d":245,"n":4,"t":5},"speaker":"B","text":"そもそも地上波放送のテレビ番組を見ていません。"},{"id":{"d":245,"n":5,"t":5},"speaker":"A","text":"海外ドラマは日本のドラマと、何が違いますか？"},{"id":{"d":245,"n":6,"t":5},"speaker":"B","text":"海外ドラマは、その時の社会状況がストーリーに織り込まれていてリアリティーがあります。"},{"id":{"d":245,"n":7,"t":5},"speaker":"A","text":"私は、日本のドラマと海外ドラマが、それほど違うようには思いませんが。"},{"id":{"d":245,"n":8,"t":5},"speaker":"B","text":"日本のドラマは最初から話数も製作費も決まっていて、全てがテンプレートな感じがします。"},{"id":{"d":245,"n":9,"t":5},"speaker":"A","text":"そう言われると、確かに、日本も以前は放映期間の長いドラマが多かったような気がします。"},{"id":{"d":245,"n":10,"t":5},"speaker":"B","text":"漫画や小説原作の作品も多いと思うのですが、なぜ、ストーリーの結末が分かっているドラマを見るのでしょうか？"},{"id":{"d":245,"n":11,"t":5},"speaker":"A","text":"その理由は考えたこともありませんでした。好きな俳優が出ているドラマであれば、ファンは大抵見るからかもしれません。"}];
INSERT INTO sentence [{"id":{"d":246,"n":1,"t":5},"speaker":"A","text":"芝居のチケットがありますが、見に行きませんか？"},{"id":{"d":246,"n":2,"t":5},"speaker":"B","text":"どのようなお芝居ですか？"},{"id":{"d":246,"n":3,"t":5},"speaker":"A","text":"大学の後輩が主宰する劇団の公演です。あなたは「小劇団に入っていた」とおっしゃっていたので、芝居に興味があるのではないかと思い、お誘いしたのです。"},{"id":{"d":246,"n":4,"t":5},"speaker":"B","text":"チケットが余分にあるということですか？どこの劇団の公演なのでしょう？"},{"id":{"d":246,"n":5,"t":5},"speaker":"A","text":"これです。妻と私の分でチケットを2枚にしておけばよかったのですが、後輩と話しているうちに4枚も買ってしまったのです。"},{"id":{"d":246,"n":6,"t":5},"speaker":"B","text":"この劇団なら知っています。せりふと動きのみの劇が、途中でミュージカルになってしまうのが特徴のようです。2枚ともチケットを頂けるのならうれしいです。"},{"id":{"d":246,"n":7,"t":5},"speaker":"A","text":"はい、もちろんどうぞ。見に行く日は選べますから、事前にホームページか電話で確認しておくといいでしょう。"},{"id":{"d":246,"n":8,"t":5},"speaker":"B","text":"要領は分かっていますので、後で調べてみます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":247,"n":1,"t":5},"speaker":"A","text":"最近、あなたは本を読みますか？"},{"id":{"d":247,"n":2,"t":5},"speaker":"B","text":"昔からミステリー小説は、よく読んでいましたが、最近はご無沙汰です。"},{"id":{"d":247,"n":3,"t":5},"speaker":"A","text":"なぜ、本を読まなくなったのですか？"},{"id":{"d":247,"n":4,"t":5},"speaker":"B","text":"スマートフォンで動画を見たり、情報を調べたりするようになったからかもしれません。"},{"id":{"d":247,"n":5,"t":5},"speaker":"A","text":"スマートフォンの電子書籍用のアプリケーションソフトでも本が読めると思いますが、利用しないのですか？"},{"id":{"d":247,"n":6,"t":5},"speaker":"B","text":"はい。もしかしたら、目が悪くなったのも本を読まなくなった理由かもしれません。"},{"id":{"d":247,"n":7,"t":5},"speaker":"A","text":"スマートフォンやタブレットなら文字を大きくできるので、紙媒体よりも読みやすいです。"},{"id":{"d":247,"n":8,"t":5},"speaker":"B","text":"そうなのですか？お金はかかりますか？"},{"id":{"d":247,"n":9,"t":5},"speaker":"A","text":"どのようなサービスを利用するか次第ですが、昔の作品や試し読みは無料のサービスが多いです。"},{"id":{"d":247,"n":10,"t":5},"speaker":"B","text":"では、帰りの電車で、少し試してみます。"}];
INSERT INTO sentence [{"id":{"d":248,"n":1,"t":5},"speaker":"A","text":"大学の文化祭シーズンですね。どこか、行きたいところはありますか？"},{"id":{"d":248,"n":2,"t":5},"speaker":"B","text":"インターナショナルスクールの文化祭に行ってみたいです。あなたも一緒に行きませんか？"},{"id":{"d":248,"n":3,"t":5},"speaker":"A","text":"良いですね。日本の大学祭とも違って面白そうです。"},{"id":{"d":248,"n":4,"t":5},"speaker":"B","text":"なんでも料理が素晴らしいらしいです。いろいろな国の料理が食べられるかもしれません。"},{"id":{"d":248,"n":5,"t":5},"speaker":"A","text":"ぜひ行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":249,"n":1,"t":5},"speaker":"A","text":"すみません。メンバー募集の掲示板にこのポスターを掲示してほしいのですがお願いできますか？"},{"id":{"d":249,"n":2,"t":5},"speaker":"B","text":"はい、メンバー募集ですね。掲示する方の本人確認をさせていただきたいのですが、何か証明書はお持ちですか？"},{"id":{"d":249,"n":3,"t":5},"speaker":"A","text":"普通免許証で良いですか？"},{"id":{"d":249,"n":4,"t":5},"speaker":"B","text":"はい。確認しましたので、掲示板のお好きなところに、告知用紙を貼ってください。"},{"id":{"d":249,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。一つ確認させていただきたいのですが、掲示期間は決まっていますか？"},{"id":{"d":249,"n":6,"t":5},"speaker":"B","text":"掲示場所がなくなってきたら、古いものから剥がすことになっています。剥がしてもよくなりましたら、電話でも構いませんのでご連絡ください。"}];
INSERT INTO sentence [{"id":{"d":250,"n":1,"t":5},"speaker":"A","text":"最近は、ボードゲームが流行しているらしいです。"},{"id":{"d":250,"n":2,"t":5},"speaker":"B","text":"そういえば、私はボードゲームが出てくるアニメーションを見ましたよ。とても面白かったです。"},{"id":{"d":250,"n":3,"t":5},"speaker":"A","text":"そうでしたか。今度、ボードゲームの専門店に行ってみませんか？"},{"id":{"d":250,"n":4,"t":5},"speaker":"B","text":"良いですね。何か買いたいゲームがあるのでしょうか？"},{"id":{"d":250,"n":5,"t":5},"speaker":"A","text":"1つありますが、もっといろいろ見てみたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":251,"n":1,"t":5},"speaker":"A","text":"昨日テレビを見ていたら、街のサーフショップが紹介されていました。あの店のマスターは君のおじさんですか？"},{"id":{"d":251,"n":2,"t":5},"speaker":"B","text":"はい、そうです。おじは、市のサーフィンクラブで子どもたちや初心者の人たちに教えています。"},{"id":{"d":251,"n":3,"t":5},"speaker":"A","text":"私もサーフィンをしてみたいのですが、サーフボードは借りられますか？"},{"id":{"d":251,"n":4,"t":5},"speaker":"B","text":"もちろん借りられますよ。ただし事前に予約が必要です。"},{"id":{"d":251,"n":5,"t":5},"speaker":"A","text":"なるほど。では、今度連絡してみようと思います。"},{"id":{"d":251,"n":6,"t":5},"speaker":"B","text":"もし、良かったら、学校の帰りにでも、一緒におじの店に行ってみましょうか？"},{"id":{"d":251,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。それは助かります。"}];
INSERT INTO sentence [{"id":{"d":252,"n":1,"t":5},"speaker":"A","text":"カラオケで、あなたがよく歌う歌は何ですか？"},{"id":{"d":252,"n":2,"t":5},"speaker":"B","text":"私は、歌唱曲を、一緒に行く人たちに合わせるようにしています。"},{"id":{"d":252,"n":3,"t":5},"speaker":"A","text":"人に合わせられるのでしたら、いろいろなジャンルの曲を知っているのですね？"},{"id":{"d":252,"n":4,"t":5},"speaker":"B","text":"はい、もともと歌が好きです。カラオケには、1人で行くときもありますよ。"},{"id":{"d":252,"n":5,"t":5},"speaker":"A","text":"1人のときは、何を歌いますか？"},{"id":{"d":252,"n":6,"t":5},"speaker":"B","text":"やっぱり自分が好きな歌を歌います。例えば、Jポップやアニメーションソングなどが好きです。"}];
INSERT INTO sentence [{"id":{"d":253,"n":1,"t":5},"speaker":"A","text":"吉田君、あなたはマンガが好きですか？"},{"id":{"d":253,"n":2,"t":5},"speaker":"B","text":"はい、好きです。人気があるものは、一通り読んでいます。"},{"id":{"d":253,"n":3,"t":5},"speaker":"A","text":"スマートフォンやタブレットで読むのですか？"},{"id":{"d":253,"n":4,"t":5},"speaker":"B","text":"はい。雑誌に連載中のものはそうです。単行本になっているものは、本屋や古書店で購入するときもあります。"},{"id":{"d":253,"n":5,"t":5},"speaker":"A","text":"マンガ本がたまってきたらどうしているのですか？"},{"id":{"d":253,"n":6,"t":5},"speaker":"B","text":"古書店に売っています。マンガ本を手放す際は、部屋を片付けることが目的なので、安い値段がつけられても気にしません。"}];
INSERT INTO sentence [{"id":{"d":254,"n":1,"t":5},"speaker":"A","text":"お客さま、軽自動車規格のワゴン車はこちらにございます。"},{"id":{"d":254,"n":2,"t":5},"speaker":"B","text":"1人キャンプができるような車を探しているのですが、お薦めはありますか？"},{"id":{"d":254,"n":3,"t":5},"speaker":"A","text":"はい。軽自動車ですと、車内で寝られるほどの十分なスペースはないと思いますがよろしいですか？"},{"id":{"d":254,"n":4,"t":5},"speaker":"B","text":"日常生活でも使うので、問題ありません。"},{"id":{"d":254,"n":5,"t":5},"speaker":"A","text":"多目的ということでしたら、ワゴン車は最適です。乗車人数は多い方がよろしいですか？"},{"id":{"d":254,"n":6,"t":5},"speaker":"B","text":"いえ、乗車人数は数名です。むしろ、キャンプ用品や釣り道具を載せられるスペースがきちんと確保できる方がありがたいです。"}];
INSERT INTO sentence [{"id":{"d":255,"n":1,"t":5},"speaker":"A","text":"連休中、掃除洗濯などの家事以外で何をしますか？"},{"id":{"d":255,"n":2,"t":5},"speaker":"B","text":"果実酒造りをします。私は、去年から果実酒造りが趣味です。"},{"id":{"d":255,"n":3,"t":5},"speaker":"A","text":"梅酒くらいしか頭に浮かびませんが、他に、どのようなものがありますか？"},{"id":{"d":255,"n":4,"t":5},"speaker":"B","text":"柑橘系だとレモン、ユズ、ミカン、夏ミカン、他にもリンゴ、イチゴ、パイナップルでも果実酒が造れます。"},{"id":{"d":255,"n":5,"t":5},"speaker":"A","text":"ベースはホワイトリカーですか？"},{"id":{"d":255,"n":6,"t":5},"speaker":"B","text":"そうですね。焼酎系やラム酒、ブランデー、ジンもスタンダードだと思います。度数が高い方が保存には適しています。"}];
INSERT INTO sentence [{"id":{"d":256,"n":1,"t":5},"speaker":"A","text":"古橋さんは、来週の句会に参加されますか？"},{"id":{"d":256,"n":2,"t":5},"speaker":"B","text":"そのつもりです。渡辺さんは参加されますか？"},{"id":{"d":256,"n":3,"t":5},"speaker":"A","text":"私は孫の結婚式があり、出席できそうもありません。"},{"id":{"d":256,"n":4,"t":5},"speaker":"B","text":"お孫さんのご結婚おめでとうございます。でも、渡辺さんはわが町内会で毎回入選作品に選ばれるほど素晴らしい句をお詠みになりますから、句会のご欠席は残念です。"},{"id":{"d":256,"n":5,"t":5},"speaker":"A","text":"いつも選んでいただいて感謝の気持ちでいっぱいですが、私の句は素人の戯れ事です。"},{"id":{"d":256,"n":6,"t":5},"speaker":"B","text":"句会のみんなが感心しているわけですから、それほど謙遜しないでください。"}];
INSERT INTO sentence [{"id":{"d":257,"n":1,"t":5},"speaker":"A","text":"町内会の夏祭りのステージへの出演者が少ないので、町内会長が困っていましたよ。"},{"id":{"d":257,"n":2,"t":5},"speaker":"B","text":"経費をかけられないとお聞きしたのですが、何か良いアイデアはないでしょうか？"},{"id":{"d":257,"n":3,"t":5},"speaker":"A","text":"私たち有志でバンドを組むというアイデアはいかがでしょうか？"},{"id":{"d":257,"n":4,"t":5},"speaker":"B","text":"良いですね。そのメンバーには私も入っているのですか？"},{"id":{"d":257,"n":5,"t":5},"speaker":"A","text":"もちろんです。一緒に夏祭りを盛り上げましょう。"}];
INSERT INTO sentence [{"id":{"d":258,"n":1,"t":5},"speaker":"A","text":"権田さん、ここにいたのですね。映画の途中で出て行ったので探しましたよ。"},{"id":{"d":258,"n":2,"t":5},"speaker":"B","text":"すみません。私は長々としたエンドロールを見ることが嫌なのです。"},{"id":{"d":258,"n":3,"t":5},"speaker":"A","text":"最近はエンドロール後に、少しだけ話があることも少なくありませんよ。"},{"id":{"d":258,"n":4,"t":5},"speaker":"B","text":"知っていますが、それは次回作のお知らせのようなものですから、さほど重要ではないと思っているのです。"},{"id":{"d":258,"n":5,"t":5},"speaker":"A","text":"そう言われてしまうと、反論しにくいのですが、でも、楽しみが1つ増えるかもしれませんよ。"}];
INSERT INTO sentence [{"id":{"d":259,"n":1,"t":5},"speaker":"A","text":"先ほどから山下達郎の曲がかかっていますね。"},{"id":{"d":259,"n":2,"t":5},"speaker":"B","text":"これは、竹内まりやが作詞作曲した曲ですよ。2人の曲は曲調が似ているかもしれません。"},{"id":{"d":259,"n":3,"t":5},"speaker":"A","text":"そういえば、今、海外ではこのジャンルの日本の音楽が流行しているそうです。"},{"id":{"d":259,"n":4,"t":5},"speaker":"B","text":"それは聞いたことがあります。シティポップと呼ばれていますね。"},{"id":{"d":259,"n":5,"t":5},"speaker":"A","text":"外国の方が、かつての日本の音楽に興味があるのは不思議ですね。良い音楽は時代も超えるのでしょう。"}];
INSERT INTO sentence [{"id":{"d":260,"n":1,"t":5},"speaker":"A","text":"最近のご家庭では、ひな人形は飾らないのでしょうか？"},{"id":{"d":260,"n":2,"t":5},"speaker":"B","text":"そのようです。私の家でも飾らなくなって随分たちます。"},{"id":{"d":260,"n":3,"t":5},"speaker":"A","text":"なぜ飾らなくなったのですか？"},{"id":{"d":260,"n":4,"t":5},"speaker":"B","text":"私には3人の娘がおり、娘たちが子どもの頃はひな人形を飾っていました。でも、3人とも就職や進学で家を出た頃から何となく飾らなくなってしまいました。"},{"id":{"d":260,"n":5,"t":5},"speaker":"A","text":"なるほど、家に娘さんがいらっしゃれば、ひな人形を飾ろうという気持ちになるのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":261,"n":1,"t":5},"speaker":"A","text":"あなたは、展覧会にカメラを持って行っているようですが、何を撮影しているのですか？"},{"id":{"d":261,"n":2,"t":5},"speaker":"B","text":"美術館です。私は建築デザインにも興味があるので、美術館自体の写真も必ず撮ることにしています。"},{"id":{"d":261,"n":3,"t":5},"speaker":"A","text":"そうですか。展覧会は、普通は撮影禁止なのに、なぜカメラを持っていくのだろうと不思議に思っていました。"},{"id":{"d":261,"n":4,"t":5},"speaker":"B","text":"国立美術館のような有名設計者の建物も良いですが、個人宅を改造したような美術館も趣があり、すてきです。"}];
INSERT INTO sentence [{"id":{"d":262,"n":1,"t":5},"speaker":"A","text":"あなたは、商店街のくじを何回引けるのですか？"},{"id":{"d":262,"n":2,"t":5},"speaker":"B","text":"私は、3回分の歳末福引券を持っています。あなたは、何回引けますか？"},{"id":{"d":262,"n":3,"t":5},"speaker":"A","text":"私も3回分なのですが、あと2枚あればもう1回引けます。"},{"id":{"d":262,"n":4,"t":5},"speaker":"B","text":"それはちょうど良かったです。2枚余計に持っているのであなたに差し上げます。これでもう1回は、くじが引けますね。"},{"id":{"d":262,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。とても助かります。"}];
INSERT INTO sentence [{"id":{"d":263,"n":1,"t":5},"speaker":"A","text":"先輩、寮内でホラー映画大会をするそうですが、ご存じですか？"},{"id":{"d":263,"n":2,"t":5},"speaker":"B","text":"もちろん知っていますよ。わが寮の恒例行事ですからね。"},{"id":{"d":263,"n":3,"t":5},"speaker":"A","text":"恒例行事にもかかわらず、まだ映画のタイトルを知らされていません。なぜでしょうか？"},{"id":{"d":263,"n":4,"t":5},"speaker":"B","text":"映画のタイトルが分かると面白くないからでしょう。"},{"id":{"d":263,"n":5,"t":5},"speaker":"A","text":"なるほど。では、当日までのお楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":264,"n":1,"t":5},"speaker":"A","text":"僕は、音楽を聴きながら何かをすることができません。君はどのような時に音楽を聴きますか？"},{"id":{"d":264,"n":2,"t":5},"speaker":"B","text":"私は逆です。いつも音楽が鳴っていないと嫌です。私は、勉強の時や、趣味のお菓子作りの時も、音楽をかけています。"},{"id":{"d":264,"n":3,"t":5},"speaker":"A","text":"別の作業をするときに、音楽が流れていると、歌詞が気になったりすることはありませんか？"},{"id":{"d":264,"n":4,"t":5},"speaker":"B","text":"私は外国語は分かりませんから、歌も楽器の1つとして聴いています。洋楽は全部インストゥルメンタルと同じように聴いているので、歌詞が気になることはありませんよ。"}];
INSERT INTO sentence [{"id":{"d":265,"n":1,"t":5},"speaker":"A","text":"トランプがあるのですが、先輩何かゲームをしませんか？"},{"id":{"d":265,"n":2,"t":5},"speaker":"B","text":"2人でできるゲームにはどのようなものがありますか？"},{"id":{"d":265,"n":3,"t":5},"speaker":"A","text":"神経衰弱、スピード、ドボン、それにポーカーもありますね。"},{"id":{"d":265,"n":4,"t":5},"speaker":"B","text":"神経衰弱は、カードの場所を覚えるのが不得意なので、別のものがいいです。"},{"id":{"d":265,"n":5,"t":5},"speaker":"A","text":"では、スピードをやりましょう。"}];
INSERT INTO sentence [{"id":{"d":266,"n":1,"t":5},"speaker":"A","text":"川島さんは、こいのぼりを持っていらっしゃるのですね。"},{"id":{"d":266,"n":2,"t":5},"speaker":"B","text":"主人の父が、孫のために実家から車で運んで来てくれたのです。"},{"id":{"d":266,"n":3,"t":5},"speaker":"A","text":"それは、うらやましいですね。これから揚げるのですか？"},{"id":{"d":266,"n":4,"t":5},"speaker":"B","text":"はい、こいのぼりは大きいので家族総出で揚げなければなりません。"},{"id":{"d":266,"n":5,"t":5},"speaker":"A","text":"大仕事ですが、頑張ってくださいね。風で泳ぐこいのぼりの姿を、拝見することを楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":267,"n":1,"t":5},"speaker":"A","text":"酒井さんは、どちらへお出かけですか？"},{"id":{"d":267,"n":2,"t":5},"speaker":"B","text":"梅林公園まで、散歩です。"},{"id":{"d":267,"n":3,"t":5},"speaker":"A","text":"その、デジタルカメラで何かを撮るのですか？"},{"id":{"d":267,"n":4,"t":5},"speaker":"B","text":"公園で鳥を撮影することが日課になっています。このカメラは娘から借りたものです。"},{"id":{"d":267,"n":5,"t":5},"speaker":"A","text":"お目当ての鳥は、いるのですか？"},{"id":{"d":267,"n":6,"t":5},"speaker":"B","text":"はい。鳥の名前はまだ勉強中なのですが、私が、最初に気になった鳥は、カワセミです。"},{"id":{"d":267,"n":7,"t":5},"speaker":"A","text":"カワセミは、確か、色のきれいな鳥だと記憶しています。公園にいるのですか？"},{"id":{"d":267,"n":8,"t":5},"speaker":"B","text":"はい。公園のそばの川で簡単に見つけられます。今度ご覧になると良いですよ。"}];
INSERT INTO sentence [{"id":{"d":268,"n":1,"t":5},"speaker":"A","text":"正月三が日は何をして過ごしますか？"},{"id":{"d":268,"n":2,"t":5},"speaker":"B","text":"ハードディスクに録画しておいた、アニメーションを見て過ごします。"},{"id":{"d":268,"n":3,"t":5},"speaker":"A","text":"三が日をかけて見るとなると、かなりの数があるようですね。"},{"id":{"d":268,"n":4,"t":5},"speaker":"B","text":"途中で見ることを断念してしまう番組もありますが、それでも多過ぎて追いつきません。"},{"id":{"d":268,"n":5,"t":5},"speaker":"A","text":"面白いアニメーションがあったら、ぜひとも教えてください。"},{"id":{"d":268,"n":6,"t":5},"speaker":"B","text":"分かりました。今度紹介します。"}];
INSERT INTO sentence [{"id":{"d":269,"n":1,"t":5},"speaker":"A","text":"好きな焼き肉の部位はどこですか？"},{"id":{"d":269,"n":2,"t":5},"speaker":"B","text":"私は、自分が好きな焼き肉の部位が、どこなのかが分かりません。実は、焼き肉にはあまり詳しくないのです。"},{"id":{"d":269,"n":3,"t":5},"speaker":"A","text":"それでは、何を食べるかは、私に任せてもらえますか？"},{"id":{"d":269,"n":4,"t":5},"speaker":"B","text":"もちろんです。あなたは、なぜ焼き肉に詳しいのですか？"},{"id":{"d":269,"n":5,"t":5},"speaker":"A","text":"大学生の時に、焼き肉店でアルバイトをしていたからです。お客さまに説明するために勉強したのです。"},{"id":{"d":269,"n":6,"t":5},"speaker":"B","text":"では、食べながら私にも焼き肉の部位を教えてください。"},{"id":{"d":269,"n":7,"t":5},"speaker":"A","text":"分かりました。中に入りましょう。この店の焼き肉はおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":270,"n":1,"t":5},"speaker":"A","text":"昔のおもちゃといえば、あなたは、何を思い出しますか？"},{"id":{"d":270,"n":2,"t":5},"speaker":"B","text":"おはじき、お手玉、メンコ、ベーゴマ、紙風船、まだまだありますね。"},{"id":{"d":270,"n":3,"t":5},"speaker":"A","text":"遊び方も覚えていますか？"},{"id":{"d":270,"n":4,"t":5},"speaker":"B","text":"おはじきは忘れてしまいましたが、他は覚えています。"},{"id":{"d":270,"n":5,"t":5},"speaker":"A","text":"そういえばお土産屋に置いてあった、からくりおもちゃを見た娘に、遊び方を聞かれたことがあります。長方形の板が布でつながれていて、傾けると勝手にひっくり返る、からくりおもちゃでした。"},{"id":{"d":270,"n":6,"t":5},"speaker":"B","text":"それはパタパタのことですか？"},{"id":{"d":270,"n":7,"t":5},"speaker":"A","text":"パタパタというのですね。どのような仕組みなのでしょう？"},{"id":{"d":270,"n":8,"t":5},"speaker":"B","text":"簡単に作ることもできますよ。今度教えましょう。"}];
INSERT INTO sentence [{"id":{"d":271,"n":1,"t":5},"speaker":"A","text":"YouTubeで、よく音楽を聴いているようですが、他の無料サブスクリプション音楽配信サービスの方が便利ではありませんか？"},{"id":{"d":271,"n":2,"t":5},"speaker":"B","text":"いいえ。サブスクリプション音楽配信サービスには、基本的にアマチュアの作品が配信されていません。私は、プロかアマチュアかの区別なく、たくさんの音楽を聴きたいので、YouTubeを利用しています。"},{"id":{"d":271,"n":3,"t":5},"speaker":"A","text":"そうなのですね。YouTubeで聴くアマチュアの音楽はいかがですか？"},{"id":{"d":271,"n":4,"t":5},"speaker":"B","text":"まれにアマチュアでも、素晴らしいミュージシャンに出会うことがあるのです。もちろん、プロの音楽も聴いています。"},{"id":{"d":271,"n":5,"t":5},"speaker":"A","text":"素晴しいアマチュアミュージシャンの作品とは、例えば、どのような音楽ですか？"},{"id":{"d":271,"n":6,"t":5},"speaker":"B","text":"1本しか弦のないギターを使った音楽や、スプーンでのギター演奏、サンプラーで作ったオーケストラ音楽など、まだまだあって面白いですよ。"}];
INSERT INTO sentence [{"id":{"d":272,"n":1,"t":5},"speaker":"A","text":"君の部屋には、たくさんのぬいぐるみがありますね。ぬいぐるみを、集めるのが趣味なのですか？"},{"id":{"d":272,"n":2,"t":5},"speaker":"B","text":"クレーンゲームが趣味なのです。ぬいぐるみは、その景品です。"},{"id":{"d":272,"n":3,"t":5},"speaker":"A","text":"人形や置物、アニメーションのキャラクターもありますね。うまく取るコツなどは、ありますか？"},{"id":{"d":272,"n":4,"t":5},"speaker":"B","text":"最初に台を見て簡単に落とせるかどうかを確認します。他に、つかむ部分の種類や開き具合も確認します。インターネット上のクレーンゲームのコツを紹介しているサイトには、もっと詳しい情報も載っていますよ。"}];
INSERT INTO sentence [{"id":{"d":273,"n":1,"t":5},"speaker":"A","text":"七夕祭りは幼稚園や学校、自治体や商店街などいろいろなところで行われますね。"},{"id":{"d":273,"n":2,"t":5},"speaker":"B","text":"願い事を短冊に書く習慣や、織り姫、ひこ星の話が、好まれるのかもしれません。"},{"id":{"d":273,"n":3,"t":5},"speaker":"A","text":"全国的に、七夕祭りを行っているところは多いと思います。"},{"id":{"d":273,"n":4,"t":5},"speaker":"B","text":"そうですね、私も仙台や平塚の七夕祭りは知っています。"},{"id":{"d":273,"n":5,"t":5},"speaker":"A","text":"仙台七夕まつりは、東北三大祭りの1つで、規模も大きいそうですね？"},{"id":{"d":273,"n":6,"t":5},"speaker":"B","text":"はい。青森ねぶた祭、秋田竿燈まつりと併せて「東北三大祭り」と言われています。"}];
INSERT INTO sentence [{"id":{"d":274,"n":1,"t":5},"speaker":"A","text":"もうすぐ、バレンタインデーですね。"},{"id":{"d":274,"n":2,"t":5},"speaker":"B","text":"はい。今年もチョコレートを作ります。"},{"id":{"d":274,"n":3,"t":5},"speaker":"A","text":"毎年作っていますよね。今年は何個のチョコレートを作る予定ですか？"},{"id":{"d":274,"n":4,"t":5},"speaker":"B","text":"50個作ります。"},{"id":{"d":274,"n":5,"t":5},"speaker":"A","text":"50個ですか。なぜ、そのように多く作るのですか？"},{"id":{"d":274,"n":6,"t":5},"speaker":"B","text":"サークルのメンバー全員に渡すためです。"},{"id":{"d":274,"n":7,"t":5},"speaker":"A","text":"なるほど。材料をたくさん買わなければなりませんね。"},{"id":{"d":274,"n":8,"t":5},"speaker":"B","text":"はい。私にとって、バレンタインデー前はとても忙しいのです。"}];
INSERT INTO sentence [{"id":{"d":275,"n":1,"t":5},"speaker":"A","text":"優子さんは、クリスマスイブには何をしましたか？"},{"id":{"d":275,"n":2,"t":5},"speaker":"B","text":"家族でクリスマスパーティーをしました。"},{"id":{"d":275,"n":3,"t":5},"speaker":"A","text":"すてきですね。私もパーティーがしたいと思っていました。"},{"id":{"d":275,"n":4,"t":5},"speaker":"B","text":"健太君は何をしましたか？"},{"id":{"d":275,"n":5,"t":5},"speaker":"A","text":"私は1日中アルバイトをしていました。"},{"id":{"d":275,"n":6,"t":5},"speaker":"B","text":"何のアルバイトですか？"},{"id":{"d":275,"n":7,"t":5},"speaker":"A","text":"チキンを売るお店のアルバイトです。クリスマスイブには、お客さんが長い行列を作るほど盛況でした。"},{"id":{"d":275,"n":8,"t":5},"speaker":"B","text":"それはお、忙しくて大変でしたね。"}];
INSERT INTO sentence [{"id":{"d":276,"n":1,"t":5},"speaker":"A","text":"あきさんの誕生日は、いつですか？"},{"id":{"d":276,"n":2,"t":5},"speaker":"B","text":"11月3日です。"},{"id":{"d":276,"n":3,"t":5},"speaker":"A","text":"文化の日ですね。実は、私の誕生日も祝日です。"},{"id":{"d":276,"n":4,"t":5},"speaker":"B","text":"そうなのですね。なつさんの誕生日は、何月何日ですか？"},{"id":{"d":276,"n":5,"t":5},"speaker":"A","text":"2月11日です。"},{"id":{"d":276,"n":6,"t":5},"speaker":"B","text":"2月11日は、建国記念の日ですね？"},{"id":{"d":276,"n":7,"t":5},"speaker":"A","text":"はい、そうです。"},{"id":{"d":276,"n":8,"t":5},"speaker":"B","text":"覚えておいて、当日は、お誕生日祝いのメールを送りますね。"},{"id":{"d":276,"n":9,"t":5},"speaker":"A","text":"私も11月3日に、メールを送ります。"}];
INSERT INTO sentence [{"id":{"d":277,"n":1,"t":5},"speaker":"A","text":"しゅん君の趣味は、何ですか？"},{"id":{"d":277,"n":2,"t":5},"speaker":"B","text":"僕はドラマが好きです。新しく始まる連続ドラマの第1話は必ず見ています。"},{"id":{"d":277,"n":3,"t":5},"speaker":"A","text":"たくさん見ているのですね。特に、好きなジャンルはありますか？"},{"id":{"d":277,"n":4,"t":5},"speaker":"B","text":"コメディードラマです。笑うと、疲れが吹き飛びます。"},{"id":{"d":277,"n":5,"t":5},"speaker":"A","text":"それは、良いですね。"},{"id":{"d":277,"n":6,"t":5},"speaker":"B","text":"けいこさんは、ドラマを見ませんか？"},{"id":{"d":277,"n":7,"t":5},"speaker":"A","text":"はい、見ません。テレビを持っていませんので。そういえば、1人暮らしをしてから、全くテレビの番組を見ていませんね。"},{"id":{"d":277,"n":8,"t":5},"speaker":"B","text":"そうなのですね。それでは、けいこさんの趣味は何ですか？"},{"id":{"d":277,"n":9,"t":5},"speaker":"A","text":"YouTubeを見ることです。YouTubeを見るようになってから、テレビが必要ではなくなりました。"},{"id":{"d":277,"n":10,"t":5},"speaker":"B","text":"なるほど。YouTubeがテレビ代わりなのですね。"}];
INSERT INTO sentence [{"id":{"d":278,"n":1,"t":5},"speaker":"A","text":"お母さん、私の浴衣はまだ家にありますか？"},{"id":{"d":278,"n":2,"t":5},"speaker":"B","text":"はい。小学生の頃に着ていた浴衣のことですか？捨ててはいませんが、浴衣をどうするのですか？"},{"id":{"d":278,"n":3,"t":5},"speaker":"A","text":"大学の友達と浴衣を着て花火大会に行こうと話しているのです。"},{"id":{"d":278,"n":4,"t":5},"speaker":"B","text":"それは良いですね。でも、今のあなたの身長では、あの浴衣は小さすぎると思います。"},{"id":{"d":278,"n":5,"t":5},"speaker":"A","text":"やはりそうですよね。どうしましょう。急いで新しく買いに行った方がいいですか？"},{"id":{"d":278,"n":6,"t":5},"speaker":"B","text":"いいえ。私が学生の頃に着ていたものなら、貸してあげられますよ。着てみますか？"},{"id":{"d":278,"n":7,"t":5},"speaker":"A","text":"はい、ぜひ、着させてください。"},{"id":{"d":278,"n":8,"t":5},"speaker":"B","text":"まさか、私が学生時代に着ていた浴衣が、役に立つ日が来るとは思いませんでした。取っておいてよかったです。"},{"id":{"d":278,"n":9,"t":5},"speaker":"A","text":"本当にありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":279,"n":1,"t":5},"speaker":"A","text":"今店内に流れているBGMはとてもすてきな曲ですね。"},{"id":{"d":279,"n":2,"t":5},"speaker":"B","text":"はい。有名なクラシック曲ですね。"},{"id":{"d":279,"n":3,"t":5},"speaker":"A","text":"クラシック音楽ですか？私は詳しくないので、分かりませんでした。何という曲ですか？"},{"id":{"d":279,"n":4,"t":5},"speaker":"B","text":"これは『ノクターン』という曲です。"},{"id":{"d":279,"n":5,"t":5},"speaker":"A","text":"近藤さんは、クラシック音楽に詳しいですね。"},{"id":{"d":279,"n":6,"t":5},"speaker":"B","text":"いえ、あまり詳しくはありません。この曲は、ピアノの発表会で弾いたことがあったので知っていました。"},{"id":{"d":279,"n":7,"t":5},"speaker":"A","text":"なるほど。ピアノを習っていたのですね。"},{"id":{"d":279,"n":8,"t":5},"speaker":"B","text":"10年も昔の話です。今は、すっかり指が動かなくなりました。"},{"id":{"d":279,"n":9,"t":5},"speaker":"A","text":"私はピアノを習ったことがありませんが、老後に習ってみたいと思っています。"},{"id":{"d":279,"n":10,"t":5},"speaker":"B","text":"すてきですね。私も、老後に再チャレンジしようと思います。"}];
INSERT INTO sentence [{"id":{"d":280,"n":1,"t":5},"speaker":"A","text":"もう年末ですね。"},{"id":{"d":280,"n":2,"t":5},"speaker":"B","text":"はい。正月はゆっくり過ごせそうですか？"},{"id":{"d":280,"n":3,"t":5},"speaker":"A","text":"はい。実家に帰省する予定です。"},{"id":{"d":280,"n":4,"t":5},"speaker":"B","text":"いいですね。私も家族と過ごします。"},{"id":{"d":280,"n":5,"t":5},"speaker":"A","text":"長谷川さんは、年賀状を出しましたか？"},{"id":{"d":280,"n":6,"t":5},"speaker":"B","text":"まだです。この後、郵便局に行って出してきます。"},{"id":{"d":280,"n":7,"t":5},"speaker":"A","text":"私は、まだ1枚も書けていません。"},{"id":{"d":280,"n":8,"t":5},"speaker":"B","text":"年賀状を書くことは、なかなか時間がかかりますよね。"},{"id":{"d":280,"n":9,"t":5},"speaker":"A","text":"はい。せめて新年を迎える前には、年賀状を出したいと思っています。"},{"id":{"d":280,"n":10,"t":5},"speaker":"B","text":"まだ数日あるので、きっと間に合いますよ。"}];
INSERT INTO sentence [{"id":{"d":281,"n":1,"t":5},"speaker":"A","text":"美久さんの部屋は、おしゃれですね。"},{"id":{"d":281,"n":2,"t":5},"speaker":"B","text":"あおいさんを呼ぶために、昨日片付けました。"},{"id":{"d":281,"n":3,"t":5},"speaker":"A","text":"そうだったのですね。このノートは何ですか？"},{"id":{"d":281,"n":4,"t":5},"speaker":"B","text":"それは、歌詞ノートです。頭の中に思いついた歌詞を、記録しています。"},{"id":{"d":281,"n":5,"t":5},"speaker":"A","text":"歌を歌うことが好きなのですか？"},{"id":{"d":281,"n":6,"t":5},"speaker":"B","text":"はい、大好きです。いつか、自分の曲を作りたいと思っています。"},{"id":{"d":281,"n":7,"t":5},"speaker":"A","text":"すてきですね。ここにあるギターも、美久さんの物ですか？"},{"id":{"d":281,"n":8,"t":5},"speaker":"B","text":"はい。ギターを弾きながら、歌詞を考えています。"},{"id":{"d":281,"n":9,"t":5},"speaker":"A","text":"格好良いですね。"}];
INSERT INTO sentence [{"id":{"d":282,"n":1,"t":5},"speaker":"A","text":"お父さんは、何を見ているのですか？"},{"id":{"d":282,"n":2,"t":5},"speaker":"B","text":"礼子さんのアルバムです。"},{"id":{"d":282,"n":3,"t":5},"speaker":"A","text":"これは、私が何歳のときですか？"},{"id":{"d":282,"n":4,"t":5},"speaker":"B","text":"2歳のときですね。懐かしいです。"},{"id":{"d":282,"n":5,"t":5},"speaker":"A","text":"私は、全く覚えていません。"},{"id":{"d":282,"n":6,"t":5},"speaker":"B","text":"そうかもしれませんね。礼子さんは、ぬいぐるみが大好きだったのですよ。"},{"id":{"d":282,"n":7,"t":5},"speaker":"A","text":"ぬいぐるみと、一緒に写っている写真が多いですね。"},{"id":{"d":282,"n":8,"t":5},"speaker":"B","text":"はい。いつも握りしめていました。"},{"id":{"d":282,"n":9,"t":5},"speaker":"A","text":"そうだったのですね。他のアルバムもありますか？"},{"id":{"d":282,"n":10,"t":5},"speaker":"B","text":"0歳から小学校に入学するまでありますよ。リビングの棚の中にあるはずです。"},{"id":{"d":282,"n":11,"t":5},"speaker":"A","text":"探してきます。"}];
INSERT INTO sentence [{"id":{"d":283,"n":1,"t":5},"speaker":"A","text":"加藤さんは洋画が好きだと聞きました。"},{"id":{"d":283,"n":2,"t":5},"speaker":"B","text":"はい。毎月DVDを借りて、家で見ています。"},{"id":{"d":283,"n":3,"t":5},"speaker":"A","text":"そうなのですね。映画館には行かないのですか？"},{"id":{"d":283,"n":4,"t":5},"speaker":"B","text":"しばらく行っていません。2年前に行ったときが、最後かもしれません。"},{"id":{"d":283,"n":5,"t":5},"speaker":"A","text":"一緒に映画館に行きませんか？映画館の臨場感は格別ですよ。"},{"id":{"d":283,"n":6,"t":5},"speaker":"B","text":"はい、そうですね。久しぶりに行ってみたいです。"},{"id":{"d":283,"n":7,"t":5},"speaker":"A","text":"良かったです。テレビ広告で流れている洋画が気になったので、一緒に行けたら良いなと思っていました。"},{"id":{"d":283,"n":8,"t":5},"speaker":"B","text":"私も、何度かその広告を見ました。面白そうですよね。"},{"id":{"d":283,"n":9,"t":5},"speaker":"A","text":"サイトで見たレビューも良い評価なので、期待できると思います。"},{"id":{"d":283,"n":10,"t":5},"speaker":"B","text":"楽しみです。日程はいつにしますか？私は、今週の土曜日が空いています。"},{"id":{"d":283,"n":11,"t":5},"speaker":"A","text":"私も空いています。では、土曜日に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":284,"n":1,"t":5},"speaker":"A","text":"先輩は、出産祝いを贈ったことはありますか？"},{"id":{"d":284,"n":2,"t":5},"speaker":"B","text":"はい、あります。"},{"id":{"d":284,"n":3,"t":5},"speaker":"A","text":"同僚に出産祝いを贈りたいのですが、何を贈るべきか悩んでいるのです。"},{"id":{"d":284,"n":4,"t":5},"speaker":"B","text":"それは悩みますよね。"},{"id":{"d":284,"n":5,"t":5},"speaker":"A","text":"先輩は何を贈りましたか？"},{"id":{"d":284,"n":6,"t":5},"speaker":"B","text":"私は、乳幼児用のおもちゃを贈りました。"},{"id":{"d":284,"n":7,"t":5},"speaker":"A","text":"おもちゃというと、例えばガラガラなどですか？"},{"id":{"d":284,"n":8,"t":5},"speaker":"B","text":"はい。まさにガラガラです。"},{"id":{"d":284,"n":9,"t":5},"speaker":"A","text":"お祝い品選びの参考にさせていただきます。"}];
INSERT INTO sentence [{"id":{"d":285,"n":1,"t":5},"speaker":"A","text":"すっかり、寒くなりましたね。"},{"id":{"d":285,"n":2,"t":5},"speaker":"B","text":"はい。今日は、冬至です。"},{"id":{"d":285,"n":3,"t":5},"speaker":"A","text":"そうですね。冬至には、あなたは何かをしていますか？"},{"id":{"d":285,"n":4,"t":5},"speaker":"B","text":"私は毎年、カボチャを食べたり、ユズ湯に入ったりします。"},{"id":{"d":285,"n":5,"t":5},"speaker":"A","text":"そうでしたね。毎年のことなのに、つい忘れてしまいます。"},{"id":{"d":285,"n":6,"t":5},"speaker":"B","text":"スーパーマーケットに行くと、カボチャやユズが目立つ所に置かれていますよ。私はそれを見て、冬至を思い出します。"},{"id":{"d":285,"n":7,"t":5},"speaker":"A","text":"なるほど。まだカボチャとユズを買っていないので、スーパーマーケットへ行ってみます。"},{"id":{"d":285,"n":8,"t":5},"speaker":"B","text":"ぜひ、行ってみてください。"}];
INSERT INTO sentence [{"id":{"d":286,"n":1,"t":5},"speaker":"A","text":"この写真で、りささんの隣に写っている方は、どなたですか？"},{"id":{"d":286,"n":2,"t":5},"speaker":"B","text":"お笑い芸人の方です。"},{"id":{"d":286,"n":3,"t":5},"speaker":"A","text":"りささんは、お笑いが好きなのですか？"},{"id":{"d":286,"n":4,"t":5},"speaker":"B","text":"はい。よく劇場に足を運んでいます。"},{"id":{"d":286,"n":5,"t":5},"speaker":"A","text":"そうだったのですね。この写真の方とは、偶然町で会ったのですか？"},{"id":{"d":286,"n":6,"t":5},"speaker":"B","text":"いえ。私が劇場に行った日に写真を撮ってもらいました。"},{"id":{"d":286,"n":7,"t":5},"speaker":"A","text":"ライブが終わった後に、出待ちをしたのですか？"},{"id":{"d":286,"n":8,"t":5},"speaker":"B","text":"はい。2時間くらい待ちました。"},{"id":{"d":286,"n":9,"t":5},"speaker":"A","text":"筋金入りのファンですね。"}];
INSERT INTO sentence [{"id":{"d":287,"n":1,"t":5},"speaker":"A","text":"中村さんは、流れ星を見たことはありますか？"},{"id":{"d":287,"n":2,"t":5},"speaker":"B","text":"いいえ、ありません。中田さんはあるのですか？"},{"id":{"d":287,"n":3,"t":5},"speaker":"A","text":"はい。一度だけあります。"},{"id":{"d":287,"n":4,"t":5},"speaker":"B","text":"いつ見たのですか？"},{"id":{"d":287,"n":5,"t":5},"speaker":"A","text":"2年前に、山奥でキャンプをしていた時に見ました。"},{"id":{"d":287,"n":6,"t":5},"speaker":"B","text":"周囲に建物がない山奥では、星がきれいに見られると聞きます。"},{"id":{"d":287,"n":7,"t":5},"speaker":"A","text":"まさにそうでした。真っ暗な空に、星の明かりが輝いていました。"},{"id":{"d":287,"n":8,"t":5},"speaker":"B","text":"流れ星に願い事をしましたか？"},{"id":{"d":287,"n":9,"t":5},"speaker":"A","text":"いいえ。初めての流れ星に興奮し、願い事をするのを忘れてしまいました。"}];
INSERT INTO sentence [{"id":{"d":288,"n":1,"t":5},"speaker":"A","text":"今日は、とても良いお天気ですね。"},{"id":{"d":288,"n":2,"t":5},"speaker":"B","text":"はい。久しぶりの晴天です。"},{"id":{"d":288,"n":3,"t":5},"speaker":"A","text":"ここ何日か悪天候が続いていましたもんね。"},{"id":{"d":288,"n":4,"t":5},"speaker":"B","text":"そうなのです。洗濯物が外で干せない日が続いて、大変でした。"},{"id":{"d":288,"n":5,"t":5},"speaker":"A","text":"そうですね。今日は洗濯日和ですね。"},{"id":{"d":288,"n":6,"t":5},"speaker":"B","text":"はい。久しぶりに布団を干そうと思います。"},{"id":{"d":288,"n":7,"t":5},"speaker":"A","text":"良いですね。布団の日干しをすると、寝るときに気持ちが良いです。"},{"id":{"d":288,"n":8,"t":5},"speaker":"B","text":"分かります。いつもより、ぐっすり眠れる気がします。"}];
INSERT INTO sentence [{"id":{"d":289,"n":1,"t":5},"speaker":"A","text":"和田さんの部屋の本棚には、小説がたくさんありますね。"},{"id":{"d":289,"n":2,"t":5},"speaker":"B","text":"はい。よく小説を読んでいます。"},{"id":{"d":289,"n":3,"t":5},"speaker":"A","text":"どのような時に、小説を読んでいますか？"},{"id":{"d":289,"n":4,"t":5},"speaker":"B","text":"寝る前や、通勤時の電車内で読むことが多いです。"},{"id":{"d":289,"n":5,"t":5},"speaker":"A","text":"なるほど。好きなジャンルはありますか？"},{"id":{"d":289,"n":6,"t":5},"speaker":"B","text":"幅広く読むのですが、ミステリー小説やSF小説が特に好きです。"},{"id":{"d":289,"n":7,"t":5},"speaker":"A","text":"そうなのですか。私は、映画化されたものしか、見たことがありません。"},{"id":{"d":289,"n":8,"t":5},"speaker":"B","text":"ぜひ、小説も読んでみてください。映画とは違った面白さがありますよ。"}];
INSERT INTO sentence [{"id":{"d":290,"n":1,"t":5},"speaker":"A","text":"お母さんは、デパートの初売りに行きませんか？"},{"id":{"d":290,"n":2,"t":5},"speaker":"B","text":"初売りですか。1月3日から始まるそうですね。"},{"id":{"d":290,"n":3,"t":5},"speaker":"A","text":"はい。新春セールのお知らせのチラシが、ポストに入っていました。"},{"id":{"d":290,"n":4,"t":5},"speaker":"B","text":"せっかくなので、行ってみましょうか。"},{"id":{"d":290,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。福袋も買いたいと思います。"},{"id":{"d":290,"n":6,"t":5},"speaker":"B","text":"福袋を買うなら、朝早く行かなければいけませんね。"},{"id":{"d":290,"n":7,"t":5},"speaker":"A","text":"はい。昨年は、開店前に行列ができていました。"},{"id":{"d":290,"n":8,"t":5},"speaker":"B","text":"そうでしたね。開店時間の30分前に行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":291,"n":1,"t":5},"speaker":"A","text":"愛さんは、今日は一段とおしゃれをしていますね。"},{"id":{"d":291,"n":2,"t":5},"speaker":"B","text":"今夜は、コンサートに行く予定があるのです。"},{"id":{"d":291,"n":3,"t":5},"speaker":"A","text":"誰のコンサートですか？"},{"id":{"d":291,"n":4,"t":5},"speaker":"B","text":"私の好きな、アイドルのコンサートです。"},{"id":{"d":291,"n":5,"t":5},"speaker":"A","text":"それは楽しみですね。コンサートに行くのは何回目ですか？"},{"id":{"d":291,"n":6,"t":5},"speaker":"B","text":"今回初めて行きます。"},{"id":{"d":291,"n":7,"t":5},"speaker":"A","text":"そうなのですか。これまでは、テレビやDVDで見ていたのですか？"},{"id":{"d":291,"n":8,"t":5},"speaker":"B","text":"はい。チケットが入手困難で、毎回購入することができませんでした。"},{"id":{"d":291,"n":9,"t":5},"speaker":"A","text":"今日は、念願が、かなって得られた機会なのですね。"},{"id":{"d":291,"n":10,"t":5},"speaker":"B","text":"はい。今から緊張しています。"}];
INSERT INTO sentence [{"id":{"d":292,"n":1,"t":5},"speaker":"A","text":"お父さん、来月の第1日曜日の予定を空けておいてください。"},{"id":{"d":292,"n":2,"t":5},"speaker":"B","text":"分かりました。何があるのですか？"},{"id":{"d":292,"n":3,"t":5},"speaker":"A","text":"私の通っているダンス教室の発表会です。"},{"id":{"d":292,"n":4,"t":5},"speaker":"B","text":"そうでしたか。それは大事な予定ですね。"},{"id":{"d":292,"n":5,"t":5},"speaker":"A","text":"今年は、踊る曲の選曲やアレンジも担当しました。"},{"id":{"d":292,"n":6,"t":5},"speaker":"B","text":"そうなのですか。大活躍ですね。"},{"id":{"d":292,"n":7,"t":5},"speaker":"A","text":"一生懸命練習しているので、必ず来てくださいね。"},{"id":{"d":292,"n":8,"t":5},"speaker":"B","text":"必ず行きます。ビデオカメラの充電も忘れずにしておきますね。"}];
INSERT INTO sentence [{"id":{"d":293,"n":1,"t":5},"speaker":"A","text":"雨が続きますね。"},{"id":{"d":293,"n":2,"t":5},"speaker":"B","text":"梅雨入りしたそうですよ。"},{"id":{"d":293,"n":3,"t":5},"speaker":"A","text":"私は、雨があまり好きではありません。"},{"id":{"d":293,"n":4,"t":5},"speaker":"B","text":"どうしてですか？"},{"id":{"d":293,"n":5,"t":5},"speaker":"A","text":"服がぬれるし、セットした髪の毛が崩れてしまうからです。"},{"id":{"d":293,"n":6,"t":5},"speaker":"B","text":"私は、最近になって雨の日が好きになりました。"},{"id":{"d":293,"n":7,"t":5},"speaker":"A","text":"なぜですか？"},{"id":{"d":293,"n":8,"t":5},"speaker":"B","text":"お気に入りの傘を見つけたので、奮発して買ったからです。その傘を使うたびに、心が躍ります。"},{"id":{"d":293,"n":9,"t":5},"speaker":"A","text":"なるほど。お気に入りの傘を使うことは、雨の日を好きになるための、すてきなアイデアですね。"}];
INSERT INTO sentence [{"id":{"d":294,"n":1,"t":5},"speaker":"A","text":"はるきさんの部屋の本棚には、漫画がたくさんありますね。"},{"id":{"d":294,"n":2,"t":5},"speaker":"B","text":"はい。漫画を読むことが好きなので、よく買っています。"},{"id":{"d":294,"n":3,"t":5},"speaker":"A","text":"ここにある漫画は、全巻そろっているのですか？"},{"id":{"d":294,"n":4,"t":5},"speaker":"B","text":"全部ではありませんが、特に好きな漫画は、全巻そろっています。"},{"id":{"d":294,"n":5,"t":5},"speaker":"A","text":"どのような漫画が好きなのですか？"},{"id":{"d":294,"n":6,"t":5},"speaker":"B","text":"私は、スポーツ漫画が好きです。"},{"id":{"d":294,"n":7,"t":5},"speaker":"A","text":"特に、好きなスポーツはありますか？"},{"id":{"d":294,"n":8,"t":5},"speaker":"B","text":"幅広く好きです。私が読んでいる漫画のテーマとなっているスポーツも、サッカー、野球、バスケットボール、バレーボールとさまざまです。"},{"id":{"d":294,"n":9,"t":5},"speaker":"A","text":"たくさんの漫画本を読んでいるのですね。"}];
INSERT INTO sentence [{"id":{"d":295,"n":1,"t":5},"speaker":"A","text":"トムさんは、日本に来て何年たちましたか？"},{"id":{"d":295,"n":2,"t":5},"speaker":"B","text":"今年で3年目になります。"},{"id":{"d":295,"n":3,"t":5},"speaker":"A","text":"もうそれほどの年月がたちましたか。日本での生活はどうですか？"},{"id":{"d":295,"n":4,"t":5},"speaker":"B","text":"楽しいです。特に、四季があるところがいいですね。"},{"id":{"d":295,"n":5,"t":5},"speaker":"A","text":"どうしてですか？"},{"id":{"d":295,"n":6,"t":5},"speaker":"B","text":"季節が変わる体験は、初めてだったからです。私の出身国では、一年中気温が高いです。"},{"id":{"d":295,"n":7,"t":5},"speaker":"A","text":"そうなのですね。特に好きな季節はありますか？"},{"id":{"d":295,"n":8,"t":5},"speaker":"B","text":"私は春が好きです。過ごしやすい気温ですし、何より春に咲く桜は、美しいです。"}];
INSERT INTO sentence [{"id":{"d":296,"n":1,"t":5},"speaker":"A","text":"今日の放課後、カラオケに行きませんか？"},{"id":{"d":296,"n":2,"t":5},"speaker":"B","text":"良いですね。行きましょう。"},{"id":{"d":296,"n":3,"t":5},"speaker":"A","text":"えりこさんには、カラオケで歌う曲のおはこはありますか？"},{"id":{"d":296,"n":4,"t":5},"speaker":"B","text":"特に、ありません。その時々のヒット曲を歌います。"},{"id":{"d":296,"n":5,"t":5},"speaker":"A","text":"そうなのですね。私は、最近の曲はあまり知りません。"},{"id":{"d":296,"n":6,"t":5},"speaker":"B","text":"それでは、よしこさんは何を歌うのですか？"},{"id":{"d":296,"n":7,"t":5},"speaker":"A","text":"私は海外の歌手が好きなので、洋楽を歌うことが多いです。"},{"id":{"d":296,"n":8,"t":5},"speaker":"B","text":"洋楽ですか。格好良いですね。カラオケであなたの歌を聴けることを楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":297,"n":1,"t":5},"speaker":"A","text":"昨年放送していた、連続ドラマの映画化が決まったそうですよ。"},{"id":{"d":297,"n":2,"t":5},"speaker":"B","text":"そうなのですか？私が見ていたドラマですか？"},{"id":{"d":297,"n":3,"t":5},"speaker":"A","text":"はい。去年の夏に放送していた、弁護士が出てくるストーリーのテレビドラマです。"},{"id":{"d":297,"n":4,"t":5},"speaker":"B","text":"確かに見ていましたね。とても面白いドラマでした。"},{"id":{"d":297,"n":5,"t":5},"speaker":"A","text":"はい。視聴率も高かったようですね。"},{"id":{"d":297,"n":6,"t":5},"speaker":"B","text":"そうなのですね。映画化はうれしいです。いつ頃に公開するのですか？"},{"id":{"d":297,"n":7,"t":5},"speaker":"A","text":"来年の冬に公開が予定されているようですよ。"},{"id":{"d":297,"n":8,"t":5},"speaker":"B","text":"来年の楽しみができました。"}];
INSERT INTO sentence [{"id":{"d":298,"n":1,"t":5},"speaker":"A","text":"お母さんは、押し入れを覗いて、何を探しているのですか？"},{"id":{"d":298,"n":2,"t":5},"speaker":"B","text":"おひなさまです。"},{"id":{"d":298,"n":3,"t":5},"speaker":"A","text":"そういえば、あさってはひな祭りの日でしたね。"},{"id":{"d":298,"n":4,"t":5},"speaker":"B","text":"このおひなさまは、元々は、おばあちゃんのものだったのですよ。"},{"id":{"d":298,"n":5,"t":5},"speaker":"A","text":"そうなのですか？代々受け継がれているのですね。"},{"id":{"d":298,"n":6,"t":5},"speaker":"B","text":"はい。大事なものなので、毎年欠かさずに飾っています。"},{"id":{"d":298,"n":7,"t":5},"speaker":"A","text":"すてきですね。私も一緒に飾ります。"},{"id":{"d":298,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":299,"n":1,"t":5},"speaker":"A","text":"最近、早い時間に目が覚めてしまいます。"},{"id":{"d":299,"n":2,"t":5},"speaker":"B","text":"今日は何時に起きたのですか？"},{"id":{"d":299,"n":3,"t":5},"speaker":"A","text":"朝の4時です。"},{"id":{"d":299,"n":4,"t":5},"speaker":"B","text":"それは、随分と早起きですね。"},{"id":{"d":299,"n":5,"t":5},"speaker":"A","text":"はい。本当は7時まで眠りたいのですが、暑苦しくて目覚めてしまうのです。"},{"id":{"d":299,"n":6,"t":5},"speaker":"B","text":"寝る時間は何時ですか？"},{"id":{"d":299,"n":7,"t":5},"speaker":"A","text":"布団に入るのは夜の12時ですが、熱帯夜だとなかなか寝付けず、1時まで眠れないこともあります。"},{"id":{"d":299,"n":8,"t":5},"speaker":"B","text":"それでは、寝不足になってしまいますね。"},{"id":{"d":299,"n":9,"t":5},"speaker":"A","text":"そうなのです。最近の一番の困りごとです。"}];
INSERT INTO sentence [{"id":{"d":300,"n":1,"t":5},"speaker":"A","text":"今日は成人の日ですね。"},{"id":{"d":300,"n":2,"t":5},"speaker":"B","text":"はい。街中で振り袖を着た人をたくさん見かけます。"},{"id":{"d":300,"n":3,"t":5},"speaker":"A","text":"私も早く成人になりたいです。"},{"id":{"d":300,"n":4,"t":5},"speaker":"B","text":"ひとみさんは、来年成人ですね。"},{"id":{"d":300,"n":5,"t":5},"speaker":"A","text":"はい。振り袖を着て、成人式に出席することを楽しみにしています。"},{"id":{"d":300,"n":6,"t":5},"speaker":"B","text":"振り袖はレンタルしますか？それとも、私の振り袖を着ますか？"},{"id":{"d":300,"n":7,"t":5},"speaker":"A","text":"お母さんの振り袖は、まだ手元にあるのですか？"},{"id":{"d":300,"n":8,"t":5},"speaker":"B","text":"はい、取ってありますよ。"},{"id":{"d":300,"n":9,"t":5},"speaker":"A","text":"それでは、お母さんの振り袖を着たいです。"},{"id":{"d":300,"n":10,"t":5},"speaker":"B","text":"分かりました。そうしましょう。ひとみさんの振り袖姿が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":301,"n":1,"t":5},"speaker":"A","text":"おじいさんの営む畑は、広いですね。"},{"id":{"d":301,"n":2,"t":5},"speaker":"B","text":"はい。私は、この畑でいろいろな野菜を作っています。"},{"id":{"d":301,"n":3,"t":5},"speaker":"A","text":"今日のような暑い日に農作業をするのは、大変ではありませんか？"},{"id":{"d":301,"n":4,"t":5},"speaker":"B","text":"大変ではありますが、楽しいですよ。炎天下では長く作業をしないようにするのと、こまめな水分補給をするよう心掛けています。"},{"id":{"d":301,"n":5,"t":5},"speaker":"A","text":"しっかり体調管理をしながら農作業をしているのですね。今日は、私にも手伝わせてください。"},{"id":{"d":301,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。それでは、一緒に野菜の収穫をしましょう。"},{"id":{"d":301,"n":7,"t":5},"speaker":"A","text":"今の時期に、お薦めの野菜は何ですか？"},{"id":{"d":301,"n":8,"t":5},"speaker":"B","text":"キュウリやナスがお薦めです。旬の野菜ですよ。"},{"id":{"d":301,"n":9,"t":5},"speaker":"A","text":"この時期においしい、夏野菜ですね。"}];
INSERT INTO sentence [{"id":{"d":302,"n":1,"t":5},"speaker":"A","text":"除夜の鐘の音が聞こえてきました。"},{"id":{"d":302,"n":2,"t":5},"speaker":"B","text":"年末年始の風物詩ですね。"},{"id":{"d":302,"n":3,"t":5},"speaker":"A","text":"はい。年末年始だという実感が湧いてきます。"},{"id":{"d":302,"n":4,"t":5},"speaker":"B","text":"私は、今日初めて、鐘の音を直接聞きました。"},{"id":{"d":302,"n":5,"t":5},"speaker":"A","text":"そうなのですか？これまでは、家の近くに寺がなかったのですか？"},{"id":{"d":302,"n":6,"t":5},"speaker":"B","text":"はい。毎年テレビ番組の中継で除夜の鐘の音を聞いていました。"},{"id":{"d":302,"n":7,"t":5},"speaker":"A","text":"そうだったのですね。直接聞く除夜の鐘の音はどうですか？"},{"id":{"d":302,"n":8,"t":5},"speaker":"B","text":"思った以上に、鐘の音が響くことに驚きました。"},{"id":{"d":302,"n":9,"t":5},"speaker":"A","text":"私は毎年聞いているので慣れましたが、初めて聞くと、確かに、鐘の音が響くことに驚くかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":303,"n":1,"t":5},"speaker":"A","text":"今日は、七夕ですね。"},{"id":{"d":303,"n":2,"t":5},"speaker":"B","text":"はい。しかし、あいにくの空模様ですね。"},{"id":{"d":303,"n":3,"t":5},"speaker":"A","text":"ええ、星空を見たかったです。"},{"id":{"d":303,"n":4,"t":5},"speaker":"B","text":"清水さんは、七夕の日に何かしていますか？"},{"id":{"d":303,"n":5,"t":5},"speaker":"A","text":"大人になってからは、特に何もしていませんが、子どもの頃は短冊を書いていましたね。"},{"id":{"d":303,"n":6,"t":5},"speaker":"B","text":"私もしていました。保育園や小学校で、クラスのみんなと短冊を書いた思い出があります。"},{"id":{"d":303,"n":7,"t":5},"speaker":"A","text":"はい。最近は、なかなか短冊を書く機会がありませんね。"},{"id":{"d":303,"n":8,"t":5},"speaker":"B","text":"そうですね。今日はたまたま、デパートで短冊を書くイベントをしているところを見かけました。"},{"id":{"d":303,"n":9,"t":5},"speaker":"A","text":"そうだったのですね。書いてきましたか？"},{"id":{"d":303,"n":10,"t":5},"speaker":"B","text":"いえ、書いていません。短冊を書いているのは、子どもだけでした。"}];
INSERT INTO sentence [{"id":{"d":304,"n":1,"t":5},"speaker":"A","text":"今月末の日曜日は、私たちの結婚記念日ですね。"},{"id":{"d":304,"n":2,"t":5},"speaker":"B","text":"はい。ちょうど10年目です。"},{"id":{"d":304,"n":3,"t":5},"speaker":"A","text":"そうですね。予定は空いていますか？もし空いていたら、ディナーに行きましょう。"},{"id":{"d":304,"n":4,"t":5},"speaker":"B","text":"空いています。ぜひ行きましょう。"},{"id":{"d":304,"n":5,"t":5},"speaker":"A","text":"良かったです。どこか行きたいお店はありますか？"},{"id":{"d":304,"n":6,"t":5},"speaker":"B","text":"はい。1つ、思い浮かぶお店があります。"},{"id":{"d":304,"n":7,"t":5},"speaker":"A","text":"それはどこですか？"},{"id":{"d":304,"n":8,"t":5},"speaker":"B","text":"ひろしさんがプロポーズをしてくれたお店です。"},{"id":{"d":304,"n":9,"t":5},"speaker":"A","text":"私たちの思い出の場所ですね。それでは、その店でディナーをしましょう。"}];
INSERT INTO sentence [{"id":{"d":305,"n":1,"t":5},"speaker":"A","text":"長野さんは、演奏ができる楽器は何かありますか？"},{"id":{"d":305,"n":2,"t":5},"speaker":"B","text":"あります。ギターです。大人になってから、趣味として習い始めました。"},{"id":{"d":305,"n":3,"t":5},"speaker":"A","text":"そうなのですね。自分用のギターを持っているのですか？"},{"id":{"d":305,"n":4,"t":5},"speaker":"B","text":"はい。ギターを習い始めた当初はレンタルしていましたが、自分の物が欲しくなったので買いました。"},{"id":{"d":305,"n":5,"t":5},"speaker":"A","text":"自分専用の物があるというのは、良いですね。"},{"id":{"d":305,"n":6,"t":5},"speaker":"B","text":"山口さんは、演奏できる楽器はありますか？"},{"id":{"d":305,"n":7,"t":5},"speaker":"A","text":"はい。私はピアノを弾きます。"},{"id":{"d":305,"n":8,"t":5},"speaker":"B","text":"習っているのですか？"},{"id":{"d":305,"n":9,"t":5},"speaker":"A","text":"はい。小学生から始めて、今も続けています。10年以上ですね。"},{"id":{"d":305,"n":10,"t":5},"speaker":"B","text":"ずっと継続しているのですね。私もギターを継続したいです。"}];
INSERT INTO sentence [{"id":{"d":306,"n":1,"t":5},"speaker":"A","text":"渡辺さんは、仕事がお休みの日は何をしていますか？"},{"id":{"d":306,"n":2,"t":5},"speaker":"B","text":"私は家でゆっくり過ごすことが好きなので、一日中家にいます。"},{"id":{"d":306,"n":3,"t":5},"speaker":"A","text":"家の中では、何をして過ごしていますか？"},{"id":{"d":306,"n":4,"t":5},"speaker":"B","text":"最近は、スマートフォンで動画を見ることが多いです。"},{"id":{"d":306,"n":5,"t":5},"speaker":"A","text":"どのような動画ですか？"},{"id":{"d":306,"n":6,"t":5},"speaker":"B","text":"お笑い芸人のネタの動画をよく見ます。"},{"id":{"d":306,"n":7,"t":5},"speaker":"A","text":"お笑いが好きなのですか？知りませんでした。"},{"id":{"d":306,"n":8,"t":5},"speaker":"B","text":"はい、好きです。テレビのネタ番組も動画も、繰り返し見ています。"},{"id":{"d":306,"n":9,"t":5},"speaker":"A","text":"私もお笑いが好きなので、お薦めの動画がありましたら、教えてください。"},{"id":{"d":306,"n":10,"t":5},"speaker":"B","text":"分かりました。星野さんのお薦め動画もぜひ教えてください。"},{"id":{"d":306,"n":11,"t":5},"speaker":"A","text":"もちろんです。お互いに教え合いましょう。"}];
INSERT INTO sentence [{"id":{"d":307,"n":1,"t":5},"speaker":"A","text":"犬のおもちゃが、ひどく傷んできましたね。"},{"id":{"d":307,"n":2,"t":5},"speaker":"B","text":"はい。ボールは空気が抜けてしまっています。"},{"id":{"d":307,"n":3,"t":5},"speaker":"A","text":"そろそろ買い替えましょう。しばらく買っていないはずです。"},{"id":{"d":307,"n":4,"t":5},"speaker":"B","text":"そうですね。新しいおもちゃを買いに行きましょう。"},{"id":{"d":307,"n":5,"t":5},"speaker":"A","text":"今日の放課後は空いていますか？"},{"id":{"d":307,"n":6,"t":5},"speaker":"B","text":"はい。空いています。"},{"id":{"d":307,"n":7,"t":5},"speaker":"A","text":"それでは、学校が終わったら、駅前のペットショップで待ち合わせをして、一緒に犬のおもちゃを選びましょう。"},{"id":{"d":307,"n":8,"t":5},"speaker":"B","text":"はい、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":308,"n":1,"t":5},"speaker":"A","text":"今日は、帰りが遅いのですか？"},{"id":{"d":308,"n":2,"t":5},"speaker":"B","text":"はい。学校の授業が終わったら、塾に行って自習をしてから帰る予定です。"},{"id":{"d":308,"n":3,"t":5},"speaker":"A","text":"今日は塾に寄らずに家に帰ってきた方が良さそうです。"},{"id":{"d":308,"n":4,"t":5},"speaker":"B","text":"どうしてですか？"},{"id":{"d":308,"n":5,"t":5},"speaker":"A","text":"天気が荒れる可能性があるからです。夕方から夜にかけて、暴風雨になるそうですよ。交通機関に影響が出るかもしれません。"},{"id":{"d":308,"n":6,"t":5},"speaker":"B","text":"そうなのですか。電車が止まってしまったら、家に帰れなくなってしまいますね。"},{"id":{"d":308,"n":7,"t":5},"speaker":"A","text":"はい。暴風雨になる前に、家に帰る方が安心です。"},{"id":{"d":308,"n":8,"t":5},"speaker":"B","text":"そうですね。分かりました。今日は、学校の授業が終わったら、すぐに家に帰ります。"}];
INSERT INTO sentence [{"id":{"d":309,"n":1,"t":5},"speaker":"A","text":"母の日は、いつですか？"},{"id":{"d":309,"n":2,"t":5},"speaker":"B","text":"5月の第2日曜日です。"},{"id":{"d":309,"n":3,"t":5},"speaker":"A","text":"そうでした。今週ですね。"},{"id":{"d":309,"n":4,"t":5},"speaker":"B","text":"はい。お母さんに何かプレゼントを用意しませんか？"},{"id":{"d":309,"n":5,"t":5},"speaker":"A","text":"はい、そうしましょう。お兄さんは、何かプレゼントのアイデアはありますか？"},{"id":{"d":309,"n":6,"t":5},"speaker":"B","text":"お母さんは、クラシック音楽が好きなので、演奏会のチケットはどうでしょうか？"},{"id":{"d":309,"n":7,"t":5},"speaker":"A","text":"良いですね。そうしましょう。"},{"id":{"d":309,"n":8,"t":5},"speaker":"B","text":"それに、花屋に行って、カーネーションも忘れずに買いましょう。"},{"id":{"d":309,"n":9,"t":5},"speaker":"A","text":"そうですね。花は、私が買っておきます。"},{"id":{"d":309,"n":10,"t":5},"speaker":"B","text":"ありがとうございます。お願いします。"}];
INSERT INTO sentence [{"id":{"d":310,"n":1,"t":5},"speaker":"A","text":"和田さんは、よく音楽を聴いていますね。"},{"id":{"d":310,"n":2,"t":5},"speaker":"B","text":"はい。私は、とあるバンドのファンなのです。"},{"id":{"d":310,"n":3,"t":5},"speaker":"A","text":"好きなバンドの曲を聴いているのですね。もう長くファンなのですか？"},{"id":{"d":310,"n":4,"t":5},"speaker":"B","text":"いえ、まだ好きになったばかりです。"},{"id":{"d":310,"n":5,"t":5},"speaker":"A","text":"そうなのですね。好きになったきっかけは何だったのですか？"},{"id":{"d":310,"n":6,"t":5},"speaker":"B","text":"私の見ていたアニメーションの主題歌を担当していたことです。その曲を聴いて、すてきな歌声だと思いました。"},{"id":{"d":310,"n":7,"t":5},"speaker":"A","text":"なるほど。まずは1つの曲から好きになったのですね。"},{"id":{"d":310,"n":8,"t":5},"speaker":"B","text":"はい。そこからバンドについて調べ出したら、いつの間にかファンになっていました。"}];
INSERT INTO sentence [{"id":{"d":311,"n":1,"t":5},"speaker":"A","text":"7月に入ってから、台風関連のニュースが多いですね。"},{"id":{"d":311,"n":2,"t":5},"speaker":"B","text":"はい。毎年、夏から秋にかけて台風の発生が多くなっています。"},{"id":{"d":311,"n":3,"t":5},"speaker":"A","text":"夏休みの時期と完全に同じですね。"},{"id":{"d":311,"n":4,"t":5},"speaker":"B","text":"はい。旅行の予定がなかなか立てづらいです。遠出したいのですが、台風が来ていると飛行機が欠航してしまいます。"},{"id":{"d":311,"n":5,"t":5},"speaker":"A","text":"台風の発生時期は分かりませんから、事前に回避することは難しいですね。"},{"id":{"d":311,"n":6,"t":5},"speaker":"B","text":"はい。直前にキャンセルができるプランを探してみましょうか？"},{"id":{"d":311,"n":7,"t":5},"speaker":"A","text":"そうですね。私は明日、仕事が休みなので旅行会社へ行って、問い合わせをしてみます。"},{"id":{"d":311,"n":8,"t":5},"speaker":"B","text":"それは助かります。希望通りのプランがあるといいですね。"}];
INSERT INTO sentence [{"id":{"d":312,"n":1,"t":5},"speaker":"A","text":"受験シーズンが近づいてきましたね。"},{"id":{"d":312,"n":2,"t":5},"speaker":"B","text":"はい。2月の入試まで残り1カ月です。"},{"id":{"d":312,"n":3,"t":5},"speaker":"A","text":"お正月はゆっくり過ごせましたか？"},{"id":{"d":312,"n":4,"t":5},"speaker":"B","text":"三が日はゆっくりできました。その後は、塾の冬期講習が入っていたので、勉強三昧でした。"},{"id":{"d":312,"n":5,"t":5},"speaker":"A","text":"そうだったのですか。三が日だけでもお休みがあって良かったですね。"},{"id":{"d":312,"n":6,"t":5},"speaker":"B","text":"はい。残り1カ月は、さらに気合を入れて、勉強に取り組みます。"},{"id":{"d":312,"n":7,"t":5},"speaker":"A","text":"体調管理には気を付けてくださいね。実力を十分に発揮できる状態で、試験に臨むことが一番です。"},{"id":{"d":312,"n":8,"t":5},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":313,"n":1,"t":5},"speaker":"A","text":"ひなこさんの宝物は何ですか？"},{"id":{"d":313,"n":2,"t":5},"speaker":"B","text":"友人が作ったアルバムです。私の誕生日にプレゼントしてくれました。"},{"id":{"d":313,"n":3,"t":5},"speaker":"A","text":"どのようなアルバムですか？"},{"id":{"d":313,"n":4,"t":5},"speaker":"B","text":"友人との写真が何枚も散りばめられたアルバムです。メッセージやかわいいイラストも描いてくれています。デザインを1から考えてくれました。"},{"id":{"d":313,"n":5,"t":5},"speaker":"A","text":"すてきなアルバムですね。普段から、写真はよく撮っているのですか？"},{"id":{"d":313,"n":6,"t":5},"speaker":"B","text":"はい。友人とどこかへ一緒に出掛けるときは、必ず撮ります。"},{"id":{"d":313,"n":7,"t":5},"speaker":"A","text":"思い出をたくさん残すことができているのですね。"},{"id":{"d":313,"n":8,"t":5},"speaker":"B","text":"はい。たまにスマートフォンで写真を見返すと、懐かしい気持ちになります。"}];
INSERT INTO sentence [{"id":{"d":314,"n":1,"t":5},"speaker":"A","text":"最近は、ボードゲームが流行していますね。"},{"id":{"d":314,"n":2,"t":5},"speaker":"B","text":"数人でプレーでき、簡単に盛り上がることができますからね。最近では、インターネットで無料のオンラインボードゲームも提供されています。"},{"id":{"d":314,"n":3,"t":5},"speaker":"A","text":"古くからあるボードゲームとしては「人生ゲーム」がありますね。"},{"id":{"d":314,"n":4,"t":5},"speaker":"B","text":"小さい頃は祖父母が家に遊びにきた時、「人生ゲーム」をしていました。私は、勝てなくて何回も不機嫌になっていたことを覚えています。"},{"id":{"d":314,"n":5,"t":5},"speaker":"A","text":"そうなのですね。今は、何か好きなボードゲームはありますか？"},{"id":{"d":314,"n":6,"t":5},"speaker":"B","text":"今は「カタン」というボードゲームが、一番楽しく好きです。"},{"id":{"d":314,"n":7,"t":5},"speaker":"A","text":"初めて聞きました。どのようなところが面白いのですか？"},{"id":{"d":314,"n":8,"t":5},"speaker":"B","text":"人生ゲームと同じく少し運の要素はあるのですが、サイコロの出目の確率を考えたり、必要物資を考えて場所を取ったり、とても頭を使うので面白いです。"}];
INSERT INTO sentence [{"id":{"d":315,"n":1,"t":5},"speaker":"A","text":"先日、家を掃除していたら私の幼少期の写真がたくさん出てきました。"},{"id":{"d":315,"n":2,"t":5},"speaker":"B","text":"懐かしい写真ですね。何歳の時の写真が出てきたのですか？"},{"id":{"d":315,"n":3,"t":5},"speaker":"A","text":"私が3歳か4歳の時の写真でした。まだ妹が生まれて数カ月のころのハイハイをしている写真がたくさんありました。生まれてすぐの妹と、父母、さらには父方と母方両方の祖父母が一緒に写った写真までありました。"},{"id":{"d":315,"n":4,"t":5},"speaker":"B","text":"20年ほど前の写真ですね。おじいさま、おばあさまは、やはりあなたのご両親に似ていましたか？"},{"id":{"d":315,"n":5,"t":5},"speaker":"A","text":"今の母が当時の祖母によく似ていますが、父は祖父に似ていませんでした。"},{"id":{"d":315,"n":6,"t":5},"speaker":"B","text":"お母さんがおばあさんに似てきているように、あと30年もしたらあなたもお父さんに似てくるのでしょうね。"},{"id":{"d":315,"n":7,"t":5},"speaker":"A","text":"父のような、子どもの友達とも仲良く話すことができるような親になりたいと思いますが、外見が似るのは話が違いますね。"},{"id":{"d":315,"n":8,"t":5},"speaker":"B","text":"そうですね。しかし遺伝には逆らえないというので将来はお父さんや、おじいさんのようになると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":316,"n":1,"t":5},"speaker":"A","text":"サブスクリプションの動画配信サービスを知っていますか？"},{"id":{"d":316,"n":2,"t":5},"speaker":"B","text":"知っています。今はさまざまなアプリケーションソフトで動画配信をしていますね。"},{"id":{"d":316,"n":3,"t":5},"speaker":"A","text":"そうですね。今となってはテレビ局が制作したドラマではなく、動画配信会社が個別に制作したドラマの方が、人気があったりもします。"},{"id":{"d":316,"n":4,"t":5},"speaker":"B","text":"韓国ドラマの人気作品『イカゲーム』が世界的にヒットをしていることも、もしかしたら、アプリケーションソフトで配信されていることと関係があるかもしれませんね。"},{"id":{"d":316,"n":5,"t":5},"speaker":"A","text":"なるほど。アプリケーションソフトなら字幕の設定も簡単にでき、どこの国のドラマ、映画でもいつでも視聴可能という利点がありますしね。"},{"id":{"d":316,"n":6,"t":5},"speaker":"B","text":"その通りですね。スマートフォンの使用率が増え、利用料金は月額1,500円程度であることも登録者が増える要因のようです。"},{"id":{"d":316,"n":7,"t":5},"speaker":"A","text":"月額1,500円であっても多くの人が登録しているため、動画配信会社側の収入も多くあります。収入が増えることで制作者側も有名な俳優を抜擢することが可能になります。"},{"id":{"d":316,"n":8,"t":5},"speaker":"B","text":"有名な俳優を抜擢し、ドラマを制作することで、動画配信サービスへの登録者も増えるという好循環になっているのですね。これからも、動画配信サービスは発展していきそうですね。"}];
INSERT INTO sentence [{"id":{"d":317,"n":1,"t":5},"speaker":"A","text":"有名な漫画の実写化が、多く制作されていますね。"},{"id":{"d":317,"n":2,"t":5},"speaker":"B","text":"そうですね。しかし、漫画で有名になった場合は、先にアニメーションになっているものが多くあり、アニメーションの声と実写の声のギャップに違和感を感じる人もいるようです。"},{"id":{"d":317,"n":3,"t":5},"speaker":"A","text":"実写化することで成功した作品には、例えば何がありますか？"},{"id":{"d":317,"n":4,"t":5},"speaker":"B","text":"個人的には、松山ケンイチと藤原竜也がダブル主演をした『デスノート』は、実写化に成功した作品だと思っています。この他にも、藤原竜也主演の『カイジ』は実写化成功の作品といえると思います。"},{"id":{"d":317,"n":5,"t":5},"speaker":"A","text":"藤原竜也さんの出演した映画が多いですね。"},{"id":{"d":317,"n":6,"t":5},"speaker":"B","text":"やはり引き込まれるような演技をする俳優の作品は、違和感を感じる暇もなく、見入ってしまうのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":318,"n":1,"t":5},"speaker":"A","text":"お笑いは好きですか？"},{"id":{"d":318,"n":2,"t":5},"speaker":"B","text":"大好きです。テレビでお笑い番組を放送している時には、ついチャンネルを止めてしまいます。あなたは、お笑い番組がお好きですか？"},{"id":{"d":318,"n":3,"t":5},"speaker":"A","text":"好きなのですが、私は録画したものしか見ることができません。"},{"id":{"d":318,"n":4,"t":5},"speaker":"B","text":"それは、なぜですか？"},{"id":{"d":318,"n":5,"t":5},"speaker":"A","text":"売れている方々は安心して見ることができ、大笑いしてしまいますが、若手のお笑いタレントが面白いことを話そうとして失敗した時、見ていることができなくなってしまいます。"},{"id":{"d":318,"n":6,"t":5},"speaker":"B","text":"共感性羞恥というものですね。"},{"id":{"d":318,"n":7,"t":5},"speaker":"A","text":"そうですね。あなたは、そのような状況でも、テレビ番組を見ていられるのですか？"},{"id":{"d":318,"n":8,"t":5},"speaker":"B","text":"彼らが面白いことを話そうとして失敗したところも、テレビ番組の演出として面白かったり、一緒に出ている先輩が面白くしてくれていたりするので、笑って見ることができます。"}];
INSERT INTO sentence [{"id":{"d":319,"n":1,"t":5},"speaker":"A","text":"最近明らかに、日が伸びてきましたね。"},{"id":{"d":319,"n":2,"t":5},"speaker":"B","text":"そうですね。今までだったら暗くて手元が見えないような時間でも、まだ明るいことが多々ありますね。"},{"id":{"d":319,"n":3,"t":5},"speaker":"A","text":"まだ寒いのですが、着々と冬を抜け出して春に向かっていることを感じることができますね。"},{"id":{"d":319,"n":4,"t":5},"speaker":"B","text":"私は、来春から社会人になるので楽しみな気持ちと、まだ学生でいたい気持ちが入り混じっています。"},{"id":{"d":319,"n":5,"t":5},"speaker":"A","text":"冬は、必ず春になりますから、社会人になっても、今とは異なる楽しさがあると思いますよ。"},{"id":{"d":319,"n":6,"t":5},"speaker":"B","text":"気の持ち方で、自分の置かれている状況に対する解釈が変わるということですね。"},{"id":{"d":319,"n":7,"t":5},"speaker":"A","text":"そうですね。もし、うまくいかないことがあっても自分の置かれている状況を、春夏秋冬に例えて、いつかは春が来ることを思い出して生活するのが良いですね。"},{"id":{"d":319,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。季節の移ろいも楽しみながら、これからなお、一層気を引き締めて頑張ろうと思います。"}];
INSERT INTO sentence [{"id":{"d":320,"n":1,"t":5},"speaker":"A","text":"ポーカーをご存知ですか？"},{"id":{"d":320,"n":2,"t":5},"speaker":"B","text":"知っています。ただギャンブルだと思っているのでしたことはありません。"},{"id":{"d":320,"n":3,"t":5},"speaker":"A","text":"確かに、ラスベガスでとても人気があるゲームですし、そのような考えを持つことは理解できます。"},{"id":{"d":320,"n":4,"t":5},"speaker":"B","text":"加えて、運の要素がとても大きいと感じているので、よりギャンブルだと感じてしまいます。"},{"id":{"d":320,"n":5,"t":5},"speaker":"A","text":"運の要素も、もちろんありますが、ポーカーは確率の世界です。そのため学習をすることで勝率を上げることが可能です。"},{"id":{"d":320,"n":6,"t":5},"speaker":"B","text":"2枚だけで確率など分かるのですか？正直信じることができません。"},{"id":{"d":320,"n":7,"t":5},"speaker":"A","text":"2枚の手札と3枚、1枚、1枚とめくられていくカード、相手のベットするチップ量全てを考えながら確率を組み立てるようです。"},{"id":{"d":320,"n":8,"t":5},"speaker":"B","text":"とても頭を使うのですね。他のギャンブルとは、異なるということがよく分かりました。少しポーカーについて調べてみようと思います。"}];
INSERT INTO sentence [{"id":{"d":321,"n":1,"t":5},"speaker":"A","text":"テレビで、玉置浩二さんがオーケストラとコラボレーションをしたライブを放送していました。"},{"id":{"d":321,"n":2,"t":5},"speaker":"B","text":"「玉置浩二さんの歌唱力は日本人の中でトップだ」、と声楽を専門にされている方が言っていましたが、オーケストラをバックにした歌唱はどうでしたか？"},{"id":{"d":321,"n":3,"t":5},"speaker":"A","text":"「素晴らしい」の一言に尽きます。正直、オーケストラの方がよく分かるのではないかと思っていましたが、そのようなことはなく、むしろ玉置浩二さんがオーケストラを引き立てているような雰囲気を感じました。"},{"id":{"d":321,"n":4,"t":5},"speaker":"B","text":"それはとても気になります。どの歌が良いと思いましたか？"},{"id":{"d":321,"n":5,"t":5},"speaker":"A","text":"もちろん全て素晴らしかったのですが、『メロディー』と『JUNK LAND』が素晴らしかったです。"},{"id":{"d":321,"n":6,"t":5},"speaker":"B","text":"『メロディー』は分かりますが、『JUNK LAND』も素晴らしかったのですか？"},{"id":{"d":321,"n":7,"t":5},"speaker":"A","text":"はい。正直私もこの曲はどのような演奏になるのだろうと思っていましたが、想像を遥かに超えた素晴らしい歌で、母と共に泣いてしまいました。"},{"id":{"d":321,"n":8,"t":5},"speaker":"B","text":"玉置さんの歌は、歌詞が意味を持ってしっかり伝わってくる感覚があるので、あなたたちは、より涙を誘われたのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":322,"n":1,"t":5},"speaker":"A","text":"あいみょんという歌手を知っていますか？"},{"id":{"d":322,"n":2,"t":5},"speaker":"B","text":"もちろん知っています。とても人気がありますよね。"},{"id":{"d":322,"n":3,"t":5},"speaker":"A","text":"大御所と呼ばれる方々からも、とても高い評価を受けていますね。"},{"id":{"d":322,"n":4,"t":5},"speaker":"B","text":"そうですね。他のアーティストとのコラボレーションや、歌の提供をして、活躍の幅を広げています。"},{"id":{"d":322,"n":5,"t":5},"speaker":"A","text":"そのようなあいみょんのデビュー曲は『貴方解剖純愛歌〜死ね〜』という曲です。"},{"id":{"d":322,"n":6,"t":5},"speaker":"B","text":"一度聞いたことがありますが、今の時代に受け入れられたのが不思議でした。"},{"id":{"d":322,"n":7,"t":5},"speaker":"A","text":"あの曲をあいみょんは10分で作ってしまったそうです。とても素晴らしい才能ですね。"},{"id":{"d":322,"n":8,"t":5},"speaker":"B","text":"頭の中に、常に曲が流れているのでしょうか。うらやましい才能です。"}];
INSERT INTO sentence [{"id":{"d":323,"n":1,"t":5},"speaker":"A","text":"最近私はスマートフォンでマージャンをしています。"},{"id":{"d":323,"n":2,"t":5},"speaker":"B","text":"今のスマートフォンは、無料で遊ぶことができるアプリケーションソフトもたくさんあるので良いですね。"},{"id":{"d":323,"n":3,"t":5},"speaker":"A","text":"そうですね。全く知らなかったマージャンを、1からアプリケーションソフトで勉強しながら、プレーしているので頭を使っている気がして楽しいです。"},{"id":{"d":323,"n":4,"t":5},"speaker":"B","text":"なぜマージャンを始めようと思ったのですか？"},{"id":{"d":323,"n":5,"t":5},"speaker":"A","text":"祖母が「認知症防止のためにマージャンを始めた」と言っていたので、一緒に対決することができたら喜んでくれると思い、勉強しています。"},{"id":{"d":323,"n":6,"t":5},"speaker":"B","text":"いいことですね。もうおばあちゃんとは一緒にプレーしたのですか？"},{"id":{"d":323,"n":7,"t":5},"speaker":"A","text":"まだ祖母とはしたことがありません。対決はアプリケーションソフトの自動マッチ機能でしか経験したことがありません。"},{"id":{"d":323,"n":8,"t":5},"speaker":"B","text":"早くおばあちゃんと一緒にできると良いですね。楽しそうです。"}];
INSERT INTO sentence [{"id":{"d":324,"n":1,"t":5},"speaker":"A","text":"バレンタインデーが近いですね。"},{"id":{"d":324,"n":2,"t":5},"speaker":"B","text":"そうですね。今朝のニュース番組でも特集をしていましたが、今年は義理チョコすら買わず、自分用チョコを購入する人が多いようです。"},{"id":{"d":324,"n":3,"t":5},"speaker":"A","text":"会社の人宛に、義理チョコを買う人もいないということですか？"},{"id":{"d":324,"n":4,"t":5},"speaker":"B","text":"「ほとんど出社もしていないため、会社の人宛に買う必要がないと考えています」、とニュース番組内でインタビューされた人は言っていました。"},{"id":{"d":324,"n":5,"t":5},"speaker":"A","text":"出社しないなら、自分と家族のためにチョコレートを買えばよいですね。"},{"id":{"d":324,"n":6,"t":5},"speaker":"B","text":"そのため、フェアトレードのチョコレートを買って、何か社会貢献ができたら、という考え方の人も多いようです。"},{"id":{"d":324,"n":7,"t":5},"speaker":"A","text":"自分が、チョコレートを食べながら、社会貢献をしていると考えると、よりおいしく感じそうですね。"},{"id":{"d":324,"n":8,"t":5},"speaker":"B","text":"購入した物の金額を知っている分、感謝の気持ちを込めながら、食べることができるそうです。"}];
INSERT INTO sentence [{"id":{"d":325,"n":1,"t":5},"speaker":"A","text":"漫画の、いわゆるネタバレサイトを知っていますか？"},{"id":{"d":325,"n":2,"t":5},"speaker":"B","text":"ネタバレというと無断で漫画の内容について情報発信をしているサイトということですか？"},{"id":{"d":325,"n":3,"t":5},"speaker":"A","text":"その通りです。ある人気漫画のネタバレサイトの運営会社が書類送検されました。ある漫画のせりふやストーリーを無断でサイトに掲載したことが、著作権を侵害したそうです。"},{"id":{"d":325,"n":4,"t":5},"speaker":"B","text":"文章を読むだけで漫画の内容を把握することができると、漫画を買う必要がなくなるため、もちろん違法ですね。"},{"id":{"d":325,"n":5,"t":5},"speaker":"A","text":"そうですね。漫画を読むことが好きな人は、実際に絵を見て楽しむと思っていましたがネタバレが問題になるということは、漫画の内容を説明した文章を読んでいる人もいたということですね。"},{"id":{"d":325,"n":6,"t":5},"speaker":"B","text":"つまり、絵を楽しんでいるのではなく、内容を楽しんでいる人がいるということですね。"},{"id":{"d":325,"n":7,"t":5},"speaker":"A","text":"せっかく漫画家の方は絵に時間をかけて作成しているため、絵もしっかり楽しんでほしいです。"},{"id":{"d":325,"n":8,"t":5},"speaker":"B","text":"そうですね。今は出版社が提供しているスマートフォンの無料アプリケーションソフトで漫画を手軽に楽しむこともできるので、そちらも活用してほしいです。"}];
INSERT INTO sentence [{"id":{"d":326,"n":1,"t":5},"speaker":"A","text":"今年も節分の時期ですね。豆まきはしましたか？"},{"id":{"d":326,"n":2,"t":5},"speaker":"B","text":"鬼役はいませんでしたが、家族で豆まきをしました。"},{"id":{"d":326,"n":3,"t":5},"speaker":"A","text":"鬼役がいない方が、目に見える悪者もいませんから、平和に豆まきが進みそうですね。"},{"id":{"d":326,"n":4,"t":5},"speaker":"B","text":"鬼役といえば、昔町内会の豆まきで鬼役の人が石を投げられたという記事を見たことがあります。"},{"id":{"d":326,"n":5,"t":5},"speaker":"A","text":"そのようなひどいことがあったのですね。鬼役の人は大丈夫だったのですか？"},{"id":{"d":326,"n":6,"t":5},"speaker":"B","text":"3針ほど縫ったそうですが、視覚には問題はなかったそうです。不幸中の幸いですね。"},{"id":{"d":326,"n":7,"t":5},"speaker":"A","text":"大変な話ですね。鬼という悪いものに対してならば、何をしても良いという発想になってしまう可能性があるということですね。"},{"id":{"d":326,"n":8,"t":5},"speaker":"B","text":"その話を聞いてから、豆まきで鬼役は作らないようにしようと思いました。"}];
INSERT INTO sentence [{"id":{"d":327,"n":1,"t":5},"speaker":"A","text":"北海道日本ハムファイターズの「BIGBOSS」こと新庄剛志監督をご存知ですか？"},{"id":{"d":327,"n":2,"t":5},"speaker":"B","text":"もちろん知っています。現役時代は派手な入場やプレーで大人気でしたね。"},{"id":{"d":327,"n":3,"t":5},"speaker":"A","text":"はい。その新庄さんがバラエティー番組で100万円を生まれ故郷で使うという企画をしていました。"},{"id":{"d":327,"n":4,"t":5},"speaker":"B","text":"とても派手な番組ですね。新庄さんはどのようなお金の使い方をしていましたか？"},{"id":{"d":327,"n":5,"t":5},"speaker":"A","text":"豪華なホテルに泊まって、いろいろな人にごちそうをしていました。その中でも彼の人間性がとても出ているシーンがありました。"},{"id":{"d":327,"n":6,"t":5},"speaker":"B","text":"ごちそうをしている時点でとても人間性が出ているように感じますが、どのようなシーンでしたか？"},{"id":{"d":327,"n":7,"t":5},"speaker":"A","text":"新庄さんの所属していた少年野球チームの小学6年生に野球道具をプレゼントし、自身の現役時代のグローブの思い出を語っていたのです。プロとして、見本となる人間として素晴らしい話を小学6年生の少年たちに話していました。"},{"id":{"d":327,"n":8,"t":5},"speaker":"B","text":"その子どもたちは、もしプロになれなくてもそのような生き方をしている人の話を聞けただけで素晴らしい経験ですね。"}];
INSERT INTO sentence [{"id":{"d":328,"n":1,"t":5},"speaker":"A","text":"現在中国では、旧正月といわれる時期ですね。"},{"id":{"d":328,"n":2,"t":5},"speaker":"B","text":"例年であれば、旧正月は爆竹や花火をするらしいのですが、昨今のコロナウイルス感染症と、今中国で開催されている北京2022オリンピック競技大会のため今年は爆竹や花火が禁止されているそうです。"},{"id":{"d":328,"n":3,"t":5},"speaker":"A","text":"なぜ、北京2022オリンピック競技大会が関係しているのですか？"},{"id":{"d":328,"n":4,"t":5},"speaker":"B","text":"元々北京は、大気汚染がひどかったため、大会開催にあたり禁止したそうです。"},{"id":{"d":328,"n":5,"t":5},"speaker":"A","text":"それでは旧正月のお祝いは、家でおとなしく家族や親族としているのでしょうか？"},{"id":{"d":328,"n":6,"t":5},"speaker":"B","text":"爆竹などはしていないそうですが、外出などはしているようです。北京2022オリンピック競技大会も限られた人しか、観客としては参加できないので、観戦はあまりしていないようです。"},{"id":{"d":328,"n":7,"t":5},"speaker":"A","text":"せっかくの北京2022オリンピック競技大会なので、家で観戦したら良いと思うのですが、旧正月のお祝いはしたいですね。"},{"id":{"d":328,"n":8,"t":5},"speaker":"B","text":"楽しいことは何事もしたいのが人間なので、仕方ありませんね。"}];
INSERT INTO sentence [{"id":{"d":329,"n":1,"t":5},"speaker":"A","text":"昨年まで5年間、冬の時期は北海道にいたので、雪がない2月はとても不思議な感覚があります。"},{"id":{"d":329,"n":2,"t":5},"speaker":"B","text":"確かに北海道は、この時期は雪が降り積もっていますね。ただ大学進学前はこちらにいたのではありませんか？"},{"id":{"d":329,"n":3,"t":5},"speaker":"A","text":"はい、元々は本州に住んでいましたが、5年間北海道にいましたので、雪がない感覚を忘れてしまいました。"},{"id":{"d":329,"n":4,"t":5},"speaker":"B","text":"不思議ですね。すると北海道はこの時期とても寒いのでこちらでは、暖かく過ごしているのですか？"},{"id":{"d":329,"n":5,"t":5},"speaker":"A","text":"全くそのようなことはありません。むしろこちらの方が寒いと感じています。"},{"id":{"d":329,"n":6,"t":5},"speaker":"B","text":"こちらの方が寒いですか？気温としては明らかに北海道の方が低いですが、なぜこちらの方が寒いのですか？"},{"id":{"d":329,"n":7,"t":5},"speaker":"A","text":"北海道では毎日ヒートテックを着て、温かくして生活していました。しかし、本州ではそれを着なくても生活できる程度の寒さであることが理由です。"},{"id":{"d":329,"n":8,"t":5},"speaker":"B","text":"そうなのですね。するとこちらでヒートテックを着ると、暑すぎる可能性があるということですね。"}];
INSERT INTO sentence [{"id":{"d":330,"n":1,"t":5},"speaker":"A","text":"豆まきは、何をまいていますか？"},{"id":{"d":330,"n":2,"t":5},"speaker":"B","text":"私はもちろん大豆をまいています。その後、年齢の数だけ大豆を食べます。"},{"id":{"d":330,"n":3,"t":5},"speaker":"A","text":"本州では、そのように大豆をまいて、年齢の数だけ大豆を食べることが習慣ですね。"},{"id":{"d":330,"n":4,"t":5},"speaker":"B","text":"異なる地域が存在するのですか？"},{"id":{"d":330,"n":5,"t":5},"speaker":"A","text":"北海道では、落花生を殻ごとまきます。そして年の数だけ食べるそうです。"},{"id":{"d":330,"n":6,"t":5},"speaker":"B","text":"落花生を投げるのですか？同じ豆ですが、鬼にまくものとしてより攻撃力の高いものをまいていますね。"},{"id":{"d":330,"n":7,"t":5},"speaker":"A","text":"加えて落花生は、殻の中に2つや3つ入っているものもあるので、本州の人よりも豆もたくさん食べていますね。"},{"id":{"d":330,"n":8,"t":5},"speaker":"B","text":"なぜ、北海道では、落花生を投げるのかを、調べたら面白そうですね。"}];
INSERT INTO sentence [{"id":{"d":331,"n":1,"t":5},"speaker":"A","text":"節分に、恵方巻きを恵方の方角を向いて食べるという風習は、大阪に端を発するものだといわれていますが、知っていましたか？"},{"id":{"d":331,"n":2,"t":5},"speaker":"B","text":"全国的なものだと思っていましたが違うのですか？"},{"id":{"d":331,"n":3,"t":5},"speaker":"A","text":"はい、そのようです。私の父は関西人ですが、恵方巻きの文化はもちろんあったそうです。しかし、母は関東の人間ですので習慣としてはなかったようです。"},{"id":{"d":331,"n":4,"t":5},"speaker":"B","text":"それでは、豆まきも関西の風習なのでしょうか？"},{"id":{"d":331,"n":5,"t":5},"speaker":"A","text":"豆まきは宮中で行われていた、「ついな」という鬼を払う行事から変化したもののようです。"},{"id":{"d":331,"n":6,"t":5},"speaker":"B","text":"では、なぜ大阪では恵方巻きを節分に食べることになったのでしょうか？"},{"id":{"d":331,"n":7,"t":5},"speaker":"A","text":"詳しくは分からず、起源はいまだに不明なようです。"},{"id":{"d":331,"n":8,"t":5},"speaker":"B","text":"節分の夜に、おいしい巻き物を食べることは、それだけでうれしいので、恵方巻きの習慣があって良かったです。"}];
INSERT INTO sentence [{"id":{"d":332,"n":1,"t":5},"speaker":"A","text":"ひな祭りなので、家でおひなさまを出しました。"},{"id":{"d":332,"n":2,"t":5},"speaker":"B","text":"おひなさまは、とてもきれいですね。職人の技術に感心します。"},{"id":{"d":332,"n":3,"t":5},"speaker":"A","text":"今この年になっておひなさまはきれいだと思いますが、小さい頃は、なぜ飾るのか全く理解できませんでした。"},{"id":{"d":332,"n":4,"t":5},"speaker":"B","text":"男の子は、あのような人形にあまり興味を示しませんね。なぜなのでしょうか？"},{"id":{"d":332,"n":5,"t":5},"speaker":"A","text":"分かりません。きれいだと思う以上に、場所を取るものだなと思っていたことは覚えています。"},{"id":{"d":332,"n":6,"t":5},"speaker":"B","text":"おひなさまは、男の子には関係ない行事だから仕方ないのかもしれませんね。"},{"id":{"d":332,"n":7,"t":5},"speaker":"A","text":"ただ、ひなあられや、ひし餅は、とても好きだったので、ひな人形を早く片付けて食べたいと思っていました。"},{"id":{"d":332,"n":8,"t":5},"speaker":"B","text":"小さい頃は、おいしいものを食べたいですよね。当然のことだと思います。"}];
INSERT INTO sentence [{"id":{"d":333,"n":1,"t":5},"speaker":"A","text":"実家周辺が田舎ということもあり、よく5発程度の花火が打ち上がります。"},{"id":{"d":333,"n":2,"t":5},"speaker":"B","text":"なぜ、田舎だから花火が打ち上がるのですか？都会の方が花火は打ち上がりそうです。"},{"id":{"d":333,"n":3,"t":5},"speaker":"A","text":"田舎は、個人で花火師さんに依頼をして、花火を打ち上げる人がいます。何かの記念日に打ち上げる人がいるようです。"},{"id":{"d":333,"n":4,"t":5},"speaker":"B","text":"そうだとしても、田舎で打ち上げる意味はなぜなのでしょうか？"},{"id":{"d":333,"n":5,"t":5},"speaker":"A","text":"田舎の良いところは、土地がたくさんあるところです。打ち上げ花火をしても、あまり近所迷惑になりません。加えて空気がきれいなので、花火がきれいに見えるということも理由にあると思います。"},{"id":{"d":333,"n":6,"t":5},"speaker":"B","text":"確かに、都会で記念日の度に、花火を打ち上げていたら近所迷惑も甚だしいですね。"},{"id":{"d":333,"n":7,"t":5},"speaker":"A","text":"その点、田舎では人も少ないので1カ月に数回くらいの間隔で、打ち上げ花火が打ち上がります。ちょうどいい頻度です。"},{"id":{"d":333,"n":8,"t":5},"speaker":"B","text":"近くで、きれいに上がっている花火を、家の中から見ることができるのは、うらやましいですね。"}];
INSERT INTO sentence [{"id":{"d":334,"n":1,"t":5},"speaker":"A","text":"今年の冬はとても寒く、本州でも雪が降る日が多くありますが、あなたは雪を楽しんでいますか？"},{"id":{"d":334,"n":2,"t":5},"speaker":"B","text":"先日の積雪の際には、楽しむよりも除雪作業で筋肉痛になってしまいました。"},{"id":{"d":334,"n":3,"t":5},"speaker":"A","text":"私の家の近くの公園には、小さなかまくらが作られていました。"},{"id":{"d":334,"n":4,"t":5},"speaker":"B","text":"かわいいですね。幼稚園生や学校が休みの小学生が、暇を持て余し、作ったのでしょうか？"},{"id":{"d":334,"n":5,"t":5},"speaker":"A","text":"そのかまくらは、中学生が作っていたようです。入り口はとても小さかったのですが、中学生男子2人組が走って入りに行っていました。"},{"id":{"d":334,"n":6,"t":5},"speaker":"B","text":"中学生も学校が休みで、時間を持て余したのでしょうね。小さいかまくらとは、人が入れそうな大きさですか？"},{"id":{"d":334,"n":7,"t":5},"speaker":"A","text":"いいえ。人の頭しか入っていませんでした。それでも、とても楽しそうだったので、ほほ笑ましかったです。"},{"id":{"d":334,"n":8,"t":5},"speaker":"B","text":"遊べる時にたくさん遊んでおくと、将来の思い出話のタネになるのでいいことですね。"}];
INSERT INTO sentence [{"id":{"d":335,"n":1,"t":5},"speaker":"A","text":"私の妹が、卒業祝いかつ誕生日祝いとして、私にイヤホンをプレゼントしてくれました。"},{"id":{"d":335,"n":2,"t":5},"speaker":"B","text":"とても良い妹さんですね。欲しいイヤホンをもらったのですか？"},{"id":{"d":335,"n":3,"t":5},"speaker":"A","text":"元々自分で買う予定だった物を、妹が買ってくれました。日本で発売してまだ2日しかたっていない時に買ってくれたので、ケースに傷をつけないためのカバーもまだ売られていません。"},{"id":{"d":335,"n":4,"t":5},"speaker":"B","text":"傷をつけないために大切に使わないといけませんね。"},{"id":{"d":335,"n":5,"t":5},"speaker":"A","text":"そのために、できるだけケースを外に持ち出さないようにしています。"},{"id":{"d":335,"n":6,"t":5},"speaker":"B","text":"ちなみに性能はどうですか？やはり良いですか？"},{"id":{"d":335,"n":7,"t":5},"speaker":"A","text":"もちろんとても良いです。元々ヘッドホンを作っていた会社の物なので、低音の響きがとても心地良いです。"},{"id":{"d":335,"n":8,"t":5},"speaker":"B","text":"それはとても良い物ですね。より大切にしないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":336,"n":1,"t":5},"speaker":"A","text":"筋力トレーニングを毎日欠かさずに行っていますが、その時にかける音楽にこだわり始めました。"},{"id":{"d":336,"n":2,"t":5},"speaker":"B","text":"筋力トレーニングはモチベーションが大切なので、音楽は大切ですね。どのような音楽を聴いているのですか？"},{"id":{"d":336,"n":3,"t":5},"speaker":"A","text":"Kポップを聴いています。"},{"id":{"d":336,"n":4,"t":5},"speaker":"B","text":"なぜKポップなのですか？ジムでは洋楽がかかっていることが多いので、洋楽かと思っていました。"},{"id":{"d":336,"n":5,"t":5},"speaker":"A","text":"私が聴いているのは主に女性アイドルです。Kポップの女性アイドルは、とても厳しい減量や運動をして自分を追い込んでいるとよく聞きますので、その人たちを見ながら負けられないと思いながら、筋力トレーニングをしています。"},{"id":{"d":336,"n":6,"t":5},"speaker":"B","text":"女性アイドルに負けないくらい、自分磨きをするために、筋力トレーニングをしているのですね。"},{"id":{"d":336,"n":7,"t":5},"speaker":"A","text":"その通りです。また、アップテンポの曲が多いので楽しくなるという理由もあります。"},{"id":{"d":336,"n":8,"t":5},"speaker":"B","text":"しっかり理由があって、Kポップにしているのですね。これからもKポップを聴きながら頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":337,"n":1,"t":5},"speaker":"A","text":"人気アニメーション『怪盗グルー』シリーズに登場するミニオンというキャラクターを見たことがありますか？"},{"id":{"d":337,"n":2,"t":5},"speaker":"B","text":"黄色い生き物のアニメーション映画ですね。しっかりと見たことはありませんが、ミニオンは、かわいいと聞いています。"},{"id":{"d":337,"n":3,"t":5},"speaker":"A","text":"私はミニオンが大好きです。ぬいぐるみを持っているほどです。"},{"id":{"d":337,"n":4,"t":5},"speaker":"B","text":"それは相当好きですね。どこが好きなのですか？"},{"id":{"d":337,"n":5,"t":5},"speaker":"A","text":"ミニオンはそれぞれ1匹ずつ個性があるところです。痩せているものがいたり、太っているものがいたり、目が1つのものがいたり、2つのものがいたりとさまざまです。その分、自分の好きなミニオンを決めやすいという理由があります。"},{"id":{"d":337,"n":6,"t":5},"speaker":"B","text":"個性があるから特に好きなミニオンを見つけることが可能ということですね。どのミニオンが一番好きなのですか？"},{"id":{"d":337,"n":7,"t":5},"speaker":"A","text":"私は太っている2つ目のボブというキャラクターが一番好きです。ミニオンたちの中でも弟のような存在でとても甘えんぼうでかわいいです。"},{"id":{"d":337,"n":8,"t":5},"speaker":"B","text":"名前まであるのですね。それはとても魅力的です。私も1番好きなミニオンを探してみたいと思います。"}];
INSERT INTO sentence [{"id":{"d":338,"n":1,"t":5},"speaker":"A","text":"エープリルフールを知っていますか？"},{"id":{"d":338,"n":2,"t":5},"speaker":"B","text":"4月1日のことですね。確か、うそをついていい日ですね？"},{"id":{"d":338,"n":3,"t":5},"speaker":"A","text":"正確にはうそをついていいのは午前中のみで、午後は午前についたうそを正す必要があるそうです。"},{"id":{"d":338,"n":4,"t":5},"speaker":"B","text":"1日中うそをついて良いわけではないのですね。いつうそを言っても良いものだと思っていました。"},{"id":{"d":338,"n":5,"t":5},"speaker":"A","text":"加えてエープリルフールについたうそは、いずれ現実になるそうです。例えば、「太った」といううそをついていた場合、本当に太ってしまうそうです。"},{"id":{"d":338,"n":6,"t":5},"speaker":"B","text":"つまり、痩せたい人は、「痩せた」とうそをつくのがいいということですね。"},{"id":{"d":338,"n":7,"t":5},"speaker":"A","text":"その通りです。"},{"id":{"d":338,"n":8,"t":5},"speaker":"B","text":"それでは、私は筋肉をつけたいので、今年のエープリルフールでは、「筋肉がついた」とうそをつくことにします。"}];
INSERT INTO sentence [{"id":{"d":339,"n":1,"t":5},"speaker":"A","text":"最近の、テレビゲームをご存知ですか？"},{"id":{"d":339,"n":2,"t":5},"speaker":"B","text":"私は、少ししか知りません。"},{"id":{"d":339,"n":3,"t":5},"speaker":"A","text":"それならば、最近のテレビゲームをプレーしたら驚くかもしれませんね。とても解像度が高く、まるで本当にその世界にいるかのようなクオリティーです。"},{"id":{"d":339,"n":4,"t":5},"speaker":"B","text":"今の技術はそれほど発展しているのですね。昔のドット絵のゲームからは、想像がつかないほどきれいな画面になっているということですね。"},{"id":{"d":339,"n":5,"t":5},"speaker":"A","text":"最新のゲーム画面であれば、人の髪の毛はもちろんですが、産毛のような細かいものまできれいに表現されているようです。"},{"id":{"d":339,"n":6,"t":5},"speaker":"B","text":"それは素晴らしいですね。ゲーム画面を映すテレビの技術も、とても上がっているということですね。"},{"id":{"d":339,"n":7,"t":5},"speaker":"A","text":"加えて音楽もとてもクオリティーが上がっているので、より臨場感が増しています。ゲームをしながら少し鳥肌が立つほどでした。"},{"id":{"d":339,"n":8,"t":5},"speaker":"B","text":"テレビゲームは、ただの子どもの時間潰しの道具ではなく、今となっては大人の遊び道具でもあるということですね。"}];
INSERT INTO sentence [{"id":{"d":340,"n":1,"t":5},"speaker":"A","text":"ポケットモンスターを知っていますか？"},{"id":{"d":340,"n":2,"t":5},"speaker":"B","text":"もちろん知っています。ちょうどポケットモンスターが一般的になってきたくらいの年代なので、ポケモン世代と言われる年齢だと思います。"},{"id":{"d":340,"n":3,"t":5},"speaker":"A","text":"それでは、ポケットモンスターが昔のゲームソフトのリメイクバージョンを発売していることはご存知ですか？"},{"id":{"d":340,"n":4,"t":5},"speaker":"B","text":"知っています。ちょうど私か、それより少し年上の方々が子どもの頃に遊んでいたソフトが、リメイクして発売されているので、購入者が身の回りにたくさんいました。"},{"id":{"d":340,"n":5,"t":5},"speaker":"A","text":"子どもの頃に遊んでいて思い入れがあるゲームのリメイクは、購入する人が多いですね。"},{"id":{"d":340,"n":6,"t":5},"speaker":"B","text":"大人ならではの遊び方もあります。例えば、小さい頃は格好良いポケットモンスターをみんなが好んでいましたが、大人になった今では、効率の良いポケットモンスターを選ぶということがあります。"},{"id":{"d":340,"n":7,"t":5},"speaker":"A","text":"ただ楽しむのではなく、頭を使ってできるだけ強くなるということですね。それもとても楽しそうです。"},{"id":{"d":340,"n":8,"t":5},"speaker":"B","text":"他にも、子どもの頃の思い入れのあるポケットモンスターだけで、ゲームクリアをしている人もいました。とても懐かしくて、自分自身の成長も感じることができる良いゲームです。"}];
INSERT INTO sentence [{"id":{"d":341,"n":1,"t":5},"speaker":"A","text":"今日は寒くなりそうだから、しっかり布団を掛けて寝てくださいね。"},{"id":{"d":341,"n":2,"t":5},"speaker":"B","text":"昨日の夜少し寒かったので、布団を1枚増やしたいのですが、予備の布団はどこにありますか？"},{"id":{"d":341,"n":3,"t":5},"speaker":"A","text":"和室のたんすの中に入っているから、自分で必要なだけ持っていってください。"},{"id":{"d":341,"n":4,"t":5},"speaker":"B","text":"お母さんは、布団を増やす必要ありませんか？もし必要だったら一緒に持っていきます。"},{"id":{"d":341,"n":5,"t":5},"speaker":"A","text":"私は、電気毛布を敷いているので大丈夫です。ありがとうございます。"},{"id":{"d":341,"n":6,"t":5},"speaker":"B","text":"いつもより寒くなるかもしれませんが、近くに置いておくだけでも必要ありませんか？"},{"id":{"d":341,"n":7,"t":5},"speaker":"A","text":"それでは、和室に羽毛布団を1枚置いておくので、一緒に持って行ってもらっても良いですか？"},{"id":{"d":341,"n":8,"t":5},"speaker":"B","text":"分かりました。お母さんの部屋に置いておきます。"}];
INSERT INTO sentence [{"id":{"d":342,"n":1,"t":5},"speaker":"A","text":"先輩は家で何もすることがない時、どのように時間を潰していますか？"},{"id":{"d":342,"n":2,"t":5},"speaker":"B","text":"私は家の中では、メリハリをつけて過ごすようにしています。"},{"id":{"d":342,"n":3,"t":5},"speaker":"A","text":"メリハリをつけるとは、どのように生活しているのですか？"},{"id":{"d":342,"n":4,"t":5},"speaker":"B","text":"片付けや仕事、学習などのクオリティーを上げることが可能なものを探し出して作業をし、2時間ほどたったら映画やドラマなどを見て息抜きをするということをしています。"},{"id":{"d":342,"n":5,"t":5},"speaker":"A","text":"確かに、仕事内容のクオリティーを上げる作業は大切ですね。また、生活の質を上げるための片付けや学習も大切だと思います。"},{"id":{"d":342,"n":6,"t":5},"speaker":"B","text":"あまり仕事をしたくない日は、自分自身を向上させるという意味で読書をしています。息抜きになりそうなことも学習として取り入れたりしています。"},{"id":{"d":342,"n":7,"t":5},"speaker":"A","text":"そのように、しっかりメリハリをつけて生活することで、人間として成長することができるということですね。"},{"id":{"d":342,"n":8,"t":5},"speaker":"B","text":"成長できるかどうかは分かりませんが、その方が楽しく生活することができるような気がしています。"}];
INSERT INTO sentence [{"id":{"d":343,"n":1,"t":5},"speaker":"A","text":"小さい頃、あなたは、絵描き歌をよく教えてくれましたね。"},{"id":{"d":343,"n":2,"t":5},"speaker":"B","text":"幼稚園の頃のあなたは、絵描き歌を一番楽しんでくれたので、何度も歌っていましたね。"},{"id":{"d":343,"n":3,"t":5},"speaker":"A","text":"成長した今だから思うことは、絵描き歌では、できあがりのものを知らなければ描くことができないのではないか、ということです。"},{"id":{"d":343,"n":4,"t":5},"speaker":"B","text":"その企画をテレビ番組で放送していたのを見たことがあります。"},{"id":{"d":343,"n":5,"t":5},"speaker":"A","text":"幼稚園の先生をしていた経験から考えると、どう思いますか？"},{"id":{"d":343,"n":6,"t":5},"speaker":"B","text":"確かに、今日はカエルを描く、と前もって伝えてから絵描き歌を始めていたので、何も知らないで描いたら変なことになりそうです。"},{"id":{"d":343,"n":7,"t":5},"speaker":"A","text":"今だからこそ、指示通りに描くことで、本当はどのような絵になるのか検証してみたいですね。"},{"id":{"d":343,"n":8,"t":5},"speaker":"B","text":"まずは簡単なものからスタートしてみましょう。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":344,"n":1,"t":5},"speaker":"A","text":"映画監督の濱口竜介氏がベルリン国際映画祭の審査員を務めることになりました。"},{"id":{"d":344,"n":2,"t":5},"speaker":"B","text":"とても素晴らしいことですね。日本人として誇りに思います。"},{"id":{"d":344,"n":3,"t":5},"speaker":"A","text":"濱口監督自身の作品も、今年のアカデミー賞の4部門にノミネートされています。"},{"id":{"d":344,"n":4,"t":5},"speaker":"B","text":"ニュース番組で映画について報じていることは少ない中、何度も報道されているのでとても興味があります。"},{"id":{"d":344,"n":5,"t":5},"speaker":"A","text":"私も興味があるので見てみたいです。ちなみに濱口監督は、実は、去年のベルリン国際映画祭で審査員大賞である銀熊賞を受賞するなど有名な監督なようです。"},{"id":{"d":344,"n":6,"t":5},"speaker":"B","text":"あまり映画を見ないので、恥ずかしながら名前は知りませんでしたが、日本にも素晴らしい監督がいるということですね。"},{"id":{"d":344,"n":7,"t":5},"speaker":"A","text":"その通りです。洋画ももちろんスケールが大きく、格好良いものが多いですが、邦画にも、ぜひ注目してほしいですね。"},{"id":{"d":344,"n":8,"t":5},"speaker":"B","text":"この審査員選出や、アカデミー賞ノミネートによって、邦画も大きな注目が集められたら良いと思います。"}];
INSERT INTO sentence [{"id":{"d":345,"n":1,"t":5},"speaker":"A","text":"今の若い人は、どのようなジャンルの曲を聴いているのですか？"},{"id":{"d":345,"n":2,"t":5},"speaker":"B","text":"いろいろと聴いています。少し前とは異なり、CDを買わなくても音楽を聴くことができるので、何か決まったジャンルや歌手の歌を聴く人というのはあまりいないと思います。"},{"id":{"d":345,"n":3,"t":5},"speaker":"A","text":"そうなのですね。それでは今の人たちの流行は分からないということですか？"},{"id":{"d":345,"n":4,"t":5},"speaker":"B","text":"いいえ。さまざまな音楽配信サイトに流行している曲のランキングが載っています。日本のその日の人気トップ100や、アメリカの人気トップ100、さらには世界の人気トップ100などを見ることができます。"},{"id":{"d":345,"n":5,"t":5},"speaker":"A","text":"そのような情報を見ることで、どのような曲が一番人気なのか確認することができるということですか？"},{"id":{"d":345,"n":6,"t":5},"speaker":"B","text":"その通りです。ランキングを見たら分かりますが、さまざまなジャンルの歌が人気なので、いろいろな作品を知ることができます。"},{"id":{"d":345,"n":7,"t":5},"speaker":"A","text":"その中で興味を持った曲があったら、ダウンロードをするといいということですね。"},{"id":{"d":345,"n":8,"t":5},"speaker":"B","text":"その通りです。お母さんの好きなクラシック音楽もたくさんありますので、ぜひ楽しんでください。"}];
INSERT INTO sentence [{"id":{"d":346,"n":1,"t":5},"speaker":"A","text":"最近は、インターネット番組が人気ですね。"},{"id":{"d":346,"n":2,"t":5},"speaker":"B","text":"その中で面白い番組がありました。それはギャグを購入するというものです。"},{"id":{"d":346,"n":3,"t":5},"speaker":"A","text":"ギャグを購入するとはどういうことですか？"},{"id":{"d":346,"n":4,"t":5},"speaker":"B","text":"他の人からギャグをお金で購入し、それ以降は、購入したギャグを、自分の持ちネタとして好きに使ってよいそうです。"},{"id":{"d":346,"n":5,"t":5},"speaker":"A","text":"とても面白い考え方の番組ですね。しかし地上波では放送できないものなのですか？"},{"id":{"d":346,"n":6,"t":5},"speaker":"B","text":"はい。恐らくお金がとても関わることになるのでしょう。MCも人気のあるお笑いコンビのかまいたちが担当していますので、視聴者が増え、良くない影響が出ることを危惧したと考えます。"},{"id":{"d":346,"n":7,"t":5},"speaker":"A","text":"インターネット番組でも、人気芸能人がMCをする番組を視聴する人は多くいそうですね。"},{"id":{"d":346,"n":8,"t":5},"speaker":"B","text":"結局インターネット番組でも、地上波でもあまり変わらないものになるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":347,"n":1,"t":5},"speaker":"A","text":"相澤さん、久しぶりにクラス会でもしませんか？"},{"id":{"d":347,"n":2,"t":5},"speaker":"B","text":"したいですね。5年前に一度、クラス会を行ってから、その後は一度も開催していませんし。"},{"id":{"d":347,"n":3,"t":5},"speaker":"A","text":"もう全員社会人になったので、少し良い店でも良いかもしれませんね。"},{"id":{"d":347,"n":4,"t":5},"speaker":"B","text":"私たちのクラスが、おしゃれな店でおとなしく食事をできるとは思いませんが、どうですか？"},{"id":{"d":347,"n":5,"t":5},"speaker":"A","text":"確かに、高校の時の友達と会うと、高校の時の感覚に戻ってしまうので、騒がしくなるかもしれませんね。"},{"id":{"d":347,"n":6,"t":5},"speaker":"B","text":"そのようなことを踏まえて、やはり個室のある大衆居酒屋がいいのではないかと思います。前回の店は安かったので、そこがいいかなと思うのですがいかがでしょう？"},{"id":{"d":347,"n":7,"t":5},"speaker":"A","text":"はい。料理もおいしかったので、私はあの店で十分だと思います。前回予約を間違えていたので、そこだけ気を付けてくださいね。"},{"id":{"d":347,"n":8,"t":5},"speaker":"B","text":"分かりました。前回の反省をしっかり生かして間違えないようにコースの予約をしますね。"}];
INSERT INTO sentence [{"id":{"d":348,"n":1,"t":5},"speaker":"A","text":"次の土曜日は、何の記念日か覚えていますか？"},{"id":{"d":348,"n":2,"t":5},"speaker":"B","text":"もちろん、覚えていますよ。お父さんとお母さんの結婚記念日ですよね。"},{"id":{"d":348,"n":3,"t":5},"speaker":"A","text":"そうです。2人で一緒に、お祝いをしませんか？"},{"id":{"d":348,"n":4,"t":5},"speaker":"B","text":"いいですね。お小遣いを出し合って、プレゼントを買いましょう。"},{"id":{"d":348,"n":5,"t":5},"speaker":"A","text":"私もプレゼントを用意したいと考えていました。おそろいのお箸を贈りたいと思うのですが、どうですか？"},{"id":{"d":348,"n":6,"t":5},"speaker":"B","text":"そういえば、お箸が少し古くなっていますよね。では、プレゼントはお箸にしましょう。他には何をしましょう？"},{"id":{"d":348,"n":7,"t":5},"speaker":"A","text":"夕食を2人で作って、みんなで一緒に食べましょうか？"},{"id":{"d":348,"n":8,"t":5},"speaker":"B","text":"そうしましょう。お父さんとお母さんの好きな料理をたくさん作って、喜んでもらいたいですね。"}];
INSERT INTO sentence [{"id":{"d":349,"n":1,"t":5},"speaker":"A","text":"お忙しいところすみません。インタビューにご対応いただけますか？"},{"id":{"d":349,"n":2,"t":5},"speaker":"B","text":"大丈夫ですよ。何でも聞いてください。"},{"id":{"d":349,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。今冬の降雪についてインタビューさせていただいています。今年の雪で何か影響は受けましたか？"},{"id":{"d":349,"n":4,"t":5},"speaker":"B","text":"もちろん受けました。電車が止まってしまい、登校することができませんでした。他の学生も登校できず、期末テストが延期になりました。"},{"id":{"d":349,"n":5,"t":5},"speaker":"A","text":"それは大変でしたね。その日の講義は全くなかったのですか？"},{"id":{"d":349,"n":6,"t":5},"speaker":"B","text":"全くありませんでした。しかし、家の雪かきをしないといけなかったため、私にとっては完全な休日ではありませんでした。"},{"id":{"d":349,"n":7,"t":5},"speaker":"A","text":"家には1人だったのですか？"},{"id":{"d":349,"n":8,"t":5},"speaker":"B","text":"親もいましたが、両親ともにリモートワークのため、私が1人で雪かきをしていました。おかげで筋肉痛です。"}];
INSERT INTO sentence [{"id":{"d":350,"n":1,"t":5},"speaker":"A","text":"クリスマスは、どこに行きますか？"},{"id":{"d":350,"n":2,"t":5},"speaker":"B","text":"今年のクリスマスは、土曜日なのでどこかで外食をしましょう。何か、食べたいものはありますか？"},{"id":{"d":350,"n":3,"t":5},"speaker":"A","text":"普段食べないようなものを食べたいです。少し高くても良いのでコース料理はどうですか？"},{"id":{"d":350,"n":4,"t":5},"speaker":"B","text":"とても良いですね。少しお店を探してみます。仕事次第ですが、クリスマスイブも、どこかホテルに泊まりませんか？"},{"id":{"d":350,"n":5,"t":5},"speaker":"A","text":"はい。金曜日、土曜日とホテルに宿泊するのはどうですか？"},{"id":{"d":350,"n":6,"t":5},"speaker":"B","text":"そのホテルのレストランでコース料理を食べましょう。"},{"id":{"d":350,"n":7,"t":5},"speaker":"A","text":"私が気になっているホテルがあるので、予約は私に任せてもらってもよいですか？"},{"id":{"d":350,"n":8,"t":5},"speaker":"B","text":"はい。よろしくお願いします。とても楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":351,"n":1,"t":5},"speaker":"A","text":"最近は、芸能人が減っているような気がします。"},{"id":{"d":351,"n":2,"t":5},"speaker":"B","text":"芸能人の数は増えていると思いますが、どういう意味ですか？"},{"id":{"d":351,"n":3,"t":5},"speaker":"A","text":"私の印象では芸能人は芸能を極めた人です。昔は、隠し芸などさまざまな芸を見せる人が多かったですが、最近は、昔ほどそのような人はいないような気がします。"},{"id":{"d":351,"n":4,"t":5},"speaker":"B","text":"今でもピアノがとても上手な芸能人や、アーチェリーが上手な芸能人はいます。"},{"id":{"d":351,"n":5,"t":5},"speaker":"A","text":"もちろんそのような方々は素晴らしいと思っていますが、そのような芸に関しては極めているプロがいます。1つだけ極めるのではなく、多くの芸を持っている人こそ芸能人だと思います。"},{"id":{"d":351,"n":6,"t":5},"speaker":"B","text":"例えば、どのような人がいますか？"},{"id":{"d":351,"n":7,"t":5},"speaker":"A","text":"私は、バナナマンの日村勇紀さんは、素晴らしい人だと思います。タップダンスを踊ることができますし、アイドルと一緒に踊ることもできます。また、ピアノも独学で弾くことができるそうです。"},{"id":{"d":351,"n":8,"t":5},"speaker":"B","text":"それは素晴らしいですね。確かにそのような人こそ、芸能人なのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":352,"n":1,"t":5},"speaker":"A","text":"きれいな夕焼けが見えた日の次の日は晴れる、という言い伝えがありますね。"},{"id":{"d":352,"n":2,"t":5},"speaker":"B","text":"小さい頃から親から聞いていたため間違いないと思っていますが、なぜそのような言い伝えがあるのか分かりません。"},{"id":{"d":352,"n":3,"t":5},"speaker":"A","text":"私も理由は分かりませんが、小さい頃から雨の香りが分かります。"},{"id":{"d":352,"n":4,"t":5},"speaker":"B","text":"雨の香りが分かることは、素晴らしいことなのではないでしょうか？"},{"id":{"d":352,"n":5,"t":5},"speaker":"A","text":"はい。雨の日は、関節が痛むという人がいるのと同じことだと思います。"},{"id":{"d":352,"n":6,"t":5},"speaker":"B","text":"他に何か天気を知ることができる方法はありますか？"},{"id":{"d":352,"n":7,"t":5},"speaker":"A","text":"あります。私は鎖骨を骨折し手術をしているので、湿気が多い日には鎖骨が痛くなります。雨が降りそうな日の痛みはだいぶ分かってきたような気がします。"},{"id":{"d":352,"n":8,"t":5},"speaker":"B","text":"雨を判断できることはいいですが、鎖骨を折って手術をするのは嫌ですね。"}];
INSERT INTO sentence [{"id":{"d":353,"n":1,"t":5},"speaker":"A","text":"YOASOBIが新曲を作っているとテレビ番組で発表していました。"},{"id":{"d":353,"n":2,"t":5},"speaker":"B","text":"新曲を作っているということを、テレビで放送していたのですか？とても人気があることが伺えますね。"},{"id":{"d":353,"n":3,"t":5},"speaker":"A","text":"もちろん人気があるため、そのような発表をテレビ番組でしていたとは思いますが、ただの発表ではなく驚きの内容も含まれていました。"},{"id":{"d":353,"n":4,"t":5},"speaker":"B","text":"驚きの内容とは何ですか？相当珍しいことだとは、予想がつきますが教えてください。"},{"id":{"d":353,"n":5,"t":5},"speaker":"A","text":"まず、YOASOBIが小説を元にして音楽を作成していることはご存知ですか？"},{"id":{"d":353,"n":6,"t":5},"speaker":"B","text":"はい、もちろん知っています。小説を3分の音楽にするとは素晴らしい才能だと思っています。"},{"id":{"d":353,"n":7,"t":5},"speaker":"A","text":"その小説について、次は直木賞を受賞した方々が書いた小説を用いるという発表をしていました。また、1曲だけではなく4曲も作成しているそうです。"},{"id":{"d":353,"n":8,"t":5},"speaker":"B","text":"素晴らしい小説家の書いた小説をYOASOBIがどのような音楽にするのか楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":354,"n":1,"t":5},"speaker":"A","text":"あなたは、何か楽器の演奏はできますか？"},{"id":{"d":354,"n":2,"t":5},"speaker":"B","text":"全く経験がないため演奏はできません。小学校や中学校で演奏していたはずのリコーダーも、もうできないと思います。"},{"id":{"d":354,"n":3,"t":5},"speaker":"A","text":"リコーダーに最後に触れたのは、10年近く前のことですので仕方ありませんね。"},{"id":{"d":354,"n":4,"t":5},"speaker":"B","text":"もう10年も前ですか。それはできないはずです。何か得意な楽器はありますか？"},{"id":{"d":354,"n":5,"t":5},"speaker":"A","text":"はい。私はピアノを習っていたのでピアノを弾くことができます。"},{"id":{"d":354,"n":6,"t":5},"speaker":"B","text":"ピアノを習っていたのですね。私が小学生の時には、多くの女の子がピアノを習っていた記憶があります。"},{"id":{"d":354,"n":7,"t":5},"speaker":"A","text":"その例に漏れず、私も小学5年生まではピアノを習っていました。でも、それほど楽しくなかったことを、覚えています。"},{"id":{"d":354,"n":8,"t":5},"speaker":"B","text":"楽しくない中で、5年生までよく頑張りましたね。"}];
INSERT INTO sentence [{"id":{"d":355,"n":1,"t":5},"speaker":"A","text":"バレンタインデーのチョコレートはもらえましたか？"},{"id":{"d":355,"n":2,"t":5},"speaker":"B","text":"1個もらいました。あなたは、何個もらいましたか？"},{"id":{"d":355,"n":3,"t":5},"speaker":"A","text":"私は4個もらいました。私の方が多いですね。"},{"id":{"d":355,"n":4,"t":5},"speaker":"B","text":"部活のメンバー内で、もっともらったチョコレートの数が少ない人はいるかもしれません。私が一番下とは限りませんよ。"},{"id":{"d":355,"n":5,"t":5},"speaker":"A","text":"ちなみにその1つはお母さんからもらった物ですか？"},{"id":{"d":355,"n":6,"t":5},"speaker":"B","text":"それは言いません。別に、言わなくても良いはずです。井原君こそ、その4個は誰からもらったのですか？"},{"id":{"d":355,"n":7,"t":5},"speaker":"A","text":"中村君が言わないなら、私も言いません。"}];
INSERT INTO sentence [{"id":{"d":356,"n":1,"t":5},"speaker":"A","text":"今日は成人式ですね。おめでとうございます。"},{"id":{"d":356,"n":2,"t":5},"speaker":"B","text":"20年間育てていただいて、ありがとうございました。これからもよろしくお願いします。"},{"id":{"d":356,"n":3,"t":5},"speaker":"A","text":"こちらこそしっかりと育ってくれて、ありがとうございます。今日は、会場までどのように行くのですか？"},{"id":{"d":356,"n":4,"t":5},"speaker":"B","text":"車で行きたいと思っています。会場が少し遠いので、車で送っていただけますか？"},{"id":{"d":356,"n":5,"t":5},"speaker":"A","text":"可能ですが、今日は私もお父さんも風邪をひいているので外に出ることができません。"},{"id":{"d":356,"n":6,"t":5},"speaker":"B","text":"つまり、車の中で待機しているということですか？"},{"id":{"d":356,"n":7,"t":5},"speaker":"A","text":"その通りです。風邪をうつすと嫌なので、他の友達も送っていくことはできません。"},{"id":{"d":356,"n":8,"t":5},"speaker":"B","text":"分かりました。友達に連絡しておきます。"},{"id":{"d":356,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。それではあと1時間半後に出発しますね。"}];
INSERT INTO sentence [{"id":{"d":357,"n":1,"t":5},"speaker":"A","text":"久しぶりに、小さい頃に買ってもらったおもちゃに電池を入れてみました。"},{"id":{"d":357,"n":2,"t":5},"speaker":"B","text":"あの映画で主役として登場していたおもちゃですね。"},{"id":{"d":357,"n":3,"t":5},"speaker":"A","text":"小さい頃から大好きだったので、しっかり動いてくれてうれしかったです。"},{"id":{"d":357,"n":4,"t":5},"speaker":"B","text":"あなたがうれしそうにずっと抱いていたことを、私は覚えています。プレゼントしたかいがあったと思ったことを今も覚えています。ものを大切にすることを学んでくれて良かったです。"},{"id":{"d":357,"n":5,"t":5},"speaker":"A","text":"小さい頃だったのであまり覚えていませんが、絶対に大切にすると思っていたことを覚えています。"},{"id":{"d":357,"n":6,"t":5},"speaker":"B","text":"これからも、その気持ちを持って、今後も生活してほしいと思います。"},{"id":{"d":357,"n":7,"t":5},"speaker":"A","text":"私に子どもができても、その気持ちを教えたいと思います。"}];
INSERT INTO sentence [{"id":{"d":358,"n":1,"t":5},"speaker":"A","text":"昭和の泣ける曲を、テレビで放送していました。"},{"id":{"d":358,"n":2,"t":5},"speaker":"B","text":"昭和の曲は良いものが多いですね。どのような曲が紹介されていましたか？"},{"id":{"d":358,"n":3,"t":5},"speaker":"A","text":"荒井由実さんの『ひこうき雲』など、とても有名なものがたくさん紹介されていました。"},{"id":{"d":358,"n":4,"t":5},"speaker":"B","text":"現在も、よく聴かれている歌が多いですね。"},{"id":{"d":358,"n":5,"t":5},"speaker":"A","text":"やはり昭和の歌手の方々は、歌唱力が素晴らしいと思いました。"},{"id":{"d":358,"n":6,"t":5},"speaker":"B","text":"今も歌の上手な方は多いですが、私はどうしても昭和の歌手の方に魅力を感じてしまいます。例えば、玉置浩二さんの歌はとても好きです。"},{"id":{"d":358,"n":7,"t":5},"speaker":"A","text":"昔の歌も、良い歌はずっと歌い継がれていくと良いですね。"},{"id":{"d":358,"n":8,"t":5},"speaker":"B","text":"そうですね。昭和の歌や、歌詞をしっかりと伝えられる歌手がこれからも増えるといいですね。"}];
INSERT INTO sentence [{"id":{"d":359,"n":1,"t":5},"speaker":"A","text":"カラオケは、得意ですか？"},{"id":{"d":359,"n":2,"t":5},"speaker":"B","text":"好きですが、得意ではありません。"},{"id":{"d":359,"n":3,"t":5},"speaker":"A","text":"何か苦手な要素があるのですか？"},{"id":{"d":359,"n":4,"t":5},"speaker":"B","text":"マイクを持つと高音が出ません。少し力が入ってしまうようです。"},{"id":{"d":359,"n":5,"t":5},"speaker":"A","text":"マイクを持つと力んでしまいますね。私も力んでしまうので、その気持ちが分かります。"},{"id":{"d":359,"n":6,"t":5},"speaker":"B","text":"どのようにしたら、力まないで歌うことができるのでしょうか？"},{"id":{"d":359,"n":7,"t":5},"speaker":"A","text":"1人でカラオケに行くと力まないで歌うことができます。"},{"id":{"d":359,"n":8,"t":5},"speaker":"B","text":"それでは、1人で歌うことに慣れてから、みんなと歌うのが良いですね。"}];
INSERT INTO sentence [{"id":{"d":360,"n":1,"t":5},"speaker":"A","text":"あなたは、ジグソーパズルをしたことがありますか？"},{"id":{"d":360,"n":2,"t":5},"speaker":"B","text":"もちろん、あります。小さい頃は、よくしていました。"},{"id":{"d":360,"n":3,"t":5},"speaker":"A","text":"今はパソコンの無料ソフトを使い、自分の好きな画像でジグソーパズルを作ることができます。"},{"id":{"d":360,"n":4,"t":5},"speaker":"B","text":"とても楽しそうですね。昔の思い出の写真を、ジグソーパズルにして遊ぶことができるということですね。"},{"id":{"d":360,"n":5,"t":5},"speaker":"A","text":"その通りです。そのような思い出のある写真から作ったジグソーパズルで遊ぶことももちろん面白いですが、もっと難しい遊び方もあります。"},{"id":{"d":360,"n":6,"t":5},"speaker":"B","text":"どのような遊び方ですか？"},{"id":{"d":360,"n":7,"t":5},"speaker":"A","text":"例えば、真っ白な画像をジグソーパズルにするのです。形を比べてパズルを作り上げます。"},{"id":{"d":360,"n":8,"t":5},"speaker":"B","text":"とても難しそうですが、とても楽しそうです。"}];
INSERT INTO sentence [{"id":{"d":361,"n":1,"t":5},"speaker":"A","text":"ダンスを習いに行きませんか？"},{"id":{"d":361,"n":2,"t":5},"speaker":"B","text":"それは突然のお誘いですね。なぜ、今からダンスなのですか？"},{"id":{"d":361,"n":3,"t":5},"speaker":"A","text":"ダンスを踊ると、楽しく有酸素運動ができるのではないかと思っています。一緒に、習いに行きませんか？"},{"id":{"d":361,"n":4,"t":5},"speaker":"B","text":"確かに、楽しそうですね。ただ、私はリズム感がありませんが大丈夫でしょうか？"},{"id":{"d":361,"n":5,"t":5},"speaker":"A","text":"大丈夫だと思います。リズム感がなくても先生と同じように体を動かしていたら、いつの間にかリズムを取れるようになっています。"},{"id":{"d":361,"n":6,"t":5},"speaker":"B","text":"そうだといいのですが、もし、できないままだったらと考えると腰が重いです。"},{"id":{"d":361,"n":7,"t":5},"speaker":"A","text":"私は、大学に入る前までダンスをしたことはなかったのですが、ダンスのサークルに入り、リズム感が良くなりました。だから、きっと大丈夫だと思います。"},{"id":{"d":361,"n":8,"t":5},"speaker":"B","text":"それは、心強いです。安心して、一緒に行こうと思えました。"}];
INSERT INTO sentence [{"id":{"d":362,"n":1,"t":5},"speaker":"A","text":"今放送中の大河ドラマを知っていますか？"},{"id":{"d":362,"n":2,"t":5},"speaker":"B","text":"全く確認していないので、分かりません。今は何を放送しているのですか？"},{"id":{"d":362,"n":3,"t":5},"speaker":"A","text":"今は『鎌倉殿の13人』という大河ドラマが放送されています。"},{"id":{"d":362,"n":4,"t":5},"speaker":"B","text":"鎌倉というと、源頼朝や北条家ですか？"},{"id":{"d":362,"n":5,"t":5},"speaker":"A","text":"その通りです。とても見応えのある面白いドラマです。"},{"id":{"d":362,"n":6,"t":5},"speaker":"B","text":"誰が主役をしているのですか？"},{"id":{"d":362,"n":7,"t":5},"speaker":"A","text":"主役は小栗旬さんです。源頼朝役は大泉洋さんが演じています。また、脚本は三谷幸喜さんです。"},{"id":{"d":362,"n":8,"t":5},"speaker":"B","text":"三谷幸喜脚本の作品は面白いものばかりですが、少し大河とは離れてしまうような気がします。"},{"id":{"d":362,"n":9,"t":5},"speaker":"A","text":"それが、時折面白さも交えながら、真剣な部分も描かれている素晴らしい作品です。"}];
INSERT INTO sentence [{"id":{"d":363,"n":1,"t":5},"speaker":"A","text":"小型自動販売機のカプセルトイ、いわゆるガチャガチャはお好きですか？"},{"id":{"d":363,"n":2,"t":5},"speaker":"B","text":"もちろん好きです。好きなキャラクターの商品が入っているガチャガチャを見つけたら、すぐにお金を入れてしまいます。"},{"id":{"d":363,"n":3,"t":5},"speaker":"A","text":"どのようなキャラクター商品の、ガチャガチャをしますか？"},{"id":{"d":363,"n":4,"t":5},"speaker":"B","text":"私は、自分の好きなアニメーションのキャラクターのガチャガチャをすることが多いです。"},{"id":{"d":363,"n":5,"t":5},"speaker":"A","text":"それらをコンプリートするまで、回すのですか？"},{"id":{"d":363,"n":6,"t":5},"speaker":"B","text":"基本的に、1回だけ回しています。特に、お気に入りのキャラクターのガチャガチャは、何回か回してしまいます。あなたは、ガチャガチャをしますか？"},{"id":{"d":363,"n":7,"t":5},"speaker":"A","text":"私も、結構してしまいます。何が出るか分からない緊張感が楽しくて、やめられません。"},{"id":{"d":363,"n":8,"t":5},"speaker":"B","text":"コンプリートするまで、するのですか？"},{"id":{"d":363,"n":9,"t":5},"speaker":"A","text":"私はコンプリートするまで、してしまいます。たくさんお金を使って、後々もったいないと思ってしまいます。"}];
INSERT INTO sentence [{"id":{"d":364,"n":1,"t":5},"speaker":"A","text":"今度、久しぶりに外食をしようと思うのですが、どこの店がお薦めですか？"},{"id":{"d":364,"n":2,"t":5},"speaker":"B","text":"お薦めのお店をお知らせするので、どの辺りに、誰と行くのか教えてください。"},{"id":{"d":364,"n":3,"t":5},"speaker":"A","text":"品川に1人で行きます。"},{"id":{"d":364,"n":4,"t":5},"speaker":"B","text":"時間次第ですが、ランチを少し過ぎた時間であればここのカフェがお薦めです。"},{"id":{"d":364,"n":5,"t":5},"speaker":"A","text":"駅の近くのところですね。あなたは、このカフェに行ったことがあるのですか？"},{"id":{"d":364,"n":6,"t":5},"speaker":"B","text":"私は、このカフェに行ったことがあります。また、食レポサイトでも高い評価を得ています。"},{"id":{"d":364,"n":7,"t":5},"speaker":"A","text":"純喫茶のような雰囲気ですか？"},{"id":{"d":364,"n":8,"t":5},"speaker":"B","text":"その通りです。おしゃれなジャズが流れていて気持ちが落ち着きますよ。"}];
INSERT INTO sentence [{"id":{"d":365,"n":1,"t":5},"speaker":"A","text":"北海道では、また大荒れの天気のようですね。現地の高橋さんに聞いてみましょう。"},{"id":{"d":365,"n":2,"t":5},"speaker":"B","text":"こちら、北海道札幌市大通公園から中継でお送りしています。"},{"id":{"d":365,"n":3,"t":5},"speaker":"A","text":"そちらは雪が大変なようですが、積雪量はどのようになっていますか？"},{"id":{"d":365,"n":4,"t":5},"speaker":"B","text":"大体15センチメートルほど積もっています。歩くときに、足をしっかり上げないといけません。"},{"id":{"d":365,"n":5,"t":5},"speaker":"A","text":"風はどうでしょうか？強い風が吹いていますか？"},{"id":{"d":365,"n":6,"t":5},"speaker":"B","text":"はい。時折強い横風が吹き、表面の雪が舞ってホワイトアウトのようになります。車はライトをつけなければ危ないと思います。"},{"id":{"d":365,"n":7,"t":5},"speaker":"A","text":"これからはどのような予報でしょうか？"},{"id":{"d":365,"n":8,"t":5},"speaker":"B","text":"この雪は今日1日続くようです。ただ、1週間ほど経過したら雪は止むとされています。"},{"id":{"d":365,"n":9,"t":5},"speaker":"A","text":"ありがとうございました。札幌市大通公園から高橋さんでした。"}];
INSERT INTO sentence [{"id":{"d":366,"n":1,"t":5},"speaker":"A","text":"明日は寒くなるようです。"},{"id":{"d":366,"n":2,"t":5},"speaker":"B","text":"ここ最近毎日寒いと言われているので慣れてしまいました。"},{"id":{"d":366,"n":3,"t":5},"speaker":"A","text":"そうですね。例年に比べてとても寒い気がします。"},{"id":{"d":366,"n":4,"t":5},"speaker":"B","text":"ただ今週末からは暖かくなるそうです。"},{"id":{"d":366,"n":5,"t":5},"speaker":"A","text":"やっと春が近づいてくるということですね。桜の季節ですね。"},{"id":{"d":366,"n":6,"t":5},"speaker":"B","text":"6年ぶりにここで見る桜はしっかりと春を感じながら見たいので、本当に暖かくなってほしいです。"},{"id":{"d":366,"n":7,"t":5},"speaker":"A","text":"天気予報を信じるしかありませんね。願っておきましょう。"}];
INSERT INTO sentence [{"id":{"d":367,"n":1,"t":5},"speaker":"A","text":"映画は、何がお好きですか？"},{"id":{"d":367,"n":2,"t":5},"speaker":"B","text":"私は、邦画が好きです。あなたは、何がお好きですか？"},{"id":{"d":367,"n":3,"t":5},"speaker":"A","text":"私は、洋画が好きです。特に『ワイルド・スピード』シリーズが大好きです。"},{"id":{"d":367,"n":4,"t":5},"speaker":"B","text":"車の映画ですね。シリーズがたくさんあって最初から見るのは大変そうなので、あまり見ようと思えません。"},{"id":{"d":367,"n":5,"t":5},"speaker":"A","text":"現在は、シリーズが10作品以上存在していますね。今から全部見るのは大変ですね。"},{"id":{"d":367,"n":6,"t":5},"speaker":"B","text":"シリーズのうち、特にお薦めの作品はありますか？"},{"id":{"d":367,"n":7,"t":5},"speaker":"A","text":"私は、最初に見たということもあり、『ワイルド・スピード MEGA MAX』が一番好きですが、東京を舞台にしている『ワイルド・スピードX3 TOKYO DRIFT』も面白いです。"},{"id":{"d":367,"n":8,"t":5},"speaker":"B","text":"それでは、まずその2作品を見てみようと思います。面白かった場合は、他の作品も見ようと思います。"},{"id":{"d":367,"n":9,"t":5},"speaker":"A","text":"ぜひ、気に入っていただけたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":368,"n":1,"t":5},"speaker":"A","text":"家にDVDはありますか？"},{"id":{"d":368,"n":2,"t":5},"speaker":"B","text":"もちろんあります。なぜですか？"},{"id":{"d":368,"n":3,"t":5},"speaker":"A","text":"今は、DVDが要らない時代かもしれないと思ったので、聞いてみました。"},{"id":{"d":368,"n":4,"t":5},"speaker":"B","text":"それでは、あなたの家にはDVDがないのですか？"},{"id":{"d":368,"n":5,"t":5},"speaker":"A","text":"いいえ。私の家には、DVDがたくさんあります。しかし、今となってはインターネットで動画を見ることができる時代です。"},{"id":{"d":368,"n":6,"t":5},"speaker":"B","text":"確かにそうですね。映画も、動画配信サイトに登録し、月額料を払うことで視聴することが可能ですね。"},{"id":{"d":368,"n":7,"t":5},"speaker":"A","text":"そのような時代の流れもあり、DVDはもうなくなっていくのかもしれないと思っています。"},{"id":{"d":368,"n":8,"t":5},"speaker":"B","text":"少し古い動画は、DVDで残るのではありませんか？"},{"id":{"d":368,"n":9,"t":5},"speaker":"A","text":"はい、残るかもしれませんね。他にも視聴者が限定的であることが予想される動画なども、DVDのままかもしれません。"}];
INSERT INTO sentence [{"id":{"d":369,"n":1,"t":5},"speaker":"A","text":"久しぶりに、料理漫画を読みました。とても面白かったです。"},{"id":{"d":369,"n":2,"t":5},"speaker":"B","text":"漫画は久しぶりに読むと面白いですね。どこまで読むことができたのですか？"},{"id":{"d":369,"n":3,"t":5},"speaker":"A","text":"約半分まで読むことができました。"},{"id":{"d":369,"n":4,"t":5},"speaker":"B","text":"だいぶ読みましたね。どこか懐かしいシーンはありましたか？"},{"id":{"d":369,"n":5,"t":5},"speaker":"A","text":"はい。やはり料理の漫画なので、当時興味があった料理は懐かしく感じました。"},{"id":{"d":369,"n":6,"t":5},"speaker":"B","text":"最初に読んでいた時とは異なり、今では自分でも料理をすることができるはずですので、ぜひ作ってみてはいかがですか？"},{"id":{"d":369,"n":7,"t":5},"speaker":"A","text":"いいえ。私は、材料の調達が難しい料理に憧れていたので、作ることは難しいかもしれません。"},{"id":{"d":369,"n":8,"t":5},"speaker":"B","text":"それでは簡単に調達することができそうな材料で、作ることができる料理を探して作ってください。"}];
INSERT INTO sentence [{"id":{"d":370,"n":1,"t":5},"speaker":"A","text":"「昔のテレビ番組は面白かった」とよく言われますが、あなたは、昔のテレビを見たことがありますか？"},{"id":{"d":370,"n":2,"t":5},"speaker":"B","text":"ほとんどありません。昔のテレビ番組とは、どのような時期の番組を指しているのか、正直分かっていません。"},{"id":{"d":370,"n":3,"t":5},"speaker":"A","text":"私は、昔が全て良いとは全く思いませんが、面白い番組があったことは確かです。"},{"id":{"d":370,"n":4,"t":5},"speaker":"B","text":"例えば、どのような番組が面白かったですか？"},{"id":{"d":370,"n":5,"t":5},"speaker":"A","text":"私が面白いと感じていて、好きなのは「ダウンタウンのごっつええ感じ」です。"},{"id":{"d":370,"n":6,"t":5},"speaker":"B","text":"初めて聞きました。どのような番組だったのですか？"},{"id":{"d":370,"n":7,"t":5},"speaker":"A","text":"番組名に入っているので、ダウンタウンは、もちろんレギュラーで、他にも当時は若手でしたが、現在は大人気のある芸能人が多数出演し、コントをするという番組です。"},{"id":{"d":370,"n":8,"t":5},"speaker":"B","text":"大人気の芸能人とは、どのような方々が出演していたのですか？"},{"id":{"d":370,"n":9,"t":5},"speaker":"A","text":"今田耕司さんや東野幸治さん、130Rの2人、YOUさん、篠原涼子さん他が出演していました。"},{"id":{"d":370,"n":10,"t":5},"speaker":"B","text":"とても豪華ですね。"}];
INSERT INTO sentence [{"id":{"d":371,"n":1,"t":5},"speaker":"A","text":"『夏祭り』という歌を知っていますか？"},{"id":{"d":371,"n":2,"t":5},"speaker":"B","text":"音楽ゲームの「太鼓の達人」で人気になった曲ですか？"},{"id":{"d":371,"n":3,"t":5},"speaker":"A","text":"その通りです。私は小さい頃は「太鼓の達人」専用の曲だと思っていました。"},{"id":{"d":371,"n":4,"t":5},"speaker":"B","text":"確かに、他の場面で聴くことはなかったですね。『夏祭り』がどうしたのですか？"},{"id":{"d":371,"n":5,"t":5},"speaker":"A","text":"あの歌は、太鼓の達人の影響もあり、Whiteberryの曲だと思われがちですが、本当は違うことをご存知ですか？"},{"id":{"d":371,"n":6,"t":5},"speaker":"B","text":"私はWhiteberryの曲だと思っていました。誰の曲ですか？"},{"id":{"d":371,"n":7,"t":5},"speaker":"A","text":"実は、JITTERIN’JINNの曲なのです。"},{"id":{"d":371,"n":8,"t":5},"speaker":"B","text":"そうだったのですね。あれだけの名曲なのに、彼らの歌だとあまり知られていないことがとても残念です。"},{"id":{"d":371,"n":9,"t":5},"speaker":"A","text":"他にもそのような歌がたくさんあるかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":372,"n":1,"t":5},"speaker":"A","text":"誕生日の予定は、何かありますか？"},{"id":{"d":372,"n":2,"t":5},"speaker":"B","text":"全くありません。家で普段通りの生活をしようかなと思っています。"},{"id":{"d":372,"n":3,"t":5},"speaker":"A","text":"どこかに、遊びに行くというのはどうですか？"},{"id":{"d":372,"n":4,"t":5},"speaker":"B","text":"本当はしたいのですが、1カ月後どのような状況になっているか分かりません。"},{"id":{"d":372,"n":5,"t":5},"speaker":"A","text":"その通りですね。遊びに行くとしたら、どこか行きたいところはありますか？"},{"id":{"d":372,"n":6,"t":5},"speaker":"B","text":"温泉に入りたいので、近場で箱根に行きたいです。"},{"id":{"d":372,"n":7,"t":5},"speaker":"A","text":"温泉はいいですね。コロナウイルス感染症の流行もあるので、複数人で行くのは難しいかもしれませんが、その時は1人で温泉に入りに行くのはどうですか？"},{"id":{"d":372,"n":8,"t":5},"speaker":"B","text":"いいかもしれません。1人で温泉に行って、1人で宿泊してくるのは良いかもしれません。"}];
INSERT INTO sentence [{"id":{"d":373,"n":1,"t":5},"speaker":"A","text":"梅雨の時期には、髪の毛がまとまりませんね。"},{"id":{"d":373,"n":2,"t":5},"speaker":"B","text":"そうですね。私は癖毛なので、髪の毛が全くまとまりません。"},{"id":{"d":373,"n":3,"t":5},"speaker":"A","text":"普段は、どのようにまとめていますか？"},{"id":{"d":373,"n":4,"t":5},"speaker":"B","text":"まとめずに、梅雨の時期は、できるだけ髪の毛を短くすることを心掛けています。"},{"id":{"d":373,"n":5,"t":5},"speaker":"A","text":"その上でワックスを用いて髪の毛を固めているということですね。"},{"id":{"d":373,"n":6,"t":5},"speaker":"B","text":"その方法しか、私は考えつきません。あなたは、何かしていますか？"},{"id":{"d":373,"n":7,"t":5},"speaker":"A","text":"シャンプーを変えています。CMのキャッチフレーズで「髪の毛がまとまるという」とうたわれているものを使うようにしています。"},{"id":{"d":373,"n":8,"t":5},"speaker":"B","text":"今年は、その方法を取ろうと思います。良い情報をありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":374,"n":1,"t":5},"speaker":"A","text":"最近の画像認証の精度を、ご存知ですか？"},{"id":{"d":374,"n":2,"t":5},"speaker":"B","text":"いいえ。あまり使ったことがありません。どの程度、精度が上がっているのですか？"},{"id":{"d":374,"n":3,"t":5},"speaker":"A","text":"現在は写真に撮ったものをPDFとして保存をすることもできます。"},{"id":{"d":374,"n":4,"t":5},"speaker":"B","text":"つまり手書きの書類を写真で撮影し、PDFにすることができるということですか？"},{"id":{"d":374,"n":5,"t":5},"speaker":"A","text":"その通りです。また、PDFとして扱うことができるため、文字の選択や検索をすることも可能です。"},{"id":{"d":374,"n":6,"t":5},"speaker":"B","text":"画像認証の素晴らしい成長ですね。パソコンで文字を打ち込むことが苦手な人も、手書きの書類を撮影することで、パソコン入力の遅れを取り戻せるということですね。"},{"id":{"d":374,"n":7,"t":5},"speaker":"A","text":"手書きの方が、時間はかかると思いますが、そのような方法もあります。"},{"id":{"d":374,"n":8,"t":5},"speaker":"B","text":"確かに、キーボードの打ち込みの方が早いですね。そちらを練習するべきでしょう。"}];
INSERT INTO sentence [{"id":{"d":375,"n":1,"t":5},"speaker":"A","text":"そろそろ受験の時期ですが、あなたは、勉強中にBGMをかけていますか？"},{"id":{"d":375,"n":2,"t":5},"speaker":"B","text":"私はクラシック音楽を聴いています。何を聴いていますか？"},{"id":{"d":375,"n":3,"t":5},"speaker":"A","text":"私は、何も聴いていません。なぜ、クラシック音楽を聴いているのですか？"},{"id":{"d":375,"n":4,"t":5},"speaker":"B","text":"歌詞が入っている歌だとつい歌ってしまい、集中が切れる気がするからです。"},{"id":{"d":375,"n":5,"t":5},"speaker":"A","text":"クラシック音楽でもメロディーを口ずさみませんか？"},{"id":{"d":375,"n":6,"t":5},"speaker":"B","text":"確かに、口ずさむことはありますが、ほとんど集中しているので音が全く聴こえません。"},{"id":{"d":375,"n":7,"t":5},"speaker":"A","text":"それでは、歌詞があっても良いのではありませんか？"},{"id":{"d":375,"n":8,"t":5},"speaker":"B","text":"歌詞があると、勉強に集中し始めるのが遅い気がしています。早くから集中したいので、クラシック音楽にしています。"},{"id":{"d":375,"n":9,"t":5},"speaker":"A","text":"勉強中のBGMにはしっかりとした理由があって、クラシック音楽なのですね。"}];
INSERT INTO sentence [{"id":{"d":376,"n":1,"t":5},"speaker":"A","text":"部屋を掃除していたら、CDが出てきました。"},{"id":{"d":376,"n":2,"t":5},"speaker":"B","text":"何年前に購入したCDですか？"},{"id":{"d":376,"n":3,"t":5},"speaker":"A","text":"私が中学3年生の時に購入したものがたくさん出てきたので、10年前ですね。"},{"id":{"d":376,"n":4,"t":5},"speaker":"B","text":"とても昔のものですね。懐かしいものがたくさんありましたか？"},{"id":{"d":376,"n":5,"t":5},"speaker":"A","text":"はい、懐かしいものばかりでした。その時に好きだったグループのCDがたくさん出てきたのですが、今は解散しているので悲しかったです。"},{"id":{"d":376,"n":6,"t":5},"speaker":"B","text":"10年もたっていたら、解散しているグループもありますね。実際に聴いてみましたか？"},{"id":{"d":376,"n":7,"t":5},"speaker":"A","text":"まだ、聴いていません。家に今はCDを再生する機械がありません。"},{"id":{"d":376,"n":8,"t":5},"speaker":"B","text":"CDを聴くことができないとは、時代を感じますね。"},{"id":{"d":376,"n":9,"t":5},"speaker":"A","text":"ええ。幼稚園生の時はMDをたくさん聴いていたはずなのに、今はCDすら聴きません。"}];
INSERT INTO sentence [{"id":{"d":377,"n":1,"t":5},"speaker":"A","text":"もう夏休みに入ってから1週間たち、そろそろ8月ですが宿題の進展はどうですか？"},{"id":{"d":377,"n":2,"t":5},"speaker":"B","text":"4分の3は終わりました。残りは自由研究のみです。"},{"id":{"d":377,"n":3,"t":5},"speaker":"A","text":"大物を残しましたね。終わりますか？"},{"id":{"d":377,"n":4,"t":5},"speaker":"B","text":"はい、まだあと1カ月あるので、宿題は終わると思います。北村君はどうですか？"},{"id":{"d":377,"n":5,"t":5},"speaker":"A","text":"私は自由研究以外、全くしていません。"},{"id":{"d":377,"n":6,"t":5},"speaker":"B","text":"自由研究は終わったのですか？"},{"id":{"d":377,"n":7,"t":5},"speaker":"A","text":"元々夏休みが始まる前からしていたので、既に終わっています。"},{"id":{"d":377,"n":8,"t":5},"speaker":"B","text":"それは、ずる賢いですね。あとはドリルなど簡単な問題集だけですね。"},{"id":{"d":377,"n":9,"t":5},"speaker":"A","text":"「ほとんど宿題は終わった」と言ってもいいかと思います。"},{"id":{"d":377,"n":10,"t":5},"speaker":"B","text":"もう後の夏休みは、遊べますね。うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":378,"n":1,"t":5},"speaker":"A","text":"近々、遊園地に遊びに行きませんか？"},{"id":{"d":378,"n":2,"t":5},"speaker":"B","text":"ぜひ行きたいのですが、人が多い中には、行きたくありません。"},{"id":{"d":378,"n":3,"t":5},"speaker":"A","text":"今なら入場制限をしているので、すいているとまではいきませんが、それでも相当すいているはずです。"},{"id":{"d":378,"n":4,"t":5},"speaker":"B","text":"混んでいたら行きたくないので、今のアトラクションの待ち時間を調べてみます。"},{"id":{"d":378,"n":5,"t":5},"speaker":"A","text":"今の13時という時間で混んでいなかったら、相当すいているのかなと思います。"},{"id":{"d":378,"n":6,"t":5},"speaker":"B","text":"待ち時間が0分のアトラクションがたくさんあります。とても、すいていますね。"},{"id":{"d":378,"n":7,"t":5},"speaker":"A","text":"ガラガラですね。今がチャンスかもしれません。"},{"id":{"d":378,"n":8,"t":5},"speaker":"B","text":"明日は平日なので、明日の方がすいている可能性もありますね。"},{"id":{"d":378,"n":9,"t":5},"speaker":"A","text":"明日は、仕事もないので遊園地へ行ってみましょう。"},{"id":{"d":378,"n":10,"t":5},"speaker":"B","text":"分かりました。しっかり準備します。"}];
INSERT INTO sentence [{"id":{"d":379,"n":1,"t":5},"speaker":"A","text":"今日は、とても暖かいですね。"},{"id":{"d":379,"n":2,"t":5},"speaker":"B","text":"先週までの寒さがうそのようです。体がついていくか不安です。"},{"id":{"d":379,"n":3,"t":5},"speaker":"A","text":"多分、大丈夫だと思います。風邪よりも大変なのは花粉症です。"},{"id":{"d":379,"n":4,"t":5},"speaker":"B","text":"いきなり暖かくなったので、花粉がたくさん飛ぶ可能性がありますね。"},{"id":{"d":379,"n":5,"t":5},"speaker":"A","text":"花粉症の私には大変な時期なので、この春を迎える少し前の時期が1年で一番嫌いです。"},{"id":{"d":379,"n":6,"t":5},"speaker":"B","text":"花粉症の人には、一番厳しい時期ですね。まだ花粉症の症状は出ていませんか？"},{"id":{"d":379,"n":7,"t":5},"speaker":"A","text":"今のところ大丈夫です。しかし、気を抜かないようにしないといけないので、あとはこの時期が早く過ぎてくれることを願うのみです。"},{"id":{"d":379,"n":8,"t":5},"speaker":"B","text":"1カ月ほどの戦いを、頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":380,"n":1,"t":5},"speaker":"A","text":"最近のスマートフォンは、映画のような動画を撮ることができます。"},{"id":{"d":380,"n":2,"t":5},"speaker":"B","text":"CMでもよく放送していましたが、本当ですか？あまり信じられません。"},{"id":{"d":380,"n":3,"t":5},"speaker":"A","text":"本当です。ゆっくりとピントが手前のものに移っていくので、本当に映画のような動画を撮ることができます。"},{"id":{"d":380,"n":4,"t":5},"speaker":"B","text":"それでは、簡単に映画を取ることができてしまいますね。"},{"id":{"d":380,"n":5,"t":5},"speaker":"A","text":"いつの日かスマートフォンで撮影した映画が、上映されるかもしれませんね。"},{"id":{"d":380,"n":6,"t":5},"speaker":"B","text":"ちなみに、あなたは今の段階では、スマートフォンで撮影した動画に、何が足りないと思いますか？"},{"id":{"d":380,"n":7,"t":5},"speaker":"A","text":"私は、音のきれいさが足りないと思います。"},{"id":{"d":380,"n":8,"t":5},"speaker":"B","text":"では、マイクを外付けし、実際に動画を撮影してみましょう。"}];
INSERT INTO sentence [{"id":{"d":381,"n":1,"t":5},"speaker":"A","text":"何か、お薦めの小説はありますか？"},{"id":{"d":381,"n":2,"t":5},"speaker":"B","text":"はい、『ハリー・ポッター』シリーズがお薦めです。私は『ハリー・ポッター』が大好きです。"},{"id":{"d":381,"n":3,"t":5},"speaker":"A","text":"『ハリー・ポッター』は私も読んだことがありますが、とても面白いですね。どの話が一番好きですか？"},{"id":{"d":381,"n":4,"t":5},"speaker":"B","text":"私は『ハリー・ポッターと不死鳥の騎士団』が一番好きです。"},{"id":{"d":381,"n":5,"t":5},"speaker":"A","text":"それはなぜですか？"},{"id":{"d":381,"n":6,"t":5},"speaker":"B","text":"私の好きなキャラクターは、シリウス・ブラックなのですが、この話では彼の格好良いシーンが多く描かれています。"},{"id":{"d":381,"n":7,"t":5},"speaker":"A","text":"そうなのですか。随分と前に読んで以来、読んでいないので、もう一度最初から読もうかと思います。"},{"id":{"d":381,"n":8,"t":5},"speaker":"B","text":"シリーズ全てを読むのは時間がかかりそうですが、ぜひこの話だけでも読み直してみてください。"},{"id":{"d":381,"n":9,"t":5},"speaker":"A","text":"そうですね。入社まで時間があるので、その間に読もうと思います。"}];
INSERT INTO sentence [{"id":{"d":382,"n":1,"t":5},"speaker":"A","text":"お笑いタレントは、普段は寡黙な人が多いそうです。"},{"id":{"d":382,"n":2,"t":5},"speaker":"B","text":"それではテレビ番組の時の明るい姿は、テレビ番組の中だけということですね。"},{"id":{"d":382,"n":3,"t":5},"speaker":"A","text":"そのようです。しかし、少し分かるような気がします。"},{"id":{"d":382,"n":4,"t":5},"speaker":"B","text":"お笑いタレントが、普段は寡黙であるということには、どのような理由があると思いますか？"},{"id":{"d":382,"n":5,"t":5},"speaker":"A","text":"テレビ番組で面白いことを言うために、さまざまなことにアンテナを張り巡らせているから、ということだと思います。"},{"id":{"d":382,"n":6,"t":5},"speaker":"B","text":"他のことに集中しているため、寡黙なように感じられるということですね。"},{"id":{"d":382,"n":7,"t":5},"speaker":"A","text":"そうだと思います。あの面白い話は普段の生活の一部分を切り取って、口に出しているだけなのかもしれません。"},{"id":{"d":382,"n":8,"t":5},"speaker":"B","text":"やはり何事も、プロは素晴らしいということですね。"}];
INSERT INTO sentence [{"id":{"d":383,"n":1,"t":5},"speaker":"A","text":"あなたは、ホラー映画を見ることができますか？"},{"id":{"d":383,"n":2,"t":5},"speaker":"B","text":"私は、ホラー映画を見ることができません。幼少期から怖い動画は全く駄目です。"},{"id":{"d":383,"n":3,"t":5},"speaker":"A","text":"小さい時は、何が一番怖かったですか？"},{"id":{"d":383,"n":4,"t":5},"speaker":"B","text":"私は小さい時には『ハリー・ポッター』シリーズの映画ですら怖かったです。"},{"id":{"d":383,"n":5,"t":5},"speaker":"A","text":"『ハリー・ポッター』の映画の怖い箇所があまり分かりませんが、小さい子には怖いのかもしれませんね。"},{"id":{"d":383,"n":6,"t":5},"speaker":"B","text":"妹と親と一緒に見ていましたが、妹は全く平気そうだったので、私が人一倍怖がりなのだと思います。"},{"id":{"d":383,"n":7,"t":5},"speaker":"A","text":"怖いものを見ると、あなたはどのようになるのですか？"},{"id":{"d":383,"n":8,"t":5},"speaker":"B","text":"私は、2階の寝室に上がることができなくなります。また、トイレに行くことも怖くなります。"},{"id":{"d":383,"n":9,"t":5},"speaker":"A","text":"日常生活に支障をきたすのならば、ホラー映画は見ない方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":384,"n":1,"t":5},"speaker":"A","text":"トランプの遊びは、何が一番好きですか？"},{"id":{"d":384,"n":2,"t":5},"speaker":"B","text":"私はスピードが一番好きです。反射神経を使うゲームですね。"},{"id":{"d":384,"n":3,"t":5},"speaker":"A","text":"私も若い時はスピードが好きでしたが、年齢を重ねて反射神経が悪くなりました。"},{"id":{"d":384,"n":4,"t":5},"speaker":"B","text":"それでは、今は何が一番好きですか？"},{"id":{"d":384,"n":5,"t":5},"speaker":"A","text":"私は、神経衰弱が好きです。神経衰弱であれば、半分くらいのカードは毎回取ることができます。"},{"id":{"d":384,"n":6,"t":5},"speaker":"B","text":"それでは、負けることはなさそうですね。今度勝負しましょう。"}];
INSERT INTO sentence [{"id":{"d":385,"n":1,"t":5},"speaker":"A","text":"あなたは、流れ星を見たことはありますか？"},{"id":{"d":385,"n":2,"t":5},"speaker":"B","text":"もちろんあります。私の家は、とても田舎にあるので街灯もなく、星を見やすい場所です。"},{"id":{"d":385,"n":3,"t":5},"speaker":"A","text":"良い場所ですね。私の家の近くは都会で街灯が多かったので、星を見るために、わざわざ車で星の見える場所まで移動していました。"},{"id":{"d":385,"n":4,"t":5},"speaker":"B","text":"星を見るために、わざわざ車で移動していたのですね。それは大変そうです。"},{"id":{"d":385,"n":5,"t":5},"speaker":"A","text":"本間さんの家の近辺から見られる星は、どれくらいはっきり見ることができたのですか？"},{"id":{"d":385,"n":6,"t":5},"speaker":"B","text":"星を見る場所として本に挙げられている場所と、同じくらい見えていたと記憶しています。"},{"id":{"d":385,"n":7,"t":5},"speaker":"A","text":"家の近辺から、それほど星が見えるのはうらやましいです。田舎でも、良い部分はたくさんありますね。"},{"id":{"d":385,"n":8,"t":5},"speaker":"B","text":"そうですね。都会の良いところも田舎の良いところも、あるということですね。"}];
INSERT INTO sentence [{"id":{"d":386,"n":1,"t":5},"speaker":"A","text":"最近のアニメーションを、見たことがありますか？"},{"id":{"d":386,"n":2,"t":5},"speaker":"B","text":"数年前は見たことがあったのですが、最近はあまり見ていません。"},{"id":{"d":386,"n":3,"t":5},"speaker":"A","text":"数年前のアニメーションは、アニメーションの絵がとてもひどいことがありましたね。"},{"id":{"d":386,"n":4,"t":5},"speaker":"B","text":"ちょうどその時期にアニメーションを見ていたのですぐに飽きてしまい、見なくなってしまいました。"},{"id":{"d":386,"n":5,"t":5},"speaker":"A","text":"どのようにひどい作画だったのですか？"},{"id":{"d":386,"n":6,"t":5},"speaker":"B","text":"顔の輪郭の中で、目や口などのパーツが移動していたのです。"},{"id":{"d":386,"n":7,"t":5},"speaker":"A","text":"それはひどいですね。見ていて違和感はありましたか？"},{"id":{"d":386,"n":8,"t":5},"speaker":"B","text":"もちろんありました。何も考えないで片手間に見ていても違和感があったので、しっかり見ていた人には本当に違和感が大きかったと思います。"},{"id":{"d":386,"n":9,"t":5},"speaker":"A","text":"アニメーションは、本当に好きな人が多いので、作画はしっかりして欲しいですね。"},{"id":{"d":386,"n":10,"t":5},"speaker":"B","text":"せめてキャラクターの顔は、しっかりして描いてほしいですね。"}];
INSERT INTO sentence [{"id":{"d":387,"n":1,"t":5},"speaker":"A","text":"父の日に贈るネクタイを、探しに行きませんか？"},{"id":{"d":387,"n":2,"t":5},"speaker":"B","text":"少しやらなくてはならないことが多くて、時間を取れない可能性があるので、あなたが選んでおいてください。"},{"id":{"d":387,"n":3,"t":5},"speaker":"A","text":"私は、洋服選びのセンスをもっていませんが、大丈夫ですか？"},{"id":{"d":387,"n":4,"t":5},"speaker":"B","text":"大丈夫だと思います。ネクタイは基本的にどのような柄でも格好良いですし、息子からもらったものなら親は何でもうれしいものです。"},{"id":{"d":387,"n":5,"t":5},"speaker":"A","text":"そうでしょうか。父の日も、母の日のように花を贈るという習慣だったのなら、センスのなさに悩むこともなくて良かったのですが。"},{"id":{"d":387,"n":6,"t":5},"speaker":"B","text":"確かに、決まった種類の花を贈るということならば、センスは関係ありませんね。実は、父の日には、黄色いバラを贈るという風習もあるようですよ。"},{"id":{"d":387,"n":7,"t":5},"speaker":"A","text":"そうなのですか。それは知りませんでした。では、今年は黄色いバラを贈るのはどうでしょうか？"},{"id":{"d":387,"n":8,"t":5},"speaker":"B","text":"毎年、父の日にはネクタイをプレゼントしていたので、今年もそれを楽しみにしているかもしれません。ネクタイとバラの両方を贈りましょう。"}];
INSERT INTO sentence [{"id":{"d":388,"n":1,"t":5},"speaker":"A","text":"サッカー選手は、どのようなボールでもリフティングができるのですか？"},{"id":{"d":388,"n":2,"t":5},"speaker":"B","text":"テニスボールで練習することはよくありました。ボールの中心を見極める練習ですね。"},{"id":{"d":388,"n":3,"t":5},"speaker":"A","text":"とても小さいボールなのに、リフティングができることは、素晴らしいと思います。"},{"id":{"d":388,"n":4,"t":5},"speaker":"B","text":"他のスポーツでもそのように、違う大きさのボールを使うことはありませんか？"},{"id":{"d":388,"n":5,"t":5},"speaker":"A","text":"元々小さいボールの競技は、大きなボールにすると道具が壊れてしまう気がします。"},{"id":{"d":388,"n":6,"t":5},"speaker":"B","text":"確かに、そうですね。テニスのラケットでサッカーボールやバスケットボールは打てませんね。"},{"id":{"d":388,"n":7,"t":5},"speaker":"A","text":"また、バスケットボールも、ボールの大きさが大切だと思います。"},{"id":{"d":388,"n":8,"t":5},"speaker":"B","text":"結局、いろいろな大きさのボールでできるのは、サッカーということですね。"},{"id":{"d":388,"n":9,"t":5},"speaker":"A","text":"だから、どこでもできるのでしょうね。"}];
INSERT INTO sentence [{"id":{"d":389,"n":1,"t":5},"speaker":"A","text":"突然ですが、私は歌を作りたいです。"},{"id":{"d":389,"n":2,"t":5},"speaker":"B","text":"なぜ、そのように考えるようになったのですか？"},{"id":{"d":389,"n":3,"t":5},"speaker":"A","text":"音楽が大好きで、私なら既存の歌よりももっと良い歌を作ることができると感じているからです。"},{"id":{"d":389,"n":4,"t":5},"speaker":"B","text":"だいぶ大胆に出ましたね。それではどのような歌を作るのですか？"},{"id":{"d":389,"n":5,"t":5},"speaker":"A","text":"最近私はサビの前で少し落ち着いて、サビでしっかり盛り上がる歌が一番好きなので、そのような歌を作りたいです。"},{"id":{"d":389,"n":6,"t":5},"speaker":"B","text":"そのような歌はたくさんありそうですが、盗作とは呼ばれませんか？"},{"id":{"d":389,"n":7,"t":5},"speaker":"A","text":"大丈夫だと思います。さまざまな音楽を聴いていますが、私の考えている音楽はまだ聴いたことがありません。"},{"id":{"d":389,"n":8,"t":5},"speaker":"B","text":"それでは実際に作ってみて、ぜひ聴かせてください。"}];
INSERT INTO sentence [{"id":{"d":390,"n":1,"t":5},"speaker":"A","text":"今日は、市内の祭りの日ですね。"},{"id":{"d":390,"n":2,"t":5},"speaker":"B","text":"市内の祭りがあるから、花火が昼間に上がっていたのですね。"},{"id":{"d":390,"n":3,"t":5},"speaker":"A","text":"そうです。ところで、祭りには行きますか？"},{"id":{"d":390,"n":4,"t":5},"speaker":"B","text":"迷っています。元々今日は予定がなかったので行くことはできますが、祭りは楽しいのでしょうか？"},{"id":{"d":390,"n":5,"t":5},"speaker":"A","text":"毎年開催されており、とても大規模なもののようですよ。"},{"id":{"d":390,"n":6,"t":5},"speaker":"B","text":"その口ぶりだと、あなたも行ったことはないのですか？"},{"id":{"d":390,"n":7,"t":5},"speaker":"A","text":"はい。行きたいと思っていたのですが、仕事や他の予定と重なってしまい、全く行くことができていませんでした。"},{"id":{"d":390,"n":8,"t":5},"speaker":"B","text":"それでは、私も初めてなので、初めて同士、今年は一緒に祭りへ行ってみませんか？"},{"id":{"d":390,"n":9,"t":5},"speaker":"A","text":"良いですね。早速、浴衣を準備しますね。"}];
INSERT INTO sentence [{"id":{"d":391,"n":1,"t":5},"speaker":"A","text":"毎週楽しみに見ていたテレビドラマが、昨日は、最終回でした。"},{"id":{"d":391,"n":2,"t":5},"speaker":"B","text":"昨日が最終回というと、ずっと高視聴率で話題になっていたドラマですよね？"},{"id":{"d":391,"n":3,"t":5},"speaker":"A","text":"そうです。あなたも見ていましたか？"},{"id":{"d":391,"n":4,"t":5},"speaker":"B","text":"いえ、私は見ていなかったのですが、会社で近くの席の人たちが、よくそのドラマの話をしていたので、自然と耳に入ってきました。"},{"id":{"d":391,"n":5,"t":5},"speaker":"A","text":"そうでしたか。私もドラマを見た翌日は、会社の友達と感想を言い合って盛り上がりました。"},{"id":{"d":391,"n":6,"t":5},"speaker":"B","text":"ドラマが終わってしまって、残念ですね。"},{"id":{"d":391,"n":7,"t":5},"speaker":"A","text":"本当に残念です。週に一度の楽しみがなくなってしまいました。ただ、視聴率が良かったので、続編が放送されるのではないかと期待しています。"},{"id":{"d":391,"n":8,"t":5},"speaker":"B","text":"それはあり得ますね。もしくは、映画化される可能性もありますね。"},{"id":{"d":391,"n":9,"t":5},"speaker":"A","text":"はい。とにかく、何かの形で続きが見たいです。"}];
INSERT INTO sentence [{"id":{"d":392,"n":1,"t":5},"speaker":"A","text":"最近は、さまざまな無料の動画編集ソフトが公開されていますね。"},{"id":{"d":392,"n":2,"t":5},"speaker":"B","text":"素人でも、上手に使いこなせば、プロのような動画編集が可能になっていますね。"},{"id":{"d":392,"n":3,"t":5},"speaker":"A","text":"私は元々そのような編集をすることが好きだったので、そのような無料ソフトの公開がとてもうれしいです。"},{"id":{"d":392,"n":4,"t":5},"speaker":"B","text":"それではさまざまな動画を編集しているということですか？"},{"id":{"d":392,"n":5,"t":5},"speaker":"A","text":"いいえ、あまりできていません。まず動画を保存しておく容量があまりありません。加えて本当に使いたい機能は有料だったりします。"},{"id":{"d":392,"n":6,"t":5},"speaker":"B","text":"無料で使える範囲を広げることで、有料への移行を簡単にしているということですね。"},{"id":{"d":392,"n":7,"t":5},"speaker":"A","text":"あくまでも予想ですが、私はそうだと思います。難しいですが無料の範囲で一番良いものを作ることにやりがいを感じています。"},{"id":{"d":392,"n":8,"t":5},"speaker":"B","text":"ぜひ今度、作成した動画を見せてください。気になります。"},{"id":{"d":392,"n":9,"t":5},"speaker":"A","text":"分かりました。それまでに見せられるくらいクオリティーを高めておきますね。"}];
INSERT INTO sentence [{"id":{"d":393,"n":1,"t":5},"speaker":"A","text":"私が小さい時に、近くのスーパーマーケットで塗り絵コンテストが開催されていました。"},{"id":{"d":393,"n":2,"t":5},"speaker":"B","text":"私の家の近くのスーパーマーケットでも、同様のイベントがありました。どこのスーパーマーケットでもあるのかもしれませんね。"},{"id":{"d":393,"n":3,"t":5},"speaker":"A","text":"その塗り絵コンテストに、作品を提出したことはありますか？"},{"id":{"d":393,"n":4,"t":5},"speaker":"B","text":"いいえ。塗り絵には名前を書く欄があったので、恥ずかしくてできませんでした。あなたは、どうですか？"},{"id":{"d":393,"n":5,"t":5},"speaker":"A","text":"私は、友達と一緒に塗り絵をして、コンテストにも参加していましたよ。"},{"id":{"d":393,"n":6,"t":5},"speaker":"B","text":"名前は書いていたのですか？"},{"id":{"d":393,"n":7,"t":5},"speaker":"A","text":"名前は書いていましたが、アニメーションのキャラクターの名前など偽名を使っていました。"},{"id":{"d":393,"n":8,"t":5},"speaker":"B","text":"なるほど、その手がありましたか。それなら恥ずかしくありませんね。"},{"id":{"d":393,"n":9,"t":5},"speaker":"A","text":"はい。友達と相談して思いついた方法です。コンテストでは何度か入賞することもでき、とても楽しかった思い出があります。"}];
INSERT INTO sentence [{"id":{"d":394,"n":1,"t":5},"speaker":"A","text":"この曲は、キーが高いので少し歌いにくいです。"},{"id":{"d":394,"n":2,"t":5},"speaker":"B","text":"それでは、キーを下げたら良いのではありませんか？"},{"id":{"d":394,"n":3,"t":5},"speaker":"A","text":"私は原曲が好きなので、キーを下げると、原曲を汚しているような気持ちになってしまいます。"},{"id":{"d":394,"n":4,"t":5},"speaker":"B","text":"それでは、この曲を歌うのを諦めるしかありませんね。"},{"id":{"d":394,"n":5,"t":5},"speaker":"A","text":"私の今の音域でトレーニングをしたら、歌えるようになると思いますか？"},{"id":{"d":394,"n":6,"t":5},"speaker":"B","text":"はい。声の質は良いと思うので、多分しっかりと正しいトレーニングを受ければ歌えるようになると思います。"},{"id":{"d":394,"n":7,"t":5},"speaker":"A","text":"それでは、トレーニングを受けようと思います。"},{"id":{"d":394,"n":8,"t":5},"speaker":"B","text":"ただ、今の声のままで、歌える歌を歌うこともいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":395,"n":1,"t":5},"speaker":"A","text":"あなたは、ミステリー小説を読みますか？"},{"id":{"d":395,"n":2,"t":5},"speaker":"B","text":"ミステリー小説ではありませんが、推理小説ならよく読みます。"},{"id":{"d":395,"n":3,"t":5},"speaker":"A","text":"推理小説の何を読みますか？"},{"id":{"d":395,"n":4,"t":5},"speaker":"B","text":"私は『アルセーヌ・ルパン』シリーズが大好きなので小さい頃からよく読んでいます。"},{"id":{"d":395,"n":5,"t":5},"speaker":"A","text":"それでは、『アルセーヌ・ルパン』シリーズの本を何冊も持っているということですか？"},{"id":{"d":395,"n":6,"t":5},"speaker":"B","text":"その通りです。私の部屋の本棚には、たくさん本がありますが、『アルセーヌ・ルパン』シリーズの小説がその中でも一番多くあります。"},{"id":{"d":395,"n":7,"t":5},"speaker":"A","text":"小さい頃に読んだとき、『アルセーヌ・ルパン』シリーズのストーリーは理解できていましたか？"},{"id":{"d":395,"n":8,"t":5},"speaker":"B","text":"とても難しかったですが、主人公のアルセーヌ・ルパンに憧れて、変装したくなった記憶があります。"},{"id":{"d":395,"n":9,"t":5},"speaker":"A","text":"変装したいとは、かわいらしい発想ですね。"}];
INSERT INTO sentence [{"id":{"d":396,"n":1,"t":5},"speaker":"A","text":"白黒の画像でも、カラーに見えることはありませんか？"},{"id":{"d":396,"n":2,"t":5},"speaker":"B","text":"よくあります。あなたもありますか？"},{"id":{"d":396,"n":3,"t":5},"speaker":"A","text":"はい、私もあります。不思議ですよね。日常の生活から得た経験で、色を勝手に付けているのでしょうか？"},{"id":{"d":396,"n":4,"t":5},"speaker":"B","text":"そうかもしれません。黒の濃淡だけで、色がはっきりと想像できてしまうなんて驚きですよね。"},{"id":{"d":396,"n":5,"t":5},"speaker":"A","text":"もし、経験がそうさせているのだとしたら、同じ白黒の画像を見ても、人によって見える色が違うのでしょうか？"},{"id":{"d":396,"n":6,"t":5},"speaker":"B","text":"その可能性は高そうですね。もし、1箇所だけ正しい色を付けた場合は、全員が同じ色を指摘するのか気になります。"},{"id":{"d":396,"n":7,"t":5},"speaker":"A","text":"確かに、基準となる色が存在するならば、同じ色に見えるようになりそうですね。"},{"id":{"d":396,"n":8,"t":5},"speaker":"B","text":"面白そうなので実験してみたいです。"}];
INSERT INTO sentence [{"id":{"d":397,"n":1,"t":5},"speaker":"A","text":"今日はひな祭りということで、桜餅を食べました。"},{"id":{"d":397,"n":2,"t":5},"speaker":"B","text":"桜餅はとてもおいしいですね。頻繁に食べたくなります。"},{"id":{"d":397,"n":3,"t":5},"speaker":"A","text":"桜の葉は食べますか？"},{"id":{"d":397,"n":4,"t":5},"speaker":"B","text":"いいえ。私は小学生の時に食べて、おいしいと思わなかったのでそれ以来食べていません。あなたは、食べていますか？"},{"id":{"d":397,"n":5,"t":5},"speaker":"A","text":"私は食べています。しかし今日の桜餅は、葉っぱを食べない方がおいしかったです。"},{"id":{"d":397,"n":6,"t":5},"speaker":"B","text":"元々の味が強いときは、葉っぱはない方がおいしいかもしれませんね。"},{"id":{"d":397,"n":7,"t":5},"speaker":"A","text":"葉っぱがついていても、おいしいものはありますが、それよりも葉っぱなしでもおいしいと、さすがだなと思ってしまいます。"},{"id":{"d":397,"n":8,"t":5},"speaker":"B","text":"きちんとした和菓子という感じがしますね。今日が、ひな祭りで良かったです。"}];
INSERT INTO sentence [{"id":{"d":398,"n":1,"t":5},"speaker":"A","text":"生後半年の赤ちゃんに、初めて会いました。"},{"id":{"d":398,"n":2,"t":5},"speaker":"B","text":"街で出会ったなどではなく、知り合いの方のお子さんですか？"},{"id":{"d":398,"n":3,"t":5},"speaker":"A","text":"その通りです。サークルの先輩のお子さんに会ってきました。"},{"id":{"d":398,"n":4,"t":5},"speaker":"B","text":"どうでしたか？やはりかわいかったですか？"},{"id":{"d":398,"n":5,"t":5},"speaker":"A","text":"とてもかわいかったです。赤ちゃんは、目がくりくりしていて、ずっと見つめてきました。僕を認識してくれたのでしょうか？"},{"id":{"d":398,"n":6,"t":5},"speaker":"B","text":"はい。多分、知らない人が現れたので、興味を持ったのでしょう。赤ちゃんは泣いてしまわなかったですか？"},{"id":{"d":398,"n":7,"t":5},"speaker":"A","text":"赤ちゃんはずっと泣かず、僕を見つめていました。知らない人への恐怖よりも、何か気を引かれることがあったのでしょう。"},{"id":{"d":398,"n":8,"t":5},"speaker":"B","text":"人の赤ちゃんで、それほどかわいいと思うようでしたら、自分の赤ちゃんは、さぞかわいいと感じるでしょうね。"}];
INSERT INTO sentence [{"id":{"d":399,"n":1,"t":5},"speaker":"A","text":"そろそろ誕生日ですね。何か欲しいものはありますか？"},{"id":{"d":399,"n":2,"t":5},"speaker":"B","text":"今はあまり欲しいものがありません。強いていうならCDが欲しいです。"},{"id":{"d":399,"n":3,"t":5},"speaker":"A","text":"CDですか？今音楽はサブスクリプションの音楽配信サービスを利用して聴いていると思っていました。"},{"id":{"d":399,"n":4,"t":5},"speaker":"B","text":"確かにそうなのですが、サブスクリプション化されていない音楽のCDもあるので、それは実際に購入しないと聴くことができません。"},{"id":{"d":399,"n":5,"t":5},"speaker":"A","text":"それでは、CDをレンタルしたら良いのではありませんか？"},{"id":{"d":399,"n":6,"t":5},"speaker":"B","text":"CDに特典が付いていますが、レンタルだとその特典を手に入れることができません。"},{"id":{"d":399,"n":7,"t":5},"speaker":"A","text":"つまり、特典が欲しいということですか？"},{"id":{"d":399,"n":8,"t":5},"speaker":"B","text":"サブスクリプションの音楽配信サービスで聴くことができないアーティストなので、それならせめて、CDを入手して、特典も得たいということです。"}];
INSERT INTO sentence [{"id":{"d":400,"n":1,"t":5},"speaker":"A","text":"最近は、有名な曲のダンスを踊っている動画をSNSに投稿する人が多いですね。"},{"id":{"d":400,"n":2,"t":5},"speaker":"B","text":"動画の投稿をしやすくなったこと、動画の視聴もしてもらえることで、投稿した人の自己肯定感が上がることが要因だと考えられます。"},{"id":{"d":400,"n":3,"t":5},"speaker":"A","text":"私は恥ずかしくなるので、そのような動画を撮ることをしようとは思いませんが、他の人は恥ずかしくないのでしょうか？"},{"id":{"d":400,"n":4,"t":5},"speaker":"B","text":"恥ずかしさよりも、他人からの高評価を得られたうれしさの方が勝るのでしょう。"},{"id":{"d":400,"n":5,"t":5},"speaker":"A","text":"さまざまな人がいますね。どのような曲のダンスの曲が多いのですか？"},{"id":{"d":400,"n":6,"t":5},"speaker":"B","text":"いろいろなジャンルがあります。有名なアイドルのダンスも、もちろんありますし、創作したものもあります。"},{"id":{"d":400,"n":7,"t":5},"speaker":"A","text":"創作とは、自分で創り出したダンスを投稿しているということですか？"},{"id":{"d":400,"n":8,"t":5},"speaker":"B","text":"その通りです。ダンスの振り付けを創り出すことを専門にしている人もいるようです。"},{"id":{"d":400,"n":9,"t":5},"speaker":"A","text":"それはコレオグラファーではないのですか？"},{"id":{"d":400,"n":10,"t":5},"speaker":"B","text":"そうですね。まさにコレオグラファーのようなことをしている一般の方も、たくさんいるようですよ。"}];
INSERT INTO sentence [{"id":{"d":401,"n":1,"t":5},"speaker":"A","text":"今シーズン、何回スノーボードをしに行きましたか？"},{"id":{"d":401,"n":2,"t":5},"speaker":"B","text":"今シーズンは1回も行けませんでした。とても行きたかったのですが、行くことができませんでした。"},{"id":{"d":401,"n":3,"t":5},"speaker":"A","text":"今までは、毎シーズンスノーボードをしに行っていたのですか？"},{"id":{"d":401,"n":4,"t":5},"speaker":"B","text":"いいえ、昨シーズンも行くことができていませんでした。しかし、それ以前は、毎シーズン5回以上は行っていました。"},{"id":{"d":401,"n":5,"t":5},"speaker":"A","text":"たくさん行っていますね。誰と行くことが多かったのですか？"},{"id":{"d":401,"n":6,"t":5},"speaker":"B","text":"仲の良い先輩と行っていました。その先輩から、初めてスノーボードの滑り方を教えてもらったのです。"},{"id":{"d":401,"n":7,"t":5},"speaker":"A","text":"良い先輩ですね。どれほど上手になりましたか？"},{"id":{"d":401,"n":8,"t":5},"speaker":"B","text":"少し怖くてジャンプには挑戦していませんが、普通の滑りはできるようになりました。"}];
INSERT INTO sentence [{"id":{"d":402,"n":1,"t":5},"speaker":"A","text":"最近は、いきなり暖かくなってきましたね。"},{"id":{"d":402,"n":2,"t":5},"speaker":"B","text":"そうですね。桜の開花時期が近づいているような感覚があります。"},{"id":{"d":402,"n":3,"t":5},"speaker":"A","text":"今年は、花見の予定はあるのですか？"},{"id":{"d":402,"n":4,"t":5},"speaker":"B","text":"今のところありませんが、仕事上の先輩方が企画してくれるそうなので期待しているところです。"},{"id":{"d":402,"n":5,"t":5},"speaker":"A","text":"それはとても楽しそうですね。既に先輩方とはお会いしたのですか？"},{"id":{"d":402,"n":6,"t":5},"speaker":"B","text":"このご時世ということもあり、まだお会いすることができていません。初対面の場は、花見の席となる可能性が大きいです。"},{"id":{"d":402,"n":7,"t":5},"speaker":"A","text":"少しかわいそうですね。それでは一緒に花見を楽しむことができたらいいですね。"},{"id":{"d":402,"n":8,"t":5},"speaker":"B","text":"とても緊張すると思いますが、ぜひ花見に参加したいです。"}];
INSERT INTO sentence [{"id":{"d":403,"n":1,"t":5},"speaker":"A","text":"今年のクリスマスは、何か予定がありますか？"},{"id":{"d":403,"n":2,"t":5},"speaker":"B","text":"私は何もありません。あなたは、何か予定があるのですか？"},{"id":{"d":403,"n":3,"t":5},"speaker":"A","text":"私も何もありません。何も予定がない人たちで集まって、遊びに行きたいと思っています。"},{"id":{"d":403,"n":4,"t":5},"speaker":"B","text":"楽しそうですね。同期の中で、参加できない人はいるのでしょうか？"},{"id":{"d":403,"n":5,"t":5},"speaker":"A","text":"はい、半数は参加できないようです。仕事の人や、彼女や、彼氏と過ごす人など、不参加の理由はさまざまです。"},{"id":{"d":403,"n":6,"t":5},"speaker":"B","text":"クリスマスの翌日から仕事も休みになるので、仕事がある人は仕事が終わったら呼んでみましょう。"},{"id":{"d":403,"n":7,"t":5},"speaker":"A","text":"できるだけたくさんで遊んだ方が楽しいので、良いですね。そうしましょう。"},{"id":{"d":403,"n":8,"t":5},"speaker":"B","text":"それでは、私も他の方に当たってみます。"}];
INSERT INTO sentence [{"id":{"d":404,"n":1,"t":5},"speaker":"A","text":"今日は、暴風雨警報が出ていますね。"},{"id":{"d":404,"n":2,"t":5},"speaker":"B","text":"どこに出ているのですか？日本全体に出ているのですか？"},{"id":{"d":404,"n":3,"t":5},"speaker":"A","text":"警報が出ているのは、関東地方のみです。しかし、全国的に雨風はとても強い予報です。"},{"id":{"d":404,"n":4,"t":5},"speaker":"B","text":"それは、なぜですか？"},{"id":{"d":404,"n":5,"t":5},"speaker":"A","text":"空気が温められて、積乱雲が発生しているからです。そのため、雨が激しく降る予報です。"},{"id":{"d":404,"n":6,"t":5},"speaker":"B","text":"風も強く、雨も降るのならば、今日は気温も低いですか？"},{"id":{"d":404,"n":7,"t":5},"speaker":"A","text":"予報では、気温は高くなるそうです。積乱雲は、地上に温められた空気の層があると発生しますので、地上は温かいでしょう。"},{"id":{"d":404,"n":8,"t":5},"speaker":"B","text":"それでは、家にいながらも、水分補給などは忘れないようにした方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":405,"n":1,"t":5},"speaker":"A","text":"今日は、とても暖かくなります。"},{"id":{"d":405,"n":2,"t":5},"speaker":"B","text":"昨日は寒かったので、体温調節が大変ですね。"},{"id":{"d":405,"n":3,"t":5},"speaker":"A","text":"寒暖差が激しいのが、この季節の特徴ですので、しっかり体調管理には気を付けてください。"},{"id":{"d":405,"n":4,"t":5},"speaker":"B","text":"上に羽織ることができる物は、持って行くべきですか？"},{"id":{"d":405,"n":5,"t":5},"speaker":"A","text":"帰宅時間にもよりますが、今日は要らないと思います。"},{"id":{"d":405,"n":6,"t":5},"speaker":"B","text":"帰宅は22時ごろになると思うのですが、それでも上着は要りませんか？"},{"id":{"d":405,"n":7,"t":5},"speaker":"A","text":"22時でしたら、持っていた方がいいかもしれません。しかし、荷物になりますので、厚手のコートではなくて良いと思います。"},{"id":{"d":405,"n":8,"t":5},"speaker":"B","text":"やはり、少し面倒臭いので持って行かないことにしました。"}];
INSERT INTO sentence [{"id":{"d":406,"n":1,"t":5},"speaker":"A","text":"放課後に、一緒に遊びませんか？"},{"id":{"d":406,"n":2,"t":5},"speaker":"B","text":"遊びましょう。今日は何をしますか？"},{"id":{"d":406,"n":3,"t":5},"speaker":"A","text":"秘密基地を作りませんか？昨日、外を歩いていたらとても良い場所を見つけました。"},{"id":{"d":406,"n":4,"t":5},"speaker":"B","text":"良いですね。近くに何か、秘密基地を便利にできそうな物はありましたか？"},{"id":{"d":406,"n":5,"t":5},"speaker":"A","text":"すぐ近くに、ソファがありました。少し汚いですが、表面に何かカバーをかけたら大丈夫だと思います。"},{"id":{"d":406,"n":6,"t":5},"speaker":"B","text":"ソファを置いた秘密基地は、とても豪華ですね。今までの私たちの秘密基地の中で、一番豪華になりそうです。"},{"id":{"d":406,"n":7,"t":5},"speaker":"A","text":"それでは宿題が終わったら、電話してください。私は、学校で宿題を終わらせたので、いつでも行くことができます。"},{"id":{"d":406,"n":8,"t":5},"speaker":"B","text":"分かりました。15分もしたら行くことができると思います。"}];
INSERT INTO sentence [{"id":{"d":407,"n":1,"t":5},"speaker":"A","text":"最近は、オンラインゲームがとても流行していますね。"},{"id":{"d":407,"n":2,"t":5},"speaker":"B","text":"家にいながら、他の場所にいる人と遊ぶことができるという良さがありますね。"},{"id":{"d":407,"n":3,"t":5},"speaker":"A","text":"また、オンラインゲームも無料のものが多く出てきていることが、大きな理由でしょう。"},{"id":{"d":407,"n":4,"t":5},"speaker":"B","text":"確かに最近は、クオリティーが高い無料ゲームがたくさんありますね。"},{"id":{"d":407,"n":5,"t":5},"speaker":"A","text":"それぞれのゲームに対応させた無料ボイスチャットも存在しているので、そのことも無料オンラインゲームを発展させている要因かもしれませんね。"},{"id":{"d":407,"n":6,"t":5},"speaker":"B","text":"しかし、普段ゲームをあまりしない身からすると、顔も知らない人と話しながらゲームをすると、不安になりそうです。"},{"id":{"d":407,"n":7,"t":5},"speaker":"A","text":"声だけであれば、個人を特定することも難しいので、大丈夫と考える人が多いのでしょう。"},{"id":{"d":407,"n":8,"t":5},"speaker":"B","text":"もう少し安心できる環境が整ってから、私はオンラインゲームをしたいと思います。"}];
INSERT INTO sentence [{"id":{"d":408,"n":1,"t":5},"speaker":"A","text":"最近は、地上波テレビ局の放送するコント番組が少なくなってしまいましたね。"},{"id":{"d":408,"n":2,"t":5},"speaker":"B","text":"昔はたくさんのコント番組がありましたが、今はコントを番組にしても視聴率が取れないのでしょう。"},{"id":{"d":408,"n":3,"t":5},"speaker":"A","text":"しかし、YouTubeではさまざまなコントの動画がアップされています。"},{"id":{"d":408,"n":4,"t":5},"speaker":"B","text":"テレビでは下品すぎるとすぐにクレームが入る時代なので、放送できるものの幅が狭くなっているのかもしれません。"},{"id":{"d":408,"n":5,"t":5},"speaker":"A","text":"子どもたちはYouTubeで、親の知らないところでテレビ番組以上に規制のない動画を見ているということに、親は気付かないのでしょうか？"},{"id":{"d":408,"n":6,"t":5},"speaker":"B","text":"はい。今のそのような苦言を呈す人は、「子どものため」と言いながら、自分が規制のない動画を見たくないということなのかもしれませんね。"},{"id":{"d":408,"n":7,"t":5},"speaker":"A","text":"しかしそのような人も、子どもの頃には規制のない動画を笑って見ていたはずです。"},{"id":{"d":408,"n":8,"t":5},"speaker":"B","text":"子どもの時に、見せてもらえなかったのかもしれません。理由は分かりませんが、視聴者から批判的な意見が出てしまうようであれば、コントの番組はこれからも増えないように思います。"}];
INSERT INTO sentence [{"id":{"d":409,"n":1,"t":5},"speaker":"A","text":"小さい時に、好きだった漫画は何ですか？"},{"id":{"d":409,"n":2,"t":5},"speaker":"B","text":"私は『キャプテン翼』が好きでした。"},{"id":{"d":409,"n":3,"t":5},"speaker":"A","text":"『キャプテン翼』は世界的にも人気のある漫画ですね。どのようなところが好きなのですか？"},{"id":{"d":409,"n":4,"t":5},"speaker":"B","text":"やはり自分がサッカーをしていたこともあり、作中の用語が分かることが大きく、そうしたところが好きだったのだと思います。"},{"id":{"d":409,"n":5,"t":5},"speaker":"A","text":"確かに小さい子はあまり言葉を知らないので、知らない言葉が多く登場する漫画は苦手かもしれませんね。"},{"id":{"d":409,"n":6,"t":5},"speaker":"B","text":"加えて、さまざまな技が出てくることも好きでした。"},{"id":{"d":409,"n":7,"t":5},"speaker":"A","text":"それは実現可能な技ですか？"},{"id":{"d":409,"n":8,"t":5},"speaker":"B","text":"もちろん実現可能な技が大半でしたが、中には不可能なものもありました。しかし、友達と『キャプテン翼』に登場するサッカーの技の練習を、たくさんしたこともいい思い出です。"},{"id":{"d":409,"n":9,"t":5},"speaker":"A","text":"漫画に登場するサッカーの技を、練習してできるようになった時が、一番うれしいでしょうね。"}];
INSERT INTO sentence [{"id":{"d":410,"n":1,"t":5},"speaker":"A","text":"ホワイトデーには、どのようなものを返したら良いのでしょうか？"},{"id":{"d":410,"n":2,"t":5},"speaker":"B","text":"一般的に、倍額のものを返すといわれますね。"},{"id":{"d":410,"n":3,"t":5},"speaker":"A","text":"元々の値段がどれほどか分からない場合は、どのようにすれば良いですか？"},{"id":{"d":410,"n":4,"t":5},"speaker":"B","text":"その時には、自分の普段の感謝の分を渡せばよいと思います。"},{"id":{"d":410,"n":5,"t":5},"speaker":"A","text":"買いに行くのは、やはりデパートの方がいいですか？"},{"id":{"d":410,"n":6,"t":5},"speaker":"B","text":"デパートにこだわる必要はありませんが、デパートに良いものが置いてあることは確かですね。"},{"id":{"d":410,"n":7,"t":5},"speaker":"A","text":"そうですね。近くにデパートはないので、取り寄せられるものを、少し探してみようと思います。"},{"id":{"d":410,"n":8,"t":5},"speaker":"B","text":"それが良いですね。ぜひ良い物を見つけてください。"}];
INSERT INTO sentence [{"id":{"d":411,"n":1,"t":5},"speaker":"A","text":"大学の講義があった時には、土日や祝日はとてもうれしかったです。しかし、講義もなくなった今は、毎日祝日のようなものなので暇です。"},{"id":{"d":411,"n":2,"t":5},"speaker":"B","text":"夏休みや、春休みなどの長期休みは、暇ですね。加えて、このご時世では、外で遊ぶこともはばかられます。"},{"id":{"d":411,"n":3,"t":5},"speaker":"A","text":"普段は、何をしているのですか？"},{"id":{"d":411,"n":4,"t":5},"speaker":"B","text":"私は、家で音楽を聴きながら本を読む、映画についてインターネットで検索するなどして、少しでも気になる作品があったら見たりしています。"},{"id":{"d":411,"n":5,"t":5},"speaker":"A","text":"あなたは、そのような毎日を過ごしているのですね。"},{"id":{"d":411,"n":6,"t":5},"speaker":"B","text":"しかし、最近は面白いゲームが発売されたので、そのゲームの攻略に時間を使っています。"},{"id":{"d":411,"n":7,"t":5},"speaker":"A","text":"今まであまり考えたことがありませんでしたが、このような長期休みにゲームが多く発売されているような気がします。"},{"id":{"d":411,"n":8,"t":5},"speaker":"B","text":"長期休みは、ゲーム会社にとっては1番の書き入れ時なのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":412,"n":1,"t":5},"speaker":"A","text":"笑いの本場といえば、大阪ですね。"},{"id":{"d":412,"n":2,"t":5},"speaker":"B","text":"そのような印象がありますね。しかし、なぜなのでしょうか？"},{"id":{"d":412,"n":3,"t":5},"speaker":"A","text":"詳しくは知りませんが、大阪では小学生からコンビを組んでお笑いをすると聞いたことがあります。"},{"id":{"d":412,"n":4,"t":5},"speaker":"B","text":"関東では、そのような小学生はいませんね。"},{"id":{"d":412,"n":5,"t":5},"speaker":"A","text":"そうですね。私が小学生の頃は、友達とドッジボールやサッカーなどをして遊んでいました。"},{"id":{"d":412,"n":6,"t":5},"speaker":"B","text":"私たちが、そのような遊びをしているときに、大阪では、お笑いのネタを作ったり披露したりしているということですね。"},{"id":{"d":412,"n":7,"t":5},"speaker":"A","text":"その話が本当ならば、彼らは子どもの頃から、熱心に笑いの訓練をしているということになりますね。"},{"id":{"d":412,"n":8,"t":5},"speaker":"B","text":"そうですね。大阪が、笑いの本場と言われる理由がわかります。"}];
INSERT INTO sentence [{"id":{"d":413,"n":1,"t":5},"speaker":"A","text":"日本には、春夏秋冬がきれいにありますね。"},{"id":{"d":413,"n":2,"t":5},"speaker":"B","text":"そうですね。どれか1つでも欠けたら日本ではなくなってしまうくらい、それぞれの季節で特徴的な表情を見せてくれます。"},{"id":{"d":413,"n":3,"t":5},"speaker":"A","text":"よく質問で「春夏秋冬のうち、季節が1つだけしかないとしたら、どの季節が良いですか？」というものが聞かれますが、季節がどれか1つになったら植物は育ちませんし、生活が困難になるでしょうね。"},{"id":{"d":413,"n":4,"t":5},"speaker":"B","text":"その季節だからこそ採ることができる野菜や魚がありますが、動植物が育つためには、収穫時季以外の他の季節も必要ということですね。"},{"id":{"d":413,"n":5,"t":5},"speaker":"A","text":"その通りです。このようなことを無視して、その季節の食べ物や気候などを常に感じることができるとしたらどの季節が良いですか？"},{"id":{"d":413,"n":6,"t":5},"speaker":"B","text":"私は秋が良いです。食べ物もおいしく、運動もしやすい。また、日も長く、長時間活動することができます。"},{"id":{"d":413,"n":7,"t":5},"speaker":"A","text":"確かに、秋は素晴らしいですね。プールで泳ぐことや、海でバーベキューをすることが大好きなので、私は夏が良いです。"},{"id":{"d":413,"n":8,"t":5},"speaker":"B","text":"海でのバーベキューは、とても楽しそうですね。それぞれの季節に楽しいことがたくさんありますね。"}];
INSERT INTO sentence [{"id":{"d":414,"n":1,"t":5},"speaker":"A","text":"先ほどから、あなたは何を考えているのですか？"},{"id":{"d":414,"n":2,"t":5},"speaker":"B","text":"私は、クリスマスプレゼントを何にしようか考えています。"},{"id":{"d":414,"n":3,"t":5},"speaker":"A","text":"誰にあげるプレゼントなのですか？"},{"id":{"d":414,"n":4,"t":5},"speaker":"B","text":"サンタクロースにあげる物です。"},{"id":{"d":414,"n":5,"t":5},"speaker":"A","text":"サンタクロースは、プレゼントを持ってくる人ですよね？"},{"id":{"d":414,"n":6,"t":5},"speaker":"B","text":"だから、わが家のサンタクロースですよ。"},{"id":{"d":414,"n":7,"t":5},"speaker":"A","text":"ご主人のことですか？"},{"id":{"d":414,"n":8,"t":5},"speaker":"B","text":"そうとも言います。"}];
INSERT INTO sentence [{"id":{"d":415,"n":1,"t":5},"speaker":"A","text":"先日、娘にけん玉をもらいました。"},{"id":{"d":415,"n":2,"t":5},"speaker":"B","text":"けん玉ですか？"},{"id":{"d":415,"n":3,"t":5},"speaker":"A","text":"はい。けん玉は、健康に良いのだそうです。"},{"id":{"d":415,"n":4,"t":5},"speaker":"B","text":"けん玉は、体全体を使うといいますね。"},{"id":{"d":415,"n":5,"t":5},"speaker":"A","text":"けん玉をしてみましたが、玉が全くのりません。"},{"id":{"d":415,"n":6,"t":5},"speaker":"B","text":"膝を使うと良いといいますよ。"},{"id":{"d":415,"n":7,"t":5},"speaker":"A","text":"このようなことで、健康になるのでしょうか？"},{"id":{"d":415,"n":8,"t":5},"speaker":"B","text":"はい。けん玉は全身の良い運動になります。練習して成功するようになったら、楽しくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":416,"n":1,"t":5},"speaker":"A","text":"近頃、俳句が流行しているのを知っていますか？"},{"id":{"d":416,"n":2,"t":5},"speaker":"B","text":"知っています。テレビ番組でも、よく句を詠んでいますよね。"},{"id":{"d":416,"n":3,"t":5},"speaker":"A","text":"自分で作ろうとすると、難しくてできません。"},{"id":{"d":416,"n":4,"t":5},"speaker":"B","text":"いきなり作るのは難しいかもしれませんね。"},{"id":{"d":416,"n":5,"t":5},"speaker":"A","text":"私は、季語が分かりません。"},{"id":{"d":416,"n":6,"t":5},"speaker":"B","text":"何も調べず、俳句を作ろうとしていますか？"},{"id":{"d":416,"n":7,"t":5},"speaker":"A","text":"はい。俳句を感性で作ってはいけないのですか？"},{"id":{"d":416,"n":8,"t":5},"speaker":"B","text":"はい。歳時記を買い、季語などを調べてから俳句を作ると良いと思います。"}];
INSERT INTO sentence [{"id":{"d":417,"n":1,"t":5},"speaker":"A","text":"お借りしていたDVDをお返しします。どうもありがとうございました。"},{"id":{"d":417,"n":2,"t":5},"speaker":"B","text":"どういたしまして。『浅田家！』はどうでしたか？"},{"id":{"d":417,"n":3,"t":5},"speaker":"A","text":"とても良かったです。"},{"id":{"d":417,"n":4,"t":5},"speaker":"B","text":"日本アカデミー賞優秀作品賞を取ったのですよ。"},{"id":{"d":417,"n":5,"t":5},"speaker":"A","text":"そうなのですね、嵐の二宮和也君の演技が素晴らしかったです。"},{"id":{"d":417,"n":6,"t":5},"speaker":"B","text":"嵐のファンでなくても、映画の世界に引き込まれますよね。"},{"id":{"d":417,"n":7,"t":5},"speaker":"A","text":"はい。東日本大震災を取り上げていたのも、良かったと思います。"},{"id":{"d":417,"n":8,"t":5},"speaker":"B","text":"作品のテーマになっている写真集も今度見てみてください。"}];
INSERT INTO sentence [{"id":{"d":418,"n":1,"t":5},"speaker":"A","text":"カラオケに行きませんか？"},{"id":{"d":418,"n":2,"t":5},"speaker":"B","text":"良いですね。"},{"id":{"d":418,"n":3,"t":5},"speaker":"A","text":"あなたは、どのような歌が好きですか？"},{"id":{"d":418,"n":4,"t":5},"speaker":"B","text":"私は、演歌が好きでよく歌います。"},{"id":{"d":418,"n":5,"t":5},"speaker":"A","text":"私はJポップばかり歌いますが、カラオケに付き合ってもらえますか？"},{"id":{"d":418,"n":6,"t":5},"speaker":"B","text":"はい。私はJポップも歌いますから大丈夫ですよ。"},{"id":{"d":418,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。あまり詳しくありませんが、演歌もたまには聴きますので、あなたの歌を聴くのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":419,"n":1,"t":5},"speaker":"A","text":"今日は、月食が見られるそうです。"},{"id":{"d":419,"n":2,"t":5},"speaker":"B","text":"本当ですか？私はまだ見たことがありません。"},{"id":{"d":419,"n":3,"t":5},"speaker":"A","text":"今晩は、皆既月食だそうですから、初めて月食を見るのなら、しっかり観察すると良いですよ。"},{"id":{"d":419,"n":4,"t":5},"speaker":"B","text":"それは楽しみです。"},{"id":{"d":419,"n":5,"t":5},"speaker":"A","text":"8時20分ごろに空を見上げると良いらしいです。"},{"id":{"d":419,"n":6,"t":5},"speaker":"B","text":"晴れているといいですね。"},{"id":{"d":419,"n":7,"t":5},"speaker":"A","text":"そういえば、天気予報はチェックしていませんでした。"},{"id":{"d":419,"n":8,"t":5},"speaker":"B","text":"今からインターネットで、天気を調べてみます。"}];
INSERT INTO sentence [{"id":{"d":420,"n":1,"t":5},"speaker":"A","text":"おひなさまを飾りましょう。"},{"id":{"d":420,"n":2,"t":5},"speaker":"B","text":"まず、お内裏さまを飾るのですね。"},{"id":{"d":420,"n":3,"t":5},"speaker":"A","text":"男びなは、向かって左側ですよ。"},{"id":{"d":420,"n":4,"t":5},"speaker":"B","text":"わが家のおひなさまの飾り方は、男びなは右側だったような気がします。"},{"id":{"d":420,"n":5,"t":5},"speaker":"A","text":"関西出身ですか？関西と関東は飾り方が反対なのですよね。"},{"id":{"d":420,"n":6,"t":5},"speaker":"B","text":"はい。母が大阪出身ですが、どうしておひなさまの飾り方が違うのでしょう？"},{"id":{"d":420,"n":7,"t":5},"speaker":"A","text":"私は、理由までは知りません。"},{"id":{"d":420,"n":8,"t":5},"speaker":"B","text":"今度、理由を調べてみます。"}];
INSERT INTO sentence [{"id":{"d":421,"n":1,"t":5},"speaker":"A","text":"バンドのメンバーが足りません。"},{"id":{"d":421,"n":2,"t":5},"speaker":"B","text":"どのパートが足りませんか？"},{"id":{"d":421,"n":3,"t":5},"speaker":"A","text":"ドラムとベースが足りません。ボーカルとリードギターはたくさんいるのです。"},{"id":{"d":421,"n":4,"t":5},"speaker":"B","text":"みんなで歌いながら、ギターを弾くバンドですか？"},{"id":{"d":421,"n":5,"t":5},"speaker":"A","text":"はい。楽しそうでしょう？"},{"id":{"d":421,"n":6,"t":5},"speaker":"B","text":"はい。それならもう、ドラムとベースは要らないと思います。"},{"id":{"d":421,"n":7,"t":5},"speaker":"A","text":"私の目指す大合奏バンドには、ドラムとベースも必要なのです。"}];
INSERT INTO sentence [{"id":{"d":422,"n":1,"t":5},"speaker":"A","text":"昨夜は熱帯夜で、眠れませんでした。"},{"id":{"d":422,"n":2,"t":5},"speaker":"B","text":"暑かったですよね。"},{"id":{"d":422,"n":3,"t":5},"speaker":"A","text":"寝るときは、クーラーをつけていますか？"},{"id":{"d":422,"n":4,"t":5},"speaker":"B","text":"私の部屋は、クーラーがないので、扇風機のお休みモードを使っています。"},{"id":{"d":422,"n":5,"t":5},"speaker":"A","text":"私も同じです。"},{"id":{"d":422,"n":6,"t":5},"speaker":"B","text":"防犯上、窓は開けられないので大変です。"},{"id":{"d":422,"n":7,"t":5},"speaker":"A","text":"アイスノンを使うと、少しだけ涼しいですよ。"},{"id":{"d":422,"n":8,"t":5},"speaker":"B","text":"私も使っています。"}];
INSERT INTO sentence [{"id":{"d":423,"n":1,"t":5},"speaker":"A","text":"除夜の鐘をつきに行きませんか？"},{"id":{"d":423,"n":2,"t":5},"speaker":"B","text":"はい。除夜の鐘はつけるものなのですか？"},{"id":{"d":423,"n":3,"t":5},"speaker":"A","text":"はい。わが家の近所のお寺は、順番に並べば除夜の鐘をつかせていただけるのです。"},{"id":{"d":423,"n":4,"t":5},"speaker":"B","text":"それは貴重な体験ですね。"},{"id":{"d":423,"n":5,"t":5},"speaker":"A","text":"その代わり、山道をたくさん歩かないと、お寺にたどり着かないのです。"},{"id":{"d":423,"n":6,"t":5},"speaker":"B","text":"寒い山道をどのくらい歩くのですか？"},{"id":{"d":423,"n":7,"t":5},"speaker":"A","text":"40分くらいかかると思います。"},{"id":{"d":423,"n":8,"t":5},"speaker":"B","text":"少し考えさせてください。"}];
INSERT INTO sentence [{"id":{"d":424,"n":1,"t":5},"speaker":"A","text":"今日は、色鉛筆で塗り絵をしましょう。"},{"id":{"d":424,"n":2,"t":5},"speaker":"B","text":"先生、まこと君は25色の色鉛筆なのに私は12色しかありません。"},{"id":{"d":424,"n":3,"t":5},"speaker":"A","text":"色の数で、塗り絵の出来栄えが変わってくるものではないのですよ。"},{"id":{"d":424,"n":4,"t":5},"speaker":"B","text":"でも、たくさん色があった方が良い塗り絵になると思います。"},{"id":{"d":424,"n":5,"t":5},"speaker":"A","text":"それでは、今日は赤と、青と、黄色だけで塗りましょう。"},{"id":{"d":424,"n":6,"t":5},"speaker":"B","text":"3色だけですか？"},{"id":{"d":424,"n":7,"t":5},"speaker":"A","text":"はい。それだけでも、さまざまな色が出せるのですよ。"},{"id":{"d":424,"n":8,"t":5},"speaker":"B","text":"本当ですか？信じられません。"}];
INSERT INTO sentence [{"id":{"d":425,"n":1,"t":5},"speaker":"A","text":"葉っぱを描きたいのに、緑色の画材がないのは困ります。"},{"id":{"d":425,"n":2,"t":5},"speaker":"B","text":"最初に青を薄く塗ってみてください。"},{"id":{"d":425,"n":3,"t":5},"speaker":"A","text":"青い葉っぱになってしまいました。"},{"id":{"d":425,"n":4,"t":5},"speaker":"B","text":"その上に、黄色を塗ってください。"},{"id":{"d":425,"n":5,"t":5},"speaker":"A","text":"緑色になりました！どうしてですか？"},{"id":{"d":425,"n":6,"t":5},"speaker":"B","text":"赤色と青色と黄色は色料の三原色といって、全ての色の元になっているものなのですよ。"},{"id":{"d":425,"n":7,"t":5},"speaker":"A","text":"知りませんでした。"},{"id":{"d":425,"n":8,"t":5},"speaker":"B","text":"いろいろな色ができますから、試しに重ねて塗ってみると良いですよ。"}];
INSERT INTO sentence [{"id":{"d":426,"n":1,"t":5},"speaker":"A","text":"明日の節分は、あなたが鬼の役をしてくださいね。"},{"id":{"d":426,"n":2,"t":5},"speaker":"B","text":"分かりました、鬼のお面はどうしますか？"},{"id":{"d":426,"n":3,"t":5},"speaker":"A","text":"私が厚紙で作りますよ。"},{"id":{"d":426,"n":4,"t":5},"speaker":"B","text":"毎年、耳にかける輪ゴムの当たるところが痛いです。"},{"id":{"d":426,"n":5,"t":5},"speaker":"A","text":"今年は、輪ゴムではなくヘアゴムにしましょうか？"},{"id":{"d":426,"n":6,"t":5},"speaker":"B","text":"それは良いですね。"},{"id":{"d":426,"n":7,"t":5},"speaker":"A","text":"長さが調節できるようにしておきます。"},{"id":{"d":426,"n":8,"t":5},"speaker":"B","text":"ヘアゴムを、自分でちょうどいい長さに切れば良いのですね。"}];
INSERT INTO sentence [{"id":{"d":427,"n":1,"t":5},"speaker":"A","text":"今日のお夕飯は、恵方巻きです。"},{"id":{"d":427,"n":2,"t":5},"speaker":"B","text":"恵方巻きとは、何ですか？"},{"id":{"d":427,"n":3,"t":5},"speaker":"A","text":"節分に恵方を向いて食べる太巻きずしのことです。"},{"id":{"d":427,"n":4,"t":5},"speaker":"B","text":"おいしそうですけれど、切らないのですか？"},{"id":{"d":427,"n":5,"t":5},"speaker":"A","text":"恵方巻きは、丸ごと食べるものです。"},{"id":{"d":427,"n":6,"t":5},"speaker":"B","text":"私は、それほどに大きく口が開きません。"},{"id":{"d":427,"n":7,"t":5},"speaker":"A","text":"今年の恵方巻きは、太すぎるものを買ってしまいました。"},{"id":{"d":427,"n":8,"t":5},"speaker":"B","text":"では、無理はしないで、切って食べましょう。"}];
INSERT INTO sentence [{"id":{"d":428,"n":1,"t":5},"speaker":"A","text":"ママ、お友達にチョコレートをもらいました。"},{"id":{"d":428,"n":2,"t":5},"speaker":"B","text":"初めてのバレンタインチョコレートですね。"},{"id":{"d":428,"n":3,"t":5},"speaker":"A","text":"チョコレートを食べても良いですか？"},{"id":{"d":428,"n":4,"t":5},"speaker":"B","text":"はい。お家に帰って、手を洗って、うがいをしたら、食べても良いです。"},{"id":{"d":428,"n":5,"t":5},"speaker":"A","text":"真美ちゃんは、僕だけにチョコレートをくれました。"},{"id":{"d":428,"n":6,"t":5},"speaker":"B","text":"では来月、真美ちゃんにお礼をしましょう。"}];
INSERT INTO sentence [{"id":{"d":429,"n":1,"t":5},"speaker":"A","text":"バレンタインデーのチョコレートフェアに行きました。"},{"id":{"d":429,"n":2,"t":5},"speaker":"B","text":"私も、昨日行きました。"},{"id":{"d":429,"n":3,"t":5},"speaker":"A","text":"あなたは、たくさんチョコレートを買いましたか？"},{"id":{"d":429,"n":4,"t":5},"speaker":"B","text":"私は、自分用に1つ買っただけです。"},{"id":{"d":429,"n":5,"t":5},"speaker":"A","text":"私も自分用ですが、5つも買ってしまいました。"},{"id":{"d":429,"n":6,"t":5},"speaker":"B","text":"1人で全部食べるのですか？"},{"id":{"d":429,"n":7,"t":5},"speaker":"A","text":"はい。滅多に買えない高級チョコレートですから、毎日1つずつ楽しむのです。"},{"id":{"d":429,"n":8,"t":5},"speaker":"B","text":"私も、もう1つ買いに行きたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":430,"n":1,"t":5},"speaker":"A","text":"子どもコンサートのチケットをもらいました。"},{"id":{"d":430,"n":2,"t":5},"speaker":"B","text":"良いですね。"},{"id":{"d":430,"n":3,"t":5},"speaker":"A","text":"4枚あるので、お子さんと一緒にいかがですか？"},{"id":{"d":430,"n":4,"t":5},"speaker":"B","text":"はい、良いのですか？"},{"id":{"d":430,"n":5,"t":5},"speaker":"A","text":"もちろんです。チケットを余らせても、もったいないです。"},{"id":{"d":430,"n":6,"t":5},"speaker":"B","text":"うれしいです、私の子も喜びます。"},{"id":{"d":430,"n":7,"t":5},"speaker":"A","text":"では、土曜日の14時にコンサートホール前で待ち合わせましょう。"},{"id":{"d":430,"n":8,"t":5},"speaker":"B","text":"子どもと一緒に入口で待っています。"}];
INSERT INTO sentence [{"id":{"d":431,"n":1,"t":5},"speaker":"A","text":"日が短くなりましたね。"},{"id":{"d":431,"n":2,"t":5},"speaker":"B","text":"もうすぐ冬至ですが、カボチャを食べる以外に、何をするのですか？"},{"id":{"d":431,"n":3,"t":5},"speaker":"A","text":"ユズ湯に入るのですよ。"},{"id":{"d":431,"n":4,"t":5},"speaker":"B","text":"ユズ湯ですか？"},{"id":{"d":431,"n":5,"t":5},"speaker":"A","text":"わが家の庭にユズの木があるので、毎年たくさんの実を収穫しており、その実をお風呂に入れられるのです。"},{"id":{"d":431,"n":6,"t":5},"speaker":"B","text":"うらやましいです、買ってきたものでは、それほどにたくさんのユズはお風呂に入れられません。"},{"id":{"d":431,"n":7,"t":5},"speaker":"A","text":"そうですよね、わが家の庭で育ったユズで良ければ差し上げましょうか？"},{"id":{"d":431,"n":8,"t":5},"speaker":"B","text":"うれしいです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":432,"n":1,"t":5},"speaker":"A","text":"ついに100巻です。早速、本屋で購入してきました。"},{"id":{"d":432,"n":2,"t":5},"speaker":"B","text":"何の話ですか？"},{"id":{"d":432,"n":3,"t":5},"speaker":"A","text":"『名探偵コナン』のコミックスの話です。"},{"id":{"d":432,"n":4,"t":5},"speaker":"B","text":"あの漫画は、もう100巻にもなったのですか？"},{"id":{"d":432,"n":5,"t":5},"speaker":"A","text":"そうなのです、読むのが楽しみです。"},{"id":{"d":432,"n":6,"t":5},"speaker":"B","text":"私は、『名探偵コナン』のアニメーションを少し見たことがあるだけですが、面白い作品ですよね。"},{"id":{"d":432,"n":7,"t":5},"speaker":"A","text":"はい。コミックスで何度も読み返すのも良いものですよ。"},{"id":{"d":432,"n":8,"t":5},"speaker":"B","text":"でも、100巻全部はなかなか買えません。"}];
INSERT INTO sentence [{"id":{"d":433,"n":1,"t":5},"speaker":"A","text":"今晩、一緒に映画を見ませんか？"},{"id":{"d":433,"n":2,"t":5},"speaker":"B","text":"私は、映画を収録したDVDを持ってあなたのところへ行きますよ。"},{"id":{"d":433,"n":3,"t":5},"speaker":"A","text":"あなたのお持ちのDVDにホラー映画を収録したものは、ありますか？"},{"id":{"d":433,"n":4,"t":5},"speaker":"B","text":"私はホラーが苦手なので、怖い映画のDVDは持っていません。"},{"id":{"d":433,"n":5,"t":5},"speaker":"A","text":"それは残念です。ハロウィーンなので、お化け映画を見たいと思ったのです。"},{"id":{"d":433,"n":6,"t":5},"speaker":"B","text":"お化けが出ていて怖くない映画ならあります。"},{"id":{"d":433,"n":7,"t":5},"speaker":"A","text":"お化けが出ているのに怖くないのですか？何という作品ですか？"},{"id":{"d":433,"n":8,"t":5},"speaker":"B","text":"作品名は『ホーンテッドマンション』です。遊園地のアトラクションを題材にした映画です。"}];
INSERT INTO sentence [{"id":{"d":434,"n":1,"t":5},"speaker":"A","text":"映画『南極料理人』は面白かったですね。"},{"id":{"d":434,"n":2,"t":5},"speaker":"B","text":"はい。私は先にエッセーを読みましたが、それでも楽しめました。"},{"id":{"d":434,"n":3,"t":5},"speaker":"A","text":"今度、その本を貸してください。"},{"id":{"d":434,"n":4,"t":5},"speaker":"B","text":"良いですよ。エッセーを読むと、南極での料理の苦労や楽しさがより伝わってくると思いますよ。"},{"id":{"d":434,"n":5,"t":5},"speaker":"A","text":"それは楽しみです。映画でも料理がおいしそうでしたよね。"},{"id":{"d":434,"n":6,"t":5},"speaker":"B","text":"ええ。話しているうちに、おいしいものを食べに行きたくなりました。"},{"id":{"d":434,"n":7,"t":5},"speaker":"A","text":"そうですね。映画に出てきた、伊勢エビのエビフライを食べたいです。"},{"id":{"d":434,"n":8,"t":5},"speaker":"B","text":"伊勢エビのエビフライは、残念ながら、この辺のお店では食べられないと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":435,"n":1,"t":5},"speaker":"A","text":"明日は、体育の日ですね。"},{"id":{"d":435,"n":2,"t":5},"speaker":"B","text":"わが家の近所では、町内会対抗運動会が開かれます。"},{"id":{"d":435,"n":3,"t":5},"speaker":"A","text":"私は、会社の運動会に参加します。"},{"id":{"d":435,"n":4,"t":5},"speaker":"B","text":"会社の運動会に、参加するのですか？"},{"id":{"d":435,"n":5,"t":5},"speaker":"A","text":"はい。1年で一番、私が輝く時ですから頑張ります。"},{"id":{"d":435,"n":6,"t":5},"speaker":"B","text":"私は、走るのは嫌です。"},{"id":{"d":435,"n":7,"t":5},"speaker":"A","text":"私は、走るのが目的ではなくて、コスチューム・プレイが目的です。"},{"id":{"d":435,"n":8,"t":5},"speaker":"B","text":"変わった運動会ですね。"}];
INSERT INTO sentence [{"id":{"d":436,"n":1,"t":5},"speaker":"A","text":"雨が止んで良かったですね。"},{"id":{"d":436,"n":2,"t":5},"speaker":"B","text":"今夜のコンサートは、野外での開催ですから、晴れて良かったです。"},{"id":{"d":436,"n":3,"t":5},"speaker":"A","text":"カッパも用意したのですが、要りませんでしたね。"},{"id":{"d":436,"n":4,"t":5},"speaker":"B","text":"寒さ対策に着ておくと良いです。"},{"id":{"d":436,"n":5,"t":5},"speaker":"A","text":"夜は、寒くなるでしょうか？"},{"id":{"d":436,"n":6,"t":5},"speaker":"B","text":"はい。昼間よりは、寒いと思います。"},{"id":{"d":436,"n":7,"t":5},"speaker":"A","text":"音楽に酔いしれて、寒さなど忘れてしまいそうです。"},{"id":{"d":436,"n":8,"t":5},"speaker":"B","text":"そうかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":437,"n":1,"t":5},"speaker":"A","text":"学校のそばの駅に、ストリートピアノが設置されたのを知っていますか？"},{"id":{"d":437,"n":2,"t":5},"speaker":"B","text":"はい。誰か弾いていましたか？"},{"id":{"d":437,"n":3,"t":5},"speaker":"A","text":"私が通ったときは、誰もピアノを弾いていませんでした。"},{"id":{"d":437,"n":4,"t":5},"speaker":"B","text":"学校帰りに、ピアノを弾きに行きましょう。"},{"id":{"d":437,"n":5,"t":5},"speaker":"A","text":"そうしましょう。2人なら連弾ができますね。"},{"id":{"d":437,"n":6,"t":5},"speaker":"B","text":"何の曲にしますか？"},{"id":{"d":437,"n":7,"t":5},"speaker":"A","text":"『ホール・ニュー・ワールド』が良いです。"},{"id":{"d":437,"n":8,"t":5},"speaker":"B","text":"分かりました。私が、帰りまでに先生から楽譜を借りてきます。"}];
INSERT INTO sentence [{"id":{"d":438,"n":1,"t":5},"speaker":"A","text":"兄から、ギターをもらいました。"},{"id":{"d":438,"n":2,"t":5},"speaker":"B","text":"お兄さまは、バンドをやめてしまったのですか？"},{"id":{"d":438,"n":3,"t":5},"speaker":"A","text":"いいえ、もっと本格的にバンド活動をするので、新しいギターを買ったのです。"},{"id":{"d":438,"n":4,"t":5},"speaker":"B","text":"それで古いギターを譲ってくれたのですね。"},{"id":{"d":438,"n":5,"t":5},"speaker":"A","text":"はい。でも私は、まだ簡単なコードしか弾けません。"},{"id":{"d":438,"n":6,"t":5},"speaker":"B","text":"練習中なのですね。"},{"id":{"d":438,"n":7,"t":5},"speaker":"A","text":"そうです。1曲弾けるようになったら、演奏を聴いてください。"},{"id":{"d":438,"n":8,"t":5},"speaker":"B","text":"もちろんです。"}];
