INSERT INTO sentence [{"id":{"d":24,"n":1,"t":5},"speaker":"A","text":"ホラー映画は嫌いです。"},{"id":{"d":24,"n":2,"t":5},"speaker":"B","text":"怖いからですか？"},{"id":{"d":24,"n":3,"t":5},"speaker":"A","text":"それもありますが、映像が不気味で嫌いです。"},{"id":{"d":24,"n":4,"t":5},"speaker":"B","text":"なるほど。僕も同意します。"},{"id":{"d":24,"n":5,"t":5},"speaker":"A","text":"ゾンビやドラキュラが特に苦手です。恐怖を感じます。"},{"id":{"d":24,"n":6,"t":5},"speaker":"B","text":"一度ついた恐怖心はなかなか克服できませんよね。"},{"id":{"d":24,"n":7,"t":5},"speaker":"A","text":"はい、なかなか難しいです。"},{"id":{"d":24,"n":8,"t":5},"speaker":"B","text":"他の楽しい映画を見ましょう。"}];
INSERT INTO sentence [{"id":{"d":25,"n":1,"t":5},"speaker":"A","text":"もうすぐ今年も終わりですね。"},{"id":{"d":25,"n":2,"t":5},"speaker":"B","text":"1年もあっという間ですね。川田さんは、大みそかは何をしますか？"},{"id":{"d":25,"n":3,"t":5},"speaker":"A","text":"私は年越しそばを食べて、紅白歌合戦を見ます。"},{"id":{"d":25,"n":4,"t":5},"speaker":"B","text":"長時間放送していますが、最後まで見ますか？"},{"id":{"d":25,"n":5,"t":5},"speaker":"A","text":"はい、長いですが最後まで見ます。"},{"id":{"d":25,"n":6,"t":5},"speaker":"B","text":"夜更かししても、次の日もお休みだからゆっくりできますものね。"}];
INSERT INTO sentence [{"id":{"d":26,"n":1,"t":5},"speaker":"A","text":"おせち料理は何が好きですか？"},{"id":{"d":26,"n":2,"t":5},"speaker":"B","text":"だて巻きと黒豆、あと紅白なますが好きです。"},{"id":{"d":26,"n":3,"t":5},"speaker":"A","text":"伊達巻は私も好きです。おいしいですよね。ちなみに、お雑煮は好きですか？"},{"id":{"d":26,"n":4,"t":5},"speaker":"B","text":"好きです。体が温まって、お腹がいっぱいになりますよね。"},{"id":{"d":26,"n":5,"t":5},"speaker":"A","text":"人参、大根、しいたけ、鶏肉が入っていて具だくさんですものね。"},{"id":{"d":26,"n":6,"t":5},"speaker":"B","text":"思い出したら食べたくなってきました。お正月が待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":27,"n":1,"t":5},"speaker":"A","text":"毎年バレンタインデーには、チョコレートフェアがいろいろな所で行われていて、とても楽しいです。"},{"id":{"d":27,"n":2,"t":5},"speaker":"B","text":"本当にそう思います。この時期にしか買えないチョコレートが世界中から集まってきていて、うれしくなってしまいます。"},{"id":{"d":27,"n":3,"t":5},"speaker":"A","text":"私も同じです。私は、新宿伊勢丹のフェアが一番好きです。"},{"id":{"d":27,"n":4,"t":5},"speaker":"B","text":"私も行ったことがありますが、日本未上陸でフランスのショコラティエのチョコレートがたくさん売っていて、すてきですよね。"},{"id":{"d":27,"n":5,"t":5},"speaker":"A","text":"まさに。この時期のお楽しみの一つです。もちろんプレゼント用にも買いますが、自分用にも大量に買ってしまいます。"},{"id":{"d":27,"n":6,"t":5},"speaker":"B","text":"お気持ちはとてもよく分かります。チョコレートは、食べているだけで、幸せな気持ちになりますよね。"},{"id":{"d":27,"n":7,"t":5},"speaker":"A","text":"本当にその通りです。"}];
INSERT INTO sentence [{"id":{"d":28,"n":1,"t":5},"speaker":"A","text":"今日はバレンタインデーです。チョコは何個もらえるでしょうか？"},{"id":{"d":28,"n":2,"t":5},"speaker":"B","text":"100個はもらえると思います。"},{"id":{"d":28,"n":3,"t":5},"speaker":"A","text":"本当ですか？"},{"id":{"d":28,"n":4,"t":5},"speaker":"B","text":"冗談です。"},{"id":{"d":28,"n":5,"t":5},"speaker":"A","text":"本当は何個くらいもらえる予定ですか？"},{"id":{"d":28,"n":6,"t":5},"speaker":"B","text":"2個くらいだと思います。"},{"id":{"d":28,"n":7,"t":5},"speaker":"A","text":"2個でももらえれば良いですね。"}];
INSERT INTO sentence [{"id":{"d":29,"n":1,"t":5},"speaker":"A","text":"今日はバレンタインデーですが、先生にチョコレートはあげますか？"},{"id":{"d":29,"n":2,"t":5},"speaker":"B","text":"体育の先生にだけあげる予定です。"},{"id":{"d":29,"n":3,"t":5},"speaker":"A","text":"他の先生や友達にはあげないのですか？"},{"id":{"d":29,"n":4,"t":5},"speaker":"B","text":"あげません。"},{"id":{"d":29,"n":5,"t":5},"speaker":"A","text":"どうして体育の先生にだけなのですか？"},{"id":{"d":29,"n":6,"t":5},"speaker":"B","text":"部活でお世話になっているからです。"},{"id":{"d":29,"n":7,"t":5},"speaker":"A","text":"私も部活の先生にだけあげることにします。"},{"id":{"d":29,"n":8,"t":5},"speaker":"B","text":"一緒に買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":30,"n":1,"t":5},"speaker":"A","text":"今日は結婚記念日でした、忘れていました。"},{"id":{"d":30,"n":2,"t":5},"speaker":"B","text":"どうしてこのような大事な日を忘れるのですか？"},{"id":{"d":30,"n":3,"t":5},"speaker":"A","text":"うっかりしていました。今日は10月15日でしたね。"},{"id":{"d":30,"n":4,"t":5},"speaker":"B","text":"去年は覚えてくれていたのに残念です。"},{"id":{"d":30,"n":5,"t":5},"speaker":"A","text":"すみませんでした。"}];
INSERT INTO sentence [{"id":{"d":31,"n":1,"t":5},"speaker":"A","text":"今日はクリスマスですね。ケーキを食べましょうか？"},{"id":{"d":31,"n":2,"t":5},"speaker":"B","text":"はい。ケーキが大好きなので楽しみです。"},{"id":{"d":31,"n":3,"t":5},"speaker":"A","text":"今から買いに行きます。一緒に行きませんか？"},{"id":{"d":31,"n":4,"t":5},"speaker":"B","text":"はい。行きます。千葉駅前のコージーコーナーですか？"},{"id":{"d":31,"n":5,"t":5},"speaker":"A","text":"はい。そうです。コージーコーナーのケーキは甘過ぎないので、最後までおいしく食べられるから好きなのです。"},{"id":{"d":31,"n":6,"t":5},"speaker":"B","text":"甘過ぎるのは私も苦手ですね。飲み物は紅茶が良いですね。紅茶も買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":32,"n":1,"t":5},"speaker":"A","text":"クリスマスイブはどこに行きますか？"},{"id":{"d":32,"n":2,"t":5},"speaker":"B","text":"ゆずのコンサートを見に行く予定です。"},{"id":{"d":32,"n":3,"t":5},"speaker":"A","text":"私はあいみょんのコンサートに行く予定です。"},{"id":{"d":32,"n":4,"t":5},"speaker":"B","text":"歌が好きなのですね。"},{"id":{"d":32,"n":5,"t":5},"speaker":"A","text":"歌も好きですが、彼女の声が大好きです。"},{"id":{"d":32,"n":6,"t":5},"speaker":"B","text":"コンサートはどこであるのですか？"},{"id":{"d":32,"n":7,"t":5},"speaker":"A","text":"大阪です。"},{"id":{"d":32,"n":8,"t":5},"speaker":"B","text":"お互い楽しいクリスマスになりそうですね。"}];
INSERT INTO sentence [{"id":{"d":33,"n":1,"t":5},"speaker":"A","text":"サンタクロースには何をお願いしますか？"},{"id":{"d":33,"n":2,"t":5},"speaker":"B","text":"おもちゃをくださいとお願いします。"},{"id":{"d":33,"n":3,"t":5},"speaker":"A","text":"おもちゃですか。その他には何かありますか？"},{"id":{"d":33,"n":4,"t":5},"speaker":"B","text":"カードゲームをお願いする予定です。サンタさんは寝ている間に来るのですか？"},{"id":{"d":33,"n":5,"t":5},"speaker":"A","text":"おもちゃとカードゲームが、欲しいのですね。サンタさんは寝ている間にそっとプレゼントを持って来てくれるのですよ。"},{"id":{"d":33,"n":6,"t":5},"speaker":"B","text":"では、早く寝ないとサンタさんは僕の家を通り過ぎてしまいますね。"},{"id":{"d":33,"n":7,"t":5},"speaker":"A","text":"そうですね。今夜は早く寝ましょうね。"},{"id":{"d":33,"n":8,"t":5},"speaker":"B","text":"はい。おやすみなさい。"}];
INSERT INTO sentence [{"id":{"d":34,"n":1,"t":5},"speaker":"A","text":"小さい頃から、時間があると作曲をするのが好きです。"},{"id":{"d":34,"n":2,"t":5},"speaker":"B","text":"それは、またすてきなご趣味ですね。どのような曲を作られることが多いのですか？"},{"id":{"d":34,"n":3,"t":5},"speaker":"A","text":"ピアノ曲を作ることが多いです。以前からピアノを習っているので、そこで習った曲を少しアレンジすることもあります。"},{"id":{"d":34,"n":4,"t":5},"speaker":"B","text":"なんだか、おしゃれですね。よければ、今度お聞かせくださいね。"},{"id":{"d":34,"n":5,"t":5},"speaker":"A","text":"もちろんです。いつかぜひ披露させてください。"},{"id":{"d":34,"n":6,"t":5},"speaker":"B","text":"楽しみにしております。"}];
INSERT INTO sentence [{"id":{"d":35,"n":1,"t":5},"speaker":"A","text":"今日で今年の仕事も終わりですね。"},{"id":{"d":35,"n":2,"t":5},"speaker":"B","text":"1年間お疲れさまでした。"},{"id":{"d":35,"n":3,"t":5},"speaker":"A","text":"お疲れさまでした。さて、今年最後の仕事です。取引先へ年末のあいさつに伺いましょうか。"},{"id":{"d":35,"n":4,"t":5},"speaker":"B","text":"はい、そうしましょう。"},{"id":{"d":35,"n":5,"t":5},"speaker":"A","text":"年明けには新年会をみんなでしたいですね。"},{"id":{"d":35,"n":6,"t":5},"speaker":"B","text":"そうですね。ぜひしましょう。"}];
INSERT INTO sentence [{"id":{"d":36,"n":1,"t":5},"speaker":"A","text":"今日は年末の大掃除をします。"},{"id":{"d":36,"n":2,"t":5},"speaker":"B","text":"よろしくお願いします。"},{"id":{"d":36,"n":3,"t":5},"speaker":"A","text":"窓ふきや普段はしない所までお掃除をしますか？"},{"id":{"d":36,"n":4,"t":5},"speaker":"B","text":"もちろんです。"},{"id":{"d":36,"n":5,"t":5},"speaker":"A","text":"時間はどのくらいかかりますか？"},{"id":{"d":36,"n":6,"t":5},"speaker":"B","text":"2時間位だと思います。"},{"id":{"d":36,"n":7,"t":5},"speaker":"A","text":"途中で休憩はありますか？"},{"id":{"d":36,"n":8,"t":5},"speaker":"B","text":"10分くらい取る予定です。"}];
INSERT INTO sentence [{"id":{"d":37,"n":1,"t":5},"speaker":"A","text":"新年あけましておめでとうございます。"},{"id":{"d":37,"n":2,"t":5},"speaker":"B","text":"今年もよろしくお願いします。"},{"id":{"d":37,"n":3,"t":5},"speaker":"A","text":"初日の出は見ましたか？"},{"id":{"d":37,"n":4,"t":5},"speaker":"B","text":"はい、朝早く起きて見ました。"},{"id":{"d":37,"n":5,"t":5},"speaker":"A","text":"今年もお天気で良かったですよね。たこ揚げをしませんか？"},{"id":{"d":37,"n":6,"t":5},"speaker":"B","text":"たこ揚げはぜひしてみたいです。"},{"id":{"d":37,"n":7,"t":5},"speaker":"A","text":"子どもたちを呼んで、みんなでしましょう。"},{"id":{"d":37,"n":8,"t":5},"speaker":"B","text":"良いですね、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":38,"n":1,"t":5},"speaker":"A","text":"初詣に行きましょうか？"},{"id":{"d":38,"n":2,"t":5},"speaker":"B","text":"はい、行きましょう。"},{"id":{"d":38,"n":3,"t":5},"speaker":"A","text":"おさい銭は持ちましたか？"},{"id":{"d":38,"n":4,"t":5},"speaker":"B","text":"はい、持ちました。"},{"id":{"d":38,"n":5,"t":5},"speaker":"A","text":"神社は人が多いでしょうか？"},{"id":{"d":38,"n":6,"t":5},"speaker":"B","text":"どうでしょうか。初詣なので、にぎわっているかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":39,"n":1,"t":5},"speaker":"A","text":"お正月といえば何を食べますか？"},{"id":{"d":39,"n":2,"t":5},"speaker":"B","text":"おせち料理を食べます。"},{"id":{"d":39,"n":3,"t":5},"speaker":"A","text":"定番ですね。私の家では必ずお餅を食べるのですが、あなたはどうですか？"},{"id":{"d":39,"n":4,"t":5},"speaker":"B","text":"お餅も食べますよ。"},{"id":{"d":39,"n":5,"t":5},"speaker":"A","text":"しょうゆ餅とのり餅のどちらが好きですか？"},{"id":{"d":39,"n":6,"t":5},"speaker":"B","text":"両方とも好きです。お餅といえば、おしるこもおいしいですよね。"},{"id":{"d":39,"n":7,"t":5},"speaker":"A","text":"そうですね。寒いときに食べると体の芯から温まります。"}];
INSERT INTO sentence [{"id":{"d":40,"n":1,"t":5},"speaker":"A","text":"ご成人、おめでとうございます。"},{"id":{"d":40,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":40,"n":3,"t":5},"speaker":"A","text":"成人式には行きますか？"},{"id":{"d":40,"n":4,"t":5},"speaker":"B","text":"はい。友達と一緒に車で行きます。"},{"id":{"d":40,"n":5,"t":5},"speaker":"A","text":"友達がお迎えに来てくれるのですか？"},{"id":{"d":40,"n":6,"t":5},"speaker":"B","text":"はい。加藤君が家まで迎えに来てくれます。加藤君は運転免許を取って新しい車を購入したようです。"},{"id":{"d":40,"n":7,"t":5},"speaker":"A","text":"それは、楽しみですね。あまり遅くならないうちに帰って来てくださいね。"},{"id":{"d":40,"n":8,"t":5},"speaker":"B","text":"はい。分かりました。"}];
INSERT INTO sentence [{"id":{"d":41,"n":1,"t":5},"speaker":"A","text":"今日で正月休みも終わりですね。"},{"id":{"d":41,"n":2,"t":5},"speaker":"B","text":"そうですね。気分が落ち込みます。"},{"id":{"d":41,"n":3,"t":5},"speaker":"A","text":"明日から仕事ですが、頑張れそうですか？"},{"id":{"d":41,"n":4,"t":5},"speaker":"B","text":"もちろん頑張ります。新年度の初日ですものね。"},{"id":{"d":41,"n":5,"t":5},"speaker":"A","text":"新年会は、開催するのでしょうか？"},{"id":{"d":41,"n":6,"t":5},"speaker":"B","text":"どうでしょうか。ぜひ皆でしたいですね。"},{"id":{"d":41,"n":7,"t":5},"speaker":"A","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":42,"n":1,"t":5},"speaker":"A","text":"今日は大雪ですね。"},{"id":{"d":42,"n":2,"t":5},"speaker":"B","text":"長崎県にしては珍しいですね。何年ぶりでしょうか？"},{"id":{"d":42,"n":3,"t":5},"speaker":"A","text":"5年ぶりくらいでしょうかね。あの時は大雪で電車が止まってしまい、停電もあり、大変でしたね。"},{"id":{"d":42,"n":4,"t":5},"speaker":"B","text":"長崎の人は大雪には慣れていませんからね。今日も気を付けないといけませんね。"},{"id":{"d":42,"n":5,"t":5},"speaker":"A","text":"そうですね。何事もないと良いですね。"}];
INSERT INTO sentence [{"id":{"d":43,"n":1,"t":5},"speaker":"A","text":"スキーに行きませんか？"},{"id":{"d":43,"n":2,"t":5},"speaker":"B","text":"寒いのが苦手なので、あまり行きたくありません。"},{"id":{"d":43,"n":3,"t":5},"speaker":"A","text":"厚着をすれば大丈夫ですよ。"},{"id":{"d":43,"n":4,"t":5},"speaker":"B","text":"帽子と手袋も必要ですね。買い物に一緒に行ってくれませんか？"},{"id":{"d":43,"n":5,"t":5},"speaker":"A","text":"良いですよ。ゼビオに買いに行きませんか？確かセールをしていました。"},{"id":{"d":43,"n":6,"t":5},"speaker":"B","text":"本当ですか。ではゼビオに行きましょう。良い買い物がしたいです。"}];
INSERT INTO sentence [{"id":{"d":44,"n":1,"t":5},"speaker":"A","text":"アイススケートをしたことはありますか？"},{"id":{"d":44,"n":2,"t":5},"speaker":"B","text":"昔に少しだけしたことがあります。"},{"id":{"d":44,"n":3,"t":5},"speaker":"A","text":"滑れるのですか？"},{"id":{"d":44,"n":4,"t":5},"speaker":"B","text":"少しだけですが。"},{"id":{"d":44,"n":5,"t":5},"speaker":"A","text":"ぜひ滑り方を教えてください。"},{"id":{"d":44,"n":6,"t":5},"speaker":"B","text":"良いですよ。スケート場は寒いからちゃんと厚着で行きましょうね。"}];
INSERT INTO sentence [{"id":{"d":45,"n":1,"t":5},"speaker":"A","text":"今日から立春ですが、まだまだ寒いです。"},{"id":{"d":45,"n":2,"t":5},"speaker":"B","text":"そうですね。今夜は雪が降るようです。"},{"id":{"d":45,"n":3,"t":5},"speaker":"A","text":"暦だと今日から春なのですが、これではまだまだ冬のようです。"},{"id":{"d":45,"n":4,"t":5},"speaker":"B","text":"天候にも早く春が来るといいですね。"},{"id":{"d":45,"n":5,"t":5},"speaker":"A","text":"ええ、でもこの調子だとまだまだ先ですね。"},{"id":{"d":45,"n":6,"t":5},"speaker":"B","text":"暖かくなるのが待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":46,"n":1,"t":5},"speaker":"A","text":"ホワイトデーは何をお返ししますか？"},{"id":{"d":46,"n":2,"t":5},"speaker":"B","text":"何が良いか迷っています。何をお返しすると喜んでもらえるのでしょうか？"},{"id":{"d":46,"n":3,"t":5},"speaker":"A","text":"靴下やハンカチは、いかがですか？"},{"id":{"d":46,"n":4,"t":5},"speaker":"B","text":"良いですね。毎日使うものですからね。そうします。"}];
INSERT INTO sentence [{"id":{"d":47,"n":1,"t":5},"speaker":"A","text":"今日は娘の小学校の入学式です。"},{"id":{"d":47,"n":2,"t":5},"speaker":"B","text":"私の息子も今日入学式です。"},{"id":{"d":47,"n":3,"t":5},"speaker":"A","text":"そうなのですね。ご入学おめでとうございます。"},{"id":{"d":47,"n":4,"t":5},"speaker":"B","text":"ありがとうございます。山中さんもおめでとうございます。どのような学校生活になるか、親としても楽しみです。"},{"id":{"d":47,"n":5,"t":5},"speaker":"A","text":"そうですね。あっという間に小学生ですものね。小学校は幼稚園とは違うので、朝しっかり歩いて登校できるのかが心配です。"},{"id":{"d":47,"n":6,"t":5},"speaker":"B","text":"そうですね。でも登校班のお兄さんやお姉さんが、面倒を見てくれるので大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":48,"n":1,"t":5},"speaker":"A","text":"入学祝いにおもちゃを買ってあげましょう。"},{"id":{"d":48,"n":2,"t":5},"speaker":"B","text":"おもちゃは要りませんよ。"},{"id":{"d":48,"n":3,"t":5},"speaker":"A","text":"どうしてですか？"},{"id":{"d":48,"n":4,"t":5},"speaker":"B","text":"もう小さい子どもではないので、おもちゃは喜ばないと思います。"},{"id":{"d":48,"n":5,"t":5},"speaker":"A","text":"そうですか。ではお祝いは何がいいですか？"},{"id":{"d":48,"n":6,"t":5},"speaker":"B","text":"お気持ちだけで十分です。"}];
INSERT INTO sentence [{"id":{"d":49,"n":1,"t":5},"speaker":"A","text":"私は小説を書くことが好きです。"},{"id":{"d":49,"n":2,"t":5},"speaker":"B","text":"すてきですね。小説を書くために何か普段からしていることはありますか？"},{"id":{"d":49,"n":3,"t":5},"speaker":"A","text":"題材のヒントを得るためにいろいろな所へ取材に行っています。"},{"id":{"d":49,"n":4,"t":5},"speaker":"B","text":"日本全国ですか？"},{"id":{"d":49,"n":5,"t":5},"speaker":"A","text":"はい。時には海外に行くこともあります。"},{"id":{"d":49,"n":6,"t":5},"speaker":"B","text":"海外にも行くのですね。今度読ませてください。"},{"id":{"d":49,"n":7,"t":5},"speaker":"A","text":"はい、喜んで。ぜひ感想も聞かせてください。"}];
INSERT INTO sentence [{"id":{"d":50,"n":1,"t":5},"speaker":"A","text":"毎日、日記をつけています。"},{"id":{"d":50,"n":2,"t":5},"speaker":"B","text":"日記にはどのようなことを書いていますか？"},{"id":{"d":50,"n":3,"t":5},"speaker":"A","text":"身の回りで起きたことを事細かに書いています。"},{"id":{"d":50,"n":4,"t":5},"speaker":"B","text":"どのようなことがあったか思い出すには良いですね。ただ毎日続けるのは難しそうです。"},{"id":{"d":50,"n":5,"t":5},"speaker":"A","text":"書いているうちに楽しくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":51,"n":1,"t":5},"speaker":"A","text":"すみません、俳優の城田さんですか？ファンです。一緒に写真を撮ってもらえませんか？"},{"id":{"d":51,"n":2,"t":5},"speaker":"B","text":"もちろん良いですよ。"},{"id":{"d":51,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。サインももらえますか？"},{"id":{"d":51,"n":4,"t":5},"speaker":"B","text":"はい、喜んで。どこに書けばよいですか？"},{"id":{"d":51,"n":5,"t":5},"speaker":"A","text":"私のTシャツの背中に書いてください。"},{"id":{"d":51,"n":6,"t":5},"speaker":"B","text":"分かりました。お名前も入れましょうか？"},{"id":{"d":51,"n":7,"t":5},"speaker":"A","text":"はい、ありがとうございます。「あやか」と言います。"}];
INSERT INTO sentence [{"id":{"d":52,"n":1,"t":5},"speaker":"A","text":"その画像を頂くことはできますか？"},{"id":{"d":52,"n":2,"t":5},"speaker":"B","text":"大事な画像なのでお渡しすることはできません。"},{"id":{"d":52,"n":3,"t":5},"speaker":"A","text":"とてもきれいな風景なのでぜひ頂きたいのですが。"},{"id":{"d":52,"n":4,"t":5},"speaker":"B","text":"プリントアウトしたものでよろしければ差し上げます。"},{"id":{"d":52,"n":5,"t":5},"speaker":"A","text":"ぜひお願いいたします。"}];
INSERT INTO sentence [{"id":{"d":53,"n":1,"t":5},"speaker":"A","text":"その絵は、あなたが描いたのですか？"},{"id":{"d":53,"n":2,"t":5},"speaker":"B","text":"いいえ、友達が描きました。"},{"id":{"d":53,"n":3,"t":5},"speaker":"A","text":"とても上手ですね。同じクラスのお友達ですか？"},{"id":{"d":53,"n":4,"t":5},"speaker":"B","text":"4組の佐藤さんです。いろいろなコンクールで受賞している人です。"},{"id":{"d":53,"n":5,"t":5},"speaker":"A","text":"佐藤さんは絵がとてもお上手なのですね。とても繊細に描かれていて写真のようですね。"}];
INSERT INTO sentence [{"id":{"d":54,"n":1,"t":5},"speaker":"A","text":"どのような漫画が好きですか？"},{"id":{"d":54,"n":2,"t":5},"speaker":"B","text":"『ワンピース』が好きです。どの話もすごく面白くて、毎回ワクワクします。"},{"id":{"d":54,"n":3,"t":5},"speaker":"A","text":"その漫画はこの前テレビでも放映されていました。"},{"id":{"d":54,"n":4,"t":5},"speaker":"B","text":"たまにアニメーションも見ますが、僕は毎週、週刊少年ジャンプで読んでいます。"},{"id":{"d":54,"n":5,"t":5},"speaker":"A","text":"そうなのですね。僕は昔からマガジン派でした。"},{"id":{"d":54,"n":6,"t":5},"speaker":"B","text":"週刊少年マガジンは、どのような漫画が面白いのですか？"},{"id":{"d":54,"n":7,"t":5},"speaker":"A","text":"ゴルフの漫画が面白いです。"},{"id":{"d":54,"n":8,"t":5},"speaker":"B","text":"そうですか。今度、僕も見てみます。"}];
INSERT INTO sentence [{"id":{"d":55,"n":1,"t":5},"speaker":"A","text":"この歌を聴くと踊りたくなりますね。"},{"id":{"d":55,"n":2,"t":5},"speaker":"B","text":"リズムがとても良いですよね。なんという曲ですか？"},{"id":{"d":55,"n":3,"t":5},"speaker":"A","text":"『パプリカ』という曲です。"},{"id":{"d":55,"n":4,"t":5},"speaker":"B","text":"NHKでよく流れていましたね。いろいろな人が踊っていました。"},{"id":{"d":55,"n":5,"t":5},"speaker":"A","text":"そうです。簡単な踊りだからみんな覚えやすいのでしょうね。"},{"id":{"d":55,"n":6,"t":5},"speaker":"B","text":"子どもたちにも人気があります。学校でも踊るようです。"},{"id":{"d":55,"n":7,"t":5},"speaker":"A","text":"私の息子もこの前の運動会で踊っていました。"}];
INSERT INTO sentence [{"id":{"d":56,"n":1,"t":5},"speaker":"A","text":"バラエティー番組ではどのような番組をいつも見ますか？"},{"id":{"d":56,"n":2,"t":5},"speaker":"B","text":"ナインティナインの「ぐるナイ」という番組が好きです。"},{"id":{"d":56,"n":3,"t":5},"speaker":"A","text":"皆で料理の金額を当てて、おごってもらったり支払ったりする番組ですか？"},{"id":{"d":56,"n":4,"t":5},"speaker":"B","text":"はい、そうです。本当に自腹で出演者が支払っているのでしょうか？"},{"id":{"d":56,"n":5,"t":5},"speaker":"A","text":"そうだと思います。支払いが決まった時の怒り方や悲しむ顔がうそとは思えません。"},{"id":{"d":56,"n":6,"t":5},"speaker":"B","text":"だとしたら、何十万円もすぐに払える芸能人は本当にお金持ちだと思います。"},{"id":{"d":56,"n":7,"t":5},"speaker":"A","text":"僕もそう思います。"}];
INSERT INTO sentence [{"id":{"d":57,"n":1,"t":5},"speaker":"A","text":"今、流行しているアニメーションを教えてください。"},{"id":{"d":57,"n":2,"t":5},"speaker":"B","text":"『鬼滅の刃』です。"},{"id":{"d":57,"n":3,"t":5},"speaker":"A","text":"私も『鬼滅の刃』が好きです。この前、映画館で上映していた『無限列車編』は本当に面白かったです。"},{"id":{"d":57,"n":4,"t":5},"speaker":"B","text":"僕はコミックで全部読みました。感動して泣いてしまいました。"},{"id":{"d":57,"n":5,"t":5},"speaker":"A","text":"コミックを全部買ったのですか？"},{"id":{"d":57,"n":6,"t":5},"speaker":"B","text":"少しは買いましたが、全部を買うことは難しかったので、残りは歯医者さんに置いてあったものを待ち時間に読みました。"},{"id":{"d":57,"n":7,"t":5},"speaker":"A","text":"何巻まであるのですか？"},{"id":{"d":57,"n":8,"t":5},"speaker":"B","text":"24巻までです。"}];
INSERT INTO sentence [{"id":{"d":58,"n":1,"t":5},"speaker":"A","text":"早く帰らないと連続テレビドラマの時間に間に合いません。"},{"id":{"d":58,"n":2,"t":5},"speaker":"B","text":"はい、分かっています。"},{"id":{"d":58,"n":3,"t":5},"speaker":"A","text":"家に着いたらドラマを見ながら晩ご飯を食べましょう。今日のご飯は何ですか？"},{"id":{"d":58,"n":4,"t":5},"speaker":"B","text":"ハンバーグとドリアの予定です。"},{"id":{"d":58,"n":5,"t":5},"speaker":"A","text":"私はスパゲティが食べたいです。"},{"id":{"d":58,"n":6,"t":5},"speaker":"B","text":"もう今日は準備してしまいました。スパゲティは明日にしましょう。"},{"id":{"d":58,"n":7,"t":5},"speaker":"A","text":"分かりました。とにかく急いで帰りましょう。"}];
INSERT INTO sentence [{"id":{"d":59,"n":1,"t":5},"speaker":"A","text":"海外ドラマは見ますか？"},{"id":{"d":59,"n":2,"t":5},"speaker":"B","text":"いえ、日本のドラマしか見ません。"},{"id":{"d":59,"n":3,"t":5},"speaker":"A","text":"そうですか。日本のドラマでは何を見ていますか？"},{"id":{"d":59,"n":4,"t":5},"speaker":"B","text":"最近は『婚姻届に判を捺しただけですが』というドラマを見ています。"},{"id":{"d":59,"n":5,"t":5},"speaker":"A","text":"どのようなお話ですか？恋愛ドラマですか？それともサスペンスですか？"},{"id":{"d":59,"n":6,"t":5},"speaker":"B","text":"恋愛ドラマです。結婚したのにどこかギクシャクしている主人公たちが面白いのです。"},{"id":{"d":59,"n":7,"t":5},"speaker":"A","text":"結婚したのにギクシャクしているのですね。展開が気になります。"}];
INSERT INTO sentence [{"id":{"d":60,"n":1,"t":5},"speaker":"A","text":"その人形は何だか怖くありませんか？"},{"id":{"d":60,"n":2,"t":5},"speaker":"B","text":"そうでしょうか。これはこの前旅行に行った時に買いました。トルコの人形だったと思います。"},{"id":{"d":60,"n":3,"t":5},"speaker":"A","text":"見た目が、天使とも悪魔とも言えない様子で、少し不気味ですね。"},{"id":{"d":60,"n":4,"t":5},"speaker":"B","text":"あまり気にしていませんでしたが、言われてみればそうですね。なんだか僕も怖くなってきました。"},{"id":{"d":60,"n":5,"t":5},"speaker":"A","text":"そういえば、森山直太朗さんの『トルコの人形』という曲があるのですが、ご存じですか？"},{"id":{"d":60,"n":6,"t":5},"speaker":"B","text":"いいえ、知りません。どのような曲ですか？"},{"id":{"d":60,"n":7,"t":5},"speaker":"A","text":"僕も実際に聴いたことはないのですが、とても怖い歌詞らしいです。"},{"id":{"d":60,"n":8,"t":5},"speaker":"B","text":"そのような曲を聴いたら怖くてトイレに一人で行けなくなってしまいそうです。"}];
INSERT INTO sentence [{"id":{"d":61,"n":1,"t":5},"speaker":"A","text":"成人の日の思い出は何ですか？"},{"id":{"d":61,"n":2,"t":5},"speaker":"B","text":"成人式に参加するために、市民文化ホールにスーツを着て行ったことです。"},{"id":{"d":61,"n":3,"t":5},"speaker":"A","text":"写真は撮りましたか？"},{"id":{"d":61,"n":4,"t":5},"speaker":"B","text":"はい。親と一緒に成人式会場の入り口で撮りました。"},{"id":{"d":61,"n":5,"t":5},"speaker":"A","text":"良い思い出ですね。その時の写真は持っていますか？"},{"id":{"d":61,"n":6,"t":5},"speaker":"B","text":"はい、実家に飾ってあります。"},{"id":{"d":61,"n":7,"t":5},"speaker":"A","text":"今度遊びに行ったときに見せてくださいね。"},{"id":{"d":61,"n":8,"t":5},"speaker":"B","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":62,"n":1,"t":5},"speaker":"A","text":"お正月はたこ揚げをしましょう。"},{"id":{"d":62,"n":2,"t":5},"speaker":"B","text":"そうですね。では、たこを買いに行きましょう。"},{"id":{"d":62,"n":3,"t":5},"speaker":"A","text":"どこで売っていますか？おもちゃ屋さんですか？"},{"id":{"d":62,"n":4,"t":5},"speaker":"B","text":"そうですね。おもちゃ屋さんに行ってみましょう。"},{"id":{"d":62,"n":5,"t":5},"speaker":"A","text":"せっかくなら羽子板やかるたもしましょうよ。"},{"id":{"d":62,"n":6,"t":5},"speaker":"B","text":"そうですね。お正月ならではのおもちゃを買ってきましょう。"},{"id":{"d":62,"n":7,"t":5},"speaker":"A","text":"これらで遊ぶのが楽しみです。早くお正月が来てほしいです。"}];
INSERT INTO sentence [{"id":{"d":63,"n":1,"t":5},"speaker":"A","text":"冬の夜空は星がきれいです。"},{"id":{"d":63,"n":2,"t":5},"speaker":"B","text":"空気が澄んでいるからですね。星座は分かりますか？"},{"id":{"d":63,"n":3,"t":5},"speaker":"A","text":"ほとんど分かりませんが、自分の星座のいて座は分かります。"},{"id":{"d":63,"n":4,"t":5},"speaker":"B","text":"私もほとんど分かりません。天の川くらいです。"},{"id":{"d":63,"n":5,"t":5},"speaker":"A","text":"夏の七夕の時に出る天の川ですね。綺麗ですよね。"},{"id":{"d":63,"n":6,"t":5},"speaker":"B","text":"北斗七星も綺麗ですし、分かりやすいですよね。"},{"id":{"d":63,"n":7,"t":5},"speaker":"A","text":"そうですね。でも私は北斗七星と聞くと、漫画の北斗の拳しか思い浮かびません。"},{"id":{"d":63,"n":8,"t":5},"speaker":"B","text":"それは私も同意します。"}];
INSERT INTO sentence [{"id":{"d":64,"n":1,"t":5},"speaker":"A","text":"雲行きが怪しくなってきました。"},{"id":{"d":64,"n":2,"t":5},"speaker":"B","text":"そうですね。でも、今日は曇りで雨は降らないとニュースで言っていました。"},{"id":{"d":64,"n":3,"t":5},"speaker":"A","text":"では、帰る時も雨は降りませんよね。実は、今日は傘を持ってきていません。"},{"id":{"d":64,"n":4,"t":5},"speaker":"B","text":"とにかく、今日は雨は降らないと思いますから大丈夫ですよ。"},{"id":{"d":64,"n":5,"t":5},"speaker":"A","text":"良かったです。しかし、万が一降って来たらたまりませんから、早く帰ろうと思います。"},{"id":{"d":64,"n":6,"t":5},"speaker":"B","text":"そうですね、早く帰りましょう。雨が降り出すとだんだんと気温も下がって寒くなってくるので。"},{"id":{"d":64,"n":7,"t":5},"speaker":"A","text":"どのくらいまで寒くなりますかね。"},{"id":{"d":64,"n":8,"t":5},"speaker":"B","text":"今晩は3度くらいと言っていました。"},{"id":{"d":64,"n":9,"t":5},"speaker":"A","text":"それは寒いですね。早く帰ってお風呂にでも入って温まりましょう。"}];
INSERT INTO sentence [{"id":{"d":65,"n":1,"t":5},"speaker":"A","text":"昨日の夜、昔書いていた日記を見つけました。"},{"id":{"d":65,"n":2,"t":5},"speaker":"B","text":"日記ですか？どのようなことが書いてありましたか？"},{"id":{"d":65,"n":3,"t":5},"speaker":"A","text":"その日に吸ったタバコの本数や健康に関することが書いてありました。"},{"id":{"d":65,"n":4,"t":5},"speaker":"B","text":"そうなのですね。結構細かく書いていたのですね。当時は何本くらい吸っていたのですか？"},{"id":{"d":65,"n":5,"t":5},"speaker":"A","text":"1日15本くらいでした。今考えると結構吸っていましたね。"},{"id":{"d":65,"n":6,"t":5},"speaker":"B","text":"今はどのくらい吸いますか？"},{"id":{"d":65,"n":7,"t":5},"speaker":"A","text":"昨年から禁煙をしています。禁煙をしてから8カ月目です。"},{"id":{"d":65,"n":8,"t":5},"speaker":"B","text":"健康維持のためにも続けることができるといいですね。"},{"id":{"d":65,"n":9,"t":5},"speaker":"A","text":"はい、今後も継続して頑張りたいと思っています。"}];
INSERT INTO sentence [{"id":{"d":66,"n":1,"t":5},"speaker":"A","text":"梅雨の時期はジメジメして苦手です。"},{"id":{"d":66,"n":2,"t":5},"speaker":"B","text":"そうですね。この蒸し暑い感じはどうにかならないでしょうか？"},{"id":{"d":66,"n":3,"t":5},"speaker":"A","text":"クーラーでもつけましょうか？"},{"id":{"d":66,"n":4,"t":5},"speaker":"B","text":"そうですね、除湿モードでつけてみましょう。"},{"id":{"d":66,"n":5,"t":5},"speaker":"A","text":"早く梅雨が明けてスカッとした夏になってほしいです。"},{"id":{"d":66,"n":6,"t":5},"speaker":"B","text":"そうですね。ただ、夏になったら除湿モードではなく冷房に切り替えないといけませんね。"}];
INSERT INTO sentence [{"id":{"d":67,"n":1,"t":5},"speaker":"A","text":"春になったらお花見に行きましょう。"},{"id":{"d":67,"n":2,"t":5},"speaker":"B","text":"はい。近くの公園の桜を見に行きましょう。昨年もとても綺麗でした。"},{"id":{"d":67,"n":3,"t":5},"speaker":"A","text":"そうでしたね。今年はいろいろな人に声を掛けて皆で楽しみたいです。"},{"id":{"d":67,"n":4,"t":5},"speaker":"B","text":"良いですね。去年はインフルエンザが大流行したことの影響で集まることもできなかったので。"},{"id":{"d":67,"n":5,"t":5},"speaker":"A","text":"そうですね。今年は盛大にいきましょう。"}];
INSERT INTO sentence [{"id":{"d":68,"n":1,"t":5},"speaker":"A","text":"海に行って夕日を見たいです。"},{"id":{"d":68,"n":2,"t":5},"speaker":"B","text":"良いですね。ドライブがてら一緒に行きましょう。"},{"id":{"d":68,"n":3,"t":5},"speaker":"A","text":"夕日に間に合わせるためには、何時に集合したら良いでしょうか？"},{"id":{"d":68,"n":4,"t":5},"speaker":"B","text":"夕方の5時30分くらいではどうですか？"},{"id":{"d":68,"n":5,"t":5},"speaker":"A","text":"はい、大丈夫です。"},{"id":{"d":68,"n":6,"t":5},"speaker":"B","text":"それでは、5時30分に玄関集合でお願いします。"},{"id":{"d":68,"n":7,"t":5},"speaker":"A","text":"了解です。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":69,"n":1,"t":5},"speaker":"A","text":"線香花火をしたいです。"},{"id":{"d":69,"n":2,"t":5},"speaker":"B","text":"線香花火は好きですが、違う花火がしたいです。"},{"id":{"d":69,"n":3,"t":5},"speaker":"A","text":"どのような花火がしたいですか？"},{"id":{"d":69,"n":4,"t":5},"speaker":"B","text":"ロケット花火や打ち上げ花火みたいな派手な花火がいいです。"},{"id":{"d":69,"n":5,"t":5},"speaker":"A","text":"私は怖いです。あなたが火をつけてください。遠くから見ていますので。"},{"id":{"d":69,"n":6,"t":5},"speaker":"B","text":"分かりました。私が火をつけるので、あなたは見ていてください。それから最後に線香花火をしましょう。"}];
INSERT INTO sentence [{"id":{"d":70,"n":1,"t":5},"speaker":"A","text":"お彼岸はお墓参りに行っていますか？"},{"id":{"d":70,"n":2,"t":5},"speaker":"B","text":"おじいちゃんとおばあちゃんのお墓参りに毎年行っています。"},{"id":{"d":70,"n":3,"t":5},"speaker":"A","text":"私も毎年欠かさず行っています。そしてその年にあった出来事を報告しています。"},{"id":{"d":70,"n":4,"t":5},"speaker":"B","text":"私もです。不思議とお参りすると心が落ち着きますよね。"},{"id":{"d":70,"n":5,"t":5},"speaker":"A","text":"おじいちゃん達もきっと天国で会いに来てくれて喜んでいることでしょうね。"},{"id":{"d":70,"n":6,"t":5},"speaker":"B","text":"そう信じています。これからも、お墓参りは毎年かかさず行くつもりです。"}];
INSERT INTO sentence [{"id":{"d":71,"n":1,"t":5},"speaker":"A","text":"台風が近づいて暴風雨が激しくなってきています。"},{"id":{"d":71,"n":2,"t":5},"speaker":"B","text":"本当ですね。今の風速は大体どれくらいですか？"},{"id":{"d":71,"n":3,"t":5},"speaker":"A","text":"つい先ほどのニュースでは、風速25メートルと言っていました。"},{"id":{"d":71,"n":4,"t":5},"speaker":"B","text":"吹き飛ばされたりしないよう、気を付けて過ごしてください。"},{"id":{"d":71,"n":5,"t":5},"speaker":"A","text":"はい。避難用の道具や食事も買ってきたし準備万端です。何かあったらまた連絡します。"}];
INSERT INTO sentence [{"id":{"d":72,"n":1,"t":5},"speaker":"A","text":"雨上がりは光がキラキラしてまぶしいです。"},{"id":{"d":72,"n":2,"t":5},"speaker":"B","text":"そうですね。水たまりもキラキラしていて綺麗です。"},{"id":{"d":72,"n":3,"t":5},"speaker":"A","text":"雨が上がったこの瞬間は、気分が一気に晴れやかになりますね。"},{"id":{"d":72,"n":4,"t":5},"speaker":"B","text":"はい。雨が降っている間は憂鬱ですが、この雨上がりの景色が大好きなので雨を嫌いにはなれません。"}];
INSERT INTO sentence [{"id":{"d":73,"n":1,"t":5},"speaker":"A","text":"打ち上げ花火を見に行きませんか？"},{"id":{"d":73,"n":2,"t":5},"speaker":"B","text":"夏祭りの花火大会のことですか？"},{"id":{"d":73,"n":3,"t":5},"speaker":"A","text":"そうです。1500発も打ち上がるそうです。"},{"id":{"d":73,"n":4,"t":5},"speaker":"B","text":"あの花火大会はすごく有名ですものね。"},{"id":{"d":73,"n":5,"t":5},"speaker":"A","text":"ええ。去年はCOVID-19の影響で中止だったのですが、今年は開催されるようですよ。"},{"id":{"d":73,"n":6,"t":5},"speaker":"B","text":"そうなのですね。楽しみです。他にも誰か誘いますか？"},{"id":{"d":73,"n":7,"t":5},"speaker":"A","text":"会社の仲間を誘ってみましょうか？"},{"id":{"d":73,"n":8,"t":5},"speaker":"B","text":"賛成です。盛り上がりそうですね。"}];
INSERT INTO sentence [{"id":{"d":74,"n":1,"t":5},"speaker":"A","text":"節分は豆まきをしましょう。"},{"id":{"d":74,"n":2,"t":5},"speaker":"B","text":"この地域では大豆ではなく、落花生をまくのでしたよね？"},{"id":{"d":74,"n":3,"t":5},"speaker":"A","text":"そうです。まいた後、そのまま殻をむいて食べることができます。"},{"id":{"d":74,"n":4,"t":5},"speaker":"B","text":"年の数だけ食べるのですよね。その後は何をしますか？"},{"id":{"d":74,"n":5,"t":5},"speaker":"A","text":"みんなでドライブに行きませんか？"},{"id":{"d":74,"n":6,"t":5},"speaker":"B","text":"いいですね。COVID-19の影響もありますから、県内にしましょう。"},{"id":{"d":74,"n":7,"t":5},"speaker":"A","text":"そうですね。では、近くで開催しているフジ祭りに行きましょう。"},{"id":{"d":74,"n":8,"t":5},"speaker":"B","text":"あそこは綺麗ですよね。ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":75,"n":1,"t":5},"speaker":"A","text":"春先は、まだ寒いですね。"},{"id":{"d":75,"n":2,"t":5},"speaker":"B","text":"寒いですが、暖かい日もありますよ。"},{"id":{"d":75,"n":3,"t":5},"speaker":"A","text":"確かにそうですね。少し話が変わりますが、春といえばユーミンの『春よ、来い』という曲が好きです。"},{"id":{"d":75,"n":4,"t":5},"speaker":"B","text":"そうですね、良い歌ですよね。僕も好きです。"},{"id":{"d":75,"n":5,"t":5},"speaker":"A","text":"好きなミュージシャンはいますか？"},{"id":{"d":75,"n":6,"t":5},"speaker":"B","text":"季節は違いますが、夏の定番といわれるＴＵＢＥが好きです。"},{"id":{"d":75,"n":7,"t":5},"speaker":"A","text":"良いですね。季節も夏が好きですか？"},{"id":{"d":75,"n":8,"t":5},"speaker":"B","text":"いいえ、季節では春と秋が好きです。"}];
INSERT INTO sentence [{"id":{"d":76,"n":1,"t":5},"speaker":"A","text":"今日は皆で野球をするつもりでしたが、雨が降ってきたので室内でゲームをしましょう。"},{"id":{"d":76,"n":2,"t":5},"speaker":"B","text":"そうですね。では、プレイステーションの野球ゲームをしましょう。"},{"id":{"d":76,"n":3,"t":5},"speaker":"A","text":"私はテレビゲームよりも人生ゲームがしたいです。"},{"id":{"d":76,"n":4,"t":5},"speaker":"B","text":"それなら4人くらいは人数が必要ですね。"},{"id":{"d":76,"n":5,"t":5},"speaker":"A","text":"交代でやれば何人でもできますが、今日は何人くらいの人が来ますか？"},{"id":{"d":76,"n":6,"t":5},"speaker":"B","text":"全部で10人くらい来るので、3回しましょうか。"},{"id":{"d":76,"n":7,"t":5},"speaker":"A","text":"3回だとすぐに終わってしまってつまらないので、6回しましょう。"},{"id":{"d":76,"n":8,"t":5},"speaker":"B","text":"そうですね、1人2回ずつですね。"}];
INSERT INTO sentence [{"id":{"d":77,"n":1,"t":5},"speaker":"A","text":"梅雨時は、アジサイが綺麗ですね。"},{"id":{"d":77,"n":2,"t":5},"speaker":"B","text":"お花見といえば桜のイメージが強いですが、雨が降る中でアジサイを眺めるのも、風情があって良いですよね。"},{"id":{"d":77,"n":3,"t":5},"speaker":"A","text":"まさにその通りです。以前梅雨時に、鎌倉の長谷寺にアジサイを見に行きましたが、本当に美しく、映画の一場面にいるかのような気持ちになりました。"},{"id":{"d":77,"n":4,"t":5},"speaker":"B","text":"それは、すてきです。まるで荒戸源次郎の映画の世界ですね。幻想的で美しくて、想像しただけで、うっとりしてしまいます。"},{"id":{"d":77,"n":5,"t":5},"speaker":"A","text":"荒戸源次郎をご存じなのですね、実は私は、荒戸源次郎がプロデュースした鈴木清順監督の映画が大好きなのです。"},{"id":{"d":77,"n":6,"t":5},"speaker":"B","text":"おお、鈴木清順がお好きなのですね。偶然です。私も大好きなのです。"},{"id":{"d":77,"n":7,"t":5},"speaker":"A","text":"とても趣味が合いそうで、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":78,"n":1,"t":5},"speaker":"A","text":"今日は久しぶりの快晴です。公園に出かけましょうか。"},{"id":{"d":78,"n":2,"t":5},"speaker":"B","text":"そうですね。近くの大きな公園に行きましょう。"},{"id":{"d":78,"n":3,"t":5},"speaker":"A","text":"良いですね。では、私がお弁当を作ります。"},{"id":{"d":78,"n":4,"t":5},"speaker":"B","text":"ではシートは、僕が持って行きます。"},{"id":{"d":78,"n":5,"t":5},"speaker":"A","text":"お弁当の中身は、何が良いですか？"},{"id":{"d":78,"n":6,"t":5},"speaker":"B","text":"ウインナー、卵焼き、おにぎり、それとビールも欲しいです。"},{"id":{"d":78,"n":7,"t":5},"speaker":"A","text":"分かりました。でもビールはいけません。運転ができなくなってしまいます。"},{"id":{"d":78,"n":8,"t":5},"speaker":"B","text":"ではビールは我慢してお茶にしましょう。"}];
INSERT INTO sentence [{"id":{"d":79,"n":1,"t":5},"speaker":"A","text":"雨空になってきました。帰るころにはきっと傘が必要ですね。"},{"id":{"d":79,"n":2,"t":5},"speaker":"B","text":"私は今日は傘を持ってきています。"},{"id":{"d":79,"n":3,"t":5},"speaker":"A","text":"さすがです。天気予報を見てきたのですね。"},{"id":{"d":79,"n":4,"t":5},"speaker":"B","text":"はい。今はスマートフォンで見られるので便利です。"},{"id":{"d":79,"n":5,"t":5},"speaker":"A","text":"そうですね。予報では何時から雨が降りますか？"},{"id":{"d":79,"n":6,"t":5},"speaker":"B","text":"予報では、14時からの降水確率が80パーセントだそうです。"},{"id":{"d":79,"n":7,"t":5},"speaker":"A","text":"なるほど。間違いなく降ってきそうですね。私も傘を持ってくるべきでした。"},{"id":{"d":79,"n":8,"t":5},"speaker":"B","text":"もし降ってきたら、私の傘に一緒に入りましょう。"}];
INSERT INTO sentence [{"id":{"d":80,"n":1,"t":5},"speaker":"A","text":"今年は暖冬で、過ごしやすいですね。"},{"id":{"d":80,"n":2,"t":5},"speaker":"B","text":"本当ですね。ただ実は暖冬も、良いことばかりではありませんよね。地球温暖化が原因でもありますからね。"},{"id":{"d":80,"n":3,"t":5},"speaker":"A","text":"それって悪いことなのですか？"},{"id":{"d":80,"n":4,"t":5},"speaker":"B","text":"何年前か分かりませんが、温暖化が原因で地上がなくなるという内容の映画がありました。"},{"id":{"d":80,"n":5,"t":5},"speaker":"A","text":"それは怖いけれどずっと先のことだし、今が暖かいならそれで良いのではありませんか？"},{"id":{"d":80,"n":6,"t":5},"speaker":"B","text":"言われてみればそうですね。"},{"id":{"d":80,"n":7,"t":5},"speaker":"A","text":"でも来年は、寒いそうです。"},{"id":{"d":80,"n":8,"t":5},"speaker":"B","text":"そうなのですね。寒いのは嫌です。"}];
INSERT INTO sentence [{"id":{"d":81,"n":1,"t":5},"speaker":"A","text":"季節外れの雪が降っています。"},{"id":{"d":81,"n":2,"t":5},"speaker":"B","text":"雪だけでなくひょうも降っていませんか？"},{"id":{"d":81,"n":3,"t":5},"speaker":"A","text":"降っていますね。外に出たらひょうが直撃して痛そうです。"},{"id":{"d":81,"n":4,"t":5},"speaker":"B","text":"今日は気温が低いようですね。部屋の中も少し寒いです。"},{"id":{"d":81,"n":5,"t":5},"speaker":"A","text":"では暖房を入れましょうか。25度くらいにしましょう。"},{"id":{"d":81,"n":6,"t":5},"speaker":"B","text":"25度では少し低いと思います。もう少し高くしませんか？"},{"id":{"d":81,"n":7,"t":5},"speaker":"A","text":"いえ、25度で十分です。暖房だけでなく衣服でも調節しましょう。トレーナーやダウンジャケットを着たらどうですか？"},{"id":{"d":81,"n":8,"t":5},"speaker":"B","text":"そうですね。そうします。"}];
INSERT INTO sentence [{"id":{"d":82,"n":1,"t":5},"speaker":"A","text":"雨上がりの晴れ間に、虹が出ています。"},{"id":{"d":82,"n":2,"t":5},"speaker":"B","text":"本当ですね。"},{"id":{"d":82,"n":3,"t":5},"speaker":"A","text":"綺麗で幻想的ですね。"},{"id":{"d":82,"n":4,"t":5},"speaker":"B","text":"まさに。雨上がりの虹を見ると、幸せな気分になります。"},{"id":{"d":82,"n":5,"t":5},"speaker":"A","text":"それはすごくよく分かります。虹は、幸福の使いみたいですよね。"}];
INSERT INTO sentence [{"id":{"d":83,"n":1,"t":5},"speaker":"A","text":"この炎天下に、外へ仕事に行かなければならないのですか？"},{"id":{"d":83,"n":2,"t":5},"speaker":"B","text":"はい、大事な仕事ですので行かなければいけません。熱中症に気を付けて頑張りましょう。"},{"id":{"d":83,"n":3,"t":5},"speaker":"A","text":"分かりました。水分を多めに取り対策を万全にして行きましょう。"},{"id":{"d":83,"n":4,"t":5},"speaker":"B","text":"水分だけでなく塩分を取ることも大事ですね。熱中症対策用のあめなども持って行くと良いかもしれません。"},{"id":{"d":83,"n":5,"t":5},"speaker":"A","text":"そうですね。水とあめを準備してきます。"}];
INSERT INTO sentence [{"id":{"d":84,"n":1,"t":5},"speaker":"A","text":"私は夏が一番好きです。"},{"id":{"d":84,"n":2,"t":5},"speaker":"B","text":"僕は夏よりも冬の方が好きです。"},{"id":{"d":84,"n":3,"t":5},"speaker":"A","text":"どうして冬が好きなのですか？"},{"id":{"d":84,"n":4,"t":5},"speaker":"B","text":"夏は暑くて夜は寝苦しいからです。冬は寒くても服を着込めば温かくて眠れます。あなたはどうして夏が好きなのですか？"},{"id":{"d":84,"n":5,"t":5},"speaker":"A","text":"夏祭りなどの行事がとても好きです。花火大会も多く開催されます。"},{"id":{"d":84,"n":6,"t":5},"speaker":"B","text":"確かに夏は楽しい行事がたくさんありますね。花火は僕も好きです。"},{"id":{"d":84,"n":7,"t":5},"speaker":"A","text":"花火大会のクライマックスの巨大花火が本当に圧巻です。花火大会や夏祭りに出る夜店を回るのもとても楽しいのです。"}];
INSERT INTO sentence [{"id":{"d":85,"n":1,"t":5},"speaker":"A","text":"海の日は、いつですか？"},{"id":{"d":85,"n":2,"t":5},"speaker":"B","text":"7月の第3月曜日です。ただ、オリンピックなどの行事が重なるときは少し変わる場合もあるようです。2021年は7月22日でした。"},{"id":{"d":85,"n":3,"t":5},"speaker":"A","text":"そうなのですね。でも毎年変わってしまうと、分かりにくいですね。"},{"id":{"d":85,"n":4,"t":5},"speaker":"B","text":"そうですね。よく変わるので、困ってしまうときがあります。"},{"id":{"d":85,"n":5,"t":5},"speaker":"A","text":"同じです。やっと覚えたと思うと変わっているので、ややこしいですよね。"}];
INSERT INTO sentence [{"id":{"d":86,"n":1,"t":5},"speaker":"A","text":"今年の夏休みは、海へ泳ぎに行きたいです。"},{"id":{"d":86,"n":2,"t":5},"speaker":"B","text":"行きたいですね。ぜひ一緒に行きましょう。"},{"id":{"d":86,"n":3,"t":5},"speaker":"A","text":"地元の友達も、みんな誘って行きましょう。皆で行けば、楽しいです。"},{"id":{"d":86,"n":4,"t":5},"speaker":"B","text":"そうですね。食事は浜辺で、バーベキューをしましょう。"},{"id":{"d":86,"n":5,"t":5},"speaker":"A","text":"良いですね。いろいろ道具を準備する必要がありますね。"},{"id":{"d":86,"n":6,"t":5},"speaker":"B","text":"山田君はバーベキューが好きで、毎年友人とやっていると聞きました。道具の準備については、彼に相談してみようと思います。"},{"id":{"d":86,"n":7,"t":5},"speaker":"A","text":"良いアイデアですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":87,"n":1,"t":5},"speaker":"A","text":"恵比寿にある都立写真美術館を、知っていますか？定期的に写真展を開催しているのですが、とても面白い切り口のものが多くて楽しめます。"},{"id":{"d":87,"n":2,"t":5},"speaker":"B","text":"はい、知っています。以前、「世界報道写真展」を見に行ったことがあります。"},{"id":{"d":87,"n":3,"t":5},"speaker":"A","text":"あら、そうなのですね。私も「世界報道写真展」が大好きで、毎年見に行っています。今年の大賞もとても印象的でした。"},{"id":{"d":87,"n":4,"t":5},"speaker":"B","text":"どのような写真だったのですか？"},{"id":{"d":87,"n":5,"t":5},"speaker":"A","text":"まだ女性の力が弱い中東において、夫に対する反逆罪で体に傷を負った女性の写真でした。まだまだこのような国が世界にはあるのだな、と衝撃を受けました。"},{"id":{"d":87,"n":6,"t":5},"speaker":"B","text":"それは、ショックの大きそうな写真ですね。ただ、現実を知ることはとても大事ですよね。"},{"id":{"d":87,"n":7,"t":5},"speaker":"A","text":"はい、私もそう思います。報道写真の役割は、まさにそれだと思っています。"}];
INSERT INTO sentence [{"id":{"d":88,"n":1,"t":5},"speaker":"A","text":"今日は熱帯夜ですね。なかなか寝付けません。"},{"id":{"d":88,"n":2,"t":5},"speaker":"B","text":"そうですね。ではエアコンをつけたままにして寝ましょうか？"},{"id":{"d":88,"n":3,"t":5},"speaker":"A","text":"エアコンを朝までつけたままにしてしまうと、部屋が冷えすぎて体に悪いです。自動で切れるタイマーをセットして寝ましょう。"},{"id":{"d":88,"n":4,"t":5},"speaker":"B","text":"そうですね。それか、扇風機をつけるのはどうでしょうか？エアコンよりは健康に良いのではありませんか？"},{"id":{"d":88,"n":5,"t":5},"speaker":"A","text":"そうですね。では今夜は扇風機をつけて寝ましょう。"}];
INSERT INTO sentence [{"id":{"d":89,"n":1,"t":5},"speaker":"A","text":"ジメジメして嫌な季節になりましたね。"},{"id":{"d":89,"n":2,"t":5},"speaker":"B","text":"まさに梅雨ですね。"},{"id":{"d":89,"n":3,"t":5},"speaker":"A","text":"こうジメジメして天気が悪いと気持ちも下がる気がしませんか？"},{"id":{"d":89,"n":4,"t":5},"speaker":"B","text":"そうですね。出かけるのもおっくうになってしまいます。"},{"id":{"d":89,"n":5,"t":5},"speaker":"A","text":"はい。家にいて家事をしようとしても、洗濯物もなかなか乾かなくて困ってしまいます。"},{"id":{"d":89,"n":6,"t":5},"speaker":"B","text":"そうですね。早く梅雨が明ければ良いのですが。"},{"id":{"d":89,"n":7,"t":5},"speaker":"A","text":"そうですね。梅雨明け後のよく晴れた真夏日が待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":90,"n":1,"t":5},"speaker":"A","text":"今日は日差しが強いです。"},{"id":{"d":90,"n":2,"t":5},"speaker":"B","text":"そうですね。日なたに出るとさらに暑いです。"},{"id":{"d":90,"n":3,"t":5},"speaker":"A","text":"日差しが強いので、帽子をかぶって行きましょう。"},{"id":{"d":90,"n":4,"t":5},"speaker":"B","text":"そうですね。日焼け止めも塗ろうと思います。"},{"id":{"d":90,"n":5,"t":5},"speaker":"A","text":"水分もこまめに取りましょうね。"},{"id":{"d":90,"n":6,"t":5},"speaker":"B","text":"そうですね。少し多めに水を持っていきますが、それでも足りなくなったら途中で水を買い足そうと思います。"},{"id":{"d":90,"n":7,"t":5},"speaker":"A","text":"道中自動販売機がたくさんあると良いですね。小銭があるかどうかも確認しておきます。"}];
INSERT INTO sentence [{"id":{"d":91,"n":1,"t":5},"speaker":"A","text":"日焼けして真っ黒ですね。どこかに行ったのですか？"},{"id":{"d":91,"n":2,"t":5},"speaker":"B","text":"夏休みに家族でハワイに行きました。"},{"id":{"d":91,"n":3,"t":5},"speaker":"A","text":"まあすてきです。海外旅行なんてぜいたくですね。"},{"id":{"d":91,"n":4,"t":5},"speaker":"B","text":"家族で3年前から計画していました。お金をためて、いつも頑張っている自分へのご褒美ということで、思い切って行ってきました。"},{"id":{"d":91,"n":5,"t":5},"speaker":"A","text":"良いですね。私は夏休みに、熱海の温泉へ行きました。"},{"id":{"d":91,"n":6,"t":5},"speaker":"B","text":"それもすてきですね。温泉はとても気持ちが良いですよね。"},{"id":{"d":91,"n":7,"t":5},"speaker":"A","text":"はい。好きな時にゆっくり入って、とてもリラックスした時間を過ごせました。"}];
INSERT INTO sentence [{"id":{"d":92,"n":1,"t":5},"speaker":"A","text":"この通りには、かき氷のおいしいお店があります。"},{"id":{"d":92,"n":2,"t":5},"speaker":"B","text":"もしかして、「むじゃき」のことですか？白熊がとてもおいしいと有名なお店ですよね。"},{"id":{"d":92,"n":3,"t":5},"speaker":"A","text":"そうです。白熊は味がおいしいのももちろんですが、あのボリュームが圧巻です。"},{"id":{"d":92,"n":4,"t":5},"speaker":"B","text":"そうですね。いろいろなフルーツがたくさん乗っています。"},{"id":{"d":92,"n":5,"t":5},"speaker":"A","text":"ところで、どうして白熊と言うのでしょうか？"},{"id":{"d":92,"n":6,"t":5},"speaker":"B","text":"諸説ありますが、氷が多い地域に住んでいる動物といったら、白熊だからではありませんか？"},{"id":{"d":92,"n":7,"t":5},"speaker":"A","text":"そうなのですね。"}];
INSERT INTO sentence [{"id":{"d":93,"n":1,"t":5},"speaker":"A","text":"ついに、梅雨が明けましたね。うれしくて仕方がありません。"},{"id":{"d":93,"n":2,"t":5},"speaker":"B","text":"本当にその通りです。梅雨時の湿気が多くてじめじめした感じが苦手なので、今がまるで天国のように感じられます。"},{"id":{"d":93,"n":3,"t":5},"speaker":"A","text":"同じです。毎年梅雨の季節には、雨が多くて外出もしづらいので、これからたくさん出かけたいと思います。特に屋外スポーツを楽しみたいと思います。"},{"id":{"d":93,"n":4,"t":5},"speaker":"B","text":"それはすてきです。よければ、ぜひご一緒させてください。久しぶりに屋外でテニスでもいかがですか？"},{"id":{"d":93,"n":5,"t":5},"speaker":"A","text":"それは楽しそうです。ぜひ、近々いたしましょう！"}];
INSERT INTO sentence [{"id":{"d":94,"n":1,"t":5},"speaker":"A","text":"最近はすっかり春らしくなりつつありますね。"},{"id":{"d":94,"n":2,"t":5},"speaker":"B","text":"本当にそうですね。つくしも芽吹いていますし、春の新芽がところどころで垣間見られ、空気も心なしか暖かくなってきたのを感じます。"},{"id":{"d":94,"n":3,"t":5},"speaker":"A","text":"私も同じです。先日は春一番も吹いたみたいですしね。"},{"id":{"d":94,"n":4,"t":5},"speaker":"B","text":"そうみたいですね。まさに三寒四温という感じの日々ですよね。春が一番好きなので、うれしくて仕方ありません。"}];
INSERT INTO sentence [{"id":{"d":95,"n":1,"t":5},"speaker":"A","text":"小さい頃から日記をつけています。自分が感じたことを一日の終わりに振り返って文章にすることは、精神衛生上とても良いと感じています。"},{"id":{"d":95,"n":2,"t":5},"speaker":"B","text":"それは素晴らしい習慣ですね。私も取り入れたいと思います。例えば、どのようなことを書かれるのですか？"},{"id":{"d":95,"n":3,"t":5},"speaker":"A","text":"その日うれしかったことや悲しかったこと、驚いたことを、事実と共に書くことが多いです。"},{"id":{"d":95,"n":4,"t":5},"speaker":"B","text":"それは、続けているうちに自分の考えが整理される上に、自己理解が深まりそうですね。"},{"id":{"d":95,"n":5,"t":5},"speaker":"A","text":"まさにそうなのです。なので、日記をつけるようになってから、判断力が増したように思います。"},{"id":{"d":95,"n":6,"t":5},"speaker":"B","text":"素晴らしいです。"}];
INSERT INTO sentence [{"id":{"d":96,"n":1,"t":5},"speaker":"A","text":"お盆休みには、毎年祖母の家に行ってお墓参りをしています。"},{"id":{"d":96,"n":2,"t":5},"speaker":"B","text":"おばあ様のおうちは、どちらにあるのですか？"},{"id":{"d":96,"n":3,"t":5},"speaker":"A","text":"静岡の田舎にあります。毎年お盆の時期には、ユリの花が咲いています。お墓参りに行く途中の道すがら、ユリの花が咲いていて、なかなか風情があります。"},{"id":{"d":96,"n":4,"t":5},"speaker":"B","text":"なんだか、映画の一場面のような光景ですね。すてきです。私も毎年お盆休みには祖父母の家に行ってお墓参りをしたり、皆でスイカ割りをした後にスイカを食べたりします。"},{"id":{"d":96,"n":5,"t":5},"speaker":"A","text":"まさに夏の風物詩という感じですね！"},{"id":{"d":96,"n":6,"t":5},"speaker":"B","text":"はい、そうなのです。毎年とても楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":97,"n":1,"t":5},"speaker":"A","text":"「暑さ寒さも彼岸まで」という慣用句がありますが、聞いたことはありますか？"},{"id":{"d":97,"n":2,"t":5},"speaker":"B","text":"はい、お母さんが話しているのを聞いたことがあります。"},{"id":{"d":97,"n":3,"t":5},"speaker":"A","text":"でも今は異常気象もあって、だいぶ期間がずれていますよね。"},{"id":{"d":97,"n":4,"t":5},"speaker":"B","text":"そうですね。昔と今では全然気候が違いますよね。"},{"id":{"d":97,"n":5,"t":5},"speaker":"A","text":"旬の野菜も少し時期がずれてきている気がします。"},{"id":{"d":97,"n":6,"t":5},"speaker":"B","text":"そうなのですね。"}];
INSERT INTO sentence [{"id":{"d":98,"n":1,"t":5},"speaker":"A","text":"昨日はテレビゲームのし過ぎで徹夜してしまいました。"},{"id":{"d":98,"n":2,"t":5},"speaker":"B","text":"一睡もしなかったのですか？それは体に良くありませんね。"},{"id":{"d":98,"n":3,"t":5},"speaker":"A","text":"はい。つい夢中になってしまいました。"},{"id":{"d":98,"n":4,"t":5},"speaker":"B","text":"くれぐれも体調を崩さないように気を付けてください。ちなみに何というゲームをしていたのですか？"},{"id":{"d":98,"n":5,"t":5},"speaker":"A","text":"『ドラゴンクエスト』というロールプレイングゲームです。"},{"id":{"d":98,"n":6,"t":5},"speaker":"B","text":"徹夜するくらい夢中になれるゲームなのですか？"},{"id":{"d":98,"n":7,"t":5},"speaker":"A","text":"最後のボスがとても強いのです。"},{"id":{"d":98,"n":8,"t":5},"speaker":"B","text":"面白そうですね。僕も今度買ってみます。"}];
INSERT INTO sentence [{"id":{"d":99,"n":1,"t":5},"speaker":"A","text":"ドラマは1話見るとその先がずっと気になりますよね。"},{"id":{"d":99,"n":2,"t":5},"speaker":"B","text":"そうですね。週に1回のドラマですと、1週間ずっとドラマの続きが気になってしまいます。"},{"id":{"d":99,"n":3,"t":5},"speaker":"A","text":"今見ているドラマはありますか？"},{"id":{"d":99,"n":4,"t":5},"speaker":"B","text":"今は残念ながら興味のあるドラマがなく、ドラマを見ていないのです。ドラマではありませんが、『鬼滅の刃』というアニメーションを毎週見ています。"},{"id":{"d":99,"n":5,"t":5},"speaker":"A","text":"そのアニメーションは僕も見ています。確かに先が気になります。"},{"id":{"d":99,"n":6,"t":5},"speaker":"B","text":"来週が待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":100,"n":1,"t":5},"speaker":"A","text":"今、流れている曲は、誰が歌っているか分かりますか？"},{"id":{"d":100,"n":2,"t":5},"speaker":"B","text":"山下達郎です。クリスマスの定番曲ですよね。"},{"id":{"d":100,"n":3,"t":5},"speaker":"A","text":"曲名は分かりますか？"},{"id":{"d":100,"n":4,"t":5},"speaker":"B","text":"『クリスマス・イブ』だと思います。"},{"id":{"d":100,"n":5,"t":5},"speaker":"A","text":"この曲を聴くとクリスマスの気分になります。とてもワクワクします。"},{"id":{"d":100,"n":6,"t":5},"speaker":"B","text":"そうですね。私はこの曲のCDを持っています。"},{"id":{"d":100,"n":7,"t":5},"speaker":"A","text":"そうなのですね。ぜひ、今度私にCDを貸してください。"},{"id":{"d":100,"n":8,"t":5},"speaker":"B","text":"もちろんよいですよ。今度、学校へ持って来ます。"}];
INSERT INTO sentence [{"id":{"d":101,"n":1,"t":5},"speaker":"A","text":"私が小さい頃に放送していた連続ドラマに『ロングバケーション』があるのですが、今でもこれが一番好きなドラマです。これを超えるものがなかなか現れません。"},{"id":{"d":101,"n":2,"t":5},"speaker":"B","text":"とてもよく分かります。木村拓哉と山口智子主演のラブコメディーですよね。久保田利伸の主題歌もおしゃれでとても良いですよね。"},{"id":{"d":101,"n":3,"t":5},"speaker":"A","text":"はい、そうなのです。あのドラマが大好きで、今でも何度も見返してしまいます。主演の2人がすてきでしたし、台詞もきれいで都会的でした。あのような場面に憧れてしまいます。"},{"id":{"d":101,"n":4,"t":5},"speaker":"B","text":"同じです。主演の2人がピアノを一緒に弾く場面や、告白の場面など、本当にどの場面を切り取っても、絵になるという感じですよね。"},{"id":{"d":101,"n":5,"t":5},"speaker":"A","text":"まさにそうなのです。いつかあのような恋人と、あのドラマのような場面を体現するような恋愛をしてみたいものです。"}];
INSERT INTO sentence [{"id":{"d":102,"n":1,"t":5},"speaker":"A","text":"お芝居を見に行きませんか？"},{"id":{"d":102,"n":2,"t":5},"speaker":"B","text":"どのようなお芝居ですか？"},{"id":{"d":102,"n":3,"t":5},"speaker":"A","text":"コメディーです。テレビで話題になっていたので、見に行きたいのです。"},{"id":{"d":102,"n":4,"t":5},"speaker":"B","text":"お芝居はあまり見に行った事がありませんが、あなたが見に行きたい作品は、どのようなお芝居ですか？"},{"id":{"d":102,"n":5,"t":5},"speaker":"A","text":"出演者のパフォーマンスが素晴らしく、見ていてとても楽しいですよ。テレビで見るのと実際に見るのは全然違います。"},{"id":{"d":102,"n":6,"t":5},"speaker":"B","text":"そうなのですね。それでは一緒に行きましょう。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":103,"n":1,"t":5},"speaker":"A","text":"記念日をどう祝ってくれるかで、その人がどのような人かが分かると思いませんか？"},{"id":{"d":103,"n":2,"t":5},"speaker":"B","text":"そう思います。先日、恋人が私のお誕生日を盛大にお祝いしてくれて、彼がとても愛されて育ってきたことが伝わってきました。"},{"id":{"d":103,"n":3,"t":5},"speaker":"A","text":"それは良いお話です。私の恋人は、この間、私のお誕生日にバラの花束を贈ってくれたので、彼がサプライズ好きだということが分かりました。"},{"id":{"d":103,"n":4,"t":5},"speaker":"B","text":"あなたの恋人は、きっとサプライズ好きな家庭で育ったのですね。"},{"id":{"d":103,"n":5,"t":5},"speaker":"A","text":"はい、彼がきっとそういう家庭で育ったということが垣間見えて、心が温まりました。"}];
INSERT INTO sentence [{"id":{"d":104,"n":1,"t":5},"speaker":"A","text":"今度、「ルノワール展」を見に行きたいと思います。"},{"id":{"d":104,"n":2,"t":5},"speaker":"B","text":"それは良いですね。私もルノワールの作品がすごく好きで、近々見に行こうと思っていました。よろしければ、ぜひご一緒させてください。"},{"id":{"d":104,"n":3,"t":5},"speaker":"A","text":"はい、一緒に行きましょう。今年は印象派の美術展が多く、本当にうれしいです。私は印象派が大好きなのです。"},{"id":{"d":104,"n":4,"t":5},"speaker":"B","text":"確かにそうですね。今年は、ルノワールだけでなく、モネ、マネ、セザンヌまで、たくさんの印象派の美術展がありますからね。"},{"id":{"d":104,"n":5,"t":5},"speaker":"A","text":"はい、まるで日本人の印象派好きを反映しているかのようです。"},{"id":{"d":104,"n":6,"t":5},"speaker":"B","text":"全くそうですね。"}];
INSERT INTO sentence [{"id":{"d":105,"n":1,"t":5},"speaker":"A","text":"あけましておめでとうございます。今年もよろしくお願いいたします。"},{"id":{"d":105,"n":2,"t":5},"speaker":"B","text":"こちらこそ、よろしくお願いいたします。"},{"id":{"d":105,"n":3,"t":5},"speaker":"A","text":"今年のお正月は、どこかへ行かれましたか？"},{"id":{"d":105,"n":4,"t":5},"speaker":"B","text":"いいえ、特にどこにも行きませんでした。家族と自宅で過ごしました。みんなでお雑煮とおせちを食べながら、お正月番組を見ました。"},{"id":{"d":105,"n":5,"t":5},"speaker":"A","text":"なるほど。典型的な日本のお正月、という感じですね。"},{"id":{"d":105,"n":6,"t":5},"speaker":"B","text":"はい、そうです。これはこれで、楽しいものだな、と思いました。"},{"id":{"d":105,"n":7,"t":5},"speaker":"A","text":"全くその通りです。"}];
INSERT INTO sentence [{"id":{"d":106,"n":1,"t":5},"speaker":"A","text":"昨日の心霊ミステリーのテレビ番組は見ましたか？"},{"id":{"d":106,"n":2,"t":5},"speaker":"B","text":"見ました。とても怖かったですよね。"},{"id":{"d":106,"n":3,"t":5},"speaker":"A","text":"はい、怖かったです。霊が出てきたり、誰もいないのに物が動いたりして不気味でした。"},{"id":{"d":106,"n":4,"t":5},"speaker":"B","text":"今度は2人で一緒に心霊ミステリーのテレビ番組を見ませんか？2人で見ると、あまり怖くないかもしれません。"},{"id":{"d":106,"n":5,"t":5},"speaker":"A","text":"良いアイデアですね。では、来週は私の家で一緒に見ましょう。"}];
INSERT INTO sentence [{"id":{"d":107,"n":1,"t":5},"speaker":"A","text":"ジャガイモを使う料理には、さまざまなバリエーションがあります。"},{"id":{"d":107,"n":2,"t":5},"speaker":"B","text":"例えば、どのような料理がありますか？"},{"id":{"d":107,"n":3,"t":5},"speaker":"A","text":"煮物やシチュー、コロッケなどです。"},{"id":{"d":107,"n":4,"t":5},"speaker":"B","text":"今の寒い季節は、シチューが良いですね。"},{"id":{"d":107,"n":5,"t":5},"speaker":"A","text":"玉ネギやニンジン、マッシュルーム、お肉があるので、今日はシチューを作りましょう。"}];
INSERT INTO sentence [{"id":{"d":108,"n":1,"t":5},"speaker":"A","text":"今年、私の子どもが初めてひな祭りの日を迎えます。"},{"id":{"d":108,"n":2,"t":5},"speaker":"B","text":"それは楽しみですね。何をして過ごされるご予定ですか？"},{"id":{"d":108,"n":3,"t":5},"speaker":"A","text":"ひな人形を飾って、ちらしずしを作り、甘酒を飲もうと思っています。"},{"id":{"d":108,"n":4,"t":5},"speaker":"B","text":"すてきですね。きっとお子さまも喜ばれることでしょう。"},{"id":{"d":108,"n":5,"t":5},"speaker":"A","text":"はい、喜んでくれると良いなと思っています。"}];
INSERT INTO sentence [{"id":{"d":109,"n":1,"t":5},"speaker":"A","text":"今流れているBGMは何かご存知ですか？"},{"id":{"d":109,"n":2,"t":5},"speaker":"B","text":"はい、こちらは有名なシャンソンで、フランス・ギャルの『夢見るシャンソン人形』です。"},{"id":{"d":109,"n":3,"t":5},"speaker":"A","text":"そうなのですね、とてもすてきな曲ですね。今度、自分でもレコードを購入してみたいと思います。"},{"id":{"d":109,"n":4,"t":5},"speaker":"B","text":"はい、ぜひご購入ください。"},{"id":{"d":109,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":110,"n":1,"t":5},"speaker":"A","text":"今週末、宝塚歌劇を見に行ってきます。"},{"id":{"d":110,"n":2,"t":5},"speaker":"B","text":"あら、それは偶然です。私もちょうど先週見に行ってきたばかりです。"},{"id":{"d":110,"n":3,"t":5},"speaker":"A","text":"おお、そうなのですね。いかがでしたか？"},{"id":{"d":110,"n":4,"t":5},"speaker":"B","text":"相変わらず素晴らしかったです。久しぶりに、心が洗われる芸術に触れた気持ちです。"},{"id":{"d":110,"n":5,"t":5},"speaker":"A","text":"見に行くことが、ますます楽しみになりました。"},{"id":{"d":110,"n":6,"t":5},"speaker":"B","text":"それはなによりです。ぜひ楽しんできてください。"}];
INSERT INTO sentence [{"id":{"d":111,"n":1,"t":5},"speaker":"A","text":"私は、クラシック音楽のコンサートには行った事がありません。"},{"id":{"d":111,"n":2,"t":5},"speaker":"B","text":"私も行った事がありません。クラシック音楽が苦手なのです。"},{"id":{"d":111,"n":3,"t":5},"speaker":"A","text":"私の場合は、クラシック音楽は好きなのですが、コンサートに行く機会がありません。"},{"id":{"d":111,"n":4,"t":5},"speaker":"B","text":"そうなのですね。私は、クラシック音楽を聴くと眠くなってしまうのです。"},{"id":{"d":111,"n":5,"t":5},"speaker":"A","text":"クラシック音楽は心に残る名曲が多いし、コンサートに行って目の前で聴けば、迫力があって印象が変わると思います。"},{"id":{"d":111,"n":6,"t":5},"speaker":"B","text":"そうかもしれませんね。それでは今度、一緒に行きませんか？"},{"id":{"d":111,"n":7,"t":5},"speaker":"A","text":"いいですね。近いうちに近場で開催されるコンサートがあるか調べてみます。"}];
INSERT INTO sentence [{"id":{"d":112,"n":1,"t":5},"speaker":"A","text":"寝る前にいつも、スマートフォンで動画を見ています。"},{"id":{"d":112,"n":2,"t":5},"speaker":"B","text":"私もそうです。最近は、お笑い芸人の動画をよく見ています。"},{"id":{"d":112,"n":3,"t":5},"speaker":"A","text":"誰の動画を見ているのですか？"},{"id":{"d":112,"n":4,"t":5},"speaker":"B","text":"ネプチューンです。"},{"id":{"d":112,"n":5,"t":5},"speaker":"A","text":"ネプチューンのどのような動画ですか？"},{"id":{"d":112,"n":6,"t":5},"speaker":"B","text":"昔、フジテレビ系列で放送されていた「笑う犬」というコント番組です。"},{"id":{"d":112,"n":7,"t":5},"speaker":"A","text":"懐かしいですね。その番組は僕も知っています。面白いですよね。"}];
INSERT INTO sentence [{"id":{"d":113,"n":1,"t":5},"speaker":"A","text":"昨日、妹の産んだ赤ちゃんと遊びました。ガラガラを鳴らしたら、にっこり笑ってくれました。"},{"id":{"d":113,"n":2,"t":5},"speaker":"B","text":"めい御さんは、今おいくつですか？"},{"id":{"d":113,"n":3,"t":5},"speaker":"A","text":"3カ月です。"},{"id":{"d":113,"n":4,"t":5},"speaker":"B","text":"とてもかわいいときですね。"},{"id":{"d":113,"n":5,"t":5},"speaker":"A","text":"ええ。私は妹とも仲が良いのですが、めいの顔を見に行きたい気持ちもあって、よく妹の家へ遊びに行っています。"},{"id":{"d":113,"n":6,"t":5},"speaker":"B","text":"めい御さんの成長が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":114,"n":1,"t":5},"speaker":"A","text":"このトランプの絵柄と数字を覚えてください。"},{"id":{"d":114,"n":2,"t":5},"speaker":"B","text":"はい、覚えました。"},{"id":{"d":114,"n":3,"t":5},"speaker":"A","text":"カードを戻してください。一度だけカードを切りますね。ではここで、あなたのスボンのポケットを確認してください。ご覧のように、先ほどのカードがいつの間にか、あなたのポケットに移動しました。"},{"id":{"d":114,"n":4,"t":5},"speaker":"B","text":"これは確かに、先ほど私が引いたカードです。驚きました。どのようにしているのですか？"},{"id":{"d":114,"n":5,"t":5},"speaker":"A","text":"それは秘密です。残念ながら、教えられません。"},{"id":{"d":114,"n":6,"t":5},"speaker":"B","text":"では、もう一度見せてください。今度はタネを見破ります。"}];
INSERT INTO sentence [{"id":{"d":115,"n":1,"t":5},"speaker":"A","text":"電線にたこが引っかかってしまいました。"},{"id":{"d":115,"n":2,"t":5},"speaker":"B","text":"電力会社に電話して、たこを取ってもらいましょう。"},{"id":{"d":115,"n":3,"t":5},"speaker":"A","text":"そうですね。自分たちで、取ろうとするのは危ないですものね。"},{"id":{"d":115,"n":4,"t":5},"speaker":"B","text":"電力会社の電話番号を調べましょう。"},{"id":{"d":115,"n":5,"t":5},"speaker":"A","text":"分かりました。03-0123-4567のようです。"},{"id":{"d":115,"n":6,"t":5},"speaker":"B","text":"早速、電話しましょう。"},{"id":{"d":115,"n":7,"t":5},"speaker":"A","text":"はい、急いで電話します。"}];
INSERT INTO sentence [{"id":{"d":116,"n":1,"t":5},"speaker":"A","text":"もう、花火の季節ですね。"},{"id":{"d":116,"n":2,"t":5},"speaker":"B","text":"そうですね。夏になりましたね。"},{"id":{"d":116,"n":3,"t":5},"speaker":"A","text":"夏祭りの花火大会に、一緒に行きませんか？"},{"id":{"d":116,"n":4,"t":5},"speaker":"B","text":"ぜひ行きましょう。楽しみです。"},{"id":{"d":116,"n":5,"t":5},"speaker":"A","text":"浴衣を着て行くのはいかがですか？"},{"id":{"d":116,"n":6,"t":5},"speaker":"B","text":"いいですね。風情が感じられます。"}];
INSERT INTO sentence [{"id":{"d":117,"n":1,"t":5},"speaker":"A","text":"敬老の日には、祖父母の家に行って、祖父母と一緒に過ごす予定です。"},{"id":{"d":117,"n":2,"t":5},"speaker":"B","text":"それは、とても祖父母孝行なことですね。お2人とも、とても喜ばれることでしょう。"},{"id":{"d":117,"n":3,"t":5},"speaker":"A","text":"はい、そうだと良いなと思っています。"},{"id":{"d":117,"n":4,"t":5},"speaker":"B","text":"ちなみに、何をして過ごされるご予定ですか？"},{"id":{"d":117,"n":5,"t":5},"speaker":"A","text":"一緒に、近くの温泉に行く予定です。"},{"id":{"d":117,"n":6,"t":5},"speaker":"B","text":"それは、また楽しそうですね。"},{"id":{"d":117,"n":7,"t":5},"speaker":"A","text":"はい、とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":118,"n":1,"t":5},"speaker":"A","text":"このぬいぐるみは、小さい頃に父に買ってもらいました。"},{"id":{"d":118,"n":2,"t":5},"speaker":"B","text":"テディベアですね。とてもかわいいですね。"},{"id":{"d":118,"n":3,"t":5},"speaker":"A","text":"ありがとうございます。このぬいぐるみには、いろいろと思い出があります。"},{"id":{"d":118,"n":4,"t":5},"speaker":"B","text":"どのような思い出ですか？良ければ聞かせてください。"},{"id":{"d":118,"n":5,"t":5},"speaker":"A","text":"誕生日でもクリスマスでもない普通の日に、おもちゃ屋さんで私がぬいぐるみを欲しがって駄々をこねて泣き叫んだのです。それで父が困ってしまい、私を泣き止ませようと買ってくれたのです。"},{"id":{"d":118,"n":6,"t":5},"speaker":"B","text":"優しいお父さまですね。お父さまも困った末での行動だったのでしょうね。"},{"id":{"d":118,"n":7,"t":5},"speaker":"A","text":"はい。そのような思い出もあったためか、今も大事にしています。18歳まで、ぬいぐるみを抱いて眠っていたくらいです。"},{"id":{"d":118,"n":8,"t":5},"speaker":"B","text":"あなたにとって、本当に大切なものなのですね。"},{"id":{"d":118,"n":9,"t":5},"speaker":"A","text":"はい。そうなのです。"}];
INSERT INTO sentence [{"id":{"d":119,"n":1,"t":5},"speaker":"A","text":"最高気温が、何度以上になると真夏日というのですか？"},{"id":{"d":119,"n":2,"t":5},"speaker":"B","text":"確か、30度だと思います。"},{"id":{"d":119,"n":3,"t":5},"speaker":"A","text":"なるほど。では、真冬日は何度以下ですか？"},{"id":{"d":119,"n":4,"t":5},"speaker":"B","text":"0度未満だったと記憶しています。"},{"id":{"d":119,"n":5,"t":5},"speaker":"A","text":"よく覚えていますね。素晴らしいですね。"},{"id":{"d":119,"n":6,"t":5},"speaker":"B","text":"天候の事は、とても興味があったので、よく自分で調べていました。"},{"id":{"d":119,"n":7,"t":5},"speaker":"A","text":"そうなのですね。これから天気のことで分からないことがあったら、あなたに聞くようにします。"},{"id":{"d":119,"n":8,"t":5},"speaker":"B","text":"お手柔らかにお願いします。"}];
INSERT INTO sentence [{"id":{"d":120,"n":1,"t":5},"speaker":"A","text":"今年は暖冬で、雪も少ないです。"},{"id":{"d":120,"n":2,"t":5},"speaker":"B","text":"クリスマス頃になれば、雪が降るでしょうか？"},{"id":{"d":120,"n":3,"t":5},"speaker":"A","text":"はっきり分かりませんが、クリスマスの頃はまだあまり雪が積もらないかもしれません。"},{"id":{"d":120,"n":4,"t":5},"speaker":"B","text":"それは残念です。今年のクリスマスには、スキーをしたいので、今からたくさん雪が降ることを願っています。"}];
INSERT INTO sentence [{"id":{"d":121,"n":1,"t":5},"speaker":"A","text":"日曜日の天気はどうでしょうか？"},{"id":{"d":121,"n":2,"t":5},"speaker":"B","text":"テレビの天気予報番組のキャスターが「晴れ」だと言っています。"},{"id":{"d":121,"n":3,"t":5},"speaker":"A","text":"それでは釣りにでも行きませんか？"},{"id":{"d":121,"n":4,"t":5},"speaker":"B","text":"良いですね。どこへ釣りに行きますか？"},{"id":{"d":121,"n":5,"t":5},"speaker":"A","text":"海釣りが良いですね。船を使って沖に出ましょうか？"},{"id":{"d":121,"n":6,"t":5},"speaker":"B","text":"良いですね。それでは船を手配しておきます。"}];
INSERT INTO sentence [{"id":{"d":122,"n":1,"t":5},"speaker":"A","text":"ごみを捨てに行ったら、急に雨が降ってきました。"},{"id":{"d":122,"n":2,"t":5},"speaker":"B","text":"それは、災難でしたね。"},{"id":{"d":122,"n":3,"t":5},"speaker":"A","text":"はい。傘も持たずに、ごみを捨てに行ったのでひどくぬれてしまいました。"},{"id":{"d":122,"n":4,"t":5},"speaker":"B","text":"大変でしたね。しっかり体を拭いて風邪をひかないようにしてください。"},{"id":{"d":122,"n":5,"t":5},"speaker":"A","text":"ありがとうございます。そうします。"}];
INSERT INTO sentence [{"id":{"d":123,"n":1,"t":5},"speaker":"A","text":"お昼過ぎには、この雨も上がるようです。"},{"id":{"d":123,"n":2,"t":5},"speaker":"B","text":"それは良かったです。私は、お昼からゴルフへ行く予定です。"},{"id":{"d":123,"n":3,"t":5},"speaker":"A","text":"私は買い物へ行く予定でした。"},{"id":{"d":123,"n":4,"t":5},"speaker":"B","text":"どこへ買い物に行くのですか？"},{"id":{"d":123,"n":5,"t":5},"speaker":"A","text":"銀座のデパートに行きます。"},{"id":{"d":123,"n":6,"t":5},"speaker":"B","text":"何を買いに行くのですか？"},{"id":{"d":123,"n":7,"t":5},"speaker":"A","text":"ピアスやネックレスなどのアクセサリーを買うつもりです。"},{"id":{"d":123,"n":8,"t":5},"speaker":"B","text":"いいですね。気を付けて行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":124,"n":1,"t":5},"speaker":"A","text":"ここの島は、台風がよく上陸するという話を私は聞きました。"},{"id":{"d":124,"n":2,"t":5},"speaker":"B","text":"はい。年間で10回はこの島に台風が上陸しているのではないかと、私は思います。"},{"id":{"d":124,"n":3,"t":5},"speaker":"A","text":"それほどたくさんの台風が来るのですね。台風の時に大事なことは何ですか？"},{"id":{"d":124,"n":4,"t":5},"speaker":"B","text":"事前準備です。"},{"id":{"d":124,"n":5,"t":5},"speaker":"A","text":"どのような準備をしているのですか？"},{"id":{"d":124,"n":6,"t":5},"speaker":"B","text":"水や食料です。それに窓ガラスが割れないように、養生テープを貼ったりします。"},{"id":{"d":124,"n":7,"t":5},"speaker":"A","text":"そうなのですね。準備はとても大変そうですが、身を守るためには重要なことですね。"}];
INSERT INTO sentence [{"id":{"d":125,"n":1,"t":5},"speaker":"A","text":"今夜は、流れ星が多く見えるようです。"},{"id":{"d":125,"n":2,"t":5},"speaker":"B","text":"何時ごろに見えるのですか？ぜひ、私は流れ星を見たいです。"},{"id":{"d":125,"n":3,"t":5},"speaker":"A","text":"夜8時ごろからのようです。ふたご座流星群と呼ばれる流れ星のようです。"},{"id":{"d":125,"n":4,"t":5},"speaker":"B","text":"そうなのですね。方角はどちらですか？"},{"id":{"d":125,"n":5,"t":5},"speaker":"A","text":"特に決まった方角はなく、空の全体的に見えるようです。"},{"id":{"d":125,"n":6,"t":5},"speaker":"B","text":"楽しみですね。願い事をたくさんしようと思います。"},{"id":{"d":125,"n":7,"t":5},"speaker":"A","text":"願い事がかなうと良いですね。"}];
INSERT INTO sentence [{"id":{"d":126,"n":1,"t":5},"speaker":"A","text":"地球は、本当に青色をしていました。"},{"id":{"d":126,"n":2,"t":5},"speaker":"B","text":"あなたは、どこから地球を見たのですか？"},{"id":{"d":126,"n":3,"t":5},"speaker":"A","text":"宇宙へ行ったときに、スペースシャトルから見ました。"},{"id":{"d":126,"n":4,"t":5},"speaker":"B","text":"うらやましいです。"},{"id":{"d":126,"n":5,"t":5},"speaker":"A","text":"あなたも宇宙へ行けると良いですね。僕はきっと近い未来、誰でも簡単に宇宙に行けるようになると信じています。"},{"id":{"d":126,"n":6,"t":5},"speaker":"B","text":"そうなったら、私も宇宙へ行きたいと思います。"}];
INSERT INTO sentence [{"id":{"d":127,"n":1,"t":5},"speaker":"A","text":"朝日がまぶしいです。"},{"id":{"d":127,"n":2,"t":5},"speaker":"B","text":"今日は、朝から良い天気ですね。掃除でもしましょうか？"},{"id":{"d":127,"n":3,"t":5},"speaker":"A","text":"そうですね。どこから掃除をしましょうか？"},{"id":{"d":127,"n":4,"t":5},"speaker":"B","text":"私はリビングを担当します。あなたはキッチンをお願いします。"},{"id":{"d":127,"n":5,"t":5},"speaker":"A","text":"分かりました。お昼までには終わらせて、午後は公園にでも出かけましょう。"},{"id":{"d":127,"n":6,"t":5},"speaker":"B","text":"良いですね。それでは早く取り掛かりましょう。"}];
INSERT INTO sentence [{"id":{"d":128,"n":1,"t":5},"speaker":"A","text":"わが家は西日が差して日当たりが良いので、冬でもとても暖かいです。"},{"id":{"d":128,"n":2,"t":5},"speaker":"B","text":"良いですね。私の家は日当たりが悪いので、困っています。"},{"id":{"d":128,"n":3,"t":5},"speaker":"A","text":"引っ越しはしないのですか？"},{"id":{"d":128,"n":4,"t":5},"speaker":"B","text":"引っ越すとなるとお金も要りますし、労力もかかるのでまだ検討中です。いつか引っ越しできたら良いのですが。"},{"id":{"d":128,"n":5,"t":5},"speaker":"A","text":"そうですね。でも、部屋に日差しが全く入らないというのも寒いでしょうし、洗濯物も乾きにくいのではありませんか？"},{"id":{"d":128,"n":6,"t":5},"speaker":"B","text":"はい。どのようにかしてエアーコンディショナーで過ごしているのですが、そろそろ引っ越しについて、少し前向きに考えてみます。"},{"id":{"d":128,"n":7,"t":5},"speaker":"A","text":"良い物件があったら紹介しますね。"},{"id":{"d":128,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":129,"n":1,"t":5},"speaker":"A","text":"問題です。「きらきらひかる」で始まる歌は何という歌でしょうか？"},{"id":{"d":129,"n":2,"t":5},"speaker":"B","text":"それは『きらきら星』です。"},{"id":{"d":129,"n":3,"t":5},"speaker":"A","text":"正解です。では、この続きは分かりますか？"},{"id":{"d":129,"n":4,"t":5},"speaker":"B","text":"「おそらのほしよ」です。"},{"id":{"d":129,"n":5,"t":5},"speaker":"A","text":"よく覚えていましたね。正解です。"},{"id":{"d":129,"n":6,"t":5},"speaker":"B","text":"童謡というのは、自然と覚えているものですね。"},{"id":{"d":129,"n":7,"t":5},"speaker":"A","text":"それだけ、幼少期にたくさん歌ったということでしょうね。"}];
INSERT INTO sentence [{"id":{"d":130,"n":1,"t":5},"speaker":"A","text":"当店は、本日年末感謝セールを行っております。"},{"id":{"d":130,"n":2,"t":5},"speaker":"B","text":"セールは今日だけですか？"},{"id":{"d":130,"n":3,"t":5},"speaker":"A","text":"はい、そうです。ぜひご覧になってください。"},{"id":{"d":130,"n":4,"t":5},"speaker":"B","text":"普段より安いのですか？"},{"id":{"d":130,"n":5,"t":5},"speaker":"A","text":"はい。ほとんどの商品が、普段と比べると3割以上安くなっています。"},{"id":{"d":130,"n":6,"t":5},"speaker":"B","text":"目玉商品を教えてください。"},{"id":{"d":130,"n":7,"t":5},"speaker":"A","text":"こちらの羽毛布団です。6割引で、通常価格29,800円の物が10,000円とお買い求めやすくなっています。"},{"id":{"d":130,"n":8,"t":5},"speaker":"B","text":"それは安いですね。購入します。"},{"id":{"d":130,"n":9,"t":5},"speaker":"A","text":"ありがとうございます。他にもお安くなっている商品がたくさんあるので、ぜひご覧ください。"}];
INSERT INTO sentence [{"id":{"d":131,"n":1,"t":5},"speaker":"A","text":"土曜日も、私は仕事をしなくてはいけません。"},{"id":{"d":131,"n":2,"t":5},"speaker":"B","text":"大変ですね。どうしてですか？"},{"id":{"d":131,"n":3,"t":5},"speaker":"A","text":"仕事が終わらないからです。しかし、初めて任された仕事なのでどうしても完遂させたいのです。"},{"id":{"d":131,"n":4,"t":5},"speaker":"B","text":"そうなのですね。日曜日は休めそうですか？"},{"id":{"d":131,"n":5,"t":5},"speaker":"A","text":"土曜日に目いっぱい頑張れば終わるので、日曜は休めると思います。"},{"id":{"d":131,"n":6,"t":5},"speaker":"B","text":"頑張るのは素晴らしいことですが、あまり無理をし過ぎないようにしてください。"},{"id":{"d":131,"n":7,"t":5},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":132,"n":1,"t":5},"speaker":"A","text":"いつもお世話になっている取引先に、お歳暮を持っていきましょう。"},{"id":{"d":132,"n":2,"t":5},"speaker":"B","text":"そうですね。何を持っていきましょうか？"},{"id":{"d":132,"n":3,"t":5},"speaker":"A","text":"皆さんが食べやすいように、小分けになっているお菓子はどうでしょうか？"},{"id":{"d":132,"n":4,"t":5},"speaker":"B","text":"良いですね。どこで買いましょうか？"},{"id":{"d":132,"n":5,"t":5},"speaker":"A","text":"それでは、帰りにデパートに寄って買って帰りましょう。"},{"id":{"d":132,"n":6,"t":5},"speaker":"B","text":"分かりました。では17時にデパートの前で待ち合わせをしましょう。"}];
INSERT INTO sentence [{"id":{"d":133,"n":1,"t":5},"speaker":"A","text":"キャッチボールをしませんか？"},{"id":{"d":133,"n":2,"t":5},"speaker":"B","text":"良いですね。私は昔、野球をしていたのでキャッチボールは得意ですよ。"},{"id":{"d":133,"n":3,"t":5},"speaker":"A","text":"そうでしたか。それは知りませんでした。"},{"id":{"d":133,"n":4,"t":5},"speaker":"B","text":"学生時代の話ですからね。"},{"id":{"d":133,"n":5,"t":5},"speaker":"A","text":"ポジションはどちらでしたか？"},{"id":{"d":133,"n":6,"t":5},"speaker":"B","text":"ピッチャーでした。1日2試合を1人で投げた時もありました。"},{"id":{"d":133,"n":7,"t":5},"speaker":"A","text":"1人で2試合ですか？それは驚くべきことですね。"},{"id":{"d":133,"n":8,"t":5},"speaker":"B","text":"今では良い思い出です。"}];
INSERT INTO sentence [{"id":{"d":134,"n":1,"t":5},"speaker":"A","text":"今日は「いい夫婦の日」です。あなたは、ご存じでしたか？"},{"id":{"d":134,"n":2,"t":5},"speaker":"B","text":"知りませんでした。なぜ今日は、「いい夫婦の日」なのですか？"},{"id":{"d":134,"n":3,"t":5},"speaker":"A","text":"11月22日だからです。語呂合わせで、11が「いい」、22が「夫婦」と読めることからだそうです。"},{"id":{"d":134,"n":4,"t":5},"speaker":"B","text":"なるほど、そうなのですね。"},{"id":{"d":134,"n":5,"t":5},"speaker":"A","text":"この日に、入籍したり結婚式を挙げたりするカップルも多いと聞きました。"}];
INSERT INTO sentence [{"id":{"d":135,"n":1,"t":5},"speaker":"A","text":"今日は冬至なので、カボチャを食べましょう。"},{"id":{"d":135,"n":2,"t":5},"speaker":"B","text":"どうして冬至にはカボチャを食べるのですか？"},{"id":{"d":135,"n":3,"t":5},"speaker":"A","text":"カボチャに含まれる栄養素をたくさん取って、寒くても風邪をひかないようにするということが由来と聞いたことがあります。"},{"id":{"d":135,"n":4,"t":5},"speaker":"B","text":"なるほど。冬至は、毎年12月22日なのですか？"},{"id":{"d":135,"n":5,"t":5},"speaker":"A","text":"2021年は12月22日です。21日になる年もあります。冬至は1年で最も日が短く、夜が長い日です。"},{"id":{"d":135,"n":6,"t":5},"speaker":"B","text":"それは知りませんでした。教えてくださりありがとうございます。それでは、外が暗くならないうちに早く家に帰りましょう。"}];
INSERT INTO sentence [{"id":{"d":136,"n":1,"t":5},"speaker":"A","text":"今日は「土用の丑の日」です。ウナギを食べてスタミナをつけましょう。"},{"id":{"d":136,"n":2,"t":5},"speaker":"B","text":"そうですね。今日は、ウナギを買って帰りましょう。"},{"id":{"d":136,"n":3,"t":5},"speaker":"A","text":"3人分だと、どのくらいの値段になりますか？"},{"id":{"d":136,"n":4,"t":5},"speaker":"B","text":"5,000円くらいでしょうか？"},{"id":{"d":136,"n":5,"t":5},"speaker":"A","text":"結構高いのですね。"},{"id":{"d":136,"n":6,"t":5},"speaker":"B","text":"ウナギは高級食材ですからね。でも、元気をつけるために、今日は奮発しましょう。"}];
INSERT INTO sentence [{"id":{"d":137,"n":1,"t":5},"speaker":"A","text":"もう年の瀬とは、時が流れるのは早いものですね。"},{"id":{"d":137,"n":2,"t":5},"speaker":"B","text":"本当にその通りです。月日が過ぎ去るのは早くて、驚いてしまいます。"},{"id":{"d":137,"n":3,"t":5},"speaker":"A","text":"そうですよね。ところで、今年の年越しはどのように過ごされるご予定ですか？"},{"id":{"d":137,"n":4,"t":5},"speaker":"B","text":"今年の年越しも、例年通り「NHK紅白歌合戦」を見ながら、年越しそばを食べたいと思っています。"},{"id":{"d":137,"n":5,"t":5},"speaker":"A","text":"それは楽しみですね。ご家族と過ごされるご予定ですか？"},{"id":{"d":137,"n":6,"t":5},"speaker":"B","text":"はい、夫の実家に子どもとともに、帰省する予定です。"},{"id":{"d":137,"n":7,"t":5},"speaker":"A","text":"それは、すてきですね。良い時間をお過ごしください。"},{"id":{"d":137,"n":8,"t":5},"speaker":"B","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":138,"n":1,"t":5},"speaker":"A","text":"こどもの日は、家族で少し遠出をしましょうか。"},{"id":{"d":138,"n":2,"t":5},"speaker":"B","text":"どこへ行くのですか？"},{"id":{"d":138,"n":3,"t":5},"speaker":"A","text":"こどもの日なので、子どもたちの行きたいところにしましょう。あなたは、どこか行きたいところはありますか？"},{"id":{"d":138,"n":4,"t":5},"speaker":"B","text":"私は、海に行きたいです。"},{"id":{"d":138,"n":5,"t":5},"speaker":"A","text":"分かりました。まだ、5月なので寒くて泳げないかもしれませんが、水遊びくらいはできるかもしれませんね。"},{"id":{"d":138,"n":6,"t":5},"speaker":"B","text":"うれしいです。5月5日が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":139,"n":1,"t":5},"speaker":"A","text":"公園で、キャッチボールをしませんか？"},{"id":{"d":139,"n":2,"t":5},"speaker":"B","text":"いいですね。でも、私はグローブを持っていません。あなたは、グローブを2つ持っていますか？"},{"id":{"d":139,"n":3,"t":5},"speaker":"A","text":"グローブはあるのですが、ボールがありません。"},{"id":{"d":139,"n":4,"t":5},"speaker":"B","text":"それでは、ボールは私が持って行きます。どこの公園に行きますか？"},{"id":{"d":139,"n":5,"t":5},"speaker":"A","text":"3丁目の公園に行きましょう。3丁目の公園は、敷地が広いので思いっ切りボールを投げることができます。"},{"id":{"d":139,"n":6,"t":5},"speaker":"B","text":"分かりました。それでは、家に帰ってからボールを持って、3丁目の公園に行きますね。"},{"id":{"d":139,"n":7,"t":5},"speaker":"A","text":"はい。それでは、後ほど会いましょう。"}];
INSERT INTO sentence [{"id":{"d":140,"n":1,"t":5},"speaker":"A","text":"三が日は、何をして過ごしましょうか？"},{"id":{"d":140,"n":2,"t":5},"speaker":"B","text":"私は、実家でおせちやお雑煮を食べてゆっくりしようと思います。"},{"id":{"d":140,"n":3,"t":5},"speaker":"A","text":"せっかくなので、お正月ならではの遊びをしませんか？"},{"id":{"d":140,"n":4,"t":5},"speaker":"B","text":"例えば、どのような遊びがありますか？"},{"id":{"d":140,"n":5,"t":5},"speaker":"A","text":"たこ揚げや羽子板、それに福笑いなどがあります。"},{"id":{"d":140,"n":6,"t":5},"speaker":"B","text":"それでは、隣の公園でたこ揚げをしましょう。"},{"id":{"d":140,"n":7,"t":5},"speaker":"A","text":"良いですね。たこを準備しておきます。"}];
INSERT INTO sentence [{"id":{"d":141,"n":1,"t":5},"speaker":"A","text":"新年を迎えるにあたり、大掃除をしたいと思います。"},{"id":{"d":141,"n":2,"t":5},"speaker":"B","text":"そうですね。それでは、分担して進めましょう。"},{"id":{"d":141,"n":3,"t":5},"speaker":"A","text":"いいですね。それでは、私はキッチンを担当したいと思います。あなたは、リビングの掃除をしていただけますか？"},{"id":{"d":141,"n":4,"t":5},"speaker":"B","text":"分かりました。掃除用具はありますか？"},{"id":{"d":141,"n":5,"t":5},"speaker":"A","text":"家には、掃除機と、ほうき、ちり取りくらいしかありません。"},{"id":{"d":141,"n":6,"t":5},"speaker":"B","text":"それでは、洗剤などを買いに行きましょう。"},{"id":{"d":141,"n":7,"t":5},"speaker":"A","text":"そうですね。まずは、掃除用具をそろえましょう。"}];
INSERT INTO sentence [{"id":{"d":142,"n":1,"t":5},"speaker":"A","text":"ひな祭りのお祝いをしましょう。"},{"id":{"d":142,"n":2,"t":5},"speaker":"B","text":"お赤飯と、ハマグリのお吸い物が良いですね。"},{"id":{"d":142,"n":3,"t":5},"speaker":"A","text":"ひなあられや、ひし餅もありますよ。"},{"id":{"d":142,"n":4,"t":5},"speaker":"B","text":"ひな祭りは華やかで良いですね。"},{"id":{"d":142,"n":5,"t":5},"speaker":"A","text":"ごちそうを作って盛大にお祝いをしましょう。腕が鳴ります。"}];
INSERT INTO sentence [{"id":{"d":143,"n":1,"t":5},"speaker":"A","text":"けん玉は、できますか？"},{"id":{"d":143,"n":2,"t":5},"speaker":"B","text":"はい、できます。私は、けん玉検定の3段を持っています。"},{"id":{"d":143,"n":3,"t":5},"speaker":"A","text":"素晴らしいですね。何歳ごろからけん玉をしているのですか？"},{"id":{"d":143,"n":4,"t":5},"speaker":"B","text":"5歳の時に、父にけん玉を買ってもらって以来、練習をしています。"},{"id":{"d":143,"n":5,"t":5},"speaker":"A","text":"ぜひ、けん玉の技を見せてください。"},{"id":{"d":143,"n":6,"t":5},"speaker":"B","text":"よいですよ。ただ、ここでは狭いので外に行きましょう。"},{"id":{"d":143,"n":7,"t":5},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":144,"n":1,"t":5},"speaker":"A","text":"イラストを描くのが上手ですね。"},{"id":{"d":144,"n":2,"t":5},"speaker":"B","text":"元々、イラストを描くことが大好きだったのですが、去年趣味が高じてイラストレーターの資格を取りました。"},{"id":{"d":144,"n":3,"t":5},"speaker":"A","text":"趣味とはいえ資格まで取ったとは素晴らしいですね。"},{"id":{"d":144,"n":4,"t":5},"speaker":"B","text":"イラストは、学べば学ぶほど奥深いのです。"},{"id":{"d":144,"n":5,"t":5},"speaker":"A","text":"なるほど。道理で格段に上手だと思いました。"},{"id":{"d":144,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。何かイラストにしたいものがあったら、教えてください。"},{"id":{"d":144,"n":7,"t":5},"speaker":"A","text":"考えておきます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":145,"n":1,"t":5},"speaker":"A","text":"誕生日プレゼントには、何が欲しいですか？"},{"id":{"d":145,"n":2,"t":5},"speaker":"B","text":"私は、新しいスマートフォンが欲しいです。"},{"id":{"d":145,"n":3,"t":5},"speaker":"A","text":"どの機種にしますか？今はいろいろな機能が付いているものがたくさんあります。"},{"id":{"d":145,"n":4,"t":5},"speaker":"B","text":"そうですね。以前からiPhoneを使っていたのでiPhoneの最新機種が欲しいです。"},{"id":{"d":145,"n":5,"t":5},"speaker":"A","text":"分かりました。では、近いうちに電気店にiPhoneを見に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":146,"n":1,"t":5},"speaker":"A","text":"初詣には、いつ行きますか？"},{"id":{"d":146,"n":2,"t":5},"speaker":"B","text":"朝から、新年のあいさつをして、初詣に行こうと思っています。"},{"id":{"d":146,"n":3,"t":5},"speaker":"A","text":"なるほど。でも近くの霧島神宮は、先日国宝に指定されたので、初詣の人出が多いと思います。"},{"id":{"d":146,"n":4,"t":5},"speaker":"B","text":"おっしゃる通りですね。では、1日はやめて2日に行きましょうか？"},{"id":{"d":146,"n":5,"t":5},"speaker":"A","text":"そうですね。3日までは正月休みなので余裕もありますし、2日にしましょう。"}];
INSERT INTO sentence [{"id":{"d":147,"n":1,"t":5},"speaker":"A","text":"私のパートナーは、ギターを弾くのがとても上手です。"},{"id":{"d":147,"n":2,"t":5},"speaker":"B","text":"それは、すてきです。いつも、どのような曲を弾いてもらっているのですか？"},{"id":{"d":147,"n":3,"t":5},"speaker":"A","text":"はい。よくザ・ビートルズの曲を弾いてもらっています。"},{"id":{"d":147,"n":4,"t":5},"speaker":"B","text":"私もザ・ビートルズの曲は、大好きです。特に『レット・イット・ビー』が好きです。"},{"id":{"d":147,"n":5,"t":5},"speaker":"A","text":"それは奇遇です。実は、私もその曲が大好きで、よくパートナーに弾いてもらいます。"},{"id":{"d":147,"n":6,"t":5},"speaker":"B","text":"差し支えなければ、今度演奏を聴かせていただけませんか？"},{"id":{"d":147,"n":7,"t":5},"speaker":"A","text":"もちろんです。"},{"id":{"d":147,"n":8,"t":5},"speaker":"B","text":"ありがとうございます。楽しみにしております。"}];
INSERT INTO sentence [{"id":{"d":148,"n":1,"t":5},"speaker":"A","text":"お誕生日プレゼントに、欲しいものはありますか？"},{"id":{"d":148,"n":2,"t":5},"speaker":"B","text":"ありがとうございます。ちょうど、仕事用のメモ帳が欲しいと思っていました。"},{"id":{"d":148,"n":3,"t":5},"speaker":"A","text":"分かりました。そうしましたら、今度、見立てておきます。"},{"id":{"d":148,"n":4,"t":5},"speaker":"B","text":"催促をしたようで恐縮です。"},{"id":{"d":148,"n":5,"t":5},"speaker":"A","text":"実は、代官山に大好きな文房具屋があるので、そちらで購入しようと思っています。海外の文房具のセレクトショップなので、品数も多いのです。気に入っていただけるものが見つかるといいのですが。"},{"id":{"d":148,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":149,"n":1,"t":5},"speaker":"A","text":"カラオケに行かれることはありますか？"},{"id":{"d":149,"n":2,"t":5},"speaker":"B","text":"あまり行きません。"},{"id":{"d":149,"n":3,"t":5},"speaker":"A","text":"それは、意外です。以前、「歌うのが好き」とおっしゃっていたように記憶していました。でも今度、ご一緒にいかがですか？"},{"id":{"d":149,"n":4,"t":5},"speaker":"B","text":"歌うことは好きですが、人前で歌うことはあまり好きではありません。でも、せっかくのお誘いなので考えておきます。"},{"id":{"d":149,"n":5,"t":5},"speaker":"A","text":"気が向いたら、ぜひ行きましょう！私は定期的にカラオケに行くくらい歌うことが大好きなのです。"},{"id":{"d":149,"n":6,"t":5},"speaker":"B","text":"お心遣いをいただきありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":150,"n":1,"t":5},"speaker":"A","text":"母の日は、プレゼントをあげますか？"},{"id":{"d":150,"n":2,"t":5},"speaker":"B","text":"すみません、母の日はいつですか？"},{"id":{"d":150,"n":3,"t":5},"speaker":"A","text":"5月の第2日曜日です。"},{"id":{"d":150,"n":4,"t":5},"speaker":"B","text":"そうなのですね。まだ何も考えていなかったので、これからプレゼントを考えます。あなたは何かプレゼントを送るのですか？"},{"id":{"d":150,"n":5,"t":5},"speaker":"A","text":"僕は、母の似顔絵を書いて送ろうかと思います。"},{"id":{"d":150,"n":6,"t":5},"speaker":"B","text":"良いですね。似顔絵は、とても喜ばれると思います。"},{"id":{"d":150,"n":7,"t":5},"speaker":"A","text":"母が喜んでくれたら、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":151,"n":1,"t":5},"speaker":"A","text":"クリスマスは、何が食べたいですか？"},{"id":{"d":151,"n":2,"t":5},"speaker":"B","text":"私は、ケーキが食べたいです。"},{"id":{"d":151,"n":3,"t":5},"speaker":"A","text":"どのようなケーキが良いですか？"},{"id":{"d":151,"n":4,"t":5},"speaker":"B","text":"イチゴのショートケーキが良いです。"},{"id":{"d":151,"n":5,"t":5},"speaker":"A","text":"それでは2人で選びに行って買いませんか？"},{"id":{"d":151,"n":6,"t":5},"speaker":"B","text":"そうですね。加えて、シャンパンも買いたいです。"},{"id":{"d":151,"n":7,"t":5},"speaker":"A","text":"とても楽しみです。"}];
INSERT INTO sentence [{"id":{"d":152,"n":1,"t":5},"speaker":"A","text":"最近の海外ドラマが、とても面白いので、よく見ています。最近は、韓国ドラマの面白さに目覚めました。"},{"id":{"d":152,"n":2,"t":5},"speaker":"B","text":"私の周りにも、韓国ドラマに熱中している人が非常に多いです。"},{"id":{"d":152,"n":3,"t":5},"speaker":"A","text":"そうなのですね。特に面白いのは、『愛の不時着』です。主人公の俳優さんが本当にすてきで、いつも見とれてしまいます。"},{"id":{"d":152,"n":4,"t":5},"speaker":"B","text":"作品名を聞いたことがあります。面白いのですね。今度見てみようと思います。"},{"id":{"d":152,"n":5,"t":5},"speaker":"A","text":"はい、ぜひご覧になってください。間違いなく、お気に召すと思います。"}];
INSERT INTO sentence [{"id":{"d":153,"n":1,"t":5},"speaker":"A","text":"旬の食べ物は栄養価も高く、おいしいです。"},{"id":{"d":153,"n":2,"t":5},"speaker":"B","text":"春の食べ物といえば何でしょうか？"},{"id":{"d":153,"n":3,"t":5},"speaker":"A","text":"タケノコは、春の食べ物ですね。"},{"id":{"d":153,"n":4,"t":5},"speaker":"B","text":"そうですね。タケノコご飯や煮物にしたら、おいしいです。"},{"id":{"d":153,"n":5,"t":5},"speaker":"A","text":"歯応えが良いですよね。他に何か思いつく春の食べ物はありますか？"},{"id":{"d":153,"n":6,"t":5},"speaker":"B","text":"春の食べ物といえば、私は山菜を思いつきます。"},{"id":{"d":153,"n":7,"t":5},"speaker":"A","text":"春の山の幸はとてもおいしいですよね。"}];
INSERT INTO sentence [{"id":{"d":154,"n":1,"t":5},"speaker":"A","text":"年の瀬は何かと忙しいです。"},{"id":{"d":154,"n":2,"t":5},"speaker":"B","text":"そうですね。大掃除やおせち作りなどなど、いろいろとしなくてはいけない事があります。"},{"id":{"d":154,"n":3,"t":5},"speaker":"A","text":"ええ。クリスマスやお正月などイベントも多いので、いろいろ準備もしなくてはなりません。"},{"id":{"d":154,"n":4,"t":5},"speaker":"B","text":"お金もかかりますしね。"},{"id":{"d":154,"n":5,"t":5},"speaker":"A","text":"はい。いくらあっても足りません。宝くじが当たるといいのですけれどね。"},{"id":{"d":154,"n":6,"t":5},"speaker":"B","text":"宝くじを買ったのですか？"},{"id":{"d":154,"n":7,"t":5},"speaker":"A","text":"はい。今年こそは当たるようにと願いを込めて買いました。"},{"id":{"d":154,"n":8,"t":5},"speaker":"B","text":"当たると良いですね。"}];
INSERT INTO sentence [{"id":{"d":155,"n":1,"t":5},"speaker":"A","text":"梅雨明けが、待ち遠しいです。"},{"id":{"d":155,"n":2,"t":5},"speaker":"B","text":"梅雨が明けると真夏になってしまいます。私は暑いのは苦手です。"},{"id":{"d":155,"n":3,"t":5},"speaker":"A","text":"私は、夏が好きです。日差しが気持ち良いですし、海やプールも好きだからです。"},{"id":{"d":155,"n":4,"t":5},"speaker":"B","text":"泳ぐのは気持ち良さそうですね。その話を聞いたら少しだけ海に行きたくなりました。"},{"id":{"d":155,"n":5,"t":5},"speaker":"A","text":"今年の夏は、ぜひ一緒に海へ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":156,"n":1,"t":5},"speaker":"A","text":"秋晴れで、行楽日和です。"},{"id":{"d":156,"n":2,"t":5},"speaker":"B","text":"良い天気ですね。午後から、どこかに行きませんか？"},{"id":{"d":156,"n":3,"t":5},"speaker":"A","text":"そうですね。紅葉でも見に、山へドライブに行きましょうか？"},{"id":{"d":156,"n":4,"t":5},"speaker":"B","text":"良いですね、行きましょう。自動車を運転してもらえますか？"},{"id":{"d":156,"n":5,"t":5},"speaker":"A","text":"分かりました。仕事が片付いたらまた連絡しますね。"},{"id":{"d":156,"n":6,"t":5},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":157,"n":1,"t":5},"speaker":"A","text":"あなたは、何か楽器を弾けますか？"},{"id":{"d":157,"n":2,"t":5},"speaker":"B","text":"ギターなら少し弾けます。"},{"id":{"d":157,"n":3,"t":5},"speaker":"A","text":"素晴らしいですね。何年くらい弾いているのですか？"},{"id":{"d":157,"n":4,"t":5},"speaker":"B","text":"中学校へ入った時に始めたので、3年くらいです。"},{"id":{"d":157,"n":5,"t":5},"speaker":"A","text":"私は小さい頃からドラムの練習をしていました。ボーカルがいればバンドを組めますね。"},{"id":{"d":157,"n":6,"t":5},"speaker":"B","text":"それは楽しそうですね。ぜひ、ボーカルを探してバンド活動をしましょう。"}];
INSERT INTO sentence [{"id":{"d":158,"n":1,"t":5},"speaker":"A","text":"節句とは何のことですか？"},{"id":{"d":158,"n":2,"t":5},"speaker":"B","text":"私も詳しくは分かりませんが、「こどもの日」を端午の節句、「ひな祭り」を桃の節句といいます。"},{"id":{"d":158,"n":3,"t":5},"speaker":"A","text":"そうなのですね。節目のような意味でしょうか？"},{"id":{"d":158,"n":4,"t":5},"speaker":"B","text":"そうですね。恐らく古語からの言葉だと思います。"},{"id":{"d":158,"n":5,"t":5},"speaker":"A","text":"なるほど。昔の言葉は難しいですね。"}];
INSERT INTO sentence [{"id":{"d":159,"n":1,"t":5},"speaker":"A","text":"肝試しをしませんか？"},{"id":{"d":159,"n":2,"t":5},"speaker":"B","text":"嫌です。私は怖いのが得意ではありません。"},{"id":{"d":159,"n":3,"t":5},"speaker":"A","text":"でも、その怖さを楽しむのが肝試しの醍醐味です。1人でなければさほど怖くないと思いますよ。"},{"id":{"d":159,"n":4,"t":5},"speaker":"B","text":"そこまであなたがおっしゃるなら、一緒に行ってくれるのでしたら、肝試しをしてもよいですよ。"},{"id":{"d":159,"n":5,"t":5},"speaker":"A","text":"それでは、今夜行きましょう。怖そうな場所を探しておきます。"}];
INSERT INTO sentence [{"id":{"d":160,"n":1,"t":5},"speaker":"A","text":"このお芝居の上演は、もう終わってしまいましたか？"},{"id":{"d":160,"n":2,"t":5},"speaker":"B","text":"はい、この劇場では昨日までの上演でした。"},{"id":{"d":160,"n":3,"t":5},"speaker":"A","text":"それは残念です。どこか別の所で上演していますか？"},{"id":{"d":160,"n":4,"t":5},"speaker":"B","text":"調べてみますので、少し待ってください。"},{"id":{"d":160,"n":5,"t":5},"speaker":"A","text":"はい、お願いします。"},{"id":{"d":160,"n":6,"t":5},"speaker":"B","text":"隣町の市営ホールで上演しているようです。"},{"id":{"d":160,"n":7,"t":5},"speaker":"A","text":"ありがとうございます。そちらに問い合わせてみます。"}];
INSERT INTO sentence [{"id":{"d":161,"n":1,"t":5},"speaker":"A","text":"今度の日曜日は、どこに行きますか？"},{"id":{"d":161,"n":2,"t":5},"speaker":"B","text":"磯海水浴場に行きましょう。"},{"id":{"d":161,"n":3,"t":5},"speaker":"A","text":"そこも景色が良いのですが、今回は吹上浜海岸に行きませんか？"},{"id":{"d":161,"n":4,"t":5},"speaker":"B","text":"良いですね。きれいな浜ですし、海水も澄んでいます。"},{"id":{"d":161,"n":5,"t":5},"speaker":"A","text":"海へ行くなら、その前に水着を買わなくてはいけません。"},{"id":{"d":161,"n":6,"t":5},"speaker":"B","text":"私も買いたいです。それでは、一緒にイオンへ行って水着を買いましょう。"},{"id":{"d":161,"n":7,"t":5},"speaker":"A","text":"はい、一緒に水着を見に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":162,"n":1,"t":5},"speaker":"A","text":"クリスマスイブに、一緒にどこかへ行きませんか？"},{"id":{"d":162,"n":2,"t":5},"speaker":"B","text":"私は夕方まで仕事があります。でも、仕事が終わった後なら、あなたと会えます。"},{"id":{"d":162,"n":3,"t":5},"speaker":"A","text":"分かりました。では、仕事が終わったら私に連絡をください。"},{"id":{"d":162,"n":4,"t":5},"speaker":"B","text":"はい。では、どこへ行くか決めておいてもらえませんか？"},{"id":{"d":162,"n":5,"t":5},"speaker":"A","text":"せっかくなので、2人で決めたいです。"},{"id":{"d":162,"n":6,"t":5},"speaker":"B","text":"そうですね。では、会った時に決めましょう。"}];
INSERT INTO sentence [{"id":{"d":163,"n":1,"t":5},"speaker":"A","text":"小さい頃から、本を読むのが大好きです。特に好きなのは、小説を読むことです。"},{"id":{"d":163,"n":2,"t":5},"speaker":"B","text":"それは、良い趣味ですね。どの作家の作品が、一番お好きですか？"},{"id":{"d":163,"n":3,"t":5},"speaker":"A","text":"川端康成の作品が一番好きです。『雪国』の冒頭のくだりが特に好きです。"},{"id":{"d":163,"n":4,"t":5},"speaker":"B","text":"あの有名な一節ですね。私も大好きです。川端康成の作品は、美しい日本文化と日本語を堪能できるので、読むたびに、心を奪われてしまいます。"},{"id":{"d":163,"n":5,"t":5},"speaker":"A","text":"私も同感です。あのような作品を、原文で読むことができて幸せですね。"}];
INSERT INTO sentence [{"id":{"d":164,"n":1,"t":5},"speaker":"A","text":"私は、雨があまり好きではありません。"},{"id":{"d":164,"n":2,"t":5},"speaker":"B","text":"私も同じです。この後、雨は上がるのでしょうか？"},{"id":{"d":164,"n":3,"t":5},"speaker":"A","text":"天気予報によると、午後からは晴れるようです。"},{"id":{"d":164,"n":4,"t":5},"speaker":"B","text":"それは良かったです。今日は傘を持って来ていませんので、助かります。"},{"id":{"d":164,"n":5,"t":5},"speaker":"A","text":"そうなのですね。ただ雨上がりには、虹が出ることも期待できますので、そう考えれば雨にも良いところはありますよね。"},{"id":{"d":164,"n":6,"t":5},"speaker":"B","text":"早く晴れると良いですね。"}];
INSERT INTO sentence [{"id":{"d":165,"n":1,"t":5},"speaker":"A","text":"七夕飾りを作りましょう。"},{"id":{"d":165,"n":2,"t":5},"speaker":"B","text":"はい。何から準備すれば良いですか？"},{"id":{"d":165,"n":3,"t":5},"speaker":"A","text":"昨日は、裏の山から竹を取ってきましたので、それに飾るための短冊を用意してください。折り紙で作りましょう。"},{"id":{"d":165,"n":4,"t":5},"speaker":"B","text":"分かりました。明日生徒の皆さんには、この短冊に、願い事を書いてもらいましょう。"},{"id":{"d":165,"n":5,"t":5},"speaker":"A","text":"そうですね。どのような願い事があるか楽しみです。"}];
