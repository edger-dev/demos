INSERT INTO sentence [{"id":{"d":194,"n":1,"t":1},"speaker":"A","text":"今日の全校集会の場所は、体育館ですか？それともグラウンドですか？"},{"id":{"d":194,"n":2,"t":1},"speaker":"B","text":"今日は、晴れているのでグラウンドです。"},{"id":{"d":194,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。今日は、校長先生はどのような話をされるのでしょうか？"},{"id":{"d":194,"n":4,"t":1},"speaker":"B","text":"先月の全校集会では、次回は運動会の内容について話をするとおっしゃっていましたよ。"},{"id":{"d":194,"n":5,"t":1},"speaker":"A","text":"すっかり忘れていました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":195,"n":1,"t":1},"speaker":"A","text":"昨日のテレビのドッキリ番組は、面白かったですね。"},{"id":{"d":195,"n":2,"t":1},"speaker":"B","text":"はい。あの大物俳優が、まさかドッキリにかけられるとは、想像できませんでしたね。"},{"id":{"d":195,"n":3,"t":1},"speaker":"A","text":"そうですね。すごい緊張感があり、テレビの前で見ているだけでもひやひやしました。"},{"id":{"d":195,"n":4,"t":1},"speaker":"B","text":"私も同感です。今、思い出してもひやひやします。"},{"id":{"d":195,"n":5,"t":1},"speaker":"A","text":"それだけ、インパクトが強かったですからね。ドッキリと気付いたときの、大物俳優のリアクションも面白かったです。"},{"id":{"d":195,"n":6,"t":1},"speaker":"B","text":"そうですね。彼の新たな一面を見ることができました。"},{"id":{"d":195,"n":7,"t":1},"speaker":"A","text":"はい。また、来週の番組も楽しみです。"}];
INSERT INTO sentence [{"id":{"d":196,"n":1,"t":1},"speaker":"A","text":"グループレッスンを始める前に、来月のエレクトーン発表会のタイムスケジュールを連絡します。"},{"id":{"d":196,"n":2,"t":1},"speaker":"B","text":"はい、お願いします。"},{"id":{"d":196,"n":3,"t":1},"speaker":"A","text":"日程は12月5日日曜日、午後2時開場、午後2時30分開演になります。"},{"id":{"d":196,"n":4,"t":1},"speaker":"B","text":"鑑賞時の座席は決まっていますか？"},{"id":{"d":196,"n":5,"t":1},"speaker":"A","text":"はい、決まっています。後日、書面でお渡ししますね。演奏場所も決まっています。美香さんは3番目、芽衣さんは5番目のエレクトーンの前に座って演奏してください。"},{"id":{"d":196,"n":6,"t":1},"speaker":"B","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":197,"n":1,"t":1},"speaker":"A","text":"おばあちゃんは、いつもこの時間になるとテレビを見ていますが、何の番組を見ているのですか？"},{"id":{"d":197,"n":2,"t":1},"speaker":"B","text":"相撲番組を見ています。昔からずっと、お相撲さんが好きなのです。勝負事ですが、勝ち負けだけではないのです。"},{"id":{"d":197,"n":3,"t":1},"speaker":"A","text":"日本のスポーツですね。お相撲さんは、迫力がすごいです。"},{"id":{"d":197,"n":4,"t":1},"speaker":"B","text":"そうですね。一度、相撲を生で見てみたいです。"},{"id":{"d":197,"n":5,"t":1},"speaker":"A","text":"いつか、一緒に見に行きましょう。"},{"id":{"d":197,"n":6,"t":1},"speaker":"B","text":"まあうれしい、楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":198,"n":1,"t":1},"speaker":"A","text":"今年のプロ野球は、面白かったですね。"},{"id":{"d":198,"n":2,"t":1},"speaker":"B","text":"前年最下位チーム同士の日本シリーズというだけで、私の心は熱くなりましたね。"},{"id":{"d":198,"n":3,"t":1},"speaker":"A","text":"日本シリーズは、いつも盛り上がりますね。"},{"id":{"d":198,"n":4,"t":1},"speaker":"B","text":"本当は、阪神タイガースに優勝してもらいたかったのですが、東京ヤクルトスワローズの優勝でも納得です。"},{"id":{"d":198,"n":5,"t":1},"speaker":"A","text":"一方、パ・リーグはオリックス・バファローズが頑張りましたね。"},{"id":{"d":198,"n":6,"t":1},"speaker":"B","text":"一見、地味なチームが優勝すると、無性に私はうれしいです。"},{"id":{"d":198,"n":7,"t":1},"speaker":"A","text":"結局、東京ヤクルトスワローズが日本一になりましたが、オリックス・バファローズには来年こそ、リベンジを果たしてもらいたいですね。"},{"id":{"d":198,"n":8,"t":1},"speaker":"B","text":"私は、新庄剛志BIGBOSSが監督を務める北海道日本ハムファイターズに注目しています。来年も、私はプロ野球から目が離せません。"}];
INSERT INTO sentence [{"id":{"d":199,"n":1,"t":1},"speaker":"A","text":"すみません、娘が年長になったら、ピアノ教室に通わせたいので、詳細を教えていただけますか？"},{"id":{"d":199,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。年長さんからということですと、娘さんは今はまだ5歳ですか？"},{"id":{"d":199,"n":3,"t":1},"speaker":"A","text":"そうです。10月で、娘は6歳になります。"},{"id":{"d":199,"n":4,"t":1},"speaker":"B","text":"6歳ですと、皆さんで楽しむ、グループレッスンをお勧めしています。"},{"id":{"d":199,"n":5,"t":1},"speaker":"A","text":"そうですか。レッスンのスケジュールは決まっていますか？"},{"id":{"d":199,"n":6,"t":1},"speaker":"B","text":"細かい日程は、まだ決まっていませんが、体験レッスンでしたらご案内できます。いかがいたしますか？"},{"id":{"d":199,"n":7,"t":1},"speaker":"A","text":"体験レッスンを受講したいので、日程を教えていただけますか？"},{"id":{"d":199,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。今、日程表をお持ちしますので、少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":200,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。何かお探しですか？"},{"id":{"d":200,"n":2,"t":1},"speaker":"B","text":"薄手の上着が欲しいのですが、お薦めの品はありますか？"},{"id":{"d":200,"n":3,"t":1},"speaker":"A","text":"こちらのカーディンはいかがでしょうか。春らしいデザインで、人気の商品です。"},{"id":{"d":200,"n":4,"t":1},"speaker":"B","text":"すてきなデザインですね。ただ、私は毛糸の肌触りがあまり好きではないので、他の素材の上着がいいです。"},{"id":{"d":200,"n":5,"t":1},"speaker":"A","text":"それでは、このジャケットはどうでしょうか？着心地が良いですよ。"},{"id":{"d":200,"n":6,"t":1},"speaker":"B","text":"試着してみてもよいですか？"},{"id":{"d":200,"n":7,"t":1},"speaker":"A","text":"はい、どうぞ。サイズは、いかがいたしましょうか？"},{"id":{"d":200,"n":8,"t":1},"speaker":"B","text":"私は、いつもMサイズを着ているのですが、上着なので、Lサイズでも良いかもしれないと迷っています。"},{"id":{"d":200,"n":9,"t":1},"speaker":"A","text":"MサイズとLサイズの両方をお試しいただいて構いませんよ。"},{"id":{"d":200,"n":10,"t":1},"speaker":"B","text":"では、そうします。"},{"id":{"d":200,"n":11,"t":1},"speaker":"A","text":"試着室へご案内しますね。こちらへどうぞ。"}];
INSERT INTO sentence [{"id":{"d":201,"n":1,"t":1},"speaker":"A","text":"パパ、うちの車がかなり汚れてきましたね。"},{"id":{"d":201,"n":2,"t":1},"speaker":"B","text":"そうですね。そろそろ洗車した方がいいですね。"},{"id":{"d":201,"n":3,"t":1},"speaker":"A","text":"洗車する時は、僕も手伝いますよ。"},{"id":{"d":201,"n":4,"t":1},"speaker":"B","text":"それは、助かります。どうもありがとう。いつ、洗車をしましょうか？"},{"id":{"d":201,"n":5,"t":1},"speaker":"A","text":"今度の週末なら、僕は予定が空いています。"},{"id":{"d":201,"n":6,"t":1},"speaker":"B","text":"そうですか。それでは土曜日に洗車をする予定にしておいて、もし、天気が悪かったら日曜日に変更することにしましょう。"},{"id":{"d":201,"n":7,"t":1},"speaker":"A","text":"分かりました。そういえば、つくし公園の近くに新しく洗車場ができたらしいですよ。"},{"id":{"d":201,"n":8,"t":1},"speaker":"B","text":"それは知らなかったです。では、そこへ行ってみることにしましょう。"},{"id":{"d":201,"n":9,"t":1},"speaker":"A","text":"後で、僕が週末の天気を確認しておきますね。"},{"id":{"d":201,"n":10,"t":1},"speaker":"B","text":"どうもありがとう。"}];
INSERT INTO sentence [{"id":{"d":202,"n":1,"t":1},"speaker":"A","text":"ただいま帰りました。"},{"id":{"d":202,"n":2,"t":1},"speaker":"B","text":"お帰りなさい。外は寒かったですか？"},{"id":{"d":202,"n":3,"t":1},"speaker":"A","text":"風がとても強く吹いていて、寒く感じました。"},{"id":{"d":202,"n":4,"t":1},"speaker":"B","text":"何か温かいものを飲みますか？"},{"id":{"d":202,"n":5,"t":1},"speaker":"A","text":"ありがたいです。ちょうど、私はココアが飲みたいと思っていました。"},{"id":{"d":202,"n":6,"t":1},"speaker":"B","text":"ごめんなさい。ココアは切らしているのです。"},{"id":{"d":202,"n":7,"t":1},"speaker":"A","text":"そうですか。残念です。"},{"id":{"d":202,"n":8,"t":1},"speaker":"B","text":"コーヒーか紅茶なら、すぐに用意できますよ。"},{"id":{"d":202,"n":9,"t":1},"speaker":"A","text":"では、紅茶をお願いします。"},{"id":{"d":202,"n":10,"t":1},"speaker":"B","text":"お砂糖とミルクは入れますか？"},{"id":{"d":202,"n":11,"t":1},"speaker":"A","text":"お砂糖もミルクもたっぷり入れてください。"},{"id":{"d":202,"n":12,"t":1},"speaker":"B","text":"はい。それでは、紅茶を入れるので、先に手洗いとうがいを済ませてきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":203,"n":1,"t":1},"speaker":"A","text":"佐藤さん、こんにちは。"},{"id":{"d":203,"n":2,"t":1},"speaker":"B","text":"こんにちは、鈴木さん。お久しぶりですね。"},{"id":{"d":203,"n":3,"t":1},"speaker":"A","text":"そうですね。買い物ですか？"},{"id":{"d":203,"n":4,"t":1},"speaker":"B","text":"はい。食器用洗剤がなくなりそうなので、スーパーマーケットへ行くところです。"},{"id":{"d":203,"n":5,"t":1},"speaker":"A","text":"この近くの、四つ葉スーパーですか？もしそうでしたら、今日は火曜日なので定休日ですよ。"},{"id":{"d":203,"n":6,"t":1},"speaker":"B","text":"うっかりしていました。教えていただいて助かりました。"},{"id":{"d":203,"n":7,"t":1},"speaker":"A","text":"以前、私も、火曜日に四つ葉スーパーに行ってしまったことがあります。"},{"id":{"d":203,"n":8,"t":1},"speaker":"B","text":"そうでしたか。定休日はお店ごとに違うから、つい忘れてしまいますよね。"},{"id":{"d":203,"n":9,"t":1},"speaker":"A","text":"食器用洗剤なら、駅前のドラッグストアにも売っているので、行ってみてはどうでしょうか？"},{"id":{"d":203,"n":10,"t":1},"speaker":"B","text":"はい。そうします。"}];
INSERT INTO sentence [{"id":{"d":204,"n":1,"t":1},"speaker":"A","text":"来週の日曜日は、母の日ですね。あなたは、何かプレゼントを用意していますか？"},{"id":{"d":204,"n":2,"t":1},"speaker":"B","text":"ここ数年、ずっとカーネーションを贈っていますから、今年は違うものをあげたいのですが、何を贈ればよいか悩んでいるところです。"},{"id":{"d":204,"n":3,"t":1},"speaker":"A","text":"カーネーションは母の日の定番のプレゼントだから、毎年あげていてもママは喜んでくれると思いますよ。"},{"id":{"d":204,"n":4,"t":1},"speaker":"B","text":"それはそうなのですが、友達に母の日のプレゼントを聞いてみると、他の物をあげている人も多いようなのです。"},{"id":{"d":204,"n":5,"t":1},"speaker":"A","text":"例えば、どのようなものをプレゼントしている人がいましたか？"},{"id":{"d":204,"n":6,"t":1},"speaker":"B","text":"ハンカチやクッキー、あとはアクセサリーをあげたという話も聞きました。"},{"id":{"d":204,"n":7,"t":1},"speaker":"A","text":"どれも良さそうですね。ママは、お菓子が好きですから、クッキーだと特に喜んでくれると思います。"},{"id":{"d":204,"n":8,"t":1},"speaker":"B","text":"それでは、クッキーの詰め合わせを買って、贈り物用のラッピングをしようと思います。"},{"id":{"d":204,"n":9,"t":1},"speaker":"A","text":"今の時期なら、クッキーを販売しているお店の店員さんが、母の日に合った包装をしてくれると思いますよ。"},{"id":{"d":204,"n":10,"t":1},"speaker":"B","text":"そうですね。お店の店員さんに、相談してみます。"}];
INSERT INTO sentence [{"id":{"d":205,"n":1,"t":1},"speaker":"A","text":"横山さんは、確か猫を飼っていますよね？"},{"id":{"d":205,"n":2,"t":1},"speaker":"B","text":"はい、飼っていますが、どうして猫のことを聞いたのですか？"},{"id":{"d":205,"n":3,"t":1},"speaker":"A","text":"実は、私も猫を飼おうと思っていて、いろいろと教えていただきたいのです。"},{"id":{"d":205,"n":4,"t":1},"speaker":"B","text":"森田さんは、ペット禁止のアパートに住んでいませんでしたか？"},{"id":{"d":205,"n":5,"t":1},"speaker":"A","text":"どうしても猫が飼いたいので、先月ペットが飼えるマンションに引っ越したのです。"},{"id":{"d":205,"n":6,"t":1},"speaker":"B","text":"それは驚きました。森田さんは、本当に猫が好きなのですね。"},{"id":{"d":205,"n":7,"t":1},"speaker":"A","text":"ええ。でも、猫だけでなくペットを飼ったことが一度もないので、分からないことがたくさんあります。"},{"id":{"d":205,"n":8,"t":1},"speaker":"B","text":"私に分かることでしたら、何でもお答えしますよ。"},{"id":{"d":205,"n":9,"t":1},"speaker":"A","text":"助かります。まずは、何を買いそろえればよいか教えていただきたいのです。"},{"id":{"d":205,"n":10,"t":1},"speaker":"B","text":"分かりました。今日、お昼を一緒に食べながらお話しするというのはいかがでしょうか？"},{"id":{"d":205,"n":11,"t":1},"speaker":"A","text":"いいですね。時間を作っていただきありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":206,"n":1,"t":1},"speaker":"A","text":"最近、暑くなってきましたね。あなたは、もう衣替えをしましたか？"},{"id":{"d":206,"n":2,"t":1},"speaker":"B","text":"私は、先週衣替えをしようと思ったのですが、急に出かける用事ができてしまってできませんでした。"},{"id":{"d":206,"n":3,"t":1},"speaker":"A","text":"私は、衣替えをやろうやろうと思いつつ、今日まで手付かずのままです。"},{"id":{"d":206,"n":4,"t":1},"speaker":"B","text":"衣替えは手間がかかりますから、先延ばしにしてしまう気持ちがよく分かります。"},{"id":{"d":206,"n":5,"t":1},"speaker":"A","text":"いざ衣替えを始めると、古くなったり色あせたりした洋服が見つかって、処分するかどうか迷って時間がかかることもありますよね。"},{"id":{"d":206,"n":6,"t":1},"speaker":"B","text":"私は「取っておくか、処分するかを迷ったら、処分する」というルールを作りました。"},{"id":{"d":206,"n":7,"t":1},"speaker":"A","text":"思い切りが良いルールですね。"},{"id":{"d":206,"n":8,"t":1},"speaker":"B","text":"「もう一度、着ることもあるかもしれない」と思うこともありますが、今のところ、処分した後で後悔したことはありません。"},{"id":{"d":206,"n":9,"t":1},"speaker":"A","text":"そのようものかもしれませんね。"},{"id":{"d":206,"n":10,"t":1},"speaker":"B","text":"それに、「処分したら、代わりに新しい洋服を買ってもよい」というルールも作りました。"},{"id":{"d":206,"n":11,"t":1},"speaker":"A","text":"なるほど。まるでご褒美のようなルールですね。"}];
INSERT INTO sentence [{"id":{"d":207,"n":1,"t":1},"speaker":"A","text":"田中さん、よかったらこれをどうぞ。"},{"id":{"d":207,"n":2,"t":1},"speaker":"B","text":"これは、何ですか？"},{"id":{"d":207,"n":3,"t":1},"speaker":"A","text":"私が、自分で漬けた梅干しです。"},{"id":{"d":207,"n":4,"t":1},"speaker":"B","text":"そういえば以前、梅干しを漬けてみたいとおっしゃっていましたね。"},{"id":{"d":207,"n":5,"t":1},"speaker":"A","text":"覚えていてくださいましたか？実際に梅干を漬けてみて、そろそろ食べごろになったようなのです。"},{"id":{"d":207,"n":6,"t":1},"speaker":"B","text":"手作りの梅干しなんて初めて頂きました。とてもうれしいです。"},{"id":{"d":207,"n":7,"t":1},"speaker":"A","text":"一応、自分で試食してみたのですが、想像以上に酸っぱくなってしまいました。"},{"id":{"d":207,"n":8,"t":1},"speaker":"B","text":"私は、酸っぱい梅干しが好きなので大丈夫だと思います。食べてみるのが楽しみです。"},{"id":{"d":207,"n":9,"t":1},"speaker":"A","text":"そう言ってもらえて安心しました。もし酸味を抑えたかったら、お茶漬けに添えるといいと思います。"},{"id":{"d":207,"n":10,"t":1},"speaker":"B","text":"分かりました。大切に食べますね。どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":208,"n":1,"t":1},"speaker":"A","text":"今朝、洗濯をしようとしたら、洗濯機が故障したようで困りました。"},{"id":{"d":208,"n":2,"t":1},"speaker":"B","text":"洗濯機が、動かなくなったのですか？"},{"id":{"d":208,"n":3,"t":1},"speaker":"A","text":"いいえ、全く動かないわけではありませんが、洗濯が始まって10分くらいすると止まってしまうのです。"},{"id":{"d":208,"n":4,"t":1},"speaker":"B","text":"昨日までは、普通に動いていたのですか？"},{"id":{"d":208,"n":5,"t":1},"speaker":"A","text":"昨日は洗濯をしていませんが、最後に使ったのはおとといで、その時はいつも通りに動いていました。"},{"id":{"d":208,"n":6,"t":1},"speaker":"B","text":"一度電源を切って、再起動してみてはどうでしょう？"},{"id":{"d":208,"n":7,"t":1},"speaker":"A","text":"それも試したのですが、改善しませんでした。仕方がないので洗濯機の中の衣類は、手洗いしましたよ。"},{"id":{"d":208,"n":8,"t":1},"speaker":"B","text":"大変でしたね。修理を依頼して直るといいのですが、もうお願いしましたか？"},{"id":{"d":208,"n":9,"t":1},"speaker":"A","text":"あさって、修理担当の人が洗濯機の様子を見に来てくれることになっているのですが、かなり古い洗濯機なので、買い替えが必要かもしれません。"},{"id":{"d":208,"n":10,"t":1},"speaker":"B","text":"洗濯機が直るといいですね。"}];
INSERT INTO sentence [{"id":{"d":209,"n":1,"t":1},"speaker":"A","text":"痛い！"},{"id":{"d":209,"n":2,"t":1},"speaker":"B","text":"どうしましたか？"},{"id":{"d":209,"n":3,"t":1},"speaker":"A","text":"包丁でキュウリを切っていたら、誤って手を切ってしまいました。"},{"id":{"d":209,"n":4,"t":1},"speaker":"B","text":"大丈夫ですか？傷を見せてください。"},{"id":{"d":209,"n":5,"t":1},"speaker":"A","text":"血が出ていますが、傷は浅いと思います。"},{"id":{"d":209,"n":6,"t":1},"speaker":"B","text":"消毒をしましょう。薬箱を持ってきますね。"},{"id":{"d":209,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":209,"n":8,"t":1},"speaker":"B","text":"消毒をしますね。傷口にしみますか？"},{"id":{"d":209,"n":9,"t":1},"speaker":"A","text":"いえ、大丈夫です。"},{"id":{"d":209,"n":10,"t":1},"speaker":"B","text":"ばんそうこうを貼って様子を見ましょう。料理は私が代わって行いますよ。"},{"id":{"d":209,"n":11,"t":1},"speaker":"A","text":"すみませんが、料理の続きをお願いします。"}];
INSERT INTO sentence [{"id":{"d":210,"n":1,"t":1},"speaker":"A","text":"私のお財布を見ませんでしたか？"},{"id":{"d":210,"n":2,"t":1},"speaker":"B","text":"見ていません。なくなってしまったのですか？"},{"id":{"d":210,"n":3,"t":1},"speaker":"A","text":"どこかに置いたと思うのですが、どこだったか思い出せなくて困っています。"},{"id":{"d":210,"n":4,"t":1},"speaker":"B","text":"最後にお財布を使ったのは、今日の午前中です。自動販売機でお茶を買った時にお財布からお金を出しました。"},{"id":{"d":210,"n":5,"t":1},"speaker":"A","text":"お金を出した後、お財布をどこにしまいましたか？"},{"id":{"d":210,"n":6,"t":1},"speaker":"B","text":"自動販売機のお茶を取り出すことに気を取られていて、お財布をどうしたかよく覚えていません。"},{"id":{"d":210,"n":7,"t":1},"speaker":"A","text":"困りましたね。普段、お財布はバッグに入れて出かけていますよね。"},{"id":{"d":210,"n":8,"t":1},"speaker":"B","text":"そうですが、今日は荷物がなかったので、ズボンのポケットにお財布を入れて出かけていました。"},{"id":{"d":210,"n":9,"t":1},"speaker":"A","text":"でも、今はズボンのポケットにお財布がないのですよね？"},{"id":{"d":210,"n":10,"t":1},"speaker":"B","text":"はい、ありません。もしかしたら、上着のポケットに入れたかもしれないので、確認してきます。"}];
INSERT INTO sentence [{"id":{"d":211,"n":1,"t":1},"speaker":"A","text":"今日、犬の散歩を代わりに行ってもらえませんか？"},{"id":{"d":211,"n":2,"t":1},"speaker":"B","text":"何か、予定があるのですか？"},{"id":{"d":211,"n":3,"t":1},"speaker":"A","text":"部活のメンバーで自主練習をすることになったのです。"},{"id":{"d":211,"n":4,"t":1},"speaker":"B","text":"部活の時間以外に自主練習をするなんて、随分と熱心ですね。"},{"id":{"d":211,"n":5,"t":1},"speaker":"A","text":"来月大会があるので、良い成績を残したいのです。"},{"id":{"d":211,"n":6,"t":1},"speaker":"B","text":"そうでしたね。大会では、何か具体的に目標はありますか？"},{"id":{"d":211,"n":7,"t":1},"speaker":"A","text":"去年は準決勝に進めなかったので、今年は絶対に準決勝に進みたいですし、目標はもちろん優勝です。"},{"id":{"d":211,"n":8,"t":1},"speaker":"B","text":"私も応援しています。犬の散歩は代わりますから、練習を頑張ってくださいね。"},{"id":{"d":211,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。チームメイトと一緒に頑張ります。"}];
INSERT INTO sentence [{"id":{"d":212,"n":1,"t":1},"speaker":"A","text":"今度、大阪支店に出張するのですが、山中さんは以前、大阪支店で働いていましたよね？"},{"id":{"d":212,"n":2,"t":1},"speaker":"B","text":"はい、半年前まで大阪支店にいました。"},{"id":{"d":212,"n":3,"t":1},"speaker":"A","text":"3日間の出張予定なのですが、夕食をどのお店で食べようか考えています。大阪支店の近くでおいしいお店はありますか？"},{"id":{"d":212,"n":4,"t":1},"speaker":"B","text":"大阪支店の周りはオフィスビルが多いので、駅前まで行くとお店がたくさんありますよ。"},{"id":{"d":212,"n":5,"t":1},"speaker":"A","text":"駅というと、大阪駅ですか？"},{"id":{"d":212,"n":6,"t":1},"speaker":"B","text":"いえ、大阪支店の最寄り駅は北浜駅です。"},{"id":{"d":212,"n":7,"t":1},"speaker":"A","text":"では北浜駅に行ってみます。山中さんのお薦めのお店はありますか？"},{"id":{"d":212,"n":8,"t":1},"speaker":"B","text":"僕はあまり外食をしなかったので詳しくないのですが、一度同僚に連れて行ってもらったイタリアンレストランの料理はとてもおいしかったです。"},{"id":{"d":212,"n":9,"t":1},"speaker":"A","text":"イタリアンは僕も好きなので行ってみたいです。お店の名前を教えてもらえますか？"},{"id":{"d":212,"n":10,"t":1},"speaker":"B","text":"いいですよ。この紙にメモしますので、どうぞ。"}];
INSERT INTO sentence [{"id":{"d":213,"n":1,"t":1},"speaker":"A","text":"節分のマメを、買ってきました。"},{"id":{"d":213,"n":2,"t":1},"speaker":"B","text":"今日は、節分でしたね。今年は、誰が鬼の役をやりますか？"},{"id":{"d":213,"n":3,"t":1},"speaker":"A","text":"去年は、パパがやりましたね。今年は、公平にジャンケンで決めましょうか？"},{"id":{"d":213,"n":4,"t":1},"speaker":"B","text":"そうですね。節分といえば、恵方巻きですね。"},{"id":{"d":213,"n":5,"t":1},"speaker":"A","text":"はい。マメと一緒に恵方巻きも買おうかと思ったのですが、材料が大体そろっていますから手作りしようと思います。"},{"id":{"d":213,"n":6,"t":1},"speaker":"B","text":"作るのも楽しいですよね。何か手伝いましょうか？"},{"id":{"d":213,"n":7,"t":1},"speaker":"A","text":"助かります。私はかんぴょうを煮るので、だし巻き卵を作ってもらえますか？"},{"id":{"d":213,"n":8,"t":1},"speaker":"B","text":"うまくできるかどうか、自信がありません。"},{"id":{"d":213,"n":9,"t":1},"speaker":"A","text":"巻きずしの具材なので、少しくらい形が崩れても大丈夫ですよ。"},{"id":{"d":213,"n":10,"t":1},"speaker":"B","text":"そうですか。では、作り始めましょう。"}];
INSERT INTO sentence [{"id":{"d":214,"n":1,"t":1},"speaker":"A","text":"こんにちは。江藤さん、先週はレッスンを休んでいましたね。"},{"id":{"d":214,"n":2,"t":1},"speaker":"B","text":"こんにちは。先週は足首が痛くて休みましたが、もう大丈夫です。"},{"id":{"d":214,"n":3,"t":1},"speaker":"A","text":"足首ですか。もしかして、練習のし過ぎで痛めてしまったのですか？"},{"id":{"d":214,"n":4,"t":1},"speaker":"B","text":"いえ、道路の段差につまずいて、足首をひねってしまったのです。"},{"id":{"d":214,"n":5,"t":1},"speaker":"A","text":"それは痛そうですね。"},{"id":{"d":214,"n":6,"t":1},"speaker":"B","text":"少し腫れてしまったので念のために病院へ行きましたが、捻挫しただけで骨に異常はありませんでした。"},{"id":{"d":214,"n":7,"t":1},"speaker":"A","text":"もう腫れは治まったのですか？"},{"id":{"d":214,"n":8,"t":1},"speaker":"B","text":"はい。痛みもなくなり、病院で医者に「運動しても大丈夫」と言われました。"},{"id":{"d":214,"n":9,"t":1},"speaker":"A","text":"安心しました。でも、無理はしないでくださいね。"},{"id":{"d":214,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。万が一痛みが出たら、途中で休憩させてもらいます。"}];
INSERT INTO sentence [{"id":{"d":215,"n":1,"t":1},"speaker":"A","text":"カーテンを、閉めてもよいですか？"},{"id":{"d":215,"n":2,"t":1},"speaker":"B","text":"閉めてもよいですが、昼間なのになぜ閉めるのですか？"},{"id":{"d":215,"n":3,"t":1},"speaker":"A","text":"テレビの画面に日の光が当たって、画面が見にくいのです。"},{"id":{"d":215,"n":4,"t":1},"speaker":"B","text":"私も前から気になっていました。今日のように天気が良いと、特に見づらいですよね。"},{"id":{"d":215,"n":5,"t":1},"speaker":"A","text":"カーテンを閉めたら、日光が遮られてましになるかなと思ったのです。"},{"id":{"d":215,"n":6,"t":1},"speaker":"B","text":"ただ、根本的には解決しませんよね。いっその事、配置換えをしましょうか？"},{"id":{"d":215,"n":7,"t":1},"speaker":"A","text":"どういうことですか？"},{"id":{"d":215,"n":8,"t":1},"speaker":"B","text":"テレビとソファの位置を逆にすれば、テレビの画面に日が当たりにくくなるのと思います。"},{"id":{"d":215,"n":9,"t":1},"speaker":"A","text":"なるほど。試したいところですが、ソファもテレビも重いから移動させられるか不安です。"},{"id":{"d":215,"n":10,"t":1},"speaker":"B","text":"聡と健司に、手伝ってもらいましょう。"},{"id":{"d":215,"n":11,"t":1},"speaker":"A","text":"そうですね。二人に声を掛けてきます。"}];
INSERT INTO sentence [{"id":{"d":216,"n":1,"t":1},"speaker":"A","text":"おはよう。今日は雨ですね。"},{"id":{"d":216,"n":2,"t":1},"speaker":"B","text":"さっき、テレビのニュース番組で大雨警報が発令されたと報道されていました。"},{"id":{"d":216,"n":3,"t":1},"speaker":"A","text":"今日は、仕事で必要な荷物が多いのに、天気が悪いとは気が重いです。"},{"id":{"d":216,"n":4,"t":1},"speaker":"B","text":"私は今日、車で出勤するつもりなので、駅まで送りましょうか？"},{"id":{"d":216,"n":5,"t":1},"speaker":"A","text":"よいのですか？"},{"id":{"d":216,"n":6,"t":1},"speaker":"B","text":"今日は栄町の営業所に寄る用事があって、車を使うと都合が良いのです。あそこは駅と同じ方向だから良いですよ。"},{"id":{"d":216,"n":7,"t":1},"speaker":"A","text":"ありがたいです。"},{"id":{"d":216,"n":8,"t":1},"speaker":"B","text":"7時半くらいに出発しようと思っているのですが、大丈夫ですか？"},{"id":{"d":216,"n":9,"t":1},"speaker":"A","text":"十分間に合います。それでは、顔を洗ってくるので、一緒に朝ご飯を食べましょう。"}];
INSERT INTO sentence [{"id":{"d":217,"n":1,"t":1},"speaker":"A","text":"玄関の電気がつきません。"},{"id":{"d":217,"n":2,"t":1},"speaker":"B","text":"電球が、切れてしまったのでしょうね。新しい電球に替えましょう。"},{"id":{"d":217,"n":3,"t":1},"speaker":"A","text":"私が、新しい電球を取ってきます。どこにありますか？"},{"id":{"d":217,"n":4,"t":1},"speaker":"B","text":"物置の右側の棚の一番下の段にあります。"},{"id":{"d":217,"n":5,"t":1},"speaker":"A","text":"分かりました。取りに行ってきますね。"},{"id":{"d":217,"n":6,"t":1},"speaker":"B","text":"すみませんが、ついでに脚立も持ってきてください。"},{"id":{"d":217,"n":7,"t":1},"speaker":"A","text":"そうですね、玄関の電球は高いところにあるから脚立が必要ですね。"},{"id":{"d":217,"n":8,"t":1},"speaker":"B","text":"お願いします。電球を取り替える時は、私も手伝います。"}];
INSERT INTO sentence [{"id":{"d":218,"n":1,"t":1},"speaker":"A","text":"次の日曜日に、友達5人でクリスマス会を開催することになりました。"},{"id":{"d":218,"n":2,"t":1},"speaker":"B","text":"すてきな計画ですね。どこで開催するのですか？"},{"id":{"d":218,"n":3,"t":1},"speaker":"A","text":"麗奈さんの家です。ご両親が承諾してくださったそうです。"},{"id":{"d":218,"n":4,"t":1},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":218,"n":5,"t":1},"speaker":"A","text":"ただ、プレゼント交換をするのですが、どのようなものを用意すればよいか悩んでいます。"},{"id":{"d":218,"n":6,"t":1},"speaker":"B","text":"自分がもらったらうれしい物を考えてみたらどうですか？"},{"id":{"d":218,"n":7,"t":1},"speaker":"A","text":"前に入浴剤を頂いた時、とてもうれしかったです。"},{"id":{"d":218,"n":8,"t":1},"speaker":"B","text":"入浴剤は良いプレゼントだと思いますよ。"},{"id":{"d":218,"n":9,"t":1},"speaker":"A","text":"ショッピングモールにある雑貨屋さんで、かわいい入浴剤を売っていますよ。"},{"id":{"d":218,"n":10,"t":1},"speaker":"B","text":"明日、見に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":219,"n":1,"t":1},"speaker":"A","text":"トレーナーの裾に染みが付いていますよ。"},{"id":{"d":219,"n":2,"t":1},"speaker":"B","text":"お昼にパスタを食べた時、ソースを付けてしまったのです。"},{"id":{"d":219,"n":3,"t":1},"speaker":"A","text":"早く洗った方が、汚れが取れやすいので、すぐに洗いましょう。"},{"id":{"d":219,"n":4,"t":1},"speaker":"B","text":"お願いしてよいですか？"},{"id":{"d":219,"n":5,"t":1},"speaker":"A","text":"しみ抜きは割と得意なので、やってみます。トレーナーを脱いでください。"},{"id":{"d":219,"n":6,"t":1},"speaker":"B","text":"お気に入りのトレーナーなので、きれいになったらうれしいです。お願いします。"},{"id":{"d":219,"n":7,"t":1},"speaker":"A","text":"もしどうしても染みが取れなかったら、クリーニングに出しましょう。"},{"id":{"d":219,"n":8,"t":1},"speaker":"B","text":"そうですね。完全に取れなくても、目立たなくなればよいです。"},{"id":{"d":219,"n":9,"t":1},"speaker":"A","text":"分かりました。できるだけのことはやってみます。"},{"id":{"d":219,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":220,"n":1,"t":1},"speaker":"A","text":"何を見ているのですか？"},{"id":{"d":220,"n":2,"t":1},"speaker":"B","text":"姉が送ってくれた、めいの写真です。"},{"id":{"d":220,"n":3,"t":1},"speaker":"A","text":"見せていただいてもよいですか？"},{"id":{"d":220,"n":4,"t":1},"speaker":"B","text":"もちろんです。どうぞ。"},{"id":{"d":220,"n":5,"t":1},"speaker":"A","text":"かわいらしいですね。めい御さんは何歳ですか？"},{"id":{"d":220,"n":6,"t":1},"speaker":"B","text":"2歳です。かなりおしゃべりがうまくなっていて、本当にかわいいです。"},{"id":{"d":220,"n":7,"t":1},"speaker":"A","text":"よく会っているのですか？"},{"id":{"d":220,"n":8,"t":1},"speaker":"B","text":"それが、姉の家族は北海道に住んでいるので、なかなか会えないのです。"},{"id":{"d":220,"n":9,"t":1},"speaker":"A","text":"北海道とは、遠いですね。"},{"id":{"d":220,"n":10,"t":1},"speaker":"B","text":"会えない間にめいがどんどん成長していくので、時々、写真や動画を送ってもらえるように姉に頼んでいるのです。"},{"id":{"d":220,"n":11,"t":1},"speaker":"A","text":"優しいお姉さんですね。"},{"id":{"d":220,"n":12,"t":1},"speaker":"B","text":"はい。写真や動画を何度も見返して、めいと会えない寂しさを紛らわせています。"}];
INSERT INTO sentence [{"id":{"d":221,"n":1,"t":1},"speaker":"A","text":"そろそろ、お風呂に入ってはどうですか？"},{"id":{"d":221,"n":2,"t":1},"speaker":"B","text":"僕は後でよいので、先に入ってください。"},{"id":{"d":221,"n":3,"t":1},"speaker":"A","text":"何かすることがあるのですか？"},{"id":{"d":221,"n":4,"t":1},"speaker":"B","text":"これから、テレビでサッカーの試合が放送されるので、見終わってからお風呂に入ります。"},{"id":{"d":221,"n":5,"t":1},"speaker":"A","text":"応援しているサッカーチームの試合なのですか？"},{"id":{"d":221,"n":6,"t":1},"speaker":"B","text":"はい。リーグ優勝がかかった大事な試合なので、ぜひリアルタイムで見たいのです。"},{"id":{"d":221,"n":7,"t":1},"speaker":"A","text":"楽しみですね。それでは、私は先にお風呂に入りますね。"},{"id":{"d":221,"n":8,"t":1},"speaker":"B","text":"どうぞ、ごゆっくり。"},{"id":{"d":221,"n":9,"t":1},"speaker":"A","text":"お風呂から上がったら、忘れずに換気扇を回してくださいね。"},{"id":{"d":221,"n":10,"t":1},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":222,"n":1,"t":1},"speaker":"A","text":"ちょっと、お聞きしてよいですか？"},{"id":{"d":222,"n":2,"t":1},"speaker":"B","text":"はい。エアコンをお探しですか？"},{"id":{"d":222,"n":3,"t":1},"speaker":"A","text":"ええ。リビングのエアコンを買い替えようと思っているのですが、どれが良いのか分からなくて決められません。"},{"id":{"d":222,"n":4,"t":1},"speaker":"B","text":"まずは、部屋の広さに適したエアコンが良いですね。リビングはどのくらいの広さですか？"},{"id":{"d":222,"n":5,"t":1},"speaker":"A","text":"15畳くらいだと思います。"},{"id":{"d":222,"n":6,"t":1},"speaker":"B","text":"その広さでしたら、こちらの段にあるエアコンが該当しますね。"},{"id":{"d":222,"n":7,"t":1},"speaker":"A","text":"5台ありますね。どのような違いがありますか？"},{"id":{"d":222,"n":8,"t":1},"speaker":"B","text":"それぞれ、特徴があります。音が静かなもの、電気代が安いもの、掃除が楽なもの、といった性能の違いがあります。"},{"id":{"d":222,"n":9,"t":1},"speaker":"A","text":"リビングに取り付けるので、音が静かなものがいいです。"},{"id":{"d":222,"n":10,"t":1},"speaker":"B","text":"それでは、こちらのエアコンがお薦めです。"},{"id":{"d":222,"n":11,"t":1},"speaker":"A","text":"実際に、音を確認することはできますか？"},{"id":{"d":222,"n":12,"t":1},"speaker":"B","text":"はい。少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":223,"n":1,"t":1},"speaker":"A","text":"今日の夕食は、カレーライスですね？"},{"id":{"d":223,"n":2,"t":1},"speaker":"B","text":"当たりです。"},{"id":{"d":223,"n":3,"t":1},"speaker":"A","text":"匂いですぐに分かりました。"},{"id":{"d":223,"n":4,"t":1},"speaker":"B","text":"でも、いつものカレーとは違う匂いのはずなのですが、分かりますか？"},{"id":{"d":223,"n":5,"t":1},"speaker":"A","text":"そう言われれば、少し匂いが違いますね。どうしてでしょう？"},{"id":{"d":223,"n":6,"t":1},"speaker":"B","text":"いつもはカレールーを使うのですが、今日はカレールーを使わずに、全て手作りしたのです。"},{"id":{"d":223,"n":7,"t":1},"speaker":"A","text":"すごいですね。作り方はどうやって調べたのですか？"},{"id":{"d":223,"n":8,"t":1},"speaker":"B","text":"インターネットです。情報がたくさんあるし、分かりやすく解説してあるページを見つけました。"},{"id":{"d":223,"n":9,"t":1},"speaker":"A","text":"手作りだと、時間がかかったでしょうね。"},{"id":{"d":223,"n":10,"t":1},"speaker":"B","text":"3時間かかりました。食べるのを楽しみにしていてくださいね。"}];
INSERT INTO sentence [{"id":{"d":224,"n":1,"t":1},"speaker":"A","text":"今日、車で出かける予定はありますか？"},{"id":{"d":224,"n":2,"t":1},"speaker":"B","text":"駅の方に車で行くつもりです。"},{"id":{"d":224,"n":3,"t":1},"speaker":"A","text":"市役所の前の道で工事をしているので、違う道を通った方がいいですよ。"},{"id":{"d":224,"n":4,"t":1},"speaker":"B","text":"通行止めになっているのですか？"},{"id":{"d":224,"n":5,"t":1},"speaker":"A","text":"いえ、通行止めではなく片側交互通行ですが、交通量が多いのでとても混雑しています。"},{"id":{"d":224,"n":6,"t":1},"speaker":"B","text":"でも、あの道を通らないとすると、どうすれば駅の方へ行けるでしょうか？"},{"id":{"d":224,"n":7,"t":1},"speaker":"A","text":"少し遠回りになりますが、消防署のある通りを使って行くのがいいと思います。"},{"id":{"d":224,"n":8,"t":1},"speaker":"B","text":"そこなら、通ったことがあります。事前に教えてくださって、ありがとうございます。"},{"id":{"d":224,"n":9,"t":1},"speaker":"A","text":"気を付けて、行ってきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":225,"n":1,"t":1},"speaker":"A","text":"クイズ番組を見ているのですか？"},{"id":{"d":225,"n":2,"t":1},"speaker":"B","text":"難しい問題が多いですが、結構面白いですよ。"},{"id":{"d":225,"n":3,"t":1},"speaker":"A","text":"漢字の問題ですね。"},{"id":{"d":225,"n":4,"t":1},"speaker":"B","text":"熟語が表示されて、10秒以内に読み方を答えなければ失格になるというルールです。"},{"id":{"d":225,"n":5,"t":1},"speaker":"A","text":"私は漢字があまり得意ではないので、答えられそうにありません。"},{"id":{"d":225,"n":6,"t":1},"speaker":"B","text":"私も自信がありませんが、簡単な問題と難しい問題が混ざっているようです。"},{"id":{"d":225,"n":7,"t":1},"speaker":"A","text":"問題が表示されましたね。この熟語は読めますか？"},{"id":{"d":225,"n":8,"t":1},"speaker":"B","text":"見たことはあるのですが、読み方は分かりません。"},{"id":{"d":225,"n":9,"t":1},"speaker":"A","text":"私も、見たことのある熟語です。でも読み方が思い出せません。"},{"id":{"d":225,"n":10,"t":1},"speaker":"B","text":"思い出せないと悔しいですよね。"},{"id":{"d":225,"n":11,"t":1},"speaker":"A","text":"ああ、時間切れになってしまいました。残念です。"}];
INSERT INTO sentence [{"id":{"d":226,"n":1,"t":1},"speaker":"A","text":"会議は終わりましたか？"},{"id":{"d":226,"n":2,"t":1},"speaker":"B","text":"はい。予定より少し時間がかかりましたが、終わりました。"},{"id":{"d":226,"n":3,"t":1},"speaker":"A","text":"会議室にいらっしゃった間に、電話がありましたよ。"},{"id":{"d":226,"n":4,"t":1},"speaker":"B","text":"どなたからですか？"},{"id":{"d":226,"n":5,"t":1},"speaker":"A","text":"ミナト工業の大林さんです。"},{"id":{"d":226,"n":6,"t":1},"speaker":"B","text":"何か伝言はありましたか？"},{"id":{"d":226,"n":7,"t":1},"speaker":"A","text":"次のプロジェクトに関する資料をメールに添付して送ってくださったそうで、確認してくださいとのことでした。"},{"id":{"d":226,"n":8,"t":1},"speaker":"B","text":"分かりました。ありがとう。"}];
INSERT INTO sentence [{"id":{"d":227,"n":1,"t":1},"speaker":"A","text":"髪を切ったのですね。お似合いですよ。"},{"id":{"d":227,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。思い切ってショートヘアにしました。"},{"id":{"d":227,"n":3,"t":1},"speaker":"A","text":"ずっとロングヘアでしたよね。"},{"id":{"d":227,"n":4,"t":1},"speaker":"B","text":"思い返してみたら、5年以上ロングヘアでした。"},{"id":{"d":227,"n":5,"t":1},"speaker":"A","text":"頭が軽くなったように感じるのではありませんか？"},{"id":{"d":227,"n":6,"t":1},"speaker":"B","text":"はい、軽くなりました。それに髪を洗ったり乾かしたりすることがとても楽です。"},{"id":{"d":227,"n":7,"t":1},"speaker":"A","text":"そうでしょうね。随分切りましたね。"},{"id":{"d":227,"n":8,"t":1},"speaker":"B","text":"20センチメートル以上切りました。美容師さんにも「本当に切ってよいのですか？」と念を押されました。"},{"id":{"d":227,"n":9,"t":1},"speaker":"A","text":"美容師さんも、緊張したかもしれませんね。"},{"id":{"d":227,"n":10,"t":1},"speaker":"B","text":"実際に「ドキドキします」と言っていましたよ。"}];
INSERT INTO sentence [{"id":{"d":228,"n":1,"t":1},"speaker":"A","text":"デザートに、何か食べたいですね。"},{"id":{"d":228,"n":2,"t":1},"speaker":"B","text":"そういえば、会社の同僚にお土産をもらったので、一緒に食べましょうか？"},{"id":{"d":228,"n":3,"t":1},"speaker":"A","text":"うれしいです。"},{"id":{"d":228,"n":4,"t":1},"speaker":"B","text":"軽井沢へ行ったそうで、おいしいスイーツのお店で買ってくれたようです。"},{"id":{"d":228,"n":5,"t":1},"speaker":"A","text":"わざわざ買ってきてくれるなんてありがたいですね。"},{"id":{"d":228,"n":6,"t":1},"speaker":"B","text":"冷蔵庫に入っているので取って来ます。"},{"id":{"d":228,"n":7,"t":1},"speaker":"A","text":"私はお皿とスプーンを用意しますね。"},{"id":{"d":228,"n":8,"t":1},"speaker":"B","text":"飲み物は何がいいでしょうか？"},{"id":{"d":228,"n":9,"t":1},"speaker":"A","text":"どのようなスイーツか、見てから決めましょうか？"},{"id":{"d":228,"n":10,"t":1},"speaker":"B","text":"そうですね。取りあえず、見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":229,"n":1,"t":1},"speaker":"A","text":"熱心に新聞を読んでいますね。"},{"id":{"d":229,"n":2,"t":1},"speaker":"B","text":"水曜日に、新刊本を紹介する記事が掲載されていて、毎週楽しみにしているのです。"},{"id":{"d":229,"n":3,"t":1},"speaker":"A","text":"今週は、どのような本が紹介されていますか？"},{"id":{"d":229,"n":4,"t":1},"speaker":"B","text":"若い作家さんが書いた推理小説です。先月、文学賞を受賞した作品のようです。"},{"id":{"d":229,"n":5,"t":1},"speaker":"A","text":"もしかして、畑中美佐さんの作品ですか？"},{"id":{"d":229,"n":6,"t":1},"speaker":"B","text":"そうです。よく分かりましたね。"},{"id":{"d":229,"n":7,"t":1},"speaker":"A","text":"おととい本屋に行った時に見かけました。文学賞受賞作品として目立つように陳列されていたのです。"},{"id":{"d":229,"n":8,"t":1},"speaker":"B","text":"そうでしたか。この記事で、その作品はとても評価されています。"},{"id":{"d":229,"n":9,"t":1},"speaker":"A","text":"読んでみたいですね。"},{"id":{"d":229,"n":10,"t":1},"speaker":"B","text":"私は買うつもりなので、読み終わったら貸しますよ。"},{"id":{"d":229,"n":11,"t":1},"speaker":"A","text":"ありがとうございます。畑中さんの推理小説を読んだ感想も聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":230,"n":1,"t":1},"speaker":"A","text":"お邪魔します。"},{"id":{"d":230,"n":2,"t":1},"speaker":"B","text":"どうぞ、上がってください。このスリッパをどうぞ。"},{"id":{"d":230,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":230,"n":4,"t":1},"speaker":"B","text":"よろしければ、コートはお預かりしてこちらへ掛けておきますよ。"},{"id":{"d":230,"n":5,"t":1},"speaker":"A","text":"お願いします。"},{"id":{"d":230,"n":6,"t":1},"speaker":"B","text":"リビングはこちらになります。ソファにお掛けください。"},{"id":{"d":230,"n":7,"t":1},"speaker":"A","text":"では、失礼いたします。"},{"id":{"d":230,"n":8,"t":1},"speaker":"B","text":"飲み物は、コーヒーで大丈夫ですか？"},{"id":{"d":230,"n":9,"t":1},"speaker":"A","text":"お気遣いなく。"},{"id":{"d":230,"n":10,"t":1},"speaker":"B","text":"私も飲みたいので、ご一緒しましょう。"},{"id":{"d":230,"n":11,"t":1},"speaker":"A","text":"ではブラックでお願いします。"},{"id":{"d":230,"n":12,"t":1},"speaker":"B","text":"はい。少しお待ちくださいね。"}];
INSERT INTO sentence [{"id":{"d":231,"n":1,"t":1},"speaker":"A","text":"ワイシャツのボタンが、取れてしまいました。"},{"id":{"d":231,"n":2,"t":1},"speaker":"B","text":"取れたボタンは、見つかりましたか？"},{"id":{"d":231,"n":3,"t":1},"speaker":"A","text":"同僚が見つけて、渡してくれました。"},{"id":{"d":231,"n":4,"t":1},"speaker":"B","text":"良かったです。私がボタンを付けましょうか？"},{"id":{"d":231,"n":5,"t":1},"speaker":"A","text":"自分で付けられるので大丈夫です。裁縫箱はどこにありますか？"},{"id":{"d":231,"n":6,"t":1},"speaker":"B","text":"クロードの棚にあります。少し奥の方にあるので、分かりにくいかもしれません。"},{"id":{"d":231,"n":7,"t":1},"speaker":"A","text":"探してみます。"},{"id":{"d":231,"n":8,"t":1},"speaker":"B","text":"もし見つからなかったら、私を呼んでくださいね。"}];
INSERT INTO sentence [{"id":{"d":232,"n":1,"t":1},"speaker":"A","text":"すみません。昨日、このお店で買ったセーターを交換していただけますか？"},{"id":{"d":232,"n":2,"t":1},"speaker":"B","text":"交換の理由をお伺いできますか？"},{"id":{"d":232,"n":3,"t":1},"speaker":"A","text":"夫のために買ったのですが、家で夫に着てもらったら、サイズが小さかったのです。"},{"id":{"d":232,"n":4,"t":1},"speaker":"B","text":"分かりました。どのサイズに交換しますか？"},{"id":{"d":232,"n":5,"t":1},"speaker":"A","text":"MサイズをLサイズに交換してほしいです。"},{"id":{"d":232,"n":6,"t":1},"speaker":"B","text":"Lサイズですね。在庫があるかどうか確認しますので、少々お待ちください。"},{"id":{"d":232,"n":7,"t":1},"speaker":"A","text":"お願いします。"},{"id":{"d":232,"n":8,"t":1},"speaker":"B","text":"お待たせしました。こちらでよろしいでしょうか？"},{"id":{"d":232,"n":9,"t":1},"speaker":"A","text":"はい。在庫があってよかったです。"}];
INSERT INTO sentence [{"id":{"d":233,"n":1,"t":1},"speaker":"A","text":"きれいな花ですね。"},{"id":{"d":233,"n":2,"t":1},"speaker":"B","text":"たまにはお花を飾りたいと思って、買ってみました。"},{"id":{"d":233,"n":3,"t":1},"speaker":"A","text":"どこのお店で買ったのですか？"},{"id":{"d":233,"n":4,"t":1},"speaker":"B","text":"駅の南口に新しいできた花屋です。"},{"id":{"d":233,"n":5,"t":1},"speaker":"A","text":"ああ、あのお店ですか。おしゃれなお店ですよね。"},{"id":{"d":233,"n":6,"t":1},"speaker":"B","text":"はい。花の飾り方もすてきで、家に飾りたくなりました。"},{"id":{"d":233,"n":7,"t":1},"speaker":"A","text":"これは、何という花でしょうか？"},{"id":{"d":233,"n":8,"t":1},"speaker":"B","text":"ピンク色の花がガーベラで、白い花がカサブランカです。"},{"id":{"d":233,"n":9,"t":1},"speaker":"A","text":"どちらもきれいです。長持ちするといいですね。"},{"id":{"d":233,"n":10,"t":1},"speaker":"B","text":"花瓶の水を取り替えることを、忘れないように気を付けようと思っています。"}];
INSERT INTO sentence [{"id":{"d":234,"n":1,"t":1},"speaker":"A","text":"図書館に行ってきます。"},{"id":{"d":234,"n":2,"t":1},"speaker":"B","text":"さっき美容院から帰宅したばかりなのに、また出かけるのですか？"},{"id":{"d":234,"n":3,"t":1},"speaker":"A","text":"美容院から帰る途中で図書館に寄るつもりだったのですが、忘れてそのまま帰宅してしまったのです。"},{"id":{"d":234,"n":4,"t":1},"speaker":"B","text":"図書館へは、どうしても今日行く必要があるのですか？"},{"id":{"d":234,"n":5,"t":1},"speaker":"A","text":"借りている本の返却期限が今日なのです。"},{"id":{"d":234,"n":6,"t":1},"speaker":"B","text":"僕は後で歯医者に行くから、その時に図書館へ寄りますよ。"},{"id":{"d":234,"n":7,"t":1},"speaker":"A","text":"でも、歯医者と図書館では反対方向です。"},{"id":{"d":234,"n":8,"t":1},"speaker":"B","text":"良い運動になるし、今日は時間があるから大丈夫です。"},{"id":{"d":234,"n":9,"t":1},"speaker":"A","text":"それではお願いします。この本です。"},{"id":{"d":234,"n":10,"t":1},"speaker":"B","text":"お預かりします。"}];
INSERT INTO sentence [{"id":{"d":235,"n":1,"t":1},"speaker":"A","text":"今日も暑いですね。"},{"id":{"d":235,"n":2,"t":1},"speaker":"B","text":"この時間が、一日で一番暑いですよね。"},{"id":{"d":235,"n":3,"t":1},"speaker":"A","text":"朝の天気予報では、予想最高気温が32度となっていました。"},{"id":{"d":235,"n":4,"t":1},"speaker":"B","text":"昨日より、気温が高いですね。"},{"id":{"d":235,"n":5,"t":1},"speaker":"A","text":"おまけに今日は風が吹いていないので、余計に暑く感じます。"},{"id":{"d":235,"n":6,"t":1},"speaker":"B","text":"本当ですね。暑くて外に出る気になりません。"},{"id":{"d":235,"n":7,"t":1},"speaker":"A","text":"昼過ぎまでは家で過ごす方がよさそうです。"},{"id":{"d":235,"n":8,"t":1},"speaker":"B","text":"そうですね。熱中症にならないように、こまめに水分補給しましょう。"}];
INSERT INTO sentence [{"id":{"d":236,"n":1,"t":1},"speaker":"A","text":"坂下さん、受付までどうぞ。"},{"id":{"d":236,"n":2,"t":1},"speaker":"B","text":"はい、坂下です。"},{"id":{"d":236,"n":3,"t":1},"speaker":"A","text":"今日の診察代は1,200円になります。"},{"id":{"d":236,"n":4,"t":1},"speaker":"B","text":"ちょうどあります。"},{"id":{"d":236,"n":5,"t":1},"speaker":"A","text":"では領収書をお渡しします。次回の予約はいつにしますか？"},{"id":{"d":236,"n":6,"t":1},"speaker":"B","text":"来週の火曜日で、空いている時間はありますか？"},{"id":{"d":236,"n":7,"t":1},"speaker":"A","text":"午後4時以降なら、空いています。"},{"id":{"d":236,"n":8,"t":1},"speaker":"B","text":"その時間は都合がつきません。再来週の火曜日だと何時が空いていますか？"},{"id":{"d":236,"n":9,"t":1},"speaker":"A","text":"その日なら、午後はずっと空いています。"},{"id":{"d":236,"n":10,"t":1},"speaker":"B","text":"それでは、午後2時に予約をお願いします。"},{"id":{"d":236,"n":11,"t":1},"speaker":"A","text":"再来週の火曜日、午後2時ですね。予約を入れておきます。"}];
INSERT INTO sentence [{"id":{"d":237,"n":1,"t":1},"speaker":"A","text":"新しいフライパンを買いました。"},{"id":{"d":237,"n":2,"t":1},"speaker":"B","text":"今使っているフライパンは、問題があるのですか？"},{"id":{"d":237,"n":3,"t":1},"speaker":"A","text":"長い間使ったせいで焦げ付くようになってしまったのです。"},{"id":{"d":237,"n":4,"t":1},"speaker":"B","text":"確かに、だいぶ前から使っていますね。"},{"id":{"d":237,"n":5,"t":1},"speaker":"A","text":"今のフライパンは重くて、料理する時に大変だったので、軽いフライパンを選びました。"},{"id":{"d":237,"n":6,"t":1},"speaker":"B","text":"持ってもよろしいですか？"},{"id":{"d":237,"n":7,"t":1},"speaker":"A","text":"もちろんです。重さを比べてみてください。"},{"id":{"d":237,"n":8,"t":1},"speaker":"B","text":"両方持つと、新しいフライパンがとても軽いことが分かります。"},{"id":{"d":237,"n":9,"t":1},"speaker":"A","text":"これなら、料理する時も洗う時も負担が少ないです。"},{"id":{"d":237,"n":10,"t":1},"speaker":"B","text":"良い買い物をしましたね。"}];
INSERT INTO sentence [{"id":{"d":238,"n":1,"t":1},"speaker":"A","text":"今日は、とても眠いです。"},{"id":{"d":238,"n":2,"t":1},"speaker":"B","text":"寝不足ですか？"},{"id":{"d":238,"n":3,"t":1},"speaker":"A","text":"はい。昨日はゲームをしていて、気が付いたら夜中になっていました。"},{"id":{"d":238,"n":4,"t":1},"speaker":"B","text":"ゲームがお好きなのですね。"},{"id":{"d":238,"n":5,"t":1},"speaker":"A","text":"ゲームが唯一の趣味です。"},{"id":{"d":238,"n":6,"t":1},"speaker":"B","text":"私はゲームをしないのですが、好きな人は熱中してしまうものなのですね。"},{"id":{"d":238,"n":7,"t":1},"speaker":"A","text":"今やっているゲームは物語がどんどん展開していって、時間の経過を忘れてしまうのです。"},{"id":{"d":238,"n":8,"t":1},"speaker":"B","text":"私は趣味がないので、そのように好きになれることがある方がうらやましいです。"},{"id":{"d":238,"n":9,"t":1},"speaker":"A","text":"でも、ほどほどにしないといけないと反省しています。"}];
