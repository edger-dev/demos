INSERT INTO sentence [{"id":{"d":240,"n":1,"t":1},"speaker":"A","text":"昨夜、夜更かししてしまい、今日は眠いです。"},{"id":{"d":240,"n":2,"t":1},"speaker":"B","text":"何時頃に寝たのですか？"},{"id":{"d":240,"n":3,"t":1},"speaker":"A","text":"午前1時過ぎくらいです。"},{"id":{"d":240,"n":4,"t":1},"speaker":"B","text":"それは遅いですね。何をしていたのですか？"},{"id":{"d":240,"n":5,"t":1},"speaker":"A","text":"友達が面白いと話していた韓国ドラマを見ていました。本当に面白くて、止まらなくなってしまいました。"},{"id":{"d":240,"n":6,"t":1},"speaker":"B","text":"夜はドラマを見て過ごすことが多いのですか？"},{"id":{"d":240,"n":7,"t":1},"speaker":"A","text":"そうですね、ドラマを見たりYouTubeを見たりしています。田中さんは見ませんか？"},{"id":{"d":240,"n":8,"t":1},"speaker":"B","text":"夜は読書することの方が多いですね。"},{"id":{"d":240,"n":9,"t":1},"speaker":"A","text":"いいですね。お薦めの本があったら、今度教えてください。"},{"id":{"d":240,"n":10,"t":1},"speaker":"B","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":241,"n":1,"t":1},"speaker":"A","text":"最近、自炊するようにしているのですが、料理のレパートリーが少なくて困っています。"},{"id":{"d":241,"n":2,"t":1},"speaker":"B","text":"料理が好きなのですね。"},{"id":{"d":241,"n":3,"t":1},"speaker":"A","text":"割と好きですが、献立に悩みます。"},{"id":{"d":241,"n":4,"t":1},"speaker":"B","text":"私は料理自体が好きになれません。"},{"id":{"d":241,"n":5,"t":1},"speaker":"A","text":"では、外食をしているのですか？"},{"id":{"d":241,"n":6,"t":1},"speaker":"B","text":"家でリラックスしたいので、お店で買ってテイクアウトすることが多いです。"},{"id":{"d":241,"n":7,"t":1},"speaker":"A","text":"私も時々そうします。今はテイクアウトできるお店が増えていますよね。"},{"id":{"d":241,"n":8,"t":1},"speaker":"B","text":"はい。便利で助かっています。自炊した方が経済的ですけどね。"},{"id":{"d":241,"n":9,"t":1},"speaker":"A","text":"バランスが難しいですね。"}];
INSERT INTO sentence [{"id":{"d":242,"n":1,"t":1},"speaker":"A","text":"朝食の時に、家族みんなは何を飲みますか？"},{"id":{"d":242,"n":2,"t":1},"speaker":"B","text":"食事によります。和食なら私と夫は日本茶を飲みます。娘もお茶です。息子はプロテインです。"},{"id":{"d":242,"n":3,"t":1},"speaker":"A","text":"なるほど。洋食の時もあるのですか？"},{"id":{"d":242,"n":4,"t":1},"speaker":"B","text":"ええ、洋食の時は私と夫がコーヒーで娘がオレンジジュースです。息子はやっぱりプロテインですね。"},{"id":{"d":242,"n":5,"t":1},"speaker":"A","text":"息子さんはいつもプロテインなのですね。何かスポーツをしているのですか？"},{"id":{"d":242,"n":6,"t":1},"speaker":"B","text":"サッカーをしています。外国のサッカー選手のまねをして、プロテインを飲んでいるようです。"},{"id":{"d":242,"n":7,"t":1},"speaker":"A","text":"そうなのですね。"}];
INSERT INTO sentence [{"id":{"d":243,"n":1,"t":1},"speaker":"A","text":"お母さん、寝る時に寒いので、ストーブを使ってもよいですか？"},{"id":{"d":243,"n":2,"t":1},"speaker":"B","text":"石油ストーブは駄目ですよ。あなたの部屋は散らかっているので、火事になってしまいそうです。"},{"id":{"d":243,"n":3,"t":1},"speaker":"A","text":"そうですか。では、エアコンをつけてよいですか？"},{"id":{"d":243,"n":4,"t":1},"speaker":"B","text":"よいですよ。でも電気代が心配なので、寝るときには切タイマーをつけてくださいね。"},{"id":{"d":243,"n":5,"t":1},"speaker":"A","text":"分かりました。"},{"id":{"d":243,"n":6,"t":1},"speaker":"B","text":"できれば早く部屋を掃除して、石油ストーブを使える状態にしてほしいです。"},{"id":{"d":243,"n":7,"t":1},"speaker":"A","text":"そうですね、今週末までには部屋をきれいにしたいと思います。"},{"id":{"d":243,"n":8,"t":1},"speaker":"B","text":"それでは、それまでに石油を買っておきましょう。"}];
INSERT INTO sentence [{"id":{"d":244,"n":1,"t":1},"speaker":"A","text":"目玉焼きに、何をかけますか？"},{"id":{"d":244,"n":2,"t":1},"speaker":"B","text":"私はしょうゆをかけます。黄身にはしょうゆが合うと思います。"},{"id":{"d":244,"n":3,"t":1},"speaker":"A","text":"家族みんなもしょうゆなのですか？"},{"id":{"d":244,"n":4,"t":1},"speaker":"B","text":"妻はしょうゆですが、息子はケチャップ、娘はマヨネーズが多いです。おばあちゃんは塩ですし、おじいちゃんは塩コショウです。簡単な料理ですが食べ方は、いろいろあるようですよね。"}];
INSERT INTO sentence [{"id":{"d":245,"n":1,"t":1},"speaker":"A","text":"おじいちゃんの家に手土産を持って行きたいのですが、何かいいものはありますか？"},{"id":{"d":245,"n":2,"t":1},"speaker":"B","text":"家族で住んでいらっしゃるなら、お菓子がいいと思います。"},{"id":{"d":245,"n":3,"t":1},"speaker":"A","text":"おじいちゃん、おばあちゃんの2人家族です。和菓子は良さそうです。"},{"id":{"d":245,"n":4,"t":1},"speaker":"B","text":"名産品のお菓子があればそれもいいと思います。きっと喜ばれるでしょう。"},{"id":{"d":245,"n":5,"t":1},"speaker":"A","text":"なるほど。近所の和菓子屋さんを見てみます。"},{"id":{"d":245,"n":6,"t":1},"speaker":"B","text":"和菓子は賞味期限が短いものがありますので注意してくださいね。"},{"id":{"d":245,"n":7,"t":1},"speaker":"A","text":"確かにそうですね。気を付けます。"}];
