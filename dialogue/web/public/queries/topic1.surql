INSERT INTO sentence [{"id":{"d":544,"n":1,"t":1},"speaker":"A","text":"電子レンジの調子が悪いです。"},{"id":{"d":544,"n":2,"t":1},"speaker":"B","text":"どのように調子が悪いのですか？"},{"id":{"d":544,"n":3,"t":1},"speaker":"A","text":"スタートして時間がたっても、食品が温まりません。"},{"id":{"d":544,"n":4,"t":1},"speaker":"B","text":"電源は入っていますか？"},{"id":{"d":544,"n":5,"t":1},"speaker":"A","text":"もちろん、入っています。"},{"id":{"d":544,"n":6,"t":1},"speaker":"B","text":"この電子レンジは、いつ頃買ったか覚えていますか？"},{"id":{"d":544,"n":7,"t":1},"speaker":"A","text":"確か、6年前くらいに買いました。"},{"id":{"d":544,"n":8,"t":1},"speaker":"B","text":"それでは、故障しても仕方ないかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":545,"n":1,"t":1},"speaker":"A","text":"ティッシュペーパーが見当たりません。"},{"id":{"d":545,"n":2,"t":1},"speaker":"B","text":"いつも、リビングのテーブルの上に置いてありますよ。"},{"id":{"d":545,"n":3,"t":1},"speaker":"A","text":"そこにないのです。"},{"id":{"d":545,"n":4,"t":1},"speaker":"B","text":"誰かが使って、どこかへ置きっ放しにしたのかもしれませんね。"},{"id":{"d":545,"n":5,"t":1},"speaker":"A","text":"困りますね。"},{"id":{"d":545,"n":6,"t":1},"speaker":"B","text":"ソファの近くなどにありませんか？"},{"id":{"d":545,"n":7,"t":1},"speaker":"A","text":"そこはまだ見ていませんでした。ちょっと探してみます。"}];
INSERT INTO sentence [{"id":{"d":546,"n":1,"t":1},"speaker":"A","text":"そろそろ寝ましょうか？"},{"id":{"d":546,"n":2,"t":1},"speaker":"B","text":"はい。布団はもう敷いてありますか？"},{"id":{"d":546,"n":3,"t":1},"speaker":"A","text":"いえ、まだです。"},{"id":{"d":546,"n":4,"t":1},"speaker":"B","text":"それでは、敷いておきますね。"},{"id":{"d":546,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":546,"n":6,"t":1},"speaker":"B","text":"シーツはどこにありますか？"},{"id":{"d":546,"n":7,"t":1},"speaker":"A","text":"押し入れの中の棚に入れてあります。"},{"id":{"d":546,"n":8,"t":1},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":547,"n":1,"t":1},"speaker":"A","text":"トイレの電気がつきません。"},{"id":{"d":547,"n":2,"t":1},"speaker":"B","text":"電球が切れたようですね。"},{"id":{"d":547,"n":3,"t":1},"speaker":"A","text":"そうですね。新しい電球はありますか？"},{"id":{"d":547,"n":4,"t":1},"speaker":"B","text":"買い置きしていませんので、家にはありません。"},{"id":{"d":547,"n":5,"t":1},"speaker":"A","text":"それでは、私が買ってきましょうか？"},{"id":{"d":547,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":547,"n":7,"t":1},"speaker":"A","text":"どの電球を買えばよいか、分からなくなりそうです。"},{"id":{"d":547,"n":8,"t":1},"speaker":"B","text":"今付いている電球を外して持っていけば、同じものを買えると思います。"},{"id":{"d":547,"n":9,"t":1},"speaker":"A","text":"なるほど。今から外してきます。"}];
INSERT INTO sentence [{"id":{"d":548,"n":1,"t":1},"speaker":"A","text":"今度の月曜日は祝日ですね。"},{"id":{"d":548,"n":2,"t":1},"speaker":"B","text":"そうでしたか。何の日か、分かりますか？"},{"id":{"d":548,"n":3,"t":1},"speaker":"A","text":"いえ、分かりません。カレンダーを見ましょう。"},{"id":{"d":548,"n":4,"t":1},"speaker":"B","text":"海の日と書いてあります。"},{"id":{"d":548,"n":5,"t":1},"speaker":"A","text":"そういえば、そうでしたね。"},{"id":{"d":548,"n":6,"t":1},"speaker":"B","text":"山の日もありますよね。いつだか分かりますか？"},{"id":{"d":548,"n":7,"t":1},"speaker":"A","text":"8月だったような気がします。"},{"id":{"d":548,"n":8,"t":1},"speaker":"B","text":"カレンダーによると、8月11日です。"},{"id":{"d":548,"n":9,"t":1},"speaker":"A","text":"8月だという点は合っていましたね。"}];
INSERT INTO sentence [{"id":{"d":549,"n":1,"t":1},"speaker":"A","text":"漬物をどうぞ。"},{"id":{"d":549,"n":2,"t":1},"speaker":"B","text":"これは何の漬物ですか？"},{"id":{"d":549,"n":3,"t":1},"speaker":"A","text":"キュウリとナスのぬか漬けです。私が漬けたのです。"},{"id":{"d":549,"n":4,"t":1},"speaker":"B","text":"とてもおいしいです。"},{"id":{"d":549,"n":5,"t":1},"speaker":"A","text":"漬かり具合はどうですか？"},{"id":{"d":549,"n":6,"t":1},"speaker":"B","text":"ちょうどいいです。お上手ですね。"},{"id":{"d":549,"n":7,"t":1},"speaker":"A","text":"お口に合ってよかったです。"},{"id":{"d":549,"n":8,"t":1},"speaker":"B","text":"とてもおいしいです。漬け方のコツがあれば教えてください。"},{"id":{"d":549,"n":9,"t":1},"speaker":"A","text":"いいですよ。"}];
INSERT INTO sentence [{"id":{"d":550,"n":1,"t":1},"speaker":"A","text":"巻きずしですか？"},{"id":{"d":550,"n":2,"t":1},"speaker":"B","text":"はい。これから巻くところです。"},{"id":{"d":550,"n":3,"t":1},"speaker":"A","text":"私もお手伝いしてよいですか？"},{"id":{"d":550,"n":4,"t":1},"speaker":"B","text":"もちろん。ぜひ、手伝ってください。"},{"id":{"d":550,"n":5,"t":1},"speaker":"A","text":"まずは、のりに酢飯をのせますよね。"},{"id":{"d":550,"n":6,"t":1},"speaker":"B","text":"はい。あとは好きな具材をのせてください。"},{"id":{"d":550,"n":7,"t":1},"speaker":"A","text":"これでどうでしょう？"},{"id":{"d":550,"n":8,"t":1},"speaker":"B","text":"いいと思います。巻きすでしっかり巻いてくださいね。"}];
INSERT INTO sentence [{"id":{"d":551,"n":1,"t":1},"speaker":"A","text":"すみません。先月発行された雑誌を買いたいのですが。"},{"id":{"d":551,"n":2,"t":1},"speaker":"B","text":"先月号ですと、出版社に注文して取り寄せることになりますが、よろしいですか？"},{"id":{"d":551,"n":3,"t":1},"speaker":"A","text":"出版社に在庫はあるでしょうか？"},{"id":{"d":551,"n":4,"t":1},"speaker":"B","text":"確認しますので、少々お待ちください。"},{"id":{"d":551,"n":5,"t":1},"speaker":"A","text":"どうでしたか？"},{"id":{"d":551,"n":6,"t":1},"speaker":"B","text":"在庫はあるそうです。注文しますか？"},{"id":{"d":551,"n":7,"t":1},"speaker":"A","text":"お願いします。"},{"id":{"d":551,"n":8,"t":1},"speaker":"B","text":"こちらの用紙に連絡先をご記入ください。"},{"id":{"d":551,"n":9,"t":1},"speaker":"A","text":"はい。分かりました。"}];
INSERT INTO sentence [{"id":{"d":552,"n":1,"t":1},"speaker":"A","text":"ちょっとおなかがすきました。"},{"id":{"d":552,"n":2,"t":1},"speaker":"B","text":"夕食にはまだ早い時間ですよ。"},{"id":{"d":552,"n":3,"t":1},"speaker":"A","text":"何か少しだけ食べたいです。"},{"id":{"d":552,"n":4,"t":1},"speaker":"B","text":"冷蔵庫に何か入っているかもしれません。"},{"id":{"d":552,"n":5,"t":1},"speaker":"A","text":"プリンがあります。食べてもよいですか？"},{"id":{"d":552,"n":6,"t":1},"speaker":"B","text":"それはパパの分なので、取っておいてください。"},{"id":{"d":552,"n":7,"t":1},"speaker":"A","text":"では、イチゴは食べてもよいですか？"},{"id":{"d":552,"n":8,"t":1},"speaker":"B","text":"いいですよ。洗ってから食べてくださいね。"}];
INSERT INTO sentence [{"id":{"d":553,"n":1,"t":1},"speaker":"A","text":"和室の畳が傷んできましたね。"},{"id":{"d":553,"n":2,"t":1},"speaker":"B","text":"そうですね。色もあせてきました。"},{"id":{"d":553,"n":3,"t":1},"speaker":"A","text":"畳の表替えが必要ですね。"},{"id":{"d":553,"n":4,"t":1},"speaker":"B","text":"畳の裏返しで大丈夫ですよ。"},{"id":{"d":553,"n":5,"t":1},"speaker":"A","text":"畳を裏返すのですか？"},{"id":{"d":553,"n":6,"t":1},"speaker":"B","text":"畳は両面使えるので、裏返すときれいな面が表になるのです。"},{"id":{"d":553,"n":7,"t":1},"speaker":"A","text":"知りませんでした。それでは裏返しましょう。"},{"id":{"d":553,"n":8,"t":1},"speaker":"B","text":"業者さんに依頼しておきますね。"}];
INSERT INTO sentence [{"id":{"d":554,"n":1,"t":1},"speaker":"A","text":"回転ずしに来るのは久しぶりです。"},{"id":{"d":554,"n":2,"t":1},"speaker":"B","text":"私も半年以上来ていません。"},{"id":{"d":554,"n":3,"t":1},"speaker":"A","text":"まずは何を食べますか？"},{"id":{"d":554,"n":4,"t":1},"speaker":"B","text":"マグロがいいです。"},{"id":{"d":554,"n":5,"t":1},"speaker":"A","text":"赤身ですか？"},{"id":{"d":554,"n":6,"t":1},"speaker":"B","text":"せっかくなので、大トロにします。"},{"id":{"d":554,"n":7,"t":1},"speaker":"A","text":"いいですね。ちょうど、大トロが来ましたよ。"},{"id":{"d":554,"n":8,"t":1},"speaker":"B","text":"では、取りますね。おいしそうです。"}];
INSERT INTO sentence [{"id":{"d":555,"n":1,"t":1},"speaker":"A","text":"飲み物がなくなりそうですよ。何か注文しますか？"},{"id":{"d":555,"n":2,"t":1},"speaker":"B","text":"ビールをお代わりします。"},{"id":{"d":555,"n":3,"t":1},"speaker":"A","text":"一緒に、おつまみを頼みましょうか？"},{"id":{"d":555,"n":4,"t":1},"speaker":"B","text":"そうですね。メニューを見ましょう。"},{"id":{"d":555,"n":5,"t":1},"speaker":"A","text":"エダマメはどうですか？"},{"id":{"d":555,"n":6,"t":1},"speaker":"B","text":"ビールに合うので頼みましょう。あと、だし巻き卵が食べたいです。"},{"id":{"d":555,"n":7,"t":1},"speaker":"A","text":"それでは、ビールとその2品を注文しましょう。"},{"id":{"d":555,"n":8,"t":1},"speaker":"B","text":"あなたもビールをお代わりしますか？"},{"id":{"d":555,"n":9,"t":1},"speaker":"A","text":"私はまだ大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":556,"n":1,"t":1},"speaker":"A","text":"振り込みをお願いします。"},{"id":{"d":556,"n":2,"t":1},"speaker":"B","text":"振込用紙はお持ちですか？"},{"id":{"d":556,"n":3,"t":1},"speaker":"A","text":"これです。"},{"id":{"d":556,"n":4,"t":1},"speaker":"B","text":"こちらは、所定の振込手数料がかかりますが、よろしいですか？"},{"id":{"d":556,"n":5,"t":1},"speaker":"A","text":"手数料はいくらですか？"},{"id":{"d":556,"n":6,"t":1},"speaker":"B","text":"250円です。"},{"id":{"d":556,"n":7,"t":1},"speaker":"A","text":"分かりました。お願いします。"},{"id":{"d":556,"n":8,"t":1},"speaker":"B","text":"処理をいたしますので、お掛けになってお待ちください。"}];
INSERT INTO sentence [{"id":{"d":557,"n":1,"t":1},"speaker":"A","text":"寒くて足先が冷たくなってきました。"},{"id":{"d":557,"n":2,"t":1},"speaker":"B","text":"暖房をつけましょうか？"},{"id":{"d":557,"n":3,"t":1},"speaker":"A","text":"そうしましょう。エアコンをつけますか？それとも床暖房にしますか？"},{"id":{"d":557,"n":4,"t":1},"speaker":"B","text":"足元から温めたいので、床暖房がいいです。"},{"id":{"d":557,"n":5,"t":1},"speaker":"A","text":"そうですね。では床暖房をつけますね。"},{"id":{"d":557,"n":6,"t":1},"speaker":"B","text":"床暖房をつけるのは、今年初めてですね。"},{"id":{"d":557,"n":7,"t":1},"speaker":"A","text":"ええ。冬になったことを実感しますね。"},{"id":{"d":557,"n":8,"t":1},"speaker":"B","text":"冬は苦手なので、早く春になってほしいです。"}];
INSERT INTO sentence [{"id":{"d":558,"n":1,"t":1},"speaker":"A","text":"今、揺れませんでしたか？"},{"id":{"d":558,"n":2,"t":1},"speaker":"B","text":"少し揺れていますね。"},{"id":{"d":558,"n":3,"t":1},"speaker":"A","text":"地震でしょうか？"},{"id":{"d":558,"n":4,"t":1},"speaker":"B","text":"そのようですね。落ち着いて行動しましょう。"},{"id":{"d":558,"n":5,"t":1},"speaker":"A","text":"収まったようですね。"},{"id":{"d":558,"n":6,"t":1},"speaker":"B","text":"大したことがなくてよかったです。"},{"id":{"d":558,"n":7,"t":1},"speaker":"A","text":"テレビで地震速報を確認します。"},{"id":{"d":558,"n":8,"t":1},"speaker":"B","text":"私はインターネットで情報を調べます。"}];
INSERT INTO sentence [{"id":{"d":559,"n":1,"t":1},"speaker":"A","text":"このスカートの上にこのセーターを着ると合わないでしょうか？"},{"id":{"d":559,"n":2,"t":1},"speaker":"B","text":"いいえ、合っていますよ。"},{"id":{"d":559,"n":3,"t":1},"speaker":"A","text":"このカーディガンにしようかとも思うのですが。"},{"id":{"d":559,"n":4,"t":1},"speaker":"B","text":"そのカーディガンでも、スカートに合いますね。"},{"id":{"d":559,"n":5,"t":1},"speaker":"A","text":"どちらがいいと思いますか？"},{"id":{"d":559,"n":6,"t":1},"speaker":"B","text":"どちらも合っていますが、強いて言えばカーディガンでしょうか？"},{"id":{"d":559,"n":7,"t":1},"speaker":"A","text":"そうですか。カーディガンの下にシャツを着るとしたら、何色がいいと思いますか？"},{"id":{"d":559,"n":8,"t":1},"speaker":"B","text":"シャツは白が無難だと思います。"}];
INSERT INTO sentence [{"id":{"d":560,"n":1,"t":1},"speaker":"A","text":"小倉さんは、1人暮らしですね。どのような間取りですか？"},{"id":{"d":560,"n":2,"t":1},"speaker":"B","text":"ワンルームです。"},{"id":{"d":560,"n":3,"t":1},"speaker":"A","text":"トイレとお風呂は別々ですか？"},{"id":{"d":560,"n":4,"t":1},"speaker":"B","text":"はい。それを条件にして、部屋を探しました。"},{"id":{"d":560,"n":5,"t":1},"speaker":"A","text":"キッチンはどのような感じですか？"},{"id":{"d":560,"n":6,"t":1},"speaker":"B","text":"狭いですが、2口コンロがあって、冷蔵庫と小さい食器棚を置いています。"},{"id":{"d":560,"n":7,"t":1},"speaker":"A","text":"料理はしますか？"},{"id":{"d":560,"n":8,"t":1},"speaker":"B","text":"料理は好きなので、基本的に自炊しています。"}];
INSERT INTO sentence [{"id":{"d":561,"n":1,"t":1},"speaker":"A","text":"随分と厚着していますね。"},{"id":{"d":561,"n":2,"t":1},"speaker":"B","text":"部屋でじっとしていたせいか、とても寒いです。"},{"id":{"d":561,"n":3,"t":1},"speaker":"A","text":"何か温かいものを飲みますか？"},{"id":{"d":561,"n":4,"t":1},"speaker":"B","text":"そうですね。紅茶を入れて飲みます。"},{"id":{"d":561,"n":5,"t":1},"speaker":"A","text":"ちょうど私も飲もうと思っていたところなので、私が用意します。"},{"id":{"d":561,"n":6,"t":1},"speaker":"B","text":"いいのですか？ありがとうございます。"},{"id":{"d":561,"n":7,"t":1},"speaker":"A","text":"ミルクティーにしますか？それとも、レモンティーがいいですか？"},{"id":{"d":561,"n":8,"t":1},"speaker":"B","text":"ミルクティーをお願いします。"}];
INSERT INTO sentence [{"id":{"d":562,"n":1,"t":1},"speaker":"A","text":"和也、部屋が散らかっていますよ。"},{"id":{"d":562,"n":2,"t":1},"speaker":"B","text":"すみません。"},{"id":{"d":562,"n":3,"t":1},"speaker":"A","text":"少し片付けてください。"},{"id":{"d":562,"n":4,"t":1},"speaker":"B","text":"今はテスト前で忙しくて、時間がありません。"},{"id":{"d":562,"n":5,"t":1},"speaker":"A","text":"これほど散らかっていると、勉強に集中できませんよ。"},{"id":{"d":562,"n":6,"t":1},"speaker":"B","text":"机の上は整理しているので、大丈夫です。"},{"id":{"d":562,"n":7,"t":1},"speaker":"A","text":"テストが終わったら、片付けると約束してください。"},{"id":{"d":562,"n":8,"t":1},"speaker":"B","text":"分かりました。必ずきれいにします。"}];
INSERT INTO sentence [{"id":{"d":563,"n":1,"t":1},"speaker":"A","text":"包丁の切れ味が悪いです。"},{"id":{"d":563,"n":2,"t":1},"speaker":"B","text":"久しぶりに、研ぎましょうか？"},{"id":{"d":563,"n":3,"t":1},"speaker":"A","text":"すみませんが、お願いします。"},{"id":{"d":563,"n":4,"t":1},"speaker":"B","text":"包丁を研ぐのは割と好きなので、気にしないでください。"},{"id":{"d":563,"n":5,"t":1},"speaker":"A","text":"ついでに、果物ナイフも研いでもらえますか？"},{"id":{"d":563,"n":6,"t":1},"speaker":"B","text":"お安いご用です。果物ナイフはどこにありますか？"},{"id":{"d":563,"n":7,"t":1},"speaker":"A","text":"シンクの引き出しに入っているので持ってきます。"},{"id":{"d":563,"n":8,"t":1},"speaker":"B","text":"では、僕は砥石を出してきます。"}];
INSERT INTO sentence [{"id":{"d":564,"n":1,"t":1},"speaker":"A","text":"ポストにお知らせが入っていました。"},{"id":{"d":564,"n":2,"t":1},"speaker":"B","text":"何が書いてありますか？"},{"id":{"d":564,"n":3,"t":1},"speaker":"A","text":"マンションのエレベーターの定期点検があるそうです。"},{"id":{"d":564,"n":4,"t":1},"speaker":"B","text":"点検中は、エレベーターが使えなくなるのでしょうか？"},{"id":{"d":564,"n":5,"t":1},"speaker":"A","text":"そのように書いてありますね。"},{"id":{"d":564,"n":6,"t":1},"speaker":"B","text":"階段を使うのは大変なので、点検中は外出しないようにしましょう。"},{"id":{"d":564,"n":7,"t":1},"speaker":"A","text":"点検の日時を忘れないようにする必要がありますね。"},{"id":{"d":564,"n":8,"t":1},"speaker":"B","text":"そうですね。カレンダーにメモしておいてくれませんか？"},{"id":{"d":564,"n":9,"t":1},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":565,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。お部屋をお探しですか？"},{"id":{"d":565,"n":2,"t":1},"speaker":"B","text":"はい。1人暮らし用の部屋を探しています。"},{"id":{"d":565,"n":3,"t":1},"speaker":"A","text":"間取りはワンルームか1DKくらいでお考えですか？"},{"id":{"d":565,"n":4,"t":1},"speaker":"B","text":"できれば1DKがいいです。"},{"id":{"d":565,"n":5,"t":1},"speaker":"A","text":"駅からの距離はどのくらいまで大丈夫ですか？"},{"id":{"d":565,"n":6,"t":1},"speaker":"B","text":"歩いて20分くらいまでにしたいです。"},{"id":{"d":565,"n":7,"t":1},"speaker":"A","text":"家賃はどのくらいをご希望でしょうか？"},{"id":{"d":565,"n":8,"t":1},"speaker":"B","text":"最大で7万円くらいで、なるべく安く抑えたいです。"},{"id":{"d":565,"n":9,"t":1},"speaker":"A","text":"かしこまりました。条件に合う物件の情報を検索しますので、少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":566,"n":1,"t":1},"speaker":"A","text":"もし別荘を建てるなら、どこにしますか？"},{"id":{"d":566,"n":2,"t":1},"speaker":"B","text":"北海道がいいです。"},{"id":{"d":566,"n":3,"t":1},"speaker":"A","text":"夏の間を、涼しい北海道の別荘で過ごすという考えですか？"},{"id":{"d":566,"n":4,"t":1},"speaker":"B","text":"その通りです、季節によって住む場所を変えたいです。"},{"id":{"d":566,"n":5,"t":1},"speaker":"A","text":"私は外国に別荘が欲しいです。"},{"id":{"d":566,"n":6,"t":1},"speaker":"B","text":"具体的には、どの国がいいですか？"},{"id":{"d":566,"n":7,"t":1},"speaker":"A","text":"ニュージーランドです。"},{"id":{"d":566,"n":8,"t":1},"speaker":"B","text":"ニュージーランドは自然が豊かで良いですね。"}];
INSERT INTO sentence [{"id":{"d":567,"n":1,"t":1},"speaker":"A","text":"もしもし、ピザの宅配をお願いします。"},{"id":{"d":567,"n":2,"t":1},"speaker":"B","text":"ご注文の品を承ります。"},{"id":{"d":567,"n":3,"t":1},"speaker":"A","text":"シーフードピザをお願いします。"},{"id":{"d":567,"n":4,"t":1},"speaker":"B","text":"サイズはどうしますか？"},{"id":{"d":567,"n":5,"t":1},"speaker":"A","text":"Lサイズでお願いします。"},{"id":{"d":567,"n":6,"t":1},"speaker":"B","text":"他にご注文はありますか？"},{"id":{"d":567,"n":7,"t":1},"speaker":"A","text":"ポテトのMサイズをお願いします。"},{"id":{"d":567,"n":8,"t":1},"speaker":"B","text":"かしこまりました。お届けする住所をお願いします。"}];
INSERT INTO sentence [{"id":{"d":568,"n":1,"t":1},"speaker":"A","text":"このスニーカーは、他のサイズもありますか？"},{"id":{"d":568,"n":2,"t":1},"speaker":"B","text":"どのサイズをお探しですか？"},{"id":{"d":568,"n":3,"t":1},"speaker":"A","text":"24センチメートルです。"},{"id":{"d":568,"n":4,"t":1},"speaker":"B","text":"こちらにございます。"},{"id":{"d":568,"n":5,"t":1},"speaker":"A","text":"試しに履いてもよいですか？"},{"id":{"d":568,"n":6,"t":1},"speaker":"B","text":"どうぞ。この椅子で履き替えてください。"},{"id":{"d":568,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":568,"n":8,"t":1},"speaker":"B","text":"いかがでしょうか？"},{"id":{"d":568,"n":9,"t":1},"speaker":"A","text":"ぴったりです。このスニーカーに決めます。"}];
INSERT INTO sentence [{"id":{"d":569,"n":1,"t":1},"speaker":"A","text":"麺類では何が好きですか？"},{"id":{"d":569,"n":2,"t":1},"speaker":"B","text":"たくさんありますから、迷いますね。あなたは何が好きですか？"},{"id":{"d":569,"n":3,"t":1},"speaker":"A","text":"私はうどんが好きです。"},{"id":{"d":569,"n":4,"t":1},"speaker":"B","text":"うどんもおいしいですよね。それに、そばも好きだし、ラーメンも好きです。"},{"id":{"d":569,"n":5,"t":1},"speaker":"A","text":"好きなものが多くて決められませんか？"},{"id":{"d":569,"n":6,"t":1},"speaker":"B","text":"その通りです。"},{"id":{"d":569,"n":7,"t":1},"speaker":"A","text":"それでは、今日のお昼に食べたい麺類なら決められますか？"},{"id":{"d":569,"n":8,"t":1},"speaker":"B","text":"それなら決められます。今日の気分はパスタです。"}];
INSERT INTO sentence [{"id":{"d":570,"n":1,"t":1},"speaker":"A","text":"ご注文はお決まりですか？"},{"id":{"d":570,"n":2,"t":1},"speaker":"B","text":"チョコミントとバニラをお願いします。"},{"id":{"d":570,"n":3,"t":1},"speaker":"A","text":"ダブルですね。コーンとカップはどちらがいいですか？"},{"id":{"d":570,"n":4,"t":1},"speaker":"B","text":"コーンがいいです。"},{"id":{"d":570,"n":5,"t":1},"speaker":"A","text":"かしこまりました。あちらでお会計をお願いします。"},{"id":{"d":570,"n":6,"t":1},"speaker":"B","text":"スプーンを2つ頂いてもよいですか？"},{"id":{"d":570,"n":7,"t":1},"speaker":"A","text":"分かりました。2つお付けしますね。"},{"id":{"d":570,"n":8,"t":1},"speaker":"B","text":"どうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":571,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。ご注文がお決まりでしたらお声がけください。"},{"id":{"d":571,"n":2,"t":1},"speaker":"B","text":"チョコレートケーキ2つと、チーズケーキを1つください。"},{"id":{"d":571,"n":3,"t":1},"speaker":"A","text":"かしこまりました。合計3つでよろしいですか？"},{"id":{"d":571,"n":4,"t":1},"speaker":"B","text":"あと、プリンを2つ追加してください。"},{"id":{"d":571,"n":5,"t":1},"speaker":"A","text":"それでは、合計5個ですね。"},{"id":{"d":571,"n":6,"t":1},"speaker":"B","text":"はい。それでお願いします。"},{"id":{"d":571,"n":7,"t":1},"speaker":"A","text":"ドライアイスはお付けしますか？"},{"id":{"d":571,"n":8,"t":1},"speaker":"B","text":"そうですね。付けてください。"}];
INSERT INTO sentence [{"id":{"d":572,"n":1,"t":1},"speaker":"A","text":"明日は早起きしなければいけませんが、起きられるかどうか心配です。"},{"id":{"d":572,"n":2,"t":1},"speaker":"B","text":"目覚ましのアラームをセットしておけば、起きられるのではありませんか？"},{"id":{"d":572,"n":3,"t":1},"speaker":"A","text":"アラームが鳴っても、知らないうちに消してしまうのです。"},{"id":{"d":572,"n":4,"t":1},"speaker":"B","text":"それは困りますね。"},{"id":{"d":572,"n":5,"t":1},"speaker":"A","text":"本当に早起きが苦手で、苦労しています。"},{"id":{"d":572,"n":6,"t":1},"speaker":"B","text":"明日の朝、モーニングコールをしてあげましょうか？"},{"id":{"d":572,"n":7,"t":1},"speaker":"A","text":"本当ですか？ご迷惑ではありませんか？"},{"id":{"d":572,"n":8,"t":1},"speaker":"B","text":"私は早起きが苦になりませんので、大丈夫ですよ。"},{"id":{"d":572,"n":9,"t":1},"speaker":"A","text":"それでは、お言葉に甘えてお願いします。"}];
INSERT INTO sentence [{"id":{"d":573,"n":1,"t":1},"speaker":"A","text":"バス停がありました。ここからバスに乗りましょう。"},{"id":{"d":573,"n":2,"t":1},"speaker":"B","text":"次のバスはいつ頃来るでしょうか？"},{"id":{"d":573,"n":3,"t":1},"speaker":"A","text":"ここに時刻表があるので見てみましょう。"},{"id":{"d":573,"n":4,"t":1},"speaker":"B","text":"路線によって、時間が違うようですね。"},{"id":{"d":573,"n":5,"t":1},"speaker":"A","text":"私たちが乗るのは、34番という路線です。"},{"id":{"d":573,"n":6,"t":1},"speaker":"B","text":"34番だと、10分後に来ます。"},{"id":{"d":573,"n":7,"t":1},"speaker":"A","text":"あまり待たずに乗れそうですね。"},{"id":{"d":573,"n":8,"t":1},"speaker":"B","text":"ラッキーですね。"}];
INSERT INTO sentence [{"id":{"d":574,"n":1,"t":1},"speaker":"A","text":"具合が悪そうですね。大丈夫ですか？"},{"id":{"d":574,"n":2,"t":1},"speaker":"B","text":"少しおなかが痛いです。"},{"id":{"d":574,"n":3,"t":1},"speaker":"A","text":"胃の辺りですか？"},{"id":{"d":574,"n":4,"t":1},"speaker":"B","text":"はい。お昼ご飯の後からずっと痛みがあります。"},{"id":{"d":574,"n":5,"t":1},"speaker":"A","text":"病院に行きますか？"},{"id":{"d":574,"n":6,"t":1},"speaker":"B","text":"いえ、それほど深刻ではありませんので大丈夫です。"},{"id":{"d":574,"n":7,"t":1},"speaker":"A","text":"それではしばらく、休憩室で休んでいてください。"},{"id":{"d":574,"n":8,"t":1},"speaker":"B","text":"すみませんが、そうさせてもらいます。"},{"id":{"d":574,"n":9,"t":1},"speaker":"A","text":"気にしないでください。"}];
INSERT INTO sentence [{"id":{"d":575,"n":1,"t":1},"speaker":"A","text":"郵送したい物があるのですが、ここから一番近いポストはどこにありますか？"},{"id":{"d":575,"n":2,"t":1},"speaker":"B","text":"区役所のある交差点にありますよ。"},{"id":{"d":575,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。投函してきますね。"},{"id":{"d":575,"n":4,"t":1},"speaker":"B","text":"それを送りたいのですか？その大きさだと、ポストに入らないと思いますよ。"},{"id":{"d":575,"n":5,"t":1},"speaker":"A","text":"そうですか。それでは、郵便局から送ることにします。"},{"id":{"d":575,"n":6,"t":1},"speaker":"B","text":"郵便局は、少し離れた所にあります。"},{"id":{"d":575,"n":7,"t":1},"speaker":"A","text":"すみませんが、地図を書いてもらえませんか？"},{"id":{"d":575,"n":8,"t":1},"speaker":"B","text":"いいですよ。"}];
INSERT INTO sentence [{"id":{"d":576,"n":1,"t":1},"speaker":"A","text":"貯金箱がいっぱいになってきました。"},{"id":{"d":576,"n":2,"t":1},"speaker":"B","text":"いつから貯めているのですか？"},{"id":{"d":576,"n":3,"t":1},"speaker":"A","text":"3年くらい前からです。"},{"id":{"d":576,"n":4,"t":1},"speaker":"B","text":"それは頑張りましたね。"},{"id":{"d":576,"n":5,"t":1},"speaker":"A","text":"500円玉だけ貯めていったのです。"},{"id":{"d":576,"n":6,"t":1},"speaker":"B","text":"この貯金箱の中身が全部500円玉ですか？"},{"id":{"d":576,"n":7,"t":1},"speaker":"A","text":"はい。かなりの金額になっているはずです。"},{"id":{"d":576,"n":8,"t":1},"speaker":"B","text":"開ける日が楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":577,"n":1,"t":1},"speaker":"A","text":"今朝はトーストを食べましょう。"},{"id":{"d":577,"n":2,"t":1},"speaker":"B","text":"はい。ちょうど、パンを食べたい気分でした。"},{"id":{"d":577,"n":3,"t":1},"speaker":"A","text":"焼き加減はどのくらいが好みですか？"},{"id":{"d":577,"n":4,"t":1},"speaker":"B","text":"しっかり焼いてある方が好きです。"},{"id":{"d":577,"n":5,"t":1},"speaker":"A","text":"私もこんがり焼く方が好きです。私は焼く前にバターを塗りますが、どうしますか？"},{"id":{"d":577,"n":6,"t":1},"speaker":"B","text":"私はそのままでお願いします。"},{"id":{"d":577,"n":7,"t":1},"speaker":"A","text":"何も付けないのですか？"},{"id":{"d":577,"n":8,"t":1},"speaker":"B","text":"はい。何も付けないトーストが好きなのです。"}];
INSERT INTO sentence [{"id":{"d":578,"n":1,"t":1},"speaker":"A","text":"ママ、ハンバーグの作り方を教えてください。"},{"id":{"d":578,"n":2,"t":1},"speaker":"B","text":"いいですよ。まずは玉ネギをみじん切りにします。"},{"id":{"d":578,"n":3,"t":1},"speaker":"A","text":"目が痛くなりそうです。"},{"id":{"d":578,"n":4,"t":1},"speaker":"B","text":"そうですね。ママも玉ネギをみじん切りにすると涙が出てくることがあります。"},{"id":{"d":578,"n":5,"t":1},"speaker":"A","text":"我慢するしかないのでしょうか？"},{"id":{"d":578,"n":6,"t":1},"speaker":"B","text":"サングラスでもかけてみますか？"},{"id":{"d":578,"n":7,"t":1},"speaker":"A","text":"何もしないよりはましかもしれませんね。うちにサングラスはありますか？"},{"id":{"d":578,"n":8,"t":1},"speaker":"B","text":"パパが持っているので、頼めば貸してくれると思いますよ。"}];
INSERT INTO sentence [{"id":{"d":579,"n":1,"t":1},"speaker":"A","text":"ママ、ゲームをしてよいですか？"},{"id":{"d":579,"n":2,"t":1},"speaker":"B","text":"宿題は終わりましたか？"},{"id":{"d":579,"n":3,"t":1},"speaker":"A","text":"はい。もう全部終わらせて、明日の準備もしました。"},{"id":{"d":579,"n":4,"t":1},"speaker":"B","text":"頑張りましたね。それでは夕飯まで1時間ゲームをしていてよいですよ。"},{"id":{"d":579,"n":5,"t":1},"speaker":"A","text":"1時間では短いです。"},{"id":{"d":579,"n":6,"t":1},"speaker":"B","text":"夕食後にお皿洗いをしてくれたら、その後でまたゲームをしてよいですよ。"},{"id":{"d":579,"n":7,"t":1},"speaker":"A","text":"お皿洗いの後は、また1時間ゲームをしてよいですか？"},{"id":{"d":579,"n":8,"t":1},"speaker":"B","text":"はい。頑張ってお手伝いしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":580,"n":1,"t":1},"speaker":"A","text":"もっときれいな字を書けるようになりたいです。"},{"id":{"d":580,"n":2,"t":1},"speaker":"B","text":"あなたの字は汚くありませんよ。"},{"id":{"d":580,"n":3,"t":1},"speaker":"A","text":"そうでしょうか。自分では気に入りません。"},{"id":{"d":580,"n":4,"t":1},"speaker":"B","text":"急いで書いているのではありませんか？"},{"id":{"d":580,"n":5,"t":1},"speaker":"A","text":"授業でノートをとる時は、時間がありませんのでささっと書いていますね。"},{"id":{"d":580,"n":6,"t":1},"speaker":"B","text":"ゆっくり書くことを意識するだけでも、かなりきれいに書けますよ。"},{"id":{"d":580,"n":7,"t":1},"speaker":"A","text":"なるほど。ちょっと書いてみます。どうでしょうか？"},{"id":{"d":580,"n":8,"t":1},"speaker":"B","text":"とてもきれいに書けていますよ。"}];
INSERT INTO sentence [{"id":{"d":581,"n":1,"t":1},"speaker":"A","text":"木下さんは、箸の上の方を持ちますね。"},{"id":{"d":581,"n":2,"t":1},"speaker":"B","text":"そうですね。いつも、この辺りで持ちます。"},{"id":{"d":581,"n":3,"t":1},"speaker":"A","text":"箸の上の方を持つ人は、遠くにお嫁に行くと言われています。"},{"id":{"d":581,"n":4,"t":1},"speaker":"B","text":"聞いたことがあります。箸を下で持つ人は、近くにお嫁に行くといわれますね。"},{"id":{"d":581,"n":5,"t":1},"speaker":"A","text":"木下さんは遠くへお嫁に行くかもしれませんね。"},{"id":{"d":581,"n":6,"t":1},"speaker":"B","text":"榊さんはどの位置で箸を持ちますか？"},{"id":{"d":581,"n":7,"t":1},"speaker":"A","text":"この辺です。割と下の方です。"},{"id":{"d":581,"n":8,"t":1},"speaker":"B","text":"それでは榊さんは近くにお嫁に行くようですね。"},{"id":{"d":581,"n":9,"t":1},"speaker":"A","text":"もし当たったら驚きですね。"}];
INSERT INTO sentence [{"id":{"d":582,"n":1,"t":1},"speaker":"A","text":"大樹、箸の持ち方がおかしいですよ。"},{"id":{"d":582,"n":2,"t":1},"speaker":"B","text":"おかしいですか？"},{"id":{"d":582,"n":3,"t":1},"speaker":"A","text":"箸が交差しています。"},{"id":{"d":582,"n":4,"t":1},"speaker":"B","text":"どうすればよいでしょうか？"},{"id":{"d":582,"n":5,"t":1},"speaker":"A","text":"正しい持ち方を教えるので、見てください。"},{"id":{"d":582,"n":6,"t":1},"speaker":"B","text":"なるほど。箸が交わらないように持つのですね。"},{"id":{"d":582,"n":7,"t":1},"speaker":"A","text":"そうです。やってみてください。"},{"id":{"d":582,"n":8,"t":1},"speaker":"B","text":"こうでしょうか？"},{"id":{"d":582,"n":9,"t":1},"speaker":"A","text":"合っています。気を付けて、正しい持ち方を身に付けましょう。"}];
INSERT INTO sentence [{"id":{"d":583,"n":1,"t":1},"speaker":"A","text":"美穂、制服が脱ぎっ放しですよ。"},{"id":{"d":583,"n":2,"t":1},"speaker":"B","text":"そうでしたね。"},{"id":{"d":583,"n":3,"t":1},"speaker":"A","text":"片付けてください。"},{"id":{"d":583,"n":4,"t":1},"speaker":"B","text":"疲れているので、後でやります。"},{"id":{"d":583,"n":5,"t":1},"speaker":"A","text":"ここに制服があると邪魔になるので、すぐにやってください。"},{"id":{"d":583,"n":6,"t":1},"speaker":"B","text":"本当にくたくたなのです。後で必ずやります。"},{"id":{"d":583,"n":7,"t":1},"speaker":"A","text":"では、あなたの部屋に置いておくので、ハンガーには自分で掛けてください。"},{"id":{"d":583,"n":8,"t":1},"speaker":"B","text":"はい。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":584,"n":1,"t":1},"speaker":"A","text":"ママ、この漢字は何と読むのですか？"},{"id":{"d":584,"n":2,"t":1},"speaker":"B","text":"これは、タンポポと読みます。"},{"id":{"d":584,"n":3,"t":1},"speaker":"A","text":"これでタンポポと読むのですか？想像もつきませんでした。"},{"id":{"d":584,"n":4,"t":1},"speaker":"B","text":"漢字から受けるイメージとタンポポのイメージが異なっていますよね。"},{"id":{"d":584,"n":5,"t":1},"speaker":"A","text":"ママはどうして読めたのですか？"},{"id":{"d":584,"n":6,"t":1},"speaker":"B","text":"前に本に出てきて、辞書を引いて調べたからです。"},{"id":{"d":584,"n":7,"t":1},"speaker":"A","text":"そうやって自分で調べると、記憶に残るのかもしれませんね。"},{"id":{"d":584,"n":8,"t":1},"speaker":"B","text":"はい。あなたもこれからは辞書で調べてみるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":585,"n":1,"t":1},"speaker":"A","text":"今日は学校で、自分の名前の由来を調べるという宿題が出ました。"},{"id":{"d":585,"n":2,"t":1},"speaker":"B","text":"そういえば、あなたの「日和」という名前の由来を話したことがありませんね。"},{"id":{"d":585,"n":3,"t":1},"speaker":"A","text":"はい。ぜひ教えてください。"},{"id":{"d":585,"n":4,"t":1},"speaker":"B","text":"お日さまのように暖かくて、周りのみんなに元気を与えるような人になってほしいという願いを込めて名付けました。"},{"id":{"d":585,"n":5,"t":1},"speaker":"A","text":"そうだったのですね。お父さんとお母さんのどちらが考えたのですか？"},{"id":{"d":585,"n":6,"t":1},"speaker":"B","text":"2人で話し合って決めました。"},{"id":{"d":585,"n":7,"t":1},"speaker":"A","text":"由来を知って、自分の名前がますます好きになりました。"},{"id":{"d":585,"n":8,"t":1},"speaker":"B","text":"そう言ってもらえると、お父さんもうれしいです。"}];
INSERT INTO sentence [{"id":{"d":586,"n":1,"t":1},"speaker":"A","text":"家にスマートフォンを置いてきてしまいました。"},{"id":{"d":586,"n":2,"t":1},"speaker":"B","text":"それは困りましたね。いつ気が付いたのですか？"},{"id":{"d":586,"n":3,"t":1},"speaker":"A","text":"電車に乗ってからです。"},{"id":{"d":586,"n":4,"t":1},"speaker":"B","text":"それでは、取りに帰ることもできませんでしたね。"},{"id":{"d":586,"n":5,"t":1},"speaker":"A","text":"はい。諦めて出社しましたが、電話がかかってきたりメールが届いたりしていないか心配です。"},{"id":{"d":586,"n":6,"t":1},"speaker":"B","text":"仕事にも支障があるのではありませんか？"},{"id":{"d":586,"n":7,"t":1},"speaker":"A","text":"そうですね。業務上の連絡が入る場合もあるので、本当に困りました。"},{"id":{"d":586,"n":8,"t":1},"speaker":"B","text":"課長に事情を話して、取りに帰ってはどうですか？"},{"id":{"d":586,"n":9,"t":1},"speaker":"A","text":"そうします。今から上司に相談してきます。"}];
INSERT INTO sentence [{"id":{"d":587,"n":1,"t":1},"speaker":"A","text":"結婚式の招待状が届きました。"},{"id":{"d":587,"n":2,"t":1},"speaker":"B","text":"友達が結婚するのですか？"},{"id":{"d":587,"n":3,"t":1},"speaker":"A","text":"はい。大学の友達です。"},{"id":{"d":587,"n":4,"t":1},"speaker":"B","text":"結婚する相手も知っている人ですか？"},{"id":{"d":587,"n":5,"t":1},"speaker":"A","text":"大学のサークルの先輩なので、よく知っています。"},{"id":{"d":587,"n":6,"t":1},"speaker":"B","text":"大学の時からずっとお付き合いしていたのですね。"},{"id":{"d":587,"n":7,"t":1},"speaker":"A","text":"はい。結婚するだろうと思っていたので、予想通りという感じです。"},{"id":{"d":587,"n":8,"t":1},"speaker":"B","text":"結婚式には、大学の仲間がたくさん出席しそうですね。"},{"id":{"d":587,"n":9,"t":1},"speaker":"A","text":"はい。久しぶりに会う人が多いので楽しみです。"}];
INSERT INTO sentence [{"id":{"d":588,"n":1,"t":1},"speaker":"A","text":"家の外から音が聞こえますね。"},{"id":{"d":588,"n":2,"t":1},"speaker":"B","text":"近所で工事をしているようです。"},{"id":{"d":588,"n":3,"t":1},"speaker":"A","text":"朝からずっと聞こえていて、ちょっと気になりますね。"},{"id":{"d":588,"n":4,"t":1},"speaker":"B","text":"そうですね。私も今日は家でテストの勉強をするつもりでしたが、図書館ですることにします。"},{"id":{"d":588,"n":5,"t":1},"speaker":"A","text":"私は映画を見ようと思うのですが、工事の音が邪魔になりそうです。"},{"id":{"d":588,"n":6,"t":1},"speaker":"B","text":"ヘッドホンを使いますか？"},{"id":{"d":588,"n":7,"t":1},"speaker":"A","text":"そうします。貸してもらえますか？"},{"id":{"d":588,"n":8,"t":1},"speaker":"B","text":"はい、どうぞ。"}];
INSERT INTO sentence [{"id":{"d":589,"n":1,"t":1},"speaker":"A","text":"お買い上げありがとうございます。合計で1,800円になります。"},{"id":{"d":589,"n":2,"t":1},"speaker":"B","text":"これでお願いします。"},{"id":{"d":589,"n":3,"t":1},"speaker":"A","text":"2,000円お預かりしましたので、200円のお返しになります。お箸をお付けしますか？"},{"id":{"d":589,"n":4,"t":1},"speaker":"B","text":"はい。お願いします。"},{"id":{"d":589,"n":5,"t":1},"speaker":"A","text":"何膳お付けしますか？"},{"id":{"d":589,"n":6,"t":1},"speaker":"B","text":"2膳ください。"},{"id":{"d":589,"n":7,"t":1},"speaker":"A","text":"かしこまりました。こちらになります。"},{"id":{"d":589,"n":8,"t":1},"speaker":"B","text":"ありがとう。"}];
INSERT INTO sentence [{"id":{"d":590,"n":1,"t":1},"speaker":"A","text":"すみませんが、お手洗いを貸してください。"},{"id":{"d":590,"n":2,"t":1},"speaker":"B","text":"どうぞ。廊下に出て、右側のドアです。"},{"id":{"d":590,"n":3,"t":1},"speaker":"A","text":"このドアですか？"},{"id":{"d":590,"n":4,"t":1},"speaker":"B","text":"はい、そうです。"},{"id":{"d":590,"n":5,"t":1},"speaker":"A","text":"電気のスイッチはどこでしょうか？"},{"id":{"d":590,"n":6,"t":1},"speaker":"B","text":"ドアの右側にあります。"},{"id":{"d":590,"n":7,"t":1},"speaker":"A","text":"スイッチが2つあります。"},{"id":{"d":590,"n":8,"t":1},"speaker":"B","text":"上が電気のスイッチです。"},{"id":{"d":590,"n":9,"t":1},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":591,"n":1,"t":1},"speaker":"A","text":"この音は、お琴の音色でしょうか？"},{"id":{"d":591,"n":2,"t":1},"speaker":"B","text":"隣の和室で、母が生徒さんにお琴を教えているのです。"},{"id":{"d":591,"n":3,"t":1},"speaker":"A","text":"お琴の先生をしているのですか？"},{"id":{"d":591,"n":4,"t":1},"speaker":"B","text":"はい。母は子どもの頃からお琴を習っていたそうです。"},{"id":{"d":591,"n":5,"t":1},"speaker":"A","text":"誰かに教えられるくらいのレベルまで上達するなんて、素晴らしいですね。"},{"id":{"d":591,"n":6,"t":1},"speaker":"B","text":"初心者にしか教えられませんと言っていますよ。"},{"id":{"d":591,"n":7,"t":1},"speaker":"A","text":"お母さまのお琴の演奏を聴いてみたいです。"},{"id":{"d":591,"n":8,"t":1},"speaker":"B","text":"機会があれば、ぜひ。母も張り切って演奏すると思います。"}];
INSERT INTO sentence [{"id":{"d":592,"n":1,"t":1},"speaker":"A","text":"血液型は何型ですか？"},{"id":{"d":592,"n":2,"t":1},"speaker":"B","text":"私はB型です。"},{"id":{"d":592,"n":3,"t":1},"speaker":"A","text":"B型の人は好奇心旺盛で楽天家だと、よく言われていますね。"},{"id":{"d":592,"n":4,"t":1},"speaker":"B","text":"自分でも、当てはまるような気がします。"},{"id":{"d":592,"n":5,"t":1},"speaker":"A","text":"悪い面だと、飽き性とかわがままだと言われているようです。"},{"id":{"d":592,"n":6,"t":1},"speaker":"B","text":"自覚はありませんが、そうかもしれません。"},{"id":{"d":592,"n":7,"t":1},"speaker":"A","text":"自分自身の性格は、自分ではなかなか分かりませんよね。"},{"id":{"d":592,"n":8,"t":1},"speaker":"B","text":"そうですね。自分の思う自分と、周りから見た自分で違いがあるようにも感じます。"}];
INSERT INTO sentence [{"id":{"d":593,"n":1,"t":1},"speaker":"A","text":"暗くなってきました。"},{"id":{"d":593,"n":2,"t":1},"speaker":"B","text":"そろそろ、カーテンを閉めましょう。"},{"id":{"d":593,"n":3,"t":1},"speaker":"A","text":"私がします。"},{"id":{"d":593,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。リビングと子ども部屋のカーテンを閉めてください。"},{"id":{"d":593,"n":5,"t":1},"speaker":"A","text":"分かりました。和室の障子も閉めましょうか？"},{"id":{"d":593,"n":6,"t":1},"speaker":"B","text":"和室の障子は滑りが悪くて閉めにくいのですが、大丈夫ですか？"},{"id":{"d":593,"n":7,"t":1},"speaker":"A","text":"ちょっとしてみます。"},{"id":{"d":593,"n":8,"t":1},"speaker":"B","text":"もし閉められなければ私がするので、声を掛けてください。"}];
INSERT INTO sentence [{"id":{"d":594,"n":1,"t":1},"speaker":"A","text":"失礼ですが、あなたの名前の漢字はどのように読むのですか？"},{"id":{"d":594,"n":2,"t":1},"speaker":"B","text":"「りゅうあ」と読みます。読み方を聞かれることは慣れているので、気にしないでください。"},{"id":{"d":594,"n":3,"t":1},"speaker":"A","text":"やはり、聞かれることが多いのですね。"},{"id":{"d":594,"n":4,"t":1},"speaker":"B","text":"すんなり読まれたことはほとんどありませんね。"},{"id":{"d":594,"n":5,"t":1},"speaker":"A","text":"ありふれた名前だったら楽だなと思うでしょうね。"},{"id":{"d":594,"n":6,"t":1},"speaker":"B","text":"そうですね。もっと読みやすい漢字を使った名前が良かったと思うこともあります。"},{"id":{"d":594,"n":7,"t":1},"speaker":"A","text":"名前を覚えてもらいやすいという利点もありますよ。"},{"id":{"d":594,"n":8,"t":1},"speaker":"B","text":"それもそうですね。読み方を聞かれた人には、一度で名前を覚えてもらえています。"}];
INSERT INTO sentence [{"id":{"d":595,"n":1,"t":1},"speaker":"A","text":"お会計は1,720円になります。"},{"id":{"d":595,"n":2,"t":1},"speaker":"B","text":"領収書をください。"},{"id":{"d":595,"n":3,"t":1},"speaker":"A","text":"かしこまりました。宛名はどういたしますか？"},{"id":{"d":595,"n":4,"t":1},"speaker":"B","text":"上水流でお願いします。"},{"id":{"d":595,"n":5,"t":1},"speaker":"A","text":"すみませんが、どのような漢字でしょうか？"},{"id":{"d":595,"n":6,"t":1},"speaker":"B","text":"上下の上、次が水、3文字目が流れるという字です。"},{"id":{"d":595,"n":7,"t":1},"speaker":"A","text":"これでよろしいでしょうか？"},{"id":{"d":595,"n":8,"t":1},"speaker":"B","text":"はい。大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":596,"n":1,"t":1},"speaker":"A","text":"こちらのお部屋はいかがでしょうか？"},{"id":{"d":596,"n":2,"t":1},"speaker":"B","text":"日当たりが良いですね。"},{"id":{"d":596,"n":3,"t":1},"speaker":"A","text":"南向きのお部屋になっています。"},{"id":{"d":596,"n":4,"t":1},"speaker":"B","text":"お風呂はどこですか？"},{"id":{"d":596,"n":5,"t":1},"speaker":"A","text":"こちらです。隣がトイレです。"},{"id":{"d":596,"n":6,"t":1},"speaker":"B","text":"お風呂とトイレが別なのはいいですね。"},{"id":{"d":596,"n":7,"t":1},"speaker":"A","text":"それぞれ少々狭いですが、別になっております。"},{"id":{"d":596,"n":8,"t":1},"speaker":"B","text":"洗濯機はどこに置くことになりますか？"},{"id":{"d":596,"n":9,"t":1},"speaker":"A","text":"脱衣所に、洗濯機置き場があります。"}];
INSERT INTO sentence [{"id":{"d":597,"n":1,"t":1},"speaker":"A","text":"沖縄にはどのような方言がありますか？"},{"id":{"d":597,"n":2,"t":1},"speaker":"B","text":"たくさんありますよ。有名な言葉だと、「めんそーれ」です。"},{"id":{"d":597,"n":3,"t":1},"speaker":"A","text":"聞いたことがあります。どういう意味ですか？"},{"id":{"d":597,"n":4,"t":1},"speaker":"B","text":"ようこそ、という意味です。ちなみに同じ沖縄でも、石垣島では「めんそーれ」とは違う言葉があります。"},{"id":{"d":597,"n":5,"t":1},"speaker":"A","text":"そうなのですか？石垣島では何と言うのですか？"},{"id":{"d":597,"n":6,"t":1},"speaker":"B","text":"「おーりとーり」です。"},{"id":{"d":597,"n":7,"t":1},"speaker":"A","text":"それは初めて聞きました。沖縄県の中でも、方言が異なるのですね。"},{"id":{"d":597,"n":8,"t":1},"speaker":"B","text":"そうですね。特に離島はかなり違うと思います。"}];
INSERT INTO sentence [{"id":{"d":598,"n":1,"t":1},"speaker":"A","text":"夜ご飯に天ぷらを揚げました。"},{"id":{"d":598,"n":2,"t":1},"speaker":"B","text":"天ぷらは大好きなので、うれしいです。"},{"id":{"d":598,"n":3,"t":1},"speaker":"A","text":"天ぷらには何を付けて食べますか？"},{"id":{"d":598,"n":4,"t":1},"speaker":"B","text":"私は塩を付けるのが好きです。"},{"id":{"d":598,"n":5,"t":1},"speaker":"A","text":"それでは塩を用意しますね。私は天つゆを付けます。"},{"id":{"d":598,"n":6,"t":1},"speaker":"B","text":"天つゆもおいしいですよね。"},{"id":{"d":598,"n":7,"t":1},"speaker":"A","text":"天つゆには大根おろしがあると、より良いです。"},{"id":{"d":598,"n":8,"t":1},"speaker":"B","text":"そうですね。天つゆと大根おろしはとても合いますね。"}];
INSERT INTO sentence [{"id":{"d":599,"n":1,"t":1},"speaker":"A","text":"私はスーパーマーケットでアルバイトをしています。"},{"id":{"d":599,"n":2,"t":1},"speaker":"B","text":"レジ打ちの仕事ですか？"},{"id":{"d":599,"n":3,"t":1},"speaker":"A","text":"いいえ、商品を陳列棚に補充する仕事です。"},{"id":{"d":599,"n":4,"t":1},"speaker":"B","text":"接客ではないのですね。"},{"id":{"d":599,"n":5,"t":1},"speaker":"A","text":"そう思っていましたが、いろいろと質問してくるお客さまがいるので、接客の要素もあります。"},{"id":{"d":599,"n":6,"t":1},"speaker":"B","text":"週に何日くらい働いていますか？"},{"id":{"d":599,"n":7,"t":1},"speaker":"A","text":"3日くらいです。大学へ通いながらアルバイトをしているので、結構忙しいです。"},{"id":{"d":599,"n":8,"t":1},"speaker":"B","text":"そうですよね。私もアルバイトは週に2日が限界です。"}];
INSERT INTO sentence [{"id":{"d":600,"n":1,"t":1},"speaker":"A","text":"このコンビニエンスストアのアルバイトに応募した理由は何ですか？"},{"id":{"d":600,"n":2,"t":1},"speaker":"B","text":"家から近いことと、普段利用していて、店員の皆さんの雰囲気が良いと思ったことが理由です。"},{"id":{"d":600,"n":3,"t":1},"speaker":"A","text":"勤務時間の希望はありますか？"},{"id":{"d":600,"n":4,"t":1},"speaker":"B","text":"夕方から夜の時間帯でお願いしたいです。"},{"id":{"d":600,"n":5,"t":1},"speaker":"A","text":"曜日はどうでしょう。希望の曜日、もしくは都合の悪い曜日があれば教えてください。"},{"id":{"d":600,"n":6,"t":1},"speaker":"B","text":"金曜日以外なら大丈夫です。"},{"id":{"d":600,"n":7,"t":1},"speaker":"A","text":"面接は以上になります。結果は電話でお伝えしますね。"},{"id":{"d":600,"n":8,"t":1},"speaker":"B","text":"はい。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":601,"n":1,"t":1},"speaker":"A","text":"最後にお風呂に入ったのは誰ですか？"},{"id":{"d":601,"n":2,"t":1},"speaker":"B","text":"私だと思います。"},{"id":{"d":601,"n":3,"t":1},"speaker":"A","text":"電気がつけっ放しでしたよ。"},{"id":{"d":601,"n":4,"t":1},"speaker":"B","text":"うっかりしていました。"},{"id":{"d":601,"n":5,"t":1},"speaker":"A","text":"おとといも消し忘れていましたよね。気を付けてください。"},{"id":{"d":601,"n":6,"t":1},"speaker":"B","text":"はい。すみません。最近ぼうっとしてしまうことが多いです。"},{"id":{"d":601,"n":7,"t":1},"speaker":"A","text":"何かあったのですか？"},{"id":{"d":601,"n":8,"t":1},"speaker":"B","text":"少し疲れがたまっているのかもしれません。"}];
INSERT INTO sentence [{"id":{"d":602,"n":1,"t":1},"speaker":"A","text":"ソニアさんが日本に来て一番驚いたことは何ですか？"},{"id":{"d":602,"n":2,"t":1},"speaker":"B","text":"人の多さです。"},{"id":{"d":602,"n":3,"t":1},"speaker":"A","text":"ニュージーランドより、ずっと多いですか？"},{"id":{"d":602,"n":4,"t":1},"speaker":"B","text":"はい。特に初めて東京を歩いた時は、人がたくさんいてびっくりしました。"},{"id":{"d":602,"n":5,"t":1},"speaker":"A","text":"今はもう慣れましたか？"},{"id":{"d":602,"n":6,"t":1},"speaker":"B","text":"そうですね。慣れたと思いますが、人混みを歩くのは慣れなくて疲れます。"},{"id":{"d":602,"n":7,"t":1},"speaker":"A","text":"それは私も同じです。雑踏は疲れますよね。"},{"id":{"d":602,"n":8,"t":1},"speaker":"B","text":"はい。それと満員電車も慣れません。"}];
INSERT INTO sentence [{"id":{"d":603,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。お車のご購入をご検討でしょうか？"},{"id":{"d":603,"n":2,"t":1},"speaker":"B","text":"どの車種にするか決めていませんので、いろいろと見てみようと思っています。"},{"id":{"d":603,"n":3,"t":1},"speaker":"A","text":"さようですか。今はどのようなお車ですか？"},{"id":{"d":603,"n":4,"t":1},"speaker":"B","text":"今は軽自動車ですが、家族全員で旅行にも行けるような車に買い替えたいです。"},{"id":{"d":603,"n":5,"t":1},"speaker":"A","text":"ご家族は何名ですか？"},{"id":{"d":603,"n":6,"t":1},"speaker":"B","text":"5人家族です。"},{"id":{"d":603,"n":7,"t":1},"speaker":"A","text":"それでは、こちらのワゴンタイプのお車がお勧めです。"},{"id":{"d":603,"n":8,"t":1},"speaker":"B","text":"試乗できますか？"},{"id":{"d":603,"n":9,"t":1},"speaker":"A","text":"もちろんできます。"}];
INSERT INTO sentence [{"id":{"d":604,"n":1,"t":1},"speaker":"A","text":"美咲、起きなくて大丈夫ですか？"},{"id":{"d":604,"n":2,"t":1},"speaker":"B","text":"今、何時ですか？"},{"id":{"d":604,"n":3,"t":1},"speaker":"A","text":"もう7時15分ですよ。"},{"id":{"d":604,"n":4,"t":1},"speaker":"B","text":"目覚まし時計を6時半にセットしておいたはずなのですが、鳴らなかったのでしょうか？"},{"id":{"d":604,"n":5,"t":1},"speaker":"A","text":"私は6時から起きていますが、目覚まし時計の音は聞いていません。"},{"id":{"d":604,"n":6,"t":1},"speaker":"B","text":"どうやら、セットし忘れてしまったようです。"},{"id":{"d":604,"n":7,"t":1},"speaker":"A","text":"急いで支度をした方がいいですよ。"},{"id":{"d":604,"n":8,"t":1},"speaker":"B","text":"はい。顔を洗ってきます。"}];
INSERT INTO sentence [{"id":{"d":605,"n":1,"t":1},"speaker":"A","text":"卵料理では、何が好きですか？"},{"id":{"d":605,"n":2,"t":1},"speaker":"B","text":"卵料理というと、いろいろありますね。"},{"id":{"d":605,"n":3,"t":1},"speaker":"A","text":"ゆで卵、卵焼き、目玉焼き、スクランブルエッグ、温泉卵、オムレツといったところでしょうか。"},{"id":{"d":605,"n":4,"t":1},"speaker":"B","text":"食べる場面によって、好きな料理法も変わります。"},{"id":{"d":605,"n":5,"t":1},"speaker":"A","text":"どういう意味ですか？"},{"id":{"d":605,"n":6,"t":1},"speaker":"B","text":"朝ご飯なら目玉焼きが一番いいですし、お弁当のおかずなら卵焼きがいいです。"},{"id":{"d":605,"n":7,"t":1},"speaker":"A","text":"そういうことですか。確かにそうですね。同じ朝ご飯でも、旅館の朝ご飯だったら温泉卵が最高です。"},{"id":{"d":605,"n":8,"t":1},"speaker":"B","text":"同感です。ホテルの朝食なら、オムレツを食べたいです。"},{"id":{"d":605,"n":9,"t":1},"speaker":"A","text":"私もそうです。"}];
INSERT INTO sentence [{"id":{"d":606,"n":1,"t":1},"speaker":"A","text":"スマートフォンを買い替えました。"},{"id":{"d":606,"n":2,"t":1},"speaker":"B","text":"見せてください。これは最新機種ですね。"},{"id":{"d":606,"n":3,"t":1},"speaker":"A","text":"はい。新しい機種が出ると、どうしても買いたくなってしまうのです。"},{"id":{"d":606,"n":4,"t":1},"speaker":"B","text":"何か新しい機能が付いているのでしょうか？"},{"id":{"d":606,"n":5,"t":1},"speaker":"A","text":"まだ詳しく調べていませんが、カメラで撮影した画像がとてもクリアできれいだという評判です。"},{"id":{"d":606,"n":6,"t":1},"speaker":"B","text":"本体の大きさが、少し大きくなったようですね。"},{"id":{"d":606,"n":7,"t":1},"speaker":"A","text":"そうですね。今までの機種より一回り大きいと思います。"},{"id":{"d":606,"n":8,"t":1},"speaker":"B","text":"最新機種だと、高かったのではありませんか？"},{"id":{"d":606,"n":9,"t":1},"speaker":"A","text":"そうですね。貯金をはたいて買いました。"}];
INSERT INTO sentence [{"id":{"d":607,"n":1,"t":1},"speaker":"A","text":"池田さん、終業時間になりましたよ。"},{"id":{"d":607,"n":2,"t":1},"speaker":"B","text":"まだ仕事が終わりませんので、残業します。"},{"id":{"d":607,"n":3,"t":1},"speaker":"A","text":"今週はずっと残業していますよね。何かあったのですか？"},{"id":{"d":607,"n":4,"t":1},"speaker":"B","text":"取引先から発注数の訂正があったり、会議の日程が変更になったり、トラブルが相次いでいるのです。"},{"id":{"d":607,"n":5,"t":1},"speaker":"A","text":"それは大変ですね。何か手伝いましょうか？"},{"id":{"d":607,"n":6,"t":1},"speaker":"B","text":"手伝っていただくなんて、申し訳ありません。"},{"id":{"d":607,"n":7,"t":1},"speaker":"A","text":"同じ部署ですし、お互いさまですよ。何か私にできることはありませんか？"},{"id":{"d":607,"n":8,"t":1},"speaker":"B","text":"とてもありがたいです。それでは、データの修正作業をお願いします。"}];
INSERT INTO sentence [{"id":{"d":608,"n":1,"t":1},"speaker":"A","text":"それでは勉強を始めましょう。先週出した宿題はしてありますか？"},{"id":{"d":608,"n":2,"t":1},"speaker":"B","text":"してあります。これです。"},{"id":{"d":608,"n":3,"t":1},"speaker":"A","text":"よくできていますね。それでは今日は先週の続きをしましょう。"},{"id":{"d":608,"n":4,"t":1},"speaker":"B","text":"先生、その前に今日の学校の授業で分からない問題があったので教えてください。"},{"id":{"d":608,"n":5,"t":1},"speaker":"A","text":"どの問題ですか？"},{"id":{"d":608,"n":6,"t":1},"speaker":"B","text":"このページの、問4です。"},{"id":{"d":608,"n":7,"t":1},"speaker":"A","text":"図形の問題ですね。補助線を引く必要がありますが、どこに引けばよいか分かりますか？"},{"id":{"d":608,"n":8,"t":1},"speaker":"B","text":"円の中心と原点を結ぶように引くのでしょうか？"},{"id":{"d":608,"n":9,"t":1},"speaker":"A","text":"その通りです。実際に線を引いてみましょう。"}];
INSERT INTO sentence [{"id":{"d":609,"n":1,"t":1},"speaker":"A","text":"あゆみさん、さっきまで傘を持っていませんでしたか？"},{"id":{"d":609,"n":2,"t":1},"speaker":"B","text":"持っていたはずです。おかしいですね、どこかに置き忘れてしまったのでしょうか。"},{"id":{"d":609,"n":3,"t":1},"speaker":"A","text":"恐らくそうでしょうね。どこに置き忘れたか、思い出せますか？"},{"id":{"d":609,"n":4,"t":1},"speaker":"B","text":"このショッピングモールに入る時に傘を閉じて、それからずっと持っていたはずですが、気が付いたら持っていませんでしたという感じです。"},{"id":{"d":609,"n":5,"t":1},"speaker":"A","text":"ショッピングモールに入ってからの行動を思い出してみましょう。"},{"id":{"d":609,"n":6,"t":1},"speaker":"B","text":"フードコートでお昼ご飯を食べて、食べ終わってからトイレに行って、その後雑貨屋さんで買い物をしましたね。"},{"id":{"d":609,"n":7,"t":1},"speaker":"A","text":"その途中のどこかで置き忘れたのでしょうね。1カ所ずつ、戻って探してみましょう。"},{"id":{"d":609,"n":8,"t":1},"speaker":"B","text":"はい。付き合わせてしまってすみません。"}];
INSERT INTO sentence [{"id":{"d":610,"n":1,"t":1},"speaker":"A","text":"生まれ変わったらやり直したいことはありますか？"},{"id":{"d":610,"n":2,"t":1},"speaker":"B","text":"大学受験です。もっと真面目に勉強しておけばよかったと思っています。"},{"id":{"d":610,"n":3,"t":1},"speaker":"A","text":"希望の大学に行けなかったのですか？"},{"id":{"d":610,"n":4,"t":1},"speaker":"B","text":"はい。生まれ変わったら、しっかり受験勉強をして第一志望だった大学に行きたいです。あなたはどうですか？"},{"id":{"d":610,"n":5,"t":1},"speaker":"A","text":"私は生まれ変わったら、小さい頃からピアノを習いたいです。"},{"id":{"d":610,"n":6,"t":1},"speaker":"B","text":"あなたは今でも十分ピアノがうまいと思いますよ。"},{"id":{"d":610,"n":7,"t":1},"speaker":"A","text":"いいえ、私は小学生になってから始めたので、上達に限界があると感じています。"},{"id":{"d":610,"n":8,"t":1},"speaker":"B","text":"人それぞれ後悔ややり残したことがあるのですね。"}];
INSERT INTO sentence [{"id":{"d":611,"n":1,"t":1},"speaker":"A","text":"これからは、お手伝いをしたらお小遣いを頂きたいです。"},{"id":{"d":611,"n":2,"t":1},"speaker":"B","text":"よいですよ。お小遣いをいくらにするか、決めておきましょう。"},{"id":{"d":611,"n":3,"t":1},"speaker":"A","text":"お風呂を洗ったら50円でどうですか？"},{"id":{"d":611,"n":4,"t":1},"speaker":"B","text":"妥当な金額ですね。他には食事の後で洗い物をしてくれたら100円あげます。"},{"id":{"d":611,"n":5,"t":1},"speaker":"A","text":"100円ですか。それはありがたいです。"},{"id":{"d":611,"n":6,"t":1},"speaker":"B","text":"その代わり、洗うだけではなく、拭いて食器棚へしまうところまでやってくださいね。"},{"id":{"d":611,"n":7,"t":1},"speaker":"A","text":"分かりました。掃除機をかけたら50円でよいですか？"},{"id":{"d":611,"n":8,"t":1},"speaker":"B","text":"はい、それでよいです。たくさんお手伝いをしてくれそうで、楽しみです。"}];
