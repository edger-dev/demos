INSERT INTO sentence [{"id":{"d":247,"n":1,"t":1},"speaker":"A","text":"冷蔵庫に、いつも入っている冷凍食品はありますか？"},{"id":{"d":247,"n":2,"t":1},"speaker":"B","text":"ありますよ。野菜や麺類、ご飯、おかず類などいろいろあります。"},{"id":{"d":247,"n":3,"t":1},"speaker":"A","text":"たくさん入っていますね。なぜ、そのようにあるのですか？"},{"id":{"d":247,"n":4,"t":1},"speaker":"B","text":"夫婦共働きなので、冷凍食品をうまく利用して食事を準備する時間を節約しています。"},{"id":{"d":247,"n":5,"t":1},"speaker":"A","text":"最近の冷凍食品は調理が簡単でおいしいものがたくさんありますものね。"},{"id":{"d":247,"n":6,"t":1},"speaker":"B","text":"はい。中学生の息子二人のお弁当やおやつ、夜食としてとても重宝しています。"}];
INSERT INTO sentence [{"id":{"d":248,"n":1,"t":1},"speaker":"A","text":"お母さん、明日から制服の衣替えです。夏服はどこにありますか？"},{"id":{"d":248,"n":2,"t":1},"speaker":"B","text":"もうそのような時季ですか。私の部屋の洋服だんすにあります。"},{"id":{"d":248,"n":3,"t":1},"speaker":"A","text":"私の部屋に夏服を持ってきてもよいですか？代わりに冬服をしまってきます。"},{"id":{"d":248,"n":4,"t":1},"speaker":"B","text":"ええ。ついでにサイズが大丈夫かどうか、一度着て確かめてください。"},{"id":{"d":248,"n":5,"t":1},"speaker":"A","text":"分かりました。"},{"id":{"d":248,"n":6,"t":1},"speaker":"B","text":"半年間で身長が伸びたようですから、もしサイズが合わなくなっていたらお直しに出しましょうね。"},{"id":{"d":248,"n":7,"t":1},"speaker":"A","text":"急に、心配になってきました。明日着ていけなかったらどうしましょう。"}];
INSERT INTO sentence [{"id":{"d":249,"n":1,"t":1},"speaker":"A","text":"お父さん、子どもの時に釣りをしたことがありますか？"},{"id":{"d":249,"n":2,"t":1},"speaker":"B","text":"あります。趣味の一つでした。"},{"id":{"d":249,"n":3,"t":1},"speaker":"A","text":"どこで釣っていましたか？"},{"id":{"d":249,"n":4,"t":1},"speaker":"B","text":"海でも川でも湖でも、どこでも釣っていました。釣り好きの友達もたくさんいました。"},{"id":{"d":249,"n":5,"t":1},"speaker":"A","text":"私も今日、友達に釣りに誘われました。道具は貸してくれるそうです。"},{"id":{"d":249,"n":6,"t":1},"speaker":"B","text":"釣り道具なら物置にまだあるはずです。しかし、古い道具ですし、いろいろな釣り方があるから、友達に確認した方がいいでしょう。"},{"id":{"d":249,"n":7,"t":1},"speaker":"A","text":"友達を連れてきますから、道具を見てもらってもよいですか？"},{"id":{"d":249,"n":8,"t":1},"speaker":"B","text":"その方がいいでしょう。明日、道具は物置から出しておきます。"}];
INSERT INTO sentence [{"id":{"d":250,"n":1,"t":1},"speaker":"A","text":"課長、会議室の予約をしますが、参加者は何人ですか？"},{"id":{"d":250,"n":2,"t":1},"speaker":"B","text":"まだ返事が来ていないところもありますが、20人前後だと思います。"},{"id":{"d":250,"n":3,"t":1},"speaker":"A","text":"それでは、大会議室を予約しておきます。少し広すぎるかもしれませんが、狭いよりは良いでしょう。"},{"id":{"d":250,"n":4,"t":1},"speaker":"B","text":"はい、それで構いません。よろしくお願いします。"},{"id":{"d":250,"n":5,"t":1},"speaker":"A","text":"会議室にはプロジェクターも用意しますか？"},{"id":{"d":250,"n":6,"t":1},"speaker":"B","text":"お願いします。レジュメも少し多めに30部ほど準備しておいてください。"},{"id":{"d":250,"n":7,"t":1},"speaker":"A","text":"分かりました。早速、準備に取り掛かります。"}];
INSERT INTO sentence [{"id":{"d":251,"n":1,"t":1},"speaker":"A","text":"お巡りさん、ここから税務署までどのくらいかかりますか？"},{"id":{"d":251,"n":2,"t":1},"speaker":"B","text":"徒歩だと、ここから10分ほどかかります。向かいのバス停からバスも出ていますよ。"},{"id":{"d":251,"n":3,"t":1},"speaker":"A","text":"10分で着くのなら歩いて行こうと思います。行き方を教えていただけますか？"},{"id":{"d":251,"n":4,"t":1},"speaker":"B","text":"この道に沿って400メートルほど行くと大通りにぶつかります。そこを左に曲がって300メートルほどで税務署です。"},{"id":{"d":251,"n":5,"t":1},"speaker":"A","text":"曲がり角に何か目印はありますか？"},{"id":{"d":251,"n":6,"t":1},"speaker":"B","text":"この道と大通りがぶつかるところには、進行方向右手に市役所があります。"},{"id":{"d":251,"n":7,"t":1},"speaker":"A","text":"市役所の前を通っていくのですか？"},{"id":{"d":251,"n":8,"t":1},"speaker":"B","text":"いいえ、横断歩道は渡りません。市役所が見えたらその四つ角をそのまま左に曲がって、300メートル進むと左手に税務署があります。"},{"id":{"d":251,"n":9,"t":1},"speaker":"A","text":"よく分りました。丁寧にありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":252,"n":1,"t":1},"speaker":"A","text":"おじいさん、最近午前中はどこに行っているのですか？昼過ぎになっても帰って来ない日がたまにあるようですが。"},{"id":{"d":252,"n":2,"t":1},"speaker":"B","text":"健康のために散歩をしています。たまに散歩コースの途中にある市民図書館で雑誌や本を読んでくることがありますが、そういう日は帰宅が遅くなってしまいます。"},{"id":{"d":252,"n":3,"t":1},"speaker":"A","text":"そうだったのですね。特に帰りが遅い日は、おばあさんが心配していましたよ。今度から先に予定を伝えてから出かけるようにしてくださいね。"},{"id":{"d":252,"n":4,"t":1},"speaker":"B","text":"それはおばあさんに、申し訳ないことをしました。次回からは、出かける前に声をるようにします。"},{"id":{"d":252,"n":5,"t":1},"speaker":"A","text":"はい、そうしてください。たまにはおばあさんもお散歩に誘ってみるのも良いかもしれませんね。"},{"id":{"d":252,"n":6,"t":1},"speaker":"B","text":"それは良い案です。久しぶりに一緒に出かけるのも楽しそうです。"}];
INSERT INTO sentence [{"id":{"d":253,"n":1,"t":1},"speaker":"A","text":"もう、このような時間ですね。もしよければ今日はわが家で夕飯を食べていきませんか？"},{"id":{"d":253,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。ご迷惑でなければ、ぜひいただきたいです。"},{"id":{"d":253,"n":3,"t":1},"speaker":"A","text":"今日の夕飯はそばだそうですよ。"},{"id":{"d":253,"n":4,"t":1},"speaker":"B","text":"私はそばが好物なのでうれしいです。"},{"id":{"d":253,"n":5,"t":1},"speaker":"A","text":"それは良かったです。わが家のそばはとてもおいしいので楽しみにしていてください。冷たいそばに、ネギ、とろろ、大根おろし、納豆、さらには野菜のてんぷらなど、いろいろな具材をたくさんのせて食べるのですよ。"},{"id":{"d":253,"n":6,"t":1},"speaker":"B","text":"とてもおいしそうです。一つ一つの具材はどれもそばによく合いそうですが、全部を一気にのせるのは初めてなので、どのような味になるか楽しみです。"},{"id":{"d":253,"n":7,"t":1},"speaker":"A","text":"ぜひ、お召し上がりください。話していたらおなかがすいてきました。夕飯が待ちきれません。"},{"id":{"d":253,"n":8,"t":1},"speaker":"B","text":"ええ、私もすっかり空腹です。"}];
INSERT INTO sentence [{"id":{"d":254,"n":1,"t":1},"speaker":"A","text":"最近の回転ずし屋は何でもありますね。"},{"id":{"d":254,"n":2,"t":1},"speaker":"B","text":"そうですね。メニュー表にはラーメンやフライドポテト、カレーも載っています。"},{"id":{"d":254,"n":3,"t":1},"speaker":"A","text":"本当ですね。見てください、カツ丼までありますよ。"},{"id":{"d":254,"n":4,"t":1},"speaker":"B","text":"食事だけでなく、デザート類も本当にたくさんありますね。ケーキやプリンもあるようです。"},{"id":{"d":254,"n":5,"t":1},"speaker":"A","text":"まるで、ファミリーレストランのような豊富な品ぞろえですね。驚きました。"},{"id":{"d":254,"n":6,"t":1},"speaker":"B","text":"そうですね。さて、鈴木さん、最初の注文は決まりましたか？"},{"id":{"d":254,"n":7,"t":1},"speaker":"A","text":"これだけたくさんあると迷ってしまいますが、気になっていたカツ丼にしてみようと思います。"},{"id":{"d":254,"n":8,"t":1},"speaker":"B","text":"良いですね。私はありきたりですが、おすしからいきます。マグロとアジを一皿ずつに頼みます。そして、デザートにチョコレートケーキを頼もうと思います。"},{"id":{"d":254,"n":9,"t":1},"speaker":"A","text":"僕も、最後にケーキを頼もうと思っていました。デザートも楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":255,"n":1,"t":1},"speaker":"A","text":"市の刊行物を見たいのですが、まとめて置いてあるところはありますか？"},{"id":{"d":255,"n":2,"t":1},"speaker":"B","text":"3階の西側、階段を上った右側に郷土資料室があります。広報以外の刊行物はそちらでご覧になれます。"},{"id":{"d":255,"n":3,"t":1},"speaker":"A","text":"上の階に行く、エレベーターはありませんか？"},{"id":{"d":255,"n":4,"t":1},"speaker":"B","text":"階段脇を抜けて直進すると、奥にエレベーターがあります。3階で降りたら右に進んでください。右手側に郷土資料室があります。"},{"id":{"d":255,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。資料室内の刊行物は借りられますか？"},{"id":{"d":255,"n":6,"t":1},"speaker":"B","text":"資料室内の資料は閲覧だけになっております。"},{"id":{"d":255,"n":7,"t":1},"speaker":"A","text":"そうですか。いくつか資料を照らし合わせたいのですが、どうすればよいのでしょうか？"},{"id":{"d":255,"n":8,"t":1},"speaker":"B","text":"資料室を出るとコピー機があります。有料ですがコピーしたものをお持ち帰りいただくことは可能です。"},{"id":{"d":255,"n":9,"t":1},"speaker":"A","text":"分かりました。どうもありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":256,"n":1,"t":1},"speaker":"A","text":"明日の野球の試合は、何時から始まりますか？応援に行こうと思います。"},{"id":{"d":256,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。試合は10時に始まる予定です。"},{"id":{"d":256,"n":3,"t":1},"speaker":"A","text":"明日の朝は少しだけ仕事があるので、もしかしたら1時間ほど遅れてしまうかもしれませんが、終わり次第会場へ行きますね。"},{"id":{"d":256,"n":4,"t":1},"speaker":"B","text":"試合は午後もやっているので、急がなくて大丈夫ですよ。気を付けて来てくださいね。"},{"id":{"d":256,"n":5,"t":1},"speaker":"A","text":"はい、ありがとうございます。明日の試合、けがに気を付けて頑張ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":257,"n":1,"t":1},"speaker":"A","text":"お母さん、僕の冬用の部屋着をどこにしまったか教えてください。"},{"id":{"d":257,"n":2,"t":1},"speaker":"B","text":"冬物は物置にしまったはずです。物置のダンボールに入っていると思いますよ。"},{"id":{"d":257,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。来週から一気に寒くなるそうなので、今週末のうちに暖かい部屋着を出しておきたくて。"},{"id":{"d":257,"n":4,"t":1},"speaker":"B","text":"まあ、来週はそれほど寒くなるのですか？"},{"id":{"d":257,"n":5,"t":1},"speaker":"A","text":"今朝のテレビ番組の天気予報では、来週は今週よりも5度以上気温が下がると言っていました。"},{"id":{"d":257,"n":6,"t":1},"speaker":"B","text":"それは大変ですね。そのように寒くなるのなら、部屋着だけでなく冬服やコートも出さなければいけませんね。"},{"id":{"d":257,"n":7,"t":1},"speaker":"A","text":"そうかもしれません。では、今週末、物置にある冬物を全部出しておきますね。"},{"id":{"d":257,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":258,"n":1,"t":1},"speaker":"A","text":"おじいさん、「ちょうず」とはなんですか？"},{"id":{"d":258,"n":2,"t":1},"speaker":"B","text":"ちょうずとは、神社や寺院ある参拝前に手や口を清める水のことです。"},{"id":{"d":258,"n":3,"t":1},"speaker":"A","text":"そのもの自体はよく知っていましたが、それをちょうずと呼ぶことを知りませんでした。「ちょうず」はどのような字を書くのですか？"},{"id":{"d":258,"n":4,"t":1},"speaker":"B","text":"手足の「手」に、水玉の「水」と書いて、ちょうずと読みます。"}];
INSERT INTO sentence [{"id":{"d":259,"n":1,"t":1},"speaker":"A","text":"裁判を傍聴したことがありますか？"},{"id":{"d":259,"n":2,"t":1},"speaker":"B","text":"いいえ、ありません。"},{"id":{"d":259,"n":3,"t":1},"speaker":"A","text":"そうですか。実は、大学のゼミの課題で裁判を傍聴してレポートにまとめる必要があるのですが、初めてで何も分からないので、裁判を傍聴したことのある人から話を聞きたいと思っているのです。"},{"id":{"d":259,"n":4,"t":1},"speaker":"B","text":"なるほど、そうでしたか。もしかしたら、高山先輩は経験があるかもしれません。彼は法学部で、裁判官を目指していると聞いたことがあります。彼に尋ねてみてはいかがですか？"},{"id":{"d":259,"n":5,"t":1},"speaker":"A","text":"高山先輩が詳しいとは知りませんでした。教えてくれてありがとうございます。早速彼に連絡してみます。"},{"id":{"d":259,"n":6,"t":1},"speaker":"B","text":"どういたしまして。良いレポートが書けるといいですね。"}];
INSERT INTO sentence [{"id":{"d":260,"n":1,"t":1},"speaker":"A","text":"今日の昼食は出前にしましょう。何か、食べたいものはありますか？"},{"id":{"d":260,"n":2,"t":1},"speaker":"B","text":"メニューはお任せしますが、Uber Eatsというサービスを使ってみたいです。"},{"id":{"d":260,"n":3,"t":1},"speaker":"A","text":"初めて聞きました。それはどのようなサービスなのですか？"},{"id":{"d":260,"n":4,"t":1},"speaker":"B","text":"アプリケーションを通してオンラインで食べたい料理を注文すると、それを自宅まで配達してくれるサービスです。"},{"id":{"d":260,"n":5,"t":1},"speaker":"A","text":"出前のようなものでしょうか？"},{"id":{"d":260,"n":6,"t":1},"speaker":"B","text":"よく似ていますが、出前は料理を提供するお店の人が配達する一方で、Uber Eatsは料理を提供するお店とは関係のない配達員の人が料理の配達を担当します。出前をやっていないお店にも注文ができる、複数の店の料理を一気に届けてもらえる、などの良さがあります。"},{"id":{"d":260,"n":7,"t":1},"speaker":"A","text":"それは便利ですね。"},{"id":{"d":260,"n":8,"t":1},"speaker":"B","text":"では早速アプリケーションで注文しましょう。せっかくなので2つのお店で料理を注文してみましょうか？"},{"id":{"d":260,"n":9,"t":1},"speaker":"A","text":"そうですね。まずは、ここの中華料理店でチャーハン、カシューナッツと鶏肉の炒め物を頼みましょう。デザートは、こちらのケーキ屋で杏仁豆腐とマンゴープリンを頼むのはどうでしょうか？"},{"id":{"d":260,"n":10,"t":1},"speaker":"B","text":"いいですね。その内容で注文します。"}];
INSERT INTO sentence [{"id":{"d":261,"n":1,"t":1},"speaker":"A","text":"河原での清掃作業も今年4回目ですが、ごみの量は変わってないような気がします。"},{"id":{"d":261,"n":2,"t":1},"speaker":"B","text":"平気でごみを捨てる人がいるからでしょう。最近は釣りをする人がいて、その人たちのマナーも問題となっています。"},{"id":{"d":261,"n":3,"t":1},"speaker":"A","text":"確かに、ここ数年は、この川で釣りをしている人をよく見かけます。"},{"id":{"d":261,"n":4,"t":1},"speaker":"B","text":"釣りをすること自体は良いのですが、ごみの処理はきちんとしてほしいです。ごみ捨て禁止の看板を立てるなど、何か対策をした方がいいのかもしれません。"},{"id":{"d":261,"n":5,"t":1},"speaker":"A","text":"そうかもしれませんね。今日の作業が終わったら、早速市役所へ相談に行きませんか？"},{"id":{"d":261,"n":6,"t":1},"speaker":"B","text":"ええ、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":262,"n":1,"t":1},"speaker":"A","text":"市の防災速報をスマートフォンで受け取りたいのですが、どうすればよいか教えていただけませんか？"},{"id":{"d":262,"n":2,"t":1},"speaker":"B","text":"はい。まずはこちらのQRコードをスマートフォンで読み取ってください。"},{"id":{"d":262,"n":3,"t":1},"speaker":"A","text":"QRコードの読み取り方が分かりません。どうすれば良いですか？"},{"id":{"d":262,"n":4,"t":1},"speaker":"B","text":"カメラのアプリケーションを起動して、QRコードを読み取るモードに切り替えてください。"},{"id":{"d":262,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。読み取ることができました。"},{"id":{"d":262,"n":6,"t":1},"speaker":"B","text":"そうしましたら、ページの上の方にある「インストール」ボタンを押してください。"},{"id":{"d":262,"n":7,"t":1},"speaker":"A","text":"押しました。インストールが始まったようです。"},{"id":{"d":262,"n":8,"t":1},"speaker":"B","text":"こちらの画面はインストールの進展状況を表示しています。これが100％になればインストール完了です。"},{"id":{"d":262,"n":9,"t":1},"speaker":"A","text":"100パーセントになりました。これで無事に、インストールできたということですね。どうもありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":263,"n":1,"t":1},"speaker":"A","text":"君の家は庭が広いですね。あの木は何ですか？"},{"id":{"d":263,"n":2,"t":1},"speaker":"B","text":"柿の木です。秋になればたくさんの柿が採れます。"},{"id":{"d":263,"n":3,"t":1},"speaker":"A","text":"それはすごいですね。鳥に実を取られたりはしないのですか？"},{"id":{"d":263,"n":4,"t":1},"speaker":"B","text":"渋柿なのであまり取られません。ただ、熟して甘くなると鳥に食べられてしまうこともあるので、毎年渋いうちに収穫するようにしています。"},{"id":{"d":263,"n":5,"t":1},"speaker":"A","text":"そうなのですか。収穫した柿はどのようにして食べているのですか？"},{"id":{"d":263,"n":6,"t":1},"speaker":"B","text":"そのままでは渋くてとても食べられないので、家では焼酎に漬けて渋抜きをしてから食べることが多いです。"},{"id":{"d":263,"n":7,"t":1},"speaker":"A","text":"渋いままで食べたことはありますか？"},{"id":{"d":263,"n":8,"t":1},"speaker":"B","text":"子どもの頃、一度口にしたことがあります。強烈な渋さに気絶したことを覚えています。"},{"id":{"d":263,"n":9,"t":1},"speaker":"A","text":"そう言われるとどれくらい渋いのか、一度試してみたくなります。"},{"id":{"d":263,"n":10,"t":1},"speaker":"B","text":"鳥も食べないのですから、やめた方が賢明です。"}];
INSERT INTO sentence [{"id":{"d":264,"n":1,"t":1},"speaker":"A","text":"今日はどうなさいますか？いつもの髪形にしますか？"},{"id":{"d":264,"n":2,"t":1},"speaker":"B","text":"今日はカラーをお願いします。髪形はいつも通りラフなウェーブにしてください。"},{"id":{"d":264,"n":3,"t":1},"speaker":"A","text":"かしこまりました。カラーはいかがいたしますか？"},{"id":{"d":264,"n":4,"t":1},"speaker":"B","text":"明るめのベージュに染めてください。"},{"id":{"d":264,"n":5,"t":1},"speaker":"A","text":"分かりました。明るめのベージュですと、例えば、こちらのサンプルのような色味はいかがですか？"},{"id":{"d":264,"n":6,"t":1},"speaker":"B","text":"イメージ通りです。その色でお願いします。"},{"id":{"d":264,"n":7,"t":1},"speaker":"A","text":"分かりました。では、まずはカットから始めていきますね。"},{"id":{"d":264,"n":8,"t":1},"speaker":"B","text":"はい、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":265,"n":1,"t":1},"speaker":"A","text":"お母さん、廊下に来客用のスリッパが出ていましたが、今日は誰か来るのですか？"},{"id":{"d":265,"n":2,"t":1},"speaker":"B","text":"今日は、あなたのクラスの家庭訪問ですよ。もう少しで先生がいらっしゃる時間です。"},{"id":{"d":265,"n":3,"t":1},"speaker":"A","text":"すっかり忘れていました。何か準備で手伝えることはありますか？"},{"id":{"d":265,"n":4,"t":1},"speaker":"B","text":"では、先生にお出しするお菓子を買ってきてもらえますか？"},{"id":{"d":265,"n":5,"t":1},"speaker":"A","text":"分かりました。スナック菓子でもよいですか？"},{"id":{"d":265,"n":6,"t":1},"speaker":"B","text":"スナック菓子はいけません。お煎餅かクッキーにしてください。"},{"id":{"d":265,"n":7,"t":1},"speaker":"A","text":"はい。"}];
INSERT INTO sentence [{"id":{"d":266,"n":1,"t":1},"speaker":"A","text":"コーヒーを買いに少し出かけます。先輩の分も買ってきますが、何がいいですか？"},{"id":{"d":266,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。どこまで行きますか？コンビニエンスストアですか？それとも、もう少し先にあるカフェですか？"},{"id":{"d":266,"n":3,"t":1},"speaker":"A","text":"どちらでもいいですよ。"},{"id":{"d":266,"n":4,"t":1},"speaker":"B","text":"それでは、カフェで豆乳カフェオレをお願いします。"},{"id":{"d":266,"n":5,"t":1},"speaker":"A","text":"承知しました。"},{"id":{"d":266,"n":6,"t":1},"speaker":"B","text":"あなたは、何にするのですか？"},{"id":{"d":266,"n":7,"t":1},"speaker":"A","text":"私は抹茶ラテにするつもりです。ああ、でもカプチーノも捨て難いので悩みます。"},{"id":{"d":266,"n":8,"t":1},"speaker":"B","text":"どちらもおいしそうですね。両方買ってしまうのはどうですか？"},{"id":{"d":266,"n":9,"t":1},"speaker":"A","text":"お店に着くまでに決められなければ、そうします。では、行ってきます。"}];
INSERT INTO sentence [{"id":{"d":267,"n":1,"t":1},"speaker":"A","text":"あら、かき氷のメニューが出ていますね。"},{"id":{"d":267,"n":2,"t":1},"speaker":"B","text":"もうそのような時季ですか？"},{"id":{"d":267,"n":3,"t":1},"speaker":"A","text":"そろそろ夏ですね。かき氷は何味が好きですか？"},{"id":{"d":267,"n":4,"t":1},"speaker":"B","text":"練乳とイチゴを合わせたものが好きです。あなたはどうですか？"},{"id":{"d":267,"n":5,"t":1},"speaker":"A","text":"私はブルーハワイです。ブルーハワイにモモやサクランボをトッピングしたものが特に好きです。"},{"id":{"d":267,"n":6,"t":1},"speaker":"B","text":"まるでカクテルのようですね。私が好きなかき氷のトッピングはあんこです。練乳とあんこはとても良く合い、とてもおいしいです。"},{"id":{"d":267,"n":7,"t":1},"speaker":"A","text":"それもおいしそうですね。食後にどら焼きを頼んでいましたが、かき氷も食べたくなってきました。"},{"id":{"d":267,"n":8,"t":1},"speaker":"B","text":"私も、あなたと同じくかき氷を食べたくなってきました。追加で注文しましょうか？"},{"id":{"d":267,"n":9,"t":1},"speaker":"A","text":"そうですね。今年初のかき氷といきましょう。"}];
INSERT INTO sentence [{"id":{"d":268,"n":1,"t":1},"speaker":"A","text":"リビングルームと客間とダイニングの窓を、シャッター付き窓に交換でよろしいですね。"},{"id":{"d":268,"n":2,"t":1},"speaker":"B","text":"はい。それから1階のトイレ、バス、キッチンの窓枠を、格子付きの窓枠に交換をお願いします。"},{"id":{"d":268,"n":3,"t":1},"speaker":"A","text":"承知しました。これで窓の防犯対策は万全ですね。"},{"id":{"d":268,"n":4,"t":1},"speaker":"B","text":"これで一安心です。ところで、シャッターは地震で家がゆがむと開けにくくなったりしませんか？"},{"id":{"d":268,"n":5,"t":1},"speaker":"A","text":"木造の家ではないので大丈夫でしょう。玄関はどうしますか？人通りの多い道に面しているので、ドアに鍵が2つあれば、防犯としては十分だと思いますが。ドアスコープは外からも見えるので付けない方がいいです。"},{"id":{"d":268,"n":6,"t":1},"speaker":"B","text":"なるほど。では、それでお願いします。インターフォンにモニターが付いているので、ドアスコープはなくても問題ありません。"},{"id":{"d":268,"n":7,"t":1},"speaker":"A","text":"分かりました。では、これで工事を進めます。これでもう空き巣に入られることはないでしょう。"}];
INSERT INTO sentence [{"id":{"d":269,"n":1,"t":1},"speaker":"A","text":"今のマンションを出て、一戸建て住宅に住むことを考えています。"},{"id":{"d":269,"n":2,"t":1},"speaker":"B","text":"私は一戸建てに住んでいますが、マンションの方が良かったと思っていますよ。"},{"id":{"d":269,"n":3,"t":1},"speaker":"A","text":"それは、なぜですか？"},{"id":{"d":269,"n":4,"t":1},"speaker":"B","text":"一戸建てでは町内会の集まりがあったり、地域のごみ収集係になったりと面倒なことが多いです。近所に問題のある人がいても引っ越しはもうできません。"},{"id":{"d":269,"n":5,"t":1},"speaker":"A","text":"会合や持ち回りの係は、マンションにもありますよ。マンション住まいの方が、引っ越ししやすいのは確かにそうかもしれませんが。"},{"id":{"d":269,"n":6,"t":1},"speaker":"B","text":"それから、一戸建ては20年もすれば、あちこち傷んできます。建て替えるとなると、かなりの金額がかかります。"},{"id":{"d":269,"n":7,"t":1},"speaker":"A","text":"なるほど、そういう問題もあるのですね。それでも、子どもの頃からアパート暮らしだったので、一戸建てに住むことは夢なのです。"},{"id":{"d":269,"n":8,"t":1},"speaker":"B","text":"そうでしたか。それならば、一戸建てを選ぶときには周りの土地の状況も知っておいた方がいいです。"},{"id":{"d":269,"n":9,"t":1},"speaker":"A","text":"先ほどの、近所付き合いということですね。他にも大切なことがあったら、教えてください。"}];
INSERT INTO sentence [{"id":{"d":270,"n":1,"t":1},"speaker":"A","text":"お母さん、雑貨店とは何ですか？学校の帰りにおばあさんから雑貨店までの道を尋ねられたのですが、そもそも雑貨店の意味が分かりませんでした。"},{"id":{"d":270,"n":2,"t":1},"speaker":"B","text":"私もよく分かりませんが、生活用品全般の物を扱っているお店のことだと思います。そのおばあさんに、何を探しているかは聞いてみましたか？"},{"id":{"d":270,"n":3,"t":1},"speaker":"A","text":"急須だと言っていました。近くに100円ショップがあったのでそこまで案内しましたが、良かったのでしょうか？"},{"id":{"d":270,"n":4,"t":1},"speaker":"B","text":"その方が立派な急須を探しているなら違っていたかも知れませんが、そうでないなら100円ショップは良い提案だったと思いますよ。そもそも立派な急須を置いているような昔ながらの雑貨店は、残念ながらもうこの辺りにはないかもしれません。"},{"id":{"d":270,"n":5,"t":1},"speaker":"A","text":"いつも100円ショップを利用していたので、そこしか思いつきませんでした。確かに言われてみれば、この辺りはいつの間にか新しいお店ばかりになっていますね。"}];
INSERT INTO sentence [{"id":{"d":271,"n":1,"t":1},"speaker":"A","text":"ウーロン茶、麦茶、日本茶、紅茶、夏に常備するならどれでしょう。"},{"id":{"d":271,"n":2,"t":1},"speaker":"B","text":"私が常備している飲み物は、麦茶です。冷水用ティーバッグで作り置きしています。"},{"id":{"d":271,"n":3,"t":1},"speaker":"A","text":"他の飲み物は、ご自宅の冷蔵庫に置きませんか？"},{"id":{"d":271,"n":4,"t":1},"speaker":"B","text":"あとは牛乳くらいでしょうか。日本茶や紅茶はホットでしか飲みません。"},{"id":{"d":271,"n":5,"t":1},"speaker":"A","text":"ペットボトルで買ってくるようなことはないのですか？"},{"id":{"d":271,"n":6,"t":1},"speaker":"B","text":"そう言われて思い出しました。麦茶にしているのは冷水用ティーバッグが軽いからです。ペットボトルだと何本も必要で、買い物が大変になってしまうのです。"},{"id":{"d":271,"n":7,"t":1},"speaker":"A","text":"なるほど、それは一理ありますね。"}];
INSERT INTO sentence [{"id":{"d":272,"n":1,"t":1},"speaker":"A","text":"お母さんのカレーライスは、とてもおいしいです。"},{"id":{"d":272,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。このカレーライスの隠し味は何だと思いますか？"},{"id":{"d":272,"n":3,"t":1},"speaker":"A","text":"分かりません。教えてください。"},{"id":{"d":272,"n":4,"t":1},"speaker":"B","text":"実は、チョコレートです。"},{"id":{"d":272,"n":5,"t":1},"speaker":"A","text":"チョコレートですか。気が付きませんでした。"},{"id":{"d":272,"n":6,"t":1},"speaker":"B","text":"チョコレートを少し入れると味に深みが出るのですよ。レシピを教えるので今度ぜひ作ってみてください。分量には気を付けてくださいね。"},{"id":{"d":272,"n":7,"t":1},"speaker":"A","text":"分かりました。来週は私が作ります。"},{"id":{"d":272,"n":8,"t":1},"speaker":"B","text":"はい。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":273,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。何をお探しですか？"},{"id":{"d":273,"n":2,"t":1},"speaker":"B","text":"こんにちは。普段着にも仕事着にもなる服を探しています。"},{"id":{"d":273,"n":3,"t":1},"speaker":"A","text":"それでは、このジャケットがお薦めです。"},{"id":{"d":273,"n":4,"t":1},"speaker":"B","text":"すてきなデザインですね。これは冬物ですか？"},{"id":{"d":273,"n":5,"t":1},"speaker":"A","text":"はい。生地が厚く、防寒着にもなります。"},{"id":{"d":273,"n":6,"t":1},"speaker":"B","text":"いいですね。お値段はいくらですか？"},{"id":{"d":273,"n":7,"t":1},"speaker":"A","text":"元値は1万円ですが、今ならセールで7,000円です。"},{"id":{"d":273,"n":8,"t":1},"speaker":"B","text":"試着してもよいですか？"},{"id":{"d":273,"n":9,"t":1},"speaker":"A","text":"もちろんです。試着室にご案内します。"}];
INSERT INTO sentence [{"id":{"d":274,"n":1,"t":1},"speaker":"A","text":"松本さん、お昼ご飯を一緒に食べに行きませんか？"},{"id":{"d":274,"n":2,"t":1},"speaker":"B","text":"いいですね、何を食べますか？"},{"id":{"d":274,"n":3,"t":1},"speaker":"A","text":"松本さんは、いつもお昼に何を食べていますか？"},{"id":{"d":274,"n":4,"t":1},"speaker":"B","text":"毎日会社の食堂で食べています。佐藤さんのお薦めはありますか？"},{"id":{"d":274,"n":5,"t":1},"speaker":"A","text":"駅前にお薦めのラーメン屋があります。ラーメンは好きですか？"},{"id":{"d":274,"n":6,"t":1},"speaker":"B","text":"ラーメンは私の好物です。"},{"id":{"d":274,"n":7,"t":1},"speaker":"A","text":"良かったです。そこのチャーシュー麺はとてもおいしいですよ。"},{"id":{"d":274,"n":8,"t":1},"speaker":"B","text":"そうなのですね。では、大盛りを頼みたいと思います。"},{"id":{"d":274,"n":9,"t":1},"speaker":"A","text":"私は、いつも大盛りを頼みます。"}];
INSERT INTO sentence [{"id":{"d":275,"n":1,"t":1},"speaker":"A","text":"今日の夕食は何ですか？"},{"id":{"d":275,"n":2,"t":1},"speaker":"B","text":"ギョウザにしようと思っています。"},{"id":{"d":275,"n":3,"t":1},"speaker":"A","text":"ちょうど、私はギョウザが食べたいと思っていました。"},{"id":{"d":275,"n":4,"t":1},"speaker":"B","text":"ギョウザの手作りをするつもりなので、一緒に包みましょう。"},{"id":{"d":275,"n":5,"t":1},"speaker":"A","text":"はい、手伝います。"},{"id":{"d":275,"n":6,"t":1},"speaker":"B","text":"焼き加減はどのくらいが好きですか？"},{"id":{"d":275,"n":7,"t":1},"speaker":"A","text":"しっかり焦げ目をつけたギョウザが好きです。"},{"id":{"d":275,"n":8,"t":1},"speaker":"B","text":"分かりました。焼き過ぎには注意しますね。"}];
INSERT INTO sentence [{"id":{"d":276,"n":1,"t":1},"speaker":"A","text":"キッチンにクモの巣を見つけました。"},{"id":{"d":276,"n":2,"t":1},"speaker":"B","text":"それは大変です。そういえば、しばらく掃除をしていませんでしたね。"},{"id":{"d":276,"n":3,"t":1},"speaker":"A","text":"はい。今日は大掃除をしましょう。"},{"id":{"d":276,"n":4,"t":1},"speaker":"B","text":"そうですね。私は、キッチンとトイレを掃除します。"},{"id":{"d":276,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。では私は、洗面所とリビングを掃除しますね。"},{"id":{"d":276,"n":6,"t":1},"speaker":"B","text":"お願いします。浴室は、誰が担当しましょうか？"},{"id":{"d":276,"n":7,"t":1},"speaker":"A","text":"お父さんにお願いしましょう。"},{"id":{"d":276,"n":8,"t":1},"speaker":"B","text":"そうですね。お父さんは外出しているようですが、いつ帰ってきますか？"},{"id":{"d":276,"n":9,"t":1},"speaker":"A","text":"お父さんは、散髪に行きました。あと30分ほどで帰ると思います。"}];
INSERT INTO sentence [{"id":{"d":277,"n":1,"t":1},"speaker":"A","text":"大野さん、こんばんは。"},{"id":{"d":277,"n":2,"t":1},"speaker":"B","text":"田中さん、こんばんは。どこに行くのですか？"},{"id":{"d":277,"n":3,"t":1},"speaker":"A","text":"スーパーマーケットです。夕飯の材料を買います。"},{"id":{"d":277,"n":4,"t":1},"speaker":"B","text":"私も、スーパーマーケットに行きます。一緒に行きましょう。"},{"id":{"d":277,"n":5,"t":1},"speaker":"A","text":"はい。大野さんは、何を買いますか？"},{"id":{"d":277,"n":6,"t":1},"speaker":"B","text":"お刺身を買うつもりです。"},{"id":{"d":277,"n":7,"t":1},"speaker":"A","text":"いいですね。夕方はセールで安くなりますから。"},{"id":{"d":277,"n":8,"t":1},"speaker":"B","text":"はい。セールを狙って、いつも夕方にスーパーマーケットに行っています。"},{"id":{"d":277,"n":9,"t":1},"speaker":"A","text":"夕方のセールは見逃せませんね。私も、夕方に行くことが多いです。"}];
INSERT INTO sentence [{"id":{"d":278,"n":1,"t":1},"speaker":"A","text":"花さんは、お酒のおつまみに何を食べますか？"},{"id":{"d":278,"n":2,"t":1},"speaker":"B","text":"私は有り合わせのもので適当に作ったものを食べることが多いです。"},{"id":{"d":278,"n":3,"t":1},"speaker":"A","text":"いいですね。例えば、何を作るのですか？"},{"id":{"d":278,"n":4,"t":1},"speaker":"B","text":"昨日は、さばの缶詰めとキャベツを炒めたものと、茶わん蒸しを作りました。"},{"id":{"d":278,"n":5,"t":1},"speaker":"A","text":"豪華ですね。私も自分で作れるようになりたいです。"},{"id":{"d":278,"n":6,"t":1},"speaker":"B","text":"太郎さんは、おつまみに何を食べるのですか？"},{"id":{"d":278,"n":7,"t":1},"speaker":"A","text":"僕は、いつもコンビニエンスストアで買っていますね。ポテトチップスや柿の種が多いです。"},{"id":{"d":278,"n":8,"t":1},"speaker":"B","text":"スナック菓子が好きなのですね。"},{"id":{"d":278,"n":9,"t":1},"speaker":"A","text":"はい、そうです。子どもの頃から好みが変わらないのです。"}];
INSERT INTO sentence [{"id":{"d":279,"n":1,"t":1},"speaker":"A","text":"寒くなってきましたね。"},{"id":{"d":279,"n":2,"t":1},"speaker":"B","text":"ええ、冬が始まった気がします。"},{"id":{"d":279,"n":3,"t":1},"speaker":"A","text":"この時期になると、石焼きイモが食べたくなります。"},{"id":{"d":279,"n":4,"t":1},"speaker":"B","text":"石焼きイモを、食べたいですね。最近は、町で屋台を見かけなくなりました。"},{"id":{"d":279,"n":5,"t":1},"speaker":"A","text":"そうですね。この前、スーパーマーケットで見かけました。"},{"id":{"d":279,"n":6,"t":1},"speaker":"B","text":"スーパーマーケットでも売っているのですね。"},{"id":{"d":279,"n":7,"t":1},"speaker":"A","text":"会社のとなりのスーパーマーケットにもあるようですよ。"},{"id":{"d":279,"n":8,"t":1},"speaker":"B","text":"そうなのですか。今日の会社終わりに行ってみます。"}];
INSERT INTO sentence [{"id":{"d":280,"n":1,"t":1},"speaker":"A","text":"今月末、私の家で美咲さんの誕生日会をしましょう。"},{"id":{"d":280,"n":2,"t":1},"speaker":"B","text":"いいですね。プレゼントは各自用意するとして、食べ物を一緒に決めましょう。"},{"id":{"d":280,"n":3,"t":1},"speaker":"A","text":"そうですね。愛さんは、美咲さんの好きな食べ物を知っていますか？"},{"id":{"d":280,"n":4,"t":1},"speaker":"B","text":"ドーナツやシュークリームを、おやつに食べている姿をよく見ます。"},{"id":{"d":280,"n":5,"t":1},"speaker":"A","text":"甘いものが好きなようですね。それでは、スイーツをたくさん用意しましょう。"},{"id":{"d":280,"n":6,"t":1},"speaker":"B","text":"はい。あとは、ケーキも買いたいです。"},{"id":{"d":280,"n":7,"t":1},"speaker":"A","text":"そうですね。ケーキの種類はどうしますか？"},{"id":{"d":280,"n":8,"t":1},"speaker":"B","text":"美咲さんはイチゴが好きなので、ショートケーキ良いと思います。"},{"id":{"d":280,"n":9,"t":1},"speaker":"A","text":"分かりました。近くのケーキ屋で買っておきます。"},{"id":{"d":280,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。甘いものばかりになってしまうので、ピザの出前も取りましょう。"},{"id":{"d":280,"n":11,"t":1},"speaker":"A","text":"はい。豪華なパーティーになりそうですね。"}];
INSERT INTO sentence [{"id":{"d":281,"n":1,"t":1},"speaker":"A","text":"和也さんは普段から自炊をしていますか？"},{"id":{"d":281,"n":2,"t":1},"speaker":"B","text":"これまでは全くしていなかったのですが、最近になって始めました。"},{"id":{"d":281,"n":3,"t":1},"speaker":"A","text":"何かきっかけがあったのですか？"},{"id":{"d":281,"n":4,"t":1},"speaker":"B","text":"料理教室に通い始めたのです。習った料理を、家で作っています。"},{"id":{"d":281,"n":5,"t":1},"speaker":"A","text":"料理教室ですか。楽しそうですね。"},{"id":{"d":281,"n":6,"t":1},"speaker":"B","text":"手際は悪いですが、楽しんで通っています。"},{"id":{"d":281,"n":7,"t":1},"speaker":"A","text":"これまでに、どのような料理を習いましたか？"},{"id":{"d":281,"n":8,"t":1},"speaker":"B","text":"野菜の炒め物や炊き込みご飯などです。"},{"id":{"d":281,"n":9,"t":1},"speaker":"A","text":"いいですね。私も通ってみたいです。"},{"id":{"d":281,"n":10,"t":1},"speaker":"B","text":"体験教室があるので、もし良かったら紹介しますよ。"},{"id":{"d":281,"n":11,"t":1},"speaker":"A","text":"本当ですか。ぜひ、よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":282,"n":1,"t":1},"speaker":"A","text":"裁判所の前にマスコミが集まっていますね。どうしたのでしょうか？"},{"id":{"d":282,"n":2,"t":1},"speaker":"B","text":"注目の裁判が行われるようですよ。今朝のテレビのニュース番組で見ました。"},{"id":{"d":282,"n":3,"t":1},"speaker":"A","text":"そうなのですか。朝のニュースを見る習慣がないので、知りませんでした。"},{"id":{"d":282,"n":4,"t":1},"speaker":"B","text":"朝は忙しいのですか？"},{"id":{"d":282,"n":5,"t":1},"speaker":"A","text":"寝坊をしてしまうのです。家を出る10分前に起きるので、身支度で精いっぱいです。"},{"id":{"d":282,"n":6,"t":1},"speaker":"B","text":"だからいつも学校に着いてから食パンを食べているのですね。"},{"id":{"d":282,"n":7,"t":1},"speaker":"A","text":"はい。学校の売店で朝食を買っています。でも本当は、家で朝食を食べたいです。"},{"id":{"d":282,"n":8,"t":1},"speaker":"B","text":"それには、早寝早起きが必要ですね。徐々に習慣づけをしていきましょう。"},{"id":{"d":282,"n":9,"t":1},"speaker":"A","text":"はい。夜更かしをしないことから始めます。"}];
INSERT INTO sentence [{"id":{"d":283,"n":1,"t":1},"speaker":"A","text":"お母さん、お父さんが家を出るとき、傘を持っていましたか？"},{"id":{"d":283,"n":2,"t":1},"speaker":"B","text":"いいえ、持っていませんでした。なぜですか？"},{"id":{"d":283,"n":3,"t":1},"speaker":"A","text":"今日は夕方から土砂降りになるそうです。テレビ番組の天気予報で言っていました。"},{"id":{"d":283,"n":4,"t":1},"speaker":"B","text":"そうなのですか。コンビニエンスストアで傘を買って帰るしかなさそうですね。"},{"id":{"d":283,"n":5,"t":1},"speaker":"A","text":"ビニール傘がどんどん増えます。もう家に5本もあります。"},{"id":{"d":283,"n":6,"t":1},"speaker":"B","text":"日頃から、もっと天気予報を気にしなければなりませんね。"},{"id":{"d":283,"n":7,"t":1},"speaker":"A","text":"はい。あとは、折り畳み傘を持ち歩いた方がいいかもしれません。"},{"id":{"d":283,"n":8,"t":1},"speaker":"B","text":"そうですね。家にある折り畳み傘は折れているので、新しい物を買いましょう。"},{"id":{"d":283,"n":9,"t":1},"speaker":"A","text":"そうですね。一人一本買いましょう。"}];
INSERT INTO sentence [{"id":{"d":284,"n":1,"t":1},"speaker":"A","text":"今日は勇斗さんが帰国する日ですね。"},{"id":{"d":284,"n":2,"t":1},"speaker":"B","text":"はい、1時間後に空港で待ち合わせをしています。"},{"id":{"d":284,"n":3,"t":1},"speaker":"A","text":"勇斗さんが帰ってきたら、何をしますか？"},{"id":{"d":284,"n":4,"t":1},"speaker":"B","text":"まずは、家でご飯を食べましょう。日本料理を作る予定です。"},{"id":{"d":284,"n":5,"t":1},"speaker":"A","text":"勇斗さんにとっては、久しぶりの日本食ですね。"},{"id":{"d":284,"n":6,"t":1},"speaker":"B","text":"はい。勇斗さんの部屋は片付いていますか？"},{"id":{"d":284,"n":7,"t":1},"speaker":"A","text":"昨日、掃除をしておきました。"},{"id":{"d":284,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。すぐに風呂に入れるように、風呂を沸かした方がいいかもしれません。"},{"id":{"d":284,"n":9,"t":1},"speaker":"A","text":"そうですね。1時間後に沸かしておきます。"},{"id":{"d":284,"n":10,"t":1},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":285,"n":1,"t":1},"speaker":"A","text":"今日は夕飯は要りません。"},{"id":{"d":285,"n":2,"t":1},"speaker":"B","text":"どこかで食べてくるのですか？"},{"id":{"d":285,"n":3,"t":1},"speaker":"A","text":"はい。会社の人と居酒屋で飲み会をします。"},{"id":{"d":285,"n":4,"t":1},"speaker":"B","text":"分かりました。会社へは、電車で行くということですね。"},{"id":{"d":285,"n":5,"t":1},"speaker":"A","text":"はい。お酒を飲むので、車は家に置いていきます。"},{"id":{"d":285,"n":6,"t":1},"speaker":"B","text":"それでは、今日は私が車を借りてもよいですか？子どもを連れて、隣町の動物園に行きたいのです。"},{"id":{"d":285,"n":7,"t":1},"speaker":"A","text":"どうぞ使ってください。キーを渡しておきますね。"},{"id":{"d":285,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。助かります。"}];
INSERT INTO sentence [{"id":{"d":286,"n":1,"t":1},"speaker":"A","text":"天気予報によると、明日は晴天だそうです。"},{"id":{"d":286,"n":2,"t":1},"speaker":"B","text":"そうなのですか。それでは、公園に行ってピクニックをするのはどうですか？"},{"id":{"d":286,"n":3,"t":1},"speaker":"A","text":"いいですね。気持ちが良さそうです。"},{"id":{"d":286,"n":4,"t":1},"speaker":"B","text":"朝、家でサンドイッチを手作りしますね。"},{"id":{"d":286,"n":5,"t":1},"speaker":"A","text":"私も手伝います。材料はそろっていますか？"},{"id":{"d":286,"n":6,"t":1},"speaker":"B","text":"レタスと卵、ツナ缶がありません。"},{"id":{"d":286,"n":7,"t":1},"speaker":"A","text":"私がスーパーマーケットへ買いに行きますね。ついでに買うものは何かありますか？"},{"id":{"d":286,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。かつお節がなくなりそうなので、お願いできますか？"},{"id":{"d":286,"n":9,"t":1},"speaker":"A","text":"分かりました。今から行ってきます。"}];
INSERT INTO sentence [{"id":{"d":287,"n":1,"t":1},"speaker":"A","text":"一週間後は結婚記念日ですね。"},{"id":{"d":287,"n":2,"t":1},"speaker":"B","text":"そうですね。ディナーを食べに行きましょう。"},{"id":{"d":287,"n":3,"t":1},"speaker":"A","text":"そうですね。おいしいワインを飲みながら、ステーキを食べたいです。"},{"id":{"d":287,"n":4,"t":1},"speaker":"B","text":"いいですね。イタリアンレストランを予約しましょう。お薦めのお店があります。"},{"id":{"d":287,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。服装は、カジュアルではない方がいいですか？"},{"id":{"d":287,"n":6,"t":1},"speaker":"B","text":"そうですね。Tシャツやジーパンでは浮いてしまうかもしれません。"},{"id":{"d":287,"n":7,"t":1},"speaker":"A","text":"それでは、私は新しいワンピースを着ていきます。"},{"id":{"d":287,"n":8,"t":1},"speaker":"B","text":"私もジャケットを着ていきます。今から楽しみです。"}];
INSERT INTO sentence [{"id":{"d":288,"n":1,"t":1},"speaker":"A","text":"1人暮らしはどうですか？"},{"id":{"d":288,"n":2,"t":1},"speaker":"B","text":"楽しいですが、大変なことが多いです。"},{"id":{"d":288,"n":3,"t":1},"speaker":"A","text":"一番大変なことは、何ですか？"},{"id":{"d":288,"n":4,"t":1},"speaker":"B","text":"料理です。始めの一週間は毎日3食作っていたのですが、やめてしまいました。"},{"id":{"d":288,"n":5,"t":1},"speaker":"A","text":"毎日は大変ですよね。今の食事はどうしているのですか？"},{"id":{"d":288,"n":6,"t":1},"speaker":"B","text":"冷凍食品やレトルト食品に頼っています。最近は、おいしいものがたくさんあります。"},{"id":{"d":288,"n":7,"t":1},"speaker":"A","text":"スーパーマーケットやコンビニエンスストアで、手軽に買えて便利ですよね。私もよく買います。"},{"id":{"d":288,"n":8,"t":1},"speaker":"B","text":"この前買った、冷凍食品のチャーハンがおいしかったです。お米がパラパラでした。"},{"id":{"d":288,"n":9,"t":1},"speaker":"A","text":"食べたことがありません。今度、買ってみます。"}];
INSERT INTO sentence [{"id":{"d":289,"n":1,"t":1},"speaker":"A","text":"あおいさんに、このセーターをあげます。"},{"id":{"d":289,"n":2,"t":1},"speaker":"B","text":"お姉さんのお気に入りですよね。もらってよいのですか？"},{"id":{"d":289,"n":3,"t":1},"speaker":"A","text":"私には、小さくなってしまったので、あおいさんが着てください。"},{"id":{"d":289,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。暖かくなってきたので、来年用に取っておきますね。"},{"id":{"d":289,"n":5,"t":1},"speaker":"A","text":"そうしてください。衣替えは終わりましたか？"},{"id":{"d":289,"n":6,"t":1},"speaker":"B","text":"まだできていません。お姉さんはどうですか？"},{"id":{"d":289,"n":7,"t":1},"speaker":"A","text":"私は先ほど終えて、来年着ない洋服を整理しているところです。"},{"id":{"d":289,"n":8,"t":1},"speaker":"B","text":"なるほど。それでお下がりをくれたのですね。"},{"id":{"d":289,"n":9,"t":1},"speaker":"A","text":"その通りです。"}];
INSERT INTO sentence [{"id":{"d":290,"n":1,"t":1},"speaker":"A","text":"結衣さんはどこでアルバイトをしていますか？"},{"id":{"d":290,"n":2,"t":1},"speaker":"B","text":"私は、マンションの隣の定食屋でアルバイトをしています。"},{"id":{"d":290,"n":3,"t":1},"speaker":"A","text":"何度か行ったことがあります。私は煮物が好きです。"},{"id":{"d":290,"n":4,"t":1},"speaker":"B","text":"私も好きです。賄いが出るので、毎回おいしい食事ができるのですよ。"},{"id":{"d":290,"n":5,"t":1},"speaker":"A","text":"それはとてもいいですね。今もアルバイトを募集していますか？"},{"id":{"d":290,"n":6,"t":1},"speaker":"B","text":"店のドアに求人募集の張り紙があるので、まだ募集していると思います。"},{"id":{"d":290,"n":7,"t":1},"speaker":"A","text":"本当ですか。明日、見てみます。"},{"id":{"d":290,"n":8,"t":1},"speaker":"B","text":"ぜひ一、緒に働きましょう。"}];
INSERT INTO sentence [{"id":{"d":291,"n":1,"t":1},"speaker":"A","text":"ワイシャツに染みを付けてしまいました。"},{"id":{"d":291,"n":2,"t":1},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":291,"n":3,"t":1},"speaker":"A","text":"昨日うどん屋でカレーうどんを食べたときに、汁が飛んでしまったのです。"},{"id":{"d":291,"n":4,"t":1},"speaker":"B","text":"なるほど。この染みは、手洗いでは落ちなそうです。"},{"id":{"d":291,"n":5,"t":1},"speaker":"A","text":"そうですよね。クリーニング屋に持っていきます。"},{"id":{"d":291,"n":6,"t":1},"speaker":"B","text":"そうしてください。汁が飛びそうなときは、前掛けをしなければなりませんよ。"},{"id":{"d":291,"n":7,"t":1},"speaker":"A","text":"肝に銘じます。"},{"id":{"d":291,"n":8,"t":1},"speaker":"B","text":"ついでに、お父さんのスーツも一緒に持って行ってください。"},{"id":{"d":291,"n":9,"t":1},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":292,"n":1,"t":1},"speaker":"A","text":"今日は、近くの神社でお祭りがあるようです。"},{"id":{"d":292,"n":2,"t":1},"speaker":"B","text":"そうなのですか？"},{"id":{"d":292,"n":3,"t":1},"speaker":"A","text":"はい。ポストにチラシが入っていました。一緒に行きませんか？"},{"id":{"d":292,"n":4,"t":1},"speaker":"B","text":"行きましょう。屋台は出るのですか？"},{"id":{"d":292,"n":5,"t":1},"speaker":"A","text":"はい。出るようですよ。何か食べたいものはありますか？"},{"id":{"d":292,"n":6,"t":1},"speaker":"B","text":"私は焼きそばや、たこ焼きが食べたいです。"},{"id":{"d":292,"n":7,"t":1},"speaker":"A","text":"いいですね。私も好きです。"},{"id":{"d":292,"n":8,"t":1},"speaker":"B","text":"潤さんは、何か食べたいものはありますか？"},{"id":{"d":292,"n":9,"t":1},"speaker":"A","text":"私は、綿あめが食べたいです。"},{"id":{"d":292,"n":10,"t":1},"speaker":"B","text":"お祭りの定番ですね。ぜひ、食べましょう。"}];
INSERT INTO sentence [{"id":{"d":293,"n":1,"t":1},"speaker":"A","text":"今日は、一段と寒いですね。"},{"id":{"d":293,"n":2,"t":1},"speaker":"B","text":"はい。かなり冷え込んでいます。"},{"id":{"d":293,"n":3,"t":1},"speaker":"A","text":"寒い日は、温かいシチューが食べたくなります。"},{"id":{"d":293,"n":4,"t":1},"speaker":"B","text":"いいですね。冷蔵庫に牛肉があるので、ビーフシチューを作りましょう。"},{"id":{"d":293,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。私も何か手伝います。"},{"id":{"d":293,"n":6,"t":1},"speaker":"B","text":"それでは、ニンジンの皮むきをお願いします。"},{"id":{"d":293,"n":7,"t":1},"speaker":"A","text":"分かりました。何本使いますか？"},{"id":{"d":293,"n":8,"t":1},"speaker":"B","text":"2本使いましょう。"},{"id":{"d":293,"n":9,"t":1},"speaker":"A","text":"はい。ピーラーは、どこにありますか？"},{"id":{"d":293,"n":10,"t":1},"speaker":"B","text":"キッチンの引き出しの中にあります。"}];
INSERT INTO sentence [{"id":{"d":294,"n":1,"t":1},"speaker":"A","text":"今週の土曜日は、翔さんの誕生日ですね。"},{"id":{"d":294,"n":2,"t":1},"speaker":"B","text":"はい。"},{"id":{"d":294,"n":3,"t":1},"speaker":"A","text":"何か、食べたいものはありますか？"},{"id":{"d":294,"n":4,"t":1},"speaker":"B","text":"おすしが食べたいです。"},{"id":{"d":294,"n":5,"t":1},"speaker":"A","text":"おすしですね。出前を取りたいですか。それとも、すし屋に行きたいですか？"},{"id":{"d":294,"n":6,"t":1},"speaker":"B","text":"回転ずし屋に行きたいです。"},{"id":{"d":294,"n":7,"t":1},"speaker":"A","text":"分かりました。お父さんも仕事が休みなので、家族みんなで行きましょう。"},{"id":{"d":294,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":295,"n":1,"t":1},"speaker":"A","text":"風が強くなってきました。"},{"id":{"d":295,"n":2,"t":1},"speaker":"B","text":"はい。風の音が聞こえます。"},{"id":{"d":295,"n":3,"t":1},"speaker":"A","text":"ベランダに洗濯物を干しましたか？"},{"id":{"d":295,"n":4,"t":1},"speaker":"B","text":"干しました。"},{"id":{"d":295,"n":5,"t":1},"speaker":"A","text":"今のうちに、部屋の中に取り込んだ方がいいかもしれません。"},{"id":{"d":295,"n":6,"t":1},"speaker":"B","text":"そうですね。風で吹き飛んだら大変です。"},{"id":{"d":295,"n":7,"t":1},"speaker":"A","text":"洗濯物を干したのは、いつですか？"},{"id":{"d":295,"n":8,"t":1},"speaker":"B","text":"1時間前です。まだ乾いていないはずです。"},{"id":{"d":295,"n":9,"t":1},"speaker":"A","text":"それでは、今日は風呂場で干しましょう。"},{"id":{"d":295,"n":10,"t":1},"speaker":"B","text":"はい。今から取り込んできます。"},{"id":{"d":295,"n":11,"t":1},"speaker":"A","text":"私も行きます。"}];
INSERT INTO sentence [{"id":{"d":296,"n":1,"t":1},"speaker":"A","text":"花屋にカーネーションが、たくさん置いてありますね。"},{"id":{"d":296,"n":2,"t":1},"speaker":"B","text":"もうすぐ、母の日ですからね。"},{"id":{"d":296,"n":3,"t":1},"speaker":"A","text":"高田さんは、母の日に何かする予定はありますか？"},{"id":{"d":296,"n":4,"t":1},"speaker":"B","text":"母と一緒に食事に行きます。カーネーションも渡しますよ。"},{"id":{"d":296,"n":5,"t":1},"speaker":"A","text":"すてきですね。お母さまも喜ぶと思います。"},{"id":{"d":296,"n":6,"t":1},"speaker":"B","text":"新井さんは、何か予定はありますか？"},{"id":{"d":296,"n":7,"t":1},"speaker":"A","text":"私は、マッサージ機をプレゼントします。"},{"id":{"d":296,"n":8,"t":1},"speaker":"B","text":"それもすてきですね。"}];
INSERT INTO sentence [{"id":{"d":297,"n":1,"t":1},"speaker":"A","text":"ここのレストランのステーキは、絶品ですね。"},{"id":{"d":297,"n":2,"t":1},"speaker":"B","text":"はい。とても柔らかいです。"},{"id":{"d":297,"n":3,"t":1},"speaker":"A","text":"お店のレビューが高かったので期待していましたが、期待以上でした。"},{"id":{"d":297,"n":4,"t":1},"speaker":"B","text":"レストランの人気の理由が分かりましたね。"},{"id":{"d":297,"n":5,"t":1},"speaker":"A","text":"どうしたらこの様に柔らかく焼けるのでしょうか？"},{"id":{"d":297,"n":6,"t":1},"speaker":"B","text":"私も知りたいです。家ではいつも硬くしてしまいます。"},{"id":{"d":297,"n":7,"t":1},"speaker":"A","text":"私もお肉を焼くと、硬くしてしまいます。生焼けが心配で、つい焼きすぎてしまいます。"}];
INSERT INTO sentence [{"id":{"d":298,"n":1,"t":1},"speaker":"A","text":"すみません。道をお尋ねしてもよいですか？"},{"id":{"d":298,"n":2,"t":1},"speaker":"B","text":"はい。もちろんです。"},{"id":{"d":298,"n":3,"t":1},"speaker":"A","text":"コンビニエンスストアはどこにありますか？"},{"id":{"d":298,"n":4,"t":1},"speaker":"B","text":"大通りを真っすぐ歩いて、突き当りを右に曲がったところにあります。"},{"id":{"d":298,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。もう一つお伺いしてもよいですか？"},{"id":{"d":298,"n":6,"t":1},"speaker":"B","text":"はい。何でしょうか？"},{"id":{"d":298,"n":7,"t":1},"speaker":"A","text":"この近くに、薬局はありますか？"},{"id":{"d":298,"n":8,"t":1},"speaker":"B","text":"すぐ近くにはありませんが、最寄り駅のそばにはありますよ。"},{"id":{"d":298,"n":9,"t":1},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":299,"n":1,"t":1},"speaker":"A","text":"お母さん、サバの缶詰は使い切りましたか？"},{"id":{"d":299,"n":2,"t":1},"speaker":"B","text":"いえ、まだ冷蔵庫に残っています。"},{"id":{"d":299,"n":3,"t":1},"speaker":"A","text":"どのくらい残っていますか？"},{"id":{"d":299,"n":4,"t":1},"speaker":"B","text":"3缶です。"},{"id":{"d":299,"n":5,"t":1},"speaker":"A","text":"今日中に食べなければなりません。賞味期限が今日までです。"},{"id":{"d":299,"n":6,"t":1},"speaker":"B","text":"そうなのですね。それでは、今日の夜ご飯に使いましょう。"},{"id":{"d":299,"n":7,"t":1},"speaker":"A","text":"お願いします。メニューは何にしますか？"},{"id":{"d":299,"n":8,"t":1},"speaker":"B","text":"サバとキャベツの炒め物はどうですか？"},{"id":{"d":299,"n":9,"t":1},"speaker":"A","text":"いいですね。私も手伝います。"}];
INSERT INTO sentence [{"id":{"d":300,"n":1,"t":1},"speaker":"A","text":"大谷さんは、朝食に必ず食べるものはありますか？"},{"id":{"d":300,"n":2,"t":1},"speaker":"B","text":"私は、ゆで卵を食べます。"},{"id":{"d":300,"n":3,"t":1},"speaker":"A","text":"ゆで卵のゆで加減は、人によって好みが分かれますよね？"},{"id":{"d":300,"n":4,"t":1},"speaker":"B","text":"はい。半熟が好きな方も、固めが好きな方もいますね。"},{"id":{"d":300,"n":5,"t":1},"speaker":"A","text":"大谷さんは、どのくらいのゆで加減が好きですか？"},{"id":{"d":300,"n":6,"t":1},"speaker":"B","text":"私は固めが好きですね。毎朝、お鍋に入れた水と卵を、しっかり火にかけています。"},{"id":{"d":300,"n":7,"t":1},"speaker":"A","text":"そうなのですね。"},{"id":{"d":300,"n":8,"t":1},"speaker":"B","text":"高橋さんはどうですか？"},{"id":{"d":300,"n":9,"t":1},"speaker":"A","text":"私は半熟卵が好きです。ただ、火加減に失敗して、黄身をぱさぱさにしてしまうことも多いです。"},{"id":{"d":300,"n":10,"t":1},"speaker":"B","text":"半熟卵は難しいですよね。"}];
INSERT INTO sentence [{"id":{"d":301,"n":1,"t":1},"speaker":"A","text":"森田さんは、ファミリーレストランのキャンペーンを知っていますか？"},{"id":{"d":301,"n":2,"t":1},"speaker":"B","text":"いいえ、知りません。何のキャンペーンですか？"},{"id":{"d":301,"n":3,"t":1},"speaker":"A","text":"毎月最終日だけ、2,500円で食べ放題になるキャンペーンです。"},{"id":{"d":301,"n":4,"t":1},"speaker":"B","text":"そうなのですか。それは良いですね。"},{"id":{"d":301,"n":5,"t":1},"speaker":"A","text":"ファミリーレストランはメニューが豊富なので、食べ放題も楽しいと思います。"},{"id":{"d":301,"n":6,"t":1},"speaker":"B","text":"そうですね。ファミリーレストランに行くといつもメニューに迷うので、食べ放題は魅力的です。"},{"id":{"d":301,"n":7,"t":1},"speaker":"A","text":"今月の最終日は土曜日なので、学校がお休みです。ランチを食べに行きませんか？"},{"id":{"d":301,"n":8,"t":1},"speaker":"B","text":"ぜひ、行きたいです。予定を空けておきますね。"}];
INSERT INTO sentence [{"id":{"d":302,"n":1,"t":1},"speaker":"A","text":"今度の旅行で、泊まる場所を決めましょう。"},{"id":{"d":302,"n":2,"t":1},"speaker":"B","text":"そうですね。ホテルと旅館では、どちらがいいですか？"},{"id":{"d":302,"n":3,"t":1},"speaker":"A","text":"私は旅館がいいです。和室が落ち着きます。"},{"id":{"d":302,"n":4,"t":1},"speaker":"B","text":"分かりました。私も旅館が好きなので、旅館にしましょう。"},{"id":{"d":302,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。たかしさんは、何か希望はありますか？"},{"id":{"d":302,"n":6,"t":1},"speaker":"B","text":"朝食が付いているプランがいいです。"},{"id":{"d":302,"n":7,"t":1},"speaker":"A","text":"その方がゆっくりできそうですね。そうしましょう。"},{"id":{"d":302,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。ウェブで検索してみます。"}];
INSERT INTO sentence [{"id":{"d":303,"n":1,"t":1},"speaker":"A","text":"放課後にカラオケに行きませんか？"},{"id":{"d":303,"n":2,"t":1},"speaker":"B","text":"いいですね。行きたいです。"},{"id":{"d":303,"n":3,"t":1},"speaker":"A","text":"真由美さんは、カラオケに行ったら何を歌いますか？"},{"id":{"d":303,"n":4,"t":1},"speaker":"B","text":"私はアニメを見ることが好きなので、アニメの主題歌を歌います。"},{"id":{"d":303,"n":5,"t":1},"speaker":"A","text":"アニメの主題歌ですか。私もアニメをよく見るので、知っている歌が多いと思います。"},{"id":{"d":303,"n":6,"t":1},"speaker":"B","text":"よかったです。春香さんは何を歌いますか？"},{"id":{"d":303,"n":7,"t":1},"speaker":"A","text":"私はその時々のヒット曲を歌います。"},{"id":{"d":303,"n":8,"t":1},"speaker":"B","text":"歌をたくさん知っているのですね？"},{"id":{"d":303,"n":9,"t":1},"speaker":"A","text":"はい。音楽を聴くことが好きで、通学時間中は常に聴いてきます。"}];
INSERT INTO sentence [{"id":{"d":304,"n":1,"t":1},"speaker":"A","text":"食べ物屋に忘れ物をしてしまいました。"},{"id":{"d":304,"n":2,"t":1},"speaker":"B","text":"何を忘れたのですか？"},{"id":{"d":304,"n":3,"t":1},"speaker":"A","text":"セーターです。脱いだまま、置いてきてしまいました。"},{"id":{"d":304,"n":4,"t":1},"speaker":"B","text":"なぜ、セーターを脱いだのですか？"},{"id":{"d":304,"n":5,"t":1},"speaker":"A","text":"鍋料理を食べていたら、暑くなって脱いだのです。その日はとても寒くて厚着をしていました。"},{"id":{"d":304,"n":6,"t":1},"speaker":"B","text":"そういうことでしたか。お店の人に忘れ物があるかどうか、確認しましたか？"},{"id":{"d":304,"n":7,"t":1},"speaker":"A","text":"いえ、できていません。忘れ物に気付いたときには、閉店時間を過ぎていました。"},{"id":{"d":304,"n":8,"t":1},"speaker":"B","text":"それでは、明日になってから確認するしかありませんね。"},{"id":{"d":304,"n":9,"t":1},"speaker":"A","text":"はい。仕事の合間に食べ物屋に寄ってみます。"}];
INSERT INTO sentence [{"id":{"d":305,"n":1,"t":1},"speaker":"A","text":"麗子さんは何のアルバイトをしていますか？"},{"id":{"d":305,"n":2,"t":1},"speaker":"B","text":"ペットショップで働いています。"},{"id":{"d":305,"n":3,"t":1},"speaker":"A","text":"動物が好きなのですか？"},{"id":{"d":305,"n":4,"t":1},"speaker":"B","text":"はい。小さいときは、毎週動物園に行っていました。"},{"id":{"d":305,"n":5,"t":1},"speaker":"A","text":"そうなのですか。家で犬や猫を飼っていますか？"},{"id":{"d":305,"n":6,"t":1},"speaker":"B","text":"いいえ、飼っていません。私の住んでいるアパートでは、動物を飼うことが禁止されています。"},{"id":{"d":305,"n":7,"t":1},"speaker":"A","text":"そうなのですね。将来は飼いたいと考えていますか？"},{"id":{"d":305,"n":8,"t":1},"speaker":"B","text":"はい。飼いたいです。飼うことのできる住まいに引っ越せるよう、お金をためています。"}];
INSERT INTO sentence [{"id":{"d":306,"n":1,"t":1},"speaker":"A","text":"何か音が鳴り続いていますね？"},{"id":{"d":306,"n":2,"t":1},"speaker":"B","text":"はい。これは目覚まし時計のアラームです。"},{"id":{"d":306,"n":3,"t":1},"speaker":"A","text":"2階の部屋で鳴っている気がします。"},{"id":{"d":306,"n":4,"t":1},"speaker":"B","text":"お父さんの部屋ですね。昨日は帰りが遅かったので、まだ起きていないのだと思います。"},{"id":{"d":306,"n":5,"t":1},"speaker":"A","text":"そうでしたか。私がアラームを止めに行きましょうか？"},{"id":{"d":306,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。お願いします。"},{"id":{"d":306,"n":7,"t":1},"speaker":"A","text":"お父さんを、起こしますか？"},{"id":{"d":306,"n":8,"t":1},"speaker":"B","text":"あと30分は寝かせてあげましょう。"},{"id":{"d":306,"n":9,"t":1},"speaker":"A","text":"そうですね。分かりました。"}];
INSERT INTO sentence [{"id":{"d":307,"n":1,"t":1},"speaker":"A","text":"ポストに公民館からのお知らせが入っていました。"},{"id":{"d":307,"n":2,"t":1},"speaker":"B","text":"何のお知らせですか？"},{"id":{"d":307,"n":3,"t":1},"speaker":"A","text":"年明けに行われる、イベントのお知らせです。"},{"id":{"d":307,"n":4,"t":1},"speaker":"B","text":"イベントですか。どのようなイベントが開催されるのですか？"},{"id":{"d":307,"n":5,"t":1},"speaker":"A","text":"百人一首大会です。誰でも参加できるそうですよ。"},{"id":{"d":307,"n":6,"t":1},"speaker":"B","text":"まさとさんは、百人一首を全て暗記していますよね。参加してみたらどうですか？"},{"id":{"d":307,"n":7,"t":1},"speaker":"A","text":"はい。参加してみようと思います。"},{"id":{"d":307,"n":8,"t":1},"speaker":"B","text":"優勝者には、何か景品があるのですか？"},{"id":{"d":307,"n":9,"t":1},"speaker":"A","text":"はい。うどん屋の無料券が30回分もらえるそうです。"},{"id":{"d":307,"n":10,"t":1},"speaker":"B","text":"それはいいですね。頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":308,"n":1,"t":1},"speaker":"A","text":"今日は暑いですね？"},{"id":{"d":308,"n":2,"t":1},"speaker":"B","text":"はい。朝のニュースで、今日は猛暑日になると言っていました。"},{"id":{"d":308,"n":3,"t":1},"speaker":"A","text":"暑い日には、冷たいものが食べたくなります。"},{"id":{"d":308,"n":4,"t":1},"speaker":"B","text":"分かります。石井さんは、何が食べたいですか？"},{"id":{"d":308,"n":5,"t":1},"speaker":"A","text":"冷やし中華です。夏は、うどんやそばの代わりに冷やし中華ばかり食べています。"},{"id":{"d":308,"n":6,"t":1},"speaker":"B","text":"私も冷やし中華は好きです。"},{"id":{"d":308,"n":7,"t":1},"speaker":"A","text":"林さんは、暑い日に何が食べたくなりますか？"},{"id":{"d":308,"n":8,"t":1},"speaker":"B","text":"かき氷です。子どもの頃から好きですね。"},{"id":{"d":308,"n":9,"t":1},"speaker":"A","text":"私も子どもの頃よく食べていました。"}];
INSERT INTO sentence [{"id":{"d":309,"n":1,"t":1},"speaker":"A","text":"朝食はパンを食べますか？ご飯を食べますか？"},{"id":{"d":309,"n":2,"t":1},"speaker":"B","text":"私はご飯が好きなので、毎日ご飯を食べます。"},{"id":{"d":309,"n":3,"t":1},"speaker":"A","text":"何かご飯にかけて食べていますか？"},{"id":{"d":309,"n":4,"t":1},"speaker":"B","text":"何もかけないときもあれば、卵かけご飯や納豆ご飯にすることもあります。"},{"id":{"d":309,"n":5,"t":1},"speaker":"A","text":"いろいろな食べ方をしているのですね。"},{"id":{"d":309,"n":6,"t":1},"speaker":"B","text":"はい。森さんは、パンとご飯どちらを食べていますか？"},{"id":{"d":309,"n":7,"t":1},"speaker":"A","text":"私は半々くらいです。パンもご飯も好きなので、その日の気分で決めています。"},{"id":{"d":309,"n":8,"t":1},"speaker":"B","text":"それはいいですね。"}];
INSERT INTO sentence [{"id":{"d":310,"n":1,"t":1},"speaker":"A","text":"ピザ生地が出来上がりました。"},{"id":{"d":310,"n":2,"t":1},"speaker":"B","text":"あとは、この上に具をのせるのですね？"},{"id":{"d":310,"n":3,"t":1},"speaker":"A","text":"はい。好きなものをのせてよいですよ。"},{"id":{"d":310,"n":4,"t":1},"speaker":"B","text":"迷ってしまいますね。お姉さんは、何をのせますか？"},{"id":{"d":310,"n":5,"t":1},"speaker":"A","text":"私はトマトとピーマンとチーズをのせます。"},{"id":{"d":310,"n":6,"t":1},"speaker":"B","text":"いいですね。それでは私は、ハムとバジルをのせます。"},{"id":{"d":310,"n":7,"t":1},"speaker":"A","text":"おいしそうです。甘いピザも作りますか？"},{"id":{"d":310,"n":8,"t":1},"speaker":"B","text":"そうですね。チョコレートとマシュマロをのせて、デザートにしましょう。"}];
INSERT INTO sentence [{"id":{"d":311,"n":1,"t":1},"speaker":"A","text":"高木さんは、健康のために気を付けていることはありますか？"},{"id":{"d":311,"n":2,"t":1},"speaker":"B","text":"私は、毎日ジョギングをしています。"},{"id":{"d":311,"n":3,"t":1},"speaker":"A","text":"毎日ですか。何キロメートルくらい走っているのですか？"},{"id":{"d":311,"n":4,"t":1},"speaker":"B","text":"家から消防署までを往復しています。距離でいうと、4キロメートルほどです。"},{"id":{"d":311,"n":5,"t":1},"speaker":"A","text":"素晴らしいですね。"},{"id":{"d":311,"n":6,"t":1},"speaker":"B","text":"橋本さんは、何か気を付けていることはありますか？"},{"id":{"d":311,"n":7,"t":1},"speaker":"A","text":"私は、食事の際に野菜を多く取り入れていることくらいです。"},{"id":{"d":311,"n":8,"t":1},"speaker":"B","text":"それも素晴らしいですね。おいしい野菜料理を、今度ぜひ教えてください。"},{"id":{"d":311,"n":9,"t":1},"speaker":"A","text":"はい。私もジョギングを教わりたいです。"}];
INSERT INTO sentence [{"id":{"d":312,"n":1,"t":1},"speaker":"A","text":"今日の昼食は何を食べたいですか？"},{"id":{"d":312,"n":2,"t":1},"speaker":"B","text":"今日は休日なので料理をしたくありません。テイクアウトをしませんか？"},{"id":{"d":312,"n":3,"t":1},"speaker":"A","text":"では、久しぶりに商店街のパン屋へでも行きますか？"},{"id":{"d":312,"n":4,"t":1},"speaker":"B","text":"いいですね。とてもおいしいのですがなかなか行く機会がありませんでした。久しぶりに行きたいです。"},{"id":{"d":312,"n":5,"t":1},"speaker":"A","text":"早速、出かけましょう。そういえば、最近は店内に入れる人数が制限されているようです。食べたいパンがあれば私が買ってくるので教えてください。"},{"id":{"d":312,"n":6,"t":1},"speaker":"B","text":"クロワッサンダマンドをお願いします。私が一番好きなパンです。"},{"id":{"d":312,"n":7,"t":1},"speaker":"A","text":"了解しました。みんなが好きなカボチャパンも買ってきますね。他は私が全部選んで購入して良いですか？"},{"id":{"d":312,"n":8,"t":1},"speaker":"B","text":"はい。惣菜パンやデザート系のパンぜひも買ってきてください。"}];
INSERT INTO sentence [{"id":{"d":313,"n":1,"t":1},"speaker":"A","text":"1人暮らしを始めたと聞きました。いつから始めていたのですか？"},{"id":{"d":313,"n":2,"t":1},"speaker":"B","text":"先月から始めました。今は料理することに苦労しています。"},{"id":{"d":313,"n":3,"t":1},"speaker":"A","text":"冷凍食品やインスタント食品は使っていないのですか？"},{"id":{"d":313,"n":4,"t":1},"speaker":"B","text":"はい、健康に気を使ってできるだけ自分で作った料理を食べるようにしています。"},{"id":{"d":313,"n":5,"t":1},"speaker":"A","text":"偉いですね。何を作ることが多いですか？"},{"id":{"d":313,"n":6,"t":1},"speaker":"B","text":"最近の得意料理はマーボー春雨です。簡単で量もたくさん作れるのでよく作っています。"},{"id":{"d":313,"n":7,"t":1},"speaker":"A","text":"副菜はどのようなものを食べることが多いですか？"},{"id":{"d":313,"n":8,"t":1},"speaker":"B","text":"ポテトサラダが簡単なので作っています。次の目標は大好きなナスの揚げ浸しを作ることです。"}];
INSERT INTO sentence [{"id":{"d":314,"n":1,"t":1},"speaker":"A","text":"普段は、練習着姿しか見たことがないので、伊藤先輩のスーツ姿が新鮮です。"},{"id":{"d":314,"n":2,"t":1},"speaker":"B","text":"そうですね。普段スーツを着ない人がスーツを着ていると、それだけで一瞬誰か分からなくなることもあります。"},{"id":{"d":314,"n":3,"t":1},"speaker":"A","text":"そういえば、父も最近はテレワークが多く、普段着で家にいることが多いのですが、数少ない出社の際にスーツを着ていて思わず二度見してしまいました。"},{"id":{"d":314,"n":4,"t":1},"speaker":"B","text":"着る服によってがらりと印象は変わるものですし、服装に気を使うことはやはり大事ですね。"}];
INSERT INTO sentence [{"id":{"d":315,"n":1,"t":1},"speaker":"A","text":"今日は、友香ちゃんの誕生日パーティーをするので、ケーキを買ってこようと思います。どのようなケーキが良いですか？"},{"id":{"d":315,"n":2,"t":1},"speaker":"B","text":"私は果物アレルギーなので、チーズケーキか、チョコレートケーキが良いです。"},{"id":{"d":315,"n":3,"t":1},"speaker":"A","text":"分かりました。いつ頃、果物アレルギーが発症したのですか？"},{"id":{"d":315,"n":4,"t":1},"speaker":"B","text":"3年ほど前にイチゴを食べたらいきなり呼吸ができなくなってしまい、昨年アレルギー検査をしたらアレルギーであることが分かりました。"},{"id":{"d":315,"n":5,"t":1},"speaker":"A","text":"最近、果物アレルギーになってしまったのですね。"},{"id":{"d":315,"n":6,"t":1},"speaker":"B","text":"はい。最近ようやく果物を食べない生活に慣れてきたところです。"}];
INSERT INTO sentence [{"id":{"d":316,"n":1,"t":1},"speaker":"A","text":"感染症のまん延に伴い、日本では保存食品や缶詰が流行しているようです。"},{"id":{"d":316,"n":2,"t":1},"speaker":"B","text":"いつ外出できなくなるか分からない不安から、購入者が続出しているようですね。"},{"id":{"d":316,"n":3,"t":1},"speaker":"A","text":"調理の必要がないことに加えて、日持ちもしますので災害の際にも有効活用でき、良い習慣になっていると思います。先月末にはインスタント食品を加熱するため専用の機械が発売されました。普段はお湯を沸かして温めますが、その機械は電気で温めるそうです。"},{"id":{"d":316,"n":4,"t":1},"speaker":"B","text":"それはすごいですね。時間が短縮できますね。どのような大きさのものでも温めることは可能なのですか？"},{"id":{"d":316,"n":5,"t":1},"speaker":"A","text":"大中小と分けられていて、それぞれの大きさに対応した温めの時間になるそうです。"},{"id":{"d":316,"n":6,"t":1},"speaker":"B","text":"最適な温度に加熱されるのですね。前もって調理された料理が、インスタント食品として発売される日も近いかもしれませんね。"},{"id":{"d":316,"n":7,"t":1},"speaker":"A","text":"既に、ほとんど毎日インスタント食品を食べて暮らしている人もいるそうなので、不可能ではなさそうです。"}];
INSERT INTO sentence [{"id":{"d":317,"n":1,"t":1},"speaker":"A","text":"最近の若い人は、図書館では勉強をしないようです。"},{"id":{"d":317,"n":2,"t":1},"speaker":"B","text":"それならば、どこで勉強をしているのでしょうか？"},{"id":{"d":317,"n":3,"t":1},"speaker":"A","text":"スターバックスやドトールコーヒーといったカフェで勉強する人が多いようです。"},{"id":{"d":317,"n":4,"t":1},"speaker":"B","text":"確かに、ミスタードーナツでも勉強や仕事をしている人を最近多く見かけますね。お金がかかりそうですが、SNSなどに投稿できるという利点があるのでしょうか？"},{"id":{"d":317,"n":5,"t":1},"speaker":"A","text":"それも、あるかもしれません。雑音がある方が、集中できるという人もいるようです。私も浪人中は、毎朝マクドナルドでコーヒーを頼み、予備校が開くまで勉強していました。"},{"id":{"d":317,"n":6,"t":1},"speaker":"B","text":"なるほど、そういう理由があるのですね。"}];
INSERT INTO sentence [{"id":{"d":318,"n":1,"t":1},"speaker":"A","text":"近頃、来年の就職後に住むための家を探しています。"},{"id":{"d":318,"n":2,"t":1},"speaker":"B","text":"春から社会人ということで、1人暮らしを始めるのですね？"},{"id":{"d":318,"n":3,"t":1},"speaker":"A","text":"はい、しかし自分の求めている条件の家があまりありません。"},{"id":{"d":318,"n":4,"t":1},"speaker":"B","text":"どのような条件の家を探しているのですか？"},{"id":{"d":318,"n":5,"t":1},"speaker":"A","text":"内定先は、最近テレワークが主な勤務形態となっているため、大学の先輩方によるとテレワークの部屋があった方がいいということでした。そこで仕事用の部屋があるような家を探しています。"},{"id":{"d":318,"n":6,"t":1},"speaker":"B","text":"リビングとは別に、テレワークの部屋があった方がいいということですか？"},{"id":{"d":318,"n":7,"t":1},"speaker":"A","text":"そのようにおっしゃっていました。また、家賃補助が支給されるのは8万円の家までなのでその範囲に収めることが難しいです。"},{"id":{"d":318,"n":8,"t":1},"speaker":"B","text":"東京から少し離れた場所に住むことで安く、少し広い家はありそうですね。"}];
INSERT INTO sentence [{"id":{"d":319,"n":1,"t":1},"speaker":"A","text":"最寄り駅から電車移動をするとき、電車代が高くて外出する気になれません。"},{"id":{"d":319,"n":2,"t":1},"speaker":"B","text":"一区間の電車代が全国一高いということを聞いたことがあります。"},{"id":{"d":319,"n":3,"t":1},"speaker":"A","text":"その通りです。最近値下がりをしたので少し使いやすくなりましたが、依然として高いです。"},{"id":{"d":319,"n":4,"t":1},"speaker":"B","text":"高校への登校は、どのようにしていたのですか？電車通学は高そうですが。"},{"id":{"d":319,"n":5,"t":1},"speaker":"A","text":"1、2年生の時は、片道1時間でしたが自転車で通学していました。3年生の時は、受験勉強をしたかったので高校の直通バスに乗っていました。"},{"id":{"d":319,"n":6,"t":1},"speaker":"B","text":"そうですか。友達と遊ぶ時は、どうしていたのですか？"},{"id":{"d":319,"n":7,"t":1},"speaker":"A","text":"高校時代には、電車移動時の電車代が高すぎるということがあったので、あまり遊んでいませんでした。クラス会には参加していましたが、個別に仲の良い友達と外出はほとんどなかったです。"}];
INSERT INTO sentence [{"id":{"d":320,"n":1,"t":1},"speaker":"A","text":"私の住んでいるところは、数年前まで村でした。"},{"id":{"d":320,"n":2,"t":1},"speaker":"B","text":"元々ということは、今はもう合併などして村ではないのですか？"},{"id":{"d":320,"n":3,"t":1},"speaker":"A","text":"はい。住所がとても短くなったので、その点はとてもうれしいです。"},{"id":{"d":320,"n":4,"t":1},"speaker":"B","text":"住所以外に、何か変わったことはありますか？"},{"id":{"d":320,"n":5,"t":1},"speaker":"A","text":"村では村役場と言いますが、市では市役所ということが幼い頃には衝撃的なことでした。"},{"id":{"d":320,"n":6,"t":1},"speaker":"B","text":"小さい頃は、いきなり同じ意味のものが言葉だけ変わったら衝撃を受けますよね？"},{"id":{"d":320,"n":7,"t":1},"speaker":"A","text":"そうですね。加えてその頃、友達に役場と言ってもあまり通じていなかったこともあったので、その点も衝撃を受けました。"},{"id":{"d":320,"n":8,"t":1},"speaker":"B","text":"幼少期から、そのような体験ができたことは良いことでしたね。いろいろな言葉を学ぶ機会になっていますね。"}];
INSERT INTO sentence [{"id":{"d":321,"n":1,"t":1},"speaker":"A","text":"支払いは、最近どのようにしていますか？"},{"id":{"d":321,"n":2,"t":1},"speaker":"B","text":"私は、キャッシュレス決済にしています。現金を持ち歩くのが億劫になってしまいました。"},{"id":{"d":321,"n":3,"t":1},"speaker":"A","text":"最近は、コンビニエンスストアやスーパーマーケットなど、多くの店舗でクレジットカードによる支払いやQRコード決済、QUICPayに対応していますね。"},{"id":{"d":321,"n":4,"t":1},"speaker":"B","text":"現金を持ち歩く習慣がなくなってしまったので、支払いが現金のみの店だった場合、お金を引き出しに行かないといけません。なぜ全店舗でキャッシュレス決済が導入されないのでしょうか？"},{"id":{"d":321,"n":5,"t":1},"speaker":"A","text":"店舗にとって、客に現金を使ってもらうことの良さも、もちろんあります。大きな利点は手数料がキャッシュレス決済業者に取られないということです。"},{"id":{"d":321,"n":6,"t":1},"speaker":"B","text":"売り上げが、そのまま利益になるということですね。"},{"id":{"d":321,"n":7,"t":1},"speaker":"A","text":"その通りです。そのため昔ながらの八百屋などはキャッシュレス決済の機械の扱いを学ぶ必要があることや、手数料が取られることから、キャッシュレスを導入しないと考えられます。"},{"id":{"d":321,"n":8,"t":1},"speaker":"B","text":"昔からの店は、常連の方も現金に慣れているということがあるので、よほどの利点がない限り導入しないと思います。"}];
INSERT INTO sentence [{"id":{"d":322,"n":1,"t":1},"speaker":"A","text":"新年のテレビ特番で本物と偽物を比較し、本物を当てるという企画の番組をしていました。"},{"id":{"d":322,"n":2,"t":1},"speaker":"B","text":"プロフェッショナルとアマチュアを比較したり、さまざまな比較を行ったりしているものですね。"},{"id":{"d":322,"n":3,"t":1},"speaker":"A","text":"その中で本物のカニとカニカマを比較するという場面がありました。"},{"id":{"d":322,"n":4,"t":1},"speaker":"B","text":"味付けは同じもので、カニ本来のうまみを当てるという問題でしたね。"},{"id":{"d":322,"n":5,"t":1},"speaker":"A","text":"そうです。そのカニカマに惑わされている人が、とても多かったことを覚えています。"},{"id":{"d":322,"n":6,"t":1},"speaker":"B","text":"私は出題されていたカニカマがとても気になり、スーパーマーケットで探したのですが、ほとんど売り切れていました。"},{"id":{"d":322,"n":7,"t":1},"speaker":"A","text":"とても人気になり、企業も力をより入れ始め、「サラダプラス ほぼホタテ」や「大粒ほぼカキフライ」などさまざまなほぼシリーズを開発しているようです。"},{"id":{"d":322,"n":8,"t":1},"speaker":"B","text":"とても興味が湧きました。食べ比べしてみたいです。"}];
INSERT INTO sentence [{"id":{"d":323,"n":1,"t":1},"speaker":"A","text":"先日、ケーキ屋に行った時に、賞味期限が30分のシュークリームというものを見つけました。"},{"id":{"d":323,"n":2,"t":1},"speaker":"B","text":"とても短いですね。なぜ30分なのですか？"},{"id":{"d":323,"n":3,"t":1},"speaker":"A","text":"注文を受けてその場でクリームを作り、中に入れてくれるようです。"},{"id":{"d":323,"n":4,"t":1},"speaker":"B","text":"クリームが作り立てのうちに食べてもらいたいので、賞味期限という言葉を使っているということでしょうね。"},{"id":{"d":323,"n":5,"t":1},"speaker":"A","text":"外はカリカリで中は程よく甘く、くどくないクリームでとてもおいしかったです。"},{"id":{"d":323,"n":6,"t":1},"speaker":"B","text":"おいしいシュークリームは、いくつでも食べることができてしまうので、体型管理には大きな敵ですが、ぜひ食べてみたいです。"},{"id":{"d":323,"n":7,"t":1},"speaker":"A","text":"賞味期限がとても短いので、持って帰ることは難しいですが、機会があったら、ぜひお連れしますね。"},{"id":{"d":323,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。その時にはたくさん食べたいと思います。"}];
INSERT INTO sentence [{"id":{"d":324,"n":1,"t":1},"speaker":"A","text":"日本の料理は、とてもおいしいと海外の人からも人気ですね。"},{"id":{"d":324,"n":2,"t":1},"speaker":"B","text":"東京2020オリンピック・パラリンピックでも、食事がとてもおいしいと選手に好評だったようです。"},{"id":{"d":324,"n":3,"t":1},"speaker":"A","text":"その中でも、何が一番の人気だったのですか？"},{"id":{"d":324,"n":4,"t":1},"speaker":"B","text":"ギョウザが一番人気だったそうです。"},{"id":{"d":324,"n":5,"t":1},"speaker":"A","text":"ギョウザは本当においしいですよね。しかし人気が出ると料理を作る方がたくさん作らないといけないので大変ですね。"},{"id":{"d":324,"n":6,"t":1},"speaker":"B","text":"それが、東京2020オリンピック・パラリンピックの選手村で出されていたギョウザは冷凍食品だったそうです。"},{"id":{"d":324,"n":7,"t":1},"speaker":"A","text":"なんと、冷凍のギョウザが人気だったのですね。そのギョウザを私もぜひ食べてみたいです。"}];
INSERT INTO sentence [{"id":{"d":325,"n":1,"t":1},"speaker":"A","text":"銀行の通帳が、なくなってきていることは知っていますか？"},{"id":{"d":325,"n":2,"t":1},"speaker":"B","text":"知りませんでした。今はどのような形で口座の内容を確認できるのですか？"},{"id":{"d":325,"n":3,"t":1},"speaker":"A","text":"スマートフォンのアプリケーションやダイレクトバンキングを利用して、確認することができます。"},{"id":{"d":325,"n":4,"t":1},"speaker":"B","text":"今は、わざわざ銀行に行かなくても残高確認できる便利な世の中になったのですね。"},{"id":{"d":325,"n":5,"t":1},"speaker":"A","text":"加えてスマートフォンの生体認証などを用いてログインした場合、送金をすることも可能です。"},{"id":{"d":325,"n":6,"t":1},"speaker":"B","text":"送金までできるのですね。銀行に行くのはお金を出し入れするときのみになりますね。"},{"id":{"d":325,"n":7,"t":1},"speaker":"A","text":"それがネットバンクだと、コンビニエンスストアで手数料なしで引き出しが可能になるため、より手軽になります。"},{"id":{"d":325,"n":8,"t":1},"speaker":"B","text":"どんどん便利になっていますね。"}];
INSERT INTO sentence [{"id":{"d":326,"n":1,"t":1},"speaker":"A","text":"ノンフライヤーを知っていますか？"},{"id":{"d":326,"n":2,"t":1},"speaker":"B","text":"言葉だけは知っています。少し前に流行しましたが、実際どのような機械なのかは分かりません。"},{"id":{"d":326,"n":3,"t":1},"speaker":"A","text":"油を使うことなく揚げ物を作れるという機械です。父が購入し、毎日使っているようです。"},{"id":{"d":326,"n":4,"t":1},"speaker":"B","text":"すごい仕組みですね。お父さんはどのような料理に使っているのですか？"},{"id":{"d":326,"n":5,"t":1},"speaker":"A","text":"父は、スーパーマーケットでシューストリングカットのフライドポテトを買ってきて食べているようです。"},{"id":{"d":326,"n":6,"t":1},"speaker":"B","text":"やはり油を使わず、ヘルシーに揚げ物を作ることができる機械ということですね。"},{"id":{"d":326,"n":7,"t":1},"speaker":"A","text":"それだけでなく、ピザパンも作っているようです。高温でキープすることができる点でさまざまな応用ができるようです。"},{"id":{"d":326,"n":8,"t":1},"speaker":"B","text":"揚げ物以外にも活用できるとはすごいですね。料理の幅が広がりそうです。"}];
INSERT INTO sentence [{"id":{"d":327,"n":1,"t":1},"speaker":"A","text":"オムライスには、何をかけますか？"},{"id":{"d":327,"n":2,"t":1},"speaker":"B","text":"私はケチャップをかけます。デミグラスソースやホワイトソースも気になりますが、簡単でおいしいケチャップで落ち着いています。"},{"id":{"d":327,"n":3,"t":1},"speaker":"A","text":"私もケチャップが一番好きなのですが、オムライス専門店に行った際カレーが上にのっていてとてもおいしかったことが忘れられません。"},{"id":{"d":327,"n":4,"t":1},"speaker":"B","text":"普通のカレーがかかっているのですか？"},{"id":{"d":327,"n":5,"t":1},"speaker":"A","text":"はい、普通のカレーがかかっていました。中のご飯は、ケチャップライスでなくカレーが混ざったご飯でした。"},{"id":{"d":327,"n":6,"t":1},"speaker":"B","text":"それはとてもおいしそうですね。家でも簡単に作ることができそうです。"},{"id":{"d":327,"n":7,"t":1},"speaker":"A","text":"ただ、家でカレーを作ったときは他のものに使おうと思っていても、そのままカレーライスとして食べてしまっていることが多々あります。"},{"id":{"d":327,"n":8,"t":1},"speaker":"B","text":"いつか家で作って、感想を聞くことができる日を楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":328,"n":1,"t":1},"speaker":"A","text":"「蜂蜜太郎」というお菓子を知っていますか？"},{"id":{"d":328,"n":2,"t":1},"speaker":"B","text":"初めて聞きました。どのようなお菓子ですか？"},{"id":{"d":328,"n":3,"t":1},"speaker":"A","text":"カリカリとして香ばしく、甘いお菓子です。カロリーは高いのですが、おいしいので食べてしまいます。"},{"id":{"d":328,"n":4,"t":1},"speaker":"B","text":"聞いているだけで、ずっと食べてしまいそうなお菓子ですね。何か他のお菓子に似ているということはないのですか？"},{"id":{"d":328,"n":5,"t":1},"speaker":"A","text":"かりんとうに少し似ているかもしれませんが、かりんとうよりも好みだとウェブ上のレビュー欄に口コミを書いている人が多数いました。"},{"id":{"d":328,"n":6,"t":1},"speaker":"B","text":"かりんとうよりもおいしいお菓子と聞いたらより食べたくなりました。どこで売っているのですか？"},{"id":{"d":328,"n":7,"t":1},"speaker":"A","text":"全てのスーパーマーケットではありませんが、スーパーマーケットの焼き菓子コーナーに売っていることがあります。また、ネット販売もしているようです。"},{"id":{"d":328,"n":8,"t":1},"speaker":"B","text":"スーパーマーケットに売っているのですね。今度買い物に出た時に探してみます。"}];
INSERT INTO sentence [{"id":{"d":329,"n":1,"t":1},"speaker":"A","text":"テレビ番組で、スナック菓子のうまい棒が値上げするというニュースが放送されていました。"},{"id":{"d":329,"n":2,"t":1},"speaker":"B","text":"10円ではなくなるのですか。子どもの頃からずっと10円だったので少し悲しいです。"},{"id":{"d":329,"n":3,"t":1},"speaker":"A","text":"私の家の近くのスーパーマーケットでは一時期、9円で売られていたこともありました。"},{"id":{"d":329,"n":4,"t":1},"speaker":"B","text":"値上げは、いくらになる予定か、知っていますか？"},{"id":{"d":329,"n":5,"t":1},"speaker":"A","text":"ニュースでは12円になると言っていました。ワンコインでなくなると子どもは、買うことを面倒がりそうですね。"},{"id":{"d":329,"n":6,"t":1},"speaker":"B","text":"そうですね。私が子どもの頃は、10円玉だけ握って出かけて、うまい棒を買ってから友達と遊びに行っていましたが、これからは12円を握って行かないといけないのですね。"}];
INSERT INTO sentence [{"id":{"d":330,"n":1,"t":1},"speaker":"A","text":"私の家の近くに、ジムが1つあります。"},{"id":{"d":330,"n":2,"t":1},"speaker":"B","text":"何という名前のジムですか？"},{"id":{"d":330,"n":3,"t":1},"speaker":"A","text":"市の健康づくりセンターに付いているジムです。"},{"id":{"d":330,"n":4,"t":1},"speaker":"B","text":"どこかの企業のジムではなく、市が運営しているジムということですね。"},{"id":{"d":330,"n":5,"t":1},"speaker":"A","text":"その通りです。そのおかげで市内の人ならば、とても安く使うことができます。"},{"id":{"d":330,"n":6,"t":1},"speaker":"B","text":"いくらくらいで、どのような施設を使うことができるのですか？"},{"id":{"d":330,"n":7,"t":1},"speaker":"A","text":"一般の方であれば3時間410円です。学生はさらに半額になります。施設は器具が完備されており、流水バスルーム、低温サウナ、マッサージチェアなどがあります。"},{"id":{"d":330,"n":8,"t":1},"speaker":"B","text":"そのような施設を使うことができて、その値段は安いですね。うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":331,"n":1,"t":1},"speaker":"A","text":"大雪のため、JR北海道の運休が続いています。"},{"id":{"d":331,"n":2,"t":1},"speaker":"B","text":"映像で見ましたが、大通がある札幌市中央区で人の背丈と同じくらい積雪していました。"},{"id":{"d":331,"n":3,"t":1},"speaker":"A","text":"札幌は、それほど積雪はしないと思っていましたが今年は異常なほど降っていますね。"},{"id":{"d":331,"n":4,"t":1},"speaker":"B","text":"5年半いましたが、一度も背丈と同じほど雪が降ったことはありませんでした。"},{"id":{"d":331,"n":5,"t":1},"speaker":"A","text":"それほどすごい降雪量だったのですね。ただJRは除雪車などを持っており、雪には慣れていそうですが電車はなぜ止まってしまったのですか？"},{"id":{"d":331,"n":6,"t":1},"speaker":"B","text":"切り替えポイントは人の手でないと除雪できないそうで、その部分にも積もってしまったため時間がかかっているそうです。"},{"id":{"d":331,"n":7,"t":1},"speaker":"A","text":"人力でやらないといけない部分に時間がかかっているのは、仕方ないことですね。列車を使い、仕事や学校へ行く予定の人もいるはずなので早く復旧してほしいと思います。"},{"id":{"d":331,"n":8,"t":1},"speaker":"B","text":"天候のことは、誰にも何もできない仕方ないことなので、運休からの復旧時間を短くすることが重要ですね。"}];
INSERT INTO sentence [{"id":{"d":332,"n":1,"t":1},"speaker":"A","text":"ギョウザはお好きですか？"},{"id":{"d":332,"n":2,"t":1},"speaker":"B","text":"大好きです。家族でたくさんのギョウザを包んで食べています。"},{"id":{"d":332,"n":3,"t":1},"speaker":"A","text":"ギョウザを包む時間で家族だんらんの時間を作ることができることはいいですね。ところでギョウザといえばどの地域をイメージしますか？"},{"id":{"d":332,"n":4,"t":1},"speaker":"B","text":"ギョウザの購入頻度の比較でも毎年競っていることから、やはり宇都宮と浜松をイメージします。"},{"id":{"d":332,"n":5,"t":1},"speaker":"A","text":"多くの人がその地域を挙げると思います。しかし今年の1位は異なったようです。"},{"id":{"d":332,"n":6,"t":1},"speaker":"B","text":"他の地域でギョウザの売上が多いところは考えつきません。一体どこですか？"},{"id":{"d":332,"n":7,"t":1},"speaker":"A","text":"2021年は宮崎が購入頻度と支出金額で1位だったそうです。宮崎では冠婚葬祭の際にもギョウザを用いる地域があるほどギョウザ文化が根強いようです。"},{"id":{"d":332,"n":8,"t":1},"speaker":"B","text":"なるほど、宮崎ですか。冠婚葬祭でギョウザが用いられる地域があるとは驚きです。"}];
INSERT INTO sentence [{"id":{"d":333,"n":1,"t":1},"speaker":"A","text":"実家の近くに、ホテルが建つ予定です。"},{"id":{"d":333,"n":2,"t":1},"speaker":"B","text":"建つ予定ということはまだ建っていないということですか？"},{"id":{"d":333,"n":3,"t":1},"speaker":"A","text":"多分、東京東京2020オリンピック・パラリンピックに合わせて建設し、海外の観客のための宿として提供する予定のものだったのでしょう。"},{"id":{"d":333,"n":4,"t":1},"speaker":"B","text":"では建設途中ということですか？"},{"id":{"d":333,"n":5,"t":1},"speaker":"A","text":"外側は建設されていますが、少し中を覗くとエレベーターが設置されていないことや配線が全てむき出しになっていることから途中で止めたことが分かります。"},{"id":{"d":333,"n":6,"t":1},"speaker":"B","text":"東京2020オリンピック・パラリンピックが延期することになるとは、誰も思わないことなので仕方ありませんね。"},{"id":{"d":333,"n":7,"t":1},"speaker":"A","text":"はい。建設工事の再開と、ホテルの完成が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":334,"n":1,"t":1},"speaker":"A","text":"北海道限定のビールである「サッポロ クラシック」を知っていますか？"},{"id":{"d":334,"n":2,"t":1},"speaker":"B","text":"知りませんでした。札幌では有名なビールですか？"},{"id":{"d":334,"n":3,"t":1},"speaker":"A","text":"基本的に、札幌の居酒屋でビールを注文すると「サッポロ クラシック」が出てきます。それくらい有名です。"},{"id":{"d":334,"n":4,"t":1},"speaker":"B","text":"本州だと「スーパードライ」や「ザ・プレミアム・モルツ」が出てきますが、札幌では「サッポロ クラシック」が出てくるのですね。味はどうですか？"},{"id":{"d":334,"n":5,"t":1},"speaker":"A","text":"とてもおいしいですよ。苦味が少ないのでビールが苦手な人でも飲めるビールだと思います。"},{"id":{"d":334,"n":6,"t":1},"speaker":"B","text":"それはとても惹かれますね。喉越しが良いのですか？それとも味自体良いのですか？"},{"id":{"d":334,"n":7,"t":1},"speaker":"A","text":"私は味がとてもおいしいと思います。ビールは喉越しといいますが、私は喉越しはもちろんのことで味がおいしいのが「サッポロ クラシック」だと考えています。"},{"id":{"d":334,"n":8,"t":1},"speaker":"B","text":"とても飲みたくなったので取り寄せてみます。"}];
INSERT INTO sentence [{"id":{"d":335,"n":1,"t":1},"speaker":"A","text":"どのような系統の洋服が好きですか？"},{"id":{"d":335,"n":2,"t":1},"speaker":"B","text":"私はシャツを着るのが好きです。もちろんストリート系の洋服も大好きです。"},{"id":{"d":335,"n":3,"t":1},"speaker":"A","text":"ストリート系が好きなのはとても良いですね。動きやすいですし、実は着こなすのが難しいですよね？"},{"id":{"d":335,"n":4,"t":1},"speaker":"B","text":"そうなのです。見た目が怖くならないように気を付けながら洋服を選んでいます。"},{"id":{"d":335,"n":5,"t":1},"speaker":"A","text":"シャツ系はどのようなものが好きですか？"},{"id":{"d":335,"n":6,"t":1},"speaker":"B","text":"最近は黒のシャツが好みです。ジーパンや白のパンツに合わせて着ることが多いです。"},{"id":{"d":335,"n":7,"t":1},"speaker":"A","text":"今日も素敵な黒いシャツをお召しになられていますね。よくお似合いです。"},{"id":{"d":335,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。うれしいです。"}];
INSERT INTO sentence [{"id":{"d":336,"n":1,"t":1},"speaker":"A","text":"時間を楽しむ飲み物を知っていますか？"},{"id":{"d":336,"n":2,"t":1},"speaker":"B","text":"知りません。バーなど空間を楽しみながら、時間を楽しむことは知っていますが、時間を楽しむ飲み物は知りません。"},{"id":{"d":336,"n":3,"t":1},"speaker":"A","text":"この前テレビで特集されていましたが、「ICHIJI」や「NIJI」という名前のお酒があるようです。"},{"id":{"d":336,"n":4,"t":1},"speaker":"B","text":"確かに時間の名前のお酒ですが、時間を楽しむとはどういうことですか？"},{"id":{"d":336,"n":5,"t":1},"speaker":"A","text":"このご時世、あまり人と集まって楽しくお酒を飲むことはできません。そこで今は7時だから一緒に「SHICHIJI」のお酒を飲もうとリモートで飲み会をするようです。"},{"id":{"d":336,"n":6,"t":1},"speaker":"B","text":"それで、時間を楽しむということなのですね。ただ、居酒屋でお酒を飲んでいる時も同じお酒を頼むことはほとんどありませんね。"},{"id":{"d":336,"n":7,"t":1},"speaker":"A","text":"私もそう思いましたが、発想としては面白いと思いました。また、毎日ある時間限定で販売しているお酒もあるようで、そのお酒も「時間を楽しむお酒」としてテレビで特集されていました。"},{"id":{"d":336,"n":8,"t":1},"speaker":"B","text":"時間で限定し、販売するのは、確かに時間を楽しむお酒ですね。"}];
INSERT INTO sentence [{"id":{"d":337,"n":1,"t":1},"speaker":"A","text":"「行きつけの喫茶店を一軒持っていると良い」と親に言われました。"},{"id":{"d":337,"n":2,"t":1},"speaker":"B","text":"喫茶店の行きつけはとてもおしゃれですね。行きつけの喫茶店はできましたか？"},{"id":{"d":337,"n":3,"t":1},"speaker":"A","text":"大学生活では喫茶店に行くだけの時間に起床することができていなかったので、見つけることができませんでした。「行きつけの喫茶店を」というアドバイスは、朝にしっかり起きるようになるという理由もあったのかと今になって思います。"},{"id":{"d":337,"n":4,"t":1},"speaker":"B","text":"なぜ、行きつけを見つけるべきかという理由は言われてないのですか？"},{"id":{"d":337,"n":5,"t":1},"speaker":"A","text":"はい、理由は特に言われていませんでした。"},{"id":{"d":337,"n":6,"t":1},"speaker":"B","text":"その理由も自分で見つけることが勉強だったのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":338,"n":1,"t":1},"speaker":"A","text":"普段は録画したものばかり見ているのでCMは見ませんが、東京2020オリンピック・パラリンピックの期間はライブの動画を見るのでCMをよく見ます。"},{"id":{"d":338,"n":2,"t":1},"speaker":"B","text":"分かります。久しぶりに見ると面白いCMがたくさんあって、たまには良いと思いますよね。"},{"id":{"d":338,"n":3,"t":1},"speaker":"A","text":"はい。最近は、CMソングもとても良いものが多く、CMを見終わってから調べてしまうこともあります。"},{"id":{"d":338,"n":4,"t":1},"speaker":"B","text":"ちなみにそのCMの内容は覚えていますか？"},{"id":{"d":338,"n":5,"t":1},"speaker":"A","text":"音楽が良いものは、内容を全く覚えていません。音楽の印象が強くなってしまいます。"},{"id":{"d":338,"n":6,"t":1},"speaker":"B","text":"私もその傾向があります。今まで見たCMの中で一番好きなものは何ですか？"},{"id":{"d":338,"n":7,"t":1},"speaker":"A","text":"私は7年ほど前のバランス栄養食「カロリーメイト」のCMが一番好きです。浪人生で受験を控えており、大みそかに予備校から帰宅して久しぶりにテレビを見た時に、満島ひかりさんが歌う、中島みゆきさんの「ファイト！」が流れてきたのですが、その歌詞が当時の自分の状況によく当てはまっていて涙があふれてしまったことを覚えています。"}];
INSERT INTO sentence [{"id":{"d":339,"n":1,"t":1},"speaker":"A","text":"スキニーパンツの流行は、もう終わったそうですね。今朝のテレビ番組で言っていました。"},{"id":{"d":339,"n":2,"t":1},"speaker":"B","text":"数年前まではスキニーパンツがとても人気だったのに、流行は早く変化するものですね。"},{"id":{"d":339,"n":3,"t":1},"speaker":"A","text":"流行とはそのようなものですね。現在は、ワイドパンツが流行しているようです。"},{"id":{"d":339,"n":4,"t":1},"speaker":"B","text":"今、流行のワイドパンツも、数年後には流行が終わってしまうのでしょうね。"},{"id":{"d":339,"n":5,"t":1},"speaker":"A","text":"きっとそうでしょうね。次は、何が流行するのかワクワクします。"},{"id":{"d":339,"n":6,"t":1},"speaker":"B","text":"そうですね。流行は、これまで自分では選ばなかった服に挑戦してみるきっかけにもなりますね。"}];
INSERT INTO sentence [{"id":{"d":340,"n":1,"t":1},"speaker":"A","text":"関東風と関西風でさまざまな料理が異なることがご存じですか？"},{"id":{"d":340,"n":2,"t":1},"speaker":"B","text":"知っています。たとえば、おでんやうどんなどはだしが異なりますね。"},{"id":{"d":340,"n":3,"t":1},"speaker":"A","text":"その通りです。関東と関西で異なる料理の中でも、当店で提供しているのは関西風のすき焼きです。"},{"id":{"d":340,"n":4,"t":1},"speaker":"B","text":"関西風と関東風のすき焼きではどの点が異なるのですか？"},{"id":{"d":340,"n":5,"t":1},"speaker":"A","text":"割り下を使わないものが関西風のすき焼きです。つまり煮るものが関東風、焼いて作るものが関西風です。"},{"id":{"d":340,"n":6,"t":1},"speaker":"B","text":"つまり焼いて作るすき焼きを今日は食べることができるのですね。初めて食べるので楽しみです。"},{"id":{"d":340,"n":7,"t":1},"speaker":"A","text":"楽しみにしていただいていたようで、うれしいです。それでは提供を始めます。"},{"id":{"d":340,"n":8,"t":1},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":341,"n":1,"t":1},"speaker":"A","text":"古本屋へ行ったことはありますか？"},{"id":{"d":341,"n":2,"t":1},"speaker":"B","text":"浪人生の時に御茶ノ水の予備校に通っていたので、神保町の古本屋街へ行ったことは何度もあります。"},{"id":{"d":341,"n":3,"t":1},"speaker":"A","text":"古本屋にはさまざまな掘り出し物があり、年季の入った本の匂いがとても良い場所ですね。"},{"id":{"d":341,"n":4,"t":1},"speaker":"B","text":"教授もよく行かれるのですか？"},{"id":{"d":341,"n":5,"t":1},"speaker":"A","text":"たまに、少しでも参考になりそうな書籍がないか確認しに行きます。"},{"id":{"d":341,"n":6,"t":1},"speaker":"B","text":"どれくらいの頻度で、参考になる書籍を発見することができるのですか？"},{"id":{"d":341,"n":7,"t":1},"speaker":"A","text":"ほとんど発見することはできません。しかし、発見することができた書籍はとても研究に参考になるものばかりです。"},{"id":{"d":341,"n":8,"t":1},"speaker":"B","text":"新しい古本屋を発見することは、教授にとって研究の1つということですね。"}];
INSERT INTO sentence [{"id":{"d":342,"n":1,"t":1},"speaker":"A","text":"救急車に乗ったことはありますか？"},{"id":{"d":342,"n":2,"t":1},"speaker":"B","text":"一度もありません。ありがたいことに健康で、大きなけがもしたことがないので、無縁の生活を送っています。乗ったことがあるのですか？"},{"id":{"d":342,"n":3,"t":1},"speaker":"A","text":"一度だけあります。幼稚園生くらいの時に食べたものをもどしたところ、少し血を吐いたことがあったようです。"},{"id":{"d":342,"n":4,"t":1},"speaker":"B","text":"それは大変ですね。大丈夫でしたか？"},{"id":{"d":342,"n":5,"t":1},"speaker":"A","text":"少し血も混ざっていましたが、ほとんどはトマトだったようで全く問題はありませんでした。"},{"id":{"d":342,"n":6,"t":1},"speaker":"B","text":"食事の際に、トマトを食べてそれが血に見えたということですね。先生もびっくりしたことでしょう。"},{"id":{"d":342,"n":7,"t":1},"speaker":"A","text":"医師から「これはトマトですね」と笑って言われたと母が言っていました。私は小さすぎてあまり記憶にはありませんが、救急車に乗ったのはこの1回だけです。"},{"id":{"d":342,"n":8,"t":1},"speaker":"B","text":"唯一乗った経験が小さくて記憶がないのは少しもったいないですね。しかし、健康に生活できているのは何より幸せなことですね。"}];
INSERT INTO sentence [{"id":{"d":343,"n":1,"t":1},"speaker":"A","text":"どのような物件をお探しですか？"},{"id":{"d":343,"n":2,"t":1},"speaker":"B","text":"仕事に行かないといけない日もあるので、職場に近いことがまず条件として欠かせません。そのため職場まで20分以内に着けるような家がいいと考えています。"},{"id":{"d":343,"n":3,"t":1},"speaker":"A","text":"分かりました。他の条件はございますか？"},{"id":{"d":343,"n":4,"t":1},"speaker":"B","text":"職場に行く日ももちろんありますが、ほとんどはリモートワークなので仕事用の部屋が1つある家がいいです。"},{"id":{"d":343,"n":5,"t":1},"speaker":"A","text":"小さい部屋でも大丈夫ならこのような物件があります。しかし、スーパーマーケットなどへは少し距離があり、駅までも少し遠い物件です。"},{"id":{"d":343,"n":6,"t":1},"speaker":"B","text":"駅やスーパーマーケットまで遠いのは、生活がしづらいのでできれば避けたいです。"},{"id":{"d":343,"n":7,"t":1},"speaker":"A","text":"分かりました。それでは少し広めの部屋で家賃も高くなりますが、こちらはいかがですか？"},{"id":{"d":343,"n":8,"t":1},"speaker":"B","text":"会社からの補助が出る価格を超えてしまうので結構です。先ほどは仕事スペースが欲しいと申し上げましたが、「ワークブース」という家の中でも個室を作ることができるキットを見つけたので、仕事用の部屋がなくても大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":344,"n":1,"t":1},"speaker":"A","text":"先週、あるテレビでおつまみを紹介していました。"},{"id":{"d":344,"n":2,"t":1},"speaker":"B","text":"うちの店にあるようなおつまみはありましたか？"},{"id":{"d":344,"n":3,"t":1},"speaker":"A","text":"ありませんでした。家で簡単に作ることができるようなものが多かったので、店にあるようなものではありませんでした。"},{"id":{"d":344,"n":4,"t":1},"speaker":"B","text":"どのようなものがあったか教えてください。せっかくなので今から少し作ってみようと思います。"},{"id":{"d":344,"n":5,"t":1},"speaker":"A","text":"出演者が一番おいしいと舌鼓を打っていたのは塩辛バターキャベツでした。"},{"id":{"d":344,"n":6,"t":1},"speaker":"B","text":"当店で提供しているアンチョビキャベツのようなものですか？"},{"id":{"d":344,"n":7,"t":1},"speaker":"A","text":"似ていると思います。キャベツに料理酒をかけてレンジで加熱し、バターと塩辛をのせて加熱していました。"},{"id":{"d":344,"n":8,"t":1},"speaker":"B","text":"分かりました。今から準備をするので少し待っていてください。感想をお待ちしていますね。"}];
INSERT INTO sentence [{"id":{"d":345,"n":1,"t":1},"speaker":"A","text":"赤ちゃんにプレゼントを買っていこうと思っているのですが、何かいいものはありませんか？"},{"id":{"d":345,"n":2,"t":1},"speaker":"B","text":"うちの子どもは何にでも興味を示すので、どのようなおもちゃでもうれしいです。"},{"id":{"d":345,"n":3,"t":1},"speaker":"A","text":"それでは、積み木を持っていきますね。"},{"id":{"d":345,"n":4,"t":1},"speaker":"B","text":"積み木は口に入れてしまう可能性があるので、少し控えていただきたいです。すみません。"},{"id":{"d":345,"n":5,"t":1},"speaker":"A","text":"私が持って行こうとしているのはお米でできた積み木です。口に入れても、お米なので大丈夫ですよ。"},{"id":{"d":345,"n":6,"t":1},"speaker":"B","text":"お米の積み木なんてあるのですね。それなら安心です。よろしくお願いします。"},{"id":{"d":345,"n":7,"t":1},"speaker":"A","text":"それでは来月の第1週にでも持って行きますね。他に何か欲しいものはありますか？"},{"id":{"d":345,"n":8,"t":1},"speaker":"B","text":"いえ、十分です。お気遣いありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":346,"n":1,"t":1},"speaker":"A","text":"すみません、一番近くの薬局を教えていただいてもよいですか？"},{"id":{"d":346,"n":2,"t":1},"speaker":"B","text":"そこの道を突き当たって左です。"},{"id":{"d":346,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。左に曲がればすぐに分かりますか？"},{"id":{"d":346,"n":4,"t":1},"speaker":"B","text":"分かると思います。ちなみに、どこの病院で診てもらいましたか？"},{"id":{"d":346,"n":5,"t":1},"speaker":"A","text":"すぐ近くの総合病院ですが、何か関係があるのですか？"},{"id":{"d":346,"n":6,"t":1},"speaker":"B","text":"総合病院で診察してもらったならば、薬は左に曲がって3軒目の薬局が良いですよ。総合病院と提携しているのですぐに薬を処方してくれます。時間の無駄が少ないという利点があります。"},{"id":{"d":346,"n":7,"t":1},"speaker":"A","text":"ご親切に、どうもありがとうございます。では、その薬局へ行こうと思います。"}];
INSERT INTO sentence [{"id":{"d":347,"n":1,"t":1},"speaker":"A","text":"今日の晩ご飯は出前にしましょう。"},{"id":{"d":347,"n":2,"t":1},"speaker":"B","text":"久しぶりに出前を頼むので楽しみです。どこの出前を取りますか？"},{"id":{"d":347,"n":3,"t":1},"speaker":"A","text":"最近、家の近くにピザ屋ができたので、そこにしませんか？私の記憶では、配達をお願いするともう1枚無料で注文することができたはずです。"},{"id":{"d":347,"n":4,"t":1},"speaker":"B","text":"それは、とても良いサービスですね。その店で頼むことにします。早速メニューを決めましょう。"},{"id":{"d":347,"n":5,"t":1},"speaker":"A","text":"私は耳が好きなので、耳のあるピザで、普段外食では食べないような具がのっているものが望ましいです。"},{"id":{"d":347,"n":6,"t":1},"speaker":"B","text":"珍しい具としてプルコギのピザがありました。これにしませんか？写真を見る限りとてもおいしそうです。"}];
INSERT INTO sentence [{"id":{"d":348,"n":1,"t":1},"speaker":"A","text":"革靴の手入れをしっかりしていますか？"},{"id":{"d":348,"n":2,"t":1},"speaker":"B","text":"あまりしていません。やり方も分からず、安い革靴を購入しているので悪くなったら替えようと考えています。"},{"id":{"d":348,"n":3,"t":1},"speaker":"A","text":"安い靴でもしっかりと手入れをすることで、物持ちが良くなりますよ。よろしければ、靴の手入れ方法をお教えしますか？"},{"id":{"d":348,"n":4,"t":1},"speaker":"B","text":"とても教えていただきたいです。誰から教えてもらったのですか？"},{"id":{"d":348,"n":5,"t":1},"speaker":"A","text":"私は父から教わりました。「社会人は靴を見られる」と言われました。"},{"id":{"d":348,"n":6,"t":1},"speaker":"B","text":"それでは、くたびれた靴を履いていたら好印象を持たれないということですか？"},{"id":{"d":348,"n":7,"t":1},"speaker":"A","text":"靴が全てではありませんが、靴で評価が上がる可能性があるそうです。"},{"id":{"d":348,"n":8,"t":1},"speaker":"B","text":"ぜひ、手入れの仕方を教えてください。今日からしっかり手入れをします。"}];
INSERT INTO sentence [{"id":{"d":349,"n":1,"t":1},"speaker":"A","text":"庭の雑草が、伸びてきていますね。"},{"id":{"d":349,"n":2,"t":1},"speaker":"B","text":"そうですね。家計との相談ですが、芝刈りなどをしなくても問題ないようにするために、人工芝にするのはどうですか？"},{"id":{"d":349,"n":3,"t":1},"speaker":"A","text":"家計の面では問題ありませんが、人工芝にする方法を知りません。"},{"id":{"d":349,"n":4,"t":1},"speaker":"B","text":"恐らく芝生を全部剥がして、人工芝を敷けばよいのだろうと思っています。"},{"id":{"d":349,"n":5,"t":1},"speaker":"A","text":"人工芝はどこで購入するのですか？購入のために値上がりしているガソリンを使って、車を走らせ遠くまで行くのは気が引けます。"},{"id":{"d":349,"n":6,"t":1},"speaker":"B","text":"近くのホームセンターで売っていることは、既に調べてあるのでそこで購入しましょう。"},{"id":{"d":349,"n":7,"t":1},"speaker":"A","text":"既に計画していたことだったのですね。それではまずは芝生を剥がすことから始めましょう。"},{"id":{"d":349,"n":8,"t":1},"speaker":"B","text":"できるだけ根を残さないように気を付けましょうね。"}];
INSERT INTO sentence [{"id":{"d":350,"n":1,"t":1},"speaker":"A","text":"今日は久しぶりに、バーベキューをしませんか？"},{"id":{"d":350,"n":2,"t":1},"speaker":"B","text":"いいですね。最近全くしていなかったのでとても楽しみです。メインは何にしますか？"},{"id":{"d":350,"n":3,"t":1},"speaker":"A","text":"昨日、お父さんがお土産として買ってきてくれた、ジンギスカンを食べようかなと思っています。"},{"id":{"d":350,"n":4,"t":1},"speaker":"B","text":"北海道のお土産として、買ってきてくれたものですね。とてもおいしそうなお肉だったので楽しみです。"},{"id":{"d":350,"n":5,"t":1},"speaker":"A","text":"ジンギスカンといえば、お肉はもちろんですが、野菜も欠かせません。今から野菜を買いに行きましょう。"},{"id":{"d":350,"n":6,"t":1},"speaker":"B","text":"野菜は、何を買うのですか？私の印象としてはキャベツやタマネギかなと思うのですが。"},{"id":{"d":350,"n":7,"t":1},"speaker":"A","text":"基本的に、何があってもおいしいです。私は特に、キャベツ、タマネギ、ピーマン、もやし、ニンジンは欠かせません。"},{"id":{"d":350,"n":8,"t":1},"speaker":"B","text":"たくさん野菜を食べることができそうで、健康にも良さそうですね。早速、買い物に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":351,"n":1,"t":1},"speaker":"A","text":"今日の料理はグラタンにしようと思うのですが、ホワイトソースを作るのが少し面倒くさいです。"},{"id":{"d":351,"n":2,"t":1},"speaker":"B","text":"市販されているホワイトソースの素を使えば、簡単に作ることができますよ。"},{"id":{"d":351,"n":3,"t":1},"speaker":"A","text":"それは分かっていますが、ベシャメルソースを毎回作っているので、市販のものを使うのは今までを否定してしまう気がして使うことができません。"},{"id":{"d":351,"n":4,"t":1},"speaker":"B","text":"毎回作るのは、とても大変です。今日は私が作ります。"},{"id":{"d":351,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。材料は作業台の上に置いてあるものを使ってください。"},{"id":{"d":351,"n":6,"t":1},"speaker":"B","text":"分かりました。ただ調べていたら、「餅で作るホワイトソース」という内容のウェブページを見つけたので今日はこのレシピを試して良いですか？"},{"id":{"d":351,"n":7,"t":1},"speaker":"A","text":"ぜひ試してみてください。小麦粉を使ったレシピとどのような違いがあるのかとても興味があります。"},{"id":{"d":351,"n":8,"t":1},"speaker":"B","text":"分かりました。それでは、まずお餅を切るところから始めますね。"}];
INSERT INTO sentence [{"id":{"d":352,"n":1,"t":1},"speaker":"A","text":"お客さま、お待たせしました。こちらの窓口でお伺いします。"},{"id":{"d":352,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"},{"id":{"d":352,"n":3,"t":1},"speaker":"A","text":"本日はどのようなご用件でしょうか？"},{"id":{"d":352,"n":4,"t":1},"speaker":"B","text":"口座を開設したいです。ネット上の説明だと分からなかったので、直接お伺いしに来ました。"},{"id":{"d":352,"n":5,"t":1},"speaker":"A","text":"かしこまりました。普通預金でよろしいですか？"},{"id":{"d":352,"n":6,"t":1},"speaker":"B","text":"普通でお願いします。開設にお金はかかりますか？"},{"id":{"d":352,"n":7,"t":1},"speaker":"A","text":"全くかかりませんよ。それでは、運転免許証などの身分証明書のご提示をお願いします。"},{"id":{"d":352,"n":8,"t":1},"speaker":"B","text":"はい、免許証を提出します。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":353,"n":1,"t":1},"speaker":"A","text":"晩ご飯の時に、お茶を飲もうと思っているのでお湯を沸かしておいてください。"},{"id":{"d":353,"n":2,"t":1},"speaker":"B","text":"もう沸かしてあります。少し時間がたっていますが、まだ熱いと思います。"},{"id":{"d":353,"n":3,"t":1},"speaker":"A","text":"仕事が早いですね。ありがとうございます。"},{"id":{"d":353,"n":4,"t":1},"speaker":"B","text":"今日のお茶は何にしますか？ほうじ茶でよいですか？"},{"id":{"d":353,"n":5,"t":1},"speaker":"A","text":"ほうじ茶をお願いします。晩ご飯は後10分ほどで食べられると思います。"},{"id":{"d":353,"n":6,"t":1},"speaker":"B","text":"それでは、5分後にもう一度お湯を沸かしますね。"},{"id":{"d":353,"n":7,"t":1},"speaker":"A","text":"沸かさなくても大丈夫だと思いますよ。そのままで平気です。"},{"id":{"d":353,"n":8,"t":1},"speaker":"B","text":"今は冬なので、お湯の温度が下がるのが早いでしょう。やはり沸かしておきます。"}];
INSERT INTO sentence [{"id":{"d":354,"n":1,"t":1},"speaker":"A","text":"家の中で履いているスリッパが、だんだん傷んできました。"},{"id":{"d":354,"n":2,"t":1},"speaker":"B","text":"実は、私のスリッパも傷んできているので新しく買いに行きますか？"},{"id":{"d":354,"n":3,"t":1},"speaker":"A","text":"このスリッパは、洗濯はできないのですか？私のスリッパは洗濯したら少し戻りそうな予感がしています。"},{"id":{"d":354,"n":4,"t":1},"speaker":"B","text":"きっと大丈夫だと思います。洗濯してみましょう。"},{"id":{"d":354,"n":5,"t":1},"speaker":"A","text":"もし洗濯して、も靴底がペラペラのままだった時に新しく買いましょう。"},{"id":{"d":354,"n":6,"t":1},"speaker":"B","text":"ちなみにこのスリッパはどこで買ったか覚えていますか？"},{"id":{"d":354,"n":7,"t":1},"speaker":"A","text":"私が帰省した時に家にあった物なので、どこで買ったのか知りません。"},{"id":{"d":354,"n":8,"t":1},"speaker":"B","text":"どこで買ったのかあまり覚えていないので、もしかしたら同じ物を購入してくることはできないかもしれませんが、できるだけ同じものを探してきますね。"},{"id":{"d":354,"n":9,"t":1},"speaker":"A","text":"これは温かくて気に入っているので、ぜひよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":355,"n":1,"t":1},"speaker":"A","text":"卵焼きを作ったことはありますか？"},{"id":{"d":355,"n":2,"t":1},"speaker":"B","text":"もちろんあります。お弁当を作るときに入れるために作ったこともありますし、アルバイト先の居酒屋でも作ったことがあります。"},{"id":{"d":355,"n":3,"t":1},"speaker":"A","text":"アルバイトではどのような卵焼きを作っていましたか？"},{"id":{"d":355,"n":4,"t":1},"speaker":"B","text":"明太チーズ卵焼きを作っていました。とてもおいしいメニューでその居酒屋の人気メニューでした。"},{"id":{"d":355,"n":5,"t":1},"speaker":"A","text":"想像しただけでおいしそうです。居酒屋の卵焼きはとてもおいしいので大好きです。"},{"id":{"d":355,"n":6,"t":1},"speaker":"B","text":"私は大根おろしと一緒に食べる卵焼きが大好きです。話していたら卵焼きが食べたくなってきました。今日の夕飯用に今から作ろうと思います。"}];
INSERT INTO sentence [{"id":{"d":356,"n":1,"t":1},"speaker":"A","text":"スマートスピーカーを知っていますか？"},{"id":{"d":356,"n":2,"t":1},"speaker":"B","text":"どのようなものですか？スマートフォンとは異なりますか？"},{"id":{"d":356,"n":3,"t":1},"speaker":"A","text":"はい、少し違います。スマートスピーカーはAmazonのAlexaのようなものです。"},{"id":{"d":356,"n":4,"t":1},"speaker":"B","text":"最近、CMでAlexaに呼びかけているものを見ましたがその事なのですね。どのようなことができるのですか？"},{"id":{"d":356,"n":5,"t":1},"speaker":"A","text":"登録してあるAlexa同士でビデオ通話をすることができます。"},{"id":{"d":356,"n":6,"t":1},"speaker":"B","text":"このご時世にぴったりの機能ですね。祖父母と簡単に連絡を取ることができそうです。"},{"id":{"d":356,"n":7,"t":1},"speaker":"A","text":"他にもインターネットに接続した家電のオンオフが可能です。"},{"id":{"d":356,"n":8,"t":1},"speaker":"B","text":"とても最先端な家になりますね。設定は簡単ですか？"},{"id":{"d":356,"n":9,"t":1},"speaker":"A","text":"登録をしてしまえば良いので、簡単に設定することができます。"}];
INSERT INTO sentence [{"id":{"d":357,"n":1,"t":1},"speaker":"A","text":"小学生の時、雨の日は何を履いていましたか？"},{"id":{"d":357,"n":2,"t":1},"speaker":"B","text":"常に長靴を履いていました。あなたは何を履いていましたか？"},{"id":{"d":357,"n":3,"t":1},"speaker":"A","text":"私は低学年ときは長靴を履いていましたが、3年生以降は普段の靴を履いていました。"},{"id":{"d":357,"n":4,"t":1},"speaker":"B","text":"それではぬれてしまうのではありませんか？"},{"id":{"d":357,"n":5,"t":1},"speaker":"A","text":"靴を3足持っていたので、1足雨にぬれても問題なかったのです。そのため、歩きやすい普段靴を履いていました。"},{"id":{"d":357,"n":6,"t":1},"speaker":"B","text":"私も何足か持っていましたが、靴がぬれて靴下がびしょびしょになるところが気持ち悪かったので、長靴を履いていました。"},{"id":{"d":357,"n":7,"t":1},"speaker":"A","text":"私も靴下がぬれることは嫌でしたが、学校に着いたら靴下を脱いで棚に置いて乾かしていました。"},{"id":{"d":357,"n":8,"t":1},"speaker":"B","text":"乾かすことができるのは良いですね。そのようなことができるなら、私も運動靴を履いて登校していたと思います。"}];
INSERT INTO sentence [{"id":{"d":358,"n":1,"t":1},"speaker":"A","text":"お風呂に入ってゆっくりしていたら、のぼせてしまいました。"},{"id":{"d":358,"n":2,"t":1},"speaker":"B","text":"お風呂から何も物音がしないので、寝てしまったのかと思いました。"},{"id":{"d":358,"n":3,"t":1},"speaker":"A","text":"寝ていたわけではなく、スマートフォンで動画を見ていたら時間がたっていました。"},{"id":{"d":358,"n":4,"t":1},"speaker":"B","text":"動画に集中しすぎると時間の感覚がなくなるので気を付けてください。"},{"id":{"d":358,"n":5,"t":1},"speaker":"A","text":"一人の空間になると動画を見たくなってしまうことが多く、トイレでもよく動画を見てしまいます。"},{"id":{"d":358,"n":6,"t":1},"speaker":"B","text":"トイレでは、のぼせるなどの心配はありませんが、他の人の利用の邪魔になるようなことはしてはいけませんよ。"},{"id":{"d":358,"n":7,"t":1},"speaker":"A","text":"邪魔にならないように家族があまり使わない2階のトイレに行った時に限って、動画を見るようにしています。"},{"id":{"d":358,"n":8,"t":1},"speaker":"B","text":"トイレも冬は寒いと思いますので、風邪をひかないように気を付けてくださいね。"}];
INSERT INTO sentence [{"id":{"d":359,"n":1,"t":1},"speaker":"A","text":"もしリフォームできるとしたら、どのようなキッチンに憧れますか？"},{"id":{"d":359,"n":2,"t":1},"speaker":"B","text":"私は、対面キッチンにしたいと考えています。"},{"id":{"d":359,"n":3,"t":1},"speaker":"A","text":"それはなぜですか？今のキッチンとは全く異なりますね。"},{"id":{"d":359,"n":4,"t":1},"speaker":"B","text":"子どものことを、しっかり確認しながら料理をすることができると思うからです。"},{"id":{"d":359,"n":5,"t":1},"speaker":"A","text":"今のキッチンのように1つの部屋になっているものではあまり見ることができませんか？"},{"id":{"d":359,"n":6,"t":1},"speaker":"B","text":"見ることはできますが、わざわざ扉を開けないと見ることができないため、少し不便でした。"},{"id":{"d":359,"n":7,"t":1},"speaker":"A","text":"しかし、キッチンに子どもが入ってこないことは、安全の面で良かったのではありませんか？"},{"id":{"d":359,"n":8,"t":1},"speaker":"B","text":"そうですね。入ってはいけない部屋として扱うことができたので、その点では良い作りだと思います。"}];
INSERT INTO sentence [{"id":{"d":360,"n":1,"t":1},"speaker":"A","text":"スカジャンを知っていますか？"},{"id":{"d":360,"n":2,"t":1},"speaker":"B","text":"もちろん知っています。学生の頃によく着ていました。"},{"id":{"d":360,"n":3,"t":1},"speaker":"A","text":"ちなみに、今もスカジャンは持っていますか？"},{"id":{"d":360,"n":4,"t":1},"speaker":"B","text":"青の龍虎のスカジャンを持っています。冬は寒いので今も愛用しています。"},{"id":{"d":360,"n":5,"t":1},"speaker":"A","text":"その色と刺しゅうは私が一番好きなものです。とてもうらやましいです。"},{"id":{"d":360,"n":6,"t":1},"speaker":"B","text":"高かったですが、自分への内定祝いとして購入しました。"},{"id":{"d":360,"n":7,"t":1},"speaker":"A","text":"スカジャンは高いですよね。ちなみにいくらしましたか？"},{"id":{"d":360,"n":8,"t":1},"speaker":"B","text":"2万円ちょっとしました。ただ、高校生の時からずっと欲しかったものなので、買うことができてとてもうれしかったです。"}];
INSERT INTO sentence [{"id":{"d":361,"n":1,"t":1},"speaker":"A","text":"今日のお弁当は、とてもおいしかったです。"},{"id":{"d":361,"n":2,"t":1},"speaker":"B","text":"それは良かったです。どの料理が一番おいしかったですか？"},{"id":{"d":361,"n":3,"t":1},"speaker":"A","text":"シューマイが一番おいしかったです。卵焼きもチーズの味がしておいしかったです。"},{"id":{"d":361,"n":4,"t":1},"speaker":"B","text":"シューマイは冷凍食品のものですね。私もあのシューマイはおいしいと思います。"},{"id":{"d":361,"n":5,"t":1},"speaker":"A","text":"あれは冷凍食品だったのですね。最近の冷凍食品はすごいですね。"},{"id":{"d":361,"n":6,"t":1},"speaker":"B","text":"卵焼きはパルメザンチーズを混ぜてありました。チーズの味がしたというのは正解です。"},{"id":{"d":361,"n":7,"t":1},"speaker":"A","text":"卵焼きにチーズが入っていたのが初めてだったので少し驚きましたが、とてもおいしかったです。"},{"id":{"d":361,"n":8,"t":1},"speaker":"B","text":"そのように言ってもらえるなら作ったかいがあります。お弁当箱をしっかり出しておいてくださいね。"},{"id":{"d":361,"n":9,"t":1},"speaker":"A","text":"分かりました。ごちそうさまでした。"}];
INSERT INTO sentence [{"id":{"d":362,"n":1,"t":1},"speaker":"A","text":"ナッツ類の詰め合わせを買ってきました。"},{"id":{"d":362,"n":2,"t":1},"speaker":"B","text":"たくさん入っていますね。このような詰め合わせを買うのは珍しいですね。"},{"id":{"d":362,"n":3,"t":1},"speaker":"A","text":"スーパーマーケットのレジの隣に置いてあり、安くておいしそうだったので買ってしまいました。"},{"id":{"d":362,"n":4,"t":1},"speaker":"B","text":"レジに並んでいるときに、目に入ってしまうものですね。私もつい買ってしまうことが多々あります。"},{"id":{"d":362,"n":5,"t":1},"speaker":"A","text":"お手頃な価格のものが並んでいると、つい買ってしまいます。"},{"id":{"d":362,"n":6,"t":1},"speaker":"B","text":"この詰め合わせは、何が入っていたのですか？"},{"id":{"d":362,"n":7,"t":1},"speaker":"A","text":"これはアーモンドやピーナッツ、カシューナッツなどが入っています。"},{"id":{"d":362,"n":8,"t":1},"speaker":"B","text":"しっかり食べたいものが入っていますね。どれくらいの値段でしたか？"},{"id":{"d":362,"n":9,"t":1},"speaker":"A","text":"500円ほどで安かったです。"}];
INSERT INTO sentence [{"id":{"d":363,"n":1,"t":1},"speaker":"A","text":"ご注文をお伺いします。"},{"id":{"d":363,"n":2,"t":1},"speaker":"B","text":"ラーメンライスを3つください。"},{"id":{"d":363,"n":3,"t":1},"speaker":"A","text":"かしこまりました。当店では学生応援キャンペーンを行なっていますが、学生さんですか？"},{"id":{"d":363,"n":4,"t":1},"speaker":"B","text":"そうです。すぐ近くの大学に通っています。"},{"id":{"d":363,"n":5,"t":1},"speaker":"A","text":"それでは学生証を提示いただいてもよろしいですか？"},{"id":{"d":363,"n":6,"t":1},"speaker":"B","text":"学生応援キャンペーンはどのようなものですか？"},{"id":{"d":363,"n":7,"t":1},"speaker":"A","text":"ラーメンとライスともに大盛りで提供することが可能です。また、くじ引きを引いていただいて、当たればギョウザをサービスいたします。"},{"id":{"d":363,"n":8,"t":1},"speaker":"B","text":"それでは学生証を提示いたしますので、大盛りをお願いします。"}];
INSERT INTO sentence [{"id":{"d":364,"n":1,"t":1},"speaker":"A","text":"今日の昼食はピザにします。"},{"id":{"d":364,"n":2,"t":1},"speaker":"B","text":"出前のピザですか？"},{"id":{"d":364,"n":3,"t":1},"speaker":"A","text":"違います。近所のスーパーマーケットで買ったピザです。"},{"id":{"d":364,"n":4,"t":1},"speaker":"B","text":"スーパーマーケットのものですか。おいしそうですか？どうしてもスーパーマーケットのピザは冷凍食品を温めたもののような感じがしてしまいます。"},{"id":{"d":364,"n":5,"t":1},"speaker":"A","text":"おいしそうなピザですよ。というのも今日買ってきたピザは石窯焼きのピザです。"},{"id":{"d":364,"n":6,"t":1},"speaker":"B","text":"スーパーマーケットに石窯焼きのピザがあるのですか？"},{"id":{"d":364,"n":7,"t":1},"speaker":"A","text":"ありました。加えてスーパーマーケットなので出前よりも安く買うことができます。"},{"id":{"d":364,"n":8,"t":1},"speaker":"B","text":"とても良いですね。そのスーパーマーケットは普通のパンもおいしそうです。"},{"id":{"d":364,"n":9,"t":1},"speaker":"A","text":"その通りです。おいしいパンがたくさんあって飽きることがありません。"}];
INSERT INTO sentence [{"id":{"d":365,"n":1,"t":1},"speaker":"A","text":"CGMという言葉を知っていますか？"},{"id":{"d":365,"n":2,"t":1},"speaker":"B","text":"全く知りません。何かの略語ですか？"},{"id":{"d":365,"n":3,"t":1},"speaker":"A","text":"これは「Consumer Generated Media」の略です。"},{"id":{"d":365,"n":4,"t":1},"speaker":"B","text":"つまり、消費者が作り上げたメディアということですか？"},{"id":{"d":365,"n":5,"t":1},"speaker":"A","text":"その通りです。一般の方でもメディアを作ることができるようになったため使われるようになりました。"},{"id":{"d":365,"n":6,"t":1},"speaker":"B","text":"消費者が作り上げたメディアとは、例えばどのようなものがありますか？"},{"id":{"d":365,"n":7,"t":1},"speaker":"A","text":"簡単なものは口コミサイトがCGMです。また、ブログの書き込みもCGMの1つに数えられます。"},{"id":{"d":365,"n":8,"t":1},"speaker":"B","text":"説明を受けると確かに口コミサイトはCGMですね。"}];
INSERT INTO sentence [{"id":{"d":366,"n":1,"t":1},"speaker":"A","text":"マンションの外壁の色の塗り替えが行われています。"},{"id":{"d":366,"n":2,"t":1},"speaker":"B","text":"一体、どのような色に塗り変わるのか楽しみですね。"},{"id":{"d":366,"n":3,"t":1},"speaker":"A","text":"先に塗り終わった場所を見ていると、色は変わらないようです。"},{"id":{"d":366,"n":4,"t":1},"speaker":"B","text":"それは悲しいですね。何のために塗り直しているのでしょうか？"},{"id":{"d":366,"n":5,"t":1},"speaker":"A","text":"多分少し剥げてきているところがあったため、塗り直しているのだと思います。"},{"id":{"d":366,"n":6,"t":1},"speaker":"B","text":"なるほど。剥げている部分を修復していたのですね。塗り直した後もそれほど色が変わっていなかったので、不思議に思っていたところでした。"}];
INSERT INTO sentence [{"id":{"d":367,"n":1,"t":1},"speaker":"A","text":"今日もとても寒いですが、半ズボンで登校しますか？"},{"id":{"d":367,"n":2,"t":1},"speaker":"B","text":"半ズボン以外は、はきたくないので半ズボンで登校します。"},{"id":{"d":367,"n":3,"t":1},"speaker":"A","text":"小学校で、あなた以外に半ズボンの人はいますか？"},{"id":{"d":367,"n":4,"t":1},"speaker":"B","text":"私の仲の良い友達はほとんど半ズボンです。"},{"id":{"d":367,"n":5,"t":1},"speaker":"A","text":"本当ですか？小学生は元気ですね。"},{"id":{"d":367,"n":6,"t":1},"speaker":"B","text":"長ズボンで登校すると、学校まで走って行って、そのあと朝の会まで校庭で遊んでいる間に、とても暑くなってしまいます。"},{"id":{"d":367,"n":7,"t":1},"speaker":"A","text":"その話を聞くと確かに半ズボンの方がいいと思います。しかし、風邪だけはひかないでくださいね。"},{"id":{"d":367,"n":8,"t":1},"speaker":"B","text":"もちろんです。汗をかいたらしっかり拭くようにしています。"}];
INSERT INTO sentence [{"id":{"d":368,"n":1,"t":1},"speaker":"A","text":"ウィンドブレーカーを、着たことはありますか？"},{"id":{"d":368,"n":2,"t":1},"speaker":"B","text":"もちろんあります。毎日着ています。"},{"id":{"d":368,"n":3,"t":1},"speaker":"A","text":"とてもシャカシャカ音が出る上着ですが、いつ頃から着ていますか？"},{"id":{"d":368,"n":4,"t":1},"speaker":"B","text":"初めて着たのは、小学5年生くらいだったと思います。"},{"id":{"d":368,"n":5,"t":1},"speaker":"A","text":"小学生の時からウィンドブレーカーを着ていたのですか？早いですね。"},{"id":{"d":368,"n":6,"t":1},"speaker":"B","text":"サッカーの練習の時に友達が着ていて、シャカシャカ音が出る方が、足が速い気がしてうらやましく、親に頼んで買ってもらった記憶があります。結局ウィンドブレーカーを着ても足は速くなりませんでしたが、速くなった気はしていました。"}];
INSERT INTO sentence [{"id":{"d":369,"n":1,"t":1},"speaker":"A","text":"今月はアルバイトをとても頑張ってくれたので、今日はご飯をごちそうします。"},{"id":{"d":369,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。お言葉に甘えてごちそうになります。"},{"id":{"d":369,"n":3,"t":1},"speaker":"A","text":"何か食べたいものはありますか？私は近くのおすし屋さんでテイクアウトはどうかと考えているのですが。"},{"id":{"d":369,"n":4,"t":1},"speaker":"B","text":"おすしがいいです。あのおすし屋さんはとてもおいしいのでうれしいです。"},{"id":{"d":369,"n":5,"t":1},"speaker":"A","text":"何か特別に食べたいものはありますか？もしなければ、私が勝手に決めてしまいますが。"},{"id":{"d":369,"n":6,"t":1},"speaker":"B","text":"なんでもおいしくいただきます。"},{"id":{"d":369,"n":7,"t":1},"speaker":"A","text":"分かりました。ではこのセットにしましょう。それから、確かあのすし屋は骨煎餅がおいしかったと記憶していますが、それもテイクアウトできると思いますか？"},{"id":{"d":369,"n":8,"t":1},"speaker":"B","text":"お店のホームページによると骨煎餅もテイクアウトできるようですよ。"},{"id":{"d":369,"n":9,"t":1},"speaker":"A","text":"それはよかったです。では早速お店へ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":370,"n":1,"t":1},"speaker":"A","text":"就職に合わせて、スーツを仕立てに行きましょう。"},{"id":{"d":370,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。うれしいです。"},{"id":{"d":370,"n":3,"t":1},"speaker":"A","text":"どのくらいの頻度で出社するのですか？また、テレワークの際にもスーツを着るのですか？"},{"id":{"d":370,"n":4,"t":1},"speaker":"B","text":"週に2日程度は出社する予定です。テレワークの時にはスーツは着なくて良いようです。"},{"id":{"d":370,"n":5,"t":1},"speaker":"A","text":"それでは、スーツは何着必要ですか？"},{"id":{"d":370,"n":6,"t":1},"speaker":"B","text":"2、3着はあるとうれしいです。必要になると思います。しかし、就活のために仕立てたスーツがあることを考えると今回仕立てるのは1着で良いと思います。"},{"id":{"d":370,"n":7,"t":1},"speaker":"A","text":"それでは1着のみ購入しましょう。体のサイズを覚えておけば、また購入しに行くことはできるはずです。"},{"id":{"d":370,"n":8,"t":1},"speaker":"B","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":371,"n":1,"t":1},"speaker":"A","text":"このメニューに生クリームは付いていますか？"},{"id":{"d":371,"n":2,"t":1},"speaker":"B","text":"はい、こちらのお菓子には生クリームが付いています。何か問題がありますか？"},{"id":{"d":371,"n":3,"t":1},"speaker":"A","text":"どのような生クリームですか？実はあまり得意でない生クリームがあるので気になりました。"},{"id":{"d":371,"n":4,"t":1},"speaker":"B","text":"市販の生クリームです。ケーキ屋のようなしっかりと自店で作るものではありません。"},{"id":{"d":371,"n":5,"t":1},"speaker":"A","text":"生クリームだけ少し頂くことはできますか？"},{"id":{"d":371,"n":6,"t":1},"speaker":"B","text":"追加で料金がかかりますが、よろしいですか？"},{"id":{"d":371,"n":7,"t":1},"speaker":"A","text":"構いません。よろしくお願いします。"},{"id":{"d":371,"n":8,"t":1},"speaker":"B","text":"かしこまりました。少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":372,"n":1,"t":1},"speaker":"A","text":"鉄板焼きを食べに行きませんか？"},{"id":{"d":372,"n":2,"t":1},"speaker":"B","text":"興味はありますが、突然どうしたのですか？"},{"id":{"d":372,"n":3,"t":1},"speaker":"A","text":"先日パソコンで調べ物をしていたところ、おいしそうな鉄板焼きの店が出てきたので食べたくなりました。"},{"id":{"d":372,"n":4,"t":1},"speaker":"B","text":"どのようなところですか？"},{"id":{"d":372,"n":5,"t":1},"speaker":"A","text":"都内のホテルの最上階にある鉄板焼き屋でした。隣にはバーも付いているようでした。"},{"id":{"d":372,"n":6,"t":1},"speaker":"B","text":"とても高そうなところですね。支払いはどうするのですか？"},{"id":{"d":372,"n":7,"t":1},"speaker":"A","text":"結婚記念日ですので、私がお小遣いから出しますよ。"},{"id":{"d":372,"n":8,"t":1},"speaker":"B","text":"それはありがとうございます。ごちそうさまです。"}];
INSERT INTO sentence [{"id":{"d":373,"n":1,"t":1},"speaker":"A","text":"今日は比較的、暖かい1日でしたね。"},{"id":{"d":373,"n":2,"t":1},"speaker":"B","text":"昨日までの寒さがうそのようでした。やっと春が来たという気分になりました。"},{"id":{"d":373,"n":3,"t":1},"speaker":"A","text":"そうですね。午前中に外を歩いていたら暖かかったです。"},{"id":{"d":373,"n":4,"t":1},"speaker":"B","text":"今日は雲もなく、きれいな青空でしたので日差しもありより、暖かかったのかもしれませんね。"},{"id":{"d":373,"n":5,"t":1},"speaker":"A","text":"歩いていて風も心地よく吹いていたので、気持ち良かったです。"},{"id":{"d":373,"n":6,"t":1},"speaker":"B","text":"いつまでこの天気は続くのでしょうか？"},{"id":{"d":373,"n":7,"t":1},"speaker":"A","text":"当分続きそうですよ。15度近くまで気温が上がる日もあるようです。"},{"id":{"d":373,"n":8,"t":1},"speaker":"B","text":"とてもうれしいです。またいきなり気温が下がることがないことを願います。"}];
INSERT INTO sentence [{"id":{"d":374,"n":1,"t":1},"speaker":"A","text":"1人暮らしをする上で、お薦めの料理は何ですか？"},{"id":{"d":374,"n":2,"t":1},"speaker":"B","text":"私は1人暮らしをしている時にお好み焼きをよく食べていました。"},{"id":{"d":374,"n":3,"t":1},"speaker":"A","text":"なぜお好み焼きなのですか？"},{"id":{"d":374,"n":4,"t":1},"speaker":"B","text":"お好み焼きは小麦粉と卵、キャベツがあれば簡単に作ることができるからです。ブタ肉があるとよりおいしく作ることができます。"},{"id":{"d":374,"n":5,"t":1},"speaker":"A","text":"確かにそうですね。簡単に作ることができますね。"},{"id":{"d":374,"n":6,"t":1},"speaker":"B","text":"その上、卵は基本的に冷蔵庫に常にありますし小麦粉、キャベツは安いです。"},{"id":{"d":374,"n":7,"t":1},"speaker":"A","text":"キャベツも、1玉全部を使わなくてよいですしね。"},{"id":{"d":374,"n":8,"t":1},"speaker":"B","text":"コストパフォーマンスがとても良いのです。また、冷凍することもできるので、たくさん作って冷凍して保存もできます。"},{"id":{"d":374,"n":9,"t":1},"speaker":"A","text":"素晴らしいですね。たくさん作って保存しておきます。"}];
INSERT INTO sentence [{"id":{"d":375,"n":1,"t":1},"speaker":"A","text":"ひつまぶしを食べたことはありますか？"},{"id":{"d":375,"n":2,"t":1},"speaker":"B","text":"ありません。名古屋の名物ですね。"},{"id":{"d":375,"n":3,"t":1},"speaker":"A","text":"その通りです。通常はウナギで作るもので、私も食べたことはありませんが、とてもおいしいそうです。"},{"id":{"d":375,"n":4,"t":1},"speaker":"B","text":"そのひつまぶしがどうしたのですか？"},{"id":{"d":375,"n":5,"t":1},"speaker":"A","text":"家で簡単に作ることができるのです。"},{"id":{"d":375,"n":6,"t":1},"speaker":"B","text":"ウナギを使った料理は、家で作る場合でも高くありませんか？"},{"id":{"d":375,"n":7,"t":1},"speaker":"A","text":"そこでウナギではなくサバ缶を使います。安く作ることができますし、とてもおいしいのでお薦めです。"},{"id":{"d":375,"n":8,"t":1},"speaker":"B","text":"サバ缶でひつまぶしを作ることができるとはにわかに信じ難いですが、作ってみます。"},{"id":{"d":375,"n":9,"t":1},"speaker":"A","text":"レシピは後ほど送ります。ぜひ感想も教えてくださいね。"}];
INSERT INTO sentence [{"id":{"d":376,"n":1,"t":1},"speaker":"A","text":"この風呂場は、浴室暖房付きですか？"},{"id":{"d":376,"n":2,"t":1},"speaker":"B","text":"浴室暖房付きです。また、追いだき機能も付いています。"},{"id":{"d":376,"n":3,"t":1},"speaker":"A","text":"素晴らしいですね。"},{"id":{"d":376,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。それではこちらの家を、ご契約でよろしいですか？"},{"id":{"d":376,"n":5,"t":1},"speaker":"A","text":"もう少し見せください。洗濯機はどこに置けば良いのですか？"},{"id":{"d":376,"n":6,"t":1},"speaker":"B","text":"こちらの引き戸を開けたところです。"},{"id":{"d":376,"n":7,"t":1},"speaker":"A","text":"引き戸の中に洗濯機を入れるのですか。斬新ですね。扉が傷むことはありませんか？"},{"id":{"d":376,"n":8,"t":1},"speaker":"B","text":"そちらは対策していただきたいです。"},{"id":{"d":376,"n":9,"t":1},"speaker":"A","text":"分かりました。少し考えさせてください。"}];
INSERT INTO sentence [{"id":{"d":377,"n":1,"t":1},"speaker":"A","text":"オーダーメイドのワイシャツがあることを知っていましたか？"},{"id":{"d":377,"n":2,"t":1},"speaker":"B","text":"知りませんでした。ワイシャツもオーダーメイドできるのですね。"},{"id":{"d":377,"n":3,"t":1},"speaker":"A","text":"自分の体に合わせたワイシャツを作ることができます。今日作ってきたのですが、いろいろと驚くことがありました。"},{"id":{"d":377,"n":4,"t":1},"speaker":"B","text":"どのようなことに驚きましたか？"},{"id":{"d":377,"n":5,"t":1},"speaker":"A","text":"私は腕が長いそうです。その影響でちょうど良いシャツはあまり存在しないとのことでした。"},{"id":{"d":377,"n":6,"t":1},"speaker":"B","text":"自分の体型の特徴を知ることができるのは良いですね。"},{"id":{"d":377,"n":7,"t":1},"speaker":"A","text":"はい。とても有益でした。オーダーメイドのワイシャツ作りはとても楽しかったです。"}];
INSERT INTO sentence [{"id":{"d":378,"n":1,"t":1},"speaker":"A","text":"今日は業務スーパーでパンを買ってきました。"},{"id":{"d":378,"n":2,"t":1},"speaker":"B","text":"業務スーパーの商品は、とても量が多そうですが、保存は大丈夫ですか？"},{"id":{"d":378,"n":3,"t":1},"speaker":"A","text":"まとめて冷凍したので大丈夫です。"},{"id":{"d":378,"n":4,"t":1},"speaker":"B","text":"全て入るほど冷凍庫が大きいのですか？"},{"id":{"d":378,"n":5,"t":1},"speaker":"A","text":"業務スーパーの商品の中でも量の少ないものを選択したので、大丈夫でした。"},{"id":{"d":378,"n":6,"t":1},"speaker":"B","text":"どのように食べるのですか？"},{"id":{"d":378,"n":7,"t":1},"speaker":"A","text":"食パンは、マヨネーズを上に乗せて魚焼きグリルで焼いて食べます。"},{"id":{"d":378,"n":8,"t":1},"speaker":"B","text":"すごくおいしそうですね。私もやってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":379,"n":1,"t":1},"speaker":"A","text":"普段、眼鏡はかけていますか？"},{"id":{"d":379,"n":2,"t":1},"speaker":"B","text":"私は目が良いのでかけたことがありません。あなたはかけていますか？"},{"id":{"d":379,"n":3,"t":1},"speaker":"A","text":"家ではほとんど眼鏡をかけています。最近は外でも眼鏡をかけるようになりました。"},{"id":{"d":379,"n":4,"t":1},"speaker":"B","text":"眼鏡はいつからかけているのですか？"},{"id":{"d":379,"n":5,"t":1},"speaker":"A","text":"小学生の時から着けるようになりました。"},{"id":{"d":379,"n":6,"t":1},"speaker":"B","text":"コンタクトレンズにしたことはありましたか？"},{"id":{"d":379,"n":7,"t":1},"speaker":"A","text":"高校と大学の最初の頃はコンタクトでした。"},{"id":{"d":379,"n":8,"t":1},"speaker":"B","text":"なぜ高校や大学の頃はコンタクトにしていたのですか？"},{"id":{"d":379,"n":9,"t":1},"speaker":"A","text":"スポーツをしていたので、そちらの方が動きやすかったのです。"},{"id":{"d":379,"n":10,"t":1},"speaker":"B","text":"今は眼鏡ですが、もうスポーツはやられていないのですか？"},{"id":{"d":379,"n":11,"t":1},"speaker":"A","text":"その通りです。"}];
INSERT INTO sentence [{"id":{"d":380,"n":1,"t":1},"speaker":"A","text":"キャベツの千切りの、簡単な作り方を知っていますか？"},{"id":{"d":380,"n":2,"t":1},"speaker":"B","text":"普段は重ねて切っています。簡単な方法はどのようなやり方ですか？"},{"id":{"d":380,"n":3,"t":1},"speaker":"A","text":"ピーラーを使うことによって、キャベツの千切りを簡単に作ることができるそうです。"},{"id":{"d":380,"n":4,"t":1},"speaker":"B","text":"それでは一枚一枚の水洗いはどのようにするのですか？"},{"id":{"d":380,"n":5,"t":1},"speaker":"A","text":"千切りを作ってから、水で洗います。"},{"id":{"d":380,"n":6,"t":1},"speaker":"B","text":"そうすると、キャベツが水を含んでしまいませんか？"},{"id":{"d":380,"n":7,"t":1},"speaker":"A","text":"しかし、パリパリになりますよ。"},{"id":{"d":380,"n":8,"t":1},"speaker":"B","text":"私は、水を含んでしまうのが嫌いなので、今まで通りの作り方をしようと思います。"}];
INSERT INTO sentence [{"id":{"d":381,"n":1,"t":1},"speaker":"A","text":"立体駐車場に、駐車したことはありますか？"},{"id":{"d":381,"n":2,"t":1},"speaker":"B","text":"怖くて駐車したことがありません。上がることができるか不安です。"},{"id":{"d":381,"n":3,"t":1},"speaker":"A","text":"たまにとても狭い立体駐車場がありますが、基本的には大丈夫です。"},{"id":{"d":381,"n":4,"t":1},"speaker":"B","text":"とても狭く感じてしまいます。駐車券を取ることも怖いです。"},{"id":{"d":381,"n":5,"t":1},"speaker":"A","text":"駐車券は車幅を把握することができれば簡単に取ることができますよ。"},{"id":{"d":381,"n":6,"t":1},"speaker":"B","text":"物と距離が近くなった時に、警報音が鳴ることが怖くて駐車券を取ることができません。"},{"id":{"d":381,"n":7,"t":1},"speaker":"A","text":"確かに、警報音は怖いですね。自分で大丈夫と思っていても大きな音が鳴るので不安になってしまいます。"},{"id":{"d":381,"n":8,"t":1},"speaker":"B","text":"まさにそうなのです。"}];
INSERT INTO sentence [{"id":{"d":382,"n":1,"t":1},"speaker":"A","text":"カレーには何を付けますか？"},{"id":{"d":382,"n":2,"t":1},"speaker":"B","text":"私は福神漬けを付けます。他に選択肢は何があったのですか？思い付きませんでした。"},{"id":{"d":382,"n":3,"t":1},"speaker":"A","text":"ラッキョウやアチャール、キャベツがあるようです。"},{"id":{"d":382,"n":4,"t":1},"speaker":"B","text":"ラッキョウとキャベツは分かりますが、アチャールとは何ですか？"},{"id":{"d":382,"n":5,"t":1},"speaker":"A","text":"私もよく知りませんが、インドのピクルスのようです。"},{"id":{"d":382,"n":6,"t":1},"speaker":"B","text":"つまりカレーには漬物が合うということですね。"},{"id":{"d":382,"n":7,"t":1},"speaker":"A","text":"確かにキャベツ以外全て漬物ですね。なぜ漬物が合うのでしょうか？"},{"id":{"d":382,"n":8,"t":1},"speaker":"B","text":"少し甘さも感じることができるのでカレーの辛さをマイルドにしてくれるという理由だと思います。"}];
INSERT INTO sentence [{"id":{"d":383,"n":1,"t":1},"speaker":"A","text":"靴下に穴が開きました。新しいものを買いに行きましょう。"},{"id":{"d":383,"n":2,"t":1},"speaker":"B","text":"私もパンツに穴が開いたので新しいものが欲しかったのでぜひ行きたいです。"},{"id":{"d":383,"n":3,"t":1},"speaker":"A","text":"他に何か買うものはありますか？"},{"id":{"d":383,"n":4,"t":1},"speaker":"B","text":"私は特にありませんが、何かありますか？"},{"id":{"d":383,"n":5,"t":1},"speaker":"A","text":"これから春になるので、それに合わせて長袖Tシャツが欲しいです。"},{"id":{"d":383,"n":6,"t":1},"speaker":"B","text":"分かりました。靴下のメーカーに何かこだわりはありますか？"},{"id":{"d":383,"n":7,"t":1},"speaker":"A","text":"特にありません。こだわりは形にしかありません。くるぶしソックスがいいです。"},{"id":{"d":383,"n":8,"t":1},"speaker":"B","text":"分かりました。それでは、比較的大きな衣料品店に行きましょう。"},{"id":{"d":383,"n":9,"t":1},"speaker":"A","text":"全てそこで購入することができたら良いですね。"}];
INSERT INTO sentence [{"id":{"d":384,"n":1,"t":1},"speaker":"A","text":"北海道では、どのような靴を履くべきですか？"},{"id":{"d":384,"n":2,"t":1},"speaker":"B","text":"どの時期に、どこへ行きますか？"},{"id":{"d":384,"n":3,"t":1},"speaker":"A","text":"3月に、札幌へ行きます。"},{"id":{"d":384,"n":4,"t":1},"speaker":"B","text":"札幌ならば、地下の道も充実しているので、それほど靴に気を使う必要はないと思います。"},{"id":{"d":384,"n":5,"t":1},"speaker":"A","text":"北海道大学構内にも行きたいと思っています。それでも普通の靴で大丈夫ですか？"},{"id":{"d":384,"n":6,"t":1},"speaker":"B","text":"普通の靴で問題は、全くありませんが、歩き方を知らないと滑るので怖いと思いますよ。"},{"id":{"d":384,"n":7,"t":1},"speaker":"A","text":"やはりかんじきを持って行くべきですか？"},{"id":{"d":384,"n":8,"t":1},"speaker":"B","text":"不安であれば、持って行くことを推奨します。"}];
INSERT INTO sentence [{"id":{"d":385,"n":1,"t":1},"speaker":"A","text":"炊き込みご飯を作りましょう。"},{"id":{"d":385,"n":2,"t":1},"speaker":"B","text":"炊き込みご飯はとてもおいしいので大好きです。何を炊き込むのですか？"},{"id":{"d":385,"n":3,"t":1},"speaker":"A","text":"今朝、友人が持ってきてくれたタケノコを炊き込もうと思います。"},{"id":{"d":385,"n":4,"t":1},"speaker":"B","text":"久しぶりにタケノコの炊き込みご飯を食べます。ただ、採れたてだったら炊き込まない方がおいしいのではありませんか？"},{"id":{"d":385,"n":5,"t":1},"speaker":"A","text":"たくさん頂いているので、問題はありません。炊き込まない料理もしっかり作りますよ。"},{"id":{"d":385,"n":6,"t":1},"speaker":"B","text":"小さい頃に食べたタケノコの刺身が忘れられません。もう一度食べたいです。"},{"id":{"d":385,"n":7,"t":1},"speaker":"A","text":"刺身として食べることができるタケノコはとても少ないですし、採れたてでないとできないので今度タケノコを掘りに行ってみましょう。"},{"id":{"d":385,"n":8,"t":1},"speaker":"B","text":"ぜひ行きましょう。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":386,"n":1,"t":1},"speaker":"A","text":"久しぶりに食べ放題に行きませんか？"},{"id":{"d":386,"n":2,"t":1},"speaker":"B","text":"確かに、久しぶりに行きたいですね。どこの食べ放題に行きますか？"},{"id":{"d":386,"n":3,"t":1},"speaker":"A","text":"私は中華かしゃぶしゃぶ、イタリアンの食べ放題がいいです。"},{"id":{"d":386,"n":4,"t":1},"speaker":"B","text":"たくさん候補がありますね。それでは中華の食べ放題に行きましょう。"},{"id":{"d":386,"n":5,"t":1},"speaker":"A","text":"なんと、今検索をしてみましたら、中華の食べ放題は閉店しているようです。"},{"id":{"d":386,"n":6,"t":1},"speaker":"B","text":"それは残念です。それではしゃぶしゃぶの食べ放題のお店はどうですか？"},{"id":{"d":386,"n":7,"t":1},"speaker":"A","text":"しゃぶしゃぶは開店しているようなので予約します。"},{"id":{"d":386,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":387,"n":1,"t":1},"speaker":"A","text":"お母さん、このワイシャツにアイロンをかけてください。"},{"id":{"d":387,"n":2,"t":1},"speaker":"B","text":"アイロンは自分でかけてください。それほど難しいことでもないはずですよ。"},{"id":{"d":387,"n":3,"t":1},"speaker":"A","text":"かけること自体は難しくないのですが、仕上がりがうまくいきません。"},{"id":{"d":387,"n":4,"t":1},"speaker":"B","text":"それではきれいに仕上げるためのコツを教えるので、その通りにやってみてください。"},{"id":{"d":387,"n":5,"t":1},"speaker":"A","text":"分かりました。それではコツを教えてください。"},{"id":{"d":387,"n":6,"t":1},"speaker":"B","text":"まず、アイロンをかけるときにはしっかり逆の手で伸ばしながらかけてください。そうすることでしっかりシワが伸びます。"},{"id":{"d":387,"n":7,"t":1},"speaker":"A","text":"なるほど。このときに何か気を付けることはありますか？"},{"id":{"d":387,"n":8,"t":1},"speaker":"B","text":"既についている線を基準にして、アイロンをかけてください。そうすると、きれいな形に仕上がりますよ。"}];
INSERT INTO sentence [{"id":{"d":388,"n":1,"t":1},"speaker":"A","text":"近くのコンビニエンスストアへおでんを買いに行きませんか？"},{"id":{"d":388,"n":2,"t":1},"speaker":"B","text":"寒くなってきたらおでんが食べたくなりますね。買いに行きましょう。"},{"id":{"d":388,"n":3,"t":1},"speaker":"A","text":"もし買いたい具が決まっていたら買ってきますよ。"},{"id":{"d":388,"n":4,"t":1},"speaker":"B","text":"実際にどの具が一番染みているか、見てから買いたので大丈夫です。何を買うか決まっているようでしたら私が買ってきますよ。"},{"id":{"d":388,"n":5,"t":1},"speaker":"A","text":"もう決まっているので、お言葉に甘えてお願いしても良いですか？"},{"id":{"d":388,"n":6,"t":1},"speaker":"B","text":"もちろんです。何を買ってきますか？"},{"id":{"d":388,"n":7,"t":1},"speaker":"A","text":"卵と牛すじとダイコンをお願いします。つゆは多めでお願いします。"},{"id":{"d":388,"n":8,"t":1},"speaker":"B","text":"分かりました。では行ってきますね。"}];
INSERT INTO sentence [{"id":{"d":389,"n":1,"t":1},"speaker":"A","text":"夏になると「冷やし中華始めました」という張り紙をよく見ますが、なぜあのような張り紙をするのでしょうか？"},{"id":{"d":389,"n":2,"t":1},"speaker":"B","text":"私もよく分かりません。少し前に芸人の方がギターで演奏をしながら「冷やし中華はじめました」と歌っていたので注意してみるようになりましたが、それまでは気付きもしませんでした。"},{"id":{"d":389,"n":3,"t":1},"speaker":"A","text":"それではなぜ張り紙をするのか予想してみましょう。"},{"id":{"d":389,"n":4,"t":1},"speaker":"B","text":"正直、私は夏でなくても冷やし中華を食べることはできると思っています。"},{"id":{"d":389,"n":5,"t":1},"speaker":"A","text":"その通りだと思います。なぜ夏に冷やし中華となっているのでしょうか？"},{"id":{"d":389,"n":6,"t":1},"speaker":"B","text":"「冷やし」と付いているので、涼しく感じるということが理由の1つとしてあるのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":390,"n":1,"t":1},"speaker":"A","text":"今日のポテトサラダに、マカロニは入っていますか？"},{"id":{"d":390,"n":2,"t":1},"speaker":"B","text":"今日は、私が作ったので入っていません。"},{"id":{"d":390,"n":3,"t":1},"speaker":"A","text":"お母さんが作ったということは、タマネギが生のまま入っていますか？"},{"id":{"d":390,"n":4,"t":1},"speaker":"B","text":"その通りです。しっかり水でさらして辛味を抜いていますよ。"},{"id":{"d":390,"n":5,"t":1},"speaker":"A","text":"数日たった後にまだ余っていたら、量を増すためにマカロニを入れませんか？"},{"id":{"d":390,"n":6,"t":1},"speaker":"B","text":"いいですね。2日たってもまだ残っているようだったら、マカロニを入れましょう。"},{"id":{"d":390,"n":7,"t":1},"speaker":"A","text":"私はマカロニ入りを食べたいので、2日後にも残るように計算して分けましょう。"},{"id":{"d":390,"n":8,"t":1},"speaker":"B","text":"それでは、あなたにとりわけは任せますね。"}];
INSERT INTO sentence [{"id":{"d":391,"n":1,"t":1},"speaker":"A","text":"こちらの物件に対して、質問はありますか？"},{"id":{"d":391,"n":2,"t":1},"speaker":"B","text":"こちらの物件のエアコンは、最新のものですか？"},{"id":{"d":391,"n":3,"t":1},"speaker":"A","text":"この物件は、先日建ったばかりなので、最新のエアコンを使っています。電気代も安く済ませることが可能です。"},{"id":{"d":391,"n":4,"t":1},"speaker":"B","text":"それはいいですね。それでは、ガスはプロパンですか？都市ガスですか？"},{"id":{"d":391,"n":5,"t":1},"speaker":"A","text":"こちらのガスは都市ガスです。プロパンガスと比較して安くなっています。"},{"id":{"d":391,"n":6,"t":1},"speaker":"B","text":"それでは最後の質問です。こちらの物件のお風呂には、追いだき機能は付いていますか？"},{"id":{"d":391,"n":7,"t":1},"speaker":"A","text":"追いだきは付いていません。お風呂に入る際は、入る直前にお湯を沸かしてください。"},{"id":{"d":391,"n":8,"t":1},"speaker":"B","text":"追いだきは付いていませんか。分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":392,"n":1,"t":1},"speaker":"A","text":"裁判所から書類が届いていましたよ。"},{"id":{"d":392,"n":2,"t":1},"speaker":"B","text":"なぜ裁判所から連絡が来るのでしょうか？心当たりがありません。"},{"id":{"d":392,"n":3,"t":1},"speaker":"A","text":"書類を確認した方がいいと思いますが、アルバイト先が潰れたということに関係があるのではないでしょうか？"},{"id":{"d":392,"n":4,"t":1},"speaker":"B","text":"しかし、アルバイト先が潰れたのは半年前のことです。まだ何かすることがあるのでしょうか？"},{"id":{"d":392,"n":5,"t":1},"speaker":"A","text":"給料の未払いなどがあると思いますので、その連絡ではないでしょうか？"},{"id":{"d":392,"n":6,"t":1},"speaker":"B","text":"そういえば店長が重要な書類が届くと、連絡をしてきていた記憶があります。"},{"id":{"d":392,"n":7,"t":1},"speaker":"A","text":"では、その書類でしょう。早く提出した方がいいかもしれませんね。"},{"id":{"d":392,"n":8,"t":1},"speaker":"B","text":"早速、開けて書類の記入をしようと思います。"}];
INSERT INTO sentence [{"id":{"d":393,"n":1,"t":1},"speaker":"A","text":"明日から北海道に行きますが、どの靴を履いて行くか決めることができません。"},{"id":{"d":393,"n":2,"t":1},"speaker":"B","text":"北海道に行くのであれば、冬靴の方がいいのではありませんか？"},{"id":{"d":393,"n":3,"t":1},"speaker":"A","text":"もちろんその通りですが、本州には冬靴はほとんど売っていません。"},{"id":{"d":393,"n":4,"t":1},"speaker":"B","text":"冬に靴を変える必要がないので当たり前ですね。"},{"id":{"d":393,"n":5,"t":1},"speaker":"A","text":"元々持っていた冬靴は、どうしたのですか？"},{"id":{"d":393,"n":6,"t":1},"speaker":"B","text":"本州に帰ってくるときに、全て捨ててしまいました。"},{"id":{"d":393,"n":7,"t":1},"speaker":"A","text":"それでは普通の靴に防水スプレーをかけて行くしかありませんね。"},{"id":{"d":393,"n":8,"t":1},"speaker":"B","text":"転ばないための歩き方は、分かっているのでそれもいいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":394,"n":1,"t":1},"speaker":"A","text":"今日の昼食は、久しぶりにパンを買って食べました。"},{"id":{"d":394,"n":2,"t":1},"speaker":"B","text":"どこのパンを食べたのですか？"},{"id":{"d":394,"n":3,"t":1},"speaker":"A","text":"北海道でしか見たことがないパン屋です。「ボストンベイク」という名前です。"},{"id":{"d":394,"n":4,"t":1},"speaker":"B","text":"どのようなパンを置いている店ですか？"},{"id":{"d":394,"n":5,"t":1},"speaker":"A","text":"さまざまな種類のパンを置いています。私はその中でもサンドイッチが一番好きです。"},{"id":{"d":394,"n":6,"t":1},"speaker":"B","text":"サンドイッチはおいしいですね。しかし、値段も高くありませんか？"},{"id":{"d":394,"n":7,"t":1},"speaker":"A","text":"それが「ボストンベイク」は量も多く安い店です。今日購入したものは20センチメートルほどの長さがあって250円でした。"},{"id":{"d":394,"n":8,"t":1},"speaker":"B","text":"それはとても安いですね。北海道に行ったら買いに行きたいです。"}];
INSERT INTO sentence [{"id":{"d":395,"n":1,"t":1},"speaker":"A","text":"近くにお薦めの美容院はありますか？"},{"id":{"d":395,"n":2,"t":1},"speaker":"B","text":"あまり美容院に詳しくありませんが、私がよく行く美容院なら紹介することができます。"},{"id":{"d":395,"n":3,"t":1},"speaker":"A","text":"ぜひ紹介してください。最近、引っ越してきたため何も分かりません。"},{"id":{"d":395,"n":4,"t":1},"speaker":"B","text":"私が頻繁に行っているところはスーパーマーケットの目の前にあるところです。また、元々行っていたのは少し離れますが、隣駅のショッピングモール内の美容院です。"},{"id":{"d":395,"n":5,"t":1},"speaker":"A","text":"どちらの方がお薦めですか？"},{"id":{"d":395,"n":6,"t":1},"speaker":"B","text":"私は、今通っている場所の方がいいですが、男性ならショッピングモールの方がいいかもしれません。"},{"id":{"d":395,"n":7,"t":1},"speaker":"A","text":"それはなぜですか？"},{"id":{"d":395,"n":8,"t":1},"speaker":"B","text":"美容師の方に男性が多いような気がするので、入りやすいのではないかと思います。"}];
INSERT INTO sentence [{"id":{"d":396,"n":1,"t":1},"speaker":"A","text":"近年は、多くのものがデータ化されてきていますね。"},{"id":{"d":396,"n":2,"t":1},"speaker":"B","text":"そうですね。インターネットが使えない人は、これから生活していくことが難しいのではないかと思うほどデジタル化が進んでいます。"},{"id":{"d":396,"n":3,"t":1},"speaker":"A","text":"特に、デジタル化を感じることは何ですか？"},{"id":{"d":396,"n":4,"t":1},"speaker":"B","text":"私は、新聞を取らなくなったことが、デジタル化による日常生活の変化だと感じます。"},{"id":{"d":396,"n":5,"t":1},"speaker":"A","text":"昔は新聞を取っていたのですか？"},{"id":{"d":396,"n":6,"t":1},"speaker":"B","text":"取っていました。朝4時半や5時頃に新聞配達のバイクの音が聞こえていましたが、最近は全く聞きません。"},{"id":{"d":396,"n":7,"t":1},"speaker":"A","text":"確かに最近は新聞配達のバイクの音を聞くことが少なくなったような気がします。"},{"id":{"d":396,"n":8,"t":1},"speaker":"B","text":"その分、サブスクリプションで新聞を読んでいる人が増えているということでしょう。"},{"id":{"d":396,"n":9,"t":1},"speaker":"A","text":"少し前までの日常が、変化してきているということですね。"}];
INSERT INTO sentence [{"id":{"d":397,"n":1,"t":1},"speaker":"A","text":"だしを取るために昆布を使います。"},{"id":{"d":397,"n":2,"t":1},"speaker":"B","text":"だしをしっかり昆布から取るとは本格的な料理ですね。"},{"id":{"d":397,"n":3,"t":1},"speaker":"A","text":"たまにはしっかり作ろうと思いました。"},{"id":{"d":397,"n":4,"t":1},"speaker":"B","text":"しかし私は1人暮らしをしているときに昆布でだしを取ろうとした時になぜか全くおいしくないものが出来上がりました。"},{"id":{"d":397,"n":5,"t":1},"speaker":"A","text":"それはあまり良い昆布ではなかっただけではありませんか？"},{"id":{"d":397,"n":6,"t":1},"speaker":"B","text":"そのスーパーマーケットの中で、一番高い昆布を買ったのでそのようなことはないと思います。"},{"id":{"d":397,"n":7,"t":1},"speaker":"A","text":"それでは取り方を間違えたのでしょう。しっかり教えますね。"},{"id":{"d":397,"n":8,"t":1},"speaker":"B","text":"動画を参考にしながらやったのですが間違っていたのでしょう。ぜひ教えてください。"}];
INSERT INTO sentence [{"id":{"d":398,"n":1,"t":1},"speaker":"A","text":"先日テレビの特集であんトーストをやっていました。"},{"id":{"d":398,"n":2,"t":1},"speaker":"B","text":"とてもおいしいメニューですね。どのような特集をやっていたのですか？"},{"id":{"d":398,"n":3,"t":1},"speaker":"A","text":"さまざまな種類のパンに対してご当地の粒あんやこしあん、バターを用いて一番おいしく食べる方法を考えるというものでした。"},{"id":{"d":398,"n":4,"t":1},"speaker":"B","text":"見ているだけでおなかがすいてしまいそうな気がします。"},{"id":{"d":398,"n":5,"t":1},"speaker":"A","text":"その中で、全国展開のスーパーマーケットで購入できるパンやあんこ、バターを使ったものが紹介されていたので実際に挑戦してみました。"},{"id":{"d":398,"n":6,"t":1},"speaker":"B","text":"実際に挑戦してみてどうでしたか？やはりおいしかったですか？"},{"id":{"d":398,"n":7,"t":1},"speaker":"A","text":"とてもおいしかったです。やはり特集でお薦めというだけあるなと思いました。"},{"id":{"d":398,"n":8,"t":1},"speaker":"B","text":"特別のものを使わなくてもスーパーマーケットの商品でもおいしく作ることができるのですね。私も試してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":399,"n":1,"t":1},"speaker":"A","text":"将来は持ち家に住みたいですか？それともマンションが良いですか？"},{"id":{"d":399,"n":2,"t":1},"speaker":"B","text":"私は持ち家が良いです。どちらの方がいいですか？"},{"id":{"d":399,"n":3,"t":1},"speaker":"A","text":"私も持ち家の方がいいです。ちなみに、なぜ持ち家の方がいいのですか？"},{"id":{"d":399,"n":4,"t":1},"speaker":"B","text":"将来子どもが欲しいと考えているので、子どもが好きに騒いでもマンションほど周囲を心配しなくて良いのではないかと思うので持ち家の方がいいと思っています。"},{"id":{"d":399,"n":5,"t":1},"speaker":"A","text":"確かに下の階の人を心配して生活しなくて良いので、楽かもしれませんね。私もそのように考えます。"},{"id":{"d":399,"n":6,"t":1},"speaker":"B","text":"マンションの利点はどのようなところがあるのでしょうか？"},{"id":{"d":399,"n":7,"t":1},"speaker":"A","text":"マンションは庭の手入れや建物のメンテナンスは考える必要がないところが利点でしょう。"},{"id":{"d":399,"n":8,"t":1},"speaker":"B","text":"きっと他にも利点はあるのでしょうが、私はやはり持ち家の方がいいと思います。"}];
INSERT INTO sentence [{"id":{"d":400,"n":1,"t":1},"speaker":"A","text":"新生活を始めるためには、お金がかかりますね。"},{"id":{"d":400,"n":2,"t":1},"speaker":"B","text":"やはりお金はたくさんかかりますか？"},{"id":{"d":400,"n":3,"t":1},"speaker":"A","text":"新生活を始めるに当たって必要な家電や家具を探していましたが、大学生時代の家電は6年間も使っているので使うことができず、全く必要なものを購入することができません。"},{"id":{"d":400,"n":4,"t":1},"speaker":"B","text":"どのようなものは購入できそうなのですか？"},{"id":{"d":400,"n":5,"t":1},"speaker":"A","text":"洗濯機やテーブルは購入できますが、掃除機やベッド、仕事用の机や椅子は購入できません。"},{"id":{"d":400,"n":6,"t":1},"speaker":"B","text":"それは体が痛い生活を送ることになりそうですね。大丈夫ですか？"},{"id":{"d":400,"n":7,"t":1},"speaker":"A","text":"正直、不安が大きいですが、そうするしかないと思っているので仕方ありません。今更弱音を吐いても何も変わりませんし。"},{"id":{"d":400,"n":8,"t":1},"speaker":"B","text":"頑張ってくださいね。応援しています。"}];
INSERT INTO sentence [{"id":{"d":401,"n":1,"t":1},"speaker":"A","text":"今日はスーパーマーケットにあめ玉を買いに行きませんか？"},{"id":{"d":401,"n":2,"t":1},"speaker":"B","text":"山本君の家に行く前にお菓子を買っていきましょう。小学校の宿題も持ってきていますか？"},{"id":{"d":401,"n":3,"t":1},"speaker":"A","text":"もちろん持ってきています。お菓子はいつも通りあめだけでよいですか？"},{"id":{"d":401,"n":4,"t":1},"speaker":"B","text":"私はお金を10円しか持ってきていないので、あめかガムしか買うことができません。"},{"id":{"d":401,"n":5,"t":1},"speaker":"A","text":"私は50円持ってきているので、少し大きなお菓子を買いますか？"},{"id":{"d":401,"n":6,"t":1},"speaker":"B","text":"2人合わせて60円なので、少し多めに入っているお菓子が買えそうですね。しかし、私はあめが良いです。"},{"id":{"d":401,"n":7,"t":1},"speaker":"A","text":"それでは、私はガムとあめを両方買いましょう。"}];
INSERT INTO sentence [{"id":{"d":402,"n":1,"t":1},"speaker":"A","text":"洗車をしようと思うのですが、洗車セットはどこにありますか？"},{"id":{"d":402,"n":2,"t":1},"speaker":"B","text":"私もあまり覚えていませんが、階段下収納にあったはずです。"},{"id":{"d":402,"n":3,"t":1},"speaker":"A","text":"先ほど階段下収納を確認したのですが見つけることができませんでした。階段下収納のどの辺にあると思いますか？"},{"id":{"d":402,"n":4,"t":1},"speaker":"B","text":"段ボールの下にあると思いますが、そこにない場合は階段下収納以外の場所にあると思います。"},{"id":{"d":402,"n":5,"t":1},"speaker":"A","text":"階段下収納以外の場所とは、どこですか？"},{"id":{"d":402,"n":6,"t":1},"speaker":"B","text":"洗面所の下の収納場所か2階の洗面所の下の収納場所にあると思います。"},{"id":{"d":402,"n":7,"t":1},"speaker":"A","text":"わざわざ2階に持って行くとは思えないので、きっと1階の洗面所の収納にあると思うので確認します。"},{"id":{"d":402,"n":8,"t":1},"speaker":"B","text":"見つからなかったら教えてください。私も一緒に探します。"}];
INSERT INTO sentence [{"id":{"d":403,"n":1,"t":1},"speaker":"A","text":"少し暖かくなってきたので、タンスの服を冬服から春服に少しずつ変えた方がいいですね。"},{"id":{"d":403,"n":2,"t":1},"speaker":"B","text":"これから寒くなることは、少なくなりそうですか？"},{"id":{"d":403,"n":3,"t":1},"speaker":"A","text":"少ないと思います。今週末は気温が20度近くまで上がるようです。"},{"id":{"d":403,"n":4,"t":1},"speaker":"B","text":"それは暖かいですね。冬服では暑くなりすぎて、汗をかくことになると思います。"},{"id":{"d":403,"n":5,"t":1},"speaker":"A","text":"しかし全ての冬服を片付けてしまうと、予報と異なり寒くなってしまった場合に、また取り出すのが大変だと思うのでしっかり考えながら片付けてくださいね。"},{"id":{"d":403,"n":6,"t":1},"speaker":"B","text":"少し考えながら片付けをしないといけないので、面倒です。"},{"id":{"d":403,"n":7,"t":1},"speaker":"A","text":"仕方ありませんね。ずっと、同じ服を着ているわけにはいきません。"},{"id":{"d":403,"n":8,"t":1},"speaker":"B","text":"それでは、今週中に洋服を片付けようと思います。"}];
INSERT INTO sentence [{"id":{"d":404,"n":1,"t":1},"speaker":"A","text":"お腹が減りました。何かつまむものはありませんか？"},{"id":{"d":404,"n":2,"t":1},"speaker":"B","text":"机の上にお菓子が置いてあるのでそれを食べてよいですよ。"},{"id":{"d":404,"n":3,"t":1},"speaker":"A","text":"先週私が買ってきたサラミが冷蔵庫の中にまだあると記憶しているのですが、まだありますか？"},{"id":{"d":404,"n":4,"t":1},"speaker":"B","text":"私は食べていないので食べきっていなければまだあると思いますよ。"},{"id":{"d":404,"n":5,"t":1},"speaker":"A","text":"それではお菓子だけで足りなかった場合サラミも食べようと思います。"},{"id":{"d":404,"n":6,"t":1},"speaker":"B","text":"今日の晩ご飯は焼肉ですが晩ご飯を食べられないことがないように気を付けてくださいね。"},{"id":{"d":404,"n":7,"t":1},"speaker":"A","text":"晩ご飯が焼肉とは知りませんでした。それでは何も食べないでお腹を空かせておきます。"},{"id":{"d":404,"n":8,"t":1},"speaker":"B","text":"それがいいと思います。お肉もたくさん買ってありますよ。"}];
INSERT INTO sentence [{"id":{"d":405,"n":1,"t":1},"speaker":"A","text":"昼食用にハンバーガーを買いに行きましょう。"},{"id":{"d":405,"n":2,"t":1},"speaker":"B","text":"いいですね。そういえば、ハンバーガーを食べるのは一年ぶりかもしれません。"},{"id":{"d":405,"n":3,"t":1},"speaker":"A","text":"確かに、ハンバーガーの店は少し遠いので、買いに行くことが面倒であまり食べませんね。"},{"id":{"d":405,"n":4,"t":1},"speaker":"B","text":"そうですよね。今日はなぜハンバーグにしようと思ったのですか？"},{"id":{"d":405,"n":5,"t":1},"speaker":"A","text":"買い物でちょうどそちらの方へ行く予定があったので、せっかくならハンバーガー屋まで足を伸ばそうと思いまして。"},{"id":{"d":405,"n":6,"t":1},"speaker":"B","text":"なるほど。私も一緒に買いに行きますか？それとも食べたいものをお願いした方がいいですか？"},{"id":{"d":405,"n":7,"t":1},"speaker":"A","text":"買いに行ってくるので、食べたいものを教えてください。"},{"id":{"d":405,"n":8,"t":1},"speaker":"B","text":"とりあえずシェイクとポテトが欲しいです。ハンバーガーは決めることができないので、あなたと同じものをお願いします。"}];
INSERT INTO sentence [{"id":{"d":406,"n":1,"t":1},"speaker":"A","text":"アパートとマンションの違いは分かりますか？"},{"id":{"d":406,"n":2,"t":1},"speaker":"B","text":"古いものがアパートで、新しくきれいなものがマンションというイメージがありますが、よく分かりません。"},{"id":{"d":406,"n":3,"t":1},"speaker":"A","text":"私もそのイメージがありました。しかし実際には、この2つに違いはありません。"},{"id":{"d":406,"n":4,"t":1},"speaker":"B","text":"違いがないのですか。それではなぜ名前が異なるのですか？"},{"id":{"d":406,"n":5,"t":1},"speaker":"A","text":"貸す側が決めているもののようです。"},{"id":{"d":406,"n":6,"t":1},"speaker":"B","text":"それではさまざまな違いの基準があるということですか？"},{"id":{"d":406,"n":7,"t":1},"speaker":"A","text":"その通りです。例えばあるところでは鉄筋造りであればマンション、木造はアパートという違いがあるようです。"},{"id":{"d":406,"n":8,"t":1},"speaker":"B","text":"よく分かりませんね。それではきれいなアパートも汚いマンションも存在するということですね？"},{"id":{"d":406,"n":9,"t":1},"speaker":"A","text":"その通りです。難しいですね。"}];
INSERT INTO sentence [{"id":{"d":407,"n":1,"t":1},"speaker":"A","text":"料理のレシピの適量とは、どの程度の量なのか分かりません。"},{"id":{"d":407,"n":2,"t":1},"speaker":"B","text":"適量の他にも少々など分からないものが多いですね。"},{"id":{"d":407,"n":3,"t":1},"speaker":"A","text":"少々は親指と人差し指でつまんで取ることができる程度の量のことです。"},{"id":{"d":407,"n":4,"t":1},"speaker":"B","text":"少々とは既に量が決まっていたものなのですね。それでは、適量とはどの程度なのでしょうか？"},{"id":{"d":407,"n":5,"t":1},"speaker":"A","text":"検索してみると、適量とはおいしい味付けになる量のことだと書いてありました。"},{"id":{"d":407,"n":6,"t":1},"speaker":"B","text":"それではレシピに適量と書いてあっても、味がそれぞれの人で変わるということですね。"}];
INSERT INTO sentence [{"id":{"d":408,"n":1,"t":1},"speaker":"A","text":"てんぷらを食べに行きませんか？"},{"id":{"d":408,"n":2,"t":1},"speaker":"B","text":"いいですね。久しぶりに行きたいです。"},{"id":{"d":408,"n":3,"t":1},"speaker":"A","text":"前回行ったところでよいですか？それとも、他の店を見つけに行きますか？"},{"id":{"d":408,"n":4,"t":1},"speaker":"B","text":"前回行ったところが比較的安くておいしかったので、もう一度行きたいです。"},{"id":{"d":408,"n":5,"t":1},"speaker":"A","text":"今日は休日なので混んでいるかもしれませんね。予約して行きましょう。"},{"id":{"d":408,"n":6,"t":1},"speaker":"B","text":"あのてんぷら屋さんは市内に数店舗構えていますが、今日はどこへ行きますか？"},{"id":{"d":408,"n":7,"t":1},"speaker":"A","text":"近くにできた新しい店舗に行きましょう。予約は任せて良いですか？"},{"id":{"d":408,"n":8,"t":1},"speaker":"B","text":"分かりました。予約しておきます。"}];
INSERT INTO sentence [{"id":{"d":409,"n":1,"t":1},"speaker":"A","text":"お電話ありがとうございます。キャンペーン担当です。"},{"id":{"d":409,"n":2,"t":1},"speaker":"B","text":"購入した金額に対応したクーポン券を頂けると聞いたのですが、どのように応募したらよろしいですか？"},{"id":{"d":409,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。ご購入いただいた商品はどちらですか？"},{"id":{"d":409,"n":4,"t":1},"speaker":"B","text":"私は飲料水を購入しました。お茶とスポーツドリンクです。"},{"id":{"d":409,"n":5,"t":1},"speaker":"A","text":"それぞれのキャンペーンに、ご応募ということでよろしいですか？"},{"id":{"d":409,"n":6,"t":1},"speaker":"B","text":"両方とも応募することができるのですか？"},{"id":{"d":409,"n":7,"t":1},"speaker":"A","text":"お茶はお茶で、スポーツドリンクはスポーツドリンクで応募金額に達していた場合、可能です。"},{"id":{"d":409,"n":8,"t":1},"speaker":"B","text":"それでは両方に応募したいです。よろしくお願いします。"},{"id":{"d":409,"n":9,"t":1},"speaker":"A","text":"かしこまりました。それでは、処理をいたしますので少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":410,"n":1,"t":1},"speaker":"A","text":"おいしそうなお弁当ですね。"},{"id":{"d":410,"n":2,"t":1},"speaker":"B","text":"これは節約のために、自分で作ってきたものなのです。"},{"id":{"d":410,"n":3,"t":1},"speaker":"A","text":"私には、節約しているとは思えない豪華なお弁当に見えます。"},{"id":{"d":410,"n":4,"t":1},"speaker":"B","text":"昨日の夕ご飯の残り物をお弁当箱に詰めてきただけです。"},{"id":{"d":410,"n":5,"t":1},"speaker":"A","text":"唐揚げにポテトサラダに煮物も入っているのですね。豪華な夕ご飯が目に浮かびます。"},{"id":{"d":410,"n":6,"t":1},"speaker":"B","text":"このおかずは、それほど豪華ではありませんよ。"},{"id":{"d":410,"n":7,"t":1},"speaker":"A","text":"いいえ、私の夕ご飯に比べれば、豪華です。"},{"id":{"d":410,"n":8,"t":1},"speaker":"B","text":"どのような夕ご飯だったのですか？"}];
INSERT INTO sentence [{"id":{"d":411,"n":1,"t":1},"speaker":"A","text":"郵便局で切手を買いたいのですが、封書に貼る切手代はいくらですか？"},{"id":{"d":411,"n":2,"t":1},"speaker":"B","text":"封書の大きさなどで違いますから、郵便局で聞いてみたらいかがですか？"},{"id":{"d":411,"n":3,"t":1},"speaker":"A","text":"そうですね、聞いてみます。一番近い郵便局を教えていただけますか？"},{"id":{"d":411,"n":4,"t":1},"speaker":"B","text":"この先の商店街の端に、小さな郵便局があります。"},{"id":{"d":411,"n":5,"t":1},"speaker":"A","text":"ありがとうございます、これから行ってみます。"},{"id":{"d":411,"n":6,"t":1},"speaker":"B","text":"もうすぐ郵便局が閉まる時間なので、急いだ方がいいですよ。"},{"id":{"d":411,"n":7,"t":1},"speaker":"A","text":"閉まるまで、あとどれくらいですか？"},{"id":{"d":411,"n":8,"t":1},"speaker":"B","text":"30分くらいです、急いで行ってください。"},{"id":{"d":411,"n":9,"t":1},"speaker":"A","text":"分かりました。どうもありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":412,"n":1,"t":1},"speaker":"A","text":"お豆腐を2丁ください。"},{"id":{"d":412,"n":2,"t":1},"speaker":"B","text":"絹ごし豆腐と木綿豆腐のどちらがよろしいですか？"},{"id":{"d":412,"n":3,"t":1},"speaker":"A","text":"鍋料理に入れるなら、どちらがお薦めですか？"},{"id":{"d":412,"n":4,"t":1},"speaker":"B","text":"湯豆腐なら絹ごし豆腐が、他の鍋料理なら木綿豆腐がお薦めですよ。"},{"id":{"d":412,"n":5,"t":1},"speaker":"A","text":"寄せ鍋にしようと思っているので、木綿豆腐にします。"},{"id":{"d":412,"n":6,"t":1},"speaker":"B","text":"木綿豆腐を2丁ですね。"},{"id":{"d":412,"n":7,"t":1},"speaker":"A","text":"はい。おいくらですか？"},{"id":{"d":412,"n":8,"t":1},"speaker":"B","text":"300円です。消費税も込みですよ。"}];
INSERT INTO sentence [{"id":{"d":413,"n":1,"t":1},"speaker":"A","text":"今日は何のごみの日ですか？"},{"id":{"d":413,"n":2,"t":1},"speaker":"B","text":"今日は水曜日なので資源ごみの日ですよ。"},{"id":{"d":413,"n":3,"t":1},"speaker":"A","text":"使用済み乾電池は、資源ごみでしょうか？"},{"id":{"d":413,"n":4,"t":1},"speaker":"B","text":"乾電池は危険物の日に出すのです。"},{"id":{"d":413,"n":5,"t":1},"speaker":"A","text":"危険物の日などありましたか？"},{"id":{"d":413,"n":6,"t":1},"speaker":"B","text":"月に一度だけだから、忘れがちなのですね。険物の日は第1木曜日ですよ。"}];
INSERT INTO sentence [{"id":{"d":414,"n":1,"t":1},"speaker":"A","text":"こんにちは。処方箋はこちらに入れてください。"},{"id":{"d":414,"n":2,"t":1},"speaker":"B","text":"はい、ここですね。"},{"id":{"d":414,"n":3,"t":1},"speaker":"A","text":"お薬手帳はお持ちでしょうか？"},{"id":{"d":414,"n":4,"t":1},"speaker":"B","text":"はい、これですね。"},{"id":{"d":414,"n":5,"t":1},"speaker":"A","text":"お薬のご用意ができました。手帳に貼ったこちらが今回のお薬の説明書です。"},{"id":{"d":414,"n":6,"t":1},"speaker":"B","text":"朝昼晩1錠ずつ服用ですね。"},{"id":{"d":414,"n":7,"t":1},"speaker":"A","text":"お薬手帳は、他の病院にかかるときは、必ず持って行ってくださいね。"},{"id":{"d":414,"n":8,"t":1},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":415,"n":1,"t":1},"speaker":"A","text":"おいしい紅茶を頂いたので、ご一緒にいかがですか？"},{"id":{"d":415,"n":2,"t":1},"speaker":"B","text":"いいですね、ごちそうになります。"},{"id":{"d":415,"n":3,"t":1},"speaker":"A","text":"紅茶の入れ方はそれほど上手ではないのですが。"},{"id":{"d":415,"n":4,"t":1},"speaker":"B","text":"前にいただいた時はおいしかったですよ。"},{"id":{"d":415,"n":5,"t":1},"speaker":"A","text":"それはよかったです。紅茶と一緒にミルクやレモンはいかがですか？"},{"id":{"d":415,"n":6,"t":1},"speaker":"B","text":"ではミルクをお願いします。"},{"id":{"d":415,"n":7,"t":1},"speaker":"A","text":"分かりました。ミルクを用意しますね。"},{"id":{"d":415,"n":8,"t":1},"speaker":"B","text":"では、私はお茶菓子を用意します。"}];
INSERT INTO sentence [{"id":{"d":416,"n":1,"t":1},"speaker":"A","text":"お風呂、お先にいただきました。とても気持ち良かったです。"},{"id":{"d":416,"n":2,"t":1},"speaker":"B","text":"お風呂上がりに冷たいお水をどうぞ。"},{"id":{"d":416,"n":3,"t":1},"speaker":"A","text":"ありがとうございます、おいしいです。"},{"id":{"d":416,"n":4,"t":1},"speaker":"B","text":"湯冷めをしないように、気を付けてくださいね。"},{"id":{"d":416,"n":5,"t":1},"speaker":"A","text":"お気遣いありがとうございます。そういえば、お風呂場の鏡がよく見えませんでしたね。"},{"id":{"d":416,"n":6,"t":1},"speaker":"B","text":"そこは掃除をしても汚れが取れなくて、困っているのです。"},{"id":{"d":416,"n":7,"t":1},"speaker":"A","text":"良い洗剤が、ドラッグストアに売っていますよ。"},{"id":{"d":416,"n":8,"t":1},"speaker":"B","text":"メモをするので商品名を教えてください。"}];
INSERT INTO sentence [{"id":{"d":417,"n":1,"t":1},"speaker":"A","text":"ボタンが、取れそうですよ。"},{"id":{"d":417,"n":2,"t":1},"speaker":"B","text":"本当ですね、ありがとうございます。"},{"id":{"d":417,"n":3,"t":1},"speaker":"A","text":"ソーイングセットは、持っていますか？"},{"id":{"d":417,"n":4,"t":1},"speaker":"B","text":"ソーイングセットは、持ち歩いていません。"},{"id":{"d":417,"n":5,"t":1},"speaker":"A","text":"では、これを使ってはどうでしょうか？"},{"id":{"d":417,"n":6,"t":1},"speaker":"B","text":"透明なマニュキュアですか？"},{"id":{"d":417,"n":7,"t":1},"speaker":"A","text":"ほつれかけた糸を、固められると思いますよ。"},{"id":{"d":417,"n":8,"t":1},"speaker":"B","text":"素晴らしいです、マニキュアをお借りします。"}];
INSERT INTO sentence [{"id":{"d":418,"n":1,"t":1},"speaker":"A","text":"寒くなってきたのでコートを出したら、穴が開いていました。"},{"id":{"d":418,"n":2,"t":1},"speaker":"B","text":"虫に食われたのですか？"},{"id":{"d":418,"n":3,"t":1},"speaker":"A","text":"恐らくそうだと思います。"},{"id":{"d":418,"n":4,"t":1},"speaker":"B","text":"防虫剤を入れていましたか？"},{"id":{"d":418,"n":5,"t":1},"speaker":"A","text":"1年もつ防虫剤を入れていたはずなのに、半年の商品だったようです。"},{"id":{"d":418,"n":6,"t":1},"speaker":"B","text":"気を付けて買わないと、そういうこともあるのですね。"},{"id":{"d":418,"n":7,"t":1},"speaker":"A","text":"良いコートだったのに、悲しいです。"},{"id":{"d":418,"n":8,"t":1},"speaker":"B","text":"かけつぎをしてくれるお店を、探すと良いと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":419,"n":1,"t":1},"speaker":"A","text":"昼食はカップラーメンです。"},{"id":{"d":419,"n":2,"t":1},"speaker":"B","text":"簡単で良いですね。"},{"id":{"d":419,"n":3,"t":1},"speaker":"A","text":"スープの素を入れて、お湯を注ぐだけですからね。"},{"id":{"d":419,"n":4,"t":1},"speaker":"B","text":"そろそろ、3分たつのではありませんか？"},{"id":{"d":419,"n":5,"t":1},"speaker":"A","text":"本当ですね、ではいただきます。"},{"id":{"d":419,"n":6,"t":1},"speaker":"B","text":"私も、今日のお昼はカップラーメンにしようかしら。"},{"id":{"d":419,"n":7,"t":1},"speaker":"A","text":"今日は寒いから、体が暖まって良いですよ。"},{"id":{"d":419,"n":8,"t":1},"speaker":"B","text":"今から買いに行ってきます。"}];
INSERT INTO sentence [{"id":{"d":420,"n":1,"t":1},"speaker":"A","text":"あんパンを買ってきました。"},{"id":{"d":420,"n":2,"t":1},"speaker":"B","text":"たくさん買ってきたのですね。"},{"id":{"d":420,"n":3,"t":1},"speaker":"A","text":"みんなで食べようと思って、買ってきました。"},{"id":{"d":420,"n":4,"t":1},"speaker":"B","text":"1つ頂いても良いのですか？"},{"id":{"d":420,"n":5,"t":1},"speaker":"A","text":"もちろんです。こしあんと粒あんがありますが、どちらが良いですか？"},{"id":{"d":420,"n":6,"t":1},"speaker":"B","text":"私はこしあんの方が好みです。"},{"id":{"d":420,"n":7,"t":1},"speaker":"A","text":"では、こちらのゴマが付いているものをどうぞ。"},{"id":{"d":420,"n":8,"t":1},"speaker":"B","text":"ありがとうございます、頂きます。"}];
INSERT INTO sentence [{"id":{"d":421,"n":1,"t":1},"speaker":"A","text":"ベランダの植木に水やりをしてください。"},{"id":{"d":421,"n":2,"t":1},"speaker":"B","text":"じょうろで全体に水をかければよいのですか？"},{"id":{"d":421,"n":3,"t":1},"speaker":"A","text":"はい。全体的に乾いているようなので、そうしてください。"},{"id":{"d":421,"n":4,"t":1},"speaker":"B","text":"家の中の鉢はどうしますか？"},{"id":{"d":421,"n":5,"t":1},"speaker":"A","text":"家の中にある鉢は、まだ水やりをしなくても大丈夫です。"},{"id":{"d":421,"n":6,"t":1},"speaker":"B","text":"前に水のやりすぎで、枯らしたことがありましたよね。"},{"id":{"d":421,"n":7,"t":1},"speaker":"A","text":"最近は気を付けているので、元気が良いでしょう？"},{"id":{"d":421,"n":8,"t":1},"speaker":"B","text":"葉っぱが、つやつやしていますね。"}];
INSERT INTO sentence [{"id":{"d":422,"n":1,"t":1},"speaker":"A","text":"寒くなると、おでんがおいしいですね。"},{"id":{"d":422,"n":2,"t":1},"speaker":"B","text":"そこのコンビニエンスストアで買っていきませんか？"},{"id":{"d":422,"n":3,"t":1},"speaker":"A","text":"いいですね。"},{"id":{"d":422,"n":4,"t":1},"speaker":"B","text":"ダイコンと卵とさつまあげと糸こんとちくわぶをください。"},{"id":{"d":422,"n":5,"t":1},"speaker":"A","text":"ちくわぶって何ですか？"},{"id":{"d":422,"n":6,"t":1},"speaker":"B","text":"もしかして、あなたは関西出身ですか？"},{"id":{"d":422,"n":7,"t":1},"speaker":"A","text":"そうです、関東では定番なのですか？"},{"id":{"d":422,"n":8,"t":1},"speaker":"B","text":"こちらでは定番の具の一つです。もちもちしていておいしいですよ。ぜひ食べてみてください。"}];
INSERT INTO sentence [{"id":{"d":423,"n":1,"t":1},"speaker":"A","text":"以前、東京でおでんを食べたときに、ちくわぶをお薦めされたので食べてみました。"},{"id":{"d":423,"n":2,"t":1},"speaker":"B","text":"どうでしたか？"},{"id":{"d":423,"n":3,"t":1},"speaker":"A","text":"もちもち、ぐにゅぐにゅしていて、何ともいえない食感でした。味はおいしかったです。"},{"id":{"d":423,"n":4,"t":1},"speaker":"B","text":"独特な食感ですよね。あまりお口にあわなかったですか？"},{"id":{"d":423,"n":5,"t":1},"speaker":"A","text":"そうでもありませんが、私はおでんといえばやはり牛すじが好みです。幼い頃から食べ慣れていますから。"},{"id":{"d":423,"n":6,"t":1},"speaker":"B","text":"ふるさとの味が一番なのですね。"}];
INSERT INTO sentence [{"id":{"d":424,"n":1,"t":1},"speaker":"A","text":"そのブーツは、温かそうですね。デザインもすてきです。"},{"id":{"d":424,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。ええ、とても温かいですよ。"},{"id":{"d":424,"n":3,"t":1},"speaker":"A","text":"先日の雪の日にそのようなブーツがあったら良かったです。"},{"id":{"d":424,"n":4,"t":1},"speaker":"B","text":"実は、ちょうどその雪の日に買ったのですよ。"},{"id":{"d":424,"n":5,"t":1},"speaker":"A","text":"ちょうど良いブーツがよく売っていましたね。"},{"id":{"d":424,"n":6,"t":1},"speaker":"B","text":"たまたま入ったお店にあったのです。"}];
INSERT INTO sentence [{"id":{"d":425,"n":1,"t":1},"speaker":"A","text":"回転ずしを食べに行きませんか？"},{"id":{"d":425,"n":2,"t":1},"speaker":"B","text":"ごめんなさい、私は生ものが苦手なので遠慮します。"},{"id":{"d":425,"n":3,"t":1},"speaker":"A","text":"最近の回転ずしは生ものじゃないものもありますよ。"},{"id":{"d":425,"n":4,"t":1},"speaker":"B","text":"そうなのですか？"},{"id":{"d":425,"n":5,"t":1},"speaker":"A","text":"うどんやラーメン、天丼やカレーさえあります。"},{"id":{"d":425,"n":6,"t":1},"speaker":"B","text":"それは本当におすし屋さんなのですか？"},{"id":{"d":425,"n":7,"t":1},"speaker":"A","text":"きちんとおすしは回っています。"},{"id":{"d":425,"n":8,"t":1},"speaker":"B","text":"面白そうなので一緒に行きます。"}];
INSERT INTO sentence [{"id":{"d":426,"n":1,"t":1},"speaker":"A","text":"先日、回転ずし屋さんに行ったのですが、おすしだけでなくカレーなどもありました。"},{"id":{"d":426,"n":2,"t":1},"speaker":"B","text":"最近はそういうお店が多いですよね。カレーを食べましたか？"},{"id":{"d":426,"n":3,"t":1},"speaker":"A","text":"はい。予想以上に本格的で、とてもおいしかったです。デザートやフライドポテトも食べましたが、どれもおいしかったです。"},{"id":{"d":426,"n":4,"t":1},"speaker":"B","text":"おいしくて安いのが、回転ずしの良いところですよね。"},{"id":{"d":426,"n":5,"t":1},"speaker":"A","text":"本当に安くて、びっくりしました。ぜひ、また行きたいです。"},{"id":{"d":426,"n":6,"t":1},"speaker":"B","text":"カレーが美味しくて有名な回転ずし屋を知っているので、今度ぜひ一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":427,"n":1,"t":1},"speaker":"A","text":"最近、ガソリンが高いですね。"},{"id":{"d":427,"n":2,"t":1},"speaker":"B","text":"そうですね。私は安いところを探して入れに行きます。"},{"id":{"d":427,"n":3,"t":1},"speaker":"A","text":"安いところを車で走りながら探すのですか？"},{"id":{"d":427,"n":4,"t":1},"speaker":"B","text":"毎日通る道のスタンドのガソリン価格をネットでチェックしています。"},{"id":{"d":427,"n":5,"t":1},"speaker":"A","text":"走りながら、価格を調べることは大変ですものね。"},{"id":{"d":427,"n":6,"t":1},"speaker":"B","text":"戻ったりする羽目になったら、ガソリンの無駄遣いですからね。"},{"id":{"d":427,"n":7,"t":1},"speaker":"A","text":"ネットで調べられるのは、便利ですね。"},{"id":{"d":427,"n":8,"t":1},"speaker":"B","text":"はい、便利な世の中になりました。"}];
INSERT INTO sentence [{"id":{"d":428,"n":1,"t":1},"speaker":"A","text":"今朝、寝坊してしまいました。"},{"id":{"d":428,"n":2,"t":1},"speaker":"B","text":"学校に遅刻したのですか？"},{"id":{"d":428,"n":3,"t":1},"speaker":"A","text":"学校は3限からだったので大丈夫でした。"},{"id":{"d":428,"n":4,"t":1},"speaker":"B","text":"それではゆっくり寝ていても問題ないでしょう？"},{"id":{"d":428,"n":5,"t":1},"speaker":"A","text":"今日はごみの収集日だったのです。"},{"id":{"d":428,"n":6,"t":1},"speaker":"B","text":"清掃車が来る時間に、間に合わなかったのですか？"},{"id":{"d":428,"n":7,"t":1},"speaker":"A","text":"生ごみを次のごみの収集日まで出せません。"},{"id":{"d":428,"n":8,"t":1},"speaker":"B","text":"次のごみの日は、寝坊しないようにしてください。"}];
INSERT INTO sentence [{"id":{"d":429,"n":1,"t":1},"speaker":"A","text":"卵料理は、何が好きですか？"},{"id":{"d":429,"n":2,"t":1},"speaker":"B","text":"私は、オムレツが好きです。"},{"id":{"d":429,"n":3,"t":1},"speaker":"A","text":"オムレツは、自分で上手に作れますか？"},{"id":{"d":429,"n":4,"t":1},"speaker":"B","text":"自分ではうまくできないので、レストランで食べます。"},{"id":{"d":429,"n":5,"t":1},"speaker":"A","text":"私はだし巻き卵が好きなのです。"},{"id":{"d":429,"n":6,"t":1},"speaker":"B","text":"だし巻き卵も、お店の方がおいしいですね。"},{"id":{"d":429,"n":7,"t":1},"speaker":"A","text":"私もうまく作れないので、買いに行きます。"},{"id":{"d":429,"n":8,"t":1},"speaker":"B","text":"卵料理は、簡単なようで難しいですね。"}];
INSERT INTO sentence [{"id":{"d":430,"n":1,"t":1},"speaker":"A","text":"もうそろそろ、夏物を出さなくてはいけませんね。"},{"id":{"d":430,"n":2,"t":1},"speaker":"B","text":"暑くなってきましたからね。"},{"id":{"d":430,"n":3,"t":1},"speaker":"A","text":"去年、バーゲンで買って一度も着ていないブラウスがあります。"},{"id":{"d":430,"n":4,"t":1},"speaker":"B","text":"やっと、着られますね。"},{"id":{"d":430,"n":5,"t":1},"speaker":"A","text":"合わせるパンツかスカートがありません。"},{"id":{"d":430,"n":6,"t":1},"speaker":"B","text":"今、持っている服には合わないのですか？"},{"id":{"d":430,"n":7,"t":1},"speaker":"A","text":"合わないような気がします。"},{"id":{"d":430,"n":8,"t":1},"speaker":"B","text":"では、一緒にパンツかスカートを買いに行きませんか？"}];
INSERT INTO sentence [{"id":{"d":431,"n":1,"t":1},"speaker":"A","text":"コンビニエンスストアで、新商品のスイーツを買ってきました。"},{"id":{"d":431,"n":2,"t":1},"speaker":"B","text":"これは、テレビで紹介していたチーズケーキですね？"},{"id":{"d":431,"n":3,"t":1},"speaker":"A","text":"おいしそうでしょう？"},{"id":{"d":431,"n":4,"t":1},"speaker":"B","text":"はい。私も買ってこようかしら。まだありましたか？"},{"id":{"d":431,"n":5,"t":1},"speaker":"A","text":"そう言うと思って、もう1つ買ってきました。どうぞ。"},{"id":{"d":431,"n":6,"t":1},"speaker":"B","text":"素晴らしい洞察力です。ありがとうございます、おいくらでしたか？"},{"id":{"d":431,"n":7,"t":1},"speaker":"A","text":"1つ230円でした。"},{"id":{"d":431,"n":8,"t":1},"speaker":"B","text":"はい、230円お支払いします。"}];
INSERT INTO sentence [{"id":{"d":432,"n":1,"t":1},"speaker":"A","text":"そのセーター、かわいいですね。"},{"id":{"d":432,"n":2,"t":1},"speaker":"B","text":"ありがとう、これはお母さんが編んでくれたのです。"},{"id":{"d":432,"n":3,"t":1},"speaker":"A","text":"編み物が上手なのですね。"},{"id":{"d":432,"n":4,"t":1},"speaker":"B","text":"はい。私も上手になりたくて、お母さんに教えてもらっています。"},{"id":{"d":432,"n":5,"t":1},"speaker":"A","text":"すてきですね。"},{"id":{"d":432,"n":6,"t":1},"speaker":"B","text":"でもまだまだ下手なので、これからも練習します。"},{"id":{"d":432,"n":7,"t":1},"speaker":"A","text":"私も編み物に興味があるので、もしよければぜひ教えていただきたいです。"},{"id":{"d":432,"n":8,"t":1},"speaker":"B","text":"ぜひ一緒にやりましょう。お母さんに頼んでみますね。"}];
INSERT INTO sentence [{"id":{"d":433,"n":1,"t":1},"speaker":"A","text":"タマネギを切ったら涙が出ました。"},{"id":{"d":433,"n":2,"t":1},"speaker":"B","text":"私もいつも泣いてしまいます。"},{"id":{"d":433,"n":3,"t":1},"speaker":"A","text":"涙が出なくなる方法はないのでしょうか？"},{"id":{"d":433,"n":4,"t":1},"speaker":"B","text":"おばあちゃんに聞いたら、タマネギを口にくわえるといいと言っていました。"},{"id":{"d":433,"n":5,"t":1},"speaker":"A","text":"やってみたのですか？"},{"id":{"d":433,"n":6,"t":1},"speaker":"B","text":"いえ、生のタマネギを口に入れることに抵抗があってできていません。"},{"id":{"d":433,"n":7,"t":1},"speaker":"A","text":"では私が今度やってみます。"},{"id":{"d":433,"n":8,"t":1},"speaker":"B","text":"結果を聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":434,"n":1,"t":1},"speaker":"A","text":"タンスの中に、もう着られない服が入っていました。"},{"id":{"d":434,"n":2,"t":1},"speaker":"B","text":"服が悪くなってしまったのですか？"},{"id":{"d":434,"n":3,"t":1},"speaker":"A","text":"いいえ、趣味に合わない服になってしまったのです。最近は落ち着いたものが好みです。"},{"id":{"d":434,"n":4,"t":1},"speaker":"B","text":"すてきなブラウスではありませんか？"},{"id":{"d":434,"n":5,"t":1},"speaker":"A","text":"襟のレースが、少々派手過ぎではありませんか？"},{"id":{"d":434,"n":6,"t":1},"speaker":"B","text":"そのようなことはないと思います。"},{"id":{"d":434,"n":7,"t":1},"speaker":"A","text":"色も派手過ぎると思います。"},{"id":{"d":434,"n":8,"t":1},"speaker":"B","text":"コーディネート次第で、十分シックに着られますよ。"}];
INSERT INTO sentence [{"id":{"d":435,"n":1,"t":1},"speaker":"A","text":"本棚の片付けをしていたら、このような本がありました。"},{"id":{"d":435,"n":2,"t":1},"speaker":"B","text":"古い本ですね。"},{"id":{"d":435,"n":3,"t":1},"speaker":"A","text":"パパの本ですか？"},{"id":{"d":435,"n":4,"t":1},"speaker":"B","text":"そうだと思います。"},{"id":{"d":435,"n":5,"t":1},"speaker":"A","text":"推理小説のようで最後が袋状になっています。"},{"id":{"d":435,"n":6,"t":1},"speaker":"B","text":"最後に書かれている犯人を見せないようにする本ですね。"},{"id":{"d":435,"n":7,"t":1},"speaker":"A","text":"あれ、最後のページに何か入っています。"},{"id":{"d":435,"n":8,"t":1},"speaker":"B","text":"これは、パパのへそくりですね。"}];
INSERT INTO sentence [{"id":{"d":436,"n":1,"t":1},"speaker":"A","text":"この通りの裏に、メロンパン専門店ができたのを知っていますか？"},{"id":{"d":436,"n":2,"t":1},"speaker":"B","text":"新聞に広告が入っていましたが、どこだろうと思っていました。"},{"id":{"d":436,"n":3,"t":1},"speaker":"A","text":"このすぐ裏手なのです。"},{"id":{"d":436,"n":4,"t":1},"speaker":"B","text":"近いですね。メロンパンを買っていきますか？"},{"id":{"d":436,"n":5,"t":1},"speaker":"A","text":"はい、買いに行きましょう！"},{"id":{"d":436,"n":6,"t":1},"speaker":"B","text":"あそこお店でしょうか？長い行列ができていますね。"},{"id":{"d":436,"n":7,"t":1},"speaker":"A","text":"そうですね。行列に並びますか？"},{"id":{"d":436,"n":8,"t":1},"speaker":"B","text":"午後からの会議に間に合わなくなりそうなので、今日はやめておきましょう。"}];
INSERT INTO sentence [{"id":{"d":437,"n":1,"t":1},"speaker":"A","text":"年季の入った、変わった絵柄のTシャツを着ていますね。"},{"id":{"d":437,"n":2,"t":1},"speaker":"B","text":"これは、アダチンのTシャツなのです。"},{"id":{"d":437,"n":3,"t":1},"speaker":"A","text":"アダチン？"},{"id":{"d":437,"n":4,"t":1},"speaker":"B","text":"足立区の文化芸術を振興する公式キャラクターだったのに、新しい区長に嫌われて捨てられたのです。"},{"id":{"d":437,"n":5,"t":1},"speaker":"A","text":"ああ、昔、テレビのニュース番組で聞いたことがあった気がします。"},{"id":{"d":437,"n":6,"t":1},"speaker":"B","text":"かわいそうに思ったので、何年か前にTシャツを買ってあげました。"},{"id":{"d":437,"n":7,"t":1},"speaker":"A","text":"それからずっと着ているのですか？"},{"id":{"d":437,"n":8,"t":1},"speaker":"B","text":"はい、もう何年も着ています。"}];
INSERT INTO sentence [{"id":{"d":438,"n":1,"t":1},"speaker":"A","text":"小麦粉が値上がりするそうです。"},{"id":{"d":438,"n":2,"t":1},"speaker":"B","text":"小麦粉もですか。バターもサラダ油も値上がりと聞きました。"},{"id":{"d":438,"n":3,"t":1},"speaker":"A","text":"小麦粉は、高いからといって食卓から外すこともできませんし、困りました。"},{"id":{"d":438,"n":4,"t":1},"speaker":"B","text":"本当ですね。"},{"id":{"d":438,"n":5,"t":1},"speaker":"A","text":"趣味のお菓子作りを少し減らさなくてはいけないでしょうか？"},{"id":{"d":438,"n":6,"t":1},"speaker":"B","text":"普段の食事作りではなくて、趣味のお菓子作りでしたら、減らした方がいいかもしれませんね。"},{"id":{"d":438,"n":7,"t":1},"speaker":"A","text":"クッキーもパウンドケーキも大好きなのに悲しすぎます。"},{"id":{"d":438,"n":8,"t":1},"speaker":"B","text":"では、他の支出を控えて、お菓子に使ったらいかがですか？"}];
INSERT INTO sentence [{"id":{"d":439,"n":1,"t":1},"speaker":"A","text":"今日は長靴を履いてきて失敗しました。"},{"id":{"d":439,"n":2,"t":1},"speaker":"B","text":"朝、天気予報を見てこなかったのですか？お天気キャスターは、8時には晴れると言っていましたよ。"},{"id":{"d":439,"n":3,"t":1},"speaker":"A","text":"テレビは見ませんでした。家を出るとき、空は雨雲で真っ暗だったので、今日は雨だと思ってしまいました。"},{"id":{"d":439,"n":4,"t":1},"speaker":"B","text":"それは仕方ありませんね。長靴は昇降口の靴箱に入りづらくて大変でしょう。"},{"id":{"d":439,"n":5,"t":1},"speaker":"A","text":"はい、折り畳んで入れてきました。"},{"id":{"d":439,"n":6,"t":1},"speaker":"B","text":"長靴も靴箱も壊れないといいですね。"}];
INSERT INTO sentence [{"id":{"d":440,"n":1,"t":1},"speaker":"A","text":"もしもし、ピザの配達をお願いします。"},{"id":{"d":440,"n":2,"t":1},"speaker":"B","text":"はい、お名前と電話番号をお願いします。"},{"id":{"d":440,"n":3,"t":1},"speaker":"A","text":"03-0123-4567の田中です。"},{"id":{"d":440,"n":4,"t":1},"speaker":"B","text":"ご注文をお伺いします。"},{"id":{"d":440,"n":5,"t":1},"speaker":"A","text":"マルゲリータと照り焼きチキンをLサイズ、ハーフアンドハーフでお願いします。"},{"id":{"d":440,"n":6,"t":1},"speaker":"B","text":"Lサイズのハーフアンドハーフでマルゲリータと照り焼きチキンでよろしいですか？"},{"id":{"d":440,"n":7,"t":1},"speaker":"A","text":"はい。"},{"id":{"d":440,"n":8,"t":1},"speaker":"B","text":"ご注文ありがとうございます。30分後にお持ちします。"}];
INSERT INTO sentence [{"id":{"d":441,"n":1,"t":1},"speaker":"A","text":"今日の昼食はどこへ食べに行きますか？"},{"id":{"d":441,"n":2,"t":1},"speaker":"B","text":"先ほど出前館でカレーを頼みました。"},{"id":{"d":441,"n":3,"t":1},"speaker":"A","text":"では、私は1人で食べに行きますね。"},{"id":{"d":441,"n":4,"t":1},"speaker":"B","text":"待ってください。2人前頼んであります。"},{"id":{"d":441,"n":5,"t":1},"speaker":"A","text":"もしかして、私の分ですか？"},{"id":{"d":441,"n":6,"t":1},"speaker":"B","text":"今日はあなたの誕生日でしょう？少しばかりですが、私のおごりです。"},{"id":{"d":441,"n":7,"t":1},"speaker":"A","text":"ありがとうございます！うれしくて涙が出てきました。"},{"id":{"d":441,"n":8,"t":1},"speaker":"B","text":"大サービスでトッピング全部乗せですよ。"}];
INSERT INTO sentence [{"id":{"d":442,"n":1,"t":1},"speaker":"A","text":"時報を電話で聞こうとして、天気予報電話サービスにかけてしまいました。"},{"id":{"d":442,"n":2,"t":1},"speaker":"B","text":"117と177で紛らわしいですからね。"},{"id":{"d":442,"n":3,"t":1},"speaker":"A","text":"間違えずに覚える方法はないのでしょうか？"},{"id":{"d":442,"n":4,"t":1},"speaker":"B","text":"めったに使わない番号ですから、その都度調べればよいと思います。"},{"id":{"d":442,"n":5,"t":1},"speaker":"A","text":"そうですよね。"},{"id":{"d":442,"n":6,"t":1},"speaker":"B","text":"どうして時報を聞こうと思ったのですか？"},{"id":{"d":442,"n":7,"t":1},"speaker":"A","text":"時計の時刻を合わせようと思ったのです。"},{"id":{"d":442,"n":8,"t":1},"speaker":"B","text":"次は、スマートフォンかパソコンの時間を見るといいですよ。"}];
INSERT INTO sentence [{"id":{"d":443,"n":1,"t":1},"speaker":"A","text":"おいしそうなケーキが並んでいます。"},{"id":{"d":443,"n":2,"t":1},"speaker":"B","text":"どれにしますか？"},{"id":{"d":443,"n":3,"t":1},"speaker":"A","text":"1人2個ずつ選んでも良いですか？"},{"id":{"d":443,"n":4,"t":1},"speaker":"B","text":"家族5人で2個ずつでは結構な金額になってしまいます。"},{"id":{"d":443,"n":5,"t":1},"speaker":"A","text":"駄目ですか？"},{"id":{"d":443,"n":6,"t":1},"speaker":"B","text":"2個目は、プリンかシュークリームにすればよいですよ。"},{"id":{"d":443,"n":7,"t":1},"speaker":"A","text":"分かりました。では、イチゴのケーキ2つ、チョコレートケーキ2つ、モンブランケーキ1つ、プリン3つ、シュークリーム2つをお願いします。"}];
INSERT INTO sentence [{"id":{"d":444,"n":1,"t":1},"speaker":"A","text":"たこ焼きパーティーをしませんか？"},{"id":{"d":444,"n":2,"t":1},"speaker":"B","text":"たこ焼き機を持っているのですか？"},{"id":{"d":444,"n":3,"t":1},"speaker":"A","text":"大阪にいた時に買ったのです。"},{"id":{"d":444,"n":4,"t":1},"speaker":"B","text":"では、たこ焼き作りも上手なのですね。"},{"id":{"d":444,"n":5,"t":1},"speaker":"A","text":"ええ、大阪の友人に教わりました。"},{"id":{"d":444,"n":6,"t":1},"speaker":"B","text":"私は何を持っていけばよいですか？"},{"id":{"d":444,"n":7,"t":1},"speaker":"A","text":"たこ焼きの材料はありますから、デザートを持ってきてくれますか？"},{"id":{"d":444,"n":8,"t":1},"speaker":"B","text":"了解しました。行きつけのお店でケーキを買っていきますね。"}];
INSERT INTO sentence [{"id":{"d":445,"n":1,"t":1},"speaker":"A","text":"お昼のチャーハンは、おいしかったですね。"},{"id":{"d":445,"n":2,"t":1},"speaker":"B","text":"そうでしょう？"},{"id":{"d":445,"n":3,"t":1},"speaker":"A","text":"腕が上がったのではありませんか？"},{"id":{"d":445,"n":4,"t":1},"speaker":"B","text":"パラパラに炒める技を身に付けました。"},{"id":{"d":445,"n":5,"t":1},"speaker":"A","text":"テレビでやっていたのですか？"},{"id":{"d":445,"n":6,"t":1},"speaker":"B","text":"有名シェフが、コツを教えてくれました。"},{"id":{"d":445,"n":7,"t":1},"speaker":"A","text":"テレビの情報も侮れませんね。"},{"id":{"d":445,"n":8,"t":1},"speaker":"B","text":"来週は唐揚げの予定です。"}];
INSERT INTO sentence [{"id":{"d":446,"n":1,"t":1},"speaker":"A","text":"この缶詰は開けるところがありません。"},{"id":{"d":446,"n":2,"t":1},"speaker":"B","text":"プルタブが付いていない缶詰もあるのですよ。"},{"id":{"d":446,"n":3,"t":1},"speaker":"A","text":"どうやって開けるのですか？"},{"id":{"d":446,"n":4,"t":1},"speaker":"B","text":"缶切りを使うのです。"},{"id":{"d":446,"n":5,"t":1},"speaker":"A","text":"缶切りですか？"},{"id":{"d":446,"n":6,"t":1},"speaker":"B","text":"缶切りを缶のふたに差し込み、ギコギコと動かして開けるのですよ。"},{"id":{"d":446,"n":7,"t":1},"speaker":"A","text":"使い方が難しいです。"},{"id":{"d":446,"n":8,"t":1},"speaker":"B","text":"練習すれば大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":447,"n":1,"t":1},"speaker":"A","text":"着物をリメイクしてドレスにしました。"},{"id":{"d":447,"n":2,"t":1},"speaker":"B","text":"すてきですね。"},{"id":{"d":447,"n":3,"t":1},"speaker":"A","text":"今度、めいっ子の結婚式があるので、着ていこうと思っています。"},{"id":{"d":447,"n":4,"t":1},"speaker":"B","text":"留袖を作り替えたのですね。"},{"id":{"d":447,"n":5,"t":1},"speaker":"A","text":"そうです。着物を着るのも大変ですから。"},{"id":{"d":447,"n":6,"t":1},"speaker":"B","text":"ドレスなら着るのは楽ですね。"},{"id":{"d":447,"n":7,"t":1},"speaker":"A","text":"おなか回りもゆったりしているので、フルコースも完食できます。"},{"id":{"d":447,"n":8,"t":1},"speaker":"B","text":"それが一番の利点ですね。"}];
INSERT INTO sentence [{"id":{"d":448,"n":1,"t":1},"speaker":"A","text":"障子紙を張り替えるのを、手伝ってください。"},{"id":{"d":448,"n":2,"t":1},"speaker":"B","text":"いいですよ。随分ボロボロになりましたね。"},{"id":{"d":448,"n":3,"t":1},"speaker":"A","text":"猫が穴を開けてしまうのです。"},{"id":{"d":448,"n":4,"t":1},"speaker":"B","text":"猫を飼っているのですね。"},{"id":{"d":448,"n":5,"t":1},"speaker":"A","text":"はい。猫に引っかかれても大丈夫なように、今回はプラスチック障子というのを使ってみます。"},{"id":{"d":448,"n":6,"t":1},"speaker":"B","text":"紙ではないので、破れにくいのですね。"},{"id":{"d":448,"n":7,"t":1},"speaker":"A","text":"そのようです。これならきっと大丈夫でしょう。"},{"id":{"d":448,"n":8,"t":1},"speaker":"B","text":"猫の通り道を1か所だけ開けておくのもいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":449,"n":1,"t":1},"speaker":"A","text":"デコポンの缶詰を買ってきました。"},{"id":{"d":449,"n":2,"t":1},"speaker":"B","text":"ミカンの缶詰はよくありますが、デコポンですか？"},{"id":{"d":449,"n":3,"t":1},"speaker":"A","text":"九州の産直フェアをやっていたのです。"},{"id":{"d":449,"n":4,"t":1},"speaker":"B","text":"なるほど。デコポンは熊本が産地ですものね。"},{"id":{"d":449,"n":5,"t":1},"speaker":"A","text":"初めて買ってみたのですが、きっとおいしいですよ。"},{"id":{"d":449,"n":6,"t":1},"speaker":"B","text":"どうしてそう思うのですか？"},{"id":{"d":449,"n":7,"t":1},"speaker":"A","text":"生のデコポンを買うより、高かったですから。"}];
INSERT INTO sentence [{"id":{"d":450,"n":1,"t":1},"speaker":"A","text":"今日は「日清カレーメシ」が安いです。"},{"id":{"d":450,"n":2,"t":1},"speaker":"B","text":"「日清カレーメシ」とは、何ですか？"},{"id":{"d":450,"n":3,"t":1},"speaker":"A","text":"お湯を注いで5分待ち、かき混ぜるだけで、カレーご飯ができるインスタント食品です。"},{"id":{"d":450,"n":4,"t":1},"speaker":"B","text":"知りませんでした。"},{"id":{"d":450,"n":5,"t":1},"speaker":"A","text":"いつもは1個200円もするのに今日は146円です。"},{"id":{"d":450,"n":6,"t":1},"speaker":"B","text":"お買い得ですね。"},{"id":{"d":450,"n":7,"t":1},"speaker":"A","text":"お一人さま6個までです。"},{"id":{"d":450,"n":8,"t":1},"speaker":"B","text":"私も買ってみます。"}];
INSERT INTO sentence [{"id":{"d":451,"n":1,"t":1},"speaker":"A","text":"先日あなたに教えていただいたレトルト食品を作ってみました。"},{"id":{"d":451,"n":2,"t":1},"speaker":"B","text":"おいしかったでしょう？"},{"id":{"d":451,"n":3,"t":1},"speaker":"A","text":"ええ、カレーの味が良かったです。作り方も簡単でした。"},{"id":{"d":451,"n":4,"t":1},"speaker":"B","text":"そうなのですよ、疲れて何も作りたくない時は最高ですよね。"},{"id":{"d":451,"n":5,"t":1},"speaker":"A","text":"2個目はチーズを入れてみました。"},{"id":{"d":451,"n":6,"t":1},"speaker":"B","text":"アレンジレシピも作ったのですか？"},{"id":{"d":451,"n":7,"t":1},"speaker":"A","text":"チーズを入れただけですが、まろやかになっておいしかったです。"},{"id":{"d":451,"n":8,"t":1},"speaker":"B","text":"私も今度やってみます。"}];
INSERT INTO sentence [{"id":{"d":452,"n":1,"t":1},"speaker":"A","text":"最近、小さいサツマイモが多いと思いませんか？"},{"id":{"d":452,"n":2,"t":1},"speaker":"B","text":"海外出荷用に小さくしていると聞いたことがありますよ。"},{"id":{"d":452,"n":3,"t":1},"speaker":"A","text":"海外の人は、小さいサツマイモが好みなのですか？"},{"id":{"d":452,"n":4,"t":1},"speaker":"B","text":"炊飯器でふかすから、小さい方がいいのだそうです。"},{"id":{"d":452,"n":5,"t":1},"speaker":"A","text":"炊飯器でふかすのですか？"},{"id":{"d":452,"n":6,"t":1},"speaker":"B","text":"炊飯器におイモと水を入れてスイッチを入れるだけで、立派なふかしイモができるらしいですよ。"},{"id":{"d":452,"n":7,"t":1},"speaker":"A","text":"簡単ですね。"},{"id":{"d":452,"n":8,"t":1},"speaker":"B","text":"ええ。だから、炊飯器に入る大きさのおイモが人気のようです。"}];
INSERT INTO sentence [{"id":{"d":453,"n":1,"t":1},"speaker":"A","text":"カーディガンのボタンを掛け違えていませんか？"},{"id":{"d":453,"n":2,"t":1},"speaker":"B","text":"本当ですね、ありがとうございます。"},{"id":{"d":453,"n":3,"t":1},"speaker":"A","text":"ニットはボタン穴がよく分からないものも多いですからね。"},{"id":{"d":453,"n":4,"t":1},"speaker":"B","text":"このカーディガンは、母の手作りです。"},{"id":{"d":453,"n":5,"t":1},"speaker":"A","text":"とってもすてきですよ。"},{"id":{"d":453,"n":6,"t":1},"speaker":"B","text":"ボタン穴が分かりにくいのは難点なのですけど。"},{"id":{"d":453,"n":7,"t":1},"speaker":"A","text":"気を付けて着れば、問題ありませんよ。"},{"id":{"d":453,"n":8,"t":1},"speaker":"B","text":"また間違っていたら、教えてください。"}];
INSERT INTO sentence [{"id":{"d":454,"n":1,"t":1},"speaker":"A","text":"こんばんは。"},{"id":{"d":454,"n":2,"t":1},"speaker":"B","text":"こんばんは。偶然ですね、夜中にコンビニエンスストアで会うなんて。"},{"id":{"d":454,"n":3,"t":1},"speaker":"A","text":"小腹が空いたので、肉まんを買いに来たのです。"},{"id":{"d":454,"n":4,"t":1},"speaker":"B","text":"私は明日の朝食用のパンを買いに来ました。"},{"id":{"d":454,"n":5,"t":1},"speaker":"A","text":"新商品の特製肉まんがおいしそうです。"},{"id":{"d":454,"n":6,"t":1},"speaker":"B","text":"私も、今度買おうと思っていました。"},{"id":{"d":454,"n":7,"t":1},"speaker":"A","text":"では、今日買って一緒に食べませんか？"},{"id":{"d":454,"n":8,"t":1},"speaker":"B","text":"いいですね。イートインコーナーで食べましょう。"}];
INSERT INTO sentence [{"id":{"d":455,"n":1,"t":1},"speaker":"A","text":"今度、公民館でフラワーアレンジメント教室があるのです。"},{"id":{"d":455,"n":2,"t":1},"speaker":"B","text":"1回だけの教室ですか？"},{"id":{"d":455,"n":3,"t":1},"speaker":"A","text":"そうです。"},{"id":{"d":455,"n":4,"t":1},"speaker":"B","text":"誰でも参加できるのですか？"},{"id":{"d":455,"n":5,"t":1},"speaker":"A","text":"会費の3,000円を支払えば、誰でも参加可能です。"},{"id":{"d":455,"n":6,"t":1},"speaker":"B","text":"一緒に行ってもよいですか？"},{"id":{"d":455,"n":7,"t":1},"speaker":"A","text":"はい、ぜひ一緒に行きましょう。そのつもりでお知らせしたのです。"},{"id":{"d":455,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。日時を教えてください。"}];
INSERT INTO sentence [{"id":{"d":456,"n":1,"t":1},"speaker":"A","text":"ママ、お風呂がお水のままです。"},{"id":{"d":456,"n":2,"t":1},"speaker":"B","text":"さっきガスをつけておいたのに、どうしたのでしょう。"},{"id":{"d":456,"n":3,"t":1},"speaker":"A","text":"壊れてしまったのですか？"},{"id":{"d":456,"n":4,"t":1},"speaker":"B","text":"風呂釜が、壊れてしまったのかもしれませんね。"},{"id":{"d":456,"n":5,"t":1},"speaker":"A","text":"今日は、お風呂に入れないのですか？"},{"id":{"d":456,"n":6,"t":1},"speaker":"B","text":"パパが帰ってきたら、銭湯に行きましょうか？"},{"id":{"d":456,"n":7,"t":1},"speaker":"A","text":"銭湯は初めてです。楽しみです。"},{"id":{"d":456,"n":8,"t":1},"speaker":"B","text":"喜んでいるようで良かったです。早速、出かける準備をしましょう。"}];
INSERT INTO sentence [{"id":{"d":457,"n":1,"t":1},"speaker":"A","text":"今日はギョウザパーティーにしましょう。"},{"id":{"d":457,"n":2,"t":1},"speaker":"B","text":"みんなで皮に好きなものを包んで焼くのですね。"},{"id":{"d":457,"n":3,"t":1},"speaker":"A","text":"はい。肉ギョウザと野菜ギョウザのどちらが好きですか？"},{"id":{"d":457,"n":4,"t":1},"speaker":"B","text":"私は肉ギョウザが好きです。あなたはどうですか？"},{"id":{"d":457,"n":5,"t":1},"speaker":"A","text":"私は野菜ギョウザの方が好みです。"},{"id":{"d":457,"n":6,"t":1},"speaker":"B","text":"そうなのですね。ちなみに、私は変わり種のギョウザも好きですよ。"},{"id":{"d":457,"n":7,"t":1},"speaker":"A","text":"今日はエビとチーズとポテトサラダとウインナーとキムチを用意してあります。"},{"id":{"d":457,"n":8,"t":1},"speaker":"B","text":"ではまずはキムチを包むことにします。"}];
INSERT INTO sentence [{"id":{"d":458,"n":1,"t":1},"speaker":"A","text":"あめはいかがですか？"},{"id":{"d":458,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":458,"n":3,"t":1},"speaker":"A","text":"「マスクの中のほどよいミントキャンディ」という商品名のあめです。"},{"id":{"d":458,"n":4,"t":1},"speaker":"B","text":"ずっとマスクをしていると、こういうのがうれしいです。"},{"id":{"d":458,"n":5,"t":1},"speaker":"A","text":"あまり辛くなくておいしいですよ。"},{"id":{"d":458,"n":6,"t":1},"speaker":"B","text":"スースーしますがミントの刺激が強すぎず、本当に程よい感じですね。おいしいです。"}];
INSERT INTO sentence [{"id":{"d":459,"n":1,"t":1},"speaker":"A","text":"随分大きいサンドイッチですね。"},{"id":{"d":459,"n":2,"t":1},"speaker":"B","text":"向かいの喫茶店で新しく始まったメニューです。"},{"id":{"d":459,"n":3,"t":1},"speaker":"A","text":"テイクアウトもできるのですか？"},{"id":{"d":459,"n":4,"t":1},"speaker":"B","text":"ランチタイム限定なのですが、テイクアウトできます。"},{"id":{"d":459,"n":5,"t":1},"speaker":"A","text":"コーヒーも、一緒に買えますか？"},{"id":{"d":459,"n":6,"t":1},"speaker":"B","text":"もちろんです。私はマグボトルを持って行って入れてもらいました。"},{"id":{"d":459,"n":7,"t":1},"speaker":"A","text":"それは良い考えですね。"},{"id":{"d":459,"n":8,"t":1},"speaker":"B","text":"最近は、環境のために、自分のマグボトルを使うようにしています。"}];
INSERT INTO sentence [{"id":{"d":460,"n":1,"t":1},"speaker":"A","text":"そろそろ衣替えの季節ですね。"},{"id":{"d":460,"n":2,"t":1},"speaker":"B","text":"そうですね。昨日、冬物の服を押し入れから出してみたのですが、どれもカビが生えていました。"},{"id":{"d":460,"n":3,"t":1},"speaker":"A","text":"それは大変ですね。カビを防止するために、何か工夫していなかったのですか？"},{"id":{"d":460,"n":4,"t":1},"speaker":"B","text":"押し入れに、すのこを敷いていたのですが、あまり役に立ちませんでした。"},{"id":{"d":460,"n":5,"t":1},"speaker":"A","text":"今年は、雨の日が多かったですからね。"},{"id":{"d":460,"n":6,"t":1},"speaker":"B","text":"あなたのところでは何か工夫をしていますか？"},{"id":{"d":460,"n":7,"t":1},"speaker":"A","text":"はい、うちは除湿剤をたくさん置いています。"},{"id":{"d":460,"n":8,"t":1},"speaker":"B","text":"それで、カビを防止できますか？"},{"id":{"d":460,"n":9,"t":1},"speaker":"A","text":"はい、これまで、服にカビが生えたことはありません。"},{"id":{"d":460,"n":10,"t":1},"speaker":"B","text":"そうですか。うちもそうすることにします。"}];
INSERT INTO sentence [{"id":{"d":461,"n":1,"t":1},"speaker":"A","text":"あなたの家では、朝食は和食ですか？それとも、洋食ですか？"},{"id":{"d":461,"n":2,"t":1},"speaker":"B","text":"うちは洋食です。パンとコーヒー、それにサラダと目玉焼きです。"},{"id":{"d":461,"n":3,"t":1},"speaker":"A","text":"うちは典型的な和食なのです。ご飯とおみそ汁、それに納豆とサケの焼き物です。"},{"id":{"d":461,"n":4,"t":1},"speaker":"B","text":"毎日同じメニューだと、家族から不満が出ませんか？"},{"id":{"d":461,"n":5,"t":1},"speaker":"A","text":"特には出ません。朝食とはそのようなものだと思っているのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":462,"n":1,"t":1},"speaker":"A","text":"最近、家のリフォームをしました。"},{"id":{"d":462,"n":2,"t":1},"speaker":"B","text":"そうですか。どのようにリフォームしたのですか？"},{"id":{"d":462,"n":3,"t":1},"speaker":"A","text":"テラスを拡張して、サロンとして使えるようにしました。それから、狭かった浴室を広げて、ジャグジーに変えました。"},{"id":{"d":462,"n":4,"t":1},"speaker":"B","text":"随分、大規模なリフォームですね。費用がかかったのではありませんか？"},{"id":{"d":462,"n":5,"t":1},"speaker":"A","text":"前から懇意にしていた大工さんに頼んだのと、自分で安い材料を探したので、それほどかかりませんでした。"},{"id":{"d":462,"n":6,"t":1},"speaker":"B","text":"今度、見せていただきたいですね。"},{"id":{"d":462,"n":7,"t":1},"speaker":"A","text":"そのうちパーティーをしますので、そのときにお招きします。"},{"id":{"d":462,"n":8,"t":1},"speaker":"B","text":"それは、ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":463,"n":1,"t":1},"speaker":"A","text":"山本さんは、もう新型コロナワクチンを接種しましたか？"},{"id":{"d":463,"n":2,"t":1},"speaker":"B","text":"いえ、まだなのです。接種券もまだ届いていないのですよ。"},{"id":{"d":463,"n":3,"t":1},"speaker":"A","text":"私は昨日接種券が届いたので、今日、予約の申し込みをしたところです。"},{"id":{"d":463,"n":4,"t":1},"speaker":"B","text":"いつ接種するのですか？"},{"id":{"d":463,"n":5,"t":1},"speaker":"A","text":"来月末になりそうです。"},{"id":{"d":463,"n":6,"t":1},"speaker":"B","text":"1カ月後ですか。その頃には、今の流行の波も収まっていると良いですね。"}];
INSERT INTO sentence [{"id":{"d":464,"n":1,"t":1},"speaker":"A","text":"東京国立博物館で創立150年記念特別展「国宝 東京国立博物館のすべて」があるそうですよ。"},{"id":{"d":464,"n":2,"t":1},"speaker":"B","text":"それは素晴らしいですね。山本さんは行かれますか？"},{"id":{"d":464,"n":3,"t":1},"speaker":"A","text":"もちろんです。このような企画はもう2度となさそうですからね。"},{"id":{"d":464,"n":4,"t":1},"speaker":"B","text":"私はあまり詳しくないのですが、東京国立博物館が所蔵している国宝にはどのようなものがありますか？"},{"id":{"d":464,"n":5,"t":1},"speaker":"A","text":"例えば、長谷川等伯の「松林図屏風」があります。"},{"id":{"d":464,"n":6,"t":1},"speaker":"B","text":"ああ、あの有名な水墨画ですね。それは、ぜひ見たいですね。"},{"id":{"d":464,"n":7,"t":1},"speaker":"A","text":"私もまだ実物を見たことがないので、この機会に見たいと思っています。"}];
INSERT INTO sentence [{"id":{"d":465,"n":1,"t":1},"speaker":"A","text":"先日、白バイこと交通取締用自動二輪車に捕まってしまいました。"},{"id":{"d":465,"n":2,"t":1},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":465,"n":3,"t":1},"speaker":"A","text":"交差点で、つい赤信号を見過ごして、突っ切ろうとしたのです。"},{"id":{"d":465,"n":4,"t":1},"speaker":"B","text":"それは危ないですね。事故は起きませんでしたか？"},{"id":{"d":465,"n":5,"t":1},"speaker":"A","text":"危うく人をはねるところでした。少し手前で気が付いてブレーキをかけたので、事なきを得ましたが。"},{"id":{"d":465,"n":6,"t":1},"speaker":"B","text":"そこに、ちょうど白バイが居合わせたというわけですね。"},{"id":{"d":465,"n":7,"t":1},"speaker":"A","text":"はい。若い警官で、その日、初めてパトロールに出たのだと言っていました。"},{"id":{"d":465,"n":8,"t":1},"speaker":"B","text":"その警官もびっくりしたでしょうね。交差点での事故が一番多いといいますから、気を付けてくださいね。"},{"id":{"d":465,"n":9,"t":1},"speaker":"A","text":"はい、気を付けます。"}];
INSERT INTO sentence [{"id":{"d":466,"n":1,"t":1},"speaker":"A","text":"クモの巣に水滴がからんできれいですね。"},{"id":{"d":466,"n":2,"t":1},"speaker":"B","text":"本当ですね。明け方に雨が降ったのでしょう。"},{"id":{"d":466,"n":3,"t":1},"speaker":"A","text":"日が差してきました。"},{"id":{"d":466,"n":4,"t":1},"speaker":"B","text":"光を反射して、まるで小さなダイヤモンドか何かをちりばめたようです。"},{"id":{"d":466,"n":5,"t":1},"speaker":"A","text":"それにしても、クモというのは不思議な動物ですね。体から糸を出して、体の何倍もの大きさの巣を作る。"},{"id":{"d":466,"n":6,"t":1},"speaker":"B","text":"自分はどこか見えないところに潜んでいて、獲物がかかるのを待っている。"},{"id":{"d":466,"n":7,"t":1},"speaker":"A","text":"そのような生態が神秘的に見えたからか、昔から神話や伝承によく出てきます。"},{"id":{"d":466,"n":8,"t":1},"speaker":"B","text":"ギリシア神話のアラクネとか、日本の土クモとかですね。"},{"id":{"d":466,"n":9,"t":1},"speaker":"A","text":"現代でも、ホラー作品によく登場します。"},{"id":{"d":466,"n":10,"t":1},"speaker":"B","text":"しかし、このように美しいのだから、今はこの光景を堪能しましょう。"}];
INSERT INTO sentence [{"id":{"d":467,"n":1,"t":1},"speaker":"A","text":"小川さんは、おそばはお好きですか？"},{"id":{"d":467,"n":2,"t":1},"speaker":"B","text":"ええ、大好きです。おいしいそば屋があると聞くと、必ず行ってみます。"},{"id":{"d":467,"n":3,"t":1},"speaker":"A","text":"私も好きで、最近、自分でそば打ちを始めたのですよ。"},{"id":{"d":467,"n":4,"t":1},"speaker":"B","text":"それはすごいですね。私はさすがにそこまではできません。それで、出来ばえはいかがですか？"},{"id":{"d":467,"n":5,"t":1},"speaker":"A","text":"それが、なかなか思うようにいきません。ここまで難しいものとは思いませんでしたよ。"},{"id":{"d":467,"n":6,"t":1},"speaker":"B","text":"どういうところが難しいですか？"},{"id":{"d":467,"n":7,"t":1},"speaker":"A","text":"そば粉の選定、水まわし、練り、のし、切り、全てが難しいです。"},{"id":{"d":467,"n":8,"t":1},"speaker":"B","text":"そのうち、ごちそうしていただけるのを楽しみにしていますよ。"},{"id":{"d":467,"n":9,"t":1},"speaker":"A","text":"そうなるまでには、相当な時間がかかりそうです。"}];
INSERT INTO sentence [{"id":{"d":468,"n":1,"t":1},"speaker":"A","text":"黒田さん、旭川市旭山動物園に行ったことがありますか？"},{"id":{"d":468,"n":2,"t":1},"speaker":"B","text":"いえ、まだ行ったことはありません。評判はよく聞くので、いつか行きたいと思っているのですが。"},{"id":{"d":468,"n":3,"t":1},"speaker":"A","text":"ぜひ、行くべきですよ。評判以上です。"},{"id":{"d":468,"n":4,"t":1},"speaker":"B","text":"それほど面白いのですか。太田さんはいつ行かれたのですか？"},{"id":{"d":468,"n":5,"t":1},"speaker":"A","text":"夏休みに行ってきました。子どもにせがまれてしょうことなしに行ったのですが、行って良かったです。"},{"id":{"d":468,"n":6,"t":1},"speaker":"B","text":"そうでしたか。旭川市旭山動物園といえば、ペンギンの散歩などがよくテレビ番組で放映されますね。"},{"id":{"d":468,"n":7,"t":1},"speaker":"A","text":"他にも、動物の生態がよく分かるように工夫されていて、見ていて飽きません。"},{"id":{"d":468,"n":8,"t":1},"speaker":"B","text":"私も今度行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":469,"n":1,"t":1},"speaker":"A","text":"尚子さんは、今日の夕食は何が食べたいですか？"},{"id":{"d":469,"n":2,"t":1},"speaker":"B","text":"私は、ハンバーグが食べたいです。"},{"id":{"d":469,"n":3,"t":1},"speaker":"A","text":"またハンバーグですか？一昨日食べたばかりではありませんか。お肉ばかり食べるのは体に良くありませんよ。"},{"id":{"d":469,"n":4,"t":1},"speaker":"B","text":"だって、お母さんの作るハンバーグはとてもおいしいのです。私は大好きです。"},{"id":{"d":469,"n":5,"t":1},"speaker":"A","text":"お世辞を言っても駄目です。"},{"id":{"d":469,"n":6,"t":1},"speaker":"B","text":"お世辞ではありません。本当です。"},{"id":{"d":469,"n":7,"t":1},"speaker":"A","text":"それでは、今日はハンバーグにしますけど、その代わりにお野菜をたっぷり食べてもらいますからね。"},{"id":{"d":469,"n":8,"t":1},"speaker":"B","text":"野菜ですか？あまり食べたくありませんが、我慢して食べます。"}];
INSERT INTO sentence [{"id":{"d":470,"n":1,"t":1},"speaker":"A","text":"リンゴのケーキを作りましょう。麻耶さんも手伝ってください。"},{"id":{"d":470,"n":2,"t":1},"speaker":"B","text":"分かりました。どうすればよいのですか？"},{"id":{"d":470,"n":3,"t":1},"speaker":"A","text":"まず、リンゴの皮をむいて、芯を取って、一口大に切ってください。"},{"id":{"d":470,"n":4,"t":1},"speaker":"B","text":"このくらいの大きさでよいですか？"},{"id":{"d":470,"n":5,"t":1},"speaker":"A","text":"はい、それでよいです。それから、フライパンにバターと砂糖を入れて、リンゴを炒めます。"},{"id":{"d":470,"n":6,"t":1},"speaker":"B","text":"こんがり色づいて、良い匂いがしてきました。"},{"id":{"d":470,"n":7,"t":1},"speaker":"A","text":"それから、ケーキの型にクッキングペーパーを敷いて、炒めたリンゴを入れてください。"},{"id":{"d":470,"n":8,"t":1},"speaker":"B","text":"はい、入れました。"},{"id":{"d":470,"n":9,"t":1},"speaker":"A","text":"卵、ナツメグ、シナモン、ホットケーキミックスを混ぜ、最後に溶かしバターを混ぜます。"},{"id":{"d":470,"n":10,"t":1},"speaker":"B","text":"これを、リンゴの上にかけるのですね？"},{"id":{"d":470,"n":11,"t":1},"speaker":"A","text":"そうです。その後は、オーブンで25分ほど焼きます。"},{"id":{"d":470,"n":12,"t":1},"speaker":"B","text":"簡単ですね。とてもおいしそうです。待ち遠しいですね。"}];
INSERT INTO sentence [{"id":{"d":471,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。お久しぶりですね。"},{"id":{"d":471,"n":2,"t":1},"speaker":"B","text":"こんばんは。仕事が忙しくて、なかなか来られませんでした。"},{"id":{"d":471,"n":3,"t":1},"speaker":"A","text":"しばらくお顔を見なかったので、どうしていらっしゃるのかと思っていました。"},{"id":{"d":471,"n":4,"t":1},"speaker":"B","text":"やっと一段落がついたので、気分を転換したいと思って来たのです。"},{"id":{"d":471,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。今日は何にしましょう。"},{"id":{"d":471,"n":6,"t":1},"speaker":"B","text":"今日は、何かおいしいものがありますか？"},{"id":{"d":471,"n":7,"t":1},"speaker":"A","text":"広島産の新鮮なカキが入っていますよ。"},{"id":{"d":471,"n":8,"t":1},"speaker":"B","text":"それはおいしそうですね。まずはそれを頂きましょう。"},{"id":{"d":471,"n":9,"t":1},"speaker":"A","text":"かしこまりました。お飲み物は何にしましょうか？"},{"id":{"d":471,"n":10,"t":1},"speaker":"B","text":"日本酒を冷やでお願いします。"},{"id":{"d":471,"n":11,"t":1},"speaker":"A","text":"宮城のお酒で、カキにぴったり合うものがありますよ。"},{"id":{"d":471,"n":12,"t":1},"speaker":"B","text":"それはいいですね。"}];
INSERT INTO sentence [{"id":{"d":472,"n":1,"t":1},"speaker":"A","text":"お母さんは僕の部屋の中の物を触りましたか？"},{"id":{"d":472,"n":2,"t":1},"speaker":"B","text":"あんまり散らかっていたものだから、片付けてあげたのですよ。"},{"id":{"d":472,"n":3,"t":1},"speaker":"A","text":"勝手に部屋に入らないでください。"},{"id":{"d":472,"n":4,"t":1},"speaker":"B","text":"それなら自分できちんと整理整頓してください。"},{"id":{"d":472,"n":5,"t":1},"speaker":"A","text":"自分なりに整理しているのですよ。"},{"id":{"d":472,"n":6,"t":1},"speaker":"B","text":"あれで整理しているなんて、言えませんよ。"},{"id":{"d":472,"n":7,"t":1},"speaker":"A","text":"どこに何があるのか、きちんと分かっているのです。人に部屋の中の物を触られるとかえって混乱します。"},{"id":{"d":472,"n":8,"t":1},"speaker":"B","text":"言い分はわかりました。では、整理整頓もして、自分できちんと部屋を清潔に保ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":473,"n":1,"t":1},"speaker":"A","text":"天気予報では、明日は雪になるそうですよ。"},{"id":{"d":473,"n":2,"t":1},"speaker":"B","text":"困りましたね。まだ雪用のタイヤに替えていないのです。"},{"id":{"d":473,"n":3,"t":1},"speaker":"A","text":"もっと早く替えておくべきでしたね。"},{"id":{"d":473,"n":4,"t":1},"speaker":"B","text":"温暖化のせいで初雪がだんだん遅くなっていましたから、まだ大丈夫だと思っていたのです。"},{"id":{"d":473,"n":5,"t":1},"speaker":"A","text":"12月になったら、いつ雪が降ってもおかしくありませんよ。"},{"id":{"d":473,"n":6,"t":1},"speaker":"B","text":"今日中に自動車整備工場に行ってタイヤ交換をしてもらいます。"},{"id":{"d":473,"n":7,"t":1},"speaker":"A","text":"混んでいるのではありませんか？"},{"id":{"d":473,"n":8,"t":1},"speaker":"B","text":"仕方がありません。気長に待つことにします。"}];
INSERT INTO sentence [{"id":{"d":474,"n":1,"t":1},"speaker":"A","text":"鈴木さんは着こなしが上手ですね。"},{"id":{"d":474,"n":2,"t":1},"speaker":"B","text":"そのようなことはありません。いつも何を着たらいいのか迷っています。"},{"id":{"d":474,"n":3,"t":1},"speaker":"A","text":"何か、心掛けていることはありますか？"},{"id":{"d":474,"n":4,"t":1},"speaker":"B","text":"皆さんと同じですよ。ファッション雑誌を見るとか、他の人の服装を観察するとか、でしょうか？"},{"id":{"d":474,"n":5,"t":1},"speaker":"A","text":"私もそうしていますが、なかなかうまくなりません。持って生まれたセンスの違いでしょうか？"},{"id":{"d":474,"n":6,"t":1},"speaker":"B","text":"そういえば、私は母から着こなしについて口やかましく言われました。本当に嫌になるくらいに。"},{"id":{"d":474,"n":7,"t":1},"speaker":"A","text":"きっとお母さんもセンスの良い方だったのでしょうね。"},{"id":{"d":474,"n":8,"t":1},"speaker":"B","text":"センスが良かったかどうか分かりませんが、色の取り合わせだとかいろいろと教わりました。"},{"id":{"d":474,"n":9,"t":1},"speaker":"A","text":"それで自然に身に付いたのですね。"}];
INSERT INTO sentence [{"id":{"d":475,"n":1,"t":1},"speaker":"A","text":"うちの子どもは食べ物の好き嫌いが激しくて困ってしまいます。"},{"id":{"d":475,"n":2,"t":1},"speaker":"B","text":"うちも同じです。お肉ばかり食べて、野菜は少しも食べてくれません。"},{"id":{"d":475,"n":3,"t":1},"speaker":"A","text":"ニンジンとかピーマンとか入っていると、それだけ取り除けてしまうのですから。"},{"id":{"d":475,"n":4,"t":1},"speaker":"B","text":"それに、ゴボウ、ダイコン、ナス、ホウレンソウ。そういうものを見ると、露骨に嫌な顔をします。"},{"id":{"d":475,"n":5,"t":1},"speaker":"A","text":"何か工夫をしていますか？"},{"id":{"d":475,"n":6,"t":1},"speaker":"B","text":"私は、細かく切って、好きな料理に紛れ込ませるようにしています。"},{"id":{"d":475,"n":7,"t":1},"speaker":"A","text":"それは私もやっています。ハンバーグにニンジンの細切れを練り込むとか。"},{"id":{"d":475,"n":8,"t":1},"speaker":"B","text":"食感とか臭いなんかも好き嫌いの原因になりますから、そういうのを消す工夫をしています。"},{"id":{"d":475,"n":9,"t":1},"speaker":"A","text":"どういう工夫ですか？"},{"id":{"d":475,"n":10,"t":1},"speaker":"B","text":"例えば、ニンジンやゴボウをチップにしてみたり、マヨネーズを使って臭いを消したりしています。"},{"id":{"d":475,"n":11,"t":1},"speaker":"A","text":"なるほど、私もやってみましょう。"}];
INSERT INTO sentence [{"id":{"d":476,"n":1,"t":1},"speaker":"A","text":"今度の日曜日に、拙宅でバーベキューパーティーをしましょう。"},{"id":{"d":476,"n":2,"t":1},"speaker":"B","text":"それはありがとうございます。いつもお招きされるばかりで、悪いですね。"},{"id":{"d":476,"n":3,"t":1},"speaker":"A","text":"いえいえ、どうかご遠慮なさらずにいらしてください。"},{"id":{"d":476,"n":4,"t":1},"speaker":"B","text":"それではお言葉に甘えまして、お伺いします。その代わりと言っては何ですが、お肉を持参します。"},{"id":{"d":476,"n":5,"t":1},"speaker":"A","text":"どうかお気遣いなく。神戸にいる親類から神戸牛を送ってきたので、それでバーベキューパーティーを思い立ったのです。"},{"id":{"d":476,"n":6,"t":1},"speaker":"B","text":"そうですか。それでは、私は飲み物を持参します。ワインでよろしいでしょうか？"},{"id":{"d":476,"n":7,"t":1},"speaker":"A","text":"ああ、いいですね。私はビールを用意しておきますので、それではワインをお願いします。"},{"id":{"d":476,"n":8,"t":1},"speaker":"B","text":"ボルドーかブルゴーニュのおいしいのを見繕っていきます。"},{"id":{"d":476,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":477,"n":1,"t":1},"speaker":"A","text":"今度、思い切ってマンションを購入することにしました。"},{"id":{"d":477,"n":2,"t":1},"speaker":"B","text":"ほう、どのあたりのマンションですか？"},{"id":{"d":477,"n":3,"t":1},"speaker":"A","text":"渋谷の代々木上原です。ちょうど手頃で良さそうなマンションがありましたので。"},{"id":{"d":477,"n":4,"t":1},"speaker":"B","text":"良いところではありませんか。あの辺りは静かで、緑も多いし、住宅地としては絶好ですね。よく思い立ちましたね。"},{"id":{"d":477,"n":5,"t":1},"speaker":"A","text":"実は、今住んでいる借家が期限付きのもので、立ち退かなければいけないのです。"},{"id":{"d":477,"n":6,"t":1},"speaker":"B","text":"そういう事情があったのですか。それにしても、また借家にするという選択もあるでしょうに。"},{"id":{"d":477,"n":7,"t":1},"speaker":"A","text":"私もそのつもりで少し探したのですが、家賃が予想以上に高くて、それならローンを組んででも買う方が安いと思いまして。"},{"id":{"d":477,"n":8,"t":1},"speaker":"B","text":"なるほど。でも、私など、何十年もローンに拘束されるのは怖い気もします。"},{"id":{"d":477,"n":9,"t":1},"speaker":"A","text":"確かに、何らかの事情でローンが払えなくなる可能性もないわけではありませんが、そこは目をつぶることにしました。"},{"id":{"d":477,"n":10,"t":1},"speaker":"B","text":"黒田さんなら、大丈夫でしょう。何にしろ、気に入ったマンションが見つかったのは良かったですね。"}];
INSERT INTO sentence [{"id":{"d":478,"n":1,"t":1},"speaker":"A","text":"後方から、救急車のサイレンの音が聞こえませんか？"},{"id":{"d":478,"n":2,"t":1},"speaker":"B","text":"そういえば、かすかに聞こえますね。"},{"id":{"d":478,"n":3,"t":1},"speaker":"A","text":"こちらに来るでしょうか？"},{"id":{"d":478,"n":4,"t":1},"speaker":"B","text":"今のところは、何とも言えませんね。"},{"id":{"d":478,"n":5,"t":1},"speaker":"A","text":"道路が混雑しているようなので、救急車も立ち往生してしまうかもしれません。"},{"id":{"d":478,"n":6,"t":1},"speaker":"B","text":"やっぱり、こちらに来るようですよ。サイレンの音が大きくなりました。"},{"id":{"d":478,"n":7,"t":1},"speaker":"A","text":"バックミラーに映っています。やはり、難渋しているようです。"},{"id":{"d":478,"n":8,"t":1},"speaker":"B","text":"できるだけ左に寄せて、停車しましょう。"},{"id":{"d":478,"n":9,"t":1},"speaker":"A","text":"車の間を縫うようにして、何とか通れるようですね。"},{"id":{"d":478,"n":10,"t":1},"speaker":"B","text":"良かった。早く病院に着くといいですね。"}];
INSERT INTO sentence [{"id":{"d":479,"n":1,"t":1},"speaker":"A","text":"そろそろ、冬物の支度をしなければいけませんね。"},{"id":{"d":479,"n":2,"t":1},"speaker":"B","text":"クローゼットの奥にまとめてしまってあるのでしたね？"},{"id":{"d":479,"n":3,"t":1},"speaker":"A","text":"そうです。傷んでいないかどうか、点検してみてください。"},{"id":{"d":479,"n":4,"t":1},"speaker":"B","text":"はい。お母さん、私の紺色のオーバーコートが見当たりません。"},{"id":{"d":479,"n":5,"t":1},"speaker":"A","text":"古くなったから処分したのではありませんか？"},{"id":{"d":479,"n":6,"t":1},"speaker":"B","text":"気に入っていたので、私が処分したはずはありません。"},{"id":{"d":479,"n":7,"t":1},"speaker":"A","text":"それなら、探せばどこかにあるはずですよ。"},{"id":{"d":479,"n":8,"t":1},"speaker":"B","text":"やっぱり見つかりません。"},{"id":{"d":479,"n":9,"t":1},"speaker":"A","text":"一度、全部出してみましょう。あなたのコートも、どこかに紛れているのでしょう。"}];
INSERT INTO sentence [{"id":{"d":480,"n":1,"t":1},"speaker":"A","text":"お母さん、最近料理の味付けが薄くなった気がするのですが。"},{"id":{"d":480,"n":2,"t":1},"speaker":"B","text":"気が付きましたか。最近、塩分を控えるようにしているのです。"},{"id":{"d":480,"n":3,"t":1},"speaker":"A","text":"濃い味付けに慣れているせいか、何となく食べた気がしません。"},{"id":{"d":480,"n":4,"t":1},"speaker":"B","text":"あなたの血圧のことが、心配なのです。"},{"id":{"d":480,"n":5,"t":1},"speaker":"A","text":"心配してくれるのはありがたいですが、食事はもっとおいしく食べたいですね。"},{"id":{"d":480,"n":6,"t":1},"speaker":"B","text":"では、塩分の代わりに、何か他の工夫をしましょう。"},{"id":{"d":480,"n":7,"t":1},"speaker":"A","text":"そのようなことが、できるのですか？"},{"id":{"d":480,"n":8,"t":1},"speaker":"B","text":"だしをもっと濃くするとか、お酢を加えるとか、いろいろ工夫はあるようですよ。"}];
INSERT INTO sentence [{"id":{"d":481,"n":1,"t":1},"speaker":"A","text":"ステーキがなかなかおいしく焼けません。"},{"id":{"d":481,"n":2,"t":1},"speaker":"B","text":"ステーキをおいしく焼くにはいくつかコツがあります。まず、肉を常温に戻す必要があります。"},{"id":{"d":481,"n":3,"t":1},"speaker":"A","text":"冷蔵庫から出してそのまま焼いていました。それでは駄目なのですね。"},{"id":{"d":481,"n":4,"t":1},"speaker":"B","text":"冷たいままの肉を焼くと、時間がかかってパサパサになります。"},{"id":{"d":481,"n":5,"t":1},"speaker":"A","text":"私が焼くといつもパサパサになっていましたが、それが原因だったのですね。"},{"id":{"d":481,"n":6,"t":1},"speaker":"B","text":"それから、丁寧に筋切りをします。肉が縮んだり反り返ったりするのを防ぐためです。"},{"id":{"d":481,"n":7,"t":1},"speaker":"A","text":"私はそれもしていませんでした。確かに、反り返って見栄えも良くありませんでした。"},{"id":{"d":481,"n":8,"t":1},"speaker":"B","text":"塩・コショウは焼く直前に振ります。焼くまでの時間が長いと、水分とともに旨味成分が出てしまうのです。"},{"id":{"d":481,"n":9,"t":1},"speaker":"A","text":"それも逆でした。私は肉に染み込むように15分ほど前に振っていました。"},{"id":{"d":481,"n":10,"t":1},"speaker":"B","text":"焼くときは、強火で短時間で焼きます。そして、アルミで包んで余熱で火を通します。"},{"id":{"d":481,"n":11,"t":1},"speaker":"A","text":"何から何まで、私は反対のことをしていました。今度は教えられた通りにやってみます。"}];
INSERT INTO sentence [{"id":{"d":482,"n":1,"t":1},"speaker":"A","text":"あなたは、粒あこしんとこしあんのどちらが好きですか？"},{"id":{"d":482,"n":2,"t":1},"speaker":"B","text":"私は、断然こしあんが好きです。こしあんの口の中で溶けていく感じが好きです。あなたはどちらが好きですか？"},{"id":{"d":482,"n":3,"t":1},"speaker":"A","text":"私は、粒あん派です。粒々の食感が好きです。世の中には、粒あんとこしあんのどちらが好きかというアンケート調査があるそうですよ。"},{"id":{"d":482,"n":4,"t":1},"speaker":"B","text":"面白い調査ですね。結果はどうだったのですか？"},{"id":{"d":482,"n":5,"t":1},"speaker":"A","text":"3対2で粒あん好きの方が多かったそうです。"},{"id":{"d":482,"n":6,"t":1},"speaker":"B","text":"私は少数派なのですね。"},{"id":{"d":482,"n":7,"t":1},"speaker":"A","text":"それほど差があるわけでもありませんし、張り合っていると言ってもよいようです。"}];
INSERT INTO sentence [{"id":{"d":483,"n":1,"t":1},"speaker":"A","text":"ガソリンの値上がりが止まりません。先週から1リットル当たり3円上がりました。"},{"id":{"d":483,"n":2,"t":1},"speaker":"B","text":"13年ぶりだそうですね。"},{"id":{"d":483,"n":3,"t":1},"speaker":"A","text":"13年前というと2009年ですね。何があったのでしょう。"},{"id":{"d":483,"n":4,"t":1},"speaker":"B","text":"その前年の2008年にリーマンショックがありましたね。"},{"id":{"d":483,"n":5,"t":1},"speaker":"A","text":"それが原油の高騰と関係があるのでしょうか？"},{"id":{"d":483,"n":6,"t":1},"speaker":"B","text":"投資資金が土地や株式から原油に向かったといわれています。"},{"id":{"d":483,"n":7,"t":1},"speaker":"A","text":"なるほど、回り回って思わぬところに影響するのですね。"},{"id":{"d":483,"n":8,"t":1},"speaker":"B","text":"ガソリンが値上がりすると、運賃が上がって、いろいろな商品も値上がりします。"},{"id":{"d":483,"n":9,"t":1},"speaker":"A","text":"ドライブだけでなく、生活全般にわたって節約しなければならなくなりますね。"}];
INSERT INTO sentence [{"id":{"d":484,"n":1,"t":1},"speaker":"A","text":"ショッピングモールに食パン専門のパン屋ができました。"},{"id":{"d":484,"n":2,"t":1},"speaker":"B","text":"テレビ放送で見て知っています。あなたは行ってみましたか？"},{"id":{"d":484,"n":3,"t":1},"speaker":"A","text":"はい、そのテレビ放送の翌日に行ってきました。とても混んでいました。"},{"id":{"d":484,"n":4,"t":1},"speaker":"B","text":"パンはどうでしたか？おいしかったですか？"},{"id":{"d":484,"n":5,"t":1},"speaker":"A","text":"さすがに専門店だけあって、おいしかったです。プレーンの食パン以外にも、レーズンが入った食パン、生クリームが入った食パンなどでもありましたよ。"},{"id":{"d":484,"n":6,"t":1},"speaker":"B","text":"テレビ放送で見た情報では、スーパーなどで売っている普通の食パンよりも値段が高そうでした。"},{"id":{"d":484,"n":7,"t":1},"speaker":"A","text":"確かに値段は高いですが、それだけのことはあります。とてもおいしかったです。"}];
INSERT INTO sentence [{"id":{"d":485,"n":1,"t":1},"speaker":"A","text":"税務署から手紙が来ていますよ。"},{"id":{"d":485,"n":2,"t":1},"speaker":"B","text":"何でしょうね。全く心当たりがありません。"},{"id":{"d":485,"n":3,"t":1},"speaker":"A","text":"督促状ではないでしょうね？"},{"id":{"d":485,"n":4,"t":1},"speaker":"B","text":"そのようなことはないでしょう。私は、払うべき税金はきちんと払っています。"},{"id":{"d":485,"n":5,"t":1},"speaker":"A","text":"それなら何でしょう？早く開いてみてください。"},{"id":{"d":485,"n":6,"t":1},"speaker":"B","text":"税務調査の通告でした。3週間後にやってくるそうです。"},{"id":{"d":485,"n":7,"t":1},"speaker":"A","text":"税務調査なんて、何だか怖いですね。"},{"id":{"d":485,"n":8,"t":1},"speaker":"B","text":"心配は要りません、別に不正はしていませんから。"},{"id":{"d":485,"n":9,"t":1},"speaker":"A","text":"いろいろと準備が必要なのでしょう？"},{"id":{"d":485,"n":10,"t":1},"speaker":"B","text":"税理士に相談して、必要な書類など整理しておきます。"},{"id":{"d":485,"n":11,"t":1},"speaker":"A","text":"何事もないことを、祈っています。"},{"id":{"d":485,"n":12,"t":1},"speaker":"B","text":"大げさですよ。安心していてください。"}];
INSERT INTO sentence [{"id":{"d":486,"n":1,"t":1},"speaker":"A","text":"役場からハザードマップが届きました。"},{"id":{"d":486,"n":2,"t":1},"speaker":"B","text":"早速確認しましょう。この辺りは大丈夫でしょうか？"},{"id":{"d":486,"n":3,"t":1},"speaker":"A","text":"大丈夫ではありませんね。噴火の際、溶岩が流れてくる経路に入っています。"},{"id":{"d":486,"n":4,"t":1},"speaker":"B","text":"それでは、この家も溶岩に飲み込まれてしまうのですか？"},{"id":{"d":486,"n":5,"t":1},"speaker":"A","text":"噴火の場所にもよりますから、必ず溶岩が流れてくるとは限りません。"},{"id":{"d":486,"n":6,"t":1},"speaker":"B","text":"それでも、用心するに越したことはありません。防災準備をしましょう。"},{"id":{"d":486,"n":7,"t":1},"speaker":"A","text":"そうですね。防災ガイドブックを参考に、必要なものを用意しておきましょう。"},{"id":{"d":486,"n":8,"t":1},"speaker":"B","text":"善は急げ、です。今日から取り掛かりましょう。"},{"id":{"d":486,"n":9,"t":1},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":487,"n":1,"t":1},"speaker":"A","text":"今日はカレーにしようと思うのですが、それでよいですか？"},{"id":{"d":487,"n":2,"t":1},"speaker":"B","text":"賛成です。ちょうど、カレーが食べたいと思っていたのです。"},{"id":{"d":487,"n":3,"t":1},"speaker":"A","text":"それは良かったです。お肉は何を入れましょうか。ビーフ？ポーク？それともチキンがいいですか？"},{"id":{"d":487,"n":4,"t":1},"speaker":"B","text":"この間はビーフだったので、今日はチキンが食べたいです。"},{"id":{"d":487,"n":5,"t":1},"speaker":"A","text":"それではチキンにしましょう。サラダはあっさりしたものがいいですね。"},{"id":{"d":487,"n":6,"t":1},"speaker":"B","text":"そうですね。カレーがこってりしていますから、サラダはあっさりしたものがいいです。"},{"id":{"d":487,"n":7,"t":1},"speaker":"A","text":"それでは、今から作ります。2時間ほどかかります。"},{"id":{"d":487,"n":8,"t":1},"speaker":"B","text":"分かりました。その間、私は散歩でもして、腹ごなしをしてきます。"}];
INSERT INTO sentence [{"id":{"d":488,"n":1,"t":1},"speaker":"A","text":"冷やし中華が食べたいですね。"},{"id":{"d":488,"n":2,"t":1},"speaker":"B","text":"今は冬ですよ。冷やし中華を出しているところはありません。"},{"id":{"d":488,"n":3,"t":1},"speaker":"A","text":"あのようにおいしいものを、夏限定にしておくのはもったいないではありませんか？"},{"id":{"d":488,"n":4,"t":1},"speaker":"B","text":"そう思うのはあなただけです。多くの人は冷やし中華は夏の食べ物だと思っているでしょう。"},{"id":{"d":488,"n":5,"t":1},"speaker":"A","text":"韓国料理の冷麺は冬にも食べるではありませんか？"},{"id":{"d":488,"n":6,"t":1},"speaker":"B","text":"冷麺は、もともと冬に食べる料理だったのです。"},{"id":{"d":488,"n":7,"t":1},"speaker":"A","text":"それはまたなぜですか？"},{"id":{"d":488,"n":8,"t":1},"speaker":"B","text":"冬の暖かいオンドル部屋で食べる料理だったのです。"},{"id":{"d":488,"n":9,"t":1},"speaker":"A","text":"それなら、冷やし中華も冬に食べてもよいような気がします。"},{"id":{"d":488,"n":10,"t":1},"speaker":"B","text":"どうしても食べたいなら、自分で材料を見つけて作るしかありませんね。"},{"id":{"d":488,"n":11,"t":1},"speaker":"A","text":"仕方がない、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":489,"n":1,"t":1},"speaker":"A","text":"スーツを買いに行きたいのですが、どこに行ったらよいですか？"},{"id":{"d":489,"n":2,"t":1},"speaker":"B","text":"新宿に、いくつかデパートがあります。いつもカジュアルな格好なのに、スーツとは珍しいですね。"},{"id":{"d":489,"n":3,"t":1},"speaker":"A","text":"そろそろ、就職活動の準備をしようと思っているのです。"},{"id":{"d":489,"n":4,"t":1},"speaker":"B","text":"そういうことでしたら、デパートよりも、駅前にある紳士服量販店の方がいいかもしれません。"},{"id":{"d":489,"n":5,"t":1},"speaker":"A","text":"紳士服のお店ですか？"},{"id":{"d":489,"n":6,"t":1},"speaker":"B","text":"ええ。ですが、最近は女子学生向けのリクルートスーツも充実しているようですよ。"},{"id":{"d":489,"n":7,"t":1},"speaker":"A","text":"ブラウスも売っていますか？"},{"id":{"d":489,"n":8,"t":1},"speaker":"B","text":"もちろんです。就職活動向きの靴やかばんもそろっています。"},{"id":{"d":489,"n":9,"t":1},"speaker":"A","text":"それはいいですね。ありがとうございます。今度の日曜日に行ってみます。"}];
INSERT INTO sentence [{"id":{"d":490,"n":1,"t":1},"speaker":"A","text":"この間、着物の着付けの先生が、着物を着るところの動画を見ました。"},{"id":{"d":490,"n":2,"t":1},"speaker":"B","text":"着せてもらうのではなく、自分で着るのですか？"},{"id":{"d":490,"n":3,"t":1},"speaker":"A","text":"そうです。着物は、着せてもらうものだと思っていたので驚きました。それに、あまりの手際の良さにもびっくりしました。"},{"id":{"d":490,"n":4,"t":1},"speaker":"B","text":"あっという間に着てしまうのですか？"},{"id":{"d":490,"n":5,"t":1},"speaker":"A","text":"そうなのです。私がパジャマから普段着に着替えるよりも、時間がかからないくらいでした。"},{"id":{"d":490,"n":6,"t":1},"speaker":"B","text":"それは、驚きですね。そういえば、私の曽祖母は普段、着物を着ていたようです。写真が残っています。"},{"id":{"d":490,"n":7,"t":1},"speaker":"A","text":"自分で着ていたのでしょうか？"},{"id":{"d":490,"n":8,"t":1},"speaker":"B","text":"きっとそうでしょう。写真に写っていたのはよそ行きではなく、普段着でしたから。"},{"id":{"d":490,"n":9,"t":1},"speaker":"A","text":"すてきですね。"}];
INSERT INTO sentence [{"id":{"d":491,"n":1,"t":1},"speaker":"A","text":"明日からアルバイトなのですが、この服装で大丈夫でしょうか？"},{"id":{"d":491,"n":2,"t":1},"speaker":"B","text":"どのような仕事をするのですか？"},{"id":{"d":491,"n":3,"t":1},"speaker":"A","text":"事務作業です。オフィスカジュアルで通勤するように言われました。"},{"id":{"d":491,"n":4,"t":1},"speaker":"B","text":"そうですか。オフィスカジュアルといっても職場によって違うから難しいですよね。職場の皆さんはどのような服装でしたか？"},{"id":{"d":491,"n":5,"t":1},"speaker":"A","text":"男性は、チノパンにボタンダウンシャツといった感じでした。"},{"id":{"d":491,"n":6,"t":1},"speaker":"B","text":"それなら、初日はきちんとした感じのブラウスや、シャツの方が無難かもしれませんね。"},{"id":{"d":491,"n":7,"t":1},"speaker":"A","text":"Tシャツやカットソーは、避けた方がいいですか？"},{"id":{"d":491,"n":8,"t":1},"speaker":"B","text":"一概には言えませんが、職場の雰囲気に合うようなら、少しずつ取り入れていったらいいと思います。"}];
INSERT INTO sentence [{"id":{"d":492,"n":1,"t":1},"speaker":"A","text":"健太さんの誕生日会のごちそうは、何にしましょうか？鶏の唐揚げはどうでしょう？"},{"id":{"d":492,"n":2,"t":1},"speaker":"B","text":"いいですね。ちょうど昨日、健太さんが揚げ物をおなかいっぱい食べたいと言っていました。私も唐揚げは大好きです。"},{"id":{"d":492,"n":3,"t":1},"speaker":"A","text":"1人200グラムくらいで足りるでしょうか？"},{"id":{"d":492,"n":4,"t":1},"speaker":"B","text":"健太さんは、300グラムくらい食べると思います。"},{"id":{"d":492,"n":5,"t":1},"speaker":"A","text":"それは、食欲旺盛ですね。それでは、多めに用意しましょう。何人集まりますか？"},{"id":{"d":492,"n":6,"t":1},"speaker":"B","text":"男女3人ずつで、合計6人です。"},{"id":{"d":492,"n":7,"t":1},"speaker":"A","text":"では、思い切って2キログラムくらい揚げましょうか？"},{"id":{"d":492,"n":8,"t":1},"speaker":"B","text":"それはいくらなんでも多すぎです。1キログラム半くらいで十分だと思います。"},{"id":{"d":492,"n":9,"t":1},"speaker":"A","text":"では、そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":493,"n":1,"t":1},"speaker":"A","text":"タマネギのみじん切りがうまくできません。"},{"id":{"d":493,"n":2,"t":1},"speaker":"B","text":"いつも、どうやって切っているのですか？"},{"id":{"d":493,"n":3,"t":1},"speaker":"A","text":"タマネギを半分に切って両端を切り落としたら、めったやたらに、ざくざくと切ります。"},{"id":{"d":493,"n":4,"t":1},"speaker":"B","text":"それでは、いくら時間をかけてもみじん切りになりませんよ。"},{"id":{"d":493,"n":5,"t":1},"speaker":"A","text":"どうするのですか？"},{"id":{"d":493,"n":6,"t":1},"speaker":"B","text":"根元を最後までつなげておくのです。まず、縦半分に切って、繊維に沿って、細かく切り込みを入れます。次に、横から水平に切り込みを入れます。最後に、繊維と直角に細かく切っていきます。"},{"id":{"d":493,"n":7,"t":1},"speaker":"A","text":"難しいですね。"},{"id":{"d":493,"n":8,"t":1},"speaker":"B","text":"この動画を見てください。このように、縦、水平、横、と切っていくのです。"},{"id":{"d":493,"n":9,"t":1},"speaker":"A","text":"ああ、そういうことですか。練習したらできそうです。ありがとう。"}];
INSERT INTO sentence [{"id":{"d":494,"n":1,"t":1},"speaker":"A","text":"おいしそうなお弁当ですね。お母さんに作ってもらったのですか？"},{"id":{"d":494,"n":2,"t":1},"speaker":"B","text":"いいえ。自分で作りました。由美さんのお弁当もおいしそうですね。彩りがきれいです。"},{"id":{"d":494,"n":3,"t":1},"speaker":"A","text":"ありがとう。なるべく、赤、緑、黄色を入れるようにしています。"},{"id":{"d":494,"n":4,"t":1},"speaker":"B","text":"私は、お肉があれば満足なので、いつも茶色いお弁当になってしまいます。"},{"id":{"d":494,"n":5,"t":1},"speaker":"A","text":"お弁当にお肉が入っていると元気が出ますよね。一番好きなおかずは何ですか？"},{"id":{"d":494,"n":6,"t":1},"speaker":"B","text":"一番好きなのは、ハンバーグです。"},{"id":{"d":494,"n":7,"t":1},"speaker":"A","text":"私もハンバーグが好きです。でも、おいしければおいしいほど焼き立てならもっとおいしいだろうと思ってしまい、少し残念な気持ちになってしまいます。"},{"id":{"d":494,"n":8,"t":1},"speaker":"B","text":"その気持ちは、分かります。お弁当だと、冷たくなってしまいますからね。"}];
INSERT INTO sentence [{"id":{"d":495,"n":1,"t":1},"speaker":"A","text":"すみません。この近くに郵便局はありますか？"},{"id":{"d":495,"n":2,"t":1},"speaker":"B","text":"郵便局ですか？一番近い郵便局は、この道を真っすぐ入ったとこにありますが、歩いて10分くらいかかります。"},{"id":{"d":495,"n":3,"t":1},"speaker":"A","text":"少し遠いですね。切手を買いたいのですが。"},{"id":{"d":495,"n":4,"t":1},"speaker":"B","text":"切手なら、コンビニエンスストアでも売っていますよ。郵便局より種類は、少ないですが。"},{"id":{"d":495,"n":5,"t":1},"speaker":"A","text":"そうなのですか。この手紙を出したいのです。"},{"id":{"d":495,"n":6,"t":1},"speaker":"B","text":"それなら、すぐ近くのコンビニエンスストアで買えると思います。あのお花屋さんの角を曲がって、3軒目くらいにあります。"},{"id":{"d":495,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。郵便ポストの場所も教えていただけますか？"},{"id":{"d":495,"n":8,"t":1},"speaker":"B","text":"コンビニエンスストアの目の前にありますよ。"},{"id":{"d":495,"n":9,"t":1},"speaker":"A","text":"それは助かります。ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":496,"n":1,"t":1},"speaker":"A","text":"今日の日替わりランチは何ですか？"},{"id":{"d":496,"n":2,"t":1},"speaker":"B","text":"すき焼き定食です。"},{"id":{"d":496,"n":3,"t":1},"speaker":"A","text":"それでは、それをお願いします。"},{"id":{"d":496,"n":4,"t":1},"speaker":"B","text":"お飲み物は、何になさいますか？"},{"id":{"d":496,"n":5,"t":1},"speaker":"A","text":"冷たい飲み物は、何がありますか？"},{"id":{"d":496,"n":6,"t":1},"speaker":"B","text":"アイスティー、アイスコーヒー、オレンジジュースがあります。"},{"id":{"d":496,"n":7,"t":1},"speaker":"A","text":"それでは、アイスティーをお願いします。"},{"id":{"d":496,"n":8,"t":1},"speaker":"B","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":497,"n":1,"t":1},"speaker":"A","text":"近々、引っ越しをしようと考えています。"},{"id":{"d":497,"n":2,"t":1},"speaker":"B","text":"今のお住まいは駅から5分で便利なのに、お引っ越しですか？"},{"id":{"d":497,"n":3,"t":1},"speaker":"A","text":"ワンルームマンションなので、キッチンが狭いのです。"},{"id":{"d":497,"n":4,"t":1},"speaker":"B","text":"お料理をなさるのですか？"},{"id":{"d":497,"n":5,"t":1},"speaker":"A","text":"はい。外食やコンビニ弁当ばかりだったのですが、飽きてしまって、最近では自炊しています。"},{"id":{"d":497,"n":6,"t":1},"speaker":"B","text":"自炊だと、買い物が大変ですね。"},{"id":{"d":497,"n":7,"t":1},"speaker":"A","text":"そうなのです。近くにスーパーマーケットがないのも、引っ越しをしたい理由の1つです。"},{"id":{"d":497,"n":8,"t":1},"speaker":"B","text":"引っ越し先は、夜遅くまで開いているスーパーマーケットの近くが良さそうですね。"}];
INSERT INTO sentence [{"id":{"d":498,"n":1,"t":1},"speaker":"A","text":"新米の季節ですね。"},{"id":{"d":498,"n":2,"t":1},"speaker":"B","text":"おにぎりが食べたくなりました。"},{"id":{"d":498,"n":3,"t":1},"speaker":"A","text":"おにぎりの具は何が好きですか？私は、かつお節のおにぎりが好きです。"},{"id":{"d":498,"n":4,"t":1},"speaker":"B","text":"かつお節は定番ですね。私は、梅干しのおにぎりを食べるとほっとします。"},{"id":{"d":498,"n":5,"t":1},"speaker":"A","text":"おにぎりの定番といえば、昆布もいいですね。"},{"id":{"d":498,"n":6,"t":1},"speaker":"B","text":"かつお節、梅干し、昆布は、おにぎりの基本ですね。"},{"id":{"d":498,"n":7,"t":1},"speaker":"A","text":"でも、お店で買うときは、めんたいこやツナを選ぶことがあります。"},{"id":{"d":498,"n":8,"t":1},"speaker":"B","text":"私も、梅干しおにぎり1つとツナおにぎり1つ、といった組み合わせで買うことが多いです。"},{"id":{"d":498,"n":9,"t":1},"speaker":"A","text":"奇遇ですね。私も同じ組み合わせでよく買っています。おにぎりの話をしていたら、おなかがすいてきました。"}];
INSERT INTO sentence [{"id":{"d":499,"n":1,"t":1},"speaker":"A","text":"昼食は何にしましょうか？"},{"id":{"d":499,"n":2,"t":1},"speaker":"B","text":"冷蔵庫に何がありますか？"},{"id":{"d":499,"n":3,"t":1},"speaker":"A","text":"キャベツと長イモと卵があります。それから、冷凍庫にブタ肉が少し残っているはずです。"},{"id":{"d":499,"n":4,"t":1},"speaker":"B","text":"お好み焼きは、どうでしょう？"},{"id":{"d":499,"n":5,"t":1},"speaker":"A","text":"いいですね。かつお節や、青のりもあります。"},{"id":{"d":499,"n":6,"t":1},"speaker":"B","text":"小麦粉はありますか？"},{"id":{"d":499,"n":7,"t":1},"speaker":"A","text":"ああ、肝心の小麦粉が足りないかもしれません。"},{"id":{"d":499,"n":8,"t":1},"speaker":"B","text":"それでは、小麦粉を買ってきます。ついでに何か、買ってくるものはありますか？"},{"id":{"d":499,"n":9,"t":1},"speaker":"A","text":"おいしそうなお菓子があったら、買ってきてください。キャベツを刻んで、待っています。"}];
INSERT INTO sentence [{"id":{"d":500,"n":1,"t":1},"speaker":"A","text":"最近、水回りのリフォームをしました。"},{"id":{"d":500,"n":2,"t":1},"speaker":"B","text":"水回りですか？"},{"id":{"d":500,"n":3,"t":1},"speaker":"A","text":"台所、トイレ、お風呂の設備を新しくしたのです。"},{"id":{"d":500,"n":4,"t":1},"speaker":"B","text":"工事中は、仮住まいをしたのですか？"},{"id":{"d":500,"n":5,"t":1},"speaker":"A","text":"仮住まいはせずに済みました。台所が使えない日は外食を楽しみました。"},{"id":{"d":500,"n":6,"t":1},"speaker":"B","text":"それは良かったですね。"},{"id":{"d":500,"n":7,"t":1},"speaker":"A","text":"ついでに、ベランダに物干しを取り付けてもらいました。"},{"id":{"d":500,"n":8,"t":1},"speaker":"B","text":"私もリフォームを考えているのです。今度見せていただいてもよいですか？"},{"id":{"d":500,"n":9,"t":1},"speaker":"A","text":"どうぞ、遊びにいらしてください。"}];
INSERT INTO sentence [{"id":{"d":501,"n":1,"t":1},"speaker":"A","text":"大家さん、こんにちは。102号室の佐藤です。お世話になっています。"},{"id":{"d":501,"n":2,"t":1},"speaker":"B","text":"ああ、加藤さん。こんにちは。どうしましたか？"},{"id":{"d":501,"n":3,"t":1},"speaker":"A","text":"はい、それが、昨日から、お風呂のお湯が出なくなって、困っています。"},{"id":{"d":501,"n":4,"t":1},"speaker":"B","text":"それは大変です。すぐに、修理の人を呼びます。直るまで、よかったらうちのお風呂を使ってください。"},{"id":{"d":501,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。でも、大丈夫です。"},{"id":{"d":501,"n":6,"t":1},"speaker":"B","text":"遠慮しなくてもよいのですよ。"},{"id":{"d":501,"n":7,"t":1},"speaker":"A","text":"当面は、大学の体育館でシャワーを浴びて帰ることにします。"},{"id":{"d":501,"n":8,"t":1},"speaker":"B","text":"そうですか。なるべく早く直すようにしますね。"}];
INSERT INTO sentence [{"id":{"d":502,"n":1,"t":1},"speaker":"A","text":"佐藤君、1人暮らしは慣れましたか？"},{"id":{"d":502,"n":2,"t":1},"speaker":"B","text":"はい、先生。ごみの出し方など、勝手が分からないことが多かったのですが、だいぶ慣れてきました。"},{"id":{"d":502,"n":3,"t":1},"speaker":"A","text":"それはよかったです。食事はどうしていますか？"},{"id":{"d":502,"n":4,"t":1},"speaker":"B","text":"初めのうちは、はりきって自炊していたのですが、この頃はコンビニ弁当やインスタント食品が多くなってしまいました。"},{"id":{"d":502,"n":5,"t":1},"speaker":"A","text":"勉強や、課外活動が、忙しくなってきたのですね。"},{"id":{"d":502,"n":6,"t":1},"speaker":"B","text":"そうなのです。でも健康には良くありませんよね。気を付けようと思います。"}];
INSERT INTO sentence [{"id":{"d":503,"n":1,"t":1},"speaker":"A","text":"夏の昼食といえば、そうめんですね。"},{"id":{"d":503,"n":2,"t":1},"speaker":"B","text":"そうですね。そうめんは夏バテで食欲がないときでもつるつると食べやすいです。"},{"id":{"d":503,"n":3,"t":1},"speaker":"A","text":"暑い夏の救世主ですね。"},{"id":{"d":503,"n":4,"t":1},"speaker":"B","text":"はい。ネギやシソ、ミョウガなど、たくさんの薬味と一緒に食べるのが大好きです。"},{"id":{"d":503,"n":5,"t":1},"speaker":"A","text":"とてもおいしそうです。夏が待ち遠しいですね。"},{"id":{"d":503,"n":6,"t":1},"speaker":"B","text":"そういえば、天気予報によると、今週末は真夏日になるそうですよ。"},{"id":{"d":503,"n":7,"t":1},"speaker":"A","text":"本当ですか？それなら、週末の昼食はそうめんにしようと思います。"}];
INSERT INTO sentence [{"id":{"d":504,"n":1,"t":1},"speaker":"A","text":"3月だというのに、今日は寒いですね。"},{"id":{"d":504,"n":2,"t":1},"speaker":"B","text":"暖かくなったと思ったら、冬に逆戻りです。"},{"id":{"d":504,"n":3,"t":1},"speaker":"A","text":"それにしても、随分薄着ですね。"},{"id":{"d":504,"n":4,"t":1},"speaker":"B","text":"もうコートは着ないかと思って、クリーニングに出してしまったのです。"},{"id":{"d":504,"n":5,"t":1},"speaker":"A","text":"私も危うく、冬物を片付けるところでした。"},{"id":{"d":504,"n":6,"t":1},"speaker":"B","text":"暖かくなったり、寒くなったりで、なかなか衣替えができませんね。"},{"id":{"d":504,"n":7,"t":1},"speaker":"A","text":"本当ですね。デパートのショーウインドーは、すっかり春の装いなのに、まだまだ、防寒着が手放せません。"},{"id":{"d":504,"n":8,"t":1},"speaker":"B","text":"もう少しの辛抱ですね。"}];
INSERT INTO sentence [{"id":{"d":505,"n":1,"t":1},"speaker":"A","text":"和食はおいしくて健康的ですね。でも、作るとなると、献立を考えるのが大変ですね。"},{"id":{"d":505,"n":2,"t":1},"speaker":"B","text":"そうですね。それで、「まごわやさしい」という合言葉が提案されています。"},{"id":{"d":505,"n":3,"t":1},"speaker":"A","text":"「孫は優しい」にかけているのですか？"},{"id":{"d":505,"n":4,"t":1},"speaker":"B","text":"はい。覚えやすいですからね。体に優しい食材の、頭文字をつなげたものです。"},{"id":{"d":505,"n":5,"t":1},"speaker":"A","text":"どのような食材ですか？"},{"id":{"d":505,"n":6,"t":1},"speaker":"B","text":"マメ、ゴマ、わかめ、野菜、魚、シイタケ、イモ類、の7種類です。"},{"id":{"d":505,"n":7,"t":1},"speaker":"A","text":"なるほど、バランス良く食べると、体に良さそうですね。"},{"id":{"d":505,"n":8,"t":1},"speaker":"B","text":"献立を考えるときに便利です。足りない食材をおみそ汁の具材にするなど、献立のヒントになりますよ。"}];
INSERT INTO sentence [{"id":{"d":506,"n":1,"t":1},"speaker":"A","text":"卵料理は、何が好きですか？"},{"id":{"d":506,"n":2,"t":1},"speaker":"B","text":"一番好きなのは、卵焼きです。お弁当に卵焼きが入っていたら、それだけで幸せです。"},{"id":{"d":506,"n":3,"t":1},"speaker":"A","text":"卵焼きは、朝食にも、お弁当にもいいですね。"},{"id":{"d":506,"n":4,"t":1},"speaker":"B","text":"朝食といえば、パン食の時は、スクランブルエッグやオムレツをよく食べます。"},{"id":{"d":506,"n":5,"t":1},"speaker":"A","text":"ゆで卵や、目玉焼きは、食べませんか？"},{"id":{"d":506,"n":6,"t":1},"speaker":"B","text":"あまり食べません。嫌いというわけでは、ないのですが。"},{"id":{"d":506,"n":7,"t":1},"speaker":"A","text":"黄身と、白身が、混ざっている方が好きなのですか？"},{"id":{"d":506,"n":8,"t":1},"speaker":"B","text":"そういえば、そうですね。"}];
INSERT INTO sentence [{"id":{"d":507,"n":1,"t":1},"speaker":"A","text":"今朝、地震があったそうですね。"},{"id":{"d":507,"n":2,"t":1},"speaker":"B","text":"はい。気付きませんでしたか？"},{"id":{"d":507,"n":3,"t":1},"speaker":"A","text":"私は家にいたのですが、気付きませんでした。後からテレビ番組のニュースで知りました。"},{"id":{"d":507,"n":4,"t":1},"speaker":"B","text":"大した揺れではなかったので、私も家にいたら気付かなかったかもしれません。"},{"id":{"d":507,"n":5,"t":1},"speaker":"A","text":"どうして気が付いたのですか？"},{"id":{"d":507,"n":6,"t":1},"speaker":"B","text":"電車に乗っていたら、あちこちでスマホのアラームが、けたたましく鳴ったからです。"},{"id":{"d":507,"n":7,"t":1},"speaker":"A","text":"地震の時は、大きな音で知らせてくれますね。"},{"id":{"d":507,"n":8,"t":1},"speaker":"B","text":"はい。それが電車の中で一斉に鳴ったので、びっくりしました。"},{"id":{"d":507,"n":9,"t":1},"speaker":"A","text":"あれは、何度聞いても、びっくりしますね。"}];
INSERT INTO sentence [{"id":{"d":508,"n":1,"t":1},"speaker":"A","text":"サンドイッチの具は、何が好きですか？"},{"id":{"d":508,"n":2,"t":1},"speaker":"B","text":"好きな具材はたくさんありますが、1つだけ選べと言われたら、卵サンドです。"},{"id":{"d":508,"n":3,"t":1},"speaker":"A","text":"卵サンドは鉄板ですね。コンビニエンスストアでも力を入れていますよね。"},{"id":{"d":508,"n":4,"t":1},"speaker":"B","text":"この間、厚焼き卵のサンドイッチというのがあって、食べてみたらおいしかったです。"},{"id":{"d":508,"n":5,"t":1},"speaker":"A","text":"サンドイッチに分厚い厚焼き卵が入っているなんて、想像するだけでも、ごちそう感がありますね。"},{"id":{"d":508,"n":6,"t":1},"speaker":"B","text":"はい。つい、手が伸びてしまいました。おいしくて、食べ応えもありました。"},{"id":{"d":508,"n":7,"t":1},"speaker":"A","text":"それは、一度、食べてみる価値はありそうです。自分で作るなら、どのような卵サンドが好きですか？"},{"id":{"d":508,"n":8,"t":1},"speaker":"B","text":"自分で作るなら、レンチン卵サンドです。生卵と牛乳と塩を混ぜて、電子レンジで加熱した後、マヨネーズであえて食パンに挟むだけですが、十分おいしいです。"}];
INSERT INTO sentence [{"id":{"d":509,"n":1,"t":1},"speaker":"A","text":"お客さま、何かお探しですか？"},{"id":{"d":509,"n":2,"t":1},"speaker":"B","text":"しょうゆを探しています。"},{"id":{"d":509,"n":3,"t":1},"speaker":"A","text":"向こうの棚にありますので、ご案内します。"},{"id":{"d":509,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":509,"n":5,"t":1},"speaker":"A","text":"こちらにあります。"},{"id":{"d":509,"n":6,"t":1},"speaker":"B","text":"減塩タイプのものを買いたいのですが、どれでしょうか？"},{"id":{"d":509,"n":7,"t":1},"speaker":"A","text":"それですと、あちらに置いてあります。"},{"id":{"d":509,"n":8,"t":1},"speaker":"B","text":"あそこですね。分かりました。"}];
INSERT INTO sentence [{"id":{"d":510,"n":1,"t":1},"speaker":"A","text":"もしもし、優一ですか？"},{"id":{"d":510,"n":2,"t":1},"speaker":"B","text":"もしもし、優一です。お母さん、どうしたのですか？"},{"id":{"d":510,"n":3,"t":1},"speaker":"A","text":"今、雨が降ってきたのですが、そちらでも降っていますか？"},{"id":{"d":510,"n":4,"t":1},"speaker":"B","text":"外を見てみますね。こちらでも降っています。"},{"id":{"d":510,"n":5,"t":1},"speaker":"A","text":"洗濯物を干してあるので、取り込んでおいてくれますか？"},{"id":{"d":510,"n":6,"t":1},"speaker":"B","text":"はい。すぐに取り込みます。"},{"id":{"d":510,"n":7,"t":1},"speaker":"A","text":"助かります。私ももうすぐ帰りますね。"},{"id":{"d":510,"n":8,"t":1},"speaker":"B","text":"気を付けて帰ってきてください。"}];
INSERT INTO sentence [{"id":{"d":511,"n":1,"t":1},"speaker":"A","text":"お風呂に入ってきます。"},{"id":{"d":511,"n":2,"t":1},"speaker":"B","text":"私も入ろうと思っていたところです。"},{"id":{"d":511,"n":3,"t":1},"speaker":"A","text":"そうでしたか。ではお先にどうぞ。"},{"id":{"d":511,"n":4,"t":1},"speaker":"B","text":"いいえ、私は長湯なので、後でよいです。"},{"id":{"d":511,"n":5,"t":1},"speaker":"A","text":"そうですか。私はすぐに終わりますので、先に入らせてもらいますね。"},{"id":{"d":511,"n":6,"t":1},"speaker":"B","text":"どうぞ。"},{"id":{"d":511,"n":7,"t":1},"speaker":"A","text":"私が出たら、声を掛けますね。"},{"id":{"d":511,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。自分の部屋にいますので、お願いします。"}];
INSERT INTO sentence [{"id":{"d":512,"n":1,"t":1},"speaker":"A","text":"こんにちは。お久しぶりですね。"},{"id":{"d":512,"n":2,"t":1},"speaker":"B","text":"こんにちは。ご無沙汰しています。"},{"id":{"d":512,"n":3,"t":1},"speaker":"A","text":"息子さんはおいくつになりましたか？"},{"id":{"d":512,"n":4,"t":1},"speaker":"B","text":"6歳です。今年の4月に小学校に入学しました。"},{"id":{"d":512,"n":5,"t":1},"speaker":"A","text":"それほど大きくなりましたか。"},{"id":{"d":512,"n":6,"t":1},"speaker":"B","text":"確か、最後にお会いした時、息子は年少でした。"},{"id":{"d":512,"n":7,"t":1},"speaker":"A","text":"そうでした。幼稚園の制服を着ていたのを覚えています。"},{"id":{"d":512,"n":8,"t":1},"speaker":"B","text":"月日がたつのは早いですね。"},{"id":{"d":512,"n":9,"t":1},"speaker":"A","text":"本当ですね。今度機会があったら息子さんにもお会いしたいです。"}];
INSERT INTO sentence [{"id":{"d":513,"n":1,"t":1},"speaker":"A","text":"ママ、今日のおやつは何ですか？"},{"id":{"d":513,"n":2,"t":1},"speaker":"B","text":"プリンです。"},{"id":{"d":513,"n":3,"t":1},"speaker":"A","text":"うれしいです。冷蔵庫に入っていますか？"},{"id":{"d":513,"n":4,"t":1},"speaker":"B","text":"はい。冷蔵庫から出して食べてよいですよ。"},{"id":{"d":513,"n":5,"t":1},"speaker":"A","text":"見当たりません。"},{"id":{"d":513,"n":6,"t":1},"speaker":"B","text":"奥にありますよ。よく見てください。"},{"id":{"d":513,"n":7,"t":1},"speaker":"A","text":"ありました。おいしそうです。"},{"id":{"d":513,"n":8,"t":1},"speaker":"B","text":"食べる前に手を洗ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":514,"n":1,"t":1},"speaker":"A","text":"朝ですよ、起きてください。"},{"id":{"d":514,"n":2,"t":1},"speaker":"B","text":"もう朝ですか。眠いです。"},{"id":{"d":514,"n":3,"t":1},"speaker":"A","text":"昨日は眠るのが遅かったのですか？"},{"id":{"d":514,"n":4,"t":1},"speaker":"B","text":"はい。課題が終わらなくて、夜中までやっていたのです。"},{"id":{"d":514,"n":5,"t":1},"speaker":"A","text":"それは大変でしたね。課題は終わりましたか？"},{"id":{"d":514,"n":6,"t":1},"speaker":"B","text":"何とか終わりましたが、寝不足です。"},{"id":{"d":514,"n":7,"t":1},"speaker":"A","text":"でも、遅くなると遅刻してしまうので起きてください。"},{"id":{"d":514,"n":8,"t":1},"speaker":"B","text":"そうですよね。頑張って起きます。"}];
INSERT INTO sentence [{"id":{"d":515,"n":1,"t":1},"speaker":"A","text":"これは何ですか？"},{"id":{"d":515,"n":2,"t":1},"speaker":"B","text":"家庭用のたこ焼き器です。"},{"id":{"d":515,"n":3,"t":1},"speaker":"A","text":"家でたこ焼きが作れるのですか？"},{"id":{"d":515,"n":4,"t":1},"speaker":"B","text":"そうです。初めて見ましたか？"},{"id":{"d":515,"n":5,"t":1},"speaker":"A","text":"はい。どうやって作るのですか？"},{"id":{"d":515,"n":6,"t":1},"speaker":"B","text":"材料を用意して、実際に作ってみましょうか？"},{"id":{"d":515,"n":7,"t":1},"speaker":"A","text":"楽しみです。足りないものがあれば、買ってきます。"},{"id":{"d":515,"n":8,"t":1},"speaker":"B","text":"必要な材料を確認するので、少し待ってくださいね。"}];
INSERT INTO sentence [{"id":{"d":516,"n":1,"t":1},"speaker":"A","text":"目玉焼きを作りますが、焼き加減はどうしますか？"},{"id":{"d":516,"n":2,"t":1},"speaker":"B","text":"半熟でお願いします。"},{"id":{"d":516,"n":3,"t":1},"speaker":"A","text":"分かりました。私も半熟が好きです。"},{"id":{"d":516,"n":4,"t":1},"speaker":"B","text":"おいしいですよね。"},{"id":{"d":516,"n":5,"t":1},"speaker":"A","text":"調味料は何が良いですか？"},{"id":{"d":516,"n":6,"t":1},"speaker":"B","text":"塩とこしょうが良いです。あなたは何にしますか？"},{"id":{"d":516,"n":7,"t":1},"speaker":"A","text":"私はしょうゆをかけたいので、それぞれのお皿に取り分けてから味付けしましょう。"},{"id":{"d":516,"n":8,"t":1},"speaker":"B","text":"そうですね。お皿を持ってきます。"}];
INSERT INTO sentence [{"id":{"d":517,"n":1,"t":1},"speaker":"A","text":"手分けして食事の後片付けをしましょう。"},{"id":{"d":517,"n":2,"t":1},"speaker":"B","text":"そうですね。食器を洗うのと拭くのはどちらがいいですか？"},{"id":{"d":517,"n":3,"t":1},"speaker":"A","text":"私は洗う方が好きです。"},{"id":{"d":517,"n":4,"t":1},"speaker":"B","text":"それでは、私が拭いて食器棚へしまいますね。"},{"id":{"d":517,"n":5,"t":1},"speaker":"A","text":"はい。まずは食器を下げましょうか？"},{"id":{"d":517,"n":6,"t":1},"speaker":"B","text":"そうですね。お盆を持ってきます。"},{"id":{"d":517,"n":7,"t":1},"speaker":"A","text":"私はテーブルを拭くものを取ってきます。"},{"id":{"d":517,"n":8,"t":1},"speaker":"B","text":"よろしくお願いします。協力すれば、早く終わりそうですね。"}];
INSERT INTO sentence [{"id":{"d":518,"n":1,"t":1},"speaker":"A","text":"たまには外食をしましょう。"},{"id":{"d":518,"n":2,"t":1},"speaker":"B","text":"いいですね。"},{"id":{"d":518,"n":3,"t":1},"speaker":"A","text":"何か食べたいものはありますか？"},{"id":{"d":518,"n":4,"t":1},"speaker":"B","text":"久しぶりにおすしが食べたいです。"},{"id":{"d":518,"n":5,"t":1},"speaker":"A","text":"回転ずしでよいですか？"},{"id":{"d":518,"n":6,"t":1},"speaker":"B","text":"もちろん、よいですよ。"},{"id":{"d":518,"n":7,"t":1},"speaker":"A","text":"では、6時頃に出かけましょう。"},{"id":{"d":518,"n":8,"t":1},"speaker":"B","text":"分かりました。それまでに身支度を整えておきます。"}];
