INSERT INTO sentence [{"id":{"d":635,"n":1,"t":1},"speaker":"A","text":"私は異動することになりました。"},{"id":{"d":635,"n":2,"t":1},"speaker":"B","text":"いつですか？"},{"id":{"d":635,"n":3,"t":1},"speaker":"A","text":"来月です。"},{"id":{"d":635,"n":4,"t":1},"speaker":"B","text":"急ですね。寂しくなります。"},{"id":{"d":635,"n":5,"t":1},"speaker":"A","text":"木山さんにはお世話になりました。"},{"id":{"d":635,"n":6,"t":1},"speaker":"B","text":"どの部署へ異動するのですか？"},{"id":{"d":635,"n":7,"t":1},"speaker":"A","text":"経営企画部です。"},{"id":{"d":635,"n":8,"t":1},"speaker":"B","text":"それでは、同じビルですね。これからも一緒にお昼ご飯を食べたりできますね。"}];
INSERT INTO sentence [{"id":{"d":636,"n":1,"t":1},"speaker":"A","text":"何歳で結婚したいですか？"},{"id":{"d":636,"n":2,"t":1},"speaker":"B","text":"私は若いうちにしたいです。"},{"id":{"d":636,"n":3,"t":1},"speaker":"A","text":"具体的には何歳くらいですか？"},{"id":{"d":636,"n":4,"t":1},"speaker":"B","text":"20代前半ですね。"},{"id":{"d":636,"n":5,"t":1},"speaker":"A","text":"どうして若いうちに結婚したいのですか？"},{"id":{"d":636,"n":6,"t":1},"speaker":"B","text":"子どもを早く産んで、若いお母さんになりたいのです。"},{"id":{"d":636,"n":7,"t":1},"speaker":"A","text":"結婚相手も若い方がいいですか？"},{"id":{"d":636,"n":8,"t":1},"speaker":"B","text":"そうですね。できれば若いお父さんが良いなと思っています。"}];
INSERT INTO sentence [{"id":{"d":637,"n":1,"t":1},"speaker":"A","text":"今日は眼鏡をかけていませんね。"},{"id":{"d":637,"n":2,"t":1},"speaker":"B","text":"実は眼鏡をやめてコンタクトレンズにしたのです。"},{"id":{"d":637,"n":3,"t":1},"speaker":"A","text":"そうでしたか。ずっと眼鏡だったのに、どうしたのですか？"},{"id":{"d":637,"n":4,"t":1},"speaker":"B","text":"イメージチェンジしたかっただけで、特に理由はありません。"},{"id":{"d":637,"n":5,"t":1},"speaker":"A","text":"確かにイメージがとても変わりました。"},{"id":{"d":637,"n":6,"t":1},"speaker":"B","text":"何だか恥ずかしいです。"},{"id":{"d":637,"n":7,"t":1},"speaker":"A","text":"私は今の方がいいと思いますよ。コンタクトレンズには慣れましたか？"},{"id":{"d":637,"n":8,"t":1},"speaker":"B","text":"はい。もう慣れました。"}];
INSERT INTO sentence [{"id":{"d":638,"n":1,"t":1},"speaker":"A","text":"ご出身はどちらですか？"},{"id":{"d":638,"n":2,"t":1},"speaker":"B","text":"北海道です。"},{"id":{"d":638,"n":3,"t":1},"speaker":"A","text":"いつ頃まで北海道で暮らしていたのですか？"},{"id":{"d":638,"n":4,"t":1},"speaker":"B","text":"高校までです。大学が東京にあったので、それからは関東で暮らしています。"},{"id":{"d":638,"n":5,"t":1},"speaker":"A","text":"北海道にある大学には進学しようと思わなかったのですか？"},{"id":{"d":638,"n":6,"t":1},"speaker":"B","text":"生まれてからずっと北海道だったので、東京に出て行きたい気持ちが強かったです。"},{"id":{"d":638,"n":7,"t":1},"speaker":"A","text":"実際に東京で暮らしてみて、いかがでしたか？"},{"id":{"d":638,"n":8,"t":1},"speaker":"B","text":"楽しいし便利ですが、たまに自然が恋しくなりますね。"}];
INSERT INTO sentence [{"id":{"d":639,"n":1,"t":1},"speaker":"A","text":"うちの和室がフローリングならよかったですね。"},{"id":{"d":639,"n":2,"t":1},"speaker":"B","text":"どうしてですか？"},{"id":{"d":639,"n":3,"t":1},"speaker":"A","text":"畳は傷むのが早いですし、雰囲気の統一感がなくなってしまいます。"},{"id":{"d":639,"n":4,"t":1},"speaker":"B","text":"リフォームして、和室を洋室に変えることができますよ。"},{"id":{"d":639,"n":5,"t":1},"speaker":"A","text":"それはいいですね。"},{"id":{"d":639,"n":6,"t":1},"speaker":"B","text":"リフォーム会社に費用や工事にかかる日程などを聞いてみましょうか？"},{"id":{"d":639,"n":7,"t":1},"speaker":"A","text":"はい。詳しい話を聞きたいです。"},{"id":{"d":639,"n":8,"t":1},"speaker":"B","text":"それでは、リフォーム会社に問い合わせをしてみます。"}];
INSERT INTO sentence [{"id":{"d":640,"n":1,"t":1},"speaker":"A","text":"会社の歓迎会の幹事になりました。"},{"id":{"d":640,"n":2,"t":1},"speaker":"B","text":"誰かが異動してきたのですか？"},{"id":{"d":640,"n":3,"t":1},"speaker":"A","text":"はい。大阪支店から1人異動してきました。"},{"id":{"d":640,"n":4,"t":1},"speaker":"B","text":"その人の好みのお店で歓迎会をできるといいですね。"},{"id":{"d":640,"n":5,"t":1},"speaker":"A","text":"そうですね。どのようなジャンルの料理が好きか、それとなく聞いてみます。"},{"id":{"d":640,"n":6,"t":1},"speaker":"B","text":"歓迎会だけですか？プレゼントの準備などは必要ありませんか？"},{"id":{"d":640,"n":7,"t":1},"speaker":"A","text":"プレゼントも用意します。何をプレゼントするかも、幹事に任されています。"},{"id":{"d":640,"n":8,"t":1},"speaker":"B","text":"何を贈るか悩んでしまいますね。"}];
INSERT INTO sentence [{"id":{"d":641,"n":1,"t":1},"speaker":"A","text":"食器洗い洗浄機はどこに売っていますか？"},{"id":{"d":641,"n":2,"t":1},"speaker":"B","text":"こちらになります。"},{"id":{"d":641,"n":3,"t":1},"speaker":"A","text":"4人家族なのですが、どの機種が良いでしょうか？"},{"id":{"d":641,"n":4,"t":1},"speaker":"B","text":"4人用ですと、こちらとその隣にあるものがちょうど良いと思います。"},{"id":{"d":641,"n":5,"t":1},"speaker":"A","text":"洗剤はどこかに入れておくのですか？"},{"id":{"d":641,"n":6,"t":1},"speaker":"B","text":"はい。投入口がありますので、あらかじめ入れておく必要があります。"},{"id":{"d":641,"n":7,"t":1},"speaker":"A","text":"洗う時間が短いのはどちらでしょうか？"},{"id":{"d":641,"n":8,"t":1},"speaker":"B","text":"こちらの方が短時間で洗浄できます。"}];
INSERT INTO sentence [{"id":{"d":642,"n":1,"t":1},"speaker":"A","text":"あの、突然すみません。"},{"id":{"d":642,"n":2,"t":1},"speaker":"B","text":"はい。何でしょうか？"},{"id":{"d":642,"n":3,"t":1},"speaker":"A","text":"以前、森川商事の徳島支店にいらっしゃった米山さんではありませんか？"},{"id":{"d":642,"n":4,"t":1},"speaker":"B","text":"はい。米山です。"},{"id":{"d":642,"n":5,"t":1},"speaker":"A","text":"私は経理部でお世話になった南です。"},{"id":{"d":642,"n":6,"t":1},"speaker":"B","text":"南さんですか。思い出しました。久しぶりですね。"},{"id":{"d":642,"n":7,"t":1},"speaker":"A","text":"本当にお久しぶりです。覚えていてくださっていてうれしいです。"},{"id":{"d":642,"n":8,"t":1},"speaker":"B","text":"今はどうされているのですか？"},{"id":{"d":642,"n":9,"t":1},"speaker":"A","text":"転職して、香川県で暮らしています。"}];
INSERT INTO sentence [{"id":{"d":643,"n":1,"t":1},"speaker":"A","text":"リビングルームの壁紙を張り替えたいです。"},{"id":{"d":643,"n":2,"t":1},"speaker":"B","text":"業者に依頼しますか？"},{"id":{"d":643,"n":3,"t":1},"speaker":"A","text":"いいえ、自分でやってみようかと。素人でもできるようですよ。"},{"id":{"d":643,"n":4,"t":1},"speaker":"B","text":"そうなのですか。どのような壁紙に替えたいですか？"},{"id":{"d":643,"n":5,"t":1},"speaker":"A","text":"今は白なので、色味があるものがいいです。"},{"id":{"d":643,"n":6,"t":1},"speaker":"B","text":"柄があってもいいですね。"},{"id":{"d":643,"n":7,"t":1},"speaker":"A","text":"そうですね。明るい雰囲気にしたいです。"},{"id":{"d":643,"n":8,"t":1},"speaker":"B","text":"どのような壁紙があるか、インターネットで見てみましょう。"}];
INSERT INTO sentence [{"id":{"d":644,"n":1,"t":1},"speaker":"A","text":"このキムチはおいしいですね。"},{"id":{"d":644,"n":2,"t":1},"speaker":"B","text":"残念ながら、私は食べられません。"},{"id":{"d":644,"n":3,"t":1},"speaker":"A","text":"なぜですか？"},{"id":{"d":644,"n":4,"t":1},"speaker":"B","text":"辛い物に弱いのです。"},{"id":{"d":644,"n":5,"t":1},"speaker":"A","text":"このキムチはそれほど辛くありませんよ。一口いかがですか？"},{"id":{"d":644,"n":6,"t":1},"speaker":"B","text":"私は本当に弱いので、結構です。"},{"id":{"d":644,"n":7,"t":1},"speaker":"A","text":"唐辛子が苦手なのですね。"},{"id":{"d":644,"n":8,"t":1},"speaker":"B","text":"はい。できれば克服したいです。"}];
INSERT INTO sentence [{"id":{"d":645,"n":1,"t":1},"speaker":"A","text":"まずは何を食べますか？"},{"id":{"d":645,"n":2,"t":1},"speaker":"B","text":"イカが食べたいです。"},{"id":{"d":645,"n":3,"t":1},"speaker":"A","text":"なかなか回ってきませんね。"},{"id":{"d":645,"n":4,"t":1},"speaker":"B","text":"あなたは何を食べたいですか？"},{"id":{"d":645,"n":5,"t":1},"speaker":"A","text":"中トロです。"},{"id":{"d":645,"n":6,"t":1},"speaker":"B","text":"中トロが来ましたよ。取りましょうか？"},{"id":{"d":645,"n":7,"t":1},"speaker":"A","text":"お願いします。"},{"id":{"d":645,"n":8,"t":1},"speaker":"B","text":"取れました。どうぞ。"}];
INSERT INTO sentence [{"id":{"d":646,"n":1,"t":1},"speaker":"A","text":"暑いので、かき氷を作りましょう。"},{"id":{"d":646,"n":2,"t":1},"speaker":"B","text":"家でかき氷を作れるのですか？"},{"id":{"d":646,"n":3,"t":1},"speaker":"A","text":"はい。かき氷機があります。"},{"id":{"d":646,"n":4,"t":1},"speaker":"B","text":"そうなのですか。作りたいです。"},{"id":{"d":646,"n":5,"t":1},"speaker":"A","text":"これがかき氷機です。"},{"id":{"d":646,"n":6,"t":1},"speaker":"B","text":"どうやって使うのですか？"},{"id":{"d":646,"n":7,"t":1},"speaker":"A","text":"上が開くので、氷を入れて、ボタンを押します。"},{"id":{"d":646,"n":8,"t":1},"speaker":"B","text":"自動でかき氷を作れるのですね。驚きました。"}];
INSERT INTO sentence [{"id":{"d":647,"n":1,"t":1},"speaker":"A","text":"このワインの産地はどこですか？"},{"id":{"d":647,"n":2,"t":1},"speaker":"B","text":"チリのワインです。"},{"id":{"d":647,"n":3,"t":1},"speaker":"A","text":"飲みやすくてとてもおいしいです。"},{"id":{"d":647,"n":4,"t":1},"speaker":"B","text":"私も気に入っていて、よく飲んでいます。"},{"id":{"d":647,"n":5,"t":1},"speaker":"A","text":"高級なワインなのでしょうか？"},{"id":{"d":647,"n":6,"t":1},"speaker":"B","text":"いえ、手軽に買える金額ですよ。"},{"id":{"d":647,"n":7,"t":1},"speaker":"A","text":"それはいいですね。専門店でなくても買えるでしょうか？"},{"id":{"d":647,"n":8,"t":1},"speaker":"B","text":"スーパーマーケットでも売っていますよ。私も近くのショッピングモールで買いました。"}];
INSERT INTO sentence [{"id":{"d":648,"n":1,"t":1},"speaker":"A","text":"今日の夕食はお鍋です。"},{"id":{"d":648,"n":2,"t":1},"speaker":"B","text":"お鍋は大好きなのでうれしいです。何鍋ですか？"},{"id":{"d":648,"n":3,"t":1},"speaker":"A","text":"それを相談しようと思っていました。どのような鍋がいいですか？"},{"id":{"d":648,"n":4,"t":1},"speaker":"B","text":"この前水炊きをしたので、それ以外がいいです。"},{"id":{"d":648,"n":5,"t":1},"speaker":"A","text":"その前は、豆乳鍋をしましたね。"},{"id":{"d":648,"n":6,"t":1},"speaker":"B","text":"キムチ鍋はどうですか？"},{"id":{"d":648,"n":7,"t":1},"speaker":"A","text":"最近しばらく食べていないので、それは良い案です。"},{"id":{"d":648,"n":8,"t":1},"speaker":"B","text":"材料はそろっていますか？"},{"id":{"d":648,"n":9,"t":1},"speaker":"A","text":"大丈夫です。食べるのが楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":649,"n":1,"t":1},"speaker":"A","text":"ズボンの裾がほつれてしまいました。"},{"id":{"d":649,"n":2,"t":1},"speaker":"B","text":"擦れてしまったようですね。"},{"id":{"d":649,"n":3,"t":1},"speaker":"A","text":"何とかなりませんか？"},{"id":{"d":649,"n":4,"t":1},"speaker":"B","text":"裾上げのようにしてまつり縫いすれば、見栄えはよくなると思います。"},{"id":{"d":649,"n":5,"t":1},"speaker":"A","text":"なるほど。そのようにしてみます。"},{"id":{"d":649,"n":6,"t":1},"speaker":"B","text":"ほつれたのを直してでも、そのズボンをはきたいのですね。"},{"id":{"d":649,"n":7,"t":1},"speaker":"A","text":"はい。気に入っていて、手放す気になれません。"},{"id":{"d":649,"n":8,"t":1},"speaker":"B","text":"物を大切にする気持ちを見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":650,"n":1,"t":1},"speaker":"A","text":"そろそろ行きましょうか？"},{"id":{"d":650,"n":2,"t":1},"speaker":"B","text":"そうですね。帰りましょう。"},{"id":{"d":650,"n":3,"t":1},"speaker":"A","text":"伝票がありません。"},{"id":{"d":650,"n":4,"t":1},"speaker":"B","text":"私が持っています。今日は私が払いますよ。"},{"id":{"d":650,"n":5,"t":1},"speaker":"A","text":"とんでもありません。私も払います。"},{"id":{"d":650,"n":6,"t":1},"speaker":"B","text":"私が誘ったので、今日は出させてください。"},{"id":{"d":650,"n":7,"t":1},"speaker":"A","text":"本当によいのですか？何だか申し訳ありません。"},{"id":{"d":650,"n":8,"t":1},"speaker":"B","text":"私が払うつもりでお誘いしたので、気にしないでください。では行きましょう。"}];
INSERT INTO sentence [{"id":{"d":651,"n":1,"t":1},"speaker":"A","text":"ネックレスをして出かけようと思うのですが、どちらが良いか迷っています。"},{"id":{"d":651,"n":2,"t":1},"speaker":"B","text":"どれとどれですか？"},{"id":{"d":651,"n":3,"t":1},"speaker":"A","text":"この2つです。"},{"id":{"d":651,"n":4,"t":1},"speaker":"B","text":"一つずつ、着けてみてください。"},{"id":{"d":651,"n":5,"t":1},"speaker":"A","text":"1つ目がこれです。"},{"id":{"d":651,"n":6,"t":1},"speaker":"B","text":"かわいいですね。"},{"id":{"d":651,"n":7,"t":1},"speaker":"A","text":"もう一つがこれです。"},{"id":{"d":651,"n":8,"t":1},"speaker":"B","text":"これもいいですが、私は最初の方が今日の服に合っていると思います。"},{"id":{"d":651,"n":9,"t":1},"speaker":"A","text":"それでは1つ目の方を着けていきます。"}];
INSERT INTO sentence [{"id":{"d":652,"n":1,"t":1},"speaker":"A","text":"もうすぐボーナスが出ますね。"},{"id":{"d":652,"n":2,"t":1},"speaker":"B","text":"はい。待ち遠しいです。"},{"id":{"d":652,"n":3,"t":1},"speaker":"A","text":"ボーナスが出たら、何か買いますか？"},{"id":{"d":652,"n":4,"t":1},"speaker":"B","text":"新しいバッグを買う予定です。"},{"id":{"d":652,"n":5,"t":1},"speaker":"A","text":"どのバッグにするか、もう決めているのですか？"},{"id":{"d":652,"n":6,"t":1},"speaker":"B","text":"2つに絞っていて、どちらにするか悩んでいます。"},{"id":{"d":652,"n":7,"t":1},"speaker":"A","text":"悩む時間も楽しいですよね。"},{"id":{"d":652,"n":8,"t":1},"speaker":"B","text":"その通りです。お店で見比べることが好きです。"}];
INSERT INTO sentence [{"id":{"d":653,"n":1,"t":1},"speaker":"A","text":"おいしそうなお菓子ですね。"},{"id":{"d":653,"n":2,"t":1},"speaker":"B","text":"コンビニエンスストアの新作のお菓子です。"},{"id":{"d":653,"n":3,"t":1},"speaker":"A","text":"そういえば、CMで見た気がします。"},{"id":{"d":653,"n":4,"t":1},"speaker":"B","text":"私もCMで知って、食べたくなって買ってきました。"},{"id":{"d":653,"n":5,"t":1},"speaker":"A","text":"コンビニエンスストアのお菓子はおいしいものが多いですよね。"},{"id":{"d":653,"n":6,"t":1},"speaker":"B","text":"一口いかがですか？"},{"id":{"d":653,"n":7,"t":1},"speaker":"A","text":"頂いてよいのですか？"},{"id":{"d":653,"n":8,"t":1},"speaker":"B","text":"はい。どうぞ、お召し上がりください。"},{"id":{"d":653,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。見た目以上においしいですね。"}];
INSERT INTO sentence [{"id":{"d":654,"n":1,"t":1},"speaker":"A","text":"勉強ですか？"},{"id":{"d":654,"n":2,"t":1},"speaker":"B","text":"仕事に必要な資格があって、その試験日が近づいているのです。"},{"id":{"d":654,"n":3,"t":1},"speaker":"A","text":"難しい試験なのですか？"},{"id":{"d":654,"n":4,"t":1},"speaker":"B","text":"合格率は15パーセントくらいです。"},{"id":{"d":654,"n":5,"t":1},"speaker":"A","text":"それは低いですね。"},{"id":{"d":654,"n":6,"t":1},"speaker":"B","text":"もっと早くから勉強しておけばよかったです。"},{"id":{"d":654,"n":7,"t":1},"speaker":"A","text":"仕事が忙しかったようですし、仕方がありませんよ。"},{"id":{"d":654,"n":8,"t":1},"speaker":"B","text":"今日からは毎日最低でも3時間は勉強します。"}];
INSERT INTO sentence [{"id":{"d":655,"n":1,"t":1},"speaker":"A","text":"あなたにとってのおふくろの味といえば何ですか？"},{"id":{"d":655,"n":2,"t":1},"speaker":"B","text":"肉じゃがだと思います。"},{"id":{"d":655,"n":3,"t":1},"speaker":"A","text":"家庭料理の定番ですね。"},{"id":{"d":655,"n":4,"t":1},"speaker":"B","text":"はい。母の肉じゃがは本当においしいです。"},{"id":{"d":655,"n":5,"t":1},"speaker":"A","text":"私はきんぴらごぼうです。"},{"id":{"d":655,"n":6,"t":1},"speaker":"B","text":"やはりお母さんのきんぴらごぼうが一番ですか？"},{"id":{"d":655,"n":7,"t":1},"speaker":"A","text":"はい。外食できんぴらごぼうが出ても、母にはかないません。"},{"id":{"d":655,"n":8,"t":1},"speaker":"B","text":"話していたら、食べたくなってきました。"},{"id":{"d":655,"n":9,"t":1},"speaker":"A","text":"私もそうです。実家に帰りたくなりますね。"}];
INSERT INTO sentence [{"id":{"d":656,"n":1,"t":1},"speaker":"A","text":"ご注文はお決まりですか？"},{"id":{"d":656,"n":2,"t":1},"speaker":"B","text":"エビドリアをお願いします。"},{"id":{"d":656,"n":3,"t":1},"speaker":"A","text":"単品でよろしいですか？"},{"id":{"d":656,"n":4,"t":1},"speaker":"B","text":"いえ、サラダセットにします。"},{"id":{"d":656,"n":5,"t":1},"speaker":"A","text":"サラダセットですね。お飲み物はいかがなさいますか？"},{"id":{"d":656,"n":6,"t":1},"speaker":"B","text":"アイスコーヒーをください。"},{"id":{"d":656,"n":7,"t":1},"speaker":"A","text":"お飲み物はいつお持ちしますか？"},{"id":{"d":656,"n":8,"t":1},"speaker":"B","text":"食後にお願いします。"},{"id":{"d":656,"n":9,"t":1},"speaker":"A","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":657,"n":1,"t":1},"speaker":"A","text":"ごちそうさまでした。おいしかったですね。"},{"id":{"d":657,"n":2,"t":1},"speaker":"B","text":"はい。ハンバーグがしっとりしていて最高でした。"},{"id":{"d":657,"n":3,"t":1},"speaker":"A","text":"何かデザートを注文しますか？"},{"id":{"d":657,"n":4,"t":1},"speaker":"B","text":"そうですね。メニューを見てみましょう。"},{"id":{"d":657,"n":5,"t":1},"speaker":"A","text":"おいしそうなデザートがいろいろとあるようです。"},{"id":{"d":657,"n":6,"t":1},"speaker":"B","text":"どれにするか迷ってしまいますね。"},{"id":{"d":657,"n":7,"t":1},"speaker":"A","text":"私はチョコレートケーキにします。"},{"id":{"d":657,"n":8,"t":1},"speaker":"B","text":"私はモンブランに決めました。"},{"id":{"d":657,"n":9,"t":1},"speaker":"A","text":"それでは注文しましょう。"}];
INSERT INTO sentence [{"id":{"d":658,"n":1,"t":1},"speaker":"A","text":"苗がたくさん売られていますね。"},{"id":{"d":658,"n":2,"t":1},"speaker":"B","text":"家の庭が寂しいので、何か買って育てようと思います。"},{"id":{"d":658,"n":3,"t":1},"speaker":"A","text":"ガーデニングをされるのですか？"},{"id":{"d":658,"n":4,"t":1},"speaker":"B","text":"昔から大好きです。"},{"id":{"d":658,"n":5,"t":1},"speaker":"A","text":"どの苗にしますか？"},{"id":{"d":658,"n":6,"t":1},"speaker":"B","text":"育てやすいものがいいですね。"},{"id":{"d":658,"n":7,"t":1},"speaker":"A","text":"育てやすさに違いがあるのですか？"},{"id":{"d":658,"n":8,"t":1},"speaker":"B","text":"はい。世話が簡単で虫に強い植物ほど育てやすいですね。"}];
INSERT INTO sentence [{"id":{"d":659,"n":1,"t":1},"speaker":"A","text":"お風呂場のタイルの目地にカビが生えてしまいました。"},{"id":{"d":659,"n":2,"t":1},"speaker":"B","text":"毎日掃除していても、カビが生えることがありますよね。"},{"id":{"d":659,"n":3,"t":1},"speaker":"A","text":"スポンジでこすったのですが、落ちませんでした。"},{"id":{"d":659,"n":4,"t":1},"speaker":"B","text":"スポンジでは落ちにくいですよ。"},{"id":{"d":659,"n":5,"t":1},"speaker":"A","text":"何を使えば落ちるでしょうか？"},{"id":{"d":659,"n":6,"t":1},"speaker":"B","text":"たわしの方が落ちると思います。"},{"id":{"d":659,"n":7,"t":1},"speaker":"A","text":"そうなのですね。今度はたわしを使ってみます。"},{"id":{"d":659,"n":8,"t":1},"speaker":"B","text":"カビ取り専用の洗剤を使うと、より効果的ですよ。"}];
INSERT INTO sentence [{"id":{"d":660,"n":1,"t":1},"speaker":"A","text":"お父さん、自転車の乗り方を教えてください。"},{"id":{"d":660,"n":2,"t":1},"speaker":"B","text":"急にどうしたのですか？"},{"id":{"d":660,"n":3,"t":1},"speaker":"A","text":"今日お友達と話していたら、みんな自転車に乗れると言っているのです。"},{"id":{"d":660,"n":4,"t":1},"speaker":"B","text":"それで自分も乗れるようになりたいと思ったのですね。"},{"id":{"d":660,"n":5,"t":1},"speaker":"A","text":"はい。みんなに追い付きたいです。"},{"id":{"d":660,"n":6,"t":1},"speaker":"B","text":"分かりました。それでは練習しましょう。"},{"id":{"d":660,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。練習は家の前でしますか？"},{"id":{"d":660,"n":8,"t":1},"speaker":"B","text":"車が来ると危ないので、大きい公園に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":661,"n":1,"t":1},"speaker":"A","text":"今日は私がピザを作ります。"},{"id":{"d":661,"n":2,"t":1},"speaker":"B","text":"生地から手作りするのですか？"},{"id":{"d":661,"n":3,"t":1},"speaker":"A","text":"はい。小麦粉から生地を作ってみたかったのです。"},{"id":{"d":661,"n":4,"t":1},"speaker":"B","text":"作り方は誰かに聞いたのですか？"},{"id":{"d":661,"n":5,"t":1},"speaker":"A","text":"いいえ、インターネットで調べました。"},{"id":{"d":661,"n":6,"t":1},"speaker":"B","text":"どのようなピザができ上がるか、とても楽しみです。"},{"id":{"d":661,"n":7,"t":1},"speaker":"A","text":"おいしくできるように頑張ります。"},{"id":{"d":661,"n":8,"t":1},"speaker":"B","text":"私はサラダを作りますね。"},{"id":{"d":661,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":662,"n":1,"t":1},"speaker":"A","text":"懸賞に当たりました。"},{"id":{"d":662,"n":2,"t":1},"speaker":"B","text":"何が当たったのですか？"},{"id":{"d":662,"n":3,"t":1},"speaker":"A","text":"商品券です。"},{"id":{"d":662,"n":4,"t":1},"speaker":"B","text":"素晴らしいですね。私は懸賞に当たったことなどありません。"},{"id":{"d":662,"n":5,"t":1},"speaker":"A","text":"私はたくさん応募しているから、たまに当たるのですよ。"},{"id":{"d":662,"n":6,"t":1},"speaker":"B","text":"それほど応募しているのですか？"},{"id":{"d":662,"n":7,"t":1},"speaker":"A","text":"はい。趣味のようなもので、懸賞の企画がないか探しては頻繁に応募しています。"},{"id":{"d":662,"n":8,"t":1},"speaker":"B","text":"まめなのですね。"}];
INSERT INTO sentence [{"id":{"d":663,"n":1,"t":1},"speaker":"A","text":"有名人の知り合いはいますか？"},{"id":{"d":663,"n":2,"t":1},"speaker":"B","text":"全くいません。あなたはいるのですか？"},{"id":{"d":663,"n":3,"t":1},"speaker":"A","text":"高校時代の野球部の先輩が今、プロ野球で活躍しています。"},{"id":{"d":663,"n":4,"t":1},"speaker":"B","text":"私でも知っている選手でしょうか？"},{"id":{"d":663,"n":5,"t":1},"speaker":"A","text":"青葉レイカーズの北園選手です。"},{"id":{"d":663,"n":6,"t":1},"speaker":"B","text":"知っています。ここ数年、好成績を残していますよね。"},{"id":{"d":663,"n":7,"t":1},"speaker":"A","text":"私自身は野球部でベンチ入りもできませんでしたが、同じ野球部に所属していただけで誇らしいです。"},{"id":{"d":663,"n":8,"t":1},"speaker":"B","text":"どのような先輩でしたか？"},{"id":{"d":663,"n":9,"t":1},"speaker":"A","text":"誰よりも熱心に練習に取り組んでいました。"}];
INSERT INTO sentence [{"id":{"d":664,"n":1,"t":1},"speaker":"A","text":"待っていた物が届きました。"},{"id":{"d":664,"n":2,"t":1},"speaker":"B","text":"荷物の中身は何ですか？"},{"id":{"d":664,"n":3,"t":1},"speaker":"A","text":"テレビで紹介されていておいしそうだったものをお取り寄せしたのです。"},{"id":{"d":664,"n":4,"t":1},"speaker":"B","text":"それは楽しみですね。どのようなものでしょうか？"},{"id":{"d":664,"n":5,"t":1},"speaker":"A","text":"これです。特上の牛肉です。"},{"id":{"d":664,"n":6,"t":1},"speaker":"B","text":"見ただけで高級だと分かります。高かったでしょうね。"},{"id":{"d":664,"n":7,"t":1},"speaker":"A","text":"はい。かなり高かったですが、どうしても食べてみたくて注文しました。"},{"id":{"d":664,"n":8,"t":1},"speaker":"B","text":"どのように調理して食べるつもりですか？"},{"id":{"d":664,"n":9,"t":1},"speaker":"A","text":"しゃぶしゃぶです。今晩、一緒に食べましょう。"},{"id":{"d":664,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":665,"n":1,"t":1},"speaker":"A","text":"さあ、手分けして大掃除をしますよ。"},{"id":{"d":665,"n":2,"t":1},"speaker":"B","text":"私はどこを掃除すればよいですか？"},{"id":{"d":665,"n":3,"t":1},"speaker":"A","text":"あなたは冷蔵庫をお願いします。"},{"id":{"d":665,"n":4,"t":1},"speaker":"B","text":"中身を全部出して、棚などを拭けばよいですか？"},{"id":{"d":665,"n":5,"t":1},"speaker":"A","text":"はい。お願いします。私は換気扇を掃除します。"},{"id":{"d":665,"n":6,"t":1},"speaker":"B","text":"換気扇は取り外して掃除するのですか？"},{"id":{"d":665,"n":7,"t":1},"speaker":"A","text":"そうですね。取り外した方が掃除しやすいので、そうするつもりです。"},{"id":{"d":665,"n":8,"t":1},"speaker":"B","text":"1人で取り外すのは大変だと思うので、手伝いが必要だったら声を掛けてくださいね。"},{"id":{"d":665,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":666,"n":1,"t":1},"speaker":"A","text":"もしもし、三洋宅配便です。"},{"id":{"d":666,"n":2,"t":1},"speaker":"B","text":"もしもし。荷物の不在連絡票がポストに入っていました。"},{"id":{"d":666,"n":3,"t":1},"speaker":"A","text":"それでは、再配達をご希望されますか？"},{"id":{"d":666,"n":4,"t":1},"speaker":"B","text":"はい。お願いします。"},{"id":{"d":666,"n":5,"t":1},"speaker":"A","text":"ご希望の日にちを教えてください。"},{"id":{"d":666,"n":6,"t":1},"speaker":"B","text":"明日でお願いします。"},{"id":{"d":666,"n":7,"t":1},"speaker":"A","text":"お時間の指定はありますか？"},{"id":{"d":666,"n":8,"t":1},"speaker":"B","text":"18時以降にしてください。"},{"id":{"d":666,"n":9,"t":1},"speaker":"A","text":"かしこまりました。明日の18時以降にお届けに伺います。"}];
INSERT INTO sentence [{"id":{"d":667,"n":1,"t":1},"speaker":"A","text":"今日のハンバーグはいつも以上においしいです。"},{"id":{"d":667,"n":2,"t":1},"speaker":"B","text":"やはりそうですか？"},{"id":{"d":667,"n":3,"t":1},"speaker":"A","text":"何か秘密があるのですか？"},{"id":{"d":667,"n":4,"t":1},"speaker":"B","text":"友達に、ハンバーグがおいしくなるコツを教えてもらったのです。"},{"id":{"d":667,"n":5,"t":1},"speaker":"A","text":"効果が絶大ですね。本当にびっくりするくらいおいしいです。"},{"id":{"d":667,"n":6,"t":1},"speaker":"B","text":"よかったです。簡単なコツなのですよ。"},{"id":{"d":667,"n":7,"t":1},"speaker":"A","text":"どのようなコツですか？"},{"id":{"d":667,"n":8,"t":1},"speaker":"B","text":"それは内緒です。"}];
INSERT INTO sentence [{"id":{"d":668,"n":1,"t":1},"speaker":"A","text":"大工になろうと思ったきっかけは何ですか？"},{"id":{"d":668,"n":2,"t":1},"speaker":"B","text":"テレビ番組で大工の仕事が取り上げられていて、魅力的だなと思いました。"},{"id":{"d":668,"n":3,"t":1},"speaker":"A","text":"どのようなところが魅力的でしたか？"},{"id":{"d":668,"n":4,"t":1},"speaker":"B","text":"やはり、材料から建物を造り上げてしまうという技術ですね。"},{"id":{"d":668,"n":5,"t":1},"speaker":"A","text":"一方で、大工になって苦労したこともあるでしょうね。"},{"id":{"d":668,"n":6,"t":1},"speaker":"B","text":"そうですね。なかなか技術が向上しなくて、親方に叱られることが多いです。"},{"id":{"d":668,"n":7,"t":1},"speaker":"A","text":"これからの抱負を教えてください。"},{"id":{"d":668,"n":8,"t":1},"speaker":"B","text":"早く一人前になって、自分も親方のような立場になりたいです。"}];
INSERT INTO sentence [{"id":{"d":669,"n":1,"t":1},"speaker":"A","text":"今、揺れを感じました。"},{"id":{"d":669,"n":2,"t":1},"speaker":"B","text":"揺れていますね。地震です。"},{"id":{"d":669,"n":3,"t":1},"speaker":"A","text":"コンロの火は消えていますか？"},{"id":{"d":669,"n":4,"t":1},"speaker":"B","text":"はい。料理をしていませんので消えています。"},{"id":{"d":669,"n":5,"t":1},"speaker":"A","text":"揺れが収まりましたね。"},{"id":{"d":669,"n":6,"t":1},"speaker":"B","text":"テレビで地震の情報を確認しましょう。"},{"id":{"d":669,"n":7,"t":1},"speaker":"A","text":"はい。被害がないといいですね。"},{"id":{"d":669,"n":8,"t":1},"speaker":"B","text":"この辺りの震度は2だそうです。"},{"id":{"d":669,"n":9,"t":1},"speaker":"A","text":"もっと大きく感じました。怖かったです。"}];
INSERT INTO sentence [{"id":{"d":670,"n":1,"t":1},"speaker":"A","text":"私の父は料理が得意です。"},{"id":{"d":670,"n":2,"t":1},"speaker":"B","text":"家でよく料理をするのですか？"},{"id":{"d":670,"n":3,"t":1},"speaker":"A","text":"週末はいつも父が料理をします。"},{"id":{"d":670,"n":4,"t":1},"speaker":"B","text":"お父さんの料理で好きなものは何ですか？"},{"id":{"d":670,"n":5,"t":1},"speaker":"A","text":"ビーフシチューです。"},{"id":{"d":670,"n":6,"t":1},"speaker":"B","text":"私も食べてみたいです。"},{"id":{"d":670,"n":7,"t":1},"speaker":"A","text":"ぜひ今度、食べに来てください。父も喜ぶと思います。"},{"id":{"d":670,"n":8,"t":1},"speaker":"B","text":"本当に伺ってもよいのですか？うれしいです。"}];
INSERT INTO sentence [{"id":{"d":671,"n":1,"t":1},"speaker":"A","text":"おばあちゃんから荷物が届きましたよ。"},{"id":{"d":671,"n":2,"t":1},"speaker":"B","text":"何でしょうか？開けてよいですか？"},{"id":{"d":671,"n":3,"t":1},"speaker":"A","text":"どうぞ、開けてください。"},{"id":{"d":671,"n":4,"t":1},"speaker":"B","text":"サクランボです。私の大好物です。"},{"id":{"d":671,"n":5,"t":1},"speaker":"A","text":"きれいな色ですね。それにおいしそうです。"},{"id":{"d":671,"n":6,"t":1},"speaker":"B","text":"少し食べてみたいです。"},{"id":{"d":671,"n":7,"t":1},"speaker":"A","text":"洗って食べましょう。"},{"id":{"d":671,"n":8,"t":1},"speaker":"B","text":"お父さんも食べるでしょうから、呼んできます。"}];
INSERT INTO sentence [{"id":{"d":672,"n":1,"t":1},"speaker":"A","text":"靴下に穴が開いていますよ。"},{"id":{"d":672,"n":2,"t":1},"speaker":"B","text":"本当ですね。今日一日、この靴下で過ごしていました。"},{"id":{"d":672,"n":3,"t":1},"speaker":"A","text":"靴を脱いでいなければ、誰も気が付きませんよ。"},{"id":{"d":672,"n":4,"t":1},"speaker":"B","text":"今日はお昼に同僚と外食したのですが、靴を脱いで上がるスタイルのお店だったのです。"},{"id":{"d":672,"n":5,"t":1},"speaker":"A","text":"それでは、その時に一緒にいた人に気付かれたかもしれませんね。"},{"id":{"d":672,"n":6,"t":1},"speaker":"B","text":"そう思うと恥ずかしいです。"},{"id":{"d":672,"n":7,"t":1},"speaker":"A","text":"よくあることですし、気にしない方がいいですよ。"},{"id":{"d":672,"n":8,"t":1},"speaker":"B","text":"そうですね。今更気にしても仕方がないよね。"}];
INSERT INTO sentence [{"id":{"d":673,"n":1,"t":1},"speaker":"A","text":"今日の試合でエラーをしてしまいました。"},{"id":{"d":673,"n":2,"t":1},"speaker":"B","text":"大きなミスではありませんでしたし、大丈夫ですよ。"},{"id":{"d":673,"n":3,"t":1},"speaker":"A","text":"でも、私のエラーが相手チームの得点につながってしまいました。"},{"id":{"d":673,"n":4,"t":1},"speaker":"B","text":"今日は全体的に守備がうまくいかなかったので、気にすることはありません。"},{"id":{"d":673,"n":5,"t":1},"speaker":"A","text":"負けるはずのないチームに負けてしまったので、責任を感じます。"},{"id":{"d":673,"n":6,"t":1},"speaker":"B","text":"あなた1人の責任ではありません。1点もとれなかったことがチーム全体として問題です。"},{"id":{"d":673,"n":7,"t":1},"speaker":"A","text":"もっと攻撃力をつけたいです。"},{"id":{"d":673,"n":8,"t":1},"speaker":"B","text":"そのために、これからの練習を頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":674,"n":1,"t":1},"speaker":"A","text":"来月でこのアルバイトを辞めることにします。"},{"id":{"d":674,"n":2,"t":1},"speaker":"B","text":"何かあったのですか？"},{"id":{"d":674,"n":3,"t":1},"speaker":"A","text":"大学の課題がとても多くなったので、アルバイトをしていては勉強する時間が確保できないのです。"},{"id":{"d":674,"n":4,"t":1},"speaker":"B","text":"それでは仕方ありませんね。でも、あなたが辞めたら寂しいです。"},{"id":{"d":674,"n":5,"t":1},"speaker":"A","text":"私もあなたと働けなくなるのは残念です。"},{"id":{"d":674,"n":6,"t":1},"speaker":"B","text":"大学の課題が落ち着いたら、また戻ってきませんか？"},{"id":{"d":674,"n":7,"t":1},"speaker":"A","text":"そうですね。しばらく勉強を頑張って課題が片付いたら、戻ってきたいです。"},{"id":{"d":674,"n":8,"t":1},"speaker":"B","text":"あなたが戻ってくることを楽しみに待っていますね。"}];
INSERT INTO sentence [{"id":{"d":675,"n":1,"t":1},"speaker":"A","text":"バレンタインのチョコレートをどうぞ。"},{"id":{"d":675,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。わざわざすみません。"},{"id":{"d":675,"n":3,"t":1},"speaker":"A","text":"先輩にはいつも仕事を教えてもらってお世話になっているので、ほんの気持ちです。"},{"id":{"d":675,"n":4,"t":1},"speaker":"B","text":"これはもしかして、手作りですか？"},{"id":{"d":675,"n":5,"t":1},"speaker":"A","text":"はい。お口に合うかどうか分かりませんが、作ってみました。"},{"id":{"d":675,"n":6,"t":1},"speaker":"B","text":"あなたの気持ちがありがたいです。手作りだと、時間がかかったでしょうね。"},{"id":{"d":675,"n":7,"t":1},"speaker":"A","text":"お気になさらないでください。昨日は休みだったので、のんびり作りました。"},{"id":{"d":675,"n":8,"t":1},"speaker":"B","text":"本当にありがとう。息子もチョコレートが大好きなので、持って帰って息子にもあげてよいですか？"},{"id":{"d":675,"n":9,"t":1},"speaker":"A","text":"もちろんです。息子さんも喜んでくれたらうれしいです。"}];
INSERT INTO sentence [{"id":{"d":676,"n":1,"t":1},"speaker":"A","text":"新商品の企画案をまとめました。ご確認をお願いしてもよろしいでしょうか？"},{"id":{"d":676,"n":2,"t":1},"speaker":"B","text":"ありがとう。コンセプトは良いですね。ただ、商品自体は今ある商品と似ていて目新しさがあまりないように思います。"},{"id":{"d":676,"n":3,"t":1},"speaker":"A","text":"やはりそうですか。新しい商品を考えるのは難しいです。"},{"id":{"d":676,"n":4,"t":1},"speaker":"B","text":"他の人とも相談しつつ、もう少しだけ他のアイデアも検討してみてもらえますか？"},{"id":{"d":676,"n":5,"t":1},"speaker":"A","text":"はい。もう一度考えてみます。"},{"id":{"d":676,"n":6,"t":1},"speaker":"B","text":"企画案を1つに絞る必要はありませんので、良いアイデアをいくつか持ってきてください。"},{"id":{"d":676,"n":7,"t":1},"speaker":"A","text":"承知しました。頑張ります。"},{"id":{"d":676,"n":8,"t":1},"speaker":"B","text":"よろしくお願いします。期待していますよ。"}];
INSERT INTO sentence [{"id":{"d":677,"n":1,"t":1},"speaker":"A","text":"私はイヌを飼っています。"},{"id":{"d":677,"n":2,"t":1},"speaker":"B","text":"何という名前ですか？"},{"id":{"d":677,"n":3,"t":1},"speaker":"A","text":"イヌの名前は「ユキ」です。"},{"id":{"d":677,"n":4,"t":1},"speaker":"B","text":"かわいい名前ですね。"},{"id":{"d":677,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。生まれた日に雪が降っていたことが名前の由来です。"},{"id":{"d":677,"n":6,"t":1},"speaker":"B","text":"私はてっきり、雪のように白いから「ユキ」と名付けたのだと思いました。"},{"id":{"d":677,"n":7,"t":1},"speaker":"A","text":"それが、むしろ毛の色は黒なのです。"},{"id":{"d":677,"n":8,"t":1},"speaker":"B","text":"見た目とギャップのある名前で面白いですね。"}];
INSERT INTO sentence [{"id":{"d":678,"n":1,"t":1},"speaker":"A","text":"次の年度末で仕事を辞めることにしました。"},{"id":{"d":678,"n":2,"t":1},"speaker":"B","text":"驚きました。転職ですか？"},{"id":{"d":678,"n":3,"t":1},"speaker":"A","text":"はい。長い間悩みましたが、ついに決心しました。"},{"id":{"d":678,"n":4,"t":1},"speaker":"B","text":"よく決心できましたね。"},{"id":{"d":678,"n":5,"t":1},"speaker":"A","text":"今の仕事を続けるより、心機一転したい気持ちが強くなりました。"},{"id":{"d":678,"n":6,"t":1},"speaker":"B","text":"あなたが辞めてしまうのは寂しいです。もう転職先は決まっているのですか？"},{"id":{"d":678,"n":7,"t":1},"speaker":"A","text":"はい。運良く希望の条件の会社が見つかりました。"},{"id":{"d":678,"n":8,"t":1},"speaker":"B","text":"それはおめでとうございます。職場が違ってしまうのは残念ですが、あなたが次の会社で活躍することを祈っています。"}];
INSERT INTO sentence [{"id":{"d":679,"n":1,"t":1},"speaker":"A","text":"引っ越しをしたそうですね。"},{"id":{"d":679,"n":2,"t":1},"speaker":"B","text":"はい。つい先週末に引っ越しました。"},{"id":{"d":679,"n":3,"t":1},"speaker":"A","text":"新しい家はどうですか？"},{"id":{"d":679,"n":4,"t":1},"speaker":"B","text":"広々していて家自体は快適なのですが、問題点もあります。"},{"id":{"d":679,"n":5,"t":1},"speaker":"A","text":"何か問題なのでしょうか？"},{"id":{"d":679,"n":6,"t":1},"speaker":"B","text":"通勤時間が長くなってしまったのです。"},{"id":{"d":679,"n":7,"t":1},"speaker":"A","text":"毎日のことなのでそれは少し大変ですね。"},{"id":{"d":679,"n":8,"t":1},"speaker":"B","text":"はい。今までより早起きしなければいけませんが、まだ慣れません。"}];
INSERT INTO sentence [{"id":{"d":680,"n":1,"t":1},"speaker":"A","text":"こんにちは。町内の自治会費の集金に来ました。"},{"id":{"d":680,"n":2,"t":1},"speaker":"B","text":"ご苦労さまです。少しお待ちください。"},{"id":{"d":680,"n":3,"t":1},"speaker":"A","text":"お手数をおかけします。"},{"id":{"d":680,"n":4,"t":1},"speaker":"B","text":"お待たせしました。5,000円でお釣りはありますか？"},{"id":{"d":680,"n":5,"t":1},"speaker":"A","text":"はい、用意しています。どうぞお釣りです。"},{"id":{"d":680,"n":6,"t":1},"speaker":"B","text":"ありがとう。去年はコロナでほとんど行事ができずに残念でした。"},{"id":{"d":680,"n":7,"t":1},"speaker":"A","text":"そうですね。今年は今のところ積極的に行事を実施していく予定ですよ。"},{"id":{"d":680,"n":8,"t":1},"speaker":"B","text":"それは楽しみです。私も積極的に行事に参加していきたいと思います。"}];
INSERT INTO sentence [{"id":{"d":681,"n":1,"t":1},"speaker":"A","text":"友人のお見舞いにメロンを持っていきたいのですが、どのような種類がありますか？"},{"id":{"d":681,"n":2,"t":1},"speaker":"B","text":"お見舞いでしたら、こちらのマスクメロンか夕張メロンがよろしいかと思います。"},{"id":{"d":681,"n":3,"t":1},"speaker":"A","text":"なるほど。そういえば、お店の入り口にとても大きなメロンがありましたがあれは何ですか？"},{"id":{"d":681,"n":4,"t":1},"speaker":"B","text":"あちらはメキシコからの輸入物でハネデューという品種です。"},{"id":{"d":681,"n":5,"t":1},"speaker":"A","text":"初めて聞きました。どれを買おうか悩みます。"},{"id":{"d":681,"n":6,"t":1},"speaker":"B","text":"お見舞いの定番といえば、やはりマスクメロンや夕張メロンを購入される方が多いですよ。"},{"id":{"d":681,"n":7,"t":1},"speaker":"A","text":"それではマスクメロンと夕張メロンを一つずつ頂きます。"},{"id":{"d":681,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。ただいまご準備いたしますね。"}];
INSERT INTO sentence [{"id":{"d":682,"n":1,"t":1},"speaker":"A","text":"すみません。福引はこちらでよいですか？"},{"id":{"d":682,"n":2,"t":1},"speaker":"B","text":"はい、そうです。福引券はお持ちですか？"},{"id":{"d":682,"n":3,"t":1},"speaker":"A","text":"これでお願いします。3回分あると思います。"},{"id":{"d":682,"n":4,"t":1},"speaker":"B","text":"確認しますね。はい、確かに3回分頂戴しました。こちらを回してください。"},{"id":{"d":682,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。もう1等は出ましたか？"},{"id":{"d":682,"n":6,"t":1},"speaker":"B","text":"まだ3等までしか出ていませんので、ぜひ上の賞を狙ってください。"},{"id":{"d":682,"n":7,"t":1},"speaker":"A","text":"がぜんその気になってきました。1等賞は旅行券で2等賞は商品券ですか？"},{"id":{"d":682,"n":8,"t":1},"speaker":"B","text":"そうです。最近は金券が景品になることが増えてきています。"},{"id":{"d":682,"n":9,"t":1},"speaker":"A","text":"金券は使い道が広くてありがたいです。それでは1等を狙って回しましょう。"}];
INSERT INTO sentence [{"id":{"d":683,"n":1,"t":1},"speaker":"A","text":"そうめんの季節になってきました。"},{"id":{"d":683,"n":2,"t":1},"speaker":"B","text":"ええ、すっかり夏ですね。そうめんもですが、私は夏といえば冷や麦の季節です。"},{"id":{"d":683,"n":3,"t":1},"speaker":"A","text":"そういえば、そうめんと冷や麦は何が違うのでしょうか？"},{"id":{"d":683,"n":4,"t":1},"speaker":"B","text":"面の太さが違うのです。そうめんに比べて、冷や麦の方が少しだけ太めなのですよ。"},{"id":{"d":683,"n":5,"t":1},"speaker":"A","text":"そうですか。"},{"id":{"d":683,"n":6,"t":1},"speaker":"B","text":"太さはほんの少しの違いなのですが、食感はかなり変わります。こしがあってとてもおいしいですよ。"},{"id":{"d":683,"n":7,"t":1},"speaker":"A","text":"いつもついそうめんばかりを手にとってしまうので、今年は冷や麦にも挑戦してみます。"}];
INSERT INTO sentence [{"id":{"d":684,"n":1,"t":1},"speaker":"A","text":"あなたはいつもロンドンブーツを履いていますね。"},{"id":{"d":684,"n":2,"t":1},"speaker":"B","text":"憧れのミュージシャンがいつもロンドンブーツを履いているので、私も彼のまねをしてよく履きます。"},{"id":{"d":684,"n":3,"t":1},"speaker":"A","text":"そうでしたか。とてもお似合いです。"},{"id":{"d":684,"n":4,"t":1},"speaker":"B","text":"それはうれしいです。ロンドンブーツを履くと背筋が伸びる気がします。"},{"id":{"d":684,"n":5,"t":1},"speaker":"A","text":"私は普段あまりブーツを履きませんのでよく分かりませんが、姿勢がよくなるのは良いことですね。"},{"id":{"d":684,"n":6,"t":1},"speaker":"B","text":"言われてみれば、確かにあなたがブーツを履いているところを見た記憶はほとんどありません。スニーカーのイメージが強いです。"},{"id":{"d":684,"n":7,"t":1},"speaker":"A","text":"はい、私は大抵スニーカーを履いています。スニーカーとジーンズの組み合わせが好きなのです。"},{"id":{"d":684,"n":8,"t":1},"speaker":"B","text":"あなたも良く似合っていますよ。"},{"id":{"d":684,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":685,"n":1,"t":1},"speaker":"A","text":"ここのファミリーレストランのオムライスビーフシチューソースは絶品の味です。"},{"id":{"d":685,"n":2,"t":1},"speaker":"B","text":"そうですか。私のお薦めは、エビとヤマイモとオクラのうどんです。"},{"id":{"d":685,"n":3,"t":1},"speaker":"A","text":"お互いに随分個性的な一品ですね。"},{"id":{"d":685,"n":4,"t":1},"speaker":"B","text":"そうですね。このファミリーレストランには何度か来ていますが、オムライスビーフシチューソースは頼んだことがありません。"},{"id":{"d":685,"n":5,"t":1},"speaker":"A","text":"せっかくの機会なので、今日はお互いのお薦めを試してみませんか？"},{"id":{"d":685,"n":6,"t":1},"speaker":"B","text":"名案ですね。それでは取り皿を頼んで、それぞれのメニューを半分ずついただきましょう。"}];
INSERT INTO sentence [{"id":{"d":686,"n":1,"t":1},"speaker":"A","text":"今日の天気は、去年の竜巻が発生した時と似ていませんか？"},{"id":{"d":686,"n":2,"t":1},"speaker":"B","text":"そうですね。去年は、このような天気の後すぐに風が強くなり、雨が降り始め、雷も鳴り始めました。"},{"id":{"d":686,"n":3,"t":1},"speaker":"A","text":"雷と同時にひょうも降ってきたように思います。"},{"id":{"d":686,"n":4,"t":1},"speaker":"B","text":"そうでした。ひょうが屋根に当たる音でうるさかったことを覚えています。"},{"id":{"d":686,"n":5,"t":1},"speaker":"A","text":"最近、竜巻のパニック映画を見たのですが、やはり竜巻発生の前にひょうが降っていました。"},{"id":{"d":686,"n":6,"t":1},"speaker":"B","text":"去年の竜巻でも、屋根が飛ばされるなどの被害が出ていましたね。"},{"id":{"d":686,"n":7,"t":1},"speaker":"A","text":"怖くなってきました。竜巻注意報が出ているか、インターネットで確認しましょう。"},{"id":{"d":686,"n":8,"t":1},"speaker":"B","text":"そうこう言っているうちに、雨は弱まってきたように感じますよ。"},{"id":{"d":686,"n":9,"t":1},"speaker":"A","text":"本当ですね。このまま収まってほしいです。"}];
INSERT INTO sentence [{"id":{"d":687,"n":1,"t":1},"speaker":"A","text":"カレーライスの具は牛肉と豚肉と鶏肉でどれがお好みですか？"},{"id":{"d":687,"n":2,"t":1},"speaker":"B","text":"私はカレーに肉は入れず、カツカレーで食べるのが好きです。"},{"id":{"d":687,"n":3,"t":1},"speaker":"A","text":"その手がありましたか。メンチカツやコロッケをのせるのも好きですか？"},{"id":{"d":687,"n":4,"t":1},"speaker":"B","text":"どちらも好きですが、メンチカツは食べやすいように切ってのせたいです。"},{"id":{"d":687,"n":5,"t":1},"speaker":"A","text":"いつもカレーの上に一品のせているのですか？"},{"id":{"d":687,"n":6,"t":1},"speaker":"B","text":"大体カレーに何かのせます。ホタテフライやエビフライを加え、海鮮カレー風にするのも好きです。"},{"id":{"d":687,"n":7,"t":1},"speaker":"A","text":"カレーのチェーン店でもありそうなメニューですね。"},{"id":{"d":687,"n":8,"t":1},"speaker":"B","text":"海鮮のフライをのせたカレーは実際にあります。一時期よく通っていましたから、メニューはよく知っています。"},{"id":{"d":687,"n":9,"t":1},"speaker":"A","text":"カレーに何か加えるのは、そこで食べたおいしさが忘れられなかったからではありませんか？"},{"id":{"d":687,"n":10,"t":1},"speaker":"B","text":"それは否定できません。"}];
INSERT INTO sentence [{"id":{"d":688,"n":1,"t":1},"speaker":"A","text":"最近、唐揚げ屋さんが増えてきたとは思いませんか？"},{"id":{"d":688,"n":2,"t":1},"speaker":"B","text":"言われてみればそうかもしれません。私もよく利用しますよ。"},{"id":{"d":688,"n":3,"t":1},"speaker":"A","text":"一口に唐揚げと言っても、肉にこだわった本格派から風変わりなものまでいろいろありますね。"},{"id":{"d":688,"n":4,"t":1},"speaker":"B","text":"そうですね。あなたは唐揚げに何かこだわりはありますか？"},{"id":{"d":688,"n":5,"t":1},"speaker":"A","text":"味の濃い一口サイズが良いです。大きい唐揚げは食べにくいです。"},{"id":{"d":688,"n":6,"t":1},"speaker":"B","text":"いいですね。私も味の濃いものが特に好きです。冷めてもおいしいので、お弁当にもよく合います。"},{"id":{"d":688,"n":7,"t":1},"speaker":"A","text":"そう言えば、運動会の時のお弁当には唐揚げが入っていた記憶があります。"},{"id":{"d":688,"n":8,"t":1},"speaker":"B","text":"私もそうでした。兄と取り合いながら食べていました。今は胸焼けをしてしまうので沢山は食べられませんが、いまでも唐揚げは大好きです。"}];
INSERT INTO sentence [{"id":{"d":689,"n":1,"t":1},"speaker":"A","text":"近所におにぎり屋さんがオープンしたようです。近いうちに食べに行きませんか？"},{"id":{"d":689,"n":2,"t":1},"speaker":"B","text":"おにぎり屋さんとは珍しいですね。おにぎり以外のメニューもあるのでしょうか？"},{"id":{"d":689,"n":3,"t":1},"speaker":"A","text":"このチラシを見るとサイドメニューも豊富なようです。唐揚げや冷ややっこに煮込みに焼き魚もあります。"},{"id":{"d":689,"n":4,"t":1},"speaker":"B","text":"まるで定食屋さんですね。"},{"id":{"d":689,"n":5,"t":1},"speaker":"A","text":"おにぎりをベースにした定食もあるようですよ。"},{"id":{"d":689,"n":6,"t":1},"speaker":"B","text":"驚きました。どれもおいしそうですね。ぜひ行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":690,"n":1,"t":1},"speaker":"A","text":"どちらから転勤されて来られたのですか？"},{"id":{"d":690,"n":2,"t":1},"speaker":"B","text":"秋田です。"},{"id":{"d":690,"n":3,"t":1},"speaker":"A","text":"そうでしたか。こちらの生活でお困りのことはありますか？"},{"id":{"d":690,"n":4,"t":1},"speaker":"B","text":"想像していたより快適です。冬でもベランダで物が干せるのでびっくりしました。"},{"id":{"d":690,"n":5,"t":1},"speaker":"A","text":"雪国では冬はベランダで洗濯物を干せないのですね。冬の間はどうしていたのですか？"},{"id":{"d":690,"n":6,"t":1},"speaker":"B","text":"私の家ではお風呂場が広くて、そこで干すようにしていました。"},{"id":{"d":690,"n":7,"t":1},"speaker":"A","text":"ベランダで洗濯物を干す家はありませんか？"},{"id":{"d":690,"n":8,"t":1},"speaker":"B","text":"洗濯物が干せるようにプラスチックで遮蔽したベランダを持つ家もあります。乾燥機を使っている家も多いと思います。"},{"id":{"d":690,"n":9,"t":1},"speaker":"A","text":"乾燥機の需要は高そうです。他に秋田の家にあって東京の家にないものはありますか？"},{"id":{"d":690,"n":10,"t":1},"speaker":"B","text":"秋田の家には屋根や駐車スペースに融雪という雪を解かす設備があります。"},{"id":{"d":690,"n":11,"t":1},"speaker":"A","text":"初めて聞きました。その融雪と言う設備は電気で動くのですか？"},{"id":{"d":690,"n":12,"t":1},"speaker":"B","text":"電気のものもありますが、私の家では融雪は灯油を使っていました。"}];
INSERT INTO sentence [{"id":{"d":691,"n":1,"t":1},"speaker":"A","text":"この頃、毎日近所の喫茶店に通っていますね。坂田さんが、それほどコーヒーが好きだとは思いませんでした。"},{"id":{"d":691,"n":2,"t":1},"speaker":"B","text":"実は、コーヒーを飲むためではなく、ラテアートを学ぶために通っています。"},{"id":{"d":691,"n":3,"t":1},"speaker":"A","text":"ラテアートとはコーヒーの上に絵を描くものですか？"},{"id":{"d":691,"n":4,"t":1},"speaker":"B","text":"そうです、コーヒーの上に泡立てたミルクをのせ、コーヒーで絵を描くのです。"},{"id":{"d":691,"n":5,"t":1},"speaker":"A","text":"その喫茶店ではラテアート教室をやっているのですか？"},{"id":{"d":691,"n":6,"t":1},"speaker":"B","text":"いいえ、教室ではありません。エスプレッソを注文すると、マスターがラテアートを教えてくれるのです。"},{"id":{"d":691,"n":7,"t":1},"speaker":"A","text":"自分の注文したエスプレッソに絵を描けるということですか？"},{"id":{"d":691,"n":8,"t":1},"speaker":"B","text":"そうです。最近は、簡単な絵なら描けるようになってきました。"},{"id":{"d":691,"n":9,"t":1},"speaker":"A","text":"それは素晴らしいです。ラテアートは難しいと思っていました。"},{"id":{"d":691,"n":10,"t":1},"speaker":"B","text":"液体のスチームミルクと泡状のフォームドミルクで下地を作ることは難しいそうです。でもここはマスターが作ってくれるので、何回か練習すれば絵を描けるようにはなりました。"}];
INSERT INTO sentence [{"id":{"d":692,"n":1,"t":1},"speaker":"A","text":"この書類を郵送したいのですが、今日は連休の初日で郵便局はお休みです。何か方法はありませんか？"},{"id":{"d":692,"n":2,"t":1},"speaker":"B","text":"枚数が多いようですからレターパックにしてはいかがですか？"},{"id":{"d":692,"n":3,"t":1},"speaker":"A","text":"レターパックは休日でも配達されるのですか？"},{"id":{"d":692,"n":4,"t":1},"speaker":"B","text":"普通郵便とは異なり、レターパックは日曜日や休日でも配達されますよ。"},{"id":{"d":692,"n":5,"t":1},"speaker":"A","text":"郵便局は閉まっていますが、休日もレターパックを売っている所はありますか？"},{"id":{"d":692,"n":6,"t":1},"speaker":"B","text":"コンビニエンスストアで売っている所があります。駅前にはコンビニエンスストアがいくつかありますから聞いてみるといいでしょう。"},{"id":{"d":692,"n":7,"t":1},"speaker":"A","text":"そのコンビニエンスストアがレターパックの郵送も取り扱ってくれるのですか？"},{"id":{"d":692,"n":8,"t":1},"speaker":"B","text":"いいえ、書類を入れて宛先を書いてからポストに投函します。詳しいことはレターパックに書いてあります。"},{"id":{"d":692,"n":9,"t":1},"speaker":"A","text":"分かりました。コンビニエンスストアに行ってきます。"}];
INSERT INTO sentence [{"id":{"d":693,"n":1,"t":1},"speaker":"A","text":"ワンルームマンションと2Kのアパートのどちらに住むか迷っています。"},{"id":{"d":693,"n":2,"t":1},"speaker":"B","text":"偶然ですが、私は大学時代にそのどちらにも住んでいたことがあります。"},{"id":{"d":693,"n":3,"t":1},"speaker":"A","text":"そうでしたか。ワンルームマンションにはどのような利点がありましたか？"},{"id":{"d":693,"n":4,"t":1},"speaker":"B","text":"部屋の掃除が楽です。またアパートよりは防犯や防音はしっかりしています。"},{"id":{"d":693,"n":5,"t":1},"speaker":"A","text":"なるほど。では、2Kアパートがワンルームマンションより優れている点はありますか？"},{"id":{"d":693,"n":6,"t":1},"speaker":"B","text":"収納はワンルームマンションよりあるはずです。料理するスペースも確保されています。"},{"id":{"d":693,"n":7,"t":1},"speaker":"A","text":"住んでいた経験から他にも何か違いがありますか？"},{"id":{"d":693,"n":8,"t":1},"speaker":"B","text":"2Kなら友人を泊めることは容易です。友人を家に招きたいのなら2Kの方がいいでしょう。"},{"id":{"d":693,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。具体的な物件が見つかりましたらまたご相談にのってください。"},{"id":{"d":693,"n":10,"t":1},"speaker":"B","text":"もちろんです。居住空間も重要ですが、家賃や場所もよく検討するのがいいでしょう。"}];
INSERT INTO sentence [{"id":{"d":694,"n":1,"t":1},"speaker":"A","text":"リビングルームに新しいソファを置こうと思っています。"},{"id":{"d":694,"n":2,"t":1},"speaker":"B","text":"前のソファはどうしたのですか？"},{"id":{"d":694,"n":3,"t":1},"speaker":"A","text":"古くなってきて、汚れや破損部分が目立つようになってきました。"},{"id":{"d":694,"n":4,"t":1},"speaker":"B","text":"年代物のソファでしたね。修理してみてはいかがですか？"},{"id":{"d":694,"n":5,"t":1},"speaker":"A","text":"それは考えてみました。業者に見積もりを頼んだのですが、かなり高額だったのです。"},{"id":{"d":694,"n":6,"t":1},"speaker":"B","text":"アンティークは修繕費も高いかもしれません。では、骨とう品店で買い取ってもらうのはどうですか？"},{"id":{"d":694,"n":7,"t":1},"speaker":"A","text":"骨とう品店は考えてみませんでした。粗大ごみでお金がかかるとばかり考えていました。"},{"id":{"d":694,"n":8,"t":1},"speaker":"B","text":"骨とう品店に買い取ってもらえれば次のソファをもっと良いものにすることができるかもしれません。"},{"id":{"d":694,"n":9,"t":1},"speaker":"A","text":"それではまずソファを買い取ってもらえそうな所に連絡しましょう。"}];
INSERT INTO sentence [{"id":{"d":695,"n":1,"t":1},"speaker":"A","text":"町内会のゲートボールクラブでメンバーを募集しているようですよ。"},{"id":{"d":695,"n":2,"t":1},"speaker":"B","text":"それは初耳です。どこで知ったのですか？"},{"id":{"d":695,"n":3,"t":1},"speaker":"A","text":"回覧板で回ってくる町内新聞に、ゲートボールクラブのメンバー募集が載っていました。"},{"id":{"d":695,"n":4,"t":1},"speaker":"B","text":"佐藤さんはゲートボールクラブに入会したいのですか。ゲートボールに興味があるとは知りませんでした。"},{"id":{"d":695,"n":5,"t":1},"speaker":"A","text":"ゴルフに行く回数も減るので、代わりに町内の交友を図ろうと思っています。"},{"id":{"d":695,"n":6,"t":1},"speaker":"B","text":"ゴルフにはもう行かないのですか？"},{"id":{"d":695,"n":7,"t":1},"speaker":"A","text":"肩と腰の痛みがひかないので思い通りにクラブが振れなくなってきています。"},{"id":{"d":695,"n":8,"t":1},"speaker":"B","text":"ゲートボールならゴルフほど肩や腰に負担はかからないということですか？"},{"id":{"d":695,"n":9,"t":1},"speaker":"A","text":"そうです。ところで君も一緒にゲートボールクラブに入りませんか？"},{"id":{"d":695,"n":10,"t":1},"speaker":"B","text":"お誘いありがとうございます。ちょっと考えてみます。"}];
INSERT INTO sentence [{"id":{"d":696,"n":1,"t":1},"speaker":"A","text":"お米のご飯は週に何回食べますか？"},{"id":{"d":696,"n":2,"t":1},"speaker":"B","text":"週2回か3回です。"},{"id":{"d":696,"n":3,"t":1},"speaker":"A","text":"あまり頻繁には食べないのですね。何か理由があるのですか？"},{"id":{"d":696,"n":4,"t":1},"speaker":"B","text":"私は1日2食なので、そもそも食事の回数が少し少ないかもしれません。"},{"id":{"d":696,"n":5,"t":1},"speaker":"A","text":"なるほど。それでもお米の回数は少ないように思います。主食は主に何を食べているのですか？"},{"id":{"d":696,"n":6,"t":1},"speaker":"B","text":"麺類が多いです。うどんやそばにそうめんと冷や麦とインスタントラーメンも食べます。"},{"id":{"d":696,"n":7,"t":1},"speaker":"A","text":"昔から麺類が多いのですか？"},{"id":{"d":696,"n":8,"t":1},"speaker":"B","text":"そうですね。麺類は調理が簡単なので、1人暮らしになってからは一層よく食べるようになりました。"}];
INSERT INTO sentence [{"id":{"d":697,"n":1,"t":1},"speaker":"A","text":"ごちそうといえば何を思い浮かべますか？"},{"id":{"d":697,"n":2,"t":1},"speaker":"B","text":"ステーキでしょうか？ボリュームのある肉の塊を見ると気分が上がります。ワインと一緒にいただくのが幸せです。"},{"id":{"d":697,"n":3,"t":1},"speaker":"A","text":"なるほど、参考になります。今度の週末に娘の誕生日パーティーをするのでごちそうを用意したいのですが、そのメニューを決めかねていたのです。"},{"id":{"d":697,"n":4,"t":1},"speaker":"B","text":"そういうことでしたら、唐揚げはどうでしょうか？"},{"id":{"d":697,"n":5,"t":1},"speaker":"A","text":"鶏の唐揚げですか？"},{"id":{"d":697,"n":6,"t":1},"speaker":"B","text":"はい。子どもの頃は、母が作ってくれる揚げたての鶏の唐揚げが一番のごちそうでした。"},{"id":{"d":697,"n":7,"t":1},"speaker":"A","text":"揚げたてというのがすてきですね。娘は鶏の唐揚げが好きなので、喜んでくれそうです。"}];
INSERT INTO sentence [{"id":{"d":698,"n":1,"t":1},"speaker":"A","text":"ピザの生地はどうしましょうか？"},{"id":{"d":698,"n":2,"t":1},"speaker":"B","text":"ピザの生地にはあまり詳しくありませんが、どのようなものがありますか？"},{"id":{"d":698,"n":3,"t":1},"speaker":"A","text":"まずは、周りのみみ部分が土手のようになっているハンドトスです。ふわっとしています。"},{"id":{"d":698,"n":4,"t":1},"speaker":"B","text":"食べ応えはありそうですが、端までチーズがあるともっとおいしそうですね。"},{"id":{"d":698,"n":5,"t":1},"speaker":"A","text":"土手がなく端までチーズがのっている生地ならばイタリアンがいいでしょう。"},{"id":{"d":698,"n":6,"t":1},"speaker":"B","text":"おいしそうですね。他にも種類があるのですか？"},{"id":{"d":698,"n":7,"t":1},"speaker":"A","text":"薄めの生地をサクサクに焼いたクリスピータイプもあります。"},{"id":{"d":698,"n":8,"t":1},"speaker":"B","text":"いろいろな種類があるのですね。私はふんわりとした生地の方が好みです。"},{"id":{"d":698,"n":9,"t":1},"speaker":"A","text":"分かりました。それでは、このイタリアンを注文することにしましょう。"}];
INSERT INTO sentence [{"id":{"d":699,"n":1,"t":1},"speaker":"A","text":"ばら肉の名前の由来を知っていますか？"},{"id":{"d":699,"n":2,"t":1},"speaker":"B","text":"何でしょうか？細切れ状にバラバラになった肉、というのはどうでしょう？"},{"id":{"d":699,"n":3,"t":1},"speaker":"A","text":"残念ながら違います。ばら肉という名前は肉の部位に関係しているそうですよ。"},{"id":{"d":699,"n":4,"t":1},"speaker":"B","text":"部位ですか。ばらがどの部位のことなのか見当もつきません。"},{"id":{"d":699,"n":5,"t":1},"speaker":"A","text":"あばらのことだそうです。あばら肉からばら肉へと変化したようです。"},{"id":{"d":699,"n":6,"t":1},"speaker":"B","text":"なるほど、そうでしたか。勉強になりました。"},{"id":{"d":699,"n":7,"t":1},"speaker":"A","text":"私も最近知りました。だから誰かに言いたくて。"},{"id":{"d":699,"n":8,"t":1},"speaker":"B","text":"ばら肉は普段からよく使うのですが、全く知りませんでした。"},{"id":{"d":699,"n":9,"t":1},"speaker":"A","text":"ばら肉はおいしいですよね。私も、よくブロック状のばら肉を買ってきて豚の角煮を作ります。"}];
INSERT INTO sentence [{"id":{"d":700,"n":1,"t":1},"speaker":"A","text":"このような暑い日にはアイスクリームが食べたくなりますね。アイスクリームは何味が好きですか？"},{"id":{"d":700,"n":2,"t":1},"speaker":"B","text":"私は抹茶アイスクリームが一番好きです。甘過ぎないところが好みです。"},{"id":{"d":700,"n":3,"t":1},"speaker":"A","text":"抹茶はおいしいですね。私もお茶系のアイスクリームは好きです。紅茶やコーヒー、ほうじ茶などもおいしいです。"},{"id":{"d":700,"n":4,"t":1},"speaker":"B","text":"ほうじ茶のアイスクリームがあるのですか？初めて聞きました。"},{"id":{"d":700,"n":5,"t":1},"speaker":"A","text":"ありますよ。期間限定商品であることが多いですが、スーパーマーケットやコンビニエンスストアでも買えます。"},{"id":{"d":700,"n":6,"t":1},"speaker":"B","text":"そうですか。とても興味があります。早速今日の帰りに探してみようと思います。"},{"id":{"d":700,"n":7,"t":1},"speaker":"A","text":"ほうじ茶の香ばしい香りと少しの苦味が程よいアクセントになり、とてもおいしいです。"}];
INSERT INTO sentence [{"id":{"d":701,"n":1,"t":1},"speaker":"B","text":"今日の服はオーバーオールですか？"},{"id":{"d":701,"n":2,"t":1},"speaker":"A","text":"ええ、どうですか？"},{"id":{"d":701,"n":3,"t":1},"speaker":"B","text":"よく似合っていますよ。オーバーオール、懐かしいですね。"},{"id":{"d":701,"n":4,"t":1},"speaker":"A","text":"懐かしい？"},{"id":{"d":701,"n":5,"t":1},"speaker":"B","text":"昔、大流行していました。私も若い頃はオーバーオールをよく着ていたものです。"},{"id":{"d":701,"n":6,"t":1},"speaker":"A","text":"そうでしたか。"},{"id":{"d":701,"n":7,"t":1},"speaker":"B","text":"Tシャツにオーバーオールを来た若者が街中にいて、どこを見ても目に入るほどでした。"},{"id":{"d":701,"n":8,"t":1},"speaker":"A","text":"それは驚きですね。全く想像がつきません。"},{"id":{"d":701,"n":9,"t":1},"speaker":"B","text":"今思うと結構面白い光景でした。最近は、街でオーバーオールを着ている人を見かけることはほとんどありませんね。"}];
INSERT INTO sentence [{"id":{"d":702,"n":1,"t":1},"speaker":"A","text":"この家の火災報知器は何年前に設置しましたか？"},{"id":{"d":702,"n":2,"t":1},"speaker":"B","text":"15年くらい前だったと思います。"},{"id":{"d":702,"n":3,"t":1},"speaker":"A","text":"10年以上経過したら機器本体の交換をした方がいいらしいですよ。"},{"id":{"d":702,"n":4,"t":1},"speaker":"B","text":"交換が必要かどうかを判断するために、まずはこの火災報知器が正常に作動しているかを調べましょう。"},{"id":{"d":702,"n":5,"t":1},"speaker":"A","text":"そうですね。火災報知器の説明書はありますか？"},{"id":{"d":702,"n":6,"t":1},"speaker":"B","text":"説明書ばかり集めて引き出しに入れていますから、そこにあるでしょう。"},{"id":{"d":702,"n":7,"t":1},"speaker":"A","text":"ありました。家には全部で何個の火災報知器がありますか？"},{"id":{"d":702,"n":8,"t":1},"speaker":"B","text":"台所を抜かすと全部で5台か6台あるはずです。"},{"id":{"d":702,"n":9,"t":1},"speaker":"A","text":"なぜ台所の火災報知器を抜かすのですか？"},{"id":{"d":702,"n":10,"t":1},"speaker":"B","text":"台所はガス会社の火災報知機で、定期的に検査員が交換してくれているので、今回調べる必要はありません。"}];
INSERT INTO sentence [{"id":{"d":703,"n":1,"t":1},"speaker":"A","text":"今週末のお誕生日会の準備について、最終確認をしましょう。"},{"id":{"d":703,"n":2,"t":1},"speaker":"B","text":"はい。ケーキは洋菓子店に予約しました。"},{"id":{"d":703,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。私は前の日までにクッキーとプリンを作っておこうと思います。"},{"id":{"d":703,"n":4,"t":1},"speaker":"B","text":"ありがとうございます、楽しみです。当日のフォークや箸は使い捨てできるものにしましょう。コップと皿も紙製のものにしましょう。"},{"id":{"d":703,"n":5,"t":1},"speaker":"A","text":"そうですね。片付けが楽なのでそれがいいです。そういえば、飲み物の準備はどうしましょうか？"},{"id":{"d":703,"n":6,"t":1},"speaker":"B","text":"ジュース類は家にあるものを当日見てから、必要なら買いにいきましょう。"},{"id":{"d":703,"n":7,"t":1},"speaker":"A","text":"分かりました。最後に、料理はどうしますか？子ども10人分の料理を当日に作るのは大変です。"},{"id":{"d":703,"n":8,"t":1},"speaker":"B","text":"料理は必要ありません。そのために、お誕生会の開始時刻を午後3時にしています。"},{"id":{"d":703,"n":9,"t":1},"speaker":"A","text":"おやつ時にすれば、料理は必要ないということですか？"},{"id":{"d":703,"n":10,"t":1},"speaker":"B","text":"はい、その通りです。"},{"id":{"d":703,"n":11,"t":1},"speaker":"A","text":"良い考えだと思います。"}];
INSERT INTO sentence [{"id":{"d":704,"n":1,"t":1},"speaker":"A","text":"外国の料理番組を見ているとハーブの多さに気が付きますね。"},{"id":{"d":704,"n":2,"t":1},"speaker":"B","text":"はい。しかもパウダー状のものではなく、生のものを使っています。"},{"id":{"d":704,"n":3,"t":1},"speaker":"A","text":"パセリ、バジル、ローレルくらいまでは用意できますが、タイムやコリアンダーとなると近所の店では生のものが売っていないときもあります。"},{"id":{"d":704,"n":4,"t":1},"speaker":"B","text":"ミント、オレガノ、セージ、レモングラスもなかなか置いている店は少ないです。"},{"id":{"d":704,"n":5,"t":1},"speaker":"A","text":"全部を置いているお店は限られているでしょうね。"},{"id":{"d":704,"n":6,"t":1},"speaker":"B","text":"そういえば、昨日見ていた外国の料理番組ではワケギやシソを使っていました。"},{"id":{"d":704,"n":7,"t":1},"speaker":"A","text":"日本の野菜も使われているのですね。"},{"id":{"d":704,"n":8,"t":1},"speaker":"B","text":"ワケギやシソも、ハーブと同じようにたくさんの油で調理されていて驚きました。"}];
INSERT INTO sentence [{"id":{"d":705,"n":1,"t":1},"speaker":"A","text":"ガス自動検針機の取り付けに業者が来ます。幸子さんは再来週の月曜日の午前中に家にいられますか？"},{"id":{"d":705,"n":2,"t":1},"speaker":"B","text":"その時間に家にはいられません。機械の取り付けに立ち会わなければならないのですか？"},{"id":{"d":705,"n":3,"t":1},"speaker":"A","text":"都合がつかない場合は業者に連絡をしなければなりません。"},{"id":{"d":705,"n":4,"t":1},"speaker":"B","text":"連絡を入れて、和雄さんの都合の良い日に業者に来てもらってはいかがですか？"},{"id":{"d":705,"n":5,"t":1},"speaker":"A","text":"私の予定は直前にならないと分かりません。幸子さんの都合で取り付け日を選んでほしいと思っています。"},{"id":{"d":705,"n":6,"t":1},"speaker":"B","text":"分かりました。業者への連絡は電話でするのですか？"},{"id":{"d":705,"n":7,"t":1},"speaker":"A","text":"取り付け日の変更は電話とホームページでも受け付けています。"},{"id":{"d":705,"n":8,"t":1},"speaker":"B","text":"私の予定を確認してからホームページにアクセスしましょう。業者への金銭の受け渡しはありますか？"},{"id":{"d":705,"n":9,"t":1},"speaker":"A","text":"全て無料ですから金銭の受け渡しはありません。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":706,"n":1,"t":1},"speaker":"A","text":"今日の午後にエアコンの取り付けに業者が来ます。しかし私は急な用事で2時間ほど外出しなければなりません。"},{"id":{"d":706,"n":2,"t":1},"speaker":"B","text":"私はずっと家にいますので、業者に応対できます。"},{"id":{"d":706,"n":3,"t":1},"speaker":"A","text":"それではお願いしたいのですが、もしかしたらエアコンの取り付けに追加料金が発生するかもしれません。"},{"id":{"d":706,"n":4,"t":1},"speaker":"B","text":"エアコン購入時に取り付けについては話し合われないのですか？"},{"id":{"d":706,"n":5,"t":1},"speaker":"A","text":"エアコンの取り付け業者は量販店とは違うので、取り付けの詳細までは分からないそうです。"},{"id":{"d":706,"n":6,"t":1},"speaker":"B","text":"取り付けの追加料金は現金受け渡しなのですか？"},{"id":{"d":706,"n":7,"t":1},"speaker":"A","text":"現金受け渡しと量販店の店員から聞きました。お金を渡しておきましょうか？"},{"id":{"d":706,"n":8,"t":1},"speaker":"B","text":"手持ちの現金はありませんので、預かっておいた方がよさそうです。"},{"id":{"d":706,"n":9,"t":1},"speaker":"A","text":"それではよろしくお願いします。もし問題が発生しましたら、ラインに連絡をください。"}];
INSERT INTO sentence [{"id":{"d":707,"n":1,"t":1},"speaker":"A","text":"除草剤をまこうと思っていますが、どのようなものが良いでしょう。"},{"id":{"d":707,"n":2,"t":1},"speaker":"B","text":"今まで使っていた除草剤では駄目なのですか？"},{"id":{"d":707,"n":3,"t":1},"speaker":"A","text":"今までの除草剤は数カ月で効果が薄れるので、年に数回まかなければなりません。少し面倒です。"},{"id":{"d":707,"n":4,"t":1},"speaker":"B","text":"もっと効果が続く除草剤があるのですか？"},{"id":{"d":707,"n":5,"t":1},"speaker":"A","text":"顆粒タイプなら半年以上効果は持続するそうです。"},{"id":{"d":707,"n":6,"t":1},"speaker":"B","text":"隣家はネコを飼っていますから、除草剤の安全性に注意してください。"},{"id":{"d":707,"n":7,"t":1},"speaker":"A","text":"食品添加物100パーセントの除草剤ならば安全でしょう。"},{"id":{"d":707,"n":8,"t":1},"speaker":"B","text":"安全性にだけ気を付けてくれれば、除草剤の種類は使う人にお任せします。"},{"id":{"d":707,"n":9,"t":1},"speaker":"A","text":"分かりました。人やペットに安全で、私の除草作業回数を減らせる除草剤を選びます。"}];
INSERT INTO sentence [{"id":{"d":708,"n":1,"t":1},"speaker":"A","text":"子どもがハムスターを飼いたいと言っているのですが、飼うべきか悩んでいます。"},{"id":{"d":708,"n":2,"t":1},"speaker":"B","text":"生き物を飼うことは良いことですが、子どもの興味は移り変わりしやすいので注意が必要です。"},{"id":{"d":708,"n":3,"t":1},"speaker":"A","text":"相澤さんのお宅ではウサギを飼っていましたよね。ペットを飼う上での心構えがあれば、教えてもらえますか？"},{"id":{"d":708,"n":4,"t":1},"speaker":"B","text":"家族みんなで飼うつもりになれば、個人の負担は少なく楽しみは多くなると思っています。"},{"id":{"d":708,"n":5,"t":1},"speaker":"A","text":"家族で協力すればペットにとっても良い環境になりそうですね。ハムスターを飼う上で何か注意する点はありますか？"},{"id":{"d":708,"n":6,"t":1},"speaker":"B","text":"ハムスターは寿命が数年ですから、子どもはその死を間近にすることとなります。かわいがっているペットであれば、その死にはきっと大きなショックを受けてしまう可能性があります。"},{"id":{"d":708,"n":7,"t":1},"speaker":"A","text":"なるほど。子どもにはハムスターの寿命を伝えた上で、本当に飼うかどうかもう一度よく話し合ってみます。"}];
INSERT INTO sentence [{"id":{"d":709,"n":1,"t":1},"speaker":"A","text":"南向きの壁の所にゴーヤーを育てようかと思っています。"},{"id":{"d":709,"n":2,"t":1},"speaker":"B","text":"ゴーヤーは比較的簡単に育てられますし、おいしい実がなりますので、ぜひとも育ててみるといいと思います。"},{"id":{"d":709,"n":3,"t":1},"speaker":"A","text":"簡単と聞いて安心しました。挑戦してみます。"},{"id":{"d":709,"n":4,"t":1},"speaker":"B","text":"ゴーヤーはどんどんツルが伸びるので、南側に植えれば夏は日よけにもなります。"},{"id":{"d":709,"n":5,"t":1},"speaker":"A","text":"そういえば、ゴーヤーを植えようと思っている場所に除草剤をまいたばかりですが、植物を植えて大丈夫ですか？"},{"id":{"d":709,"n":6,"t":1},"speaker":"B","text":"ゴーヤーは土に直接植えるのではなく、プランターに植えるので大丈夫です。"},{"id":{"d":709,"n":7,"t":1},"speaker":"A","text":"それはよかったです。芽が出たら、ツルが伸びるようにネットを張る必要があるそうです。もしよければ、その時は手伝ってもらえませんか？"},{"id":{"d":709,"n":8,"t":1},"speaker":"B","text":"もちろん構いませんよ。お任せください。"},{"id":{"d":709,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。今から収穫が楽しみです。"}];
INSERT INTO sentence [{"id":{"d":710,"n":1,"t":1},"speaker":"A","text":"学校の宿題で、家のお雑煮を調べてくるように言われました。"},{"id":{"d":710,"n":2,"t":1},"speaker":"B","text":"お雑煮を調べるなんて、面白い宿題ですね。"},{"id":{"d":710,"n":3,"t":1},"speaker":"A","text":"社会の宿題です。教科書には全国お雑煮マップが載っていました。いろいろな種類があって面白かったです。"},{"id":{"d":710,"n":4,"t":1},"speaker":"B","text":"わが家のお雑煮がどのようなものかは覚えていますか？"},{"id":{"d":710,"n":5,"t":1},"speaker":"A","text":"すまし汁だったと思うのですが、違いましたか？"},{"id":{"d":710,"n":6,"t":1},"speaker":"B","text":"わが家は京都風にしています。丸いお餅に白みそのお雑煮です。"},{"id":{"d":710,"n":7,"t":1},"speaker":"A","text":"白みそでしたか。お雑煮の具には何が入っていますか？"},{"id":{"d":710,"n":8,"t":1},"speaker":"B","text":"ダイコンやニンジンやサトイモを入れています。具は全部丸く切っているのですよ。"},{"id":{"d":710,"n":9,"t":1},"speaker":"A","text":"具を丸くする意味はなんでしょう。"},{"id":{"d":710,"n":10,"t":1},"speaker":"B","text":"お雑煮の丸い具材は、家族円満で過ごすことを願っています。"}];
INSERT INTO sentence [{"id":{"d":711,"n":1,"t":1},"speaker":"A","text":"家の郵便ポストを新しいものに換えたいのです。"},{"id":{"d":711,"n":2,"t":1},"speaker":"B","text":"郵便ポストは随分前から使っていましたが、どこか壊れましたか？"},{"id":{"d":711,"n":3,"t":1},"speaker":"A","text":"壊れたわけではありません。もっと大きな郵便ポストにしたいのです。"},{"id":{"d":711,"n":4,"t":1},"speaker":"B","text":"郵便物や宅配物が郵便ポストに入らないからですか？"},{"id":{"d":711,"n":5,"t":1},"speaker":"A","text":"今は書籍やCDも宅配便にしているので、ポストは大きい方がいいのです。"},{"id":{"d":711,"n":6,"t":1},"speaker":"B","text":"今の郵便ポストでは大きな郵便物は入らないので、交換するのもいいでしょう。"},{"id":{"d":711,"n":7,"t":1},"speaker":"A","text":"もうインターネットで調べて、いくつか新しい郵便ポスト候補がありますが、家族みんなで話し合いますか？"},{"id":{"d":711,"n":8,"t":1},"speaker":"B","text":"宅配便をよく使うのはあなたなので、あなたが使いやすい郵便ポストにしてよいですよ。"},{"id":{"d":711,"n":9,"t":1},"speaker":"A","text":"分かりました。それでは今日中に注文しようと思います。"}];
INSERT INTO sentence [{"id":{"d":712,"n":1,"t":1},"speaker":"A","text":"物置を整理していたら編み機が出てきました。"},{"id":{"d":712,"n":2,"t":1},"speaker":"B","text":"懐かしいですね。その編み機は、私が子どもの頃に祖母が使っていたものです。"},{"id":{"d":712,"n":3,"t":1},"speaker":"A","text":"セーターやマフラー、腹巻も一緒に出てきました。これも彼女の作品でしょうか？"},{"id":{"d":712,"n":4,"t":1},"speaker":"B","text":"恐らくそうです。祖母には、着なくなったセーターの毛糸をほぐして丸めさせられたものです。"},{"id":{"d":712,"n":5,"t":1},"speaker":"A","text":"この編み機は、まだ使えるのでしょうか？"},{"id":{"d":712,"n":6,"t":1},"speaker":"B","text":"古い編み機ですので、もう動かないかもしれませんね。この機に処分してしまいましょうか？"},{"id":{"d":712,"n":7,"t":1},"speaker":"A","text":"たとえ動かなくてもあなたにとってはおばあさまとの思い出の品でしょう。処分はせずこのまま大事にとっておきましょう。"}];
INSERT INTO sentence [{"id":{"d":713,"n":1,"t":1},"speaker":"A","text":"今度新しく家の自転車を買おうと思っています。何か希望はありますか？"},{"id":{"d":713,"n":2,"t":1},"speaker":"B","text":"ぜひとも電動アシスト自転車にしてください。"},{"id":{"d":713,"n":3,"t":1},"speaker":"A","text":"普通の自転車でもよいように思いますが、何か利点があるのですか？"},{"id":{"d":713,"n":4,"t":1},"speaker":"B","text":"電動アシスト自転車ならば坂道も苦になりませんし、自動車よりは気軽に乗ることができます。"},{"id":{"d":713,"n":5,"t":1},"speaker":"A","text":"電動アシスト自転車はちょっと高価なように思います。10万円以上しませんか？"},{"id":{"d":713,"n":6,"t":1},"speaker":"B","text":"今はその半値くらいから電動アシスト自転車を販売しています。中古ならばもっと安いと思います。"},{"id":{"d":713,"n":7,"t":1},"speaker":"A","text":"中古の電動アシスト自転車はモーターやバッテリーの状態が分かりませんので不安です。"},{"id":{"d":713,"n":8,"t":1},"speaker":"B","text":"新品でも安価な電動アシスト自転車があったと思います。"},{"id":{"d":713,"n":9,"t":1},"speaker":"A","text":"分かりました。今度一緒に電動アシスト自転車を見にいきましょう。"},{"id":{"d":713,"n":10,"t":1},"speaker":"B","text":"そうしましょう。きっとあなたも電動アシスト自転車に乗りたくなりますよ。"}];
INSERT INTO sentence [{"id":{"d":714,"n":1,"t":1},"speaker":"A","text":"庭の置物がどんどん増えているような気がします。"},{"id":{"d":714,"n":2,"t":1},"speaker":"B","text":"気が付きましたか。ドワーフの置物を集めているのです。"},{"id":{"d":714,"n":3,"t":1},"speaker":"A","text":"あれはドワーフなのですか。やはり白雪姫に登場する7人の小人がモデルですか？"},{"id":{"d":714,"n":4,"t":1},"speaker":"B","text":"ご名答です。7人の小人シリーズを1個ずつ集めていました。"},{"id":{"d":714,"n":5,"t":1},"speaker":"A","text":"7人の小人には一人一人に名前があるのでしたか？"},{"id":{"d":714,"n":6,"t":1},"speaker":"B","text":"そうです。置物にもちゃんと名前が書かれていますよ。"},{"id":{"d":714,"n":7,"t":1},"speaker":"A","text":"後で見てみます。なぜ、ドワーフを集めることにしたのですか？"},{"id":{"d":714,"n":8,"t":1},"speaker":"B","text":"少し前に『アメリ』というフランス映画を見て、そこで面白い使われ方をしているドワーフに興味を持ったがきっかけです。"},{"id":{"d":714,"n":9,"t":1},"speaker":"A","text":"なるほど、そうだったのですね。"},{"id":{"d":714,"n":10,"t":1},"speaker":"B","text":"ちなみに、ドワーフは正式にはガーデンノームというらしいです。"}];
INSERT INTO sentence [{"id":{"d":715,"n":1,"t":1},"speaker":"A","text":"一度これまでに届いた年賀状やはがきを処分してもよいように思います。いつの間にか随分たまってしまいました。"},{"id":{"d":715,"n":2,"t":1},"speaker":"B","text":"はがきはどのように処分するのですか？個人情報だからそのまま捨てるわけにもいかないでしょう。"},{"id":{"d":715,"n":3,"t":1},"speaker":"A","text":"はがきはシュレッダーで断裁すればよいのですが、この間壊れてしまいました。"},{"id":{"d":715,"n":4,"t":1},"speaker":"B","text":"ではまずはシュレッダーを買い替えますか？"},{"id":{"d":715,"n":5,"t":1},"speaker":"A","text":"そうですね。他にも個人情報が記載された処分すべき書類がありますし、この機にシュレッダーを新調してもよさそうです。"},{"id":{"d":715,"n":6,"t":1},"speaker":"B","text":"では早速来週にでもシュレッダーを見に電器店へ行きましょう。一応、それまでに壊れたシュレッダーを直せるか確認してみますね。"},{"id":{"d":715,"n":7,"t":1},"speaker":"A","text":"はい、ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":716,"n":1,"t":1},"speaker":"A","text":"近くの神社でとりの市が立っています。今から行ってきますが何か欲しいものはありますか？"},{"id":{"d":716,"n":2,"t":1},"speaker":"B","text":"ちょうどよかったです。とりの市でしめ縄を2つ買ってきてください。"},{"id":{"d":716,"n":3,"t":1},"speaker":"A","text":"どのようなしめ縄を買えばよいのですか？"},{"id":{"d":716,"n":4,"t":1},"speaker":"B","text":"玄関用のしめ縄は玉飾りで、神棚用のしめ縄は輪飾りでお願いします。"},{"id":{"d":716,"n":5,"t":1},"speaker":"A","text":"玉飾りと輪飾りの区別がつきませんが、どうすればよいでしょうか？"},{"id":{"d":716,"n":6,"t":1},"speaker":"B","text":"玉飾りと輪飾りの違いは、しめ縄を売っている屋台の人に尋ねてください。"},{"id":{"d":716,"n":7,"t":1},"speaker":"A","text":"分かりました。もしとても混雑していて人に尋ねられそうにないときは、しめ縄の写真を撮ってメールします。それを見て指示してくれますか？"},{"id":{"d":716,"n":8,"t":1},"speaker":"B","text":"分かりました。しめ縄も値段もいろいろですから、その方がいいでしょう。"},{"id":{"d":716,"n":9,"t":1},"speaker":"A","text":"他に何か欲しいものはありますか？"},{"id":{"d":716,"n":10,"t":1},"speaker":"B","text":"特にありません。しめ縄だけお願いします。"}];
INSERT INTO sentence [{"id":{"d":717,"n":1,"t":1},"speaker":"A","text":"ディスカウントストアに行ってきます。買ってきてほしいものはありますか？"},{"id":{"d":717,"n":2,"t":1},"speaker":"B","text":"たくさんありますがお願いしてもよいですか？トイレットペーパ、シャンプー、リンス、ティッシュ、洗濯洗剤、歯磨き粉と歯ブラシを買ってきてほしいです。"},{"id":{"d":717,"n":3,"t":1},"speaker":"A","text":"買うことは問題ありません。ただ、いつも使っているメーカや種類が分かりません。何を買ってきてもよいのですか？"},{"id":{"d":717,"n":4,"t":1},"speaker":"B","text":"できればいつもと同じものがいいので、いま使っているものをメールで送ります。メールの写真を見て同じものを買ってきてください。"},{"id":{"d":717,"n":5,"t":1},"speaker":"A","text":"分かりました。もし同じ品物がなかった場合にはどうすればよいですか？"},{"id":{"d":717,"n":6,"t":1},"speaker":"B","text":"それはその時に判断します。同じ品物がないことをメールで教えてもらえますか？"},{"id":{"d":717,"n":7,"t":1},"speaker":"A","text":"いっそのこと一緒にディスカウントストアに行きませんか？"},{"id":{"d":717,"n":8,"t":1},"speaker":"B","text":"そうしたいのは山々なのですが、私はこの後、町内会の会合に出席しなければなりません。メールは会合中でもできますので、買い物はよろしくお願いいたします。"},{"id":{"d":717,"n":9,"t":1},"speaker":"A","text":"分かりました。それではそのようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":718,"n":1,"t":1},"speaker":"A","text":"今年のクリスマスケーキはどこで買いますか？"},{"id":{"d":718,"n":2,"t":1},"speaker":"B","text":"去年までと同じく駅前の洋菓子店でよいのではないでしょうか？"},{"id":{"d":718,"n":3,"t":1},"speaker":"A","text":"その洋菓子店は先月閉店してしまいました。"},{"id":{"d":718,"n":4,"t":1},"speaker":"B","text":"それは残念です。では、私の会社の近くに評判のケーキ屋さんがあるので、そこで買うのはいかがでしょうか？"},{"id":{"d":718,"n":5,"t":1},"speaker":"A","text":"いいですね。"},{"id":{"d":718,"n":6,"t":1},"speaker":"B","text":"どのようなケーキを買うかは私に任せてくれますか？"},{"id":{"d":718,"n":7,"t":1},"speaker":"A","text":"もちろんです。種類はお任せしますが、ピースではなくホールで買うようにしてください。"},{"id":{"d":718,"n":8,"t":1},"speaker":"B","text":"分かりました。ホールケーキを買ってきます。"},{"id":{"d":718,"n":9,"t":1},"speaker":"A","text":"満員電車でケーキを運ぶのは大変かと思いますが、すみませんがよろしくお願いします。"},{"id":{"d":718,"n":10,"t":1},"speaker":"B","text":"了解しました。ケーキをつぶさずに持ち帰れるよう頑張ります。"}];
INSERT INTO sentence [{"id":{"d":719,"n":1,"t":1},"speaker":"A","text":"今日は出前のすしを取ろうと思います。特上ずしを5人前でよいでしょうか？"},{"id":{"d":719,"n":2,"t":1},"speaker":"B","text":"子どもが1人いますので、1つはわさび抜きにしましょう。"},{"id":{"d":719,"n":3,"t":1},"speaker":"A","text":"そうですね。では、特上ずし4人前と特上ずしのわさび抜き1人前にしましょう。"},{"id":{"d":719,"n":4,"t":1},"speaker":"B","text":"すみません、特上ずしにはサーモンが入っていないのですか？"},{"id":{"d":719,"n":5,"t":1},"speaker":"A","text":"はい、そのようです。"},{"id":{"d":719,"n":6,"t":1},"speaker":"B","text":"私はどうしてもサーモンが食べたいので、特上ずしではなく、こちらの上ずしにしてもよいでしょうか？"},{"id":{"d":719,"n":7,"t":1},"speaker":"A","text":"もちろんです。特上ずしのままで、サーモンの握り単品を追加で頼むこともできますが、どうしますか？"},{"id":{"d":719,"n":8,"t":1},"speaker":"B","text":"それほどたくさんは食べられないので、単品の追加ではなく、やはり上ずしでお願いします。"},{"id":{"d":719,"n":9,"t":1},"speaker":"A","text":"分かりました。では、特上ずし3人前と特上ずしのわさび抜き1人前、それから上ずし1人前で注文します。"},{"id":{"d":719,"n":10,"t":1},"speaker":"B","text":"はい。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":720,"n":1,"t":1},"speaker":"A","text":"傘立てにあった私の傘を知りませんか？黄色い水玉模様のものです。"},{"id":{"d":720,"n":2,"t":1},"speaker":"B","text":"その傘は折り畳んで、玄関横の棚に入れておきました。"},{"id":{"d":720,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。靴べらがかかっている棚ですね。"},{"id":{"d":720,"n":4,"t":1},"speaker":"B","text":"そうです。折り畳み傘なのですから、乾いたらしまってはいかがでしょう。"},{"id":{"d":720,"n":5,"t":1},"speaker":"A","text":"せっかく傘を折り畳んでも、家を出る時には開くので、畳む作業が無駄になってしまいます。"},{"id":{"d":720,"n":6,"t":1},"speaker":"B","text":"その折り畳み傘はかばんに入れて持ち運んだりしないのですか？"},{"id":{"d":720,"n":7,"t":1},"speaker":"A","text":"普段から傘を持ち歩いていません。会社には別の折り畳み傘があるので、持ち運ぶ必要がないのです。"},{"id":{"d":720,"n":8,"t":1},"speaker":"B","text":"なるほど。折り畳みの傘を、折り畳まずに使う人は珍しいですね。"}];
INSERT INTO sentence [{"id":{"d":721,"n":1,"t":1},"speaker":"A","text":"最近は正月に大きな門松を飾る家は少なくなってきたように思います。"},{"id":{"d":721,"n":2,"t":1},"speaker":"B","text":"そうかもしれませんね。そもそもあれを飾ることにはどのような意味があるのですか？"},{"id":{"d":721,"n":3,"t":1},"speaker":"A","text":"松も竹も枯れにくい植物であることから神の永遠を願うものと言われています。"},{"id":{"d":721,"n":4,"t":1},"speaker":"B","text":"家の門に門松を飾るのは何か理由があるのですか？"},{"id":{"d":721,"n":5,"t":1},"speaker":"A","text":"神が門松を目印にやってくると言われています。"},{"id":{"d":721,"n":6,"t":1},"speaker":"B","text":"それならば門松はもっと飾られてもよいように思いますが、なぜ減ってきているのでしょうか？"},{"id":{"d":721,"n":7,"t":1},"speaker":"A","text":"紙やミニチュアの門松はまだ出回っていますので、処分のことを考えて大きな門松を購入する人が少なくなっているのではないでしょうか？"},{"id":{"d":721,"n":8,"t":1},"speaker":"B","text":"門松は毎年処分するのですか？"},{"id":{"d":721,"n":9,"t":1},"speaker":"A","text":"基本的にはそうです。毎年お正月が終わると、おたき上げをして処分することが多いです。"},{"id":{"d":721,"n":10,"t":1},"speaker":"B","text":"それは大変な作業ですね。大きな門松を飾る家が減っているのも納得です。"}];
INSERT INTO sentence [{"id":{"d":722,"n":1,"t":1},"speaker":"A","text":"石井さんのご自宅にはソースを何種類置いていますか？"},{"id":{"d":722,"n":2,"t":1},"speaker":"B","text":"2種類です。食卓にはウスターソースととんかつソースがあります。"},{"id":{"d":722,"n":3,"t":1},"speaker":"A","text":"とんかつソースは濃厚ソースとも言いますが、やはりカツ類に使うのですか？"},{"id":{"d":722,"n":4,"t":1},"speaker":"B","text":"私の場合、とんかつやコロッケにはとんかつソースを使いますが、メンチカツだけはウスターソースを使います。"},{"id":{"d":722,"n":5,"t":1},"speaker":"A","text":"そのソースのこだわりはどのようなところから来ているのですか？"},{"id":{"d":722,"n":6,"t":1},"speaker":"B","text":"ウスターソースのメンチ肉にしみこむスパイシーな味わいが好きなのです。"},{"id":{"d":722,"n":7,"t":1},"speaker":"A","text":"なるほど。ソースが染み込んだメンチカツは確かにおいしいです。"},{"id":{"d":722,"n":8,"t":1},"speaker":"B","text":"そういえば、冷蔵庫にオイスターソースも置いてあったかもしれません。"}];
INSERT INTO sentence [{"id":{"d":723,"n":1,"t":1},"speaker":"A","text":"この食卓には唐辛子が2つもありますね。"},{"id":{"d":723,"n":2,"t":1},"speaker":"B","text":"種類が違うようですよ。一味唐辛子と七味唐辛子のようです。"},{"id":{"d":723,"n":3,"t":1},"speaker":"A","text":"本当ですね。あなたはどちらが好きですか？"},{"id":{"d":723,"n":4,"t":1},"speaker":"B","text":"私は普段から七味唐辛子を使うことが多いです。"},{"id":{"d":723,"n":5,"t":1},"speaker":"A","text":"そうなのですね。それはなぜですか？"},{"id":{"d":723,"n":6,"t":1},"speaker":"B","text":"七味唐辛子の方がいろいろな素材が入っていてお得な気がしますし、さまざまな風味があって好みです。"},{"id":{"d":723,"n":7,"t":1},"speaker":"A","text":"アサの実、ケシの実、ゴマやサンショウが入っているので、確かに風味は豊かですね。"},{"id":{"d":723,"n":8,"t":1},"speaker":"B","text":"はい。しかし、私以外の家族は一味唐辛子を好んで使います。"},{"id":{"d":723,"n":9,"t":1},"speaker":"A","text":"なぜ一味唐辛子を使うのかを聞いたことはありますか？"},{"id":{"d":723,"n":10,"t":1},"speaker":"B","text":"一味唐辛子の方が辛みが強いからと言っていました。"},{"id":{"d":723,"n":11,"t":1},"speaker":"A","text":"辛いものが好きなのですね。"},{"id":{"d":723,"n":12,"t":1},"speaker":"B","text":"はい。私はそこまで辛いものが得意ではありませんので、風味の豊かな七味唐辛子を使います。"}];
INSERT INTO sentence [{"id":{"d":724,"n":1,"t":1},"speaker":"A","text":"昔は吹き抜けの建物にあこがれたものですが、住んでみるといろいろ困ることも多いものです。"},{"id":{"d":724,"n":2,"t":1},"speaker":"B","text":"私は自分の家を持つときにはぜひ吹き抜けにしたいと思っていました。何が問題なのですか？"},{"id":{"d":724,"n":3,"t":1},"speaker":"A","text":"吹き抜けがある部屋には広い空間用のエアコンが必要ですし、夏と冬にその広い空間の温度を調整するためにはエアコンを常時稼働させなくてはなりません。"},{"id":{"d":724,"n":4,"t":1},"speaker":"B","text":"それはかなり光熱費がかかりそうです。"},{"id":{"d":724,"n":5,"t":1},"speaker":"A","text":"そうなのです。また、会話やテレビの音が吹き抜けで反響してしまうので、家のどこにいても音が聞こえてしまいます。"},{"id":{"d":724,"n":6,"t":1},"speaker":"B","text":"なるほど。吹き抜けのリビングには憧れがありますが、そのような問題もあるのですね。"},{"id":{"d":724,"n":7,"t":1},"speaker":"A","text":"さらに、吹き抜け部分の天井の照明をつけてしまうと、電球を交換する時はわざわざ業者にお願いする必要があります。"},{"id":{"d":724,"n":8,"t":1},"speaker":"B","text":"言われてみればそうですね。電球交換のたびに業者を呼ぶのは少し面倒そうです。"}];
INSERT INTO sentence [{"id":{"d":725,"n":1,"t":1},"speaker":"A","text":"新しい固定電話の初期設定が終わりました。この後は便利機能の設定をしていきましょう。"},{"id":{"d":725,"n":2,"t":1},"speaker":"B","text":"説明書はこれですね。まずは留守番電話機能を設定しましょう。"},{"id":{"d":725,"n":3,"t":1},"speaker":"A","text":"分かりました。説明書によると、このボタンを押せばよいようですね。"},{"id":{"d":725,"n":4,"t":1},"speaker":"B","text":"はい。そのボタンを押すと留守番電話の応答メッセージを選択する画面が出るそうです。"},{"id":{"d":725,"n":5,"t":1},"speaker":"A","text":"出ました。メッセージは3種類あるようです。どれにしますか？"},{"id":{"d":725,"n":6,"t":1},"speaker":"B","text":"強い希望はありませんが、一番上のものはどうでしょうか？"},{"id":{"d":725,"n":7,"t":1},"speaker":"A","text":"ではこの一番上のメッセージに設定します。"},{"id":{"d":725,"n":8,"t":1},"speaker":"B","text":"これで留守番電話機能の設定は完了だそうです。とても簡単で驚きました。"},{"id":{"d":725,"n":9,"t":1},"speaker":"A","text":"そうですね。他に何か設定したい機能はありますか？"},{"id":{"d":725,"n":10,"t":1},"speaker":"B","text":"この転送機能というのはどういうものですか？必要そうでしょうか？"},{"id":{"d":725,"n":11,"t":1},"speaker":"A","text":"固定電話にかかってきた電話を他のモバイルに外線電話として送ることができます。急ぎの連絡は基本的に初めから携帯電話の方にかかってきますし、個人的に転送機能はなくてもよいと思います。"},{"id":{"d":725,"n":12,"t":1},"speaker":"B","text":"確かにそうですね。他に必要そうな機能はなさそうなので、電話機の設定はこれで完了です。"}];
INSERT INTO sentence [{"id":{"d":726,"n":1,"t":1},"speaker":"A","text":"実家の整理をしていたら多量のライターが出てきました。処分したいのですがどうすればよいのでしょうか？"},{"id":{"d":726,"n":2,"t":1},"speaker":"B","text":"使い捨てのライターでしたら、ガス抜きをしてから自治体の規則に沿って処分すればよいと思います。"},{"id":{"d":726,"n":3,"t":1},"speaker":"A","text":"ライターのガス抜きはレバーを下げた状態にしておくということですか？"},{"id":{"d":726,"n":4,"t":1},"speaker":"B","text":"テープや輪ゴムでレバーを下げた状態で固定して、1日くらいそのままにしておけばよいはずです。"},{"id":{"d":726,"n":5,"t":1},"speaker":"A","text":"100個くらいはあったと思いますので、外でライターのガス抜きをした方がいいですね。"},{"id":{"d":726,"n":6,"t":1},"speaker":"B","text":"全部使い捨てライターでしたか？ジッポやデュポンとかダンヒルでしたら買い取ってもらえるはずです。"},{"id":{"d":726,"n":7,"t":1},"speaker":"A","text":"箱に入ったライターもありました。メーカーは分かりませんが調べてみます。"},{"id":{"d":726,"n":8,"t":1},"speaker":"B","text":"箱に入っているライターでしたら、箱も持っていった方がいいです。"},{"id":{"d":726,"n":9,"t":1},"speaker":"A","text":"箱に入ったままのライターの方が、価値が高いのですね。"},{"id":{"d":726,"n":10,"t":1},"speaker":"B","text":"一般にはそうです。買い取ってくれそうなライターは全部鑑定してもらってから要らないものだけ処分しましょう。"}];
INSERT INTO sentence [{"id":{"d":727,"n":1,"t":1},"speaker":"A","text":"電卓を買ってきてほしいのですが、お願いできますか？"},{"id":{"d":727,"n":2,"t":1},"speaker":"B","text":"スマートフォンの電卓機能ではいけませんか？"},{"id":{"d":727,"n":3,"t":1},"speaker":"A","text":"大きな電卓がいいのです。スマートフォンの電卓では小さ過ぎて打ちづらいのです。"},{"id":{"d":727,"n":4,"t":1},"speaker":"B","text":"100円ショップの電卓でもよいのですか？"},{"id":{"d":727,"n":5,"t":1},"speaker":"A","text":"大きめで、ルート、パーセント、MRCのキーが付いていれば、100円ショップのものでも構いません。"},{"id":{"d":727,"n":6,"t":1},"speaker":"B","text":"そのような機能が付いたものが100円ショップで売っているでしょうか？"},{"id":{"d":727,"n":7,"t":1},"speaker":"A","text":"100円より高価になっているかもしれませんが、売っていると思います。以前見かけたことはあります。"},{"id":{"d":727,"n":8,"t":1},"speaker":"B","text":"分かりました。探してみます。もしなければ文房具屋で買ってきますね。"},{"id":{"d":727,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":728,"n":1,"t":1},"speaker":"A","text":"今度の参議院議員選挙は当日に出張が入ってしまったため、期日前投票にしたいと思っています。"},{"id":{"d":728,"n":2,"t":1},"speaker":"B","text":"期日前投票所は分かりますか？確か前回より1カ所増えているはずです。"},{"id":{"d":728,"n":3,"t":1},"speaker":"A","text":"それは朗報です。市役所近くの投票所は知っていますが、もう少し駅に近い所があればうれしいです。"},{"id":{"d":728,"n":4,"t":1},"speaker":"B","text":"まさに、駅の通路内に投票所ができます。家から最も近くて通勤経路内です。"},{"id":{"d":728,"n":5,"t":1},"speaker":"A","text":"それは最高ですね。いつから投票できるのですか？"},{"id":{"d":728,"n":6,"t":1},"speaker":"B","text":"今週の木曜日からです。北口寄りの通路で、改札から20メートルほど先です。"},{"id":{"d":728,"n":7,"t":1},"speaker":"A","text":"早速木曜の帰りに投票してこようと思います。投票所は何時まで開いているのでしょうか？"},{"id":{"d":728,"n":8,"t":1},"speaker":"B","text":"確か20時までだったと思います。自信がありませんので確かめておいてください。"}];
INSERT INTO sentence [{"id":{"d":729,"n":1,"t":1},"speaker":"A","text":"必ず防災袋に入れておきたい防災グッズは何がありますか？今度の休みに準備しようと思います。"},{"id":{"d":729,"n":2,"t":1},"speaker":"B","text":"多機能ラジオライトはぜひとも入れておかなければならないと思います。"},{"id":{"d":729,"n":3,"t":1},"speaker":"A","text":"多機能ラジオライトはいろいろな種類がありますが、特に必要な機能は何かありますか？"},{"id":{"d":729,"n":4,"t":1},"speaker":"B","text":"ラジオとライトにサイレンとスマートフォンなどの充電機能は必要です。"},{"id":{"d":729,"n":5,"t":1},"speaker":"A","text":"多機能ラジオライトは乾電池でも使えるのですね。"},{"id":{"d":729,"n":6,"t":1},"speaker":"B","text":"はい。しかし、充電方法が乾電池だけの場合、乾電池の交換を忘れると使い物になりません。"},{"id":{"d":729,"n":7,"t":1},"speaker":"A","text":"電池以外にはどのような充電方法がありますか？"},{"id":{"d":729,"n":8,"t":1},"speaker":"B","text":"ソーラーパネル、手回し充電、USBケーブル充電などがあります。何通りかの方法で充電できるものが使いやすいです。バッテリーの容量も重要ですね。ある程度大きくないと用途が限られてしまい少し不便です。"},{"id":{"d":729,"n":9,"t":1},"speaker":"A","text":"充電方法の数は必ず確認して、なるべくバッテリー容量の大きい多機能ラジオライトを購入しようと思います。"}];
INSERT INTO sentence [{"id":{"d":730,"n":1,"t":1},"speaker":"A","text":"クレジットで商品を購入するときに、気を付けていることはありますか？"},{"id":{"d":730,"n":2,"t":1},"speaker":"B","text":"クレジットで使用する預金通帳は1つにして、預金はあまりしないようにしておきます。"},{"id":{"d":730,"n":3,"t":1},"speaker":"A","text":"預金通帳を1つにする理由は何ですか？"},{"id":{"d":730,"n":4,"t":1},"speaker":"B","text":"暗証番号を覚えやすくするためです。変更することもあるので預金通帳を増やすと暗証番号を覚えきれません。"},{"id":{"d":730,"n":5,"t":1},"speaker":"A","text":"今は預金通帳を調べても銀行印を確認できないので、契約書類に押す銀行印が分からないことがあります。"},{"id":{"d":730,"n":6,"t":1},"speaker":"B","text":"それも預金通帳を増やさない理由の一つです。以前は契約書類に押す銀行印をよく間違えていました。"},{"id":{"d":730,"n":7,"t":1},"speaker":"A","text":"預金金額を少なくしている理由は不正利用を警戒してのことですか？"},{"id":{"d":730,"n":8,"t":1},"speaker":"B","text":"そうです。クレジットカードを使ったインターネットショッピングを狙う犯罪が増えているそうなので、最近は特に気を付けています。"},{"id":{"d":730,"n":9,"t":1},"speaker":"A","text":"そのような犯罪の手口はますます巧妙になっていると聞くので、もし被害に遭っても少しの損失で済むように、預金金額を多くしないのは確かに有効な対策かもしれません。"},{"id":{"d":730,"n":10,"t":1},"speaker":"B","text":"はい。口座残高が少ないと無駄な買い物もしなくなる気がしていて、それもまた良いところです。"}];
INSERT INTO sentence [{"id":{"d":731,"n":1,"t":1},"speaker":"A","text":"ご自宅を建てられて先月で5年がたったそうですね。何か気になる点はありますか？"},{"id":{"d":731,"n":2,"t":1},"speaker":"B","text":"しばらく住んでいると、残念ながらいろいろと設計についての後悔も出てきます。例えば、コンセントとスイッチの位置は、もう少し考えて配置すればよかったと思います。"},{"id":{"d":731,"n":3,"t":1},"speaker":"A","text":"普通スイッチはドアの近くに設置するのではありませんか？"},{"id":{"d":731,"n":4,"t":1},"speaker":"B","text":"階段のスイッチを途中まで上らないと点灯できない所に設置してしまいました。"},{"id":{"d":731,"n":5,"t":1},"speaker":"A","text":"多少は不便かもしれませんが、上る途中ならば問題ないように思います。コンセントはどこが不満なのですか？"},{"id":{"d":731,"n":6,"t":1},"speaker":"B","text":"電子レンジと冷蔵庫を置きたい所にコンセントを配置しなかったのです。"},{"id":{"d":731,"n":7,"t":1},"speaker":"A","text":"家電や家具の配置を考慮したデザインを考えておいた方がよかったということですね。"},{"id":{"d":731,"n":8,"t":1},"speaker":"B","text":"そうしたつもりでしたが、まだまだ考えが足りなかったようです。"}];
INSERT INTO sentence [{"id":{"d":732,"n":1,"t":1},"speaker":"A","text":"のし袋が欲しいのですが、何枚かありますか？"},{"id":{"d":732,"n":2,"t":1},"speaker":"B","text":"何枚かあったと思いますが、何に使うのですか？"},{"id":{"d":732,"n":3,"t":1},"speaker":"A","text":"会社の部下で昇進や異動をする者がいるので、餞別や昇進祝いでのし袋を使いたいのです。"},{"id":{"d":732,"n":4,"t":1},"speaker":"B","text":"のし袋は水引の結び方によって意味が違うので、今あるものがその用途に合うか慎重に確認しなければいけません。"},{"id":{"d":732,"n":5,"t":1},"speaker":"A","text":"水引とは、ひものようなもののことですか？"},{"id":{"d":732,"n":6,"t":1},"speaker":"B","text":"そうです。ちょう結びのものは昇進祝いに使用できます。根本の方が丸くなっているあわじ結びはせん別に適しています。"},{"id":{"d":732,"n":7,"t":1},"speaker":"A","text":"のし袋にそのような意味があったのですか。ところで残っているのし袋は何結びですか？"},{"id":{"d":732,"n":8,"t":1},"speaker":"B","text":"結び切りと言われる小さな固い結びのものです。親戚の結婚祝いで使ったので覚えています。"},{"id":{"d":732,"n":9,"t":1},"speaker":"A","text":"結び切りは昇進祝いや餞別には使えないのですか？"},{"id":{"d":732,"n":10,"t":1},"speaker":"B","text":"結び切りは二度と繰り返すことのないようにといった意味です。どちらにも向いていません。"},{"id":{"d":732,"n":11,"t":1},"speaker":"A","text":"そうでしたか。では、明日の出勤途中に用途に合ったのし袋買うことにしましょう。"}];
INSERT INTO sentence [{"id":{"d":733,"n":1,"t":1},"speaker":"A","text":"SNSの知り合いにお礼の品を送りたい場合、どうすればよいでしょうか？"},{"id":{"d":733,"n":2,"t":1},"speaker":"B","text":"会ったこともない人に、なぜ、お礼の品を送るのですか？"},{"id":{"d":733,"n":3,"t":1},"speaker":"A","text":"先日アジア旅行をしたときにSNSの友人から頂いた情報が非常に役に立ったのです。"},{"id":{"d":733,"n":4,"t":1},"speaker":"B","text":"そういうことでしたら、電子チケットやバーコードを使うのはどうですか？"},{"id":{"d":733,"n":5,"t":1},"speaker":"A","text":"なるほど、それは良い考えです。これまでに使ったことはないのですが、確か、コンビニエンスストアなどで電子チケットやバーコードを見せると商品を受け取れるという仕組みでしたよね。"},{"id":{"d":733,"n":6,"t":1},"speaker":"B","text":"そうです。このようなシステムはソーシャルギフトと呼ばれていて、他にもいろいろな選択肢があるので用途に合うサービスを探してみるといいと思います。"},{"id":{"d":733,"n":7,"t":1},"speaker":"A","text":"ソーシャルギフトというのですね。初めて聞いた言葉でした。ありがとうございます。早速調べてみます。"},{"id":{"d":733,"n":8,"t":1},"speaker":"B","text":"良い贈り物ができるといいですね。"}];
INSERT INTO sentence [{"id":{"d":734,"n":1,"t":1},"speaker":"A","text":"庭の木を剪定するために脚立を買おうと思っています。"},{"id":{"d":734,"n":2,"t":1},"speaker":"B","text":"木の剪定なら高枝切り用のはさみの方がいいのではありませんか？"},{"id":{"d":734,"n":3,"t":1},"speaker":"A","text":"高枝切りばさみは意外に扱いが難しいのです。脚立は他にも使い道があります。"},{"id":{"d":734,"n":4,"t":1},"speaker":"B","text":"他にはどのようなところに脚立の使い道があるのですか？"},{"id":{"d":734,"n":5,"t":1},"speaker":"A","text":"例えば、家の電球を交換する時に便利だと思うのです。"},{"id":{"d":734,"n":6,"t":1},"speaker":"B","text":"今までの電球交換には机の上に椅子を置いていたので、確かに脚立を使うなら楽になります。"},{"id":{"d":734,"n":7,"t":1},"speaker":"A","text":"ゴーヤーを育てるネットを張るときにも脚立は便利そうです。"},{"id":{"d":734,"n":8,"t":1},"speaker":"B","text":"そうでしょうか？脚立は家の壁にネットを張れるほど高くないと思います。"},{"id":{"d":734,"n":9,"t":1},"speaker":"A","text":"はしご兼用脚立というものがあります。それならば高さも十分にあります。"},{"id":{"d":734,"n":10,"t":1},"speaker":"B","text":"なるほど。はしごも置いていなかったので、それならば一石二鳥です。"},{"id":{"d":734,"n":11,"t":1},"speaker":"A","text":"今度ホームセンターに行ったとき、見てきましょう。"}];
INSERT INTO sentence [{"id":{"d":735,"n":1,"t":1},"speaker":"A","text":"ホームセンターに行く予定はありますか？スコップを買ってきてほしいのですが。"},{"id":{"d":735,"n":2,"t":1},"speaker":"B","text":"再来週の日曜日にホームセンターに行くので、その時に買えばよいですか？それだと遅いでしょうか？"},{"id":{"d":735,"n":3,"t":1},"speaker":"A","text":"急ぎませんので、再来週の日曜日で構いません。"},{"id":{"d":735,"n":4,"t":1},"speaker":"B","text":"どのようなスコップを買えばよいのですか？"},{"id":{"d":735,"n":5,"t":1},"speaker":"A","text":"冬に雪かきができるような鉄製のスコップを買ってきてください。"},{"id":{"d":735,"n":6,"t":1},"speaker":"B","text":"雪かき用スコップは鉄製ではないものもありますよ。"},{"id":{"d":735,"n":7,"t":1},"speaker":"A","text":"園芸でも使いたいので鉄製のスコップを買ってきてください。"},{"id":{"d":735,"n":8,"t":1},"speaker":"B","text":"雪かき用は先が平たいスコップなので、園芸用には向かないと思います。"},{"id":{"d":735,"n":9,"t":1},"speaker":"A","text":"先が三角形のスコップを想像していました。ここでは雪はそれほど降らないので、三角形のスコップでも雪かきができると思います。"},{"id":{"d":735,"n":10,"t":1},"speaker":"B","text":"先が三角形のものはシャベルというらしいですよ。では鉄製のシャベルを買ってきますね。"},{"id":{"d":735,"n":11,"t":1},"speaker":"A","text":"そうなのですか。今までスコップとシャベルの区別を知りませんでした。はい、それをお願いします。"}];
INSERT INTO sentence [{"id":{"d":736,"n":1,"t":1},"speaker":"A","text":"床拭き用にモップを買おうかと思っています。"},{"id":{"d":736,"n":2,"t":1},"speaker":"B","text":"今まではどうやって床を拭いていたのですか？"},{"id":{"d":736,"n":3,"t":1},"speaker":"A","text":"四つんばいになって、雑巾で拭いていました。"},{"id":{"d":736,"n":4,"t":1},"speaker":"B","text":"そのような拭き方ではとても疲れたのではありませんか？"},{"id":{"d":736,"n":5,"t":1},"speaker":"A","text":"はい、雑巾での床拭きは時間と体力を随分使います。"},{"id":{"d":736,"n":6,"t":1},"speaker":"B","text":"思い切ってスチームクリーナーを購入してはいかがですか？"},{"id":{"d":736,"n":7,"t":1},"speaker":"A","text":"スチームクリーナーは水だけで掃除できるのでしたね。"},{"id":{"d":736,"n":8,"t":1},"speaker":"B","text":"スチームクリーナーはアタッチメントを換えて、いろいろな所を掃除できますよ。"},{"id":{"d":736,"n":9,"t":1},"speaker":"A","text":"スチームクリーナーで汚れはきれいに落ちるのですか？"},{"id":{"d":736,"n":10,"t":1},"speaker":"B","text":"はい。汚れがきれいに落ちることはスチームクリーナーを使っている私が保証します。"},{"id":{"d":736,"n":11,"t":1},"speaker":"A","text":"他の所の汚れも気になるので、思い切ってスチームクリーナーを購入してみましょう。"}];
INSERT INTO sentence [{"id":{"d":737,"n":1,"t":1},"speaker":"A","text":"勉強机用の椅子を新しくしてほしいと言っていましたね。"},{"id":{"d":737,"n":2,"t":1},"speaker":"B","text":"はい、背もたれが壊れてしまい寄りかかることができません。受験前に新しい椅子を購入してもらえるとありがたいです。"},{"id":{"d":737,"n":3,"t":1},"speaker":"A","text":"前と同じ椅子で構いませんか？"},{"id":{"d":737,"n":4,"t":1},"speaker":"B","text":"小学生の時に勉強机とセットで買った椅子だったので、今度は自分で選びたいと思っています。"},{"id":{"d":737,"n":5,"t":1},"speaker":"A","text":"前の椅子は小学生用でしたね。どのような椅子を使いたいのですか？"},{"id":{"d":737,"n":6,"t":1},"speaker":"B","text":"キャスター付きで座面が回転する椅子が欲しいです。"},{"id":{"d":737,"n":7,"t":1},"speaker":"A","text":"肘掛けは要らないのですか？"},{"id":{"d":737,"n":8,"t":1},"speaker":"B","text":"それほど大きな机ではないので、肘掛けがあると机にあたって邪魔です。"},{"id":{"d":737,"n":9,"t":1},"speaker":"A","text":"しっかりと考えて椅子を選んでいるようですから、具体的な購入候補があるなら教えてください。"},{"id":{"d":737,"n":10,"t":1},"speaker":"B","text":"私の欲しい椅子がまだインターネットに残っているか確認しましょう。"}];
INSERT INTO sentence [{"id":{"d":738,"n":1,"t":1},"speaker":"A","text":"玄関に置いてある傘立てがさびて見苦しいので新しいものに替えたいと思っています。"},{"id":{"d":738,"n":2,"t":1},"speaker":"B","text":"さびるのはスチール製の傘立ての弱点ですね。今度は違う材質の傘立てにしましょう。"},{"id":{"d":738,"n":3,"t":1},"speaker":"A","text":"一本ずつ別々にさせる傘立てがいいですか？"},{"id":{"d":738,"n":4,"t":1},"speaker":"B","text":"まとめてさせる傘立てでもよいと思います。"},{"id":{"d":738,"n":5,"t":1},"speaker":"A","text":"場所を取らないので、まとめてさせる傘立ての方がいいでしょう。他に何か希望はありますか？"},{"id":{"d":738,"n":6,"t":1},"speaker":"B","text":"折り畳みの傘を置ける傘立てが欲しいです。"},{"id":{"d":738,"n":7,"t":1},"speaker":"A","text":"傘を掛けるタイプの傘立てがあります。フックが付いているので折り畳みの傘も掛けられます。"},{"id":{"d":738,"n":8,"t":1},"speaker":"B","text":"掛けるタイプの傘立てで構いませんが、さびたりはしませんか？"},{"id":{"d":738,"n":9,"t":1},"speaker":"A","text":"その傘立ての材質までは覚えていません。もう一度インターネットで探してみます。"},{"id":{"d":738,"n":10,"t":1},"speaker":"B","text":"よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":739,"n":1,"t":1},"speaker":"A","text":"定期券はポイントカード付きにしていますか？"},{"id":{"d":739,"n":2,"t":1},"speaker":"B","text":"はい。特典も多いので、ポイント付きカードを使っています。"},{"id":{"d":739,"n":3,"t":1},"speaker":"A","text":"ポイントカードが増えるのはうれしいことですが、使いにくいところが出てきませんか？"},{"id":{"d":739,"n":4,"t":1},"speaker":"B","text":"現金を持ち歩く必要はなくなりましたが、カードは常に複数枚持つようになりました。"},{"id":{"d":739,"n":5,"t":1},"speaker":"A","text":"初めて行く所は、どのカードが使えるのか確認しなければ不安なときがあります。"},{"id":{"d":739,"n":6,"t":1},"speaker":"B","text":"定期券ならば基本的には同じ所で使うでしょうし、ポイントが付いた方がお得な感じがしませんか？"},{"id":{"d":739,"n":7,"t":1},"speaker":"A","text":"確かにそうですね。"},{"id":{"d":739,"n":8,"t":1},"speaker":"B","text":"私は定期券にパスモも付けていますよ。"},{"id":{"d":739,"n":9,"t":1},"speaker":"A","text":"それは普通のポイントカード一体型よりもお得なのですか？"},{"id":{"d":739,"n":10,"t":1},"speaker":"B","text":"パスモ付きならばポイントが1.5倍になります。"},{"id":{"d":739,"n":11,"t":1},"speaker":"A","text":"パスモは既に持っているので、ポイントカードも付けるとそれほどお得なのですね。"}];
INSERT INTO sentence [{"id":{"d":740,"n":1,"t":1},"speaker":"A","text":"定期券が不要になったのですが、払い戻しはできるのでしたか？"},{"id":{"d":740,"n":2,"t":1},"speaker":"B","text":"定期券の払い戻しは可能ですが、手数料が数百円かかるはずです。"},{"id":{"d":740,"n":3,"t":1},"speaker":"A","text":"それでは定期運賃から使用月分と手数料を引いた金額が払戻額になりますか？"},{"id":{"d":740,"n":4,"t":1},"speaker":"B","text":"それが基本的な算出方法だと思います。"},{"id":{"d":740,"n":5,"t":1},"speaker":"A","text":"払い戻しには他に必要なものがありますか？"},{"id":{"d":740,"n":6,"t":1},"speaker":"B","text":"本人確認できる免許証やパスポートが必要だったはずです。"},{"id":{"d":740,"n":7,"t":1},"speaker":"A","text":"本人写真が付いていれば、社員証でもよいですか？"},{"id":{"d":740,"n":8,"t":1},"speaker":"B","text":"私が払い戻したときは運転免許証を使ったので、社員証が使えるかはホームページで確認した方がいいでしょう。"},{"id":{"d":740,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。運転免許証が確実なようですので、運転免許証を持っていくこととします。"}];
INSERT INTO sentence [{"id":{"d":741,"n":1,"t":1},"speaker":"A","text":"家の階段には手すりが付いていませんが、なぜ、付けなかったのでしょうか？"},{"id":{"d":741,"n":2,"t":1},"speaker":"B","text":"引っ越しするときの荷物運びに邪魔になると思ったからです。"},{"id":{"d":741,"n":3,"t":1},"speaker":"A","text":"引っ越ししてから10年がたちますが、いまだに手すりを付けていないのはなぜですか？"},{"id":{"d":741,"n":4,"t":1},"speaker":"B","text":"生活しているうちに手すりを使わないで階段を昇降することに慣れてしまったのです。"},{"id":{"d":741,"n":5,"t":1},"speaker":"A","text":"お客さんが階段を使うとき、降りにくそうにしていました。階段に手すりを付けてはどうでしょうか？"},{"id":{"d":741,"n":6,"t":1},"speaker":"B","text":"手すりを付けられるように板を内張りしていますから、いつでも手すりは付けられますよ。"},{"id":{"d":741,"n":7,"t":1},"speaker":"A","text":"左右両側に手すりは付けられるのですか？"},{"id":{"d":741,"n":8,"t":1},"speaker":"B","text":"手すりは両側に付けられますが、狭く感じるので上るときの右側だけに付けたらどうでしょうか？"},{"id":{"d":741,"n":9,"t":1},"speaker":"A","text":"分かりました。階段右側の寸法を確認して、近いうちにホームセンターで手すりを購入してきます。"}];
INSERT INTO sentence [{"id":{"d":742,"n":1,"t":1},"speaker":"A","text":"近くの神社に一陽来復と書かれたのぼりが立っていました。これはどのような意味でしょうか？"},{"id":{"d":742,"n":2,"t":1},"speaker":"B","text":"冬が終わって春が来るという意味です。悪いことが続いた後、幸せが来るという意味にもなります。"},{"id":{"d":742,"n":3,"t":1},"speaker":"A","text":"なぜ神社で一陽来復ののぼりが立っているのですか？"},{"id":{"d":742,"n":4,"t":1},"speaker":"B","text":"大みそかから節分の期間、一陽来復のお守りが神社で売られているからです。"},{"id":{"d":742,"n":5,"t":1},"speaker":"A","text":"一陽来復のお守りにはどのようなご利益があるのですか？"},{"id":{"d":742,"n":6,"t":1},"speaker":"B","text":"金運上昇、商売繁盛など金銭運に関するお守りです。"},{"id":{"d":742,"n":7,"t":1},"speaker":"A","text":"それは魅力的です。一陽来復のお守りを1つ買ってきましょうか？"},{"id":{"d":742,"n":8,"t":1},"speaker":"B","text":"お守りと言っても持ち歩くものではなく、家の壁などに貼るものです。"},{"id":{"d":742,"n":9,"t":1},"speaker":"A","text":"そうなのですね。お守りを貼る場所は決まっているのですか？"},{"id":{"d":742,"n":10,"t":1},"speaker":"B","text":"お守りを貼る方角が毎年決まっていたと思います。お守りを買う時にどこに貼るかも聞くとよさそうです。"},{"id":{"d":742,"n":11,"t":1},"speaker":"A","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":743,"n":1,"t":1},"speaker":"A","text":"家の南側のカーテンが日に焼けて色が変わってしまったので、新しいものに替えたいと思います。"},{"id":{"d":743,"n":2,"t":1},"speaker":"B","text":"私もそれは気になっていました。カーテン生地が切れやすくなっていて、シミのようなものも付いています。"},{"id":{"d":743,"n":3,"t":1},"speaker":"A","text":"この際、カーテンの種類を変えてみますか？"},{"id":{"d":743,"n":4,"t":1},"speaker":"B","text":"今使っているロールスクリーンよりも私はブラインドタイプを使ってみたいです。"},{"id":{"d":743,"n":5,"t":1},"speaker":"A","text":"ブラインドタイプも日の光は完全に遮蔽できませんので、夏は暑いかもしれません。"},{"id":{"d":743,"n":6,"t":1},"speaker":"B","text":"そうなのですね。日の光を遮蔽できるカーテンはあるのでしょうか？"},{"id":{"d":743,"n":7,"t":1},"speaker":"A","text":"インターネットで調べると遮光カーテンや遮熱カーテンもあるようです。"},{"id":{"d":743,"n":8,"t":1},"speaker":"B","text":"遮熱カーテンが良いのではないでしょうか？"},{"id":{"d":743,"n":9,"t":1},"speaker":"A","text":"遮熱カーテンもいろいろ種類があって私一人では選べません。"},{"id":{"d":743,"n":10,"t":1},"speaker":"B","text":"内装業者にお願いするかもしれませんから、窓の寸法を測ってから専門店に行ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":744,"n":1,"t":1},"speaker":"A","text":"お風呂の温度は何度に設定していますか？"},{"id":{"d":744,"n":2,"t":1},"speaker":"B","text":"冬は39度で、夏は38度で風呂の温度を設定しています。"},{"id":{"d":744,"n":3,"t":1},"speaker":"A","text":"低めに温度を設定していますが、家族から温度を上げたりするように言われませんか？"},{"id":{"d":744,"n":4,"t":1},"speaker":"B","text":"ずっと前からこの温度なので、皆慣れてしまっているようです。"},{"id":{"d":744,"n":5,"t":1},"speaker":"A","text":"皆さん今の温度が普通の温度だと思っているかもしれませんね。"},{"id":{"d":744,"n":6,"t":1},"speaker":"B","text":"そう言えば、近くの健康センターに家族で行った時、みんな湯船の熱さに驚いていましたね。"},{"id":{"d":744,"n":7,"t":1},"speaker":"A","text":"なぜ、お風呂を低めの温度に設定しているのですか？"},{"id":{"d":744,"n":8,"t":1},"speaker":"B","text":"昔の話なので詳しくは分かりませんが、お風呂場で倒れた人がいたらしいです。"},{"id":{"d":744,"n":9,"t":1},"speaker":"A","text":"温度の高いお風呂は血圧を上げてしまいますから注意が必要です。お風呂に入っている人が倒れたりしないように温度を低めにしているということなのですね。"},{"id":{"d":744,"n":10,"t":1},"speaker":"B","text":"はい、そういう注意からお風呂の温度は低めに設定しているのです。"}];
INSERT INTO sentence [{"id":{"d":745,"n":1,"t":1},"speaker":"A","text":"お中元の時期が地方によって違っていると知っていましたか？"},{"id":{"d":745,"n":2,"t":1},"speaker":"B","text":"この辺りでは7月初旬から7月の中旬まで、他の地域は7月の中旬から8月の中旬あたりだったように思います。"},{"id":{"d":745,"n":3,"t":1},"speaker":"A","text":"私はずっと7月初旬から7月の中旬までだけが、お中元の期間だと思っていました。"},{"id":{"d":745,"n":4,"t":1},"speaker":"B","text":"お中元のCMが7月終わりにも流れていて、不思議に思わなかったのですね。"},{"id":{"d":745,"n":5,"t":1},"speaker":"A","text":"それほど後までお中元のCMは放送されていましたか？私は全く気にしていませんでした。"},{"id":{"d":745,"n":6,"t":1},"speaker":"B","text":"お中元の時期が過ぎたら、表書きを暑中見舞いや残暑見舞いとして贈ることもできます。"},{"id":{"d":745,"n":7,"t":1},"speaker":"A","text":"もうお中元の時期ですが、毎年何を贈っていましたか？"},{"id":{"d":745,"n":8,"t":1},"speaker":"B","text":"お忘れのようですが、もうカタログギフトにしていますからお中元の品物を考える必要はありません。"},{"id":{"d":745,"n":9,"t":1},"speaker":"A","text":"そうでした。毎年考えることをしないので、お中元のことが分からなくなっているのかもしれません。"},{"id":{"d":745,"n":10,"t":1},"speaker":"B","text":"今後もお中元の贈り先に変更があるときだけお知らせくださいね。"}];
INSERT INTO sentence [{"id":{"d":746,"n":1,"t":1},"speaker":"A","text":"お中元はなぜ7月15日からが多いのですか？"},{"id":{"d":746,"n":2,"t":1},"speaker":"B","text":"中元とは道教の習俗で旧暦の7月15日のことだからです。"},{"id":{"d":746,"n":3,"t":1},"speaker":"A","text":"旧暦の7月15日だから8月の中旬にお中元が始まる地方もあるのですね。"},{"id":{"d":746,"n":4,"t":1},"speaker":"B","text":"旧暦のところは仏教の盂蘭盆会と重なり、お中元はお盆のお礼ともなったようです。"},{"id":{"d":746,"n":5,"t":1},"speaker":"A","text":"残暑お見舞いはいつからが正しいのですか？"},{"id":{"d":746,"n":6,"t":1},"speaker":"B","text":"立秋を過ぎたら残暑お見舞いになります。"},{"id":{"d":746,"n":7,"t":1},"speaker":"A","text":"立秋は何月何日になりますか？"},{"id":{"d":746,"n":8,"t":1},"speaker":"B","text":"立秋は8月8日前後ですが、正確な日にちは暦を見なければ分かりません。"},{"id":{"d":746,"n":9,"t":1},"speaker":"A","text":"お盆の時期ということですね。1日前後の違いならば、残暑お見舞いで贈ってもよいように思います。"},{"id":{"d":746,"n":10,"t":1},"speaker":"B","text":"言い忘れていましたが、贈り先が目上の方ならば残暑伺いになりますから注意してください。"}];
INSERT INTO sentence [{"id":{"d":747,"n":1,"t":1},"speaker":"A","text":"お歳暮の始まりを知っていますか？"},{"id":{"d":747,"n":2,"t":1},"speaker":"B","text":"江戸時代に大家さんや取引先に借家人や商人が贈り物を持参したことが始まりと聞いたことがあります。"},{"id":{"d":747,"n":3,"t":1},"speaker":"A","text":"なぜ大家さんや取引先に贈り物をするのですか？"},{"id":{"d":747,"n":4,"t":1},"speaker":"B","text":"「日頃お世話になっています。これからもよろしくお願いします」という気持ちを込めているようです。"},{"id":{"d":747,"n":5,"t":1},"speaker":"A","text":"お歳暮も地方で贈る時期は違うのでしょうか？"},{"id":{"d":747,"n":6,"t":1},"speaker":"B","text":"東日本は11月下旬から12月20日頃まで、西日本は12月13日から12月20日までが多いですね。"},{"id":{"d":747,"n":7,"t":1},"speaker":"A","text":"西日本はお歳暮の期間が短いですね。"},{"id":{"d":747,"n":8,"t":1},"speaker":"B","text":"そうですね。もしお歳暮の期間を過ぎてしまったら、正月にお年賀として贈ります。"},{"id":{"d":747,"n":9,"t":1},"speaker":"A","text":"贈る時期はこちらの時期と先方の時期のどちらに合わせるのですか？"},{"id":{"d":747,"n":10,"t":1},"speaker":"B","text":"お歳暮を贈る時期は、先方の習慣に合わせます。"}];
INSERT INTO sentence [{"id":{"d":748,"n":1,"t":1},"speaker":"A","text":"駅前に新しいお団子屋さんがオープンしたようです。"},{"id":{"d":748,"n":2,"t":1},"speaker":"B","text":"知っています。私も新聞広告で見ました。"},{"id":{"d":748,"n":3,"t":1},"speaker":"A","text":"広告に載っていたクーポン券を持っているのですが、一緒に買いに行きませんか？"},{"id":{"d":748,"n":4,"t":1},"speaker":"B","text":"行きたいです。ところで、私もクーポン券を使えるのですか？"},{"id":{"d":748,"n":5,"t":1},"speaker":"A","text":"2枚ありますから大丈夫です。お団子1本が20円引きになります。"},{"id":{"d":748,"n":6,"t":1},"speaker":"B","text":"それはうれしいです。"},{"id":{"d":748,"n":7,"t":1},"speaker":"A","text":"はい、1枚どうぞ。"},{"id":{"d":748,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":749,"n":1,"t":1},"speaker":"A","text":"お団子にたくさん種類があって迷いますね。何にするか決めましたか？"},{"id":{"d":749,"n":2,"t":1},"speaker":"B","text":"私はみたらし団子が好きなのですが、他にも気になるものがありまだ迷っています。"},{"id":{"d":749,"n":3,"t":1},"speaker":"A","text":"私はヨモギ餅にあんこがのっているものにします。"},{"id":{"d":749,"n":4,"t":1},"speaker":"B","text":"ちょうどそれと迷っていました。"},{"id":{"d":749,"n":5,"t":1},"speaker":"A","text":"どちらも買うという選択肢もありますよ。"},{"id":{"d":749,"n":6,"t":1},"speaker":"B","text":"2本くらいは食べられそうなので、両方買うことにします。"}];
INSERT INTO sentence [{"id":{"d":750,"n":1,"t":1},"speaker":"A","text":"障子が破れてしまったのですが、自分で貼り直せるでしょうか？"},{"id":{"d":750,"n":2,"t":1},"speaker":"B","text":"道具があればそれほど難しくありませんよ。"},{"id":{"d":750,"n":3,"t":1},"speaker":"A","text":"店員さんは器用ですから、そうおっしゃいますが、私は不器用なのです。"},{"id":{"d":750,"n":4,"t":1},"speaker":"B","text":"破れた障子の穴はどれくらいですか？"},{"id":{"d":750,"n":5,"t":1},"speaker":"A","text":"5センチメートルくらいだと思います。"},{"id":{"d":750,"n":6,"t":1},"speaker":"B","text":"穴は1カ所だけですか？"},{"id":{"d":750,"n":7,"t":1},"speaker":"A","text":"そうです。"},{"id":{"d":750,"n":8,"t":1},"speaker":"B","text":"それくらいでしたら、障子紙でできたシールを貼っておくことをお勧めします。"}];
INSERT INTO sentence [{"id":{"d":751,"n":1,"t":1},"speaker":"A","text":"毎日、朝ご飯を食べに行っていたカフェが閉店していました。"},{"id":{"d":751,"n":2,"t":1},"speaker":"B","text":"急に閉店していたのですか？"},{"id":{"d":751,"n":3,"t":1},"speaker":"A","text":"はい、今朝行ってみて閉店していることが分かりました。"},{"id":{"d":751,"n":4,"t":1},"speaker":"B","text":"それで今朝はコンビニエンスストアのサンドイッチを食べていたのですね。"},{"id":{"d":751,"n":5,"t":1},"speaker":"A","text":"あのカフェのモーニングセットはおいしかったのに残念です。"},{"id":{"d":751,"n":6,"t":1},"speaker":"B","text":"他のお店にはないのですか？"},{"id":{"d":751,"n":7,"t":1},"speaker":"A","text":"朝早くにお手ごろな値段でおいしいものを食べさせてくれる所はそうそうありません。"},{"id":{"d":751,"n":8,"t":1},"speaker":"B","text":"私も心当たりがありません。良い所が見つかるといいですね。"}];
INSERT INTO sentence [{"id":{"d":752,"n":1,"t":1},"speaker":"A","text":"住んでいるマンションが昨日から外装工事を始めました。"},{"id":{"d":752,"n":2,"t":1},"speaker":"B","text":"塗装ですか？きれいになるのが楽しみですね。"},{"id":{"d":752,"n":3,"t":1},"speaker":"A","text":"塗装です。それで、しばらくは窓を開けてはいけないのだそうです。"},{"id":{"d":752,"n":4,"t":1},"speaker":"B","text":"きれいになるまでが大変なのですね。いつまで工事が続くのですか？"},{"id":{"d":752,"n":5,"t":1},"speaker":"A","text":"これから1カ月ほどかかるそうです。"},{"id":{"d":752,"n":6,"t":1},"speaker":"B","text":"ではその間はクーラーを使ってはいかがですか？"},{"id":{"d":752,"n":7,"t":1},"speaker":"A","text":"クーラーはあまり得意ではないのです。"},{"id":{"d":752,"n":8,"t":1},"speaker":"B","text":"これから夏本番なのに大変ですね。"}];
INSERT INTO sentence [{"id":{"d":753,"n":1,"t":1},"speaker":"A","text":"すみません、この辺りにコンビニエンスストアはありませんか？"},{"id":{"d":753,"n":2,"t":1},"speaker":"B","text":"ここから一番近いコンビニエンスストアは車で30分行った所です。"},{"id":{"d":753,"n":3,"t":1},"speaker":"A","text":"それほど遠いのですか？困りました。"},{"id":{"d":753,"n":4,"t":1},"speaker":"B","text":"何を買いたいのですか？"},{"id":{"d":753,"n":5,"t":1},"speaker":"A","text":"はいてきたストッキングが破れてしまったので、新しいものを買いたいのです。"},{"id":{"d":753,"n":6,"t":1},"speaker":"B","text":"ストッキングなら、そこの荒物屋に売っていますよ。"},{"id":{"d":753,"n":7,"t":1},"speaker":"A","text":"そうでしたか。それは助かりました。ありがとうございます。"},{"id":{"d":753,"n":8,"t":1},"speaker":"B","text":"どういたしまして。荒物屋には大抵のものは売っていますよ。"}];
INSERT INTO sentence [{"id":{"d":754,"n":1,"t":1},"speaker":"A","text":"回転ずしは初めてですか？"},{"id":{"d":754,"n":2,"t":1},"speaker":"B","text":"はい、ワクワクします。これはもう自由にお皿を取ってよいのですか？"},{"id":{"d":754,"n":3,"t":1},"speaker":"A","text":"はい、食べたいお皿が流れてきたら取ってよいです。流れてこないものはタブレットで注文もできます。"},{"id":{"d":754,"n":4,"t":1},"speaker":"B","text":"分かりました。まずはお茶を飲みたいのですが、持ってきてはくれないのでしょうか？"},{"id":{"d":754,"n":5,"t":1},"speaker":"A","text":"お茶は自分で入れるのです。そのカップを取って、お茶の粉を入れて、ここからお湯を入れます。"},{"id":{"d":754,"n":6,"t":1},"speaker":"B","text":"なるほど。早速やってみます。"},{"id":{"d":754,"n":7,"t":1},"speaker":"A","text":"ちょっと待ってください、そのボタンは指で押してはいけません。お湯がかかってやけどしてしまいます。"},{"id":{"d":754,"n":8,"t":1},"speaker":"B","text":"指で押そうとしていました。なるほど、カップで押すのですね。ボタンの横に書いてありました。"}];
INSERT INTO sentence [{"id":{"d":755,"n":1,"t":1},"speaker":"A","text":"初めての回転ずしでしたがとても楽しかったです。食べ過ぎました。"},{"id":{"d":755,"n":2,"t":1},"speaker":"B","text":"たくさんお皿を重ねましたね。"},{"id":{"d":755,"n":3,"t":1},"speaker":"A","text":"メニューが豊富であれもこれも食べたくなってしまいました。"},{"id":{"d":755,"n":4,"t":1},"speaker":"B","text":"デザートも全種類制覇したのではありませんか？"},{"id":{"d":755,"n":5,"t":1},"speaker":"A","text":"はい、お恥ずかしい限りです。"},{"id":{"d":755,"n":6,"t":1},"speaker":"B","text":"今度は別のお店にも行ってみましょう。"},{"id":{"d":755,"n":7,"t":1},"speaker":"A","text":"できたらお給料日の後にお願いします。"},{"id":{"d":755,"n":8,"t":1},"speaker":"B","text":"安いからと言って食べ過ぎると出費が大変ですね。"}];
INSERT INTO sentence [{"id":{"d":756,"n":1,"t":1},"speaker":"A","text":"商店街に高級食パンのお店がオープンしていました。人が並んでいると買いたくなってしまう性分なので、つい買ってきてしまいました。"},{"id":{"d":756,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。楽しみです。見かけは普通の食パンなのですね。"},{"id":{"d":756,"n":3,"t":1},"speaker":"A","text":"はい。ところが食べると全然違います。柔らかくてほんのり甘くてとてもおいしいですよ。"},{"id":{"d":756,"n":4,"t":1},"speaker":"B","text":"食べてみたのですか？"},{"id":{"d":756,"n":5,"t":1},"speaker":"A","text":"試食を配っていたので、一切れ食べてきました。"},{"id":{"d":756,"n":6,"t":1},"speaker":"B","text":"そうでしたか。それは食べるのが楽しみです。"}];
INSERT INTO sentence [{"id":{"d":757,"n":1,"t":1},"speaker":"A","text":"今日のお昼ご飯は今年初めての冷やし中華ですよ。"},{"id":{"d":757,"n":2,"t":1},"speaker":"B","text":"今日は暑いからうれしいです。"},{"id":{"d":757,"n":3,"t":1},"speaker":"A","text":"よかったです。このキュウリはうちのプランターで作った初物ですよ。"},{"id":{"d":757,"n":4,"t":1},"speaker":"B","text":"素晴らしいです。もう収穫できたのですか？"},{"id":{"d":757,"n":5,"t":1},"speaker":"A","text":"あと2本くらいは今週中に採れると思いますよ。"},{"id":{"d":757,"n":6,"t":1},"speaker":"B","text":"そうしたら、週末はまた冷やし中華ですね。"},{"id":{"d":757,"n":7,"t":1},"speaker":"A","text":"キュウリを全部冷やし中華にするのですか？"},{"id":{"d":757,"n":8,"t":1},"speaker":"B","text":"私は冷やし中華が大好きなので、毎週でもよいくらいです。"}];
INSERT INTO sentence [{"id":{"d":758,"n":1,"t":1},"speaker":"A","text":"今日はお肉の特売日です。"},{"id":{"d":758,"n":2,"t":1},"speaker":"B","text":"では今晩はすき焼きがいいです。"},{"id":{"d":758,"n":3,"t":1},"speaker":"A","text":"特売は豚肉と鶏肉ですよ。"},{"id":{"d":758,"n":4,"t":1},"speaker":"B","text":"牛肉ではないのですね、残念です。"},{"id":{"d":758,"n":5,"t":1},"speaker":"A","text":"豚肉で韓国風焼き肉なんてどうですか？"},{"id":{"d":758,"n":6,"t":1},"speaker":"B","text":"それはいいですね。サムギョプサルは大好きです。"},{"id":{"d":758,"n":7,"t":1},"speaker":"A","text":"それでは私はお肉屋さんに行きますから、野菜を買ってきてください。"},{"id":{"d":758,"n":8,"t":1},"speaker":"B","text":"分かりました。サンチュですね。"}];
INSERT INTO sentence [{"id":{"d":759,"n":1,"t":1},"speaker":"A","text":"昨日のお肉が余ってしまっていました。"},{"id":{"d":759,"n":2,"t":1},"speaker":"B","text":"安いからといって買い過ぎましたね。"},{"id":{"d":759,"n":3,"t":1},"speaker":"A","text":"はい。今日はキャベツと一緒に炒めてホイコーローにしようと思います。"},{"id":{"d":759,"n":4,"t":1},"speaker":"B","text":"ホイコーローにするにはテンメンジャンが足りません。"},{"id":{"d":759,"n":5,"t":1},"speaker":"A","text":"日本のみそにお砂糖を入れたテンメンジャン風でもよいですよ。"},{"id":{"d":759,"n":6,"t":1},"speaker":"B","text":"要するに豚肉とキャベツのみそ炒めですね。"},{"id":{"d":759,"n":7,"t":1},"speaker":"A","text":"そうです。みそ炒めでもおいしいと思います。"},{"id":{"d":759,"n":8,"t":1},"speaker":"B","text":"分かりました。今晩の夕食はそれにしましょう。"}];
INSERT INTO sentence [{"id":{"d":760,"n":1,"t":1},"speaker":"A","text":"朝ご飯に食べた豚肉とキャベツのみそ炒めはおいしかったですね。"},{"id":{"d":760,"n":2,"t":1},"speaker":"B","text":"それはよかったです。まだ余っていますよ。"},{"id":{"d":760,"n":3,"t":1},"speaker":"A","text":"たくさんあるのですか？"},{"id":{"d":760,"n":4,"t":1},"speaker":"B","text":"私たち2人分はあると思います。"},{"id":{"d":760,"n":5,"t":1},"speaker":"A","text":"そうしたら今日のお昼に食べましょう。ご飯にのせて卵を落とせば、立派などんぶりものです。"},{"id":{"d":760,"n":6,"t":1},"speaker":"B","text":"良いアイデアですね。お昼ご飯が楽しみになってきました。"}];
INSERT INTO sentence [{"id":{"d":761,"n":1,"t":1},"speaker":"A","text":"お隣からタケノコを頂きました。大きいでしょう？"},{"id":{"d":761,"n":2,"t":1},"speaker":"B","text":"これがタケノコですか？"},{"id":{"d":761,"n":3,"t":1},"speaker":"A","text":"美玖ちゃんはタケノコを見るのは初めてでしたか？"},{"id":{"d":761,"n":4,"t":1},"speaker":"B","text":"スーパーマーケットで売られている真空パックのものは知っていますが、それとは随分違います。"},{"id":{"d":761,"n":5,"t":1},"speaker":"A","text":"皮が付いたタケノコは初めてだったのですね。これがタケノコですよ。"},{"id":{"d":761,"n":6,"t":1},"speaker":"B","text":"これとスーパーマーケットのものは同じものですか？"},{"id":{"d":761,"n":7,"t":1},"speaker":"A","text":"そうですよ。これから下ごしらえをすると同じ形になりますよ。"},{"id":{"d":761,"n":8,"t":1},"speaker":"B","text":"見ていてよいですか？"},{"id":{"d":761,"n":9,"t":1},"speaker":"A","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":762,"n":1,"t":1},"speaker":"A","text":"皮付きのタケノコを初めてみました。皮をむくとスーパーマーケットで売られている白色の状態になるのですね。"},{"id":{"d":762,"n":2,"t":1},"speaker":"B","text":"その通りです。"},{"id":{"d":762,"n":3,"t":1},"speaker":"A","text":"色は同じですが、でもこれはまだ硬そうです。"},{"id":{"d":762,"n":4,"t":1},"speaker":"B","text":"これからゆでると柔らかくなるのですよ。"},{"id":{"d":762,"n":5,"t":1},"speaker":"A","text":"出来上がりが楽しみです。早くゆでてください。"},{"id":{"d":762,"n":6,"t":1},"speaker":"B","text":"ゆでるのには時間がかかりますから、その間に宿題を済ませてきてくださいね。"},{"id":{"d":762,"n":7,"t":1},"speaker":"A","text":"分かりました。今日のお夕飯には食べられますか？"},{"id":{"d":762,"n":8,"t":1},"speaker":"B","text":"そのつもりです。このタケノコは炊き込みご飯と煮物にしましょうね。"}];
INSERT INTO sentence [{"id":{"d":763,"n":1,"t":1},"speaker":"A","text":"すみません、男性用の日傘は置いてありますか？"},{"id":{"d":763,"n":2,"t":1},"speaker":"B","text":"はい、こちらにございます。"},{"id":{"d":763,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。数は少ないのですね。"},{"id":{"d":763,"n":4,"t":1},"speaker":"B","text":"最近は男性も日傘を差されるようになってきましたが、男性用の日傘は女性用よりはまだ少ないですね。"},{"id":{"d":763,"n":5,"t":1},"speaker":"A","text":"私もこの年になって日傘を求めようとは思っていなかったのですが、近年はあまりにも暑いので。"},{"id":{"d":763,"n":6,"t":1},"speaker":"B","text":"最近はそのような方が増えております。帽子もいいですが、日傘は風が通るのでとても涼しいですよ。"},{"id":{"d":763,"n":7,"t":1},"speaker":"A","text":"私もそう思って買いに来たのです。軽いとうれしいのですが、お薦めがありましたら教えてください。"},{"id":{"d":763,"n":8,"t":1},"speaker":"B","text":"それでしたら、こちらの折り畳み傘はいかがでしょうか？"}];
INSERT INTO sentence [{"id":{"d":764,"n":1,"t":1},"speaker":"A","text":"ベランダのプランターからラディッシュを採ってきてください。"},{"id":{"d":764,"n":2,"t":1},"speaker":"B","text":"お母さん、ラディッシュとは何ですか？"},{"id":{"d":764,"n":3,"t":1},"speaker":"A","text":"一緒にプランターに種をまいたでしょう？赤くて小さなカブですよ。"},{"id":{"d":764,"n":4,"t":1},"speaker":"B","text":"茶色のプランターのですね？"},{"id":{"d":764,"n":5,"t":1},"speaker":"A","text":"そうです。赤い球が大きそうなものを5つ採ってきてください。"},{"id":{"d":764,"n":6,"t":1},"speaker":"B","text":"はい。採れました。何に使うのですか？"},{"id":{"d":764,"n":7,"t":1},"speaker":"A","text":"今晩のサラダに入れるつもりです。"}];
INSERT INTO sentence [{"id":{"d":765,"n":1,"t":1},"speaker":"A","text":"冷たいお茶を入れましたので皆さんでどうぞ。"},{"id":{"d":765,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":765,"n":3,"t":1},"speaker":"A","text":"工事は今日中に終わりますか？"},{"id":{"d":765,"n":4,"t":1},"speaker":"B","text":"夕方には終わる予定です。"},{"id":{"d":765,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。お風呂が使えなくて3日間大変でした。"},{"id":{"d":765,"n":6,"t":1},"speaker":"B","text":"そうですよね。今晩からはお風呂に入れますよ。"},{"id":{"d":765,"n":7,"t":1},"speaker":"A","text":"うれしいです。"},{"id":{"d":765,"n":8,"t":1},"speaker":"B","text":"奥さん、お茶をごちそうさまでした。工事を再開します。"}];
INSERT INTO sentence [{"id":{"d":766,"n":1,"t":1},"speaker":"A","text":"随分と髪が伸びましたね。もうそろそろ切ってはどうですか？"},{"id":{"d":766,"n":2,"t":1},"speaker":"B","text":"そうします。前髪が伸びてうっとうしいとは思っていました。"},{"id":{"d":766,"n":3,"t":1},"speaker":"A","text":"私もそろそろ美容院に行こうと思っていたので、一緒に予約しましょうか？"},{"id":{"d":766,"n":4,"t":1},"speaker":"B","text":"いえ、大丈夫です。最近アプリケーションで予約ができるようになったので自分でできますよ。"},{"id":{"d":766,"n":5,"t":1},"speaker":"A","text":"そのようなアプリケーションがあるのですね。私はいつも電話です。"},{"id":{"d":766,"n":6,"t":1},"speaker":"B","text":"はお。このアプリケーションなら、営業時間外でも予約できるので便利ですよ。"},{"id":{"d":766,"n":7,"t":1},"speaker":"A","text":"便利そうですね。私にもアプリケーションでの予約方法を教えてください。"},{"id":{"d":766,"n":8,"t":1},"speaker":"B","text":"簡単ですよ。ここからダウンロードしてください。"}];
INSERT INTO sentence [{"id":{"d":767,"n":1,"t":1},"speaker":"A","text":"ドーナツを買ってきました。おやつにしましょう。"},{"id":{"d":767,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。私の好きなチョコ味もありました！"},{"id":{"d":767,"n":3,"t":1},"speaker":"A","text":"それはあなた用に選びました。その隣の抹茶味は私のですからね。"},{"id":{"d":767,"n":4,"t":1},"speaker":"B","text":"うれしいです。この紫色のものは新作ですか？"},{"id":{"d":767,"n":5,"t":1},"speaker":"A","text":"それは紫イモ味です。今だけの期間限定らしいですよ。"},{"id":{"d":767,"n":6,"t":1},"speaker":"B","text":"おいしそうですね、チョコとどちらにするか迷います。"},{"id":{"d":767,"n":7,"t":1},"speaker":"A","text":"両方食べてしまえばよいのではありませんか？"},{"id":{"d":767,"n":8,"t":1},"speaker":"B","text":"そうしたいのですがカロリーが気になります。半分ずつにしませんか？"},{"id":{"d":767,"n":9,"t":1},"speaker":"A","text":"よいですよ。"}];
INSERT INTO sentence [{"id":{"d":768,"n":1,"t":1},"speaker":"A","text":"バーゲンでお洋服を買ってきました。"},{"id":{"d":768,"n":2,"t":1},"speaker":"B","text":"たくさん買ったのですね。"},{"id":{"d":768,"n":3,"t":1},"speaker":"A","text":"70パーセント引きもあったのですよ。"},{"id":{"d":768,"n":4,"t":1},"speaker":"B","text":"幸子さんは何でも似合うからいいですね。"},{"id":{"d":768,"n":5,"t":1},"speaker":"A","text":"何でもということはありませんよ。これでも研究しているのです。"},{"id":{"d":768,"n":6,"t":1},"speaker":"B","text":"幸子さんが毎月ファッション誌を買っているのは研究のためですか？"},{"id":{"d":768,"n":7,"t":1},"speaker":"A","text":"そうです。でも資金はそれほどありませんので、お洋服を買うのはいつもバーゲンセールですけれどね。"},{"id":{"d":768,"n":8,"t":1},"speaker":"B","text":"計画的で良いのではありませんか。幸子さんの努力を見習いたいです。"}];
INSERT INTO sentence [{"id":{"d":769,"n":1,"t":1},"speaker":"A","text":"そのコートは袖がないのですか？"},{"id":{"d":769,"n":2,"t":1},"speaker":"B","text":"これはコートではなくてジレというものです。"},{"id":{"d":769,"n":3,"t":1},"speaker":"A","text":"ジレですか？初めて聞きました。"},{"id":{"d":769,"n":4,"t":1},"speaker":"B","text":"ジレはベストの一種ですね。だから袖がないのです。"},{"id":{"d":769,"n":5,"t":1},"speaker":"A","text":"トレンチコートの袖を切ってしまったのかと思いました。"},{"id":{"d":769,"n":6,"t":1},"speaker":"B","text":"確かに身頃の形はそっくりというより全く同じですよね。"},{"id":{"d":769,"n":7,"t":1},"speaker":"A","text":"寒くないのですか？"},{"id":{"d":769,"n":8,"t":1},"speaker":"B","text":"もう春ですし、下に防寒着をたくさん着ているので大丈夫です。"}];
INSERT INTO sentence [{"id":{"d":770,"n":1,"t":1},"speaker":"A","text":"今年はカラフルな色のパンツがはやっていますね。"},{"id":{"d":770,"n":2,"t":1},"speaker":"B","text":"街中でよく見かけますよね。"},{"id":{"d":770,"n":3,"t":1},"speaker":"A","text":"私も思い切って挑戦してみようかしら？"},{"id":{"d":770,"n":4,"t":1},"speaker":"B","text":"お姉さんはいつもブラックのパンツばかりですからね。"},{"id":{"d":770,"n":5,"t":1},"speaker":"A","text":"このピンクのパンツはどう思いますか？"},{"id":{"d":770,"n":6,"t":1},"speaker":"B","text":"いきなりピンクではコーディネートに困ると思います。"},{"id":{"d":770,"n":7,"t":1},"speaker":"A","text":"ではどの色が良いと思いますか？"},{"id":{"d":770,"n":8,"t":1},"speaker":"B","text":"お姉さんの服に合わせるならこのターコイズブルーが良いと思います。"}];
INSERT INTO sentence [{"id":{"d":771,"n":1,"t":1},"speaker":"A","text":"思い切って買ったカラーパンツを会社にはいていったら評判がよかったです。"},{"id":{"d":771,"n":2,"t":1},"speaker":"B","text":"それはよかったですね。"},{"id":{"d":771,"n":3,"t":1},"speaker":"A","text":"先輩や同僚にほめられました。"},{"id":{"d":771,"n":4,"t":1},"speaker":"B","text":"いつも黒ばかりでしたから、新鮮だったのかもしれませんね。"},{"id":{"d":771,"n":5,"t":1},"speaker":"A","text":"うれしかったので、同じパンツのフォレストグリーンも買ってきてしまいました。"},{"id":{"d":771,"n":6,"t":1},"speaker":"B","text":"その色も良いですね。"},{"id":{"d":771,"n":7,"t":1},"speaker":"A","text":"通勤のローテーションに入れます。"},{"id":{"d":771,"n":8,"t":1},"speaker":"B","text":"ローテーションから外れた日は貸してください。"}];
INSERT INTO sentence [{"id":{"d":772,"n":1,"t":1},"speaker":"A","text":"お姉さん、この雑誌に書いてある「抜け感」とはどういう意味ですか？"},{"id":{"d":772,"n":2,"t":1},"speaker":"B","text":"ファッションのことは詩織の方が詳しいでしょう？"},{"id":{"d":772,"n":3,"t":1},"speaker":"A","text":"モデルさんのまねをして歩いていたら、知らないおばあさんに注意されました。"},{"id":{"d":772,"n":4,"t":1},"speaker":"B","text":"その時の服が「抜け感」ファッションだったのですか？"},{"id":{"d":772,"n":5,"t":1},"speaker":"A","text":"そうです。「襟が崩れていますよ」と言われてきっちり直されました。"},{"id":{"d":772,"n":6,"t":1},"speaker":"B","text":"ただだらしなく見えたのでしょうね。"},{"id":{"d":772,"n":7,"t":1},"speaker":"A","text":"「抜け感」と「だらしなさ」の境目はどこにあるのでしょうか？"},{"id":{"d":772,"n":8,"t":1},"speaker":"B","text":"それが分かればファッションに苦労しません。"}];
INSERT INTO sentence [{"id":{"d":773,"n":1,"t":1},"speaker":"A","text":"お夕飯にあと1品作りたいです。"},{"id":{"d":773,"n":2,"t":1},"speaker":"B","text":"メインのおかずは何なのですか？"},{"id":{"d":773,"n":3,"t":1},"speaker":"A","text":"ショウガ焼きです。ポテトサラダとおみそ汁も付けました。"},{"id":{"d":773,"n":4,"t":1},"speaker":"B","text":"さっぱりしたものが欲しいので、キュウリの梅肉和えはどうですか？"},{"id":{"d":773,"n":5,"t":1},"speaker":"A","text":"それは、いいですね。"},{"id":{"d":773,"n":6,"t":1},"speaker":"B","text":"梅干しはおばあちゃんが送ってくれたものがいいです。"},{"id":{"d":773,"n":7,"t":1},"speaker":"A","text":"キュウリを切るのを手伝ってくださいね。"},{"id":{"d":773,"n":8,"t":1},"speaker":"B","text":"はい、お手伝いします。"}];
INSERT INTO sentence [{"id":{"d":774,"n":1,"t":1},"speaker":"A","text":"ゆで卵を作り過ぎてしまいました。"},{"id":{"d":774,"n":2,"t":1},"speaker":"B","text":"おでんに全部入れてしまうのは駄目なのですか？"},{"id":{"d":774,"n":3,"t":1},"speaker":"A","text":"他の具材が多過ぎて鍋からあふれそうです。"},{"id":{"d":774,"n":4,"t":1},"speaker":"B","text":"全ての具材を多くし過ぎですよ。入る分だけでも入れましょう。"},{"id":{"d":774,"n":5,"t":1},"speaker":"A","text":"4つは入りましたね、残りは明日、おでんの残り汁に入れましょうか？"},{"id":{"d":774,"n":6,"t":1},"speaker":"B","text":"ゆで卵だけなら麺つゆに浸しておけば煮卵になりますよ。"},{"id":{"d":774,"n":7,"t":1},"speaker":"A","text":"煮ていないのに煮卵ですか？"},{"id":{"d":774,"n":8,"t":1},"speaker":"B","text":"ただ漬けておくだけで煮玉子になります。明日の朝にはおいしく食べられますよ。"}];
INSERT INTO sentence [{"id":{"d":775,"n":1,"t":1},"speaker":"A","text":"煮卵というのは煮なくてもできるのですね。"},{"id":{"d":775,"n":2,"t":1},"speaker":"B","text":"そうなのです。麺つゆに漬けてあるだけでおいしかったでしょう？"},{"id":{"d":775,"n":3,"t":1},"speaker":"A","text":"はい。黄身が半熟のままなのもよかったです。"},{"id":{"d":775,"n":4,"t":1},"speaker":"B","text":"煮てしまったら半熟にはなりませんからね。"},{"id":{"d":775,"n":5,"t":1},"speaker":"A","text":"どうしてこのようなことを知っていたのですか？"},{"id":{"d":775,"n":6,"t":1},"speaker":"B","text":"テレビでラーメン屋さんが言っていました。"},{"id":{"d":775,"n":7,"t":1},"speaker":"A","text":"プロもこの作り方なのですか？"},{"id":{"d":775,"n":8,"t":1},"speaker":"B","text":"その人はこの作り方だそうです。とても参考になりました。"}];
INSERT INTO sentence [{"id":{"d":776,"n":1,"t":1},"speaker":"A","text":"お母さん、今日はカレーライスがいいです。"},{"id":{"d":776,"n":2,"t":1},"speaker":"B","text":"今日はスーパーマーケットで唐揚げを買ってきてしまったからカレーはまた明日です。"},{"id":{"d":776,"n":3,"t":1},"speaker":"A","text":"唐揚げとカレーを一緒に食べるとおいしいですよ。"},{"id":{"d":776,"n":4,"t":1},"speaker":"B","text":"そうなのですか？"},{"id":{"d":776,"n":5,"t":1},"speaker":"A","text":"はい、カレー屋さんで食べましたがとてもおいしかったです。"},{"id":{"d":776,"n":6,"t":1},"speaker":"B","text":"その場合、カレーの具はどうすればよいのですか？"},{"id":{"d":776,"n":7,"t":1},"speaker":"A","text":"お店で食べた時はカレーの具は玉ネギだけでした。"},{"id":{"d":776,"n":8,"t":1},"speaker":"B","text":"分かりました。では玉ネギだけのカレーを作ります。"}];
INSERT INTO sentence [{"id":{"d":777,"n":1,"t":1},"speaker":"A","text":"今日のお昼はカップうどんですか？"},{"id":{"d":777,"n":2,"t":1},"speaker":"B","text":"はい。このうどんがとても好きで、実家からわざわざ送ってもらいました。"},{"id":{"d":777,"n":3,"t":1},"speaker":"A","text":"ご実家からわざわざカップ麺を送ってもらったのですか？"},{"id":{"d":777,"n":4,"t":1},"speaker":"B","text":"これはこちらでは売っていない味なのです。"},{"id":{"d":777,"n":5,"t":1},"speaker":"A","text":"普通のカップうどんに見えますけれど違うのですか？"},{"id":{"d":777,"n":6,"t":1},"speaker":"B","text":"おつゆのだしが関西風なのです。"},{"id":{"d":777,"n":7,"t":1},"speaker":"A","text":"よく見るとおつゆの色が薄いですね。"},{"id":{"d":777,"n":8,"t":1},"speaker":"B","text":"私にとってはこれが故郷の味なのです。"}];
INSERT INTO sentence [{"id":{"d":778,"n":1,"t":1},"speaker":"A","text":"買い物用に保冷バッグが欲しいです。"},{"id":{"d":778,"n":2,"t":1},"speaker":"B","text":"最近は暑くなってきましたから必要ですね。"},{"id":{"d":778,"n":3,"t":1},"speaker":"A","text":"先日は家にたどり着くまでに冷凍食品が溶けてしまって大変でした。"},{"id":{"d":778,"n":4,"t":1},"speaker":"B","text":"溶けたものをまた冷凍したのですか？"},{"id":{"d":778,"n":5,"t":1},"speaker":"A","text":"そのままレンジにかけたり少し油で揚げたりすればよいものでしたので、その日の夕食にしました。"},{"id":{"d":778,"n":6,"t":1},"speaker":"B","text":"夕食が豪華になったのではありませんか？"},{"id":{"d":778,"n":7,"t":1},"speaker":"A","text":"そうですね。ただ夕飯だけでは食べ切れないほどのたくさんのおかずが一気にできあがってしまったので、腐る前に食べ切れるか心配でした。"},{"id":{"d":778,"n":8,"t":1},"speaker":"B","text":"また同じ目に遭わないために保冷バッグを買うといいですね。"}];
INSERT INTO sentence [{"id":{"d":779,"n":1,"t":1},"speaker":"A","text":"物置にかわいい保冷バッグがありました。今日はこれを持って出かけます。"},{"id":{"d":779,"n":2,"t":1},"speaker":"B","text":"いいですね。折り畳みができるのですね。"},{"id":{"d":779,"n":3,"t":1},"speaker":"A","text":"はい。広げると大きくなるのです。"},{"id":{"d":779,"n":4,"t":1},"speaker":"B","text":"今日は帰りにスーパーマーケットに行くのですか？"},{"id":{"d":779,"n":5,"t":1},"speaker":"A","text":"そのつもりです。帰り道になるスーパーマーケットは今日が冷凍食品の日ですよ。"},{"id":{"d":779,"n":6,"t":1},"speaker":"B","text":"どのくらいお買い得なのですか？"},{"id":{"d":779,"n":7,"t":1},"speaker":"A","text":"全品30パーセントオフです。"},{"id":{"d":779,"n":8,"t":1},"speaker":"B","text":"それは驚きですね。もしよければお使いをお願いしてもよいですか？"},{"id":{"d":779,"n":9,"t":1},"speaker":"A","text":"よいですよ。家を出る前までに買い物リストを渡してください。"}];
INSERT INTO sentence [{"id":{"d":780,"n":1,"t":1},"speaker":"A","text":"おはようございます。"},{"id":{"d":780,"n":2,"t":1},"speaker":"B","text":"おはようございます。朝に購買でお会いするのは珍しいですね。"},{"id":{"d":780,"n":3,"t":1},"speaker":"A","text":"今朝は朝ご飯を食べ損ねてしまったので買いに来ました。"},{"id":{"d":780,"n":4,"t":1},"speaker":"B","text":"朝ご飯ですか。そちらの棚のパンは先ほどお店の方が並べたばかりですよ。"},{"id":{"d":780,"n":5,"t":1},"speaker":"A","text":"焼きたてですね。ではこれにします。あなたも朝ご飯を買いにきたのですか？"},{"id":{"d":780,"n":6,"t":1},"speaker":"B","text":"いえ、私はお昼用に買いに来たのです。"},{"id":{"d":780,"n":7,"t":1},"speaker":"A","text":"そうでしたか。私も朝用だけでなくお昼用も買っていこうと思います。"}];
INSERT INTO sentence [{"id":{"d":781,"n":1,"t":1},"speaker":"A","text":"おいしいパンが買えてよかったです。"},{"id":{"d":781,"n":2,"t":1},"speaker":"B","text":"エピとフォカッチャサンドとクロワッサンサンドを買ったのですね。"},{"id":{"d":781,"n":3,"t":1},"speaker":"A","text":"あんパンとカレーパンも買いました。"},{"id":{"d":781,"n":4,"t":1},"speaker":"B","text":"日本はいろいろな国のパンを売っていますね。"},{"id":{"d":781,"n":5,"t":1},"speaker":"A","text":"確かに。エピとクロワッサンはフランスですか？"},{"id":{"d":781,"n":6,"t":1},"speaker":"B","text":"フォカッチャは確かイタリアです。"},{"id":{"d":781,"n":7,"t":1},"speaker":"A","text":"あんパンとカレーパンは日本ですよね？"},{"id":{"d":781,"n":8,"t":1},"speaker":"B","text":"食事で世界一周できそうですね。"}];
INSERT INTO sentence [{"id":{"d":782,"n":1,"t":1},"speaker":"A","text":"ホットケーキミックスで蒸しパンを作りました。お一ついかがですか？"},{"id":{"d":782,"n":2,"t":1},"speaker":"B","text":"いただきます。お昼のお弁当だけでは足りなかったので、ありがたいです。"},{"id":{"d":782,"n":3,"t":1},"speaker":"A","text":"簡単にたくさん作れましたので、また作ってきますよ。"},{"id":{"d":782,"n":4,"t":1},"speaker":"B","text":"ホットケーキミックスは、ホットケーキを作るための粉ではないのですか？"},{"id":{"d":782,"n":5,"t":1},"speaker":"A","text":"工夫次第で、ホットケーキミックスは蒸しパンにもドーナツにもケーキにも変身するのですよ。"},{"id":{"d":782,"n":6,"t":1},"speaker":"B","text":"知りませんでした。うちの母はホットケーキしか作ってくれませんでした。"}];
INSERT INTO sentence [{"id":{"d":783,"n":1,"t":1},"speaker":"A","text":"ホットケーキミックスで蒸しパンを作ってみました。"},{"id":{"d":783,"n":2,"t":1},"speaker":"B","text":"とてもきれいでおいしそうです。これを真子さんが作ったのですか？"},{"id":{"d":783,"n":3,"t":1},"speaker":"A","text":"はい。インターネットでレシピを調べて作ってみました。レーズンを入れたりチョコレートを入れたりするのもおいしそうでしたが、初めてなのでまずはプレーンにしました。"},{"id":{"d":783,"n":4,"t":1},"speaker":"B","text":"初めてとは思えないくらいとても上手にできています。"},{"id":{"d":783,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。"},{"id":{"d":783,"n":6,"t":1},"speaker":"B","text":"では早速いただきますね。"},{"id":{"d":783,"n":7,"t":1},"speaker":"A","text":"はい、召し上がれ。"}];
INSERT INTO sentence [{"id":{"d":784,"n":1,"t":1},"speaker":"A","text":"卵サンドをください。あと、ブレンドをホットでください。"},{"id":{"d":784,"n":2,"t":1},"speaker":"B","text":"はい。お待ちどおさまです。"},{"id":{"d":784,"n":3,"t":1},"speaker":"A","text":"これが卵サンドですか？"},{"id":{"d":784,"n":4,"t":1},"speaker":"B","text":"はい、そうです。"},{"id":{"d":784,"n":5,"t":1},"speaker":"A","text":"中の卵は厚焼き玉子ですか？"},{"id":{"d":784,"n":6,"t":1},"speaker":"B","text":"そうです。だし巻き卵です。"},{"id":{"d":784,"n":7,"t":1},"speaker":"A","text":"いつもマヨネーズであえた卵サンドなので、少し驚きました。"},{"id":{"d":784,"n":8,"t":1},"speaker":"B","text":"この辺りではだし巻き卵が一般的なのですよ。これもおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":785,"n":1,"t":1},"speaker":"A","text":"ごちそうさまでした。だし巻き卵を挟んだサンドイッチを初めて食べましたが、とてもおいしかったです。"},{"id":{"d":785,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。"},{"id":{"d":785,"n":3,"t":1},"speaker":"A","text":"ここでしか食べられないのが残念です。私の家の近くでも作ってくれたらよいのにと思いました。"},{"id":{"d":785,"n":4,"t":1},"speaker":"B","text":"だし巻きの味を気に入っていただけてよかったです。またぜひお越しください。"},{"id":{"d":785,"n":5,"t":1},"speaker":"A","text":"はい。近くに来たときにはまた食べに来ますね。"},{"id":{"d":785,"n":6,"t":1},"speaker":"B","text":"お待ちしております。"}];
INSERT INTO sentence [{"id":{"d":786,"n":1,"t":1},"speaker":"A","text":"ベランダから洗濯ばさみが落ちてしまいました。"},{"id":{"d":786,"n":2,"t":1},"speaker":"B","text":"庭に落ちてしまったのですか？"},{"id":{"d":786,"n":3,"t":1},"speaker":"A","text":"いえ、多分、雨どいに入ってしまったと思います。"},{"id":{"d":786,"n":4,"t":1},"speaker":"B","text":"それは困りましたね。手で取れそうでしょうか？"},{"id":{"d":786,"n":5,"t":1},"speaker":"A","text":"奥の方まで行ってしまったようで、取ろうと思っても手が届きません。どうしたらよいですか？"},{"id":{"d":786,"n":6,"t":1},"speaker":"B","text":"どうしようもないので、しばらくはこのままにしておくしかなさそうです。"},{"id":{"d":786,"n":7,"t":1},"speaker":"A","text":"それで大丈夫なのですか？"},{"id":{"d":786,"n":8,"t":1},"speaker":"B","text":"しばらくは晴れの日が続きそうなので大丈夫だと思います。近いうちに雨どい掃除をして、その時に取り出しましょう。"}];
INSERT INTO sentence [{"id":{"d":787,"n":1,"t":1},"speaker":"A","text":"お待たせしました。鍵の110番です。"},{"id":{"d":787,"n":2,"t":1},"speaker":"B","text":"夜中にすみません。どこかに鍵を落としてしまって入れないのです。"},{"id":{"d":787,"n":3,"t":1},"speaker":"A","text":"この鍵は難しい鍵ですね。開錠に時間がかかるかもしれません。"},{"id":{"d":787,"n":4,"t":1},"speaker":"B","text":"よろしくお願いします。"},{"id":{"d":787,"n":5,"t":1},"speaker":"A","text":"もしよろしければ近くのファミリーレストランなどでお休みいただいても大丈夫ですよ。"},{"id":{"d":787,"n":6,"t":1},"speaker":"B","text":"いえ、こちらで待たせてください。どこかに鍵が落ちていないか引き続き周りを探してみます。"},{"id":{"d":787,"n":7,"t":1},"speaker":"A","text":"分かりました。できるだけ早く開錠できるよう頑張りますね。"}];
INSERT INTO sentence [{"id":{"d":788,"n":1,"t":1},"speaker":"A","text":"そろそろ夏用のスリッパが欲しいですね。"},{"id":{"d":788,"n":2,"t":1},"speaker":"B","text":"冬はふかふかで良かったのですけれど、最近は暑いですよね。"},{"id":{"d":788,"n":3,"t":1},"speaker":"A","text":"ホームセンターに畳のスリッパがありましたよ。"},{"id":{"d":788,"n":4,"t":1},"speaker":"B","text":"去年も畳のものを買ってきたのでしたよね。"},{"id":{"d":788,"n":5,"t":1},"speaker":"A","text":"あれは夏でも涼しくてよかったです。"},{"id":{"d":788,"n":6,"t":1},"speaker":"B","text":"今年も買いに行きますか？"},{"id":{"d":788,"n":7,"t":1},"speaker":"A","text":"そうしましょう。本格的に暑くなる前には行きたいですね。"},{"id":{"d":788,"n":8,"t":1},"speaker":"B","text":"では早速来週のポイント5倍の日に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":789,"n":1,"t":1},"speaker":"A","text":"お母さん、今日のおかずは何ですか？"},{"id":{"d":789,"n":2,"t":1},"speaker":"B","text":"今日はアジの塩焼きとホウレンソウのゴマあえです。"},{"id":{"d":789,"n":3,"t":1},"speaker":"A","text":"アジの塩焼きですか。焼き魚はあまり好きではありません。"},{"id":{"d":789,"n":4,"t":1},"speaker":"B","text":"あら、お魚は嫌いでしたか？"},{"id":{"d":789,"n":5,"t":1},"speaker":"A","text":"いえ、魚は嫌いではありません。同じ材料ならアジフライとホウレンソウのバター炒めだとうれしいです。"},{"id":{"d":789,"n":6,"t":1},"speaker":"B","text":"調理法の問題でしたか？確かに両方とも同じ材料ですね。"},{"id":{"d":789,"n":7,"t":1},"speaker":"A","text":"アジフライにしてください。"},{"id":{"d":789,"n":8,"t":1},"speaker":"B","text":"衣をつけるのをお手伝いするなら変えてもよいです。"}];
INSERT INTO sentence [{"id":{"d":790,"n":1,"t":1},"speaker":"A","text":"目玉焼きはいくつ焼きますか？"},{"id":{"d":790,"n":2,"t":1},"speaker":"B","text":"私は2つお願いします。"},{"id":{"d":790,"n":3,"t":1},"speaker":"A","text":"焼き加減は半熟でよいですか？"},{"id":{"d":790,"n":4,"t":1},"speaker":"B","text":"できれば硬めでお願いします。"},{"id":{"d":790,"n":5,"t":1},"speaker":"A","text":"了解しました。"},{"id":{"d":790,"n":6,"t":1},"speaker":"B","text":"焼き加減まで聞いてくれるのですね。うれしいです。"},{"id":{"d":790,"n":7,"t":1},"speaker":"A","text":"うちに泊まってよかったでしょう？"},{"id":{"d":790,"n":8,"t":1},"speaker":"B","text":"ええ、ずっと住みたいくらいです。"}];
INSERT INTO sentence [{"id":{"d":791,"n":1,"t":1},"speaker":"A","text":"養鶏場をしている親戚から卵をたくさん頂いたので、よかったらどうぞ。"},{"id":{"d":791,"n":2,"t":1},"speaker":"B","text":"これほどたくさんにおすそ分けしていただいてよいのですか？"},{"id":{"d":791,"n":3,"t":1},"speaker":"A","text":"たくさんあるので、食べるのを手伝っていただきたいのです。"},{"id":{"d":791,"n":4,"t":1},"speaker":"B","text":"それではおいしく調理して食べないといけませんね。"},{"id":{"d":791,"n":5,"t":1},"speaker":"A","text":"卵料理は何が得意ですか？"},{"id":{"d":791,"n":6,"t":1},"speaker":"B","text":"だし巻き卵が一番得意です。"},{"id":{"d":791,"n":7,"t":1},"speaker":"A","text":"私は味付けが得意ではなくて目玉焼きばかりになってしまいます。"},{"id":{"d":791,"n":8,"t":1},"speaker":"B","text":"では、もしよければ、だし巻きを1本作ってお持ちしますか？"},{"id":{"d":791,"n":9,"t":1},"speaker":"A","text":"よいのですか？ありがとうございます、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":792,"n":1,"t":1},"speaker":"A","text":"とりあえず生ビールをください。"},{"id":{"d":792,"n":2,"t":1},"speaker":"B","text":"はい。お料理はどうされますか？"},{"id":{"d":792,"n":3,"t":1},"speaker":"A","text":"冷ややっこと枝豆をお願いします。"},{"id":{"d":792,"n":4,"t":1},"speaker":"B","text":"枝豆は本日のお通しですが、よろしいですか？"},{"id":{"d":792,"n":5,"t":1},"speaker":"A","text":"それでは枝豆をやめて、キュウリのかす漬けをお願いします。"},{"id":{"d":792,"n":6,"t":1},"speaker":"B","text":"かしこまりました。"},{"id":{"d":792,"n":7,"t":1},"speaker":"A","text":"すみません、焼き鳥の盛り合わせもお願いします。"},{"id":{"d":792,"n":8,"t":1},"speaker":"B","text":"承知しました。"}];
INSERT INTO sentence [{"id":{"d":793,"n":1,"t":1},"speaker":"A","text":"お母さん、何を頼んでもよいのですか？"},{"id":{"d":793,"n":2,"t":1},"speaker":"B","text":"よいですが、お子さまセットではないのですか？"},{"id":{"d":793,"n":3,"t":1},"speaker":"A","text":"このグラタンが食べたいです。"},{"id":{"d":793,"n":4,"t":1},"speaker":"B","text":"これは大きいお皿ですよ。食べられますか？"},{"id":{"d":793,"n":5,"t":1},"speaker":"A","text":"食べられます。"},{"id":{"d":793,"n":6,"t":1},"speaker":"B","text":"分かりました。それではグラタンとパスタを1つずつ頼みますね。"},{"id":{"d":793,"n":7,"t":1},"speaker":"A","text":"もし多かったら少しだけ手伝ってください。"},{"id":{"d":793,"n":8,"t":1},"speaker":"B","text":"もちろんです。"}];
INSERT INTO sentence [{"id":{"d":794,"n":1,"t":1},"speaker":"A","text":"すみません、お風呂を洗っただけでお湯を張るのを忘れてしまいました。"},{"id":{"d":794,"n":2,"t":1},"speaker":"B","text":"では今日は夕飯を先に食べてからお風呂にしましょう。"},{"id":{"d":794,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。そうしてもらえると助かります。"},{"id":{"d":794,"n":4,"t":1},"speaker":"B","text":"では早速夕飯の準備を始めますね。"},{"id":{"d":794,"n":5,"t":1},"speaker":"A","text":"今日の夕飯は何ですか？"},{"id":{"d":794,"n":6,"t":1},"speaker":"B","text":"今日は天ぷらです。できたてのあつあつを食べたくて、揚げるタイミングを見計らっていたのです。"},{"id":{"d":794,"n":7,"t":1},"speaker":"A","text":"では今からあつあつの天ぷらを食べられるのですね。うれしいです。"},{"id":{"d":794,"n":8,"t":1},"speaker":"B","text":"はい、10分くらいで出来上がりますのでもう少しだけ待ってくださいね。"},{"id":{"d":794,"n":9,"t":1},"speaker":"A","text":"それでは、その間に抹茶塩を用意しておきます。"}];
INSERT INTO sentence [{"id":{"d":795,"n":1,"t":1},"speaker":"A","text":"お仏壇用のお花をください。"},{"id":{"d":795,"n":2,"t":1},"speaker":"B","text":"こちらに何種類か花束がございます。"},{"id":{"d":795,"n":3,"t":1},"speaker":"A","text":"それでは、この980円の花束をお願いします。"},{"id":{"d":795,"n":4,"t":1},"speaker":"B","text":"かしこまりました。"},{"id":{"d":795,"n":5,"t":1},"speaker":"A","text":"ところで、この棚にある商品はどれも変わったお花ですね？"},{"id":{"d":795,"n":6,"t":1},"speaker":"B","text":"それはネイティブフラワーです。最近、人気があるのですよ。"},{"id":{"d":795,"n":7,"t":1},"speaker":"A","text":"初めて見るものばかりです。"},{"id":{"d":795,"n":8,"t":1},"speaker":"B","text":"主に南半球が原産国のお花で日本では珍しいかもしれません。"}];
INSERT INTO sentence [{"id":{"d":796,"n":1,"t":1},"speaker":"A","text":"アイスを買ってきましたよ。"},{"id":{"d":796,"n":2,"t":1},"speaker":"B","text":"この寒いのにアイスですか？"},{"id":{"d":796,"n":3,"t":1},"speaker":"A","text":"そのようなことを言う人にはあげませんからね。"},{"id":{"d":796,"n":4,"t":1},"speaker":"B","text":"要りませんよ、寒いですから。"},{"id":{"d":796,"n":5,"t":1},"speaker":"A","text":"こたつで食べるアイスは最高です。"},{"id":{"d":796,"n":6,"t":1},"speaker":"B","text":"おいしそうですね。"},{"id":{"d":796,"n":7,"t":1},"speaker":"A","text":"あら、寒いから要らないのでしょう？"},{"id":{"d":796,"n":8,"t":1},"speaker":"B","text":"やはり1つください。"}];
INSERT INTO sentence [{"id":{"d":797,"n":1,"t":1},"speaker":"A","text":"お母さん、今日のお弁当はひどかったです。"},{"id":{"d":797,"n":2,"t":1},"speaker":"B","text":"どうしてですか？頑張って作ったのですよ。"},{"id":{"d":797,"n":3,"t":1},"speaker":"A","text":"オムライスでキャラクター弁当を作ってくれたのはうれしかったのですが、食べるころにはケチャップが崩れていて、まるでホラー映画のようになっていました。"},{"id":{"d":797,"n":4,"t":1},"speaker":"B","text":"まあ！今日はとびきりかわいくできたのにそれは残念です。お弁当箱のふたを閉めたら押しつぶされてしまったのでしょうか？"},{"id":{"d":797,"n":5,"t":1},"speaker":"A","text":"そうかもしれません。お弁当のふたにはケチャップがたくさんついていました。"},{"id":{"d":797,"n":6,"t":1},"speaker":"B","text":"やはりそうでしたか。では今度からはケチャップは別添えにしますね。"}];
INSERT INTO sentence [{"id":{"d":798,"n":1,"t":1},"speaker":"A","text":"ピザを取ろうと思っていますが、リクエストはありますか？"},{"id":{"d":798,"n":2,"t":1},"speaker":"B","text":"食べたいものがたくさんあって悩みます。全部で何枚頼みますか？"},{"id":{"d":798,"n":3,"t":1},"speaker":"A","text":"今日は健太が帰ってきますので2枚にしようと思います。"},{"id":{"d":798,"n":4,"t":1},"speaker":"B","text":"それではハーフ・アンド・ハーフで2枚にするのはどうでしょうか？全部で4つの味が選べます。"},{"id":{"d":798,"n":5,"t":1},"speaker":"A","text":"それがいいと思います。"},{"id":{"d":798,"n":6,"t":1},"speaker":"B","text":"お母さんはどの味がよいですか？"},{"id":{"d":798,"n":7,"t":1},"speaker":"A","text":"私はマルゲリータが食べたいです。"},{"id":{"d":798,"n":8,"t":1},"speaker":"B","text":"分かりました。残りの3種類は私が好きに選んでもよいですか？"},{"id":{"d":798,"n":9,"t":1},"speaker":"A","text":"はい、構いませんよ。"}];
INSERT INTO sentence [{"id":{"d":799,"n":1,"t":1},"speaker":"A","text":"新しい甘味屋さんがオープンしていたので、帰りに寄ってきました。"},{"id":{"d":799,"n":2,"t":1},"speaker":"B","text":"何を買ってきてくれたのですか？"},{"id":{"d":799,"n":3,"t":1},"speaker":"A","text":"たい焼きです。"},{"id":{"d":799,"n":4,"t":1},"speaker":"B","text":"まだホカホカですね。"},{"id":{"d":799,"n":5,"t":1},"speaker":"A","text":"急いで帰ってきましたから。"},{"id":{"d":799,"n":6,"t":1},"speaker":"B","text":"お茶を入れますから、手を洗ってきてください。"},{"id":{"d":799,"n":7,"t":1},"speaker":"A","text":"濃い緑茶でお願いしますね。"},{"id":{"d":799,"n":8,"t":1},"speaker":"B","text":"はい、濃い緑茶を入れますね。"}];
INSERT INTO sentence [{"id":{"d":800,"n":1,"t":1},"speaker":"A","text":"お出かけですか？"},{"id":{"d":800,"n":2,"t":1},"speaker":"B","text":"はい、ポストまで手紙を出しに行くのです。"},{"id":{"d":800,"n":3,"t":1},"speaker":"A","text":"少し遠くて大変ですね。車を出しましょうか？"},{"id":{"d":800,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。でも大丈夫ですよ。コンビニエンスストアで出せますから。"},{"id":{"d":800,"n":5,"t":1},"speaker":"A","text":"コンビニエンスストアで手紙が出せますか？"},{"id":{"d":800,"n":6,"t":1},"speaker":"B","text":"ご存じないのですか？すぐそこのコンビニエンスストアの中に、ポストがありますよ。"},{"id":{"d":800,"n":7,"t":1},"speaker":"A","text":"知りませんでした。"},{"id":{"d":800,"n":8,"t":1},"speaker":"B","text":"今度、機会があったら行ってみてください。"}];
INSERT INTO sentence [{"id":{"d":801,"n":1,"t":1},"speaker":"A","text":"まあ、とても豪華なパフェです。"},{"id":{"d":801,"n":2,"t":1},"speaker":"B","text":"たくさんのフルーツがのっていてキラキラしていますね。おいしそうです。"},{"id":{"d":801,"n":3,"t":1},"speaker":"A","text":"この果物の豊富さはフルーツ専門店ならではですね。早速いただきましょう。"},{"id":{"d":801,"n":4,"t":1},"speaker":"B","text":"とてもおいしいです。毎日食べたいくらいです。"},{"id":{"d":801,"n":5,"t":1},"speaker":"A","text":"お給料日のお楽しみにしておくくらいがいいと思いますよ。"},{"id":{"d":801,"n":6,"t":1},"speaker":"B","text":"毎日は駄目ですか？"},{"id":{"d":801,"n":7,"t":1},"speaker":"A","text":"これを続けていたら、お財布は痩せるし、体は太るしで大変ですよ。"},{"id":{"d":801,"n":8,"t":1},"speaker":"B","text":"それは困ります。"}];
INSERT INTO sentence [{"id":{"d":802,"n":1,"t":1},"speaker":"A","text":"今日は暑いので、冷たいお飲み物をどうぞ。"},{"id":{"d":802,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。これは変わった味のコーラですね。"},{"id":{"d":802,"n":3,"t":1},"speaker":"A","text":"クラフトコーラなのですよ。"},{"id":{"d":802,"n":4,"t":1},"speaker":"B","text":"手作りなのですか？"},{"id":{"d":802,"n":5,"t":1},"speaker":"A","text":"コーラのもとが売られていたのです。私は炭酸を入れただけです。"},{"id":{"d":802,"n":6,"t":1},"speaker":"B","text":"そのようなものが売っているのですね。とてもおいしいです。"}];
INSERT INTO sentence [{"id":{"d":803,"n":1,"t":1},"speaker":"A","text":"おばちゃん、お母さんがいません。"},{"id":{"d":803,"n":2,"t":1},"speaker":"B","text":"あら、未来ちゃん。迷子になってしまったのですか？"},{"id":{"d":803,"n":3,"t":1},"speaker":"A","text":"お買い物をしていたら、お母さんとはぐれてしまいました。"},{"id":{"d":803,"n":4,"t":1},"speaker":"B","text":"おうちに連れて帰ってあげることはできますが、お母さんも捜しておられるかもしれませんね。"},{"id":{"d":803,"n":5,"t":1},"speaker":"A","text":"お母さんと一緒に帰りたいです。"},{"id":{"d":803,"n":6,"t":1},"speaker":"B","text":"そうですよね。おばちゃんはもう帰るところなのですけれど、一緒に捜しましょうか？"},{"id":{"d":803,"n":7,"t":1},"speaker":"A","text":"お願いします。"},{"id":{"d":803,"n":8,"t":1},"speaker":"B","text":"どこではぐれてしまったのか、思い出してください。"}];
INSERT INTO sentence [{"id":{"d":804,"n":1,"t":1},"speaker":"A","text":"娘を連れてきてくださってありがとうございました。"},{"id":{"d":804,"n":2,"t":1},"speaker":"B","text":"たまたま会えたのが良かったです。"},{"id":{"d":804,"n":3,"t":1},"speaker":"A","text":"買い物をしていたら、娘が見当たらなくなってしまって困っていたのです。"},{"id":{"d":804,"n":4,"t":1},"speaker":"B","text":"迷子センターには行かれましたか？"},{"id":{"d":804,"n":5,"t":1},"speaker":"A","text":"もう少し捜しても見つからなかったら行こうとしていたところでした。"},{"id":{"d":804,"n":6,"t":1},"speaker":"B","text":"私ももう少ししたら迷子センターに駆け込むつもりでした。"}];
INSERT INTO sentence [{"id":{"d":805,"n":1,"t":1},"speaker":"A","text":"こんにちは。おしゃれなチーズ屋さんですね。"},{"id":{"d":805,"n":2,"t":1},"speaker":"B","text":"いらっしゃいませ。どうもありがとうございます。"},{"id":{"d":805,"n":3,"t":1},"speaker":"A","text":"私はチーズに詳しくありませんが、お薦めはありますか？"},{"id":{"d":805,"n":4,"t":1},"speaker":"B","text":"普段どのようにしてチーズをお召し上がりですか？お料理に使うか、そのままでいただくかによってお薦めのものが違ってきます。"},{"id":{"d":805,"n":5,"t":1},"speaker":"A","text":"そのまま食べるのにお薦めのものを教えてください。夫の誕生日にワインと一緒にいただこうと思っています。"},{"id":{"d":805,"n":6,"t":1},"speaker":"B","text":"赤ワインですか？白ワインですか？"},{"id":{"d":805,"n":7,"t":1},"speaker":"A","text":"恐らく両方開けてしまうのだと思います。"},{"id":{"d":805,"n":8,"t":1},"speaker":"B","text":"それではこちらのセットはいかがでしょう？それぞれのワインに合うチーズが数種類ずつ入っています。サラミやスモークサーモンもセットに入っていますよ。"},{"id":{"d":805,"n":9,"t":1},"speaker":"A","text":"ではそれを1つください。"},{"id":{"d":805,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。お包みしますので少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":806,"n":1,"t":1},"speaker":"A","text":"昨日は、お昼ご飯のめんたいこスパゲティを作るのに失敗してしまいました。"},{"id":{"d":806,"n":2,"t":1},"speaker":"B","text":"一昨日、張り切ってめんたいこを買って帰ったのに残念でしたね。"},{"id":{"d":806,"n":3,"t":1},"speaker":"A","text":"お店で食べた時はしっとりしておいしかったのに、私が作ったものはぱさぱさになってしまいました。"},{"id":{"d":806,"n":4,"t":1},"speaker":"B","text":"失敗してしまったということですか？"},{"id":{"d":806,"n":5,"t":1},"speaker":"A","text":"はい。ぱさぱさというかぼそぼそという状態で、とにかくまずかったのです。"},{"id":{"d":806,"n":6,"t":1},"speaker":"B","text":"ゆで汁を少し入れるといいと聞いたことがありますよ。ゆで汁は入れましたか？"},{"id":{"d":806,"n":7,"t":1},"speaker":"A","text":"お湯を切るときにざるに入れたので、ゆで汁は全部流してしまいました。"},{"id":{"d":806,"n":8,"t":1},"speaker":"B","text":"次に作るときはゆで汁を少し残しておいてから湯切りをするといいと思います。"}];
INSERT INTO sentence [{"id":{"d":807,"n":1,"t":1},"speaker":"A","text":"洗濯物が乾いていませんね。"},{"id":{"d":807,"n":2,"t":1},"speaker":"B","text":"そうですね。今日はお天気が良くありませんでしたからね。"},{"id":{"d":807,"n":3,"t":1},"speaker":"A","text":"明日まで部屋干しをすれば乾くでしょうか？"},{"id":{"d":807,"n":4,"t":1},"speaker":"B","text":"湿度が高いのでどうでしょうか？"},{"id":{"d":807,"n":5,"t":1},"speaker":"A","text":"明日、子どもたちが持っていく体操服が乾かないと困ります。"},{"id":{"d":807,"n":6,"t":1},"speaker":"B","text":"コインランドリーに行きますか？"},{"id":{"d":807,"n":7,"t":1},"speaker":"A","text":"少し面倒ですが、そうするしかなさそうですね。"},{"id":{"d":807,"n":8,"t":1},"speaker":"B","text":"では車を出しておきます。"},{"id":{"d":807,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。洗濯物をまとめてすぐに車へ行きます。"}];
INSERT INTO sentence [{"id":{"d":808,"n":1,"t":1},"speaker":"A","text":"すごく大きなサンドイッチですね。"},{"id":{"d":808,"n":2,"t":1},"speaker":"B","text":"お母さんがテレビで見たインスタ映えサンドイッチをまねて作ったのです。"},{"id":{"d":808,"n":3,"t":1},"speaker":"A","text":"それほど具が多いサンドイッチはプロにしか作れないのだと思っていました。"},{"id":{"d":808,"n":4,"t":1},"speaker":"B","text":"具を挟んでラップでしっかり包んでしばらく休ませるのがこつだと言っていましたよ。"},{"id":{"d":808,"n":5,"t":1},"speaker":"A","text":"なるほど、ラップで包んでしっかり休ませるのですね。"},{"id":{"d":808,"n":6,"t":1},"speaker":"B","text":"もしかして、あなたもまねしようと思っていますか？"},{"id":{"d":808,"n":7,"t":1},"speaker":"A","text":"はい。家に帰ったら早速作ってみようと思います。"}];
INSERT INTO sentence [{"id":{"d":809,"n":1,"t":1},"speaker":"A","text":"こんにちは。クリーニングをお願いします。"},{"id":{"d":809,"n":2,"t":1},"speaker":"B","text":"はい。ワイシャツ1枚とブラウス1枚ですね。"},{"id":{"d":809,"n":3,"t":1},"speaker":"A","text":"いつ出来上がりますか？"},{"id":{"d":809,"n":4,"t":1},"speaker":"B","text":"3日後の14時から受け取れますよ。"},{"id":{"d":809,"n":5,"t":1},"speaker":"A","text":"料金は、今お支払いすればよいのですか？"},{"id":{"d":809,"n":6,"t":1},"speaker":"B","text":"はい。ワイシャツが300円、ブラウスが400円ですので、合計700円になります。"},{"id":{"d":809,"n":7,"t":1},"speaker":"A","text":"ブラウスの方が高いのですね？"},{"id":{"d":809,"n":8,"t":1},"speaker":"B","text":"フリルなど特別な技術が必要ですので。申し訳ありません。"}];
INSERT INTO sentence [{"id":{"d":810,"n":1,"t":1},"speaker":"A","text":"そのような薄着では寒くありませんか？"},{"id":{"d":810,"n":2,"t":1},"speaker":"B","text":"少し寒いですが、急に寒くなったのでまだ冬服を出していないのです。"},{"id":{"d":810,"n":3,"t":1},"speaker":"A","text":"風邪をひいてしまいますよ。"},{"id":{"d":810,"n":4,"t":1},"speaker":"B","text":"ストールだけは持ってきました。"},{"id":{"d":810,"n":5,"t":1},"speaker":"A","text":"昼間は大丈夫でしょうけど、帰りは夜ですからね。"},{"id":{"d":810,"n":6,"t":1},"speaker":"B","text":"なるべく早く帰れるように仕事を終わらせます。"},{"id":{"d":810,"n":7,"t":1},"speaker":"A","text":"それがいいと思います。"},{"id":{"d":810,"n":8,"t":1},"speaker":"B","text":"帰ったら衣替えをしなくてはなりませんね。"}];
INSERT INTO sentence [{"id":{"d":811,"n":1,"t":1},"speaker":"A","text":"お母さん、これをどうぞ。"},{"id":{"d":811,"n":2,"t":1},"speaker":"B","text":"これは乾パンの缶詰ですか？"},{"id":{"d":811,"n":3,"t":1},"speaker":"A","text":"そうです。会社の備蓄が賞味期限切れ間近なので持っていくように言われたのです。"},{"id":{"d":811,"n":4,"t":1},"speaker":"B","text":"なんだか懐かしいですね。"},{"id":{"d":811,"n":5,"t":1},"speaker":"A","text":"乾パンが懐かしいのですか？"},{"id":{"d":811,"n":6,"t":1},"speaker":"B","text":"ええ、昔は大喜びで食べたおやつでしたよ。"},{"id":{"d":811,"n":7,"t":1},"speaker":"A","text":"では、お母さんに全部あげます。"},{"id":{"d":811,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。でも、あなたも一緒に食べましょうよ。"},{"id":{"d":811,"n":9,"t":1},"speaker":"A","text":"私はあまり好きではありませんので、少しだけいただきます。"}];
INSERT INTO sentence [{"id":{"d":812,"n":1,"t":1},"speaker":"A","text":"リビングの棚の整理整頓をしたいです。"},{"id":{"d":812,"n":2,"t":1},"speaker":"B","text":"いつのまにか紙袋がたくさんたまってぎゅうぎゅうですね。"},{"id":{"d":812,"n":3,"t":1},"speaker":"A","text":"そうなのです。私が整理すると捨てられなくて困るので、選別して捨ててもらえないでしょうか？"},{"id":{"d":812,"n":4,"t":1},"speaker":"B","text":"選別するのですか？いっそのこと全部捨てましょう。"},{"id":{"d":812,"n":5,"t":1},"speaker":"A","text":"それはもったいないです。"},{"id":{"d":812,"n":6,"t":1},"speaker":"B","text":"ではどういうのを残せばよいのですか？"},{"id":{"d":812,"n":7,"t":1},"speaker":"A","text":"ブランド物の紙袋や丈夫そうなものは残してください。"},{"id":{"d":812,"n":8,"t":1},"speaker":"B","text":"ブランドなんて分かりませんよ。適当に捨ててしまいますね。"},{"id":{"d":812,"n":9,"t":1},"speaker":"A","text":"分かりました。それでよいです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":813,"n":1,"t":1},"speaker":"A","text":"今晩は外食ですよ。"},{"id":{"d":813,"n":2,"t":1},"speaker":"B","text":"急にどうしたのですか？"},{"id":{"d":813,"n":3,"t":1},"speaker":"A","text":"引き出しを片付けていたら、今日までの株主優待券が出てきたのです。"},{"id":{"d":813,"n":4,"t":1},"speaker":"B","text":"去年も行ったしゃぶしゃぶ屋さんの優待券ですか？"},{"id":{"d":813,"n":5,"t":1},"speaker":"A","text":"そうです。有効期限がなんと今日まででした。今日中に使いませんと無効になってしまいます。"},{"id":{"d":813,"n":6,"t":1},"speaker":"B","text":"気づいてよかったですね。お父さんには連絡したのですか？"},{"id":{"d":813,"n":7,"t":1},"speaker":"A","text":"これからメールします。"}];
INSERT INTO sentence [{"id":{"d":814,"n":1,"t":1},"speaker":"A","text":"お母さん、喉が渇きました。"},{"id":{"d":814,"n":2,"t":1},"speaker":"B","text":"では、あちらの自動販売機で飲み物を買いましょう。"},{"id":{"d":814,"n":3,"t":1},"speaker":"A","text":"私はこのジュースが飲みたいです。"},{"id":{"d":814,"n":4,"t":1},"speaker":"B","text":"好きなものを選んでよいですよ。"},{"id":{"d":814,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。小銭はありますか？"},{"id":{"d":814,"n":6,"t":1},"speaker":"B","text":"小銭はありませんが、この自動販売機はキャッシュレス機能がついているから大丈夫です。"},{"id":{"d":814,"n":7,"t":1},"speaker":"A","text":"便利ですね。"}];
INSERT INTO sentence [{"id":{"d":815,"n":1,"t":1},"speaker":"A","text":"シャツが長過ぎてズボンに入りません。"},{"id":{"d":815,"n":2,"t":1},"speaker":"B","text":"このシャツは外に出して着るようにできているのですよ。"},{"id":{"d":815,"n":3,"t":1},"speaker":"A","text":"自分はシャツはズボンの中に入れないと気持ちが悪いです。"},{"id":{"d":815,"n":4,"t":1},"speaker":"B","text":"では、裾を切ってもらうようにお直し屋さんに頼むといいですよ。"},{"id":{"d":815,"n":5,"t":1},"speaker":"A","text":"それはどこにあるお店ですか？"},{"id":{"d":815,"n":6,"t":1},"speaker":"B","text":"服を買ったお店に頼むのが一番ですが、スーパーマーケットの洋服売り場などにもよくあります。"},{"id":{"d":815,"n":7,"t":1},"speaker":"A","text":"今まで気にしたことがなかったので、そのような店があることを知りませんでした。"},{"id":{"d":815,"n":8,"t":1},"speaker":"B","text":"それほど高額ではありませんので行ってみるといいです。"}];
INSERT INTO sentence [{"id":{"d":816,"n":1,"t":1},"speaker":"A","text":"中華まんを買ってきました。"},{"id":{"d":816,"n":2,"t":1},"speaker":"B","text":"肉まんですか？"},{"id":{"d":816,"n":3,"t":1},"speaker":"A","text":"肉まんとあんまんとピザまんとカレーまん、それぞれ2つずつです。"},{"id":{"d":816,"n":4,"t":1},"speaker":"B","text":"たくさん買ってきましたね。"},{"id":{"d":816,"n":5,"t":1},"speaker":"A","text":"私たちが一つずつ取って、あとはお隣の新聞屋さんに差し入れしようと思っています。"},{"id":{"d":816,"n":6,"t":1},"speaker":"B","text":"もうすぐ夕刊の配達ですから、ちょうど皆さんおそろいでしょうね。"},{"id":{"d":816,"n":7,"t":1},"speaker":"A","text":"そう思ってたくさん買ってきました。あなたはどれがいいですか？"},{"id":{"d":816,"n":8,"t":1},"speaker":"B","text":"私はあんまんを頂きます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":817,"n":1,"t":1},"speaker":"A","text":"緊急地震速報です。"},{"id":{"d":817,"n":2,"t":1},"speaker":"B","text":"コンロに火はついていませんね？"},{"id":{"d":817,"n":3,"t":1},"speaker":"A","text":"はい、大丈夫です。"},{"id":{"d":817,"n":4,"t":1},"speaker":"B","text":"物が落ちてこない場所にいてください。"},{"id":{"d":817,"n":5,"t":1},"speaker":"A","text":"はい、分かりました。"},{"id":{"d":817,"n":6,"t":1},"speaker":"B","text":"あまり揺れませんでしたね。"},{"id":{"d":817,"n":7,"t":1},"speaker":"A","text":"震源地が遠かったようです。"},{"id":{"d":817,"n":8,"t":1},"speaker":"B","text":"このあたりは大丈夫そうですね。震源の近くが心配です。"}];
INSERT INTO sentence [{"id":{"d":818,"n":1,"t":1},"speaker":"A","text":"あなた、今日のお昼は1人で食べていてもらえますか？"},{"id":{"d":818,"n":2,"t":1},"speaker":"B","text":"分かりました。出かけるのですか？"},{"id":{"d":818,"n":3,"t":1},"speaker":"A","text":"今からご近所の奥さまたちとお茶会があるのです。"},{"id":{"d":818,"n":4,"t":1},"speaker":"B","text":"お茶会なのにお昼にもかかるのですか？"},{"id":{"d":818,"n":5,"t":1},"speaker":"A","text":"午前のお茶の時間から、お昼を挟んで午後のお茶の時間までの予定なのです。"},{"id":{"d":818,"n":6,"t":1},"speaker":"B","text":"そうですか。楽しんできてくださいね。皆さまにもよろしくお伝えください。"},{"id":{"d":818,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。では行ってきます。"},{"id":{"d":818,"n":8,"t":1},"speaker":"B","text":"いってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":819,"n":1,"t":1},"speaker":"A","text":"手羽先の食べ方が上手ですね。"},{"id":{"d":819,"n":2,"t":1},"speaker":"B","text":"そうですか？普通に食べているだけですけれど。"},{"id":{"d":819,"n":3,"t":1},"speaker":"A","text":"食べた後の皿の汚れ方が、私や他の皆とは違います。"},{"id":{"d":819,"n":4,"t":1},"speaker":"B","text":"そう言われればそうですね。"},{"id":{"d":819,"n":5,"t":1},"speaker":"A","text":"君のだけ、きれいでしょう？"},{"id":{"d":819,"n":6,"t":1},"speaker":"B","text":"そうですが、名古屋の友人たちは皆このような感じですよ。"},{"id":{"d":819,"n":7,"t":1},"speaker":"A","text":"名古屋出身なのですか？納得です。"},{"id":{"d":819,"n":8,"t":1},"speaker":"B","text":"幼い頃から手羽先を食べる機会が多かったので慣れているのかもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":820,"n":1,"t":1},"speaker":"A","text":"すみません、冷たいお茶を頂けますか？"},{"id":{"d":820,"n":2,"t":1},"speaker":"B","text":"お疲れさまです。汗だくですね。今、持ってきます。"},{"id":{"d":820,"n":3,"t":1},"speaker":"A","text":"今日は草刈りをするには暑かったです。"},{"id":{"d":820,"n":4,"t":1},"speaker":"B","text":"そうでしたね。暑い中、ありがとうございます。"},{"id":{"d":820,"n":5,"t":1},"speaker":"A","text":"梅雨時は雨が続きますから、今日のような晴れた日にしておかないと。もう1杯頂けますか？"},{"id":{"d":820,"n":6,"t":1},"speaker":"B","text":"もちろんです。そろそろ終わりそうですか？"},{"id":{"d":820,"n":7,"t":1},"speaker":"A","text":"はい、あと少しで終わります。"},{"id":{"d":820,"n":8,"t":1},"speaker":"B","text":"終わりましたら、スイカをいただきましょう。用意しておきますね。"}];
INSERT INTO sentence [{"id":{"d":821,"n":1,"t":1},"speaker":"A","text":"あめ玉はいかがですか？"},{"id":{"d":821,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。頂きます。"},{"id":{"d":821,"n":3,"t":1},"speaker":"A","text":"声がかれていらっしゃるようですので、喉あめをどうぞ。"},{"id":{"d":821,"n":4,"t":1},"speaker":"B","text":"助かります。昨日、大声を出し過ぎたので、気が付いたらがらがら声になっていました。"},{"id":{"d":821,"n":5,"t":1},"speaker":"A","text":"大声で何をされたのですか？"},{"id":{"d":821,"n":6,"t":1},"speaker":"B","text":"サッカーの応援です。夜、テレビで放送していたでしょう？"},{"id":{"d":821,"n":7,"t":1},"speaker":"A","text":"そうでしたか。昨夜、隣の家から叫び声が聞こえていましたが、それだったのかもしれませんね。"},{"id":{"d":821,"n":8,"t":1},"speaker":"B","text":"私もご近所に迷惑をかけていたかもしれません。"}];
INSERT INTO sentence [{"id":{"d":822,"n":1,"t":1},"speaker":"A","text":"いらっしゃい。最近すっかり寒くなってきましたね。"},{"id":{"d":822,"n":2,"t":1},"speaker":"B","text":"こんにちは。今日も寒いですね。夕飯は温まるように鍋にするつもりです。"},{"id":{"d":822,"n":3,"t":1},"speaker":"A","text":"ちょうどよかったです。今日は白菜と大根がお安くなっていますよ。"},{"id":{"d":822,"n":4,"t":1},"speaker":"B","text":"うれしいです。ネギもありますか？"},{"id":{"d":822,"n":5,"t":1},"speaker":"A","text":"ネギは安くはなっていませんが、太くて良いのが入っています。"},{"id":{"d":822,"n":6,"t":1},"speaker":"B","text":"では、白菜と大根とネギをください。"},{"id":{"d":822,"n":7,"t":1},"speaker":"A","text":"毎度ありがとうございます。680円です。"},{"id":{"d":822,"n":8,"t":1},"speaker":"B","text":"はい。1,000円でお願いします。"},{"id":{"d":822,"n":9,"t":1},"speaker":"A","text":"380円のお釣りです。またお待ちしています。"},{"id":{"d":822,"n":10,"t":1},"speaker":"B","text":"ありがとうございました。"}];
INSERT INTO sentence [{"id":{"d":823,"n":1,"t":1},"speaker":"A","text":"もしもし？インターホンを鳴らしてみたのですけれど、恵子さんは自宅にはいらっしゃらないのですか？"},{"id":{"d":823,"n":2,"t":1},"speaker":"B","text":"ごめんなさい、お酒が足りないような気がして、買いに出てしまったのです。"},{"id":{"d":823,"n":3,"t":1},"speaker":"A","text":"もうすぐ帰ってこられるのですよね？"},{"id":{"d":823,"n":4,"t":1},"speaker":"B","text":"はい、もう着きます。マンションの近くまで来ています。"},{"id":{"d":823,"n":5,"t":1},"speaker":"A","text":"エントランスの扉の前にいるのですが、ここでお待ちすればよいですか？"},{"id":{"d":823,"n":6,"t":1},"speaker":"B","text":"すみませんがそうしてください。"},{"id":{"d":823,"n":7,"t":1},"speaker":"A","text":"お土産にアイスケーキを買ってきましたので、急いでくださいね。"},{"id":{"d":823,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。あと5分くらいで到着します。"}];
INSERT INTO sentence [{"id":{"d":824,"n":1,"t":1},"speaker":"A","text":"この通りにはコンビニエンスストアがたくさんありますね。"},{"id":{"d":824,"n":2,"t":1},"speaker":"B","text":"そうなのです。好きなコンビニエンスストアを利用できるので便利ですよ。"},{"id":{"d":824,"n":3,"t":1},"speaker":"A","text":"確かに、ここにはほとんど全ての会社がありますね。"},{"id":{"d":824,"n":4,"t":1},"speaker":"B","text":"さて、今から私たちはどのコンビニエンスストアへ買い出しに行きますか？"},{"id":{"d":824,"n":5,"t":1},"speaker":"A","text":"どこでもよければ、私はローソンに行きたいです。ポイントをためているのです。"},{"id":{"d":824,"n":6,"t":1},"speaker":"B","text":"それはポイント活動というものですね？"},{"id":{"d":824,"n":7,"t":1},"speaker":"A","text":"そうです。ちりも積もれば山となるのですよ。"},{"id":{"d":824,"n":8,"t":1},"speaker":"B","text":"賢い使い方だと思います。"}];
INSERT INTO sentence [{"id":{"d":825,"n":1,"t":1},"speaker":"A","text":"ここのランチはとても安いですね。"},{"id":{"d":825,"n":2,"t":1},"speaker":"B","text":"ファミリーレストランの中でも安くて早くておいしいので私のお気に入りです。"},{"id":{"d":825,"n":3,"t":1},"speaker":"A","text":"それは楽しみです。メニューは決めましたか？"},{"id":{"d":825,"n":4,"t":1},"speaker":"B","text":"私はミックスランチにします。ライス大盛りにします。あなたはどうしますか？"},{"id":{"d":825,"n":5,"t":1},"speaker":"A","text":"私はサラダうどんランチでお願いします。"},{"id":{"d":825,"n":6,"t":1},"speaker":"B","text":"ランチのスープは飲み放題なのですよ。"},{"id":{"d":825,"n":7,"t":1},"speaker":"A","text":"うれしいですね。ドリンクバーに取りに行けばよいのですか？"},{"id":{"d":825,"n":8,"t":1},"speaker":"B","text":"そうです。2人分持ってきましょうか？"}];
INSERT INTO sentence [{"id":{"d":826,"n":1,"t":1},"speaker":"A","text":"ただいま帰りました。おせち用のかまぼこを買ってきました。"},{"id":{"d":826,"n":2,"t":1},"speaker":"B","text":"紅白2本を買ってきてくれたのですね。"},{"id":{"d":826,"n":3,"t":1},"speaker":"A","text":"普段、かまぼこなど買わないのでびっくりしたのですが、高いものですね？"},{"id":{"d":826,"n":4,"t":1},"speaker":"B","text":"普段使いのかまぼこはそれほど高くありませんよ。"},{"id":{"d":826,"n":5,"t":1},"speaker":"A","text":"おせち用だけが高いのですか？"},{"id":{"d":826,"n":6,"t":1},"speaker":"B","text":"普段から高いものもありますが、ぜいたく品なので買いません。"},{"id":{"d":826,"n":7,"t":1},"speaker":"A","text":"お正月は特別なのですね。"},{"id":{"d":826,"n":8,"t":1},"speaker":"B","text":"いつも食べないでいるようですけれど、本来のかまぼこはごちそうなのですよ。"}];
INSERT INTO sentence [{"id":{"d":827,"n":1,"t":1},"speaker":"A","text":"この豆乳鍋、ミズナがとてもおいしいです。"},{"id":{"d":827,"n":2,"t":1},"speaker":"B","text":"よかったです。プランターで育てたミズナを入れました。"},{"id":{"d":827,"n":3,"t":1},"speaker":"A","text":"もっと入れてもよいくらいですよ。"},{"id":{"d":827,"n":4,"t":1},"speaker":"B","text":"プランター1つで育てたのですから、お代わりするほどは量がありません。"},{"id":{"d":827,"n":5,"t":1},"speaker":"A","text":"来年はプランターを増やしましょう。"},{"id":{"d":827,"n":6,"t":1},"speaker":"B","text":"それも良いかもしれませんね。そろそろ締めのうどんを入れますよ。"}];
INSERT INTO sentence [{"id":{"d":828,"n":1,"t":1},"speaker":"A","text":"こんにちは。寒の戻りでおでんがよく売れていますね。"},{"id":{"d":828,"n":2,"t":1},"speaker":"B","text":"いらっしゃいませ。はい、ありがたいことです。今日は何になさいますか？"},{"id":{"d":828,"n":3,"t":1},"speaker":"A","text":"ダイコンと卵とがんもどきとちくわぶと野菜揚げをください。"},{"id":{"d":828,"n":4,"t":1},"speaker":"B","text":"はい。お待ちください。"},{"id":{"d":828,"n":5,"t":1},"speaker":"A","text":"つゆを多めに入れてください。"},{"id":{"d":828,"n":6,"t":1},"speaker":"B","text":"分かりました。"},{"id":{"d":828,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。ここのつゆは絶品です。"},{"id":{"d":828,"n":8,"t":1},"speaker":"B","text":"いつもどうもありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":829,"n":1,"t":1},"speaker":"A","text":"あれ、新しいビニール傘ですね。"},{"id":{"d":829,"n":2,"t":1},"speaker":"B","text":"コンビニエンスストアで傘立てに傘を入れておいたのですが、出るときになかったので買いました。"},{"id":{"d":829,"n":3,"t":1},"speaker":"A","text":"誰かが持っていってしまったのですか？"},{"id":{"d":829,"n":4,"t":1},"speaker":"B","text":"盗まれたというよりは間違えられたという感じです。"},{"id":{"d":829,"n":5,"t":1},"speaker":"A","text":"ビニール傘は多くの人が使っていますからね。"},{"id":{"d":829,"n":6,"t":1},"speaker":"B","text":"適当に他の人のものを持ってくるわけにもいかず、新しいのを買って差してきました。"},{"id":{"d":829,"n":7,"t":1},"speaker":"A","text":"最近、間違えられないように、柄に付けるチャームがあるのですよ。"},{"id":{"d":829,"n":8,"t":1},"speaker":"B","text":"そうなのですか？それは便利そうです。今度買って付けておきます。"}];
INSERT INTO sentence [{"id":{"d":830,"n":1,"t":1},"speaker":"A","text":"そろそろお昼ですね。"},{"id":{"d":830,"n":2,"t":1},"speaker":"B","text":"今、千葉君がみんなの分のお弁当を買いに行ってくれていますよ。"},{"id":{"d":830,"n":3,"t":1},"speaker":"A","text":"それはありがたいです。ここの駐車場が分かるでしょうか？"},{"id":{"d":830,"n":4,"t":1},"speaker":"B","text":"大丈夫みたいですよ。今、来ました。"},{"id":{"d":830,"n":5,"t":1},"speaker":"A","text":"荷物が重たそうですね。"},{"id":{"d":830,"n":6,"t":1},"speaker":"B","text":"10人分のお弁当ですから重いですよ。手伝ってあげてくれますか？"},{"id":{"d":830,"n":7,"t":1},"speaker":"A","text":"分かりました。半分、手伝ってきます。"},{"id":{"d":830,"n":8,"t":1},"speaker":"B","text":"よろしくお願いします。私はここでお茶を準備しておきますね。"}];
INSERT INTO sentence [{"id":{"d":831,"n":1,"t":1},"speaker":"A","text":"千葉君、遠い所からお弁当を買ってきてくれて、ありがとうございます。"},{"id":{"d":831,"n":2,"t":1},"speaker":"B","text":"皆さんに地元の名物を食べていただきたかったので、苦ではありませんでしたよ。"},{"id":{"d":831,"n":3,"t":1},"speaker":"A","text":"ご飯が見えないほどお肉がのっていますね。何というお弁当ですか？"},{"id":{"d":831,"n":4,"t":1},"speaker":"B","text":"これは千葉県民の愛する「チャー弁」というものです。"},{"id":{"d":831,"n":5,"t":1},"speaker":"A","text":"「チャー弁」とはどういう意味ですか？"},{"id":{"d":831,"n":6,"t":1},"speaker":"B","text":"「チャーシュー弁当」の略です。"},{"id":{"d":831,"n":7,"t":1},"speaker":"A","text":"見たままのネーミングですね。"},{"id":{"d":831,"n":8,"t":1},"speaker":"B","text":"そうですね。とてもおいしいのでぜひお召し上がりください。"}];
INSERT INTO sentence [{"id":{"d":832,"n":1,"t":1},"speaker":"A","text":"今日のお弁当はサンドイッチなのですね。"},{"id":{"d":832,"n":2,"t":1},"speaker":"B","text":"はい。いつもはおにぎりばかりの私が珍しいと思いましたか？"},{"id":{"d":832,"n":3,"t":1},"speaker":"A","text":"思いました。いつもはお米でないと夜まで持たないと言っているではありませんか。"},{"id":{"d":832,"n":4,"t":1},"speaker":"B","text":"そうなのです。それで、今日は夜まで持つか、少し不安です。"},{"id":{"d":832,"n":5,"t":1},"speaker":"A","text":"なぜおにぎりにしなかったのですか？"},{"id":{"d":832,"n":6,"t":1},"speaker":"B","text":"高級食パン専門店のパンを、つい行列に並んで買ってしまったのです。"},{"id":{"d":832,"n":7,"t":1},"speaker":"A","text":"1人暮らしなのに1本買ったら多いですよね？"},{"id":{"d":832,"n":8,"t":1},"speaker":"B","text":"そこでこのお弁当になったわけです。明日もサンドイッチになりそうです。"}];
INSERT INTO sentence [{"id":{"d":833,"n":1,"t":1},"speaker":"A","text":"今日のメニューはロールキャベツですよ。"},{"id":{"d":833,"n":2,"t":1},"speaker":"B","text":"すごく大きいですね。かみ切れそうもありません。"},{"id":{"d":833,"n":3,"t":1},"speaker":"A","text":"これはナイフとフォークの練習をするためにわざと大きくしたのです。"},{"id":{"d":833,"n":4,"t":1},"speaker":"B","text":"フォークは使ったことがありますが、ナイフは使ったことがありません。"},{"id":{"d":833,"n":5,"t":1},"speaker":"A","text":"洋食をお外で食べるときには、ナイフとフォークできれいに切り分けて食べるのですよ。"},{"id":{"d":833,"n":6,"t":1},"speaker":"B","text":"今度、鈴子叔母さまの結婚式に呼ばれたから練習するのですね。"},{"id":{"d":833,"n":7,"t":1},"speaker":"A","text":"その通りです。今日の食事で練習しましょう。"},{"id":{"d":833,"n":8,"t":1},"speaker":"B","text":"はい。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":834,"n":1,"t":1},"speaker":"A","text":"焼き芋屋さん、待ってください。"},{"id":{"d":834,"n":2,"t":1},"speaker":"B","text":"はい、いらっしゃいませ。"},{"id":{"d":834,"n":3,"t":1},"speaker":"A","text":"大きめのところを2本お願いします。"},{"id":{"d":834,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。お待ちください。"},{"id":{"d":834,"n":5,"t":1},"speaker":"A","text":"寒くなると焼き芋が食べたくなりますね。"},{"id":{"d":834,"n":6,"t":1},"speaker":"B","text":"この辺りは土曜日に回っていますので、またよろしくお願いします。"},{"id":{"d":834,"n":7,"t":1},"speaker":"A","text":"曜日を決めて回っていらっしゃるのですね。"},{"id":{"d":834,"n":8,"t":1},"speaker":"B","text":"はい。3月いっぱいまで毎週来ますよ。"}];
INSERT INTO sentence [{"id":{"d":835,"n":1,"t":1},"speaker":"A","text":"図書カードを頂いたので、早速本を買いに行ってきます。"},{"id":{"d":835,"n":2,"t":1},"speaker":"B","text":"何を買うつもりですか？"},{"id":{"d":835,"n":3,"t":1},"speaker":"A","text":"パッチワークの本を買おうと思っています。"},{"id":{"d":835,"n":4,"t":1},"speaker":"B","text":"そういえば先週からパッチワークの図案を考えていましたね。"},{"id":{"d":835,"n":5,"t":1},"speaker":"A","text":"はい。図案の参考になる本をカードのおかげで安く入手できそうです。"},{"id":{"d":835,"n":6,"t":1},"speaker":"B","text":"手芸用品も買える商品券があればいいですね。"},{"id":{"d":835,"n":7,"t":1},"speaker":"A","text":"そちらはスーパーマーケットの商品券があるので、帰りに買ってこようと思っています。"},{"id":{"d":835,"n":8,"t":1},"speaker":"B","text":"買い物の準備万端ですね。"}];
INSERT INTO sentence [{"id":{"d":836,"n":1,"t":1},"speaker":"A","text":"今日は温かいですね。"},{"id":{"d":836,"n":2,"t":1},"speaker":"B","text":"そうですね。そのダウンジャケットは少し暑そうにみえます。"},{"id":{"d":836,"n":3,"t":1},"speaker":"A","text":"朝は寒かったのでダウンを着てきたのですが、確かに今は暑いですね。"},{"id":{"d":836,"n":4,"t":1},"speaker":"B","text":"もうすっかり気温が上がったのですから、ダウンは脱いだ方がいいのではありませんか？"},{"id":{"d":836,"n":5,"t":1},"speaker":"A","text":"脱いで荷物になるのが嫌なのです。"},{"id":{"d":836,"n":6,"t":1},"speaker":"B","text":"汗をかいてまで着ているものではありませんよ。"},{"id":{"d":836,"n":7,"t":1},"speaker":"A","text":"脱いだ方がいいですか？"},{"id":{"d":836,"n":8,"t":1},"speaker":"B","text":"はい。脱がないと汗冷えで風邪をひいてしまうのが心配です。"}];
INSERT INTO sentence [{"id":{"d":837,"n":1,"t":1},"speaker":"A","text":"お昼はチャーハンでよいですか？"},{"id":{"d":837,"n":2,"t":1},"speaker":"B","text":"はい。パラパラのチャーハンだとうれしいです。"},{"id":{"d":837,"n":3,"t":1},"speaker":"A","text":"難しいことを言いますね。"},{"id":{"d":837,"n":4,"t":1},"speaker":"B","text":"パラパラにするのは難しいのですか？"},{"id":{"d":837,"n":5,"t":1},"speaker":"A","text":"家庭の火力には限界がありますから。できる範囲で頑張ってみます。"},{"id":{"d":837,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。何か手伝えることはありますか？"},{"id":{"d":837,"n":7,"t":1},"speaker":"A","text":"では、机を拭いて、レンゲを出しておいてください。"},{"id":{"d":837,"n":8,"t":1},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":838,"n":1,"t":1},"speaker":"A","text":"きれいな色のそうめんですね。"},{"id":{"d":838,"n":2,"t":1},"speaker":"B","text":"お中元に頂いたのですよ。"},{"id":{"d":838,"n":3,"t":1},"speaker":"A","text":"7色もあります。"},{"id":{"d":838,"n":4,"t":1},"speaker":"B","text":"少しずつ全部食べられるように小さな束にしたのです。"},{"id":{"d":838,"n":5,"t":1},"speaker":"A","text":"これなら7色全部食べられますね。"},{"id":{"d":838,"n":6,"t":1},"speaker":"B","text":"薬味もいろいろそろえてみました。"},{"id":{"d":838,"n":7,"t":1},"speaker":"A","text":"ネギ、オオバ、ミョウガとこれはパクチーですか？"},{"id":{"d":838,"n":8,"t":1},"speaker":"B","text":"はい。意外かもしれませんがパクチーはそうめんによく合うのですよ。エスニック風になります。"}];
INSERT INTO sentence [{"id":{"d":839,"n":1,"t":1},"speaker":"A","text":"今日は暇ですか？"},{"id":{"d":839,"n":2,"t":1},"speaker":"B","text":"休みですから暇ですよ。"},{"id":{"d":839,"n":3,"t":1},"speaker":"A","text":"では、梅酒づくりを手伝ってくれませんか？"},{"id":{"d":839,"n":4,"t":1},"speaker":"B","text":"もちろんです。何をすればよいのですか？"},{"id":{"d":839,"n":5,"t":1},"speaker":"A","text":"私が洗ってきれいにしますから、ウメの実のへたをようじで取ってください。"},{"id":{"d":839,"n":6,"t":1},"speaker":"B","text":"了解しました。"}];
INSERT INTO sentence [{"id":{"d":840,"n":1,"t":1},"speaker":"A","text":"今日は久しぶりに梅酒を飲みたい気分です。"},{"id":{"d":840,"n":2,"t":1},"speaker":"B","text":"いつもはビールなのに珍しいですね。"},{"id":{"d":840,"n":3,"t":1},"speaker":"A","text":"家に梅酒はありますか？"},{"id":{"d":840,"n":4,"t":1},"speaker":"B","text":"今年作ったものはまだ飲めませんが、去年仕込んだ梅酒でしたらまだ残っていますよ。"},{"id":{"d":840,"n":5,"t":1},"speaker":"A","text":"ではそれを頂きたいです。"},{"id":{"d":840,"n":6,"t":1},"speaker":"B","text":"分かりました。食前酒にしましょう。せっかくなのでおつまみもウメを使ったものにしようと思いますが、何か食べたいものはありますか？"},{"id":{"d":840,"n":7,"t":1},"speaker":"A","text":"前に作ってくれた梅シソチーズささみカツです。"},{"id":{"d":840,"n":8,"t":1},"speaker":"B","text":"ではそれにします。ささみはありませんので買ってきてくれますか？"},{"id":{"d":840,"n":9,"t":1},"speaker":"A","text":"お安いご用です。いまから行ってきますね。"}];
INSERT INTO sentence [{"id":{"d":841,"n":1,"t":1},"speaker":"A","text":"こちらがお店のエプロンです。"},{"id":{"d":841,"n":2,"t":1},"speaker":"B","text":"仕事中はこれを付ければよいのですね？"},{"id":{"d":841,"n":3,"t":1},"speaker":"A","text":"はい。エプロンの下は動きやすい服装にしてください。それから、髪はまとめるようにしてください。"},{"id":{"d":841,"n":4,"t":1},"speaker":"B","text":"分かりました。私はなで肩なのでエプロンのひもがよく肩から落ちてしまいます。"},{"id":{"d":841,"n":5,"t":1},"speaker":"A","text":"肩ひものすべり止めなど付けている方もいらっしゃいますよ。"},{"id":{"d":841,"n":6,"t":1},"speaker":"B","text":"自分で付けてもよいのですか？"},{"id":{"d":841,"n":7,"t":1},"speaker":"A","text":"お仕事に差し障りがないようなかたちでしたら自由にしてくださって構いません。"}];
INSERT INTO sentence [{"id":{"d":842,"n":1,"t":1},"speaker":"A","text":"残業前に2人分の夕食を買ってきました。"},{"id":{"d":842,"n":2,"t":1},"speaker":"B","text":"私の分も買ってきてくれたのですか？ありがとうございます。"},{"id":{"d":842,"n":3,"t":1},"speaker":"A","text":"おにぎりと焼きそばとサンドイッチとアンパンと手巻きずし、どれがいいですか？"},{"id":{"d":842,"n":4,"t":1},"speaker":"B","text":"どれもおいしそうです。ではサンドイッチをください。"},{"id":{"d":842,"n":5,"t":1},"speaker":"A","text":"はい、どうぞ。コーヒーとお茶もありますよ。どちらがいいですか？"},{"id":{"d":842,"n":6,"t":1},"speaker":"B","text":"コーヒーをお願いします。お金はこれで足りますか？"}];
INSERT INTO sentence [{"id":{"d":843,"n":1,"t":1},"speaker":"A","text":"今、プシュッという音がしましたか？"},{"id":{"d":843,"n":2,"t":1},"speaker":"B","text":"私のマグボトルの音ですね。"},{"id":{"d":843,"n":3,"t":1},"speaker":"A","text":"もしかして炭酸水が入っていますか？"},{"id":{"d":843,"n":4,"t":1},"speaker":"B","text":"はい。レモン味の炭酸水を入れています。"},{"id":{"d":843,"n":5,"t":1},"speaker":"A","text":"私が前にマイボトルに炭酸を入れていたらふたから漏れてしまいましたが、大丈夫なのですか？"},{"id":{"d":843,"n":6,"t":1},"speaker":"B","text":"これは炭酸も入れられるボトルなので大丈夫です。"},{"id":{"d":843,"n":7,"t":1},"speaker":"A","text":"炭酸が入れられるマグボトルがあるなど知りませんでした。"},{"id":{"d":843,"n":8,"t":1},"speaker":"B","text":"最近、発売されたものなのです。売り場に行ってみるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":844,"n":1,"t":1},"speaker":"A","text":"甘いものはお好きですか？"},{"id":{"d":844,"n":2,"t":1},"speaker":"B","text":"大好きですよ。"},{"id":{"d":844,"n":3,"t":1},"speaker":"A","text":"ご近所のおばあさんにおはぎを頂いたのですが、いかがですか？"},{"id":{"d":844,"n":4,"t":1},"speaker":"B","text":"これを全部ですか？"},{"id":{"d":844,"n":5,"t":1},"speaker":"A","text":"おばあさんには悪いのですが、甘過ぎて私は食べられません。"},{"id":{"d":844,"n":6,"t":1},"speaker":"B","text":"断れなかったのですね。"},{"id":{"d":844,"n":7,"t":1},"speaker":"A","text":"はい。昨年も頂いて困り果ててしまいました。"},{"id":{"d":844,"n":8,"t":1},"speaker":"B","text":"内緒で頂いておきますね。私はおはぎが好きなのでうれしいです。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":845,"n":1,"t":1},"speaker":"A","text":"太一君、お隣に回覧板を持っていってください。"},{"id":{"d":845,"n":2,"t":1},"speaker":"B","text":"このアニメが終わった後でよいですか？"},{"id":{"d":845,"n":3,"t":1},"speaker":"A","text":"あとどれくらいで終わるのですか？"},{"id":{"d":845,"n":4,"t":1},"speaker":"B","text":"5分です。"},{"id":{"d":845,"n":5,"t":1},"speaker":"A","text":"分かりました。終わってから持っていってください。"},{"id":{"d":845,"n":6,"t":1},"speaker":"B","text":"ハンコは押してありますか？"},{"id":{"d":845,"n":7,"t":1},"speaker":"A","text":"忘れていました。今から押します。"}];
INSERT INTO sentence [{"id":{"d":846,"n":1,"t":1},"speaker":"A","text":"真夏にジーンズは暑くありませんか？"},{"id":{"d":846,"n":2,"t":1},"speaker":"B","text":"暑いとは思いますが、ファッションですから。"},{"id":{"d":846,"n":3,"t":1},"speaker":"A","text":"せめてジーンズのショートパンツにされたらいかがでしょう？"},{"id":{"d":846,"n":4,"t":1},"speaker":"B","text":"ショートパンツは似合わないのですよ。"},{"id":{"d":846,"n":5,"t":1},"speaker":"A","text":"そうでしょうか？似合いそうですが。"},{"id":{"d":846,"n":6,"t":1},"speaker":"B","text":"私はジーンズが好きなので多少暑くてもはき通します。"},{"id":{"d":846,"n":7,"t":1},"speaker":"A","text":"ファッションは我慢なのですね。"},{"id":{"d":846,"n":8,"t":1},"speaker":"B","text":"涼しくて似合うファッションを見つけたら変えますけれどね。"}];
INSERT INTO sentence [{"id":{"d":847,"n":1,"t":1},"speaker":"A","text":"ハンバーガーを1つください。"},{"id":{"d":847,"n":2,"t":1},"speaker":"B","text":"お飲み物はどうなさいますか？"},{"id":{"d":847,"n":3,"t":1},"speaker":"A","text":"コーラの小さいものをお願いします。"},{"id":{"d":847,"n":4,"t":1},"speaker":"B","text":"ご一緒にフライドポテトもいかがですか？"},{"id":{"d":847,"n":5,"t":1},"speaker":"A","text":"フライドポテトは要りません。"},{"id":{"d":847,"n":6,"t":1},"speaker":"B","text":"こちらでお召し上がりになりますか？"},{"id":{"d":847,"n":7,"t":1},"speaker":"A","text":"持ち帰ります。"},{"id":{"d":847,"n":8,"t":1},"speaker":"B","text":"少々お待ちください。"}];
INSERT INTO sentence [{"id":{"d":848,"n":1,"t":1},"speaker":"A","text":"すみません。筋子と昆布のおにぎりをお願いします。"},{"id":{"d":848,"n":2,"t":1},"speaker":"B","text":"お持ち帰りですか？"},{"id":{"d":848,"n":3,"t":1},"speaker":"A","text":"はい、持ち帰りです。"},{"id":{"d":848,"n":4,"t":1},"speaker":"B","text":"かしこまりました。少々お待ちください。"},{"id":{"d":848,"n":5,"t":1},"speaker":"A","text":"冷たいお茶もありますか？"},{"id":{"d":848,"n":6,"t":1},"speaker":"B","text":"ペットボトルの500ミリリットルのものでしたらございます。"},{"id":{"d":848,"n":7,"t":1},"speaker":"A","text":"では、それもください。"},{"id":{"d":848,"n":8,"t":1},"speaker":"B","text":"全部で800円です。"}];
INSERT INTO sentence [{"id":{"d":849,"n":1,"t":1},"speaker":"A","text":"舞台が始まる前にお手洗いに行ってきます。"},{"id":{"d":849,"n":2,"t":1},"speaker":"B","text":"ハンカチを持っていますか？"},{"id":{"d":849,"n":3,"t":1},"speaker":"A","text":"ここのお手洗いはペーパータオルがあるので大丈夫ですよ。"},{"id":{"d":849,"n":4,"t":1},"speaker":"B","text":"それなら安心です。"},{"id":{"d":849,"n":5,"t":1},"speaker":"A","text":"ご心配ありがとうございます。"},{"id":{"d":849,"n":6,"t":1},"speaker":"B","text":"ゆっくり行ってきてください。"},{"id":{"d":849,"n":7,"t":1},"speaker":"A","text":"帰りにパンフレットをもらってきますので座って待っていてください。"},{"id":{"d":849,"n":8,"t":1},"speaker":"B","text":"分かりました。そこに座って待っています。"}];
INSERT INTO sentence [{"id":{"d":850,"n":1,"t":1},"speaker":"A","text":"田舎の母から荷物が送られてきました。"},{"id":{"d":850,"n":2,"t":1},"speaker":"B","text":"ありがたいですね。お米とかお野菜ですか？"},{"id":{"d":850,"n":3,"t":1},"speaker":"A","text":"私は料理ができませんので、どちらも入ってはいませんでした。"},{"id":{"d":850,"n":4,"t":1},"speaker":"B","text":"では着るものですか？"},{"id":{"d":850,"n":5,"t":1},"speaker":"A","text":"いいえ、入っていたのはレトルト食品などのインスタント食品です。"},{"id":{"d":850,"n":6,"t":1},"speaker":"B","text":"お湯さえあれば食べられるものですね。"},{"id":{"d":850,"n":7,"t":1},"speaker":"A","text":"はい。お湯やレンジで温めるだけのものです。ありがたいです。"},{"id":{"d":850,"n":8,"t":1},"speaker":"B","text":"お母さまの愛情ですね。"}];
INSERT INTO sentence [{"id":{"d":851,"n":1,"t":1},"speaker":"A","text":"干物はお好きですか？"},{"id":{"d":851,"n":2,"t":1},"speaker":"B","text":"好きですよ。"},{"id":{"d":851,"n":3,"t":1},"speaker":"A","text":"では、これをもらってはいただけませんか？"},{"id":{"d":851,"n":4,"t":1},"speaker":"B","text":"アジの干物をこれほどにたくさんですか？"},{"id":{"d":851,"n":5,"t":1},"speaker":"A","text":"はい。私が釣ったのです。"},{"id":{"d":851,"n":6,"t":1},"speaker":"B","text":"ご自分で干物にされたのですか？"},{"id":{"d":851,"n":7,"t":1},"speaker":"A","text":"干物にしたのは釣り船屋さんのおかみさんです。有料ですが加工してくれるのです。"},{"id":{"d":851,"n":8,"t":1},"speaker":"B","text":"新鮮でおいしそうです。頂きます。"}];
INSERT INTO sentence [{"id":{"d":852,"n":1,"t":1},"speaker":"A","text":"このかす漬けはおいしいですね。"},{"id":{"d":852,"n":2,"t":1},"speaker":"B","text":"義母から代々伝わるかす床を分けていただいたのです。"},{"id":{"d":852,"n":3,"t":1},"speaker":"A","text":"いつから漬けているのですか？"},{"id":{"d":852,"n":4,"t":1},"speaker":"B","text":"義父のおばあさんが作り始めたのだと聞きました。"},{"id":{"d":852,"n":5,"t":1},"speaker":"A","text":"戦前でしょうか？"},{"id":{"d":852,"n":6,"t":1},"speaker":"B","text":"そうだと思います。"},{"id":{"d":852,"n":7,"t":1},"speaker":"A","text":"素晴らしいですね。大事に次の世代につなげていきたいものですね。"},{"id":{"d":852,"n":8,"t":1},"speaker":"B","text":"ええ。プレッシャーを感じますが頑張りたいと思います。"}];
INSERT INTO sentence [{"id":{"d":853,"n":1,"t":1},"speaker":"A","text":"うちは玄関にたどり着く前に長い階段があるのですが、最近は体がきつくて困ります。"},{"id":{"d":853,"n":2,"t":1},"speaker":"B","text":"15段くらいはありますものね。"},{"id":{"d":853,"n":3,"t":1},"speaker":"A","text":"買い物帰りに重い荷物を運ぶ時のつらさといったらありません。"},{"id":{"d":853,"n":4,"t":1},"speaker":"B","text":"疲れているのに重い荷物は大変そうです。"},{"id":{"d":853,"n":5,"t":1},"speaker":"A","text":"どうしてあのような家を選んでしまったのかと時々思いますよ。"},{"id":{"d":853,"n":6,"t":1},"speaker":"B","text":"20年前は景色の良さを自慢していましたよ。"},{"id":{"d":853,"n":7,"t":1},"speaker":"A","text":"そうでした。景色は今も良いのですけれどね。"},{"id":{"d":853,"n":8,"t":1},"speaker":"B","text":"年を取ったらつらいことが出てきてしまったのですね。"}];
INSERT INTO sentence [{"id":{"d":854,"n":1,"t":1},"speaker":"A","text":"冷蔵庫を買ったのですが、アパートの冷蔵庫置き場に入らなくて困りました。"},{"id":{"d":854,"n":2,"t":1},"speaker":"B","text":"それは大変ですね。いったん持ち帰ってもらわなかったのですか？"},{"id":{"d":854,"n":3,"t":1},"speaker":"A","text":"気に入って買ったので、冷蔵庫置き場ではなく、お部屋の方に置いてもらいました。"},{"id":{"d":854,"n":4,"t":1},"speaker":"B","text":"ワンルームで大きな冷蔵庫は存在感がありそうですね。"},{"id":{"d":854,"n":5,"t":1},"speaker":"A","text":"思ったより圧迫感があります。"},{"id":{"d":854,"n":6,"t":1},"speaker":"B","text":"今後、ずっとその生活をするのですか？"},{"id":{"d":854,"n":7,"t":1},"speaker":"A","text":"引っ越しも考えないといけませんね。"},{"id":{"d":854,"n":8,"t":1},"speaker":"B","text":"できるだけ早い方がいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":855,"n":1,"t":1},"speaker":"A","text":"ゴーヤーチャンプルーを作りましたよ。"},{"id":{"d":855,"n":2,"t":1},"speaker":"B","text":"冬なのにゴーヤーですか？"},{"id":{"d":855,"n":3,"t":1},"speaker":"A","text":"去年の夏に採れたものを冷凍しておいたのです。"},{"id":{"d":855,"n":4,"t":1},"speaker":"B","text":"ゴーヤーは冷凍できるのですね。"},{"id":{"d":855,"n":5,"t":1},"speaker":"A","text":"味見をしてみましたが、それほど悪くありませんよ。"},{"id":{"d":855,"n":6,"t":1},"speaker":"B","text":"本当ですね。おいしいです。"},{"id":{"d":855,"n":7,"t":1},"speaker":"A","text":"今年も庭で育てて冷凍しようと思います。"},{"id":{"d":855,"n":8,"t":1},"speaker":"B","text":"ゴーヤーは大好きですから、また作ってください。"}];
INSERT INTO sentence [{"id":{"d":856,"n":1,"t":1},"speaker":"A","text":"2人でおそろいの服なのですね。"},{"id":{"d":856,"n":2,"t":1},"speaker":"B","text":"はい、ペアルックに憧れていたのです。"},{"id":{"d":856,"n":3,"t":1},"speaker":"A","text":"そのシャツはメンズですか？"},{"id":{"d":856,"n":4,"t":1},"speaker":"B","text":"今日のものはレディースです。"},{"id":{"d":856,"n":5,"t":1},"speaker":"A","text":"彼が着ているものもレディースなのですか？"},{"id":{"d":856,"n":6,"t":1},"speaker":"B","text":"はい。メンズ風の仕立てなのですが、これもレディースです。"},{"id":{"d":856,"n":7,"t":1},"speaker":"A","text":"彼は細身ですから着られるのですね。"},{"id":{"d":856,"n":8,"t":1},"speaker":"B","text":"そうなのです。私と1サイズしか変わりませんよ。"}];
INSERT INTO sentence [{"id":{"d":857,"n":1,"t":1},"speaker":"A","text":"タンスの前にレインスーツを置いておきました。"},{"id":{"d":857,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。明日の雨の予報は残念ながら変わっていないようです。"},{"id":{"d":857,"n":3,"t":1},"speaker":"A","text":"子どもたちの登校見守り当番が、嫌なお天気の日に当たってしまいましたね。"},{"id":{"d":857,"n":4,"t":1},"speaker":"B","text":"でもレインスーツを着れば問題ありません。これにはパンツも付いているのですね。"},{"id":{"d":857,"n":5,"t":1},"speaker":"A","text":"天気予報を聞いて慌てて買いに行ってきたのです。"},{"id":{"d":857,"n":6,"t":1},"speaker":"B","text":"ありがとうございます、助かります。この予報だとレインシューズも必要そうですね。"},{"id":{"d":857,"n":7,"t":1},"speaker":"A","text":"抜かりありません。もう玄関前に用意してありますよ。"},{"id":{"d":857,"n":8,"t":1},"speaker":"B","text":"何から何までありがとうございます。大雨にならないといいのですが。"},{"id":{"d":857,"n":9,"t":1},"speaker":"A","text":"そうですね。気を付けて行ってきてください。"}];
INSERT INTO sentence [{"id":{"d":858,"n":1,"t":1},"speaker":"A","text":"お昼ご飯にのり巻きを買ってきました。"},{"id":{"d":858,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。おみそ汁でも作りましょうか？"},{"id":{"d":858,"n":3,"t":1},"speaker":"A","text":"よいのですか？おみそ汁があればうれしいですね。"},{"id":{"d":858,"n":4,"t":1},"speaker":"B","text":"お豆腐があるので、すぐに作りますね。"},{"id":{"d":858,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。デザートにわらび餅もありますよ。"},{"id":{"d":858,"n":6,"t":1},"speaker":"B","text":"うれしいです。豪華なお昼ご飯になりますね。"},{"id":{"d":858,"n":7,"t":1},"speaker":"A","text":"デパートの地下をうろうろしたら、買いたいものがたくさんあって困りました。"},{"id":{"d":858,"n":8,"t":1},"speaker":"B","text":"どれもおいしそうですものね。"}];
INSERT INTO sentence [{"id":{"d":859,"n":1,"t":1},"speaker":"A","text":"昼休みに裏通りの喫茶店にランチを食べに行きませんか？"},{"id":{"d":859,"n":2,"t":1},"speaker":"B","text":"裏通りの喫茶店ですか？"},{"id":{"d":859,"n":3,"t":1},"speaker":"A","text":"そこです。デザートとドリンク付きのオムライスが900円だそうです。"},{"id":{"d":859,"n":4,"t":1},"speaker":"B","text":"オムライスですか。食べたいです。"},{"id":{"d":859,"n":5,"t":1},"speaker":"A","text":"昼休みになったら駆け足で行かないと行列の後ろになってしまいます。"},{"id":{"d":859,"n":6,"t":1},"speaker":"B","text":"昼休みの開始と同時に出かけられるよう、頑張って仕事を終わらせましょう。"},{"id":{"d":859,"n":7,"t":1},"speaker":"A","text":"そうですね。限定数ではないようなので食べ損なうことはないと思いますが。"},{"id":{"d":859,"n":8,"t":1},"speaker":"B","text":"昼休み後の遅刻が心配ですね。"}];
INSERT INTO sentence [{"id":{"d":860,"n":1,"t":1},"speaker":"A","text":"近いうちに子ども用食器を買いに行こうと思います。"},{"id":{"d":860,"n":2,"t":1},"speaker":"B","text":"もうそろそろ下の子も必要ですからね。"},{"id":{"d":860,"n":3,"t":1},"speaker":"A","text":"上の子の時はお母さんが1セットをくれましたが、下の子の分はないのです。"},{"id":{"d":860,"n":4,"t":1},"speaker":"B","text":"まだ上の子の分はお古にはなりませんよね？"},{"id":{"d":860,"n":5,"t":1},"speaker":"A","text":"はい。まだまだ現役で使っていますから。"},{"id":{"d":860,"n":6,"t":1},"speaker":"B","text":"格差が出るとかわいそうだと思いますし、どれを買ったらよいのか迷います。"},{"id":{"d":860,"n":7,"t":1},"speaker":"A","text":"同じシリーズの違う絵のセットがあればいいですね。"},{"id":{"d":860,"n":8,"t":1},"speaker":"B","text":"いいですね。お母さんがどこで買われたのか聞いてみます。"}];
INSERT INTO sentence [{"id":{"d":861,"n":1,"t":1},"speaker":"A","text":"今日のおやつは何ですか？"},{"id":{"d":861,"n":2,"t":1},"speaker":"B","text":"イモ餅ですよ。"},{"id":{"d":861,"n":3,"t":1},"speaker":"A","text":"前に作ってくれた甘辛いたれのお団子ですか？"},{"id":{"d":861,"n":4,"t":1},"speaker":"B","text":"そうですよ。"},{"id":{"d":861,"n":5,"t":1},"speaker":"A","text":"うれしいです。あれはイモが入っているのですか？"},{"id":{"d":861,"n":6,"t":1},"speaker":"B","text":"ジャガイモで作っているのですよ。"},{"id":{"d":861,"n":7,"t":1},"speaker":"A","text":"そうでしたか。前はおいしいことだけで満足していました。"},{"id":{"d":861,"n":8,"t":1},"speaker":"B","text":"簡単ですから、今度、作り方を教えてあげましょうね。"}];
INSERT INTO sentence [{"id":{"d":862,"n":1,"t":1},"speaker":"A","text":"最後に冷麺を頼んでもよいですか？"},{"id":{"d":862,"n":2,"t":1},"speaker":"B","text":"もちろんです。お肉はもうよいのですか？"},{"id":{"d":862,"n":3,"t":1},"speaker":"A","text":"たくさん食べましたから、もう満腹です。"},{"id":{"d":862,"n":4,"t":1},"speaker":"B","text":"冷麺は1人前でよいですか？"},{"id":{"d":862,"n":5,"t":1},"speaker":"A","text":"麺は食べたいですが、全部は無理かもしれません。ハーフサイズはあるでしょうか？それか2人で分けませんか？"},{"id":{"d":862,"n":6,"t":1},"speaker":"B","text":"では小どんぶりを頼んで2人で食べましょう。"},{"id":{"d":862,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。店員さんを呼びますね。"}];
INSERT INTO sentence [{"id":{"d":863,"n":1,"t":1},"speaker":"A","text":"作業着のボタンが取れそうです。"},{"id":{"d":863,"n":2,"t":1},"speaker":"B","text":"そこに置いておいてください。付けておきますから。"},{"id":{"d":863,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。両方の袖のボタンが取れそうです。"},{"id":{"d":863,"n":4,"t":1},"speaker":"B","text":"ボタンは落としてはいないのですよね？"},{"id":{"d":863,"n":5,"t":1},"speaker":"A","text":"はい、かろうじて付いています。"},{"id":{"d":863,"n":6,"t":1},"speaker":"B","text":"脱ぐ時にもなくさないようにしておいてくださいね。"},{"id":{"d":863,"n":7,"t":1},"speaker":"A","text":"分かりました。明日の朝までには間に合うでしょうか？"},{"id":{"d":863,"n":8,"t":1},"speaker":"B","text":"間に合わせます。朝にはお弁当と一緒に置いておきますよ。"}];
INSERT INTO sentence [{"id":{"d":864,"n":1,"t":1},"speaker":"A","text":"防災リュックに入れておいたお菓子が賞味期限切れになりそうです。"},{"id":{"d":864,"n":2,"t":1},"speaker":"B","text":"では今日のおやつはそのお菓子にしましょうよ。"},{"id":{"d":864,"n":3,"t":1},"speaker":"A","text":"ビスケットとクッキーがありますが、どちらがいいですか？"},{"id":{"d":864,"n":4,"t":1},"speaker":"B","text":"私はクッキーの方がいいです。薄くて食べやすいです。"},{"id":{"d":864,"n":5,"t":1},"speaker":"A","text":"どちらにしろ、そのうちビスケットも食べなければなりませんけれどね。"},{"id":{"d":864,"n":6,"t":1},"speaker":"B","text":"食べてしまった分は新しく買い足すのですか？"},{"id":{"d":864,"n":7,"t":1},"speaker":"A","text":"はい。なくなったままにはしておけませんから、また買いますよ。"}];
INSERT INTO sentence [{"id":{"d":865,"n":1,"t":1},"speaker":"A","text":"今日は疲れてしまってご飯が作れません。"},{"id":{"d":865,"n":2,"t":1},"speaker":"B","text":"お疲れさまでした。私が作るので休んでいてよいですよ。"},{"id":{"d":865,"n":3,"t":1},"speaker":"A","text":"ありがたいですが、あなたも最近仕事が大変そうだったので、出前を取りませんか？"},{"id":{"d":865,"n":4,"t":1},"speaker":"B","text":"いいですね。何にしますか？"},{"id":{"d":865,"n":5,"t":1},"speaker":"A","text":"あなたが食べたいものにしましょう。"},{"id":{"d":865,"n":6,"t":1},"speaker":"B","text":"かつ丼はどうでしょうか？"},{"id":{"d":865,"n":7,"t":1},"speaker":"A","text":"いいですね。お父さんも好きですから、みんなそれにしましょう。"},{"id":{"d":865,"n":8,"t":1},"speaker":"B","text":"かつ丼3つですね。私が電話します。豚汁も頼んでよいですか？"},{"id":{"d":865,"n":9,"t":1},"speaker":"A","text":"もちろんです。お願いしますね。"}];
INSERT INTO sentence [{"id":{"d":866,"n":1,"t":1},"speaker":"A","text":"パーカーのひもがかわいいですね。"},{"id":{"d":866,"n":2,"t":1},"speaker":"B","text":"これはめいっ子がリリアンで編んでくれたひもなのです。"},{"id":{"d":866,"n":3,"t":1},"speaker":"A","text":"これほどまでに長くきれいに編めるものなのですね。"},{"id":{"d":866,"n":4,"t":1},"speaker":"B","text":"根気の良い子で姉も驚いていました。"},{"id":{"d":866,"n":5,"t":1},"speaker":"A","text":"他にも何か作れるのですか？"},{"id":{"d":866,"n":6,"t":1},"speaker":"B","text":"今はリリアンに夢中で、ストラップにしたり、ブレスレットにしたり髪飾りにしたりということをしています。"},{"id":{"d":866,"n":7,"t":1},"speaker":"A","text":"いろいろなものにできるのですね。"},{"id":{"d":866,"n":8,"t":1},"speaker":"B","text":"私が子どもの頃はただただ長いひもを作っていただけです。めいっ子は素晴らしいです。"}];
INSERT INTO sentence [{"id":{"d":867,"n":1,"t":1},"speaker":"A","text":"いらっしゃいませ。"},{"id":{"d":867,"n":2,"t":1},"speaker":"B","text":"コロッケサンドとバナナチョコサンドをください。"},{"id":{"d":867,"n":3,"t":1},"speaker":"A","text":"コロッケとバナナチョコですね。少々お待ちください。"},{"id":{"d":867,"n":4,"t":1},"speaker":"B","text":"コロッケサンドはソースを多めにしてください。"},{"id":{"d":867,"n":5,"t":1},"speaker":"A","text":"かしこまりました。"},{"id":{"d":867,"n":6,"t":1},"speaker":"B","text":"コッペパンは良いですね。好きなものを挟んでもらえますから。"},{"id":{"d":867,"n":7,"t":1},"speaker":"A","text":"お客さまのご要望に応えていましたら、どんどん種類が多くなってしまいました。"}];
INSERT INTO sentence [{"id":{"d":868,"n":1,"t":1},"speaker":"A","text":"暑いとそうめんが食べたくなりますね。"},{"id":{"d":868,"n":2,"t":1},"speaker":"B","text":"そうですね。実は、私は結婚するまでそうめんは体に悪いものだと思っていました。"},{"id":{"d":868,"n":3,"t":1},"speaker":"A","text":"なぜそう思っていたのですか？"},{"id":{"d":868,"n":4,"t":1},"speaker":"B","text":"そうめんを食べるとき、実家ではつゆと薬味だけでしたので。"},{"id":{"d":868,"n":5,"t":1},"speaker":"A","text":"それの何がいけないのですか？そうめんはそうやって食べるものなのではありませんか？"},{"id":{"d":868,"n":6,"t":1},"speaker":"B","text":"ほぼ炭水化物だけではありませんか？栄養のバランスが偏っていることが気になっていました。"},{"id":{"d":868,"n":7,"t":1},"speaker":"A","text":"そう言われればそうかもしれませんね。"},{"id":{"d":868,"n":8,"t":1},"speaker":"B","text":"結婚してからは、そうめんにサラダチキンやキュウリや薄焼き卵をのせています。"},{"id":{"d":868,"n":9,"t":1},"speaker":"A","text":"まるで冷やし中華のようですね。栄養のバランスも良さそうです。"},{"id":{"d":868,"n":10,"t":1},"speaker":"B","text":"意外かもしれませんが、これがとてもおいしいのですよ。今度ぜひ試してみてください。"}];
INSERT INTO sentence [{"id":{"d":869,"n":1,"t":1},"speaker":"A","text":"お隣さんから、潮干狩りで採ってきたというアサリを頂きました。"},{"id":{"d":869,"n":2,"t":1},"speaker":"B","text":"立派ですね。もう今日のうちに食べられますか？"},{"id":{"d":869,"n":3,"t":1},"speaker":"A","text":"いいえ、一晩砂抜きをする必要があるので、食べられるのは明日です。"},{"id":{"d":869,"n":4,"t":1},"speaker":"B","text":"明日が楽しみです。アサリで何を作ってくれるのですか？"},{"id":{"d":869,"n":5,"t":1},"speaker":"A","text":"おみそ汁と酒蒸しはどうですか？"},{"id":{"d":869,"n":6,"t":1},"speaker":"B","text":"いいですね。帰りにビールを買ってきます。"},{"id":{"d":869,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。帰りは何時頃になりそうですか？"},{"id":{"d":869,"n":8,"t":1},"speaker":"B","text":"早めに帰れるよう頑張ります。19時には帰るつもりです。"}];
INSERT INTO sentence [{"id":{"d":870,"n":1,"t":1},"speaker":"A","text":"レンジフードが汚れてしまったのですが、1人では掃除できそうにありません。"},{"id":{"d":870,"n":2,"t":1},"speaker":"B","text":"手伝いましょうかと言いたいところですが、あいにく、私は休日出勤なのです。"},{"id":{"d":870,"n":3,"t":1},"speaker":"A","text":"それは残念です。これを1人で掃除するのは少々骨が折れそうです。"},{"id":{"d":870,"n":4,"t":1},"speaker":"B","text":"年末ですし、今回はお掃除のプロに頼むのはどうでしょうか？"},{"id":{"d":870,"n":5,"t":1},"speaker":"A","text":"良い案です。ついでに台所とお風呂とトイレもお願いしたいのですが。"},{"id":{"d":870,"n":6,"t":1},"speaker":"B","text":"分かりました。費用は、ボーナスから奮発します。"},{"id":{"d":870,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。ぴかぴかの家で年を越せそうでうれしいです。"}];
INSERT INTO sentence [{"id":{"d":871,"n":1,"t":1},"speaker":"A","text":"今日のお夕飯は何にするのですか？"},{"id":{"d":871,"n":2,"t":1},"speaker":"B","text":"ラーメンにしようと思いますが、どうでしょうか？"},{"id":{"d":871,"n":3,"t":1},"speaker":"A","text":"インスタントラーメンですか？"},{"id":{"d":871,"n":4,"t":1},"speaker":"B","text":"はい、時間もありませんので簡単に済ませたいです。"},{"id":{"d":871,"n":5,"t":1},"speaker":"A","text":"野菜は入れるのですか？"},{"id":{"d":871,"n":6,"t":1},"speaker":"B","text":"健康のために野菜は入れて作ります。"},{"id":{"d":871,"n":7,"t":1},"speaker":"A","text":"よかったです。どんぶりを準備しますね。どこにありますか？"},{"id":{"d":871,"n":8,"t":1},"speaker":"B","text":"食器棚の右奥にあります。"}];
INSERT INTO sentence [{"id":{"d":872,"n":1,"t":1},"speaker":"A","text":"こんにちは。お招きいただきありがとうございます。お邪魔しますね。"},{"id":{"d":872,"n":2,"t":1},"speaker":"B","text":"お待ちしておりました。どうぞお上がりください。"},{"id":{"d":872,"n":3,"t":1},"speaker":"A","text":"すてきな玄関ですね。"},{"id":{"d":872,"n":4,"t":1},"speaker":"B","text":"あこがれの1人暮らしのために頑張ってDIYしたのです。"},{"id":{"d":872,"n":5,"t":1},"speaker":"A","text":"ご自分でされたのですか？素晴らしいですね。"},{"id":{"d":872,"n":6,"t":1},"speaker":"B","text":"実は100円ショップの材料がほとんどです。"},{"id":{"d":872,"n":7,"t":1},"speaker":"A","text":"それでは、それほどお金はかかっていないのですか？"},{"id":{"d":872,"n":8,"t":1},"speaker":"B","text":"ええ。各部屋、1万円以下です。"}];
INSERT INTO sentence [{"id":{"d":873,"n":1,"t":1},"speaker":"A","text":"お部屋の装飾を100円ショップでそろえて作るなんて、器用なのですね。"},{"id":{"d":873,"n":2,"t":1},"speaker":"B","text":"昔から工作は好きだったのです。"},{"id":{"d":873,"n":3,"t":1},"speaker":"A","text":"このラックなどは作るのが大変そうですよ。"},{"id":{"d":873,"n":4,"t":1},"speaker":"B","text":"ホームセンターで電動工具の使い方を教わったので、割と簡単にできました。"},{"id":{"d":873,"n":5,"t":1},"speaker":"A","text":"電動工具を使うのですね。私は触ったこともありません。"},{"id":{"d":873,"n":6,"t":1},"speaker":"B","text":"いろいろ便利に使えるので教わってよかったと思いました。"},{"id":{"d":873,"n":7,"t":1},"speaker":"A","text":"食器なども100円ショップですか？"},{"id":{"d":873,"n":8,"t":1},"speaker":"B","text":"そうです。最近の100円ショップはおしゃれでしょう？"}];
INSERT INTO sentence [{"id":{"d":874,"n":1,"t":1},"speaker":"A","text":"実家から栗きんとんが送られてきました。"},{"id":{"d":874,"n":2,"t":1},"speaker":"B","text":"この時期に、お節料理ですか？"},{"id":{"d":874,"n":3,"t":1},"speaker":"A","text":"お節の黄色い栗きんとんとは違いますよ。このような和菓子です。"},{"id":{"d":874,"n":4,"t":1},"speaker":"B","text":"この茶巾絞りのようなものが栗きんとんなのですか？"},{"id":{"d":874,"n":5,"t":1},"speaker":"A","text":"はい。実家のある岐阜では、これが名物なのです。"},{"id":{"d":874,"n":6,"t":1},"speaker":"B","text":"栗を練って茶巾絞りにしてあるのですね。おいしそうです。"},{"id":{"d":874,"n":7,"t":1},"speaker":"A","text":"もう休憩時間ですから、召し上がってください。"},{"id":{"d":874,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。いただきます。"}];
INSERT INTO sentence [{"id":{"d":875,"n":1,"t":1},"speaker":"A","text":"連休に家を掃除しましたら、大量のビデオテープが出てきました。"},{"id":{"d":875,"n":2,"t":1},"speaker":"B","text":"VHSのビデオテープですか？"},{"id":{"d":875,"n":3,"t":1},"speaker":"A","text":"そうです。"},{"id":{"d":875,"n":4,"t":1},"speaker":"B","text":"懐かしいですね。思い出の映像などは入っていないのですか？"},{"id":{"d":875,"n":5,"t":1},"speaker":"A","text":"入っていません。もうデッキも壊れて処分してしまいましたし、捨てたいのですが、いつ捨てたらよいのか分かりません。"},{"id":{"d":875,"n":6,"t":1},"speaker":"B","text":"それは自治体によって違うと思いますよ。"},{"id":{"d":875,"n":7,"t":1},"speaker":"A","text":"燃えるごみでよければ簡単でうれしいのですが、プラスチックごみですかね？"},{"id":{"d":875,"n":8,"t":1},"speaker":"B","text":"どうでしょうか。役所に聞いてみるといいと思います。"}];
INSERT INTO sentence [{"id":{"d":876,"n":1,"t":1},"speaker":"A","text":"お母さん、私は今日このお洋服を着ていきたくありません。"},{"id":{"d":876,"n":2,"t":1},"speaker":"B","text":"なぜですか？"},{"id":{"d":876,"n":3,"t":1},"speaker":"A","text":"今日は幼稚園でお誕生日会があるのです。"},{"id":{"d":876,"n":4,"t":1},"speaker":"B","text":"お誕生日会があると、この服では駄目なのですか？"},{"id":{"d":876,"n":5,"t":1},"speaker":"A","text":"もっとかわいい服でお誕生日会に出たいです。"},{"id":{"d":876,"n":6,"t":1},"speaker":"B","text":"なるほど、そういうことでしたか。"},{"id":{"d":876,"n":7,"t":1},"speaker":"A","text":"この前買った、お花柄のワンピースを着ていってもよいですか？"},{"id":{"d":876,"n":8,"t":1},"speaker":"B","text":"よいですよ。でも今日は少し寒いので、上にこのカーディガンを羽織るのはどうですか？"},{"id":{"d":876,"n":9,"t":1},"speaker":"A","text":"そうします。では着替えてきます。"}];
INSERT INTO sentence [{"id":{"d":877,"n":1,"t":1},"speaker":"A","text":"お母さん、見てください。ソファのクッションカバーを作りました。"},{"id":{"d":877,"n":2,"t":1},"speaker":"B","text":"まあ、素晴らしい。きれいな刺しゅうがしてありますね。"},{"id":{"d":877,"n":3,"t":1},"speaker":"A","text":"今日、学校の手芸クラブで作ったのです。"},{"id":{"d":877,"n":4,"t":1},"speaker":"B","text":"あなたがこれほど器用だったとは知りませんでした。"},{"id":{"d":877,"n":5,"t":1},"speaker":"A","text":"このクッションを、お母さん、使ってくれますか？"},{"id":{"d":877,"n":6,"t":1},"speaker":"B","text":"もちろんです。とてもすてきなので他の物もこのデザインでそろえたいくらいです。"},{"id":{"d":877,"n":7,"t":1},"speaker":"A","text":"うれしいです。他の物とは具体的に何がありますか？"},{"id":{"d":877,"n":8,"t":1},"speaker":"B","text":"棚の目隠しカーテンやドイリー、ポットカバーなどがあるとうれしいですが、大変でしょうか？"},{"id":{"d":877,"n":9,"t":1},"speaker":"A","text":"分かりました。時間はかかりますが少しずつ作ってみます。"},{"id":{"d":877,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":878,"n":1,"t":1},"speaker":"A","text":"今日はデザートにコーヒーゼリーがありますよ。"},{"id":{"d":878,"n":2,"t":1},"speaker":"B","text":"コーヒーは苦いから少し苦手です。ミカンゼリーが良かったです。"},{"id":{"d":878,"n":3,"t":1},"speaker":"A","text":"コーヒーを入れ過ぎてしまったのですよ。パパが飲むと思っていたのに出かけてしまったので。"},{"id":{"d":878,"n":4,"t":1},"speaker":"B","text":"では少しだけいただきます。"},{"id":{"d":878,"n":5,"t":1},"speaker":"A","text":"苦いのが苦手なら、上にアイスクリームをのせますか？"},{"id":{"d":878,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。それなら食べられそうです。"}];
INSERT INTO sentence [{"id":{"d":879,"n":1,"t":1},"speaker":"A","text":"おめでとうございます。大当たりです。"},{"id":{"d":879,"n":2,"t":1},"speaker":"B","text":"福引で当たりを引くなんて初めてです。何が当たりましたか？"},{"id":{"d":879,"n":3,"t":1},"speaker":"A","text":"2等賞はお米10キログラムです。"},{"id":{"d":879,"n":4,"t":1},"speaker":"B","text":"お米10キログラムですか？困りました。今日は車で来ていませんので持って帰ることができません。"},{"id":{"d":879,"n":5,"t":1},"speaker":"A","text":"そうでしたか。カートもお持ちではありませんか？"},{"id":{"d":879,"n":6,"t":1},"speaker":"B","text":"ありません。今日は化粧品を買うだけだと思っていましたから。"},{"id":{"d":879,"n":7,"t":1},"speaker":"A","text":"分かりました。では、宅配も請け負いますよ。"},{"id":{"d":879,"n":8,"t":1},"speaker":"B","text":"よろしいのですか？助かります。ありがとうございます。"},{"id":{"d":879,"n":9,"t":1},"speaker":"A","text":"こちらにご住所と電話番号をご記入ください。お届けは本日の夕方でよいですか？"},{"id":{"d":879,"n":10,"t":1},"speaker":"B","text":"今日の夕方は都合をつけられません。配達は明日の午前中にお願いしてもよいでしょうか？"},{"id":{"d":879,"n":11,"t":1},"speaker":"A","text":"かしこまりました。こちらのご住所へ明日の午前中にお届けに上がります。"},{"id":{"d":879,"n":12,"t":1},"speaker":"B","text":"ありがとうございます。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":880,"n":1,"t":1},"speaker":"A","text":"このコンビニエンスストアのソフトクリームはおいしいですね。"},{"id":{"d":880,"n":2,"t":1},"speaker":"B","text":"駐車場で立って食べなければいけないのが玉にきずですけれどね。"},{"id":{"d":880,"n":3,"t":1},"speaker":"A","text":"確かに。座って食べられる所があればいいのですが。"},{"id":{"d":880,"n":4,"t":1},"speaker":"B","text":"ここの近くには公園もありませんしね。"},{"id":{"d":880,"n":5,"t":1},"speaker":"A","text":"目の前の道路は車がビュンビュン通っていますしね。落ち着いて食べられないのは少し残念です。"}];
INSERT INTO sentence [{"id":{"d":881,"n":1,"t":1},"speaker":"A","text":"こんにちは。あら、ワンちゃんとおそろいのTシャツなのですね。"},{"id":{"d":881,"n":2,"t":1},"speaker":"B","text":"はい。今年の夏は葉山へ海水浴に行ったのですが、その時に買ってきたのです。"},{"id":{"d":881,"n":3,"t":1},"speaker":"A","text":"この「げ」と書いてあるのは何ですか？"},{"id":{"d":881,"n":4,"t":1},"speaker":"B","text":"これは葉山のビーチサンダル屋さんのマークです。"},{"id":{"d":881,"n":5,"t":1},"speaker":"A","text":"よく見るとお店の名前が書いてありますね。"},{"id":{"d":881,"n":6,"t":1},"speaker":"B","text":"色遣いに一目ぼれして購入してしまいました。"},{"id":{"d":881,"n":7,"t":1},"speaker":"A","text":"とてもかわいらしいです。ワンちゃんにもよく似合っていますね。"},{"id":{"d":881,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。このバッグも実は「げ」印なのですよ。"}];
INSERT INTO sentence [{"id":{"d":882,"n":1,"t":1},"speaker":"A","text":"こんにちは。今日はタイが安いのですね。"},{"id":{"d":882,"n":2,"t":1},"speaker":"B","text":"いらっしゃいませ。この時期はお祝いが多いので、お安くさせていただいています。"},{"id":{"d":882,"n":3,"t":1},"speaker":"A","text":"うちはお祝いではありませんけれど、1匹頂きます。"},{"id":{"d":882,"n":4,"t":1},"speaker":"B","text":"はい、毎度ありがとうございます。何枚かにおろしますか？"},{"id":{"d":882,"n":5,"t":1},"speaker":"A","text":"お造りにしていただけますか？"},{"id":{"d":882,"n":6,"t":1},"speaker":"B","text":"はい。アラはどうしますか？"},{"id":{"d":882,"n":7,"t":1},"speaker":"A","text":"アラも付けてください。アラ汁にしようと思います。"},{"id":{"d":882,"n":8,"t":1},"speaker":"B","text":"かしこまりました。"}];
INSERT INTO sentence [{"id":{"d":883,"n":1,"t":1},"speaker":"A","text":"防災袋に入れてあるパンの缶詰の賞味期限を見てくれますか？"},{"id":{"d":883,"n":2,"t":1},"speaker":"B","text":"はい。まだ1年は大丈夫ですよ。"},{"id":{"d":883,"n":3,"t":1},"speaker":"A","text":"あと1年ですか。買い直すかどうか悩みますね。"},{"id":{"d":883,"n":4,"t":1},"speaker":"B","text":"買い直すのは1年後では駄目なのですか？"},{"id":{"d":883,"n":5,"t":1},"speaker":"A","text":"今、ちょうどセールをしているのです。"},{"id":{"d":883,"n":6,"t":1},"speaker":"B","text":"どのくらい安いですか？"},{"id":{"d":883,"n":7,"t":1},"speaker":"A","text":"元の価格の5割引きです。"},{"id":{"d":883,"n":8,"t":1},"speaker":"B","text":"それはお買い得ですね。でも、そのセール品の賞味期限もチェックした方がいいと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":884,"n":1,"t":1},"speaker":"A","text":"コンビニエンスストアでたくさん買い物をしたら、エコバッグを頂きました。"},{"id":{"d":884,"n":2,"t":1},"speaker":"B","text":"お父さんはエコバッグを持っていませんのでちょうど良かったですね。"},{"id":{"d":884,"n":3,"t":1},"speaker":"A","text":"はい。今度から買い物にはこのエコバッグを持っていくことにします。"},{"id":{"d":884,"n":4,"t":1},"speaker":"B","text":"それがいいと思います。大きくて使いやすそうですね。"},{"id":{"d":884,"n":5,"t":1},"speaker":"A","text":"私もそう思います。大きさもですが、色も気に入っています。"},{"id":{"d":884,"n":6,"t":1},"speaker":"B","text":"きれいな紺色ですね。とてもすてきです。"}];
INSERT INTO sentence [{"id":{"d":885,"n":1,"t":1},"speaker":"A","text":"すみません、掃除機を取ってきてもらえますか？"},{"id":{"d":885,"n":2,"t":1},"speaker":"B","text":"構いませんが、どうしましたか？"},{"id":{"d":885,"n":3,"t":1},"speaker":"A","text":"ガラスのコップを割ってしまったのです。破片が散らばっていてここから動くことができません。"},{"id":{"d":885,"n":4,"t":1},"speaker":"B","text":"足の周りに破片がいっぱいですね。これは大変です。"},{"id":{"d":885,"n":5,"t":1},"speaker":"A","text":"不注意でコップを床に落としてしまいました。すみません。"},{"id":{"d":885,"n":6,"t":1},"speaker":"B","text":"大丈夫ですよ。今日に限って靴下もはいていないのですね。"},{"id":{"d":885,"n":7,"t":1},"speaker":"A","text":"暑かったのでついさっき靴下を脱いでしまったのです。"},{"id":{"d":885,"n":8,"t":1},"speaker":"B","text":"けがをしないように片づけるまで動かないでくださいね。"}];
INSERT INTO sentence [{"id":{"d":886,"n":1,"t":1},"speaker":"A","text":"クローゼットに入れる衣装ケースを買いに行きたいのですが、荷物を運ぶために車を出してもらえないでしょうか？"},{"id":{"d":886,"n":2,"t":1},"speaker":"B","text":"もちろんよいですよ。土曜日なら1日中空いているので、この日でよいですか？"},{"id":{"d":886,"n":3,"t":1},"speaker":"A","text":"はい、ありがとうございます。助かります。"},{"id":{"d":886,"n":4,"t":1},"speaker":"B","text":"クローゼットの奥行きはもう測りましたか？"},{"id":{"d":886,"n":5,"t":1},"speaker":"A","text":"まだ、測っていません。"},{"id":{"d":886,"n":6,"t":1},"speaker":"B","text":"衣装ケースはいろいろなサイズがあるので、あらかじめ必要なサイズを確かめておくといいですよ。"},{"id":{"d":886,"n":7,"t":1},"speaker":"A","text":"そうですか。土曜日までに測っておきます。測るための巻尺は家にありましたか？"},{"id":{"d":886,"n":8,"t":1},"speaker":"B","text":"あったはずです。以前、文房具の棚で見た気がします。"},{"id":{"d":886,"n":9,"t":1},"speaker":"A","text":"分かりました。探してみます。"}];
INSERT INTO sentence [{"id":{"d":887,"n":1,"t":1},"speaker":"A","text":"今日は、亡くなったおじいさんの家の庭を草刈りしてきますね。今は空き家になっているので、たまに手入れをしなくてはなりません。"},{"id":{"d":887,"n":2,"t":1},"speaker":"B","text":"お母さん1人では大変でしょう？僕も行きますよ。"},{"id":{"d":887,"n":3,"t":1},"speaker":"A","text":"それは助かります。ありがとう。"},{"id":{"d":887,"n":4,"t":1},"speaker":"B","text":"車も出すので、ホームセンターで道具を買っていきましょう。"},{"id":{"d":887,"n":5,"t":1},"speaker":"A","text":"そうですね。鎌は1本しかありませんので、もう1本買いましょう。"},{"id":{"d":887,"n":6,"t":1},"speaker":"B","text":"ついでに水分補給のお茶とか、休憩用のお菓子も必要ですね。"},{"id":{"d":887,"n":7,"t":1},"speaker":"A","text":"まるで、遠足のようですね。"},{"id":{"d":887,"n":8,"t":1},"speaker":"B","text":"おじいさんの家に行くのは昔から楽しい遊びでしたよ。"}];
INSERT INTO sentence [{"id":{"d":888,"n":1,"t":1},"speaker":"A","text":"お姉さん、もう着なくなった服はありませんか？"},{"id":{"d":888,"n":2,"t":1},"speaker":"B","text":"たくさんありますよ。何をするのですか？"},{"id":{"d":888,"n":3,"t":1},"speaker":"A","text":"洋服屋さんに持っていこうと思います。着なくなった服を持っていくと、クーポン券と引き換えてもらえるのですよ。"},{"id":{"d":888,"n":4,"t":1},"speaker":"B","text":"そうですか。ちょうど捨てるつもりでまとめていたので、この衣装ケースに入っているものは全て持っていってよいですよ。"},{"id":{"d":888,"n":5,"t":1},"speaker":"A","text":"これほどまでにたくさんの服をもらってしまって、よいのですか？"},{"id":{"d":888,"n":6,"t":1},"speaker":"B","text":"はい。もし1人で運ぶのが大変でしたら、お店まで持っていくのを私も手伝いますよ。"},{"id":{"d":888,"n":7,"t":1},"speaker":"A","text":"そうしてもらえると助かります。明日お店へ行こうと思っていたのですが、予定は空いていますか？"},{"id":{"d":888,"n":8,"t":1},"speaker":"B","text":"明日は15時以降なら空いています。遅いでしょうか？"},{"id":{"d":888,"n":9,"t":1},"speaker":"A","text":"大丈夫です。では15時過ぎに一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":889,"n":1,"t":1},"speaker":"A","text":"もうすぐご飯ができますよ。テーブルを拭いてください。"},{"id":{"d":889,"n":2,"t":1},"speaker":"B","text":"分かりました。"},{"id":{"d":889,"n":3,"t":1},"speaker":"A","text":"お箸と取り皿も持っていってください。"},{"id":{"d":889,"n":4,"t":1},"speaker":"B","text":"はい。今日のご飯は何ですか？"},{"id":{"d":889,"n":5,"t":1},"speaker":"A","text":"今日は煮豚ときんぴらとサラダですよ。"},{"id":{"d":889,"n":6,"t":1},"speaker":"B","text":"私が好きなものばかりです。"},{"id":{"d":889,"n":7,"t":1},"speaker":"A","text":"たくさん食べてくださいね。"},{"id":{"d":889,"n":8,"t":1},"speaker":"B","text":"ご飯をお代わりします。"}];
INSERT INTO sentence [{"id":{"d":890,"n":1,"t":1},"speaker":"A","text":"日曜日のマルシェが楽しみです。当日は何時に集合すればよいでしょうか？"},{"id":{"d":890,"n":2,"t":1},"speaker":"B","text":"開店は11時からですが、品出しなどの準備がありますので10時には来ていただけますか？"},{"id":{"d":890,"n":3,"t":1},"speaker":"A","text":"10時ですね。了解しました。"},{"id":{"d":890,"n":4,"t":1},"speaker":"B","text":"よろしくお願いします。報酬はうちのお野菜だけで本当によいのですか？"},{"id":{"d":890,"n":5,"t":1},"speaker":"A","text":"もちろんです。山本さんの作る野菜の大ファンなので本当にうれしいです。"},{"id":{"d":890,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。今年の野菜の出来はとても良いので、楽しみにしていてくださいね。"}];
INSERT INTO sentence [{"id":{"d":891,"n":1,"t":1},"speaker":"A","text":"今日はビルの前にキッチンカーがいましたよ。"},{"id":{"d":891,"n":2,"t":1},"speaker":"B","text":"私が出社した時にはいませんでした。何のキッチンカーですか？"},{"id":{"d":891,"n":3,"t":1},"speaker":"A","text":"カレー屋さんのようでした。前を通ったときに良い香りがしました。"},{"id":{"d":891,"n":4,"t":1},"speaker":"B","text":"お昼にカレーも良いかもしれませんね。"},{"id":{"d":891,"n":5,"t":1},"speaker":"A","text":"買いに行きますか？"},{"id":{"d":891,"n":6,"t":1},"speaker":"B","text":"そうしましょう。お昼が楽しみです。"},{"id":{"d":891,"n":7,"t":1},"speaker":"A","text":"ええ。リピートしたくなるようなおいしいカレーだといいですね。"}];
INSERT INTO sentence [{"id":{"d":892,"n":1,"t":1},"speaker":"A","text":"先輩、お昼を食べる時間がもうありません。"},{"id":{"d":892,"n":2,"t":1},"speaker":"B","text":"まだ諦めてはいけませんよ。"},{"id":{"d":892,"n":3,"t":1},"speaker":"A","text":"あと10分しかありませんよ。もう無理です。"},{"id":{"d":892,"n":4,"t":1},"speaker":"B","text":"ちょうどすぐそこに立ち食いソバ屋があります。そこで食べましょう。"},{"id":{"d":892,"n":5,"t":1},"speaker":"A","text":"立ち食いそば屋には入ったことがありません。注文の仕方を教えてください。"},{"id":{"d":892,"n":6,"t":1},"speaker":"B","text":"分かりました。まずはここで食券を買います。"},{"id":{"d":892,"n":7,"t":1},"speaker":"A","text":"了解です。現金しか使えないのですね。"},{"id":{"d":892,"n":8,"t":1},"speaker":"B","text":"そのようですね。時間がありませんので2人ともかけそばにしましょう。"},{"id":{"d":892,"n":9,"t":1},"speaker":"A","text":"かけそばの食券2枚買えました。次はどうすればよいですか？"},{"id":{"d":892,"n":10,"t":1},"speaker":"B","text":"ここのカウンターに渡します。あとは出てくるのを待つだけです。"},{"id":{"d":892,"n":11,"t":1},"speaker":"A","text":"もう出てきそうですね。早くて驚きました。"},{"id":{"d":892,"n":12,"t":1},"speaker":"B","text":"そうでしょう。さあ、出来上がったようです。5分以内に食べて戻りましょう。"}];
INSERT INTO sentence [{"id":{"d":893,"n":1,"t":1},"speaker":"A","text":"もう出かける時間ですね。忘れ物はありませんか？"},{"id":{"d":893,"n":2,"t":1},"speaker":"B","text":"はい、多分大丈夫だと思います。"},{"id":{"d":893,"n":3,"t":1},"speaker":"A","text":"ハンカチ、チリ紙、お財布、スマートフォン、水筒、携帯扇風機、塩あめ、きちんと入っていますか？"},{"id":{"d":893,"n":4,"t":1},"speaker":"B","text":"熱中症対策の心配までしてくれなくてもよいですよ。"},{"id":{"d":893,"n":5,"t":1},"speaker":"A","text":"そうは言っても毎日、熱中症で運ばれた人のニュースを聞くのですから心配です。"},{"id":{"d":893,"n":6,"t":1},"speaker":"B","text":"日傘も差していきますし、大丈夫です。塩あめも持っています。"},{"id":{"d":893,"n":7,"t":1},"speaker":"A","text":"それなら安心です。気を付けていってらっしゃい。"},{"id":{"d":893,"n":8,"t":1},"speaker":"B","text":"いってきます。"}];
INSERT INTO sentence [{"id":{"d":894,"n":1,"t":1},"speaker":"A","text":"週末に子どもたちが遊びに来るのですが、何か料理を用意した方がいいですよね？"},{"id":{"d":894,"n":2,"t":1},"speaker":"B","text":"何人くらい来るのですか？"},{"id":{"d":894,"n":3,"t":1},"speaker":"A","text":"クラスの子が3人来るらしいです。"},{"id":{"d":894,"n":4,"t":1},"speaker":"B","text":"簡単なお昼ご飯とおやつくらいは用意してもよさそうですね。"},{"id":{"d":894,"n":5,"t":1},"speaker":"A","text":"うちの家族と合わせて7人分はちょっと大変です。"},{"id":{"d":894,"n":6,"t":1},"speaker":"B","text":"手伝いますよ。おにぎりだけ作って、おかずはパーティープレートを予約するのはどうでしょうか？"},{"id":{"d":894,"n":7,"t":1},"speaker":"A","text":"助かります。それなら大丈夫そうです。"},{"id":{"d":894,"n":8,"t":1},"speaker":"B","text":"では早速予約しますね。受け取りには僕が行ってきます。"},{"id":{"d":894,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。そういえば、その日は駅前のドーナツ店の特売日です。パーティープレートを受け取るときに一緒にドーナツも買ってきてくれませんか？"},{"id":{"d":894,"n":10,"t":1},"speaker":"B","text":"よいですよ。おやつはドーナツということですね？"},{"id":{"d":894,"n":11,"t":1},"speaker":"A","text":"はい、そのつもりです。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":895,"n":1,"t":1},"speaker":"A","text":"ケーキを買ってきましたよ。"},{"id":{"d":895,"n":2,"t":1},"speaker":"B","text":"まあ、おいしそうです。たくさん買ってきてくれたのですね。"},{"id":{"d":895,"n":3,"t":1},"speaker":"A","text":"今から2人で1つずついただいて、残りはもしよければご家族で召し上がってください。"},{"id":{"d":895,"n":4,"t":1},"speaker":"B","text":"家族の分までありがとうございます。いまお皿を用意しますね。"},{"id":{"d":895,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。あなたはどれにしますか？"},{"id":{"d":895,"n":6,"t":1},"speaker":"B","text":"私はモンブランをいただいてもよいですか？"},{"id":{"d":895,"n":7,"t":1},"speaker":"A","text":"もちろんです。それでは私はフルーツタルトにします。"},{"id":{"d":895,"n":8,"t":1},"speaker":"B","text":"紅茶を入れてきますね。掛けてお待ちください。"}];
INSERT INTO sentence [{"id":{"d":896,"n":1,"t":1},"speaker":"A","text":"何を探しているのですか？"},{"id":{"d":896,"n":2,"t":1},"speaker":"B","text":"お節用の重箱です。去年ここにしまったはずなのですが。"},{"id":{"d":896,"n":3,"t":1},"speaker":"A","text":"1年に一度しか使わないものですから、奥に入ってしまっているのでしょうか？"},{"id":{"d":896,"n":4,"t":1},"speaker":"B","text":"そうかもしれません。もっと奥まで探してみます。"},{"id":{"d":896,"n":5,"t":1},"speaker":"A","text":"そういえば、隆志に頼んで高い所に置いた年もありましたね。"},{"id":{"d":896,"n":6,"t":1},"speaker":"B","text":"言われてみればありました。去年のことのような気がしてきました。"},{"id":{"d":896,"n":7,"t":1},"speaker":"A","text":"隆志に聞いてきましょうか？"},{"id":{"d":896,"n":8,"t":1},"speaker":"B","text":"お願いします。重箱が見つからなかったら、今年はお皿でよいですね。"},{"id":{"d":896,"n":9,"t":1},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":897,"n":1,"t":1},"speaker":"A","text":"お母さん、花子の服ばかりでかわいいのはずるいです。"},{"id":{"d":897,"n":2,"t":1},"speaker":"B","text":"咲、よく見てください。花子の服は、全部あなたのおさがりですよ。"},{"id":{"d":897,"n":3,"t":1},"speaker":"A","text":"そうでしたか？私の服は、あれほどまでにかわいいアップリケは付いていなかったと思います。"},{"id":{"d":897,"n":4,"t":1},"speaker":"B","text":"あなたがたくさん着て汚した所を隠すために付けたのです。"},{"id":{"d":897,"n":5,"t":1},"speaker":"A","text":"そうなのですか？"},{"id":{"d":897,"n":6,"t":1},"speaker":"B","text":"汚れたまま着せるのは、花子もかわいそうでしょう？"},{"id":{"d":897,"n":7,"t":1},"speaker":"A","text":"そうですけれど、私の時よりかわいいのは、なんだかずるいです。"},{"id":{"d":897,"n":8,"t":1},"speaker":"B","text":"それなら、たまにはあなたの服にも付けてあげますよ。"}];
INSERT INTO sentence [{"id":{"d":898,"n":1,"t":1},"speaker":"A","text":"娘さんのサマードレスは涼しそうですね。それにとてもすてきです。"},{"id":{"d":898,"n":2,"t":1},"speaker":"B","text":"あれは私の高校生の頃の浴衣をリメークしたものなのです。"},{"id":{"d":898,"n":3,"t":1},"speaker":"A","text":"生地が浴衣のようだとは思いましたが、思い出の浴衣のリメークだったのですね。"},{"id":{"d":898,"n":4,"t":1},"speaker":"B","text":"子ども用の大きさにしたら2着できたので、毎日洗濯をしながら交互に着せています。"},{"id":{"d":898,"n":5,"t":1},"speaker":"A","text":"もしかして、髪を結んでいるシュシュも浴衣で作ったのですか？"},{"id":{"d":898,"n":6,"t":1},"speaker":"B","text":"そうです。金魚の模様がかわいいので、シュシュとポシェットも作りました。"},{"id":{"d":898,"n":7,"t":1},"speaker":"A","text":"素晴らしいですね。器用でいろいろ作れるのがうらやましいです。"},{"id":{"d":898,"n":8,"t":1},"speaker":"B","text":"それほど器用でもありませんよ。よく見ると、ほつれていたりもします。"}];
INSERT INTO sentence [{"id":{"d":899,"n":1,"t":1},"speaker":"A","text":"今日はたくさん買い物をしてきました。"},{"id":{"d":899,"n":2,"t":1},"speaker":"B","text":"これほどたくさん食べ切れますか？"},{"id":{"d":899,"n":3,"t":1},"speaker":"A","text":"1日で食べ切るわけではありません。小分けにして冷凍するなど、工夫して保存するので大丈夫ですよ。"},{"id":{"d":899,"n":4,"t":1},"speaker":"B","text":"その作業を今からするわけですね。"},{"id":{"d":899,"n":5,"t":1},"speaker":"A","text":"はい、そうです。買い物よりも保存のための作業の方が大変かもしれません。"},{"id":{"d":899,"n":6,"t":1},"speaker":"B","text":"お役に立てるか分かりませんが、手伝いますよ。"},{"id":{"d":899,"n":7,"t":1},"speaker":"A","text":"とても助かります。早速ですが、ネギを輪切りにしてこの食料保存袋に入れておいてもらえますか？"},{"id":{"d":899,"n":8,"t":1},"speaker":"B","text":"それなら私にもできそうです。"},{"id":{"d":899,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。その間に私は冷凍食品を冷蔵庫にしまいますね。"}];
INSERT INTO sentence [{"id":{"d":900,"n":1,"t":1},"speaker":"A","text":"広告に載っていた特売品の卵はどちらにありますか？"},{"id":{"d":900,"n":2,"t":1},"speaker":"B","text":"申し訳ありません。先ほど最後の1パックが売れてしまいました。"},{"id":{"d":900,"n":3,"t":1},"speaker":"A","text":"もうなくなってしまったのですか？"},{"id":{"d":900,"n":4,"t":1},"speaker":"B","text":"はい。開店前から行列ができていましたので、開店してすぐになくなってしまいました。"},{"id":{"d":900,"n":5,"t":1},"speaker":"A","text":"残念です。では、マグロの冊ももうありませんよね？"},{"id":{"d":900,"n":6,"t":1},"speaker":"B","text":"そちらは、これから次の商品が並べられますので、少しお待ちいただければご購入いただけます。"},{"id":{"d":900,"n":7,"t":1},"speaker":"A","text":"本当ですか？魚売り場の前で待っていればよいのですか？"},{"id":{"d":900,"n":8,"t":1},"speaker":"B","text":"そうです。あちらに既に数人並ばれていますが、そこで販売します。"}];
INSERT INTO sentence [{"id":{"d":901,"n":1,"t":1},"speaker":"A","text":"一番近くのスーパーマーケットが来月末で閉店するそうです。"},{"id":{"d":901,"n":2,"t":1},"speaker":"B","text":"あのスーパーマーケットがあるからこのアパートに決めたのに、困りますね。"},{"id":{"d":901,"n":3,"t":1},"speaker":"A","text":"これからは少し遠くまで買い物に行かなければなりませんね。"},{"id":{"d":901,"n":4,"t":1},"speaker":"B","text":"そうなりますね。長距離を歩くのは少し大変なので、私は自転車を買おうと思います。"},{"id":{"d":901,"n":5,"t":1},"speaker":"A","text":"良い案ですね。それでも私は自転車には乗れませんので、その手は使えません。私は引っ越しを考えようと思います。"},{"id":{"d":901,"n":6,"t":1},"speaker":"B","text":"引っ越してしまうのは寂しいです。自転車の練習をしましょうよ。二輪車ではなく三輪自転車というものもありますよ。"}];
INSERT INTO sentence [{"id":{"d":902,"n":1,"t":1},"speaker":"A","text":"おばあちゃんが畑で採れたお野菜を送ってくれましたよ。"},{"id":{"d":902,"n":2,"t":1},"speaker":"B","text":"何が入っているか楽しみです。"},{"id":{"d":902,"n":3,"t":1},"speaker":"A","text":"そうですね。開けてよいですよ。"},{"id":{"d":902,"n":4,"t":1},"speaker":"B","text":"お母さん、トウモロコシが入っていました。大好きなのでうれしいです。"},{"id":{"d":902,"n":5,"t":1},"speaker":"A","text":"よかったですね。"},{"id":{"d":902,"n":6,"t":1},"speaker":"B","text":"トマトやキュウリも入っています。夏野菜がたくさんですよ。"},{"id":{"d":902,"n":7,"t":1},"speaker":"A","text":"ナスやピーマンもありますね。どれも立派です。"},{"id":{"d":902,"n":8,"t":1},"speaker":"B","text":"ナスとピーマンは苦手です。"},{"id":{"d":902,"n":9,"t":1},"speaker":"A","text":"ピーマンは肉詰めにしましょう。苦味が目立ちませんので食べやすいと思いますよ。"},{"id":{"d":902,"n":10,"t":1},"speaker":"B","text":"せっかくおばあちゃんが送ってくれたので、頑張って少しは食べようと思います。"}];
INSERT INTO sentence [{"id":{"d":903,"n":1,"t":1},"speaker":"A","text":"お母さん、ミカンをどうぞ。"},{"id":{"d":903,"n":2,"t":1},"speaker":"B","text":"ミカンですか？これはどうしたのですか？"},{"id":{"d":903,"n":3,"t":1},"speaker":"A","text":"お隣のお庭になっていたのです。"},{"id":{"d":903,"n":4,"t":1},"speaker":"B","text":"お隣さんのですか？黙って採ってきたりはしていませんよね？"},{"id":{"d":903,"n":5,"t":1},"speaker":"A","text":"盗んではいませんよ。お隣のおばさんがくれました。"},{"id":{"d":903,"n":6,"t":1},"speaker":"B","text":"安心しました。一瞬でも疑ってごめんなさい。お隣さんにお礼に行かないといけませんね。"},{"id":{"d":903,"n":7,"t":1},"speaker":"A","text":"お礼は私が「ありがとうございます」って言いました。"},{"id":{"d":903,"n":8,"t":1},"speaker":"B","text":"きちんとお礼を言えて偉かったですね。今度はお菓子を持ってもう一度一緒にお礼を言いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":904,"n":1,"t":1},"speaker":"A","text":"牛乳が賞味期限なのですが、800ミリリットルほど残っています。"},{"id":{"d":904,"n":2,"t":1},"speaker":"B","text":"1人暮らしでその量は大変ですね。"},{"id":{"d":904,"n":3,"t":1},"speaker":"A","text":"何とか消費できる良いアイデアはありませんか？"},{"id":{"d":904,"n":4,"t":1},"speaker":"B","text":"クリームシチューと牛乳かんを今晩のメニューにしたらどうでしょうか？"},{"id":{"d":904,"n":5,"t":1},"speaker":"A","text":"たくさん料理で消費できそうですね。でも、全部は食べられませんよ。"},{"id":{"d":904,"n":6,"t":1},"speaker":"B","text":"そういう時は食べるお客を呼べばよいと思います。"},{"id":{"d":904,"n":7,"t":1},"speaker":"A","text":"食べるお客ですか？"},{"id":{"d":904,"n":8,"t":1},"speaker":"B","text":"ここにいますよ。私も夕飯に呼んでくれますか？"}];
INSERT INTO sentence [{"id":{"d":905,"n":1,"t":1},"speaker":"A","text":"昨日、飲み過ぎて、スーツのまま玄関で寝ていました。"},{"id":{"d":905,"n":2,"t":1},"speaker":"B","text":"まさか、そのまま出社したのではないでしょうね？"},{"id":{"d":905,"n":3,"t":1},"speaker":"A","text":"一応、着替えましたよ。シャワーも浴びたかったですしね。"},{"id":{"d":905,"n":4,"t":1},"speaker":"B","text":"それならよかった、と言いたいところですが、そのまま来たように見えるほどスーツがよれよれですよ。"},{"id":{"d":905,"n":5,"t":1},"speaker":"A","text":"クリーニングに出す暇がなくて、手入れの方法もよく分からないのです。脱いだら掛けておくだけでした。"},{"id":{"d":905,"n":6,"t":1},"speaker":"B","text":"営業がそのような服装では困ります。新しいスーツを買って、古いものはクリーニングに出してください。"},{"id":{"d":905,"n":7,"t":1},"speaker":"A","text":"クリーニングに出すのも新しいものを買うのもお金がかかり過ぎて大変です。"},{"id":{"d":905,"n":8,"t":1},"speaker":"B","text":"仕方がありませんね。では、自宅でできるスーツの手入れの方法を教えます。"},{"id":{"d":905,"n":9,"t":1},"speaker":"A","text":"それはありがたいです。ぜひとも教えてください。"}];
INSERT INTO sentence [{"id":{"d":906,"n":1,"t":1},"speaker":"A","text":"エコバッグ2つでは買ったものが入り切りませんね。"},{"id":{"d":906,"n":2,"t":1},"speaker":"B","text":"買い過ぎましたね。"},{"id":{"d":906,"n":3,"t":1},"speaker":"A","text":"レジに行って、有料の袋を買ってきてくれますか？"},{"id":{"d":906,"n":4,"t":1},"speaker":"B","text":"大きいのを1つで構いませんか？"},{"id":{"d":906,"n":5,"t":1},"speaker":"A","text":"分けて持ちたいので、中くらいのを2つお願いします。"},{"id":{"d":906,"n":6,"t":1},"speaker":"B","text":"了解です。"},{"id":{"d":906,"n":7,"t":1},"speaker":"A","text":"荷物を入れ終わったら、休憩所でコーヒーを飲んで一休みしてから帰りませんか？"},{"id":{"d":906,"n":8,"t":1},"speaker":"B","text":"賛成です。休日のスーパーマーケットは混んでいて疲れますものね。"}];
INSERT INTO sentence [{"id":{"d":907,"n":1,"t":1},"speaker":"A","text":"今日もパンがありませんか？"},{"id":{"d":907,"n":2,"t":1},"speaker":"B","text":"申し訳ありません。移動スーパーマーケットの巡回の順番が最後の方になると、売れてしまうのですよ。"},{"id":{"d":907,"n":3,"t":1},"speaker":"A","text":"たまにはパンが食べたいです。"},{"id":{"d":907,"n":4,"t":1},"speaker":"B","text":"予約しておいてくれれば、取り置きもできますよ。"},{"id":{"d":907,"n":5,"t":1},"speaker":"A","text":"それなら来週は食パンとあんパンとクリームパンを頼めますか？"},{"id":{"d":907,"n":6,"t":1},"speaker":"B","text":"食パン1斤ですね。あんパンやクリームパンはいろいろな種類がありますが、なんでもよいのですか？"},{"id":{"d":907,"n":7,"t":1},"speaker":"A","text":"小さいのが5つくらい入ったのがありましたよね。あれがいいです。"},{"id":{"d":907,"n":8,"t":1},"speaker":"B","text":"分かりました。あんこは粒あんでよいですか？"},{"id":{"d":907,"n":9,"t":1},"speaker":"A","text":"粒あんでもこしあんでも構いません。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":908,"n":1,"t":1},"speaker":"A","text":"今日のお昼用にパンを買いましょう。トレーは私が持ちますね。"},{"id":{"d":908,"n":2,"t":1},"speaker":"B","text":"僕が持ちたいです。"},{"id":{"d":908,"n":3,"t":1},"speaker":"A","text":"もう少し大きくなったらお願いしますね。パンを落としてしまったら、おいしいパンが台なしになってしまいますから。"},{"id":{"d":908,"n":4,"t":1},"speaker":"B","text":"分かりました。では、欲しいパンをママに言えば取ってくれますか？"},{"id":{"d":908,"n":5,"t":1},"speaker":"A","text":"よいですよ。どれにしますか？"},{"id":{"d":908,"n":6,"t":1},"speaker":"B","text":"これがいいです。"},{"id":{"d":908,"n":7,"t":1},"speaker":"A","text":"中はチョコレートと書いてありますよ。これでよいのですか？"},{"id":{"d":908,"n":8,"t":1},"speaker":"B","text":"はい、このパンがいいです。"}];
INSERT INTO sentence [{"id":{"d":909,"n":1,"t":1},"speaker":"A","text":"部活動で履いているシューズのひもが切れてしまいました。新しいひもは家にありますか？"},{"id":{"d":909,"n":2,"t":1},"speaker":"B","text":"家にはないと思いますよ。買い置きしていた分はこの前のもので最後だったはずです。"},{"id":{"d":909,"n":3,"t":1},"speaker":"A","text":"それは困りました。明日も部活動なので、今日中になんとかしたいのですが。"},{"id":{"d":909,"n":4,"t":1},"speaker":"B","text":"スポーツ用品店はこの時間にはもう閉まっていますよ。"},{"id":{"d":909,"n":5,"t":1},"speaker":"A","text":"そうですね。他に売っていそうな所はあるでしょうか？"},{"id":{"d":909,"n":6,"t":1},"speaker":"B","text":"コンビニエンスストアならまだ開いているはずですが、靴ひもが売られているかどうかは分かりませんね。"},{"id":{"d":909,"n":7,"t":1},"speaker":"A","text":"一応見てきます。もしなければ、家の他の靴のひもを取って付け替えてもよいですか？"},{"id":{"d":909,"n":8,"t":1},"speaker":"B","text":"よいですよ。お父さんのこの靴はもう履かないと言っていましたので取っても大丈夫だと思います。"}];
INSERT INTO sentence [{"id":{"d":910,"n":1,"t":1},"speaker":"A","text":"結城さん、このブラウスを着ませんか？"},{"id":{"d":910,"n":2,"t":1},"speaker":"B","text":"新品ではありませんか？どうされたのですか？"},{"id":{"d":910,"n":3,"t":1},"speaker":"A","text":"インターネット通販で買ったのですが、失敗してしまいまして。"},{"id":{"d":910,"n":4,"t":1},"speaker":"B","text":"サイズが合わなかったのですか？"},{"id":{"d":910,"n":5,"t":1},"speaker":"A","text":"ええ、私には少し小さかったのです。結城さんなら着られそうだと思い持ってきました。"},{"id":{"d":910,"n":6,"t":1},"speaker":"B","text":"返品交換はしてもらえないのですか？"},{"id":{"d":910,"n":7,"t":1},"speaker":"A","text":"できると書いてありましたが、面倒でやめてしまいました。代金は要りませんのでもらってくれませんか？"},{"id":{"d":910,"n":8,"t":1},"speaker":"B","text":"すてきなブラウスなのでうれしいですが、頂くのは申し訳ありません。ブラウス代をお支払いしますよ。"},{"id":{"d":910,"n":9,"t":1},"speaker":"A","text":"いいえ、本当に結構です。着ていただけるだけでありがたいですので、どうか受け取ってください。"}];
INSERT INTO sentence [{"id":{"d":911,"n":1,"t":1},"speaker":"A","text":"今月は家計が苦しいです。"},{"id":{"d":911,"n":2,"t":1},"speaker":"B","text":"何にお金がかかったのですか？"},{"id":{"d":911,"n":3,"t":1},"speaker":"A","text":"月初めにトイレが壊れたではありませんか、あれが主な原因です。"},{"id":{"d":911,"n":4,"t":1},"speaker":"B","text":"そのようなこともありましたね。"},{"id":{"d":911,"n":5,"t":1},"speaker":"A","text":"修理費が5万円もかかっているのです。"},{"id":{"d":911,"n":6,"t":1},"speaker":"B","text":"それほどかかったのですか？"},{"id":{"d":911,"n":7,"t":1},"speaker":"A","text":"はい。食費を抑えて頑張りましたが限界です。"},{"id":{"d":911,"n":8,"t":1},"speaker":"B","text":"では、家計費を追加で下ろしてきます。"}];
INSERT INTO sentence [{"id":{"d":912,"n":1,"t":1},"speaker":"A","text":"今日のおみそ汁は冷たいですね。"},{"id":{"d":912,"n":2,"t":1},"speaker":"B","text":"これは冷や汁というのですよ。"},{"id":{"d":912,"n":3,"t":1},"speaker":"A","text":"お母さんが作ったのは、初めてですよね？"},{"id":{"d":912,"n":4,"t":1},"speaker":"B","text":"はい。今日お向かいの奥さんに教えていただいたので、早速作ってみました。"},{"id":{"d":912,"n":5,"t":1},"speaker":"A","text":"これはその方が考えたメニューなのですか？"},{"id":{"d":912,"n":6,"t":1},"speaker":"B","text":"いえ、彼女のふるさとの味だそうです。冷たくて夏にぴったりのお料理です。"},{"id":{"d":912,"n":7,"t":1},"speaker":"A","text":"初めて食べましたがおいしいです。どこのお料理なのですか？"},{"id":{"d":912,"n":8,"t":1},"speaker":"B","text":"宮崎と聞きました。"}];
INSERT INTO sentence [{"id":{"d":913,"n":1,"t":1},"speaker":"A","text":"この隙間にぴったりの棚が欲しいです。"},{"id":{"d":913,"n":2,"t":1},"speaker":"B","text":"隙間家具でちょうど良い大きさのものがありませんか？"},{"id":{"d":913,"n":3,"t":1},"speaker":"A","text":"家具屋さんやインターネットでも探したのですが、ないのです。"},{"id":{"d":913,"n":4,"t":1},"speaker":"B","text":"では造るしかありませんね。"},{"id":{"d":913,"n":5,"t":1},"speaker":"A","text":"そうですよね。一緒に造ってくれませんか？"},{"id":{"d":913,"n":6,"t":1},"speaker":"B","text":"お安いご用です。週末にやりましょう。"},{"id":{"d":913,"n":7,"t":1},"speaker":"A","text":"分かりました。週末までに隙間のサイズを測っておきますね。"},{"id":{"d":913,"n":8,"t":1},"speaker":"B","text":"お願いします。土曜日にホームセンターで材料を買い、日曜日に造りましょう。"}];
INSERT INTO sentence [{"id":{"d":914,"n":1,"t":1},"speaker":"A","text":"お隣さんから、ウドとタラの芽を頂きました。"},{"id":{"d":914,"n":2,"t":1},"speaker":"B","text":"なんて立派なのでしょう。今日は天ぷらに決まりですね。"},{"id":{"d":914,"n":3,"t":1},"speaker":"A","text":"おそばもありますから、ざるそばと天ぷらにしましょう。"},{"id":{"d":914,"n":4,"t":1},"speaker":"B","text":"うれしいです。ごちそうですね。"},{"id":{"d":914,"n":5,"t":1},"speaker":"A","text":"春の山菜は天ぷらが一番おいしいと思うのです。"},{"id":{"d":914,"n":6,"t":1},"speaker":"B","text":"私もそう思います。抹茶塩でいただくのが大好きです。"},{"id":{"d":914,"n":7,"t":1},"speaker":"A","text":"いいですね。では今日も抹茶塩でいただきましょう。"}];
INSERT INTO sentence [{"id":{"d":915,"n":1,"t":1},"speaker":"A","text":"玄関に飾ってあるのはウメの花ですか？"},{"id":{"d":915,"n":2,"t":1},"speaker":"B","text":"いいえ。あれはモモの花です。"},{"id":{"d":915,"n":3,"t":1},"speaker":"A","text":"モモでしたか。お花屋さんから買ってきたのですか？"},{"id":{"d":915,"n":4,"t":1},"speaker":"B","text":"そうです。ひな祭り用に売っていたので買ってきました。"},{"id":{"d":915,"n":5,"t":1},"speaker":"A","text":"もうそのような時期ですか。気付けばもうすっかり春ですね。"},{"id":{"d":915,"n":6,"t":1},"speaker":"B","text":"季節のお花をめでることで四季の移ろいを感じられるのはいいことです。"},{"id":{"d":915,"n":7,"t":1},"speaker":"A","text":"そうですね。一気に春らしくなりました。"}];
INSERT INTO sentence [{"id":{"d":916,"n":1,"t":1},"speaker":"A","text":"大竹さん、今日はのり弁当と鶏めし弁当が100円引きでしたよ。"},{"id":{"d":916,"n":2,"t":1},"speaker":"B","text":"良いことを聞きました。高橋さんはのり弁当にしたのですね。"},{"id":{"d":916,"n":3,"t":1},"speaker":"A","text":"はい。私はのり弁当に入っているこの白身フライが大好きなのです。"},{"id":{"d":916,"n":4,"t":1},"speaker":"B","text":"私も白身フライは好きです。でも、ちくわの天ぷらはもっと好きです。"},{"id":{"d":916,"n":5,"t":1},"speaker":"A","text":"大竹さんはちくわ派なのですね。"},{"id":{"d":916,"n":6,"t":1},"speaker":"B","text":"ちくわはお嫌いですか？"},{"id":{"d":916,"n":7,"t":1},"speaker":"A","text":"好きですけれど、ここのお弁当屋さんはただの天ぷらでしょう？私は、ちくわは天ぷらよりも磯部揚げが好きです。"},{"id":{"d":916,"n":8,"t":1},"speaker":"B","text":"磯辺揚げもおいしいですね。ちくわと青のりが入った衣の相性はとても良いです。"}];
INSERT INTO sentence [{"id":{"d":917,"n":1,"t":1},"speaker":"A","text":"チャーハンを大盛りでお願いします。"},{"id":{"d":917,"n":2,"t":1},"speaker":"B","text":"はい、チャーハン大盛りですね。"},{"id":{"d":917,"n":3,"t":1},"speaker":"A","text":"シューマイも3個付けてください。"},{"id":{"d":917,"n":4,"t":1},"speaker":"B","text":"グリーンピースが入っていても大丈夫ですか？"},{"id":{"d":917,"n":5,"t":1},"speaker":"A","text":"はい。大丈夫ですよ。"},{"id":{"d":917,"n":6,"t":1},"speaker":"B","text":"たまにグリーンピースを抜いてほしいというお客さんもいるのですよ。"},{"id":{"d":917,"n":7,"t":1},"speaker":"A","text":"そうなのですか。私は大好きですよ。"},{"id":{"d":917,"n":8,"t":1},"speaker":"B","text":"好みがありますからね。最近はグリーンピース抜きのも作るようにしています。"}];
INSERT INTO sentence [{"id":{"d":918,"n":1,"t":1},"speaker":"A","text":"普段着とは、どういう服だと思いますか？"},{"id":{"d":918,"n":2,"t":1},"speaker":"B","text":"今、着ているような服ではないのですか？"},{"id":{"d":918,"n":3,"t":1},"speaker":"A","text":"Tシャツにジーパンですよ。これでいいのでしょうか？"},{"id":{"d":918,"n":4,"t":1},"speaker":"B","text":"なぜ普段着のことを聞くのですか？"},{"id":{"d":918,"n":5,"t":1},"speaker":"A","text":"面接の案内に「普段着でお越しください」とあったからです。"},{"id":{"d":918,"n":6,"t":1},"speaker":"B","text":"面接の普段着ということでしたら、今着ている服では駄目ですね。"},{"id":{"d":918,"n":7,"t":1},"speaker":"A","text":"そうですよね。襟付きのシャツなどを着ていくべきでしょうか？"},{"id":{"d":918,"n":8,"t":1},"speaker":"B","text":"それなら良さそうです。スーツである必要はありませんが、少しフォーマルな私服を選ぶといいと思います。"},{"id":{"d":918,"n":9,"t":1},"speaker":"A","text":"なるほど。それなら分かりやすいです。"}];
INSERT INTO sentence [{"id":{"d":919,"n":1,"t":1},"speaker":"A","text":"この間は、面接に着ていく服についてのアドバイスをありがとうございました。"},{"id":{"d":919,"n":2,"t":1},"speaker":"B","text":"どういたしまして。面接はうまくいきましたか？"},{"id":{"d":919,"n":3,"t":1},"speaker":"A","text":"手応えはあったと思います。"},{"id":{"d":919,"n":4,"t":1},"speaker":"B","text":"服装は、シャツとタイトスカートで正解だったでしょう？"},{"id":{"d":919,"n":5,"t":1},"speaker":"A","text":"他の受験者もそのような感じの服でした。浮かなくて良かったです。"},{"id":{"d":919,"n":6,"t":1},"speaker":"B","text":"最初はTシャツとジーパンで行こうとしていましたものね。"},{"id":{"d":919,"n":7,"t":1},"speaker":"A","text":"お恥ずかしい限りです。相談して良かったです。"},{"id":{"d":919,"n":8,"t":1},"speaker":"B","text":"面接に合格できるといいですね。"}];
INSERT INTO sentence [{"id":{"d":920,"n":1,"t":1},"speaker":"A","text":"久しぶりの食事会なので、張り切ってたくさんのビールを買ってきました。"},{"id":{"d":920,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。待っていましたよ。"},{"id":{"d":920,"n":3,"t":1},"speaker":"A","text":"今日は家族全員そろうのですか？"},{"id":{"d":920,"n":4,"t":1},"speaker":"B","text":"はい。もう全員そろっていますよ。息子たちも今日の食事会を楽しみにしていました。"},{"id":{"d":920,"n":5,"t":1},"speaker":"A","text":"博人も理久もお酒が飲める年齢になりましたからね。つまみは何ですか？"},{"id":{"d":920,"n":6,"t":1},"speaker":"B","text":"メインは焼き鳥です。博人が焼き鳥を焼く機械を忘年会のビンゴで当てたのですよ。"},{"id":{"d":920,"n":7,"t":1},"speaker":"A","text":"それはうれしいですね。味付けは塩ですか？タレですか？"},{"id":{"d":920,"n":8,"t":1},"speaker":"B","text":"両方用意しました。他のおつまみもたっぷり用意しましたよ。"}];
INSERT INTO sentence [{"id":{"d":921,"n":1,"t":1},"speaker":"A","text":"避難袋に現金を入れておいてくださいね。"},{"id":{"d":921,"n":2,"t":1},"speaker":"B","text":"いくらくらい入れておけばよいのですか？"},{"id":{"d":921,"n":3,"t":1},"speaker":"A","text":"10円玉以上の小銭とお札数枚くらいですかね。"},{"id":{"d":921,"n":4,"t":1},"speaker":"B","text":"最近、キャッシュレス決済が多いですが、現金は必要ですか？"},{"id":{"d":921,"n":5,"t":1},"speaker":"A","text":"避難袋を使う災害時だからこそ、現金なのですよ。停電になったらキャッシュレス決済が使えますか？"},{"id":{"d":921,"n":6,"t":1},"speaker":"B","text":"確かに、停電してしまったら使えませんね。うっかりしていました。"},{"id":{"d":921,"n":7,"t":1},"speaker":"A","text":"10円玉は公衆電話をかけるためです。1円と5円は使い道がなさそうなので入れません。"},{"id":{"d":921,"n":8,"t":1},"speaker":"B","text":"分かりました。現金をかき集めて入れておきます。"}];
INSERT INTO sentence [{"id":{"d":922,"n":1,"t":1},"speaker":"A","text":"今日のお昼ご飯は小田巻蒸しですか？豪華ですね。"},{"id":{"d":922,"n":2,"t":1},"speaker":"B","text":"そう思いますか？これを豪華と言わせた私は大したものです。"},{"id":{"d":922,"n":3,"t":1},"speaker":"A","text":"どういうことですか？"},{"id":{"d":922,"n":4,"t":1},"speaker":"B","text":"これはカップうどん1つを2つに分けて、卵を入れてレンジでチンしただけなのです。"},{"id":{"d":922,"n":5,"t":1},"speaker":"A","text":"驚きました。元はカップうどんなのですか？"},{"id":{"d":922,"n":6,"t":1},"speaker":"B","text":"この花形のかまぼこもどきに見覚えがあるでしょう？"},{"id":{"d":922,"n":7,"t":1},"speaker":"A","text":"本当ですね。これはいつものカップうどんのかまぼこです。"},{"id":{"d":922,"n":8,"t":1},"speaker":"B","text":"おつゆがおいしいから、小田巻蒸しに利用してもおいしいですよ。"}];
INSERT INTO sentence [{"id":{"d":923,"n":1,"t":1},"speaker":"A","text":"ガレージにツバメが巣を作り始めていました。"},{"id":{"d":923,"n":2,"t":1},"speaker":"B","text":"ツバメが巣を作るなんて初めてですね。"},{"id":{"d":923,"n":3,"t":1},"speaker":"A","text":"巣の下にある棚が汚れないように、カバーをした方がいいと思います。"},{"id":{"d":923,"n":4,"t":1},"speaker":"B","text":"そうですね。今日の帰りに100円ショップで見繕ってきますよ。"},{"id":{"d":923,"n":5,"t":1},"speaker":"A","text":"田舎の家には毎年ツバメが来てくれましたが、ここには来たことがなかったのでツバメがいないと思っていました。"},{"id":{"d":923,"n":6,"t":1},"speaker":"B","text":"いないことはないと思いますよ。商店街のコンビニエンスストアの看板に巣を作っていたりしますから。"},{"id":{"d":923,"n":7,"t":1},"speaker":"A","text":"そうですか。私は気が付きませんでした。"},{"id":{"d":923,"n":8,"t":1},"speaker":"B","text":"今年はわが家にも幸運が来たようです。うれしいですね。"}];
INSERT INTO sentence [{"id":{"d":924,"n":1,"t":1},"speaker":"A","text":"健康のために豆乳を飲むようにしていましたが、少し飽きてきてしまいました。"},{"id":{"d":924,"n":2,"t":1},"speaker":"B","text":"豆乳はそのまま飲まれているのですか？"},{"id":{"d":924,"n":3,"t":1},"speaker":"A","text":"はい、冷やしてそのままです。"},{"id":{"d":924,"n":4,"t":1},"speaker":"B","text":"ヨーグルト味や紅茶風味といった甘くなっている豆乳ですか？"},{"id":{"d":924,"n":5,"t":1},"speaker":"A","text":"いいえ。普通の無調整の豆乳です。"},{"id":{"d":924,"n":6,"t":1},"speaker":"B","text":"それでしたら、スープにするなどしてお料理に使えばよいのですよ。"},{"id":{"d":924,"n":7,"t":1},"speaker":"A","text":"なるほど、それなら飽きずに飲めそうです。一つ気になるのは、料理をしても栄養は変わらないのかというところです。"},{"id":{"d":924,"n":8,"t":1},"speaker":"B","text":"大丈夫だと思いますよ。私のお薦めは豆乳鍋や豆乳グラタンです。"},{"id":{"d":924,"n":9,"t":1},"speaker":"A","text":"おいしそうですね。今度試してみます。"}];
INSERT INTO sentence [{"id":{"d":925,"n":1,"t":1},"speaker":"A","text":"ところてんを作ったのですが、食べますか？"},{"id":{"d":925,"n":2,"t":1},"speaker":"B","text":"いただきます。ところで、もう味を付けてしまいましたか？"},{"id":{"d":925,"n":3,"t":1},"speaker":"A","text":"まだですけれど、どうしてですか？"},{"id":{"d":925,"n":4,"t":1},"speaker":"B","text":"それはよかったです。関東の人は酸っぱいたれで食べますよね。私はそれが苦手なのです。"},{"id":{"d":925,"n":5,"t":1},"speaker":"A","text":"関西の人は黒蜜で食べるということでしたね。でも、家には黒蜜がありません。"},{"id":{"d":925,"n":6,"t":1},"speaker":"B","text":"すぐそこにコンビニエンスストアがありましたから、買ってきます。"},{"id":{"d":925,"n":7,"t":1},"speaker":"A","text":"私が行きますよ。"},{"id":{"d":925,"n":8,"t":1},"speaker":"B","text":"いいえ、私のわがままですから私が行きます。他に買ってくるものはありますか？"},{"id":{"d":925,"n":9,"t":1},"speaker":"A","text":"黒蜜だけで大丈夫です。よろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":926,"n":1,"t":1},"speaker":"A","text":"フライドポテトを買ってきたのですが、冷めてしまいましたね。"},{"id":{"d":926,"n":2,"t":1},"speaker":"B","text":"本当ですね。見事にふにゃふにゃです。"},{"id":{"d":926,"n":3,"t":1},"speaker":"A","text":"フライドポテトは、揚げたてをお店で食べた方がいいですね。"},{"id":{"d":926,"n":4,"t":1},"speaker":"B","text":"大丈夫です。復活させます。"},{"id":{"d":926,"n":5,"t":1},"speaker":"A","text":"揚げ直すのですか？"},{"id":{"d":926,"n":6,"t":1},"speaker":"B","text":"いいえ。オーブントースターで焼くのです。"},{"id":{"d":926,"n":7,"t":1},"speaker":"A","text":"焼いたら復活しますか？"},{"id":{"d":926,"n":8,"t":1},"speaker":"B","text":"焦げないように見ながら温めれば、ほら、カリカリポテトの復活です。"}];
INSERT INTO sentence [{"id":{"d":927,"n":1,"t":1},"speaker":"A","text":"裕ちゃん、お風呂上がりにいつまでも裸でいないで、服を着なさい。"},{"id":{"d":927,"n":2,"t":1},"speaker":"B","text":"まだ暑いので服を着たくありません。"},{"id":{"d":927,"n":3,"t":1},"speaker":"A","text":"髪もきちんと拭いてください。床がぬれてしまっています。"},{"id":{"d":927,"n":4,"t":1},"speaker":"B","text":"後で床にモップをかけます。"},{"id":{"d":927,"n":5,"t":1},"speaker":"A","text":"髪が先ですよ。拭いてあげますから、ここに来てください。"},{"id":{"d":927,"n":6,"t":1},"speaker":"B","text":"きつく拭くと痛いです。"},{"id":{"d":927,"n":7,"t":1},"speaker":"A","text":"このまま、パジャマも着ましょうね。"},{"id":{"d":927,"n":8,"t":1},"speaker":"B","text":"まだお風呂上りで暑いのに、ママは意地悪です。"}];
INSERT INTO sentence [{"id":{"d":928,"n":1,"t":1},"speaker":"A","text":"メロンパン専門店がこの近くにあるのですが、食べに行きませんか？"},{"id":{"d":928,"n":2,"t":1},"speaker":"B","text":"いいですね。メロンパンは大好きです。どこにあるのですか？"},{"id":{"d":928,"n":3,"t":1},"speaker":"A","text":"大通り沿いです。アイスが挟んであるメロンパンが有名なのですが、これを半分ずつにしませんか？"},{"id":{"d":928,"n":4,"t":1},"speaker":"B","text":"おいしそうですね。私は1個食べられると思いますけど、半分ずつがいいのですか？"},{"id":{"d":928,"n":5,"t":1},"speaker":"A","text":"メロンパンだけでもカロリーが高いのに、その上、アイスというのはなかなか勇気が要ります。"},{"id":{"d":928,"n":6,"t":1},"speaker":"B","text":"それで半分なのですね。"},{"id":{"d":928,"n":7,"t":1},"speaker":"A","text":"はい。どうでしょうか？"},{"id":{"d":928,"n":8,"t":1},"speaker":"B","text":"分かりました。では、アイスサンドを1つ買いましょう。"}];
INSERT INTO sentence [{"id":{"d":929,"n":1,"t":1},"speaker":"A","text":"久しぶりに銀行に行ったら、暗証番号を忘れていてお金を下ろすのが大変でした。"},{"id":{"d":929,"n":2,"t":1},"speaker":"B","text":"それほど銀行に行っていなかったのですか？"},{"id":{"d":929,"n":3,"t":1},"speaker":"A","text":"最近はクレジット決済ばかりで、現金を下ろすことがないのですよ。"},{"id":{"d":929,"n":4,"t":1},"speaker":"B","text":"なるほど。それで暗証番号は思い出したのですか？"},{"id":{"d":929,"n":5,"t":1},"speaker":"A","text":"思い出せなかったので、係の人に暗証番号の照会方法を聞いて何とか下ろすことができました。"},{"id":{"d":929,"n":6,"t":1},"speaker":"B","text":"手続きが大変そうですね。私は忘れないように気を付けます。"},{"id":{"d":929,"n":7,"t":1},"speaker":"A","text":"とても大変でした。時間も非常にかかりました。"}];
INSERT INTO sentence [{"id":{"d":930,"n":1,"t":1},"speaker":"A","text":"このバケツは変わっていますね。"},{"id":{"d":930,"n":2,"t":1},"speaker":"B","text":"それはバケツではなく、洗濯機ですよ。"},{"id":{"d":930,"n":3,"t":1},"speaker":"A","text":"これほど小さいのに洗濯機なのですか？"},{"id":{"d":930,"n":4,"t":1},"speaker":"B","text":"1人分の洗濯を別にしたいときに役立つのです。"},{"id":{"d":930,"n":5,"t":1},"speaker":"A","text":"洗濯を別にしたい人がいるのですか？"},{"id":{"d":930,"n":6,"t":1},"speaker":"B","text":"息子のユニフォームをこれで洗っています。毎日、部活で泥だらけになって帰ってくるので、これで下洗いをするのです。"},{"id":{"d":930,"n":7,"t":1},"speaker":"A","text":"確かに泥だらけのものと自分のランジェリーは一緒に洗いたくありませんね。"},{"id":{"d":930,"n":8,"t":1},"speaker":"B","text":"はい。この洗濯機があって助かっています。"}];
INSERT INTO sentence [{"id":{"d":931,"n":1,"t":1},"speaker":"A","text":"ラーメンとご飯大盛り、それとギョーザを2人前お願いします。"},{"id":{"d":931,"n":2,"t":1},"speaker":"B","text":"はい、分かりました。"},{"id":{"d":931,"n":3,"t":1},"speaker":"A","text":"ここのギョーザがおいしくてご飯大盛りでもいけますね。"},{"id":{"d":931,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。ギョーザ、お待たせしました。ご飯とラーメンもすぐ出ます。"},{"id":{"d":931,"n":5,"t":1},"speaker":"A","text":"もうじき閉店するといううわさを聞きましたが、本当ですか？"},{"id":{"d":931,"n":6,"t":1},"speaker":"B","text":"残念ですが本当です。年内で閉店させていただきます。"},{"id":{"d":931,"n":7,"t":1},"speaker":"A","text":"この味を味わえるのもあと少しですね。"},{"id":{"d":931,"n":8,"t":1},"speaker":"B","text":"来年から息子がカレー屋にするので、よかったら食べに来てやってください。"}];
INSERT INTO sentence [{"id":{"d":932,"n":1,"t":1},"speaker":"A","text":"このアパートは1階がコンビニエンスストアなのですね。"},{"id":{"d":932,"n":2,"t":1},"speaker":"B","text":"便利ですよ。夜中におなかがすいたときに重宝します。ここにして良かったと思います。"},{"id":{"d":932,"n":3,"t":1},"speaker":"A","text":"騒音などはありませんか？"},{"id":{"d":932,"n":4,"t":1},"speaker":"B","text":"私は4階ですし、音は気になりませんね。"},{"id":{"d":932,"n":5,"t":1},"speaker":"A","text":"ここは、駅からも近くて、学校からも近くて、うらやましいです。"},{"id":{"d":932,"n":6,"t":1},"speaker":"B","text":"引っ越してきますか？"},{"id":{"d":932,"n":7,"t":1},"speaker":"A","text":"空き部屋があるのですか？"},{"id":{"d":932,"n":8,"t":1},"speaker":"B","text":"昨日、隣の部屋の人が引っ越していきましたので、部屋が空いたはずです。"}];
INSERT INTO sentence [{"id":{"d":933,"n":1,"t":1},"speaker":"A","text":"おはようございます。ご家族でご旅行ですか？"},{"id":{"d":933,"n":2,"t":1},"speaker":"B","text":"いいえ。キッチンとリビングのリフォーム中で家にいられませんので、こちらに泊まらせていただいております。"},{"id":{"d":933,"n":3,"t":1},"speaker":"A","text":"それで1週間の連泊なのですね。"},{"id":{"d":933,"n":4,"t":1},"speaker":"B","text":"怪しい家族に見えてしまいましたか？"},{"id":{"d":933,"n":5,"t":1},"speaker":"A","text":"いえ、そのようなことはありませんよ。今日もお出かけですか？"},{"id":{"d":933,"n":6,"t":1},"speaker":"B","text":"毎日、家に様子を見に行っているのです。工事は順調に進んでいるようで安心しています。"},{"id":{"d":933,"n":7,"t":1},"speaker":"A","text":"それは何よりです。完成が楽しみですね。"},{"id":{"d":933,"n":8,"t":1},"speaker":"B","text":"はい。新しいキッチンで料理を作るのが待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":934,"n":1,"t":1},"speaker":"A","text":"今日のブラウスは袖が大きいですね。"},{"id":{"d":934,"n":2,"t":1},"speaker":"B","text":"最近の流行なのですよ。袖コンシャスと言います。"},{"id":{"d":934,"n":3,"t":1},"speaker":"A","text":"そういう名前ですか。とてもすてきです。"},{"id":{"d":934,"n":4,"t":1},"speaker":"B","text":"ギャザーをたっぷり取ってあってかわいいでしょう？"},{"id":{"d":934,"n":5,"t":1},"speaker":"A","text":"はい。袖にボリュームがあるおかげで全体はほっそりして見えるのですね。"},{"id":{"d":934,"n":6,"t":1},"speaker":"B","text":"そうなのです。そこが一押しのポイントです。"},{"id":{"d":934,"n":7,"t":1},"speaker":"A","text":"今はスカートですけれど、パンツにも合いそうです。"},{"id":{"d":934,"n":8,"t":1},"speaker":"B","text":"その通りです。パンツにもよく合わせていますよ。"}];
INSERT INTO sentence [{"id":{"d":935,"n":1,"t":1},"speaker":"A","text":"おいしそうな豚肉があったので買ってきました。"},{"id":{"d":935,"n":2,"t":1},"speaker":"B","text":"大きな三枚肉ですね。"},{"id":{"d":935,"n":3,"t":1},"speaker":"A","text":"これで何を作りましょうか？"},{"id":{"d":935,"n":4,"t":1},"speaker":"B","text":"三枚肉といえば角煮でしょう？"},{"id":{"d":935,"n":5,"t":1},"speaker":"A","text":"角煮は時間がかかりますよ。"},{"id":{"d":935,"n":6,"t":1},"speaker":"B","text":"まだ5時です。夕飯までにはまだ時間がありますし、少しくらい夕飯が遅くなっても待てます。"},{"id":{"d":935,"n":7,"t":1},"speaker":"A","text":"分かりました。ではすぐ作り始めます。"},{"id":{"d":935,"n":8,"t":1},"speaker":"B","text":"何か手伝えることはありますか？"},{"id":{"d":935,"n":9,"t":1},"speaker":"A","text":"では、角煮に添えるゆで卵を作っておいてください。"},{"id":{"d":935,"n":10,"t":1},"speaker":"B","text":"了解です。"}];
INSERT INTO sentence [{"id":{"d":936,"n":1,"t":1},"speaker":"A","text":"ダウンジャケットが見つかりません。どこにあるか知りませんか？"},{"id":{"d":936,"n":2,"t":1},"speaker":"B","text":"去年、買った高いジャケットですか？"},{"id":{"d":936,"n":3,"t":1},"speaker":"A","text":"そうです。今年も着ようと思っていたのにクローゼットに見つかりません。"},{"id":{"d":936,"n":4,"t":1},"speaker":"B","text":"クリーニングに出したまま忘れているということはありませんか？"},{"id":{"d":936,"n":5,"t":1},"speaker":"A","text":"確か、取ってきましたよ。"},{"id":{"d":936,"n":6,"t":1},"speaker":"B","text":"では圧縮袋に入れていませんでしたか？"},{"id":{"d":936,"n":7,"t":1},"speaker":"A","text":"そういえば、場所を取るからと言って、圧縮袋に入れた気がします。"},{"id":{"d":936,"n":8,"t":1},"speaker":"B","text":"それならば、衣装ケースの中ですよ。きっと。"},{"id":{"d":936,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。衣装ケースの中を探してみます。"}];
INSERT INTO sentence [{"id":{"d":937,"n":1,"t":1},"speaker":"A","text":"カステラにアイスを挟んで、冷凍室に入れておきました。"},{"id":{"d":937,"n":2,"t":1},"speaker":"B","text":"今日のおやつはそれですか？"},{"id":{"d":937,"n":3,"t":1},"speaker":"A","text":"そうですよ。豪華でしょう？"},{"id":{"d":937,"n":4,"t":1},"speaker":"B","text":"確かに豪華ですが、それはみんなが飽きて食べなくなった叔父さんからのカステラのリメークですね？"},{"id":{"d":937,"n":5,"t":1},"speaker":"A","text":"ご名答です。アイスを挟めば家族みんなが食べると思いました。"},{"id":{"d":937,"n":6,"t":1},"speaker":"B","text":"でも、これは確かにおいしそうです。魅力的に変身しましたね。"},{"id":{"d":937,"n":7,"t":1},"speaker":"A","text":"それはよかったです。カステラもとてもおいしかったのですが、いかんせん多過ぎました。"},{"id":{"d":937,"n":8,"t":1},"speaker":"B","text":"そうですね。3人家族にカステラ4本は多過ぎました。"}];
INSERT INTO sentence [{"id":{"d":938,"n":1,"t":1},"speaker":"A","text":"駅前のスーパーマーケットの今日の広告を見ましたか？"},{"id":{"d":938,"n":2,"t":1},"speaker":"B","text":"まだ見ていません。何か特売品がありましたか？"},{"id":{"d":938,"n":3,"t":1},"speaker":"A","text":"今日から98円フェアをやっているらしいですよ。"},{"id":{"d":938,"n":4,"t":1},"speaker":"B","text":"何が98円になっているのですか？"},{"id":{"d":938,"n":5,"t":1},"speaker":"A","text":"お砂糖やおしょうゆ、それに野菜もです。お肉も100グラム98円になっていました。"},{"id":{"d":938,"n":6,"t":1},"speaker":"B","text":"それは驚きですね。"},{"id":{"d":938,"n":7,"t":1},"speaker":"A","text":"今から車を出してスーパーマーケットへ行くのですが、一緒に行きますか？"},{"id":{"d":938,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。ぜひお願いします。"}];
INSERT INTO sentence [{"id":{"d":939,"n":1,"t":1},"speaker":"A","text":"朝起きたらパジャマが破れていました。"},{"id":{"d":939,"n":2,"t":1},"speaker":"B","text":"驚きました。お母さん、どういう寝相だったのですか？"},{"id":{"d":939,"n":3,"t":1},"speaker":"A","text":"普通に寝ていたはずです。パジャマがもう20年も着ているものなので、生地が薄くなっていたのかもしれません。"},{"id":{"d":939,"n":4,"t":1},"speaker":"B","text":"それほど着ていたのですか？気が付きませんでした。"},{"id":{"d":939,"n":5,"t":1},"speaker":"A","text":"よれよれの生地でしたけれど、それが着心地が良かったのですよ。"},{"id":{"d":939,"n":6,"t":1},"speaker":"B","text":"そうですか。でも新しいパジャマを買わなければなりませんね。"},{"id":{"d":939,"n":7,"t":1},"speaker":"A","text":"そうします。"}];
INSERT INTO sentence [{"id":{"d":940,"n":1,"t":1},"speaker":"A","text":"今日はひなたが1人でお着替えできたのです。"},{"id":{"d":940,"n":2,"t":1},"speaker":"B","text":"素晴らしいではありませんか。"},{"id":{"d":940,"n":3,"t":1},"speaker":"A","text":"スカートがウエストゴムでどちらを向いてもよいものだったのと、着やすいトレーナーだったのがよかったようです。"},{"id":{"d":940,"n":4,"t":1},"speaker":"B","text":"一つ一つ学んでいますね。うちの子は。"},{"id":{"d":940,"n":5,"t":1},"speaker":"A","text":"下着のパンツが前後ろだったのが少しだけ残念でしたけれど。"},{"id":{"d":940,"n":6,"t":1},"speaker":"B","text":"見えないところは構いませんよ。"},{"id":{"d":940,"n":7,"t":1},"speaker":"A","text":"でも、パンツを前後ろにはいてみてください。とてもはき心地が悪いですよ。"},{"id":{"d":940,"n":8,"t":1},"speaker":"B","text":"想像してみれば確かにそうですね。はき心地が悪いと感じたら、すぐに覚えますよ。"}];
INSERT INTO sentence [{"id":{"d":941,"n":1,"t":1},"speaker":"A","text":"何の話で盛り上がっていたのですか？"},{"id":{"d":941,"n":2,"t":1},"speaker":"B","text":"シチューをご飯にかけるか、それとも、かけないかです。"},{"id":{"d":941,"n":3,"t":1},"speaker":"A","text":"シチューはご飯にかけるのではないのですか？"},{"id":{"d":941,"n":4,"t":1},"speaker":"B","text":"ご飯にかける派の人がここにもいましたか。"},{"id":{"d":941,"n":5,"t":1},"speaker":"A","text":"普通はかけないのですか？"},{"id":{"d":941,"n":6,"t":1},"speaker":"B","text":"私はかけません。"},{"id":{"d":941,"n":7,"t":1},"speaker":"A","text":"そうですか。かけるのが当たり前だと思っていました。"}];
INSERT INTO sentence [{"id":{"d":942,"n":1,"t":1},"speaker":"A","text":"ここのラーメンはスープがおいし過ぎて、持って帰りたいくらいです。"},{"id":{"d":942,"n":2,"t":1},"speaker":"B","text":"お褒めいただきありがとうございます。"},{"id":{"d":942,"n":3,"t":1},"speaker":"A","text":"テイクアウトはしていませんか？"},{"id":{"d":942,"n":4,"t":1},"speaker":"B","text":"ご期待に添えず申し訳ありませんが、スープだけのテイクアウトは行っておりません。"},{"id":{"d":942,"n":5,"t":1},"speaker":"A","text":"麺も一緒ならよいのですか？"},{"id":{"d":942,"n":6,"t":1},"speaker":"B","text":"具材も入った冷凍ラーメンでしたら、表の自動販売機でお買い求めいただけます。"},{"id":{"d":942,"n":7,"t":1},"speaker":"A","text":"そのようなものがありましたか？"},{"id":{"d":942,"n":8,"t":1},"speaker":"B","text":"はい。柱の陰になっているので気が付きにくいですが、ございますよ。そこの扉を出て左にございます。"}];
INSERT INTO sentence [{"id":{"d":943,"n":1,"t":1},"speaker":"A","text":"おばあちゃんから、いぶりがっこが送られてきました。"},{"id":{"d":943,"n":2,"t":1},"speaker":"B","text":"いぶりがっこはあまり好きではありません。"},{"id":{"d":943,"n":3,"t":1},"speaker":"A","text":"漬物として食べるから、好きではないというのですね。"},{"id":{"d":943,"n":4,"t":1},"speaker":"B","text":"でも、いぶりがっこは漬物でしょう？"},{"id":{"d":943,"n":5,"t":1},"speaker":"A","text":"最近は、いぶりがっこをクリームチーズと合わせて洋風のおつまみにして食べたりするのですよ。"},{"id":{"d":943,"n":6,"t":1},"speaker":"B","text":"クリームチーズと合わせるのですか？ちょっと食べてみたくなりました。"},{"id":{"d":943,"n":7,"t":1},"speaker":"A","text":"では早速今晩作りましょう。お酒の用意をお願いします。"},{"id":{"d":943,"n":8,"t":1},"speaker":"B","text":"分かりました。お酒はワインでよいですか？それともウイスキーでしょうか？"},{"id":{"d":943,"n":9,"t":1},"speaker":"A","text":"どちらもよく合いますよ。あなたのお好きな方にしましょう。"},{"id":{"d":943,"n":10,"t":1},"speaker":"B","text":"それでは、この前買ったばかりの赤ワインにしますね。"}];
INSERT INTO sentence [{"id":{"d":944,"n":1,"t":1},"speaker":"A","text":"前から行っているスーパーマーケットのレジが変わって困りました。"},{"id":{"d":944,"n":2,"t":1},"speaker":"B","text":"私も先日行きました。セルフレジになったのですよね。"},{"id":{"d":944,"n":3,"t":1},"speaker":"A","text":"あれは私には難し過ぎます。次からは店員さんがいるレジに並ぼうと思います。"},{"id":{"d":944,"n":4,"t":1},"speaker":"B","text":"そうでしたか。慣れるまでは少し大変に感じるかもしれませんね。"},{"id":{"d":944,"n":5,"t":1},"speaker":"A","text":"慣れれば簡単にできるようになるのでしょうか？"},{"id":{"d":944,"n":6,"t":1},"speaker":"B","text":"私はスーパーマーケットのレジ打ちのアルバイトをしたことがありましたので、セルフレジはとても便利に感じました。"},{"id":{"d":944,"n":7,"t":1},"speaker":"A","text":"そうですか。では私ももう何回かセルフレジに挑戦してみようと思います。"}];
INSERT INTO sentence [{"id":{"d":945,"n":1,"t":1},"speaker":"A","text":"免許更新のお知らせが来ていますよ。"},{"id":{"d":945,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。もうそのような時期なのですね。"},{"id":{"d":945,"n":3,"t":1},"speaker":"A","text":"今回の更新も免許試験場に行きますか？"},{"id":{"d":945,"n":4,"t":1},"speaker":"B","text":"前回は試験場に行きましたが遠くてうんざりしましたので、今回は警察署で更新しようと思います。"},{"id":{"d":945,"n":5,"t":1},"speaker":"A","text":"警察署なら近いですものね。"},{"id":{"d":945,"n":6,"t":1},"speaker":"B","text":"はい。ただし、行ったその日に免許証を頂けないのは難点です。"},{"id":{"d":945,"n":7,"t":1},"speaker":"A","text":"後日取りに行かなければなりませんでしたね。"},{"id":{"d":945,"n":8,"t":1},"speaker":"B","text":"そうです。"},{"id":{"d":945,"n":9,"t":1},"speaker":"A","text":"郵送してはもらえませんか？"},{"id":{"d":945,"n":10,"t":1},"speaker":"B","text":"郵送もしてもらえますが、手数料がかかります。もう一度取りに行くのとどちらがいいか、考えてしまいます。"}];
INSERT INTO sentence [{"id":{"d":946,"n":1,"t":1},"speaker":"A","text":"CMで見た肉うどんが食べたいです。"},{"id":{"d":946,"n":2,"t":1},"speaker":"B","text":"最近よく流れる肉うどん専門店のCMですか？"},{"id":{"d":946,"n":3,"t":1},"speaker":"A","text":"はい。とてもおいしそうで、一度食べてみたいのです。"},{"id":{"d":946,"n":4,"t":1},"speaker":"B","text":"私も気になっているのですが、家の近くにはお店がないようなのです。"},{"id":{"d":946,"n":5,"t":1},"speaker":"A","text":"そうですか。それは残念です。"},{"id":{"d":946,"n":6,"t":1},"speaker":"B","text":"そういえば、土曜日におじいちゃんのおうちに行く予定でしたよね。そのときに行きましょう。"},{"id":{"d":946,"n":7,"t":1},"speaker":"A","text":"おじいちゃんのおうちの近くにはあるのですか？"},{"id":{"d":946,"n":8,"t":1},"speaker":"B","text":"確かあったと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":947,"n":1,"t":1},"speaker":"A","text":"大家さん、すみません、お家賃の支払いを週明けまで待っていただけますか？"},{"id":{"d":947,"n":2,"t":1},"speaker":"B","text":"よいですよ。土日は手数料がかかりますからね。"},{"id":{"d":947,"n":3,"t":1},"speaker":"A","text":"そのうち振り込みにしたいと思いつつ、いつまでも手渡しですみません。"},{"id":{"d":947,"n":4,"t":1},"speaker":"B","text":"こうやって顔を見ながらお家賃を受け取るのも、大家としてはうれしいのですよ。"},{"id":{"d":947,"n":5,"t":1},"speaker":"A","text":"そう言っていただけると、こちらもうれしいです。"},{"id":{"d":947,"n":6,"t":1},"speaker":"B","text":"そういえば、親戚から送られてきたリンゴがあります。よければ持っていってください。"},{"id":{"d":947,"n":7,"t":1},"speaker":"A","text":"これほどたくさん頂いてよいのですか？いつもありがとうございます。"},{"id":{"d":947,"n":8,"t":1},"speaker":"B","text":"どういたしまして。少し酸っぱかったので、焼きリンゴにするといいですよ。"}];
INSERT INTO sentence [{"id":{"d":948,"n":1,"t":1},"speaker":"A","text":"井田さんもハンバーガーセットでよいのですね。"},{"id":{"d":948,"n":2,"t":1},"speaker":"B","text":"はい、それでお願いします。"},{"id":{"d":948,"n":3,"t":1},"speaker":"A","text":"サイドメニューは何にしますか？"},{"id":{"d":948,"n":4,"t":1},"speaker":"B","text":"フライドポテトでお願いします。"},{"id":{"d":948,"n":5,"t":1},"speaker":"A","text":"飲み物はどうしますか？"},{"id":{"d":948,"n":6,"t":1},"speaker":"B","text":"コーラゼロにしてください。"},{"id":{"d":948,"n":7,"t":1},"speaker":"A","text":"了解しました。私はサイドをナゲットにしますが、フライドポテトとシェアしませんか？"},{"id":{"d":948,"n":8,"t":1},"speaker":"B","text":"いいですね。そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":949,"n":1,"t":1},"speaker":"A","text":"新しく卵焼き用のフライパンを買いました。"},{"id":{"d":949,"n":2,"t":1},"speaker":"B","text":"だし巻き卵がきれいに作れるフライパンですね？"},{"id":{"d":949,"n":3,"t":1},"speaker":"A","text":"上手な人はきれいに作れるフライパンです。"},{"id":{"d":949,"n":4,"t":1},"speaker":"B","text":"そういうことを言うということは、うまく作れなかったのですか？"},{"id":{"d":949,"n":5,"t":1},"speaker":"A","text":"ご明察です。和食の本を見て、だし汁もきちんと量って作ったのに、スクランブルエッグのようになってしまいました。"},{"id":{"d":949,"n":6,"t":1},"speaker":"B","text":"お料理は練習すればうまくなりますよ。"},{"id":{"d":949,"n":7,"t":1},"speaker":"A","text":"そうでしょうか？高い買い物でしたのに、ショックが大き過ぎました。"},{"id":{"d":949,"n":8,"t":1},"speaker":"B","text":"頑張って練習しましょう。"}];
INSERT INTO sentence [{"id":{"d":950,"n":1,"t":1},"speaker":"A","text":"真っ白な紙を前にして何を悩んでいるのですか？"},{"id":{"d":950,"n":2,"t":1},"speaker":"B","text":"今度の新入社員歓迎会で乾杯のあいさつを頼まれたのです。"},{"id":{"d":950,"n":3,"t":1},"speaker":"A","text":"そうですか。その原稿を作っているのですね。"},{"id":{"d":950,"n":4,"t":1},"speaker":"B","text":"はい。そうですが全然進みません。何を話したらよいと思いますか？"},{"id":{"d":950,"n":5,"t":1},"speaker":"A","text":"原稿が必要なほど長々と話すのは駄目だと思います。"},{"id":{"d":950,"n":6,"t":1},"speaker":"B","text":"では、「乾杯」だけでよいのでしょうか？"},{"id":{"d":950,"n":7,"t":1},"speaker":"A","text":"それも駄目ですよ。短くても歓迎の気持ちを伝えましょうよ。"},{"id":{"d":950,"n":8,"t":1},"speaker":"B","text":"何となく方向性が見えてきました。もう少し考えます。"}];
INSERT INTO sentence [{"id":{"d":951,"n":1,"t":1},"speaker":"A","text":"実家の掃除機が壊れてしまいましたので、新しいものの購入を検討しています。"},{"id":{"d":951,"n":2,"t":1},"speaker":"B","text":"どのような形のものをお探しですか？"},{"id":{"d":951,"n":3,"t":1},"speaker":"A","text":"今までのものが重たくて大変そうでしたので、軽いものがいいです。"},{"id":{"d":951,"n":4,"t":1},"speaker":"B","text":"それではスティック型をお勧めします。"},{"id":{"d":951,"n":5,"t":1},"speaker":"A","text":"スティック型は毎回ごみ捨てをしなければならないでしょう？"},{"id":{"d":951,"n":6,"t":1},"speaker":"B","text":"ごみ捨ては簡単なのですよ。コードレスで動くものは、動かしやすいものが多いです。"},{"id":{"d":951,"n":7,"t":1},"speaker":"A","text":"本当に軽くていいですね。自宅にも欲しくなってしまいました。"},{"id":{"d":951,"n":8,"t":1},"speaker":"B","text":"古い掃除機の下取りもしておりますので、2台まとめてのご購入はいかがでしょう？"}];
INSERT INTO sentence [{"id":{"d":952,"n":1,"t":1},"speaker":"A","text":"今日、私には覚えのない荷物が届いたのですが、あなたには心当たりがありますか？"},{"id":{"d":952,"n":2,"t":1},"speaker":"B","text":"これは、テレビショッピングを見て、君にぴったりのものがあったので注文しておいたものです。"},{"id":{"d":952,"n":3,"t":1},"speaker":"A","text":"そうでしたか。何が入っているのですか？"},{"id":{"d":952,"n":4,"t":1},"speaker":"B","text":"開けてみてください。"},{"id":{"d":952,"n":5,"t":1},"speaker":"A","text":"電動のお風呂掃除ブラシが入っていました。"},{"id":{"d":952,"n":6,"t":1},"speaker":"B","text":"お風呂掃除をすると腰が痛いと言っていたでしょう？"},{"id":{"d":952,"n":7,"t":1},"speaker":"A","text":"これで掃除をしたら楽そうですね。"},{"id":{"d":952,"n":8,"t":1},"speaker":"B","text":"気に入ってくれてよかったです。お誕生日おめでとうございます。"},{"id":{"d":952,"n":9,"t":1},"speaker":"A","text":"まあ。私の誕生日プレゼントだったのですね。うれしいです。ありがとうございます。"},{"id":{"d":952,"n":10,"t":1},"speaker":"B","text":"どういたしまして。"}];
INSERT INTO sentence [{"id":{"d":953,"n":1,"t":1},"speaker":"A","text":"修、この袋は何ですか？"},{"id":{"d":953,"n":2,"t":1},"speaker":"B","text":"ツクシが入っています。学校の帰りに、友則君と一緒に採ってきたのです。"},{"id":{"d":953,"n":3,"t":1},"speaker":"A","text":"食べるために取ってきたのですか？"},{"id":{"d":953,"n":4,"t":1},"speaker":"B","text":"楽しかったのでたくさん採っただけで、そこまで考えていませんでした。そもそもツクシは食べられるのですか？"},{"id":{"d":953,"n":5,"t":1},"speaker":"A","text":"それで玄関に放り出してあったのですね。ツクシはおいしいご飯になるのですよ。"},{"id":{"d":953,"n":6,"t":1},"speaker":"B","text":"そうなのですか？知りませんでした。"},{"id":{"d":953,"n":7,"t":1},"speaker":"A","text":"今晩はツクシの卵とじにします。修も手伝ってください。"},{"id":{"d":953,"n":8,"t":1},"speaker":"B","text":"はい。手を洗ってきて、手伝います。"}];
INSERT INTO sentence [{"id":{"d":954,"n":1,"t":1},"speaker":"A","text":"これを食べたら「秋だな」と思う食べ物は何ですか？"},{"id":{"d":954,"n":2,"t":1},"speaker":"B","text":"私は脂ののったサンマです。塩焼きにすると絶品です。"},{"id":{"d":954,"n":3,"t":1},"speaker":"A","text":"まさに秋ですね。"},{"id":{"d":954,"n":4,"t":1},"speaker":"B","text":"あとはサツマイモです。秋には焼き芋が食べたくなります。あなたはどうですか？"},{"id":{"d":954,"n":5,"t":1},"speaker":"A","text":"本当はマツタケと言いたいところですが、そうそう食べられるものではありませんので、キノコということにします。"},{"id":{"d":954,"n":6,"t":1},"speaker":"B","text":"マツタケは高過ぎますものね。クリとかカキとかも秋を感じます。"},{"id":{"d":954,"n":7,"t":1},"speaker":"A","text":"いいですね。今度、サンマとサツマイモとクリとカキでオータムパーティーでもしましょう。"}];
INSERT INTO sentence [{"id":{"d":955,"n":1,"t":1},"speaker":"A","text":"今日のおやつはパンの耳ですか？"},{"id":{"d":955,"n":2,"t":1},"speaker":"B","text":"はい。昨日のサンドイッチの残りのパン耳ですよ。"},{"id":{"d":955,"n":3,"t":1},"speaker":"A","text":"揚げてあるのですね。"},{"id":{"d":955,"n":4,"t":1},"speaker":"B","text":"揚げてお砂糖をかけたのです。おいしそうでしょう？"},{"id":{"d":955,"n":5,"t":1},"speaker":"A","text":"おいしそうです。パンの耳がおやつになるのですね。"},{"id":{"d":955,"n":6,"t":1},"speaker":"B","text":"今日のおやつをこれにしようと思って、耳を切るときに大きめに切っておきました。"},{"id":{"d":955,"n":7,"t":1},"speaker":"A","text":"カリカリして甘くておいしいです。"},{"id":{"d":955,"n":8,"t":1},"speaker":"B","text":"またサンドイッチを作ったら、このおやつを作りますね。"}];
INSERT INTO sentence [{"id":{"d":956,"n":1,"t":1},"speaker":"A","text":"おばあちゃん、ユズみそが上手にできたから持ってきましたよ。"},{"id":{"d":956,"n":2,"t":1},"speaker":"B","text":"どうもありがとう。ユズの香りがしておいしそうですね。"},{"id":{"d":956,"n":3,"t":1},"speaker":"A","text":"いろいろなものにかけて食べてくださいね。"},{"id":{"d":956,"n":4,"t":1},"speaker":"B","text":"ちょうど畑のダイコンを抜いてきましたから、ふろふき大根を作りますよ。"},{"id":{"d":956,"n":5,"t":1},"speaker":"A","text":"ふろふき大根にユズみそをかけるのはいいですね。"},{"id":{"d":956,"n":6,"t":1},"speaker":"B","text":"ダイコンはたくさんありますから、いくつか持っていきませんか？"},{"id":{"d":956,"n":7,"t":1},"speaker":"A","text":"よいのですか？ありがとうございます。夕ご飯が豪華になりそうです。"},{"id":{"d":956,"n":8,"t":1},"speaker":"B","text":"余ったユズがあったら皮も刻んでのせるといいですよ。"}];
INSERT INTO sentence [{"id":{"d":957,"n":1,"t":1},"speaker":"A","text":"会社の先輩の結婚式に招待されたので、ご祝儀袋を買いたいです。一緒に選んでくれますか？"},{"id":{"d":957,"n":2,"t":1},"speaker":"B","text":"よいですよ。最近はいろいろな色柄のものがあるのですね。"},{"id":{"d":957,"n":3,"t":1},"speaker":"A","text":"ここまでたくさん種類はあるとは思いませんでした。水引も変わった形のものがありますね。このツルの水引はどうですか？"},{"id":{"d":957,"n":4,"t":1},"speaker":"B","text":"それは中に入れる金額が多くないと恥ずかしいご祝儀袋です。"},{"id":{"d":957,"n":5,"t":1},"speaker":"A","text":"そうなのですか？"},{"id":{"d":957,"n":6,"t":1},"speaker":"B","text":"ここに10万円以上と書いてありますよ。後輩から先輩にはさすがにそれほど包みません。"},{"id":{"d":957,"n":7,"t":1},"speaker":"A","text":"すてきだと思ったのに残念です。"}];
INSERT INTO sentence [{"id":{"d":958,"n":1,"t":1},"speaker":"A","text":"明日は雨になりそうですよ。"},{"id":{"d":958,"n":2,"t":1},"speaker":"B","text":"長靴を持っていませんので、スニーカーで行くしかありませんね。"},{"id":{"d":958,"n":3,"t":1},"speaker":"A","text":"物置に防水スプレーがありますから、夜のうちにスニーカーにかけておくといいですよ。"},{"id":{"d":958,"n":4,"t":1},"speaker":"B","text":"防水スプレーというものがあったのですね。物置のどこですか？"},{"id":{"d":958,"n":5,"t":1},"speaker":"A","text":"左の棚の上から2段目に置いてあったはずです。"},{"id":{"d":958,"n":6,"t":1},"speaker":"B","text":"探してきます。"},{"id":{"d":958,"n":7,"t":1},"speaker":"A","text":"暗いので懐中電灯を持っていった方がいいですよ。"},{"id":{"d":958,"n":8,"t":1},"speaker":"B","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":959,"n":1,"t":1},"speaker":"A","text":"朝ご飯を用意してくれてありがとうございます。"},{"id":{"d":959,"n":2,"t":1},"speaker":"B","text":"朝ご飯と言ってもパンをトーストにしただけですけれどね。"},{"id":{"d":959,"n":3,"t":1},"speaker":"A","text":"このトーストはとてもおいしいですよ。"},{"id":{"d":959,"n":4,"t":1},"speaker":"B","text":"そうですか？普通に焼いたつもりですけれど。"},{"id":{"d":959,"n":5,"t":1},"speaker":"A","text":"トースターで焼いたのですよね？"},{"id":{"d":959,"n":6,"t":1},"speaker":"B","text":"いいえ。家にはトースターがありませんので、魚焼きグリルで焼きました。"},{"id":{"d":959,"n":7,"t":1},"speaker":"A","text":"魚焼きグリルでパンが焼けるのですか？"},{"id":{"d":959,"n":8,"t":1},"speaker":"B","text":"パンでもお肉でも野菜でもおいしく焼けますよ。"}];
INSERT INTO sentence [{"id":{"d":960,"n":1,"t":1},"speaker":"A","text":"まだ暑いのに、もう秋冬物を売っていました。"},{"id":{"d":960,"n":2,"t":1},"speaker":"B","text":"毎年、ファッションの動きは早いですね。"},{"id":{"d":960,"n":3,"t":1},"speaker":"A","text":"そうですね。去年は、まだ暑いからと秋冬物を買わないでいたら、急に寒くなって困りました。"},{"id":{"d":960,"n":4,"t":1},"speaker":"B","text":"衣替えをしていないうちは、新しいものを2、3着買っておくと便利ですね。"},{"id":{"d":960,"n":5,"t":1},"speaker":"A","text":"でも、まだ長袖には触りたくもない暑さです。"},{"id":{"d":960,"n":6,"t":1},"speaker":"B","text":"そうですね。買い時が難しいです。"},{"id":{"d":960,"n":7,"t":1},"speaker":"A","text":"ついつい夏物のセール品の方に手が伸びてしまいます。"},{"id":{"d":960,"n":8,"t":1},"speaker":"B","text":"確かにそうですね。"}];
INSERT INTO sentence [{"id":{"d":961,"n":1,"t":1},"speaker":"A","text":"石井さんは、会社の机におわんを入れているのですか？"},{"id":{"d":961,"n":2,"t":1},"speaker":"B","text":"はい。お昼のお弁当におみそ汁が欲しかったので、おわんを置くことにしたのです。"},{"id":{"d":961,"n":3,"t":1},"speaker":"A","text":"インスタントのおみそ汁ですか？"},{"id":{"d":961,"n":4,"t":1},"speaker":"B","text":"フリーズドライのおみそ汁で、給湯室のお湯で作っています。結構おいしいですよ。"},{"id":{"d":961,"n":5,"t":1},"speaker":"A","text":"実はフリーズドライのおみそ汁は食べたことがありません。"},{"id":{"d":961,"n":6,"t":1},"speaker":"B","text":"具がたくさん入っていて、お湯だけで、今作ったおみそ汁のようになります。"},{"id":{"d":961,"n":7,"t":1},"speaker":"A","text":"私も冷たいお弁当だけでは食べた気がせず汁物が欲しいと思うことがよくあるので、明日から石井さんのまねをしてみてもよいですか？"},{"id":{"d":961,"n":8,"t":1},"speaker":"B","text":"もちろんですよ。おみそ汁はコンビニエンスストアに売っています。おわんを忘れずに持ってきてくださいね。"}];
INSERT INTO sentence [{"id":{"d":962,"n":1,"t":1},"speaker":"A","text":"この棚の商品は全部半額なのですか？"},{"id":{"d":962,"n":2,"t":1},"speaker":"B","text":"はい。来週、棚卸しで一時閉店させていただきますので、セール品となっております。"},{"id":{"d":962,"n":3,"t":1},"speaker":"A","text":"閉まるのは来週の何曜日ですか？"},{"id":{"d":962,"n":4,"t":1},"speaker":"B","text":"水曜と木曜の2日間です。"},{"id":{"d":962,"n":5,"t":1},"speaker":"A","text":"金曜日には開くのですね？"},{"id":{"d":962,"n":6,"t":1},"speaker":"B","text":"はい。ご来店をお待ちしております。"},{"id":{"d":962,"n":7,"t":1},"speaker":"A","text":"このお安い棚をもう少し見せていただきますね。"},{"id":{"d":962,"n":8,"t":1},"speaker":"B","text":"ごゆっくりお買い物をしてください。"}];
INSERT INTO sentence [{"id":{"d":963,"n":1,"t":1},"speaker":"A","text":"この服の組み合わせはおかしいでしょうか？"},{"id":{"d":963,"n":2,"t":1},"speaker":"B","text":"あまり良い組み合わせには見えません。上下のバランスはもう少し考えた方がいいですよ。"},{"id":{"d":963,"n":3,"t":1},"speaker":"A","text":"センスがありませんので、どこがまずいのか分かりません。バランスとはどういうことでしょうか？"},{"id":{"d":963,"n":4,"t":1},"speaker":"B","text":"上も下も柄物でぶかぶかというのは、おしゃれ上級者でも難しい着こなしです。"},{"id":{"d":963,"n":5,"t":1},"speaker":"A","text":"なるほど、どう変えたらよいですか？"},{"id":{"d":963,"n":6,"t":1},"speaker":"B","text":"上が大きめのサイズなら、下はスリムにした方がいいです。"},{"id":{"d":963,"n":7,"t":1},"speaker":"A","text":"それでは下を黒のスキニーにしたいと思います。それだとどうでしょうか？"},{"id":{"d":963,"n":8,"t":1},"speaker":"B","text":"それならいいと思いますよ。"},{"id":{"d":963,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。着替えてきます。"}];
INSERT INTO sentence [{"id":{"d":964,"n":1,"t":1},"speaker":"A","text":"今日のおやつは冷凍室に入っていますよ。"},{"id":{"d":964,"n":2,"t":1},"speaker":"B","text":"冷凍室ということは、アイスクリームですか？"},{"id":{"d":964,"n":3,"t":1},"speaker":"A","text":"いえ、アイスクリームではありません。"},{"id":{"d":964,"n":4,"t":1},"speaker":"B","text":"では何でしょうか？もしかして、このビニールの棒に入っているものがおやつですか？"},{"id":{"d":964,"n":5,"t":1},"speaker":"A","text":"そうです。あんず棒といいます。お母さんが子どもの頃はよく食べました。"},{"id":{"d":964,"n":6,"t":1},"speaker":"B","text":"凍らせて食べるものですか？"},{"id":{"d":964,"n":7,"t":1},"speaker":"A","text":"そうですよ。端を切って食べてごらんなさい。"},{"id":{"d":964,"n":8,"t":1},"speaker":"B","text":"おいしいです。もう1本食べてもよいですか？"},{"id":{"d":964,"n":9,"t":1},"speaker":"A","text":"どうぞ。"}];
INSERT INTO sentence [{"id":{"d":965,"n":1,"t":1},"speaker":"A","text":"冷たいお茶を買ってきましたよ。"},{"id":{"d":965,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。いただきます。"},{"id":{"d":965,"n":3,"t":1},"speaker":"A","text":"ちょっと待ってください。そのまま飲まないでくださいね。"},{"id":{"d":965,"n":4,"t":1},"speaker":"B","text":"なぜですか？私はいつもこのまま飲みますよ。"},{"id":{"d":965,"n":5,"t":1},"speaker":"A","text":"この商品には抹茶が入っていて下にたまっているので、振ってから飲むとおいしいのです。"},{"id":{"d":965,"n":6,"t":1},"speaker":"B","text":"本当ですね。下にたまっています。"},{"id":{"d":965,"n":7,"t":1},"speaker":"A","text":"こうやって振ると、抹茶が全体に行き渡ります。"},{"id":{"d":965,"n":8,"t":1},"speaker":"B","text":"振ってから飲むと確かにおいしいですね。今度から振って飲むようにします。"}];
INSERT INTO sentence [{"id":{"d":966,"n":1,"t":1},"speaker":"A","text":"洗濯物を干したのですけれど、貴司の靴下が片方ありませんでした。"},{"id":{"d":966,"n":2,"t":1},"speaker":"B","text":"その辺に落ちていませんか？"},{"id":{"d":966,"n":3,"t":1},"speaker":"A","text":"見当たらなかったです。"},{"id":{"d":966,"n":4,"t":1},"speaker":"B","text":"貴司が片方しか洗濯かごに入れていなかったのでしょうか？"},{"id":{"d":966,"n":5,"t":1},"speaker":"A","text":"部屋の方も見てきましたが、見当たりませんでした。貴司のお気に入りの靴下だったようなので、出てこないとまずいですね。"},{"id":{"d":966,"n":6,"t":1},"speaker":"B","text":"もう一度、僕が探してきます。"},{"id":{"d":966,"n":7,"t":1},"speaker":"A","text":"ありましたか？"},{"id":{"d":966,"n":8,"t":1},"speaker":"B","text":"洗濯ネットの端に引っかかっていました。これでしょう？"},{"id":{"d":966,"n":9,"t":1},"speaker":"A","text":"ありましたか。それです。"}];
INSERT INTO sentence [{"id":{"d":967,"n":1,"t":1},"speaker":"A","text":"しばらくの間、休みの日は兄さんの所に行くことにしました。"},{"id":{"d":967,"n":2,"t":1},"speaker":"B","text":"お兄さんが、どうかなさったのですか？"},{"id":{"d":967,"n":3,"t":1},"speaker":"A","text":"店を移転するので、その手伝いに行きます。"},{"id":{"d":967,"n":4,"t":1},"speaker":"B","text":"今までのお店の場所はビルになるのでしたね。"},{"id":{"d":967,"n":5,"t":1},"speaker":"A","text":"ビルには入れませんので、近くに居抜き物件を探したらしいですよ。"},{"id":{"d":967,"n":6,"t":1},"speaker":"B","text":"良い所が見つかったのですね。"},{"id":{"d":967,"n":7,"t":1},"speaker":"A","text":"そのようです。"},{"id":{"d":967,"n":8,"t":1},"speaker":"B","text":"お休みなのに大変ですけれど、お兄さんを手伝ってあげてください。"}];
INSERT INTO sentence [{"id":{"d":968,"n":1,"t":1},"speaker":"A","text":"朝ご飯の卵はスクランブルエッグでよいですか？"},{"id":{"d":968,"n":2,"t":1},"speaker":"B","text":"よいですよ。少し硬めに四角く焼いてくれるとうれしいです。"},{"id":{"d":968,"n":3,"t":1},"speaker":"A","text":"何ですか？そのリクエストは。"},{"id":{"d":968,"n":4,"t":1},"speaker":"B","text":"トーストの上にのせたいのです。"},{"id":{"d":968,"n":5,"t":1},"speaker":"A","text":"それで四角なのですね。分かりました。"},{"id":{"d":968,"n":6,"t":1},"speaker":"B","text":"お中元で頂いたトマトソースを卵にかけたいです。"},{"id":{"d":968,"n":7,"t":1},"speaker":"A","text":"それはおいしそうですね。私もそうします。"},{"id":{"d":968,"n":8,"t":1},"speaker":"B","text":"朝から高級な味を楽しめますね。"}];
INSERT INTO sentence [{"id":{"d":969,"n":1,"t":1},"speaker":"A","text":"この缶詰、賞味期限が切れそうです。"},{"id":{"d":969,"n":2,"t":1},"speaker":"B","text":"何の缶詰ですか？"},{"id":{"d":969,"n":3,"t":1},"speaker":"A","text":"ダークチェリーと書いてあります。"},{"id":{"d":969,"n":4,"t":1},"speaker":"B","text":"それは前にタルトを作ろうと思って買ってきたものです。忘れていました。"},{"id":{"d":969,"n":5,"t":1},"speaker":"A","text":"今から作ってくれてもよいのですけれど、駄目ですか？"},{"id":{"d":969,"n":6,"t":1},"speaker":"B","text":"あいにく、小麦粉が足りません。"},{"id":{"d":969,"n":7,"t":1},"speaker":"A","text":"残念です。では、このまま冷やして食べますか？"},{"id":{"d":969,"n":8,"t":1},"speaker":"B","text":"ヨーグルトがありますので、かけて食べましょう。"}];
INSERT INTO sentence [{"id":{"d":970,"n":1,"t":1},"speaker":"A","text":"しゃぶしゃぶの食べ放題に行きませんか？"},{"id":{"d":970,"n":2,"t":1},"speaker":"B","text":"私は、お肉はそれほどたくさん食べられませんよ。"},{"id":{"d":970,"n":3,"t":1},"speaker":"A","text":"お肉以外にもいろいろありますから大丈夫です。"},{"id":{"d":970,"n":4,"t":1},"speaker":"B","text":"お肉を食べないと、元が取れないのではありませんか？"},{"id":{"d":970,"n":5,"t":1},"speaker":"A","text":"お母さん世代の人は割引があるので大丈夫です。"},{"id":{"d":970,"n":6,"t":1},"speaker":"B","text":"そういうことなら行きましょうか？"},{"id":{"d":970,"n":7,"t":1},"speaker":"A","text":"家には大食漢の翔がいますから、お母さんが食べられなくても、間違いなく損はしませんよ。"},{"id":{"d":970,"n":8,"t":1},"speaker":"B","text":"それではかえってお店の人に悪いですね。"}];
INSERT INTO sentence [{"id":{"d":971,"n":1,"t":1},"speaker":"A","text":"次はタンスを運び出そうと思います。どこに置いてありますか？"},{"id":{"d":971,"n":2,"t":1},"speaker":"B","text":"タンスはありません。服は衣装ケースに全部しまってクローゼットにありますから、そのまま運んでください。"},{"id":{"d":971,"n":3,"t":1},"speaker":"A","text":"分かりました。傷が付かないようにそのまま包みますね。テーブルもないのですか？"},{"id":{"d":971,"n":4,"t":1},"speaker":"B","text":"テーブルと椅子は後輩に譲ってしまいました。あとは少しの食器と布団くらいです。"},{"id":{"d":971,"n":5,"t":1},"speaker":"A","text":"引っ越し料金の割引をしてあげたくなるほど物がないのですね。"},{"id":{"d":971,"n":6,"t":1},"speaker":"B","text":"不用品を捨て過ぎてしまったようです。"},{"id":{"d":971,"n":7,"t":1},"speaker":"A","text":"引っ越し先で新しいものを買いそろえればよいですね。"},{"id":{"d":971,"n":8,"t":1},"speaker":"B","text":"はい、その予定です。"}];
INSERT INTO sentence [{"id":{"d":972,"n":1,"t":1},"speaker":"A","text":"冷凍庫を買ってもよいですか？"},{"id":{"d":972,"n":2,"t":1},"speaker":"B","text":"今、使っている冷蔵庫の冷凍室では足りませんか？"},{"id":{"d":972,"n":3,"t":1},"speaker":"A","text":"頂き物が多いので冷凍したいのと、冷凍食品が安くなっていますので買いだめをしたいのです。"},{"id":{"d":972,"n":4,"t":1},"speaker":"B","text":"便利になるのは良いことですが、献立が冷凍食品ばかりになるのは少し寂しいです。"},{"id":{"d":972,"n":5,"t":1},"speaker":"A","text":"あら、いつもおいしいおいしいと言って食べているではありませんか？"},{"id":{"d":972,"n":6,"t":1},"speaker":"B","text":"僕がですか？"},{"id":{"d":972,"n":7,"t":1},"speaker":"A","text":"はい。あなたがお気に入りの讃岐うどんやボロネーゼは冷凍のものをレンジで温めただけですよ。"},{"id":{"d":972,"n":8,"t":1},"speaker":"B","text":"知りませんでした。手作りだとばかり思っていました。"}];
INSERT INTO sentence [{"id":{"d":973,"n":1,"t":1},"speaker":"A","text":"しばらくの間は廊下を歩かないでくださいね。"},{"id":{"d":973,"n":2,"t":1},"speaker":"B","text":"しばらくとはどのくらいですか？"},{"id":{"d":973,"n":3,"t":1},"speaker":"A","text":"ほんの10分くらいです。"},{"id":{"d":973,"n":4,"t":1},"speaker":"B","text":"何をしているのですか？"},{"id":{"d":973,"n":5,"t":1},"speaker":"A","text":"ワックスがけをしているのです。乾くまで歩かないでください。"},{"id":{"d":973,"n":6,"t":1},"speaker":"B","text":"分かりました。もう塗り終わりましたか？"},{"id":{"d":973,"n":7,"t":1},"speaker":"A","text":"あと3分の1くらいです。"},{"id":{"d":973,"n":8,"t":1},"speaker":"B","text":"トイレに行きたいと思っていたのですが、10分我慢ですね。"}];
INSERT INTO sentence [{"id":{"d":974,"n":1,"t":1},"speaker":"A","text":"もしもし？ごめんなさい、今日の待ち合わせに遅れます。"},{"id":{"d":974,"n":2,"t":1},"speaker":"B","text":"いつも時間に正確な人なのに珍しいですね。"},{"id":{"d":974,"n":3,"t":1},"speaker":"A","text":"目覚まし時計を新しくしたら、アラームのセット方法を間違えたようです。"},{"id":{"d":974,"n":4,"t":1},"speaker":"B","text":"それで寝坊してしまったのですね。"},{"id":{"d":974,"n":5,"t":1},"speaker":"A","text":"すみません。おわびにお昼をおごりますよ。"},{"id":{"d":974,"n":6,"t":1},"speaker":"B","text":"喫茶店でのんびり待っていますから、そこのお茶代で手を打ちましょう。"},{"id":{"d":974,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。すぐに支度して出ます。"},{"id":{"d":974,"n":8,"t":1},"speaker":"B","text":"慌てなくてもよいですよ。気を付けていらしてくださいね。"}];
INSERT INTO sentence [{"id":{"d":975,"n":1,"t":1},"speaker":"A","text":"今日はどのような服装をして行けばよいでしょうか？"},{"id":{"d":975,"n":2,"t":1},"speaker":"B","text":"午後から気温が上がるそうですから、薄着をして行く方がいいと思います。"},{"id":{"d":975,"n":3,"t":1},"speaker":"A","text":"どのくらいまで上がるのですか？"},{"id":{"d":975,"n":4,"t":1},"speaker":"B","text":"天気予報では30度を超すそうです。"},{"id":{"d":975,"n":5,"t":1},"speaker":"A","text":"夏日ですね。それなら、Tシャツ1枚ぐらいでよさそうですね。"},{"id":{"d":975,"n":6,"t":1},"speaker":"B","text":"外はそれでよいと思いますけれど、室内に入ると冷房が効いていますから、体が冷えるかもしれませんよ。"},{"id":{"d":975,"n":7,"t":1},"speaker":"A","text":"何か上に羽織るものを持っていく方がいいでしょうか？"},{"id":{"d":975,"n":8,"t":1},"speaker":"B","text":"そうですね、薄いカーディガンでも持っていくといいと思います。"}];
INSERT INTO sentence [{"id":{"d":976,"n":1,"t":1},"speaker":"A","text":"今日のお昼は弁当を作って、外で食べましょう。"},{"id":{"d":976,"n":2,"t":1},"speaker":"B","text":"それはいいですね。何を作りましょうか？"},{"id":{"d":976,"n":3,"t":1},"speaker":"A","text":"私が卵焼きを作りますから、あなたはサケを焼いてください。"},{"id":{"d":976,"n":4,"t":1},"speaker":"B","text":"野菜が何か欲しいですね。"},{"id":{"d":976,"n":5,"t":1},"speaker":"A","text":"それなら、ホウレンソウのおひたしはいかがでしょう。"},{"id":{"d":976,"n":6,"t":1},"speaker":"B","text":"もっと簡単なものにしませんか？確か、エダマメが残っていたはずです。"},{"id":{"d":976,"n":7,"t":1},"speaker":"A","text":"エダマメなら冷凍庫に入っていますから、解凍してください。"},{"id":{"d":976,"n":8,"t":1},"speaker":"B","text":"ありました。それにおにぎりを作れば十分ですね。"},{"id":{"d":976,"n":9,"t":1},"speaker":"A","text":"そうですね。さあ、料理を始めましょう。"}];
INSERT INTO sentence [{"id":{"d":977,"n":1,"t":1},"speaker":"A","text":"昨日、面白い料理番組をやっていました。"},{"id":{"d":977,"n":2,"t":1},"speaker":"B","text":"どのような番組ですか？"},{"id":{"d":977,"n":3,"t":1},"speaker":"A","text":"料理研究家の方が紹介していたのですが、カツオの刺身でとんかつを作るのです。"},{"id":{"d":977,"n":4,"t":1},"speaker":"B","text":"何ですか、それは。想像がつきませんね。"},{"id":{"d":977,"n":5,"t":1},"speaker":"A","text":"カツオの刺身を豚のばら肉で包んで、それをパン粉で揚げるのです。"},{"id":{"d":977,"n":6,"t":1},"speaker":"B","text":"それは独創的ですね。よくそのような料理を思い付くものですね。"},{"id":{"d":977,"n":7,"t":1},"speaker":"A","text":"豚肉だと火が通るのに時間がかかりますが、カツオだと生のままでよいので時間を短縮できるのだそうです。"},{"id":{"d":977,"n":8,"t":1},"speaker":"B","text":"素晴らしい発想ですね。それで、おいしいのでしょうか？"},{"id":{"d":977,"n":9,"t":1},"speaker":"A","text":"出演していたタレントがおいしいと言っていました。"},{"id":{"d":977,"n":10,"t":1},"speaker":"B","text":"今度、私も試しに作ってみましょう。"}];
INSERT INTO sentence [{"id":{"d":978,"n":1,"t":1},"speaker":"A","text":"最近、ちょっと味付けが薄い感じがします。"},{"id":{"d":978,"n":2,"t":1},"speaker":"B","text":"はい、塩分を控え目にしています。"},{"id":{"d":978,"n":3,"t":1},"speaker":"A","text":"少し物足りませんね。もっと、しっかりした味の方が私は好きです。"},{"id":{"d":978,"n":4,"t":1},"speaker":"B","text":"でも、あなたは健康診断で血圧が高いと言われたではありませんか？"},{"id":{"d":978,"n":5,"t":1},"speaker":"A","text":"それにしても、薄過ぎはしませんか？もう少し濃くしてほしいです。"},{"id":{"d":978,"n":6,"t":1},"speaker":"B","text":"1日に7グラム以下にするように言われていますから、このくらいがちょうどいいのです。"},{"id":{"d":978,"n":7,"t":1},"speaker":"A","text":"これでは食事の楽しみがなくなります。"},{"id":{"d":978,"n":8,"t":1},"speaker":"B","text":"そのようなことを言わずに食べてください。そのうち、体が慣れてきますから。"}];
INSERT INTO sentence [{"id":{"d":979,"n":1,"t":1},"speaker":"A","text":"最近、唐揚げの専門店が増えてきましたね。"},{"id":{"d":979,"n":2,"t":1},"speaker":"B","text":"そうそう、よく目につきますね。私も気になっていました。"},{"id":{"d":979,"n":3,"t":1},"speaker":"A","text":"行きつけのスーパーマーケットで、大分県の中津の唐揚げを店頭販売していました。"},{"id":{"d":979,"n":4,"t":1},"speaker":"B","text":"ブームの火付け役になったのが中津なのだそうです。"},{"id":{"d":979,"n":5,"t":1},"speaker":"A","text":"そうですか。どういういきさつですか？"},{"id":{"d":979,"n":6,"t":1},"speaker":"B","text":"中津には唐揚げの店がたくさんあって、それぞれに工夫をこらしているのです。それが全国に広まったのだそうです。"},{"id":{"d":979,"n":7,"t":1},"speaker":"A","text":"私がスーパーマーケットで食べた唐揚げもおいしかったです。"},{"id":{"d":979,"n":8,"t":1},"speaker":"B","text":"最近は、ファミリーレストランでも唐揚げのテイクアウトをしていますね。"},{"id":{"d":979,"n":9,"t":1},"speaker":"A","text":"食べ比べをしてみるのもいいですね。"}];
INSERT INTO sentence [{"id":{"d":980,"n":1,"t":1},"speaker":"A","text":"ジーンズが好きで、普段はだいたいこれをはいています。"},{"id":{"d":980,"n":2,"t":1},"speaker":"B","text":"ジーンズは良いですよね。私も愛用しています。"},{"id":{"d":980,"n":3,"t":1},"speaker":"A","text":"なにしろ丈夫で長持ちするし、何より気軽に着られるのがいいです。"},{"id":{"d":980,"n":4,"t":1},"speaker":"B","text":"ええ。もともとアメリカの鉱夫が作業着として着始めたそうですから。"},{"id":{"d":980,"n":5,"t":1},"speaker":"A","text":"それが今のように誰でも着るようになったのはいつからですか？"},{"id":{"d":980,"n":6,"t":1},"speaker":"B","text":"マーロン・ブランドやジェームズ・ディーンが映画の中で着たので、若者たちが競って着るようになったそうです。"},{"id":{"d":980,"n":7,"t":1},"speaker":"A","text":"日本でも同じ頃に普及し始めましたね。"},{"id":{"d":980,"n":8,"t":1},"speaker":"B","text":"はい。昔はもっぱら若者のファッションでしたが、今では年齢にかかわらず誰でも着ています。"}];
INSERT INTO sentence [{"id":{"d":981,"n":1,"t":1},"speaker":"A","text":"今夜は私が料理しましょう。"},{"id":{"d":981,"n":2,"t":1},"speaker":"B","text":"それはありがたいです。何を作ってくれるのですか？"},{"id":{"d":981,"n":3,"t":1},"speaker":"A","text":"エビとイカのアヒージョなんかどうですか？"},{"id":{"d":981,"n":4,"t":1},"speaker":"B","text":"おいしそうですね。材料はありますか？"},{"id":{"d":981,"n":5,"t":1},"speaker":"A","text":"イカは前に買っておいたのが冷凍庫にありますから、エビだけ買えばよいです。"},{"id":{"d":981,"n":6,"t":1},"speaker":"B","text":"ワインも買っていきましょう。どこか良いワインショップがありませんか？"},{"id":{"d":981,"n":7,"t":1},"speaker":"A","text":"この先に行きつけのワインショップがあります。店員さんが親切で、いろいろとアドバイスしてくれます。"},{"id":{"d":981,"n":8,"t":1},"speaker":"B","text":"少し高くてもおいしいワインを買いましょう。料理を作ってくれる代わりに、私がワインの代金を払いますよ。"},{"id":{"d":981,"n":9,"t":1},"speaker":"A","text":"それはありがとうございます。腕によりをかけて作りますね。"}];
INSERT INTO sentence [{"id":{"d":982,"n":1,"t":1},"speaker":"A","text":"あなたは着こなしが上手ですね。いつも感心しています。"},{"id":{"d":982,"n":2,"t":1},"speaker":"B","text":"そのようなことはありませんよ。いつも何を着たらよいのか迷ってばかりいます。"},{"id":{"d":982,"n":3,"t":1},"speaker":"A","text":"それは意外でした。あなたでもそうなのですか？"},{"id":{"d":982,"n":4,"t":1},"speaker":"B","text":"もちろんです。いつも他の人の服装を見て、参考にしています。"},{"id":{"d":982,"n":5,"t":1},"speaker":"A","text":"私もそうしていますが、いっこうに上手になりません。自分にはセンスがないのだと諦めています。"},{"id":{"d":982,"n":6,"t":1},"speaker":"B","text":"諦めるのは早いですよ。私の友人で、1センチメートルのスカート丈にこだわって、ああでもないこうでもないと悩んでいる人がいます。"},{"id":{"d":982,"n":7,"t":1},"speaker":"A","text":"それでも、それほどこだわれるのもセンスが良いからではありませんか？"},{"id":{"d":982,"n":8,"t":1},"speaker":"B","text":"そうかもしれませんが、そのような人でも悩んでいるのですから、私たちが迷うのも当然ですよ。"},{"id":{"d":982,"n":9,"t":1},"speaker":"A","text":"そうですね。私も諦めずに頑張ってみます。"}];
INSERT INTO sentence [{"id":{"d":983,"n":1,"t":1},"speaker":"A","text":"もしもし、三協設備さんですか？"},{"id":{"d":983,"n":2,"t":1},"speaker":"B","text":"はい、そうです。どういうご用件ですか？"},{"id":{"d":983,"n":3,"t":1},"speaker":"A","text":"台所の水栓の具合が悪いので見ていただきたいのですが。"},{"id":{"d":983,"n":4,"t":1},"speaker":"B","text":"どのような具合ですか？"},{"id":{"d":983,"n":5,"t":1},"speaker":"A","text":"混合水栓を使っているのですが、水の出が悪いのです。"},{"id":{"d":983,"n":6,"t":1},"speaker":"B","text":"分かりました。いつお伺いしましょうか？"},{"id":{"d":983,"n":7,"t":1},"speaker":"A","text":"なるべく早くお願いします。こちらはいつでもよいです。"},{"id":{"d":983,"n":8,"t":1},"speaker":"B","text":"それでは、今日の午後にお伺いします。2時頃ではいかがでしょうか？"},{"id":{"d":983,"n":9,"t":1},"speaker":"A","text":"結構です。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":984,"n":1,"t":1},"speaker":"A","text":"今日の天気はどうですか？天気予報では何と言っていますか？"},{"id":{"d":984,"n":2,"t":1},"speaker":"B","text":"午前中は晴れるけれども、午後になると雨が降ったりやんだりの天気だそうです。"},{"id":{"d":984,"n":3,"t":1},"speaker":"A","text":"それでは、洗濯物は家の中に干していきましょう。"},{"id":{"d":984,"n":4,"t":1},"speaker":"B","text":"傘はどうしましょうか？それほど激しい雨ではなさそうなので、持っていく必要はありませんかね。"},{"id":{"d":984,"n":5,"t":1},"speaker":"A","text":"念のために携帯用の傘を持っていきましょう。玄関の靴箱の中に置いてあります。"},{"id":{"d":984,"n":6,"t":1},"speaker":"B","text":"確か、1本、骨が折れている傘があったと思います。"},{"id":{"d":984,"n":7,"t":1},"speaker":"A","text":"余分な傘があるから大丈夫です。"},{"id":{"d":984,"n":8,"t":1},"speaker":"B","text":"点検してみましょう。"}];
INSERT INTO sentence [{"id":{"d":985,"n":1,"t":1},"speaker":"A","text":"今夜は炊き込みご飯にしましょう。"},{"id":{"d":985,"n":2,"t":1},"speaker":"B","text":"それはいいですね。具は何を入れるのですか？"},{"id":{"d":985,"n":3,"t":1},"speaker":"A","text":"甘グリが手に入ったので、それを入れましょう。"},{"id":{"d":985,"n":4,"t":1},"speaker":"B","text":"おいしそうですね。私も手伝います。"},{"id":{"d":985,"n":5,"t":1},"speaker":"A","text":"それは助かります。では、まず、ニンジンを短冊切りにしてください。"},{"id":{"d":985,"n":6,"t":1},"speaker":"B","text":"できました。次は何をしましょうか？"},{"id":{"d":985,"n":7,"t":1},"speaker":"A","text":"シイタケの軸を取り除いて、薄く切ってください。"},{"id":{"d":985,"n":8,"t":1},"speaker":"B","text":"これでよいですか？"},{"id":{"d":985,"n":9,"t":1},"speaker":"A","text":"はい、それで結構です。その後は、炊飯器にお米と材料、おしょうゆとお酒、だし汁の元と水を入れてかき混ぜ、スイッチを入れればできます。"},{"id":{"d":985,"n":10,"t":1},"speaker":"B","text":"楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":986,"n":1,"t":1},"speaker":"A","text":"お宅では料理のだし汁はどうしていますか？"},{"id":{"d":986,"n":2,"t":1},"speaker":"B","text":"うちはかつお節や昆布でだし汁を取っています。"},{"id":{"d":986,"n":3,"t":1},"speaker":"A","text":"そうですか。うちはだいたい市販のだし汁のもとを使っています。"},{"id":{"d":986,"n":4,"t":1},"speaker":"B","text":"以前はうちもそうでした。料理にかける時間が惜しかったものですから。"},{"id":{"d":986,"n":5,"t":1},"speaker":"A","text":"やはりそうでしたか。だし汁のもとの方が手軽ですからね。"},{"id":{"d":986,"n":6,"t":1},"speaker":"B","text":"ところが、あるとききちんとだし汁を取ってみると、だし汁のもととは比べものにならないくらいにおいしかったのです。"},{"id":{"d":986,"n":7,"t":1},"speaker":"A","text":"それほど違いますか？"},{"id":{"d":986,"n":8,"t":1},"speaker":"B","text":"それはもう、驚くほど違いました。目の覚める思いでした。"},{"id":{"d":986,"n":9,"t":1},"speaker":"A","text":"それでは、私も今度やってみます。"}];
INSERT INTO sentence [{"id":{"d":987,"n":1,"t":1},"speaker":"A","text":"中華料理を食べに行きましょう。"},{"id":{"d":987,"n":2,"t":1},"speaker":"B","text":"いいですね。どこに行きましょうか？"},{"id":{"d":987,"n":3,"t":1},"speaker":"A","text":"中華料理といったら、横浜の中華街に決まっています。"},{"id":{"d":987,"n":4,"t":1},"speaker":"B","text":"どこかおいしい店を知っていますか？"},{"id":{"d":987,"n":5,"t":1},"speaker":"A","text":"平華楼の大籠包が評判です。"},{"id":{"d":987,"n":6,"t":1},"speaker":"B","text":"それは何ですか？"},{"id":{"d":987,"n":7,"t":1},"speaker":"A","text":"大きな小籠包です。最初にストローで肉汁を吸うところがユニークなのです。"},{"id":{"d":987,"n":8,"t":1},"speaker":"B","text":"おいしそうですね。私も食べてみたいです。"},{"id":{"d":987,"n":9,"t":1},"speaker":"A","text":"早速行きましょう。"}];
INSERT INTO sentence [{"id":{"d":988,"n":1,"t":1},"speaker":"A","text":"冬は焼き芋に限りますね。"},{"id":{"d":988,"n":2,"t":1},"speaker":"B","text":"体が温まりますしね。子どもたちも大好きでよくせがまれます。"},{"id":{"d":988,"n":3,"t":1},"speaker":"A","text":"お宅はどうやって作っているのですか？"},{"id":{"d":988,"n":4,"t":1},"speaker":"B","text":"うちはアルミホイールに包んでオーブンで焼きます。お宅はどうですか？"},{"id":{"d":988,"n":5,"t":1},"speaker":"A","text":"うちはまきストーブで焼きます。アルミホイールに包んで、ストーブの中に放っておきます。"},{"id":{"d":988,"n":6,"t":1},"speaker":"B","text":"どうですか？おいしいですか？"},{"id":{"d":988,"n":7,"t":1},"speaker":"A","text":"それはもう、ねっとりしてとてもおいしいです。ぜひ一度ごちそうしたいです。"},{"id":{"d":988,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。今度お邪魔するときに、ごちそうになります。"}];
INSERT INTO sentence [{"id":{"d":989,"n":1,"t":1},"speaker":"A","text":"土用の丑の日が近づいてきましたね。"},{"id":{"d":989,"n":2,"t":1},"speaker":"B","text":"ウナギを食べる日ですね。今年はいつですか？"},{"id":{"d":989,"n":3,"t":1},"speaker":"A","text":"7月23日と8月4日の2日です。去年は1日でした。"},{"id":{"d":989,"n":4,"t":1},"speaker":"B","text":"年によって違うのですか。それは知りませんでした。"},{"id":{"d":989,"n":5,"t":1},"speaker":"A","text":"季節の変わり目の約18日間のことを土用と言います。丑の日がその間に1日ある年と2日ある年があります。"},{"id":{"d":989,"n":6,"t":1},"speaker":"B","text":"なるほど、今年は2日ある年というわけですね。"},{"id":{"d":989,"n":7,"t":1},"speaker":"A","text":"そうです。ウナギを2回食べられるということですね。"},{"id":{"d":989,"n":8,"t":1},"speaker":"B","text":"そういえば、コンビニエンスストアでも予約を募集していますね。"},{"id":{"d":989,"n":9,"t":1},"speaker":"A","text":"うちも早速予約しましょう。事前に予約すると1割引きで購入できるそうです。"}];
INSERT INTO sentence [{"id":{"d":990,"n":1,"t":1},"speaker":"A","text":"電話があったようですが、何でしたか？"},{"id":{"d":990,"n":2,"t":1},"speaker":"B","text":"石山製薬からでした。前に一度、薬を買ったことがあるのです。それ以来、時々電話がかかってきます。"},{"id":{"d":990,"n":3,"t":1},"speaker":"A","text":"随分熱心なのですね。"},{"id":{"d":990,"n":4,"t":1},"speaker":"B","text":"テレビでも宣伝しているでしょう。膝の薬だとか薬用入浴剤だとか。"},{"id":{"d":990,"n":5,"t":1},"speaker":"A","text":"あなたは何の薬を買ったのですか？"},{"id":{"d":990,"n":6,"t":1},"speaker":"B","text":"膝が痛むので試しに買ってみたのですが、値段が高かったので、続けられませんでした。"},{"id":{"d":990,"n":7,"t":1},"speaker":"A","text":"それから、勧誘の電話があるのですね。"},{"id":{"d":990,"n":8,"t":1},"speaker":"B","text":"そうです。たびたび電話があります。"},{"id":{"d":990,"n":9,"t":1},"speaker":"A","text":"どう言って断るのですか？"},{"id":{"d":990,"n":10,"t":1},"speaker":"B","text":"必要になったらこちらから電話すると言って断ります。"}];
INSERT INTO sentence [{"id":{"d":991,"n":1,"t":1},"speaker":"A","text":"ズボンにペンキが付いていますよ。"},{"id":{"d":991,"n":2,"t":1},"speaker":"B","text":"どこにですか？"},{"id":{"d":991,"n":3,"t":1},"speaker":"A","text":"お尻の所に、べったり付いています。"},{"id":{"d":991,"n":4,"t":1},"speaker":"B","text":"いつ付いたのでしょうね。全く思い当たりません。"},{"id":{"d":991,"n":5,"t":1},"speaker":"A","text":"早く洗い落とさないと取れなくなりますよ。"},{"id":{"d":991,"n":6,"t":1},"speaker":"B","text":"それは困ります。ペンキは水で洗ったくらいでは取れませんよね。"},{"id":{"d":991,"n":7,"t":1},"speaker":"A","text":"確かベンジンがあったはずですから、今、持ってきてあげましょう。"},{"id":{"d":991,"n":8,"t":1},"speaker":"B","text":"それはよかった。急いでお願いします。"}];
INSERT INTO sentence [{"id":{"d":992,"n":1,"t":1},"speaker":"A","text":"毎日、熱帯夜が続いて、よく眠れません。"},{"id":{"d":992,"n":2,"t":1},"speaker":"B","text":"今年は特に暑いですね。"},{"id":{"d":992,"n":3,"t":1},"speaker":"A","text":"何か良い方法を知りませんか？"},{"id":{"d":992,"n":4,"t":1},"speaker":"B","text":"クーラーはつけっ放しにしていますか？"},{"id":{"d":992,"n":5,"t":1},"speaker":"A","text":"はい、つけっ放しにしています。それでも、汗びっしょりになって目が覚めてしまいます。"},{"id":{"d":992,"n":6,"t":1},"speaker":"B","text":"除湿器で湿度を下げるといいですよ。"},{"id":{"d":992,"n":7,"t":1},"speaker":"A","text":"湿度のことはあまり気にしていませんでした。"},{"id":{"d":992,"n":8,"t":1},"speaker":"B","text":"体感温度が下がって、汗をあまりかかずに済みますよ。"},{"id":{"d":992,"n":9,"t":1},"speaker":"A","text":"なるほど、今度やってみます。"}];
INSERT INTO sentence [{"id":{"d":993,"n":1,"t":1},"speaker":"A","text":"今日は寒いですね。"},{"id":{"d":993,"n":2,"t":1},"speaker":"B","text":"このような日は、体の温まるものを食べたいですね。"},{"id":{"d":993,"n":3,"t":1},"speaker":"A","text":"そうなりますと、鍋料理が良いですね。"},{"id":{"d":993,"n":4,"t":1},"speaker":"B","text":"しゃぶしゃぶなどはどうでしょう。豚やタイなどのしゃぶしゃぶはおいしそうです。"},{"id":{"d":993,"n":5,"t":1},"speaker":"A","text":"キムチ鍋も良いですよ。からしが効いて、体が中から温まります。"},{"id":{"d":993,"n":6,"t":1},"speaker":"B","text":"おでんも捨てがたいですね。よく煮込んだだし汁の染みたおでんを、ふうふう言いながら食べるのはこたえられません。"},{"id":{"d":993,"n":7,"t":1},"speaker":"A","text":"すき焼きも良いですね。極上の霜降り肉を卵にからめてすすり上げるあの瞬間は、たまりません。"},{"id":{"d":993,"n":8,"t":1},"speaker":"B","text":"どれもおいしそうで、なかなか決められません。"},{"id":{"d":993,"n":9,"t":1},"speaker":"A","text":"スーパーマーケットに行って、材料を見ながら決めましょう。"}];
INSERT INTO sentence [{"id":{"d":994,"n":1,"t":1},"speaker":"A","text":"革靴をすっかり履かなくなりました。"},{"id":{"d":994,"n":2,"t":1},"speaker":"B","text":"いつもスニーカーですか？"},{"id":{"d":994,"n":3,"t":1},"speaker":"A","text":"はい、スニーカーを履き慣れると、革靴を履く気になれません。"},{"id":{"d":994,"n":4,"t":1},"speaker":"B","text":"私は仕事柄、革靴なしというわけにはいきません。"},{"id":{"d":994,"n":5,"t":1},"speaker":"A","text":"私も昔は革靴を履いていたのですが、今は納戸の奥にしまい込んでいます。"},{"id":{"d":994,"n":6,"t":1},"speaker":"B","text":"なかなか捨てる気になれないものですよね。"},{"id":{"d":994,"n":7,"t":1},"speaker":"A","text":"時々取り出して磨いています。"},{"id":{"d":994,"n":8,"t":1},"speaker":"B","text":"カビが生えていませんか？"},{"id":{"d":994,"n":9,"t":1},"speaker":"A","text":"そのとおりです。うっかり磨き忘れるとカビが生えてしまいます。"}];
INSERT INTO sentence [{"id":{"d":995,"n":1,"t":1},"speaker":"A","text":"ベランダの手すりがぐらぐらしています。"},{"id":{"d":995,"n":2,"t":1},"speaker":"B","text":"それは危ないですね。子どもたちが落ちないように気を付けなければいけません。"},{"id":{"d":995,"n":3,"t":1},"speaker":"A","text":"早く修理してください。"},{"id":{"d":995,"n":4,"t":1},"speaker":"B","text":"越してきたばかりだから、工務店を知りません。"},{"id":{"d":995,"n":5,"t":1},"speaker":"A","text":"インターネットで調べられませんか？"},{"id":{"d":995,"n":6,"t":1},"speaker":"B","text":"調べることはできますが、信用できる工務店かどうか分かりません。"},{"id":{"d":995,"n":7,"t":1},"speaker":"A","text":"管理人さんに相談してみてはいかがでしょう。管理人さんなら良い工務店をご存じでしょう。"},{"id":{"d":995,"n":8,"t":1},"speaker":"B","text":"それがいいですね。今日中に相談してみます。"},{"id":{"d":995,"n":9,"t":1},"speaker":"A","text":"なるべく早くお願いします。"}];
INSERT INTO sentence [{"id":{"d":996,"n":1,"t":1},"speaker":"A","text":"久しぶりにレストランで食事をしました。"},{"id":{"d":996,"n":2,"t":1},"speaker":"B","text":"コロナ禍でレストランは営業を制限されていましたからね。"},{"id":{"d":996,"n":3,"t":1},"speaker":"A","text":"行きつけのレストランでしたが、シェフも喜んでいました。"},{"id":{"d":996,"n":4,"t":1},"speaker":"B","text":"飲食業の人はこの2年間、大変でしたでしょうね。"},{"id":{"d":996,"n":5,"t":1},"speaker":"A","text":"シェフが言っていましたが、何度も廃業の瀬戸際に立たされたそうです。"},{"id":{"d":996,"n":6,"t":1},"speaker":"B","text":"よく持ちこたえましたね。"},{"id":{"d":996,"n":7,"t":1},"speaker":"A","text":"客の喜ぶ顔が見たいという気持ちで乗り切ったそうです。"},{"id":{"d":996,"n":8,"t":1},"speaker":"B","text":"営業を再開できてよかったですね。"},{"id":{"d":996,"n":9,"t":1},"speaker":"A","text":"私も久々においしいものが食べられてよかったです。"}];
INSERT INTO sentence [{"id":{"d":997,"n":1,"t":1},"speaker":"A","text":"西日本で豪雨が続いているようですね。"},{"id":{"d":997,"n":2,"t":1},"speaker":"B","text":"この時期になると、日本のどこかで大雨が降ります。"},{"id":{"d":997,"n":3,"t":1},"speaker":"A","text":"線状降水帯という言葉をよく聞くようになりました。"},{"id":{"d":997,"n":4,"t":1},"speaker":"B","text":"いつごろから使われるようになりましたか？"},{"id":{"d":997,"n":5,"t":1},"speaker":"A","text":"頻繁に用いられるようになったのは、平成26年に広島で土砂災害があったころからでした。"},{"id":{"d":997,"n":6,"t":1},"speaker":"B","text":"2年前に熊本県の球磨川流域で水害が発生したのも、確か線状降水帯でしたね。"},{"id":{"d":997,"n":7,"t":1},"speaker":"A","text":"そうでした。線状降水帯が発生すると水害が起こる確率が高くなります。"},{"id":{"d":997,"n":8,"t":1},"speaker":"B","text":"天気予報でも線状降水帯が発生したら知らせるようになりましたね。"},{"id":{"d":997,"n":9,"t":1},"speaker":"A","text":"あらかじめ避難の準備をしておいた方がいいですね。"}];
INSERT INTO sentence [{"id":{"d":998,"n":1,"t":1},"speaker":"A","text":"今夜はパスタにします。あなたも手伝ってくださいね。"},{"id":{"d":998,"n":2,"t":1},"speaker":"B","text":"もちろん手伝いますよ。今夜は何のパスタですか？"},{"id":{"d":998,"n":3,"t":1},"speaker":"A","text":"実家からめんたいこを送ってきたので、クリームソースを作ります。"},{"id":{"d":998,"n":4,"t":1},"speaker":"B","text":"それはおいしそうですね。これでおいしい白ワインでもあればうれしいのですがね。"},{"id":{"d":998,"n":5,"t":1},"speaker":"A","text":"それも実家から送ってきました。シチリア産の白ワインです。"},{"id":{"d":998,"n":6,"t":1},"speaker":"B","text":"素晴らしい。お父さんは何て気が利くのでしょう。それで、私は何をしましょうか？"},{"id":{"d":998,"n":7,"t":1},"speaker":"A","text":"パスタをゆでてください。アルデンテでお願いします。"},{"id":{"d":998,"n":8,"t":1},"speaker":"B","text":"かしこまりました。早速始めましょう。"}];
INSERT INTO sentence [{"id":{"d":999,"n":1,"t":1},"speaker":"A","text":"カフェラテができました。どうぞ召し上がりください。"},{"id":{"d":999,"n":2,"t":1},"speaker":"B","text":"ミルクがふっくらと泡立っていますね。ミルク泡だて器を使いましたか？"},{"id":{"d":999,"n":3,"t":1},"speaker":"A","text":"もちろんミルク泡だて器を使います。"},{"id":{"d":999,"n":4,"t":1},"speaker":"B","text":"私もミルク泡だて器を使いますが、このようなふっくらとした泡にはなりません。"},{"id":{"d":999,"n":5,"t":1},"speaker":"A","text":"こつがあるのです。私が発見しました。"},{"id":{"d":999,"n":6,"t":1},"speaker":"B","text":"ぜひ教えてください。"},{"id":{"d":999,"n":7,"t":1},"speaker":"A","text":"簡単なことです。ミルク泡だて器で泡を立てた後、電子レンジで温めるのです。"},{"id":{"d":999,"n":8,"t":1},"speaker":"B","text":"それでこのようにふっくらとするのですか？"},{"id":{"d":999,"n":9,"t":1},"speaker":"A","text":"あなたも試してみるといいです。ただし、吹きこぼれないように注意してください。"},{"id":{"d":999,"n":10,"t":1},"speaker":"B","text":"今度やってみます。"}];
INSERT INTO sentence [{"id":{"d":1000,"n":1,"t":1},"speaker":"A","text":"今日は午後になると寒くなりますから、上着を持っていく方がいいですよ。"},{"id":{"d":1000,"n":2,"t":1},"speaker":"B","text":"最近は暑くなったり寒くなったり、目まぐるしく変わりますね。"},{"id":{"d":1000,"n":3,"t":1},"speaker":"A","text":"昨日はとても暑くて、Tシャツ1枚で十分でした。"},{"id":{"d":1000,"n":4,"t":1},"speaker":"B","text":"冬のセーターなどを、いつしまったらよいのか迷います。"},{"id":{"d":1000,"n":5,"t":1},"speaker":"A","text":"梅雨が明けるまで、このような天気が続きそうです。"},{"id":{"d":1000,"n":6,"t":1},"speaker":"B","text":"梅雨はいつ明けるのでしょうか？"},{"id":{"d":1000,"n":7,"t":1},"speaker":"A","text":"天気予報では今年の梅雨は長く続くそうですよ。"},{"id":{"d":1000,"n":8,"t":1},"speaker":"B","text":"7月いっぱい続きますか？"},{"id":{"d":1000,"n":9,"t":1},"speaker":"A","text":"それほどではないでしょうが、20日前後までは続くそうです。"},{"id":{"d":1000,"n":10,"t":1},"speaker":"B","text":"いつまでもうっとうしいことですね。"}];
INSERT INTO sentence [{"id":{"d":1001,"n":1,"t":1},"speaker":"A","text":"今夜はステーキですか。おいしそうですね。"},{"id":{"d":1001,"n":2,"t":1},"speaker":"B","text":"どこの肉だと思いますか？当ててみてください。"},{"id":{"d":1001,"n":3,"t":1},"speaker":"A","text":"そう言うからには、高級な肉なのでしょうね。分かりません。教えてください。"},{"id":{"d":1001,"n":4,"t":1},"speaker":"B","text":"驚かないでください。神戸牛です。"},{"id":{"d":1001,"n":5,"t":1},"speaker":"A","text":"あの有名な神戸牛ですか。よく手に入りましたね。"},{"id":{"d":1001,"n":6,"t":1},"speaker":"B","text":"ふるさと納税の返礼品なのです。"},{"id":{"d":1001,"n":7,"t":1},"speaker":"A","text":"早速食べてみましょう。柔らかくて、口の中でとろけるようですね。"},{"id":{"d":1001,"n":8,"t":1},"speaker":"B","text":"さすが神戸牛ですね。甘味が口の中に広がって、いつまでも消えません。"}];
INSERT INTO sentence [{"id":{"d":1002,"n":1,"t":1},"speaker":"A","text":"階段が急なので、両親には上り下りするのがきついそうです。"},{"id":{"d":1002,"n":2,"t":1},"speaker":"B","text":"なるほど、これは急ですね。お年寄りにはきついですね。"},{"id":{"d":1002,"n":3,"t":1},"speaker":"A","text":"それに、足を踏み外すと下まで転げ落ちそうで、危ないのです。"},{"id":{"d":1002,"n":4,"t":1},"speaker":"B","text":"踊り場もありませんので、途中で止まることもできませんね。"},{"id":{"d":1002,"n":5,"t":1},"speaker":"A","text":"それでリフォームをお願いしたいのですが、何しろ狭いのでどうしたらよいのか分かりません。"},{"id":{"d":1002,"n":6,"t":1},"speaker":"B","text":"それでは、いっそのこと、らせん階段にしたらどうですか？"},{"id":{"d":1002,"n":7,"t":1},"speaker":"A","text":"そのようなことができるのですか？"},{"id":{"d":1002,"n":8,"t":1},"speaker":"B","text":"はい、それなら場所もとりませんし、手すりにつかまって降りられるので安全です。"},{"id":{"d":1002,"n":9,"t":1},"speaker":"A","text":"それではお願いすることにします。"}];
INSERT INTO sentence [{"id":{"d":1003,"n":1,"t":1},"speaker":"A","text":"まだ6月ですが、連日、35度を超えています。"},{"id":{"d":1003,"n":2,"t":1},"speaker":"B","text":"まったく暑いですね。外出するのが怖いくらいです。"},{"id":{"d":1003,"n":3,"t":1},"speaker":"A","text":"6月にしては記録的な暑さだそうです。"},{"id":{"d":1003,"n":4,"t":1},"speaker":"B","text":"今からこのように暑いのでは、夏になったらどれだけ暑くなることでしょう。"},{"id":{"d":1003,"n":5,"t":1},"speaker":"A","text":"まだ夏本番ではありませんが、テレビでは連日熱中症のニュースが報道されていますね。"},{"id":{"d":1003,"n":6,"t":1},"speaker":"B","text":"はい。昨日は、学校の生徒たちが集団で熱中症になったと言っていました。"},{"id":{"d":1003,"n":7,"t":1},"speaker":"A","text":"90歳の老人が家の中で熱中症で亡くなっていたのが発見されたとも報道されていました。"},{"id":{"d":1003,"n":8,"t":1},"speaker":"B","text":"水分をよく補給して、熱中症にならないように気を付けましょう。"},{"id":{"d":1003,"n":9,"t":1},"speaker":"A","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":1004,"n":1,"t":1},"speaker":"A","text":"仕事着が破れてしまいました。"},{"id":{"d":1004,"n":2,"t":1},"speaker":"B","text":"見せてごらんなさい。どこですか？"},{"id":{"d":1004,"n":3,"t":1},"speaker":"A","text":"ここです。ズボンの内側がひどく破れています。"},{"id":{"d":1004,"n":4,"t":1},"speaker":"B","text":"どうしてこのような所が破れてしまったのですか？"},{"id":{"d":1004,"n":5,"t":1},"speaker":"A","text":"柵をまたごうとしたら、くぎに引っかけてしまったのです。"},{"id":{"d":1004,"n":6,"t":1},"speaker":"B","text":"まあ。けがはありませんでしたか？"},{"id":{"d":1004,"n":7,"t":1},"speaker":"A","text":"幸いなことに、けがはありませんでした。"},{"id":{"d":1004,"n":8,"t":1},"speaker":"B","text":"それはよかったです。繕ってあげますから、脱いでおきなさい。"},{"id":{"d":1004,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1005,"n":1,"t":1},"speaker":"A","text":"今日のお昼はどこで食べますか？"},{"id":{"d":1005,"n":2,"t":1},"speaker":"B","text":"社員食堂も良いですが、少し飽きてきましたね。どこかお薦めの店はありますか？"},{"id":{"d":1005,"n":3,"t":1},"speaker":"A","text":"最近、近くにイタリアンレストランができましたよ。"},{"id":{"d":1005,"n":4,"t":1},"speaker":"B","text":"イタリアンですか。"},{"id":{"d":1005,"n":5,"t":1},"speaker":"A","text":"気が進みませんか？"},{"id":{"d":1005,"n":6,"t":1},"speaker":"B","text":"そういうわけでもないのですが、評判はどうですか？"},{"id":{"d":1005,"n":7,"t":1},"speaker":"A","text":"なかなか良いようですよ。行列もできていますし。"},{"id":{"d":1005,"n":8,"t":1},"speaker":"B","text":"ランチはいくらぐらいですか？"},{"id":{"d":1005,"n":9,"t":1},"speaker":"A","text":"1,000円と1,500円の2種類です。"},{"id":{"d":1005,"n":10,"t":1},"speaker":"B","text":"まあ、相当な値段ですね。"},{"id":{"d":1005,"n":11,"t":1},"speaker":"A","text":"試しにどうですか？"},{"id":{"d":1005,"n":12,"t":1},"speaker":"B","text":"それではそこにしましょう。"}];
INSERT INTO sentence [{"id":{"d":1006,"n":1,"t":1},"speaker":"A","text":"借家と持ち家ではどちらが良いですかね？"},{"id":{"d":1006,"n":2,"t":1},"speaker":"B","text":"私は借家の方がいいと思います。自由に引っ越しができますから。"},{"id":{"d":1006,"n":3,"t":1},"speaker":"A","text":"それは借家のメリットですね。でも、借家だとずっと家賃を払い続けなければいけませんよ。"},{"id":{"d":1006,"n":4,"t":1},"speaker":"B","text":"持ち家だって、ローンの返済に加えて、固定資産税や火災保険、修理費などいろいろ費用がかかります。"},{"id":{"d":1006,"n":5,"t":1},"speaker":"A","text":"借家にしろ持ち家にしろ、それぞれにメリットとデメリットがあるということですね。"},{"id":{"d":1006,"n":6,"t":1},"speaker":"B","text":"どういうライフプランを持っているかによるのでしょうね。"},{"id":{"d":1006,"n":7,"t":1},"speaker":"A","text":"私はどちらかというと、持ち家の方がいいです。"},{"id":{"d":1006,"n":8,"t":1},"speaker":"B","text":"私はやはり借家の方がいいです。"}];
INSERT INTO sentence [{"id":{"d":1007,"n":1,"t":1},"speaker":"A","text":"救急車をお願いしたいのですが。"},{"id":{"d":1007,"n":2,"t":1},"speaker":"B","text":"どうしましたか？"},{"id":{"d":1007,"n":3,"t":1},"speaker":"A","text":"生徒が運動中に気分が悪くなって倒れたのです。"},{"id":{"d":1007,"n":4,"t":1},"speaker":"B","text":"詳しい症状を教えてください。"},{"id":{"d":1007,"n":5,"t":1},"speaker":"A","text":"手足がまひしていて、意識がもうろうとしています。"},{"id":{"d":1007,"n":6,"t":1},"speaker":"B","text":"嘔吐はしていますか？"},{"id":{"d":1007,"n":7,"t":1},"speaker":"A","text":"少し前まで嘔吐していましたが、今はしていません。"},{"id":{"d":1007,"n":8,"t":1},"speaker":"B","text":"重度の熱中症の可能性がありますから、救急車を派遣します。"},{"id":{"d":1007,"n":9,"t":1},"speaker":"A","text":"よろしくお願いします。"},{"id":{"d":1007,"n":10,"t":1},"speaker":"B","text":"その間、涼しい所に連れて行って、横向きに寝かせ、気道を確保するようにしてください。"},{"id":{"d":1007,"n":11,"t":1},"speaker":"A","text":"分かりました。お待ちしています。"}];
INSERT INTO sentence [{"id":{"d":1008,"n":1,"t":1},"speaker":"A","text":"着替えは持ちましたか？"},{"id":{"d":1008,"n":2,"t":1},"speaker":"B","text":"いえ、持っていません。"},{"id":{"d":1008,"n":3,"t":1},"speaker":"A","text":"Tシャツを1枚、持っていきなさい。"},{"id":{"d":1008,"n":4,"t":1},"speaker":"B","text":"荷物になってしまいます。"},{"id":{"d":1008,"n":5,"t":1},"speaker":"A","text":"Tシャツ1枚ぐらい、ナップザックに入るでしょう？"},{"id":{"d":1008,"n":6,"t":1},"speaker":"B","text":"ナップザックはもういっぱいです。これ以上入りません。"},{"id":{"d":1008,"n":7,"t":1},"speaker":"A","text":"暑くなるのですから、汗をかきますよ。"},{"id":{"d":1008,"n":8,"t":1},"speaker":"B","text":"大丈夫です。そのまま着ていれば乾いてしまいます。"},{"id":{"d":1008,"n":9,"t":1},"speaker":"A","text":"しょうがありませんね。風邪をひいても知りませんよ。"}];
INSERT INTO sentence [{"id":{"d":1009,"n":1,"t":1},"speaker":"A","text":"今週末は、友達を呼んで一緒にバーベキューをしましょう。"},{"id":{"d":1009,"n":2,"t":1},"speaker":"B","text":"久しぶりですね。最後にバーベキューをしたのはいつでしたか？"},{"id":{"d":1009,"n":3,"t":1},"speaker":"A","text":"3年前になります。その頃から個々が忙しくなり、みんなで集まることができなくなりました。"},{"id":{"d":1009,"n":4,"t":1},"speaker":"B","text":"最近は仕事にも慣れてきて時間も作りやすくなったので、私もそろそろと思っていました。"},{"id":{"d":1009,"n":5,"t":1},"speaker":"A","text":"みんなに声を掛けてみていますが、全員参加できそうですよ。"},{"id":{"d":1009,"n":6,"t":1},"speaker":"B","text":"それはうれしいですね。うちでバーベキューをするのが毎年の楽しみだったのですから。"},{"id":{"d":1009,"n":7,"t":1},"speaker":"A","text":"おいしい食材も用意しなくてはいけませんね。"},{"id":{"d":1009,"n":8,"t":1},"speaker":"B","text":"今日はちょうどスーパーマーケットでセールをやっていますから、早速買いに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1010,"n":1,"t":1},"speaker":"A","text":"新しい回転ずしの店ができましたね。"},{"id":{"d":1010,"n":2,"t":1},"speaker":"B","text":"ビルの向かいのお店ですね。行ってみたのですか？"},{"id":{"d":1010,"n":3,"t":1},"speaker":"A","text":"はい、先日行ってきました。お客がたくさん来ていました。"},{"id":{"d":1010,"n":4,"t":1},"speaker":"B","text":"どうですか？おいしかったですか？"},{"id":{"d":1010,"n":5,"t":1},"speaker":"A","text":"はい、なかなかおいしかったです。店員さんのサービスも行き届いていました。"},{"id":{"d":1010,"n":6,"t":1},"speaker":"B","text":"回転ずし業界も競争が激しいですから、いろいろと工夫をしなければいけないのでしょうね。"},{"id":{"d":1010,"n":7,"t":1},"speaker":"A","text":"素材の新鮮さを競ったり、値段の安さを競ったり、いろいろ工夫をしているようですね。おかげでおいしいおすしを安くいただくことができました。"}];
INSERT INTO sentence [{"id":{"d":1011,"n":1,"t":1},"speaker":"A","text":"火災保険に入りたいのですが。"},{"id":{"d":1011,"n":2,"t":1},"speaker":"B","text":"かしこまりました。お住まいはマンションですか？一戸建てですか？"},{"id":{"d":1011,"n":3,"t":1},"speaker":"A","text":"一戸建てです。最近、中古物件を買って入居しました。"},{"id":{"d":1011,"n":4,"t":1},"speaker":"B","text":"それでは、こちらの保険がお薦めです。"},{"id":{"d":1011,"n":5,"t":1},"speaker":"A","text":"こちらの火災保険では物置や塀なども補償してもらえるのですか？"},{"id":{"d":1011,"n":6,"t":1},"speaker":"B","text":"はい、補償いたします。"},{"id":{"d":1011,"n":7,"t":1},"speaker":"A","text":"例えば、台風で木が倒れて家が破損した場合などはどうですか？"},{"id":{"d":1011,"n":8,"t":1},"speaker":"B","text":"もちろん、その場合も補償いたします。"},{"id":{"d":1011,"n":9,"t":1},"speaker":"A","text":"地震はどうですか？"},{"id":{"d":1011,"n":10,"t":1},"speaker":"B","text":"火災保険では地震は補償されません。地震保険にも加入されることをお勧めします。"},{"id":{"d":1011,"n":11,"t":1},"speaker":"A","text":"分かりました。それでは、火災保険と地震保険に同時に加入します。"}];
INSERT INTO sentence [{"id":{"d":1012,"n":1,"t":1},"speaker":"A","text":"魚に寄生するアニサキスというのを知っていますか？"},{"id":{"d":1012,"n":2,"t":1},"speaker":"B","text":"いいえ、知りません。気味が悪いですね。"},{"id":{"d":1012,"n":3,"t":1},"speaker":"A","text":"刺身などを生で食べるときには気を付けなくてはいけません。"},{"id":{"d":1012,"n":4,"t":1},"speaker":"B","text":"何か病気にでもなるのですか？"},{"id":{"d":1012,"n":5,"t":1},"speaker":"A","text":"胃や腸にひどい痛みを引き起こすらしいです。"},{"id":{"d":1012,"n":6,"t":1},"speaker":"B","text":"それほどひどいのですか？"},{"id":{"d":1012,"n":7,"t":1},"speaker":"A","text":"経験した人によると、七転八倒の苦しみだそうです。"},{"id":{"d":1012,"n":8,"t":1},"speaker":"B","text":"薬はありませんか？"},{"id":{"d":1012,"n":9,"t":1},"speaker":"A","text":"薬はありません。胃カメラで除去するか、腸に達した場合は便になって出るのを待つしかないようです。"},{"id":{"d":1012,"n":10,"t":1},"speaker":"B","text":"それは怖いですね。気を付けるようにしましょう。"}];
INSERT INTO sentence [{"id":{"d":1013,"n":1,"t":1},"speaker":"A","text":"今日は大掃除ですから、太郎ちゃんも手伝ってください。"},{"id":{"d":1013,"n":2,"t":1},"speaker":"B","text":"今日はお昼ご飯の後、和夫君の所で一緒に遊ぶことになっています。"},{"id":{"d":1013,"n":3,"t":1},"speaker":"A","text":"それなら、午前中だけでもよいですから、お手伝いしてください。"},{"id":{"d":1013,"n":4,"t":1},"speaker":"B","text":"分かりました。何をすればよいですか？"},{"id":{"d":1013,"n":5,"t":1},"speaker":"A","text":"そこにあるほうきで、クモの巣を取ってください。"},{"id":{"d":1013,"n":6,"t":1},"speaker":"B","text":"クモの巣ですか？虫は嫌いなのでやりたくありません。"},{"id":{"d":1013,"n":7,"t":1},"speaker":"A","text":"では、窓磨きをお願いします。それならよいですか？"},{"id":{"d":1013,"n":8,"t":1},"speaker":"B","text":"分かりました。窓磨きをします。雑巾はこれでよいですか？"},{"id":{"d":1013,"n":9,"t":1},"speaker":"A","text":"はい。バケツはお風呂場にあるのを使ってください。"}];
INSERT INTO sentence [{"id":{"d":1014,"n":1,"t":1},"speaker":"A","text":"先ほど、無線放送がありませんでしたか？"},{"id":{"d":1014,"n":2,"t":1},"speaker":"B","text":"ありました。どうやら、村の西側でぼやがあったそうです。"},{"id":{"d":1014,"n":3,"t":1},"speaker":"A","text":"ぼやで済んだのですか？"},{"id":{"d":1014,"n":4,"t":1},"speaker":"B","text":"そのようですよ。近所の人が気付いて、すぐに消防署に通報したのだそうです。"},{"id":{"d":1014,"n":5,"t":1},"speaker":"A","text":"そういえば、消防車のサイレンが聞こえたような気がします。"},{"id":{"d":1014,"n":6,"t":1},"speaker":"B","text":"大事に至る前に消し止めたそうです。"},{"id":{"d":1014,"n":7,"t":1},"speaker":"A","text":"それはよかったですね。"},{"id":{"d":1014,"n":8,"t":1},"speaker":"B","text":"それで、火の元に気を付けてくださいという放送が流れていました。"}];
INSERT INTO sentence [{"id":{"d":1015,"n":1,"t":1},"speaker":"A","text":"そろそろ冬服を出さなければいけませんね。"},{"id":{"d":1015,"n":2,"t":1},"speaker":"B","text":"まだ早いのではありませんか？11月になったばかりですよ。"},{"id":{"d":1015,"n":3,"t":1},"speaker":"A","text":"いいえ、寒くなってからでは遅過ぎます。傷んでいないか、かびていないか、点検する必要もありますから。"},{"id":{"d":1015,"n":4,"t":1},"speaker":"B","text":"分かりました。今持ってきます。どこにありますか？"},{"id":{"d":1015,"n":5,"t":1},"speaker":"A","text":"納戸の奥に、衣装ケースがあるでしょう？そこに入っています。"},{"id":{"d":1015,"n":6,"t":1},"speaker":"B","text":"どこですか？見当たりません。"},{"id":{"d":1015,"n":7,"t":1},"speaker":"A","text":"そのようなはずはありませんよ。よく探してください。"},{"id":{"d":1015,"n":8,"t":1},"speaker":"B","text":"ありました。布団袋の陰になっていました。ケースは5つありますが、全部持っていきますか？"},{"id":{"d":1015,"n":9,"t":1},"speaker":"A","text":"一応、全部持ってきてください。中を見てみますから。"},{"id":{"d":1015,"n":10,"t":1},"speaker":"B","text":"分かりました。今、持っていきます。"}];
INSERT INTO sentence [{"id":{"d":1016,"n":1,"t":1},"speaker":"A","text":"今日は料理する時間がありませんので、冷凍食品でよいですか？"},{"id":{"d":1016,"n":2,"t":1},"speaker":"B","text":"よいですよ。何がありますか？"},{"id":{"d":1016,"n":3,"t":1},"speaker":"A","text":"ビーフシチューとカレー、それにパスタが何種類かあります。"},{"id":{"d":1016,"n":4,"t":1},"speaker":"B","text":"パスタがいいですね。どのような種類がありますか？"},{"id":{"d":1016,"n":5,"t":1},"speaker":"A","text":"ミートソースとカルボナーラ、ペペロンチーノと和風めんたいこ、それにナポリタンがあります。"},{"id":{"d":1016,"n":6,"t":1},"speaker":"B","text":"私はあっさりとした和風めんたいこがいいです。"},{"id":{"d":1016,"n":7,"t":1},"speaker":"A","text":"私はペペロンチーノにします。"},{"id":{"d":1016,"n":8,"t":1},"speaker":"B","text":"私がサラダを作りましょう。材料がありますか？"},{"id":{"d":1016,"n":9,"t":1},"speaker":"A","text":"レタスとキュウリ、ミニトマトと生ハムがあります。"},{"id":{"d":1016,"n":10,"t":1},"speaker":"B","text":"それらを使って、簡単に作ります。その間に、パスタを温めておいてください。"}];
INSERT INTO sentence [{"id":{"d":1017,"n":1,"t":1},"speaker":"A","text":"家のすぐそばに横浜スタジアムがあります。"},{"id":{"d":1017,"n":2,"t":1},"speaker":"B","text":"うらやましいですね。プロ野球の試合をいつでも見に行けますね。"},{"id":{"d":1017,"n":3,"t":1},"speaker":"A","text":"それがそうでもないのです。いつでも見に行けると思ったら、なかなか行かないものです。"},{"id":{"d":1017,"n":4,"t":1},"speaker":"B","text":"それはもったいないです。私だったら、毎日でも見に行きますね。"},{"id":{"d":1017,"n":5,"t":1},"speaker":"A","text":"DeNAベイスターズがお好きなのですか？"},{"id":{"d":1017,"n":6,"t":1},"speaker":"B","text":"はい、横浜ベイスターズの時代からのファンなのです。"},{"id":{"d":1017,"n":7,"t":1},"speaker":"A","text":"球場にはよくおいでになるのですか？"},{"id":{"d":1017,"n":8,"t":1},"speaker":"B","text":"はい、よく行きます。ですから、すぐそばに住んでいながら行かないのはもったいないと思います。"},{"id":{"d":1017,"n":9,"t":1},"speaker":"A","text":"それは代わってあげたいくらいですね。"}];
INSERT INTO sentence [{"id":{"d":1018,"n":1,"t":1},"speaker":"A","text":"郵便局が閉鎖されていますね。"},{"id":{"d":1018,"n":2,"t":1},"speaker":"B","text":"先週から閉まっていました。移転するのだそうですよ。"},{"id":{"d":1018,"n":3,"t":1},"speaker":"A","text":"どこに移転するのですか？"},{"id":{"d":1018,"n":4,"t":1},"speaker":"B","text":"200メートルほど離れた大通り沿いに移転します。"},{"id":{"d":1018,"n":5,"t":1},"speaker":"A","text":"それまで、どこの郵便局を利用すればよいのでしょうか？"},{"id":{"d":1018,"n":6,"t":1},"speaker":"B","text":"隣町の郵便局を利用するしかありませんね。"},{"id":{"d":1018,"n":7,"t":1},"speaker":"A","text":"それはちょっと遠過ぎますね。新しい郵便局はいつできるのですか？"},{"id":{"d":1018,"n":8,"t":1},"speaker":"B","text":"半年ぐらい先になるそうです。"},{"id":{"d":1018,"n":9,"t":1},"speaker":"A","text":"それまでは隣町の郵便局で我慢するしかないのでしょうか？"},{"id":{"d":1018,"n":10,"t":1},"speaker":"B","text":"コンビニエンスストアにもポストがありますよ。"},{"id":{"d":1018,"n":11,"t":1},"speaker":"A","text":"そうでしたね。それではコンビニエンスストアを利用することにしましょう。"}];
INSERT INTO sentence [{"id":{"d":1019,"n":1,"t":1},"speaker":"A","text":"何か重要な手紙は届いていますか？"},{"id":{"d":1019,"n":2,"t":1},"speaker":"B","text":"いえ、重要そうなものは届いていません。ダイレクトメールや広告がいくつか来ています。"},{"id":{"d":1019,"n":3,"t":1},"speaker":"A","text":"どのようなダイレクトメールが来ていますか？"},{"id":{"d":1019,"n":4,"t":1},"speaker":"B","text":"パソコンの広告と、製薬会社の広告と、ワインの通販の広告ですね。"},{"id":{"d":1019,"n":5,"t":1},"speaker":"A","text":"今のところ、必要なものはなさそうですね。"},{"id":{"d":1019,"n":6,"t":1},"speaker":"B","text":"全部捨ててもよいですか？"},{"id":{"d":1019,"n":7,"t":1},"speaker":"A","text":"念のため、後で見てみますから、テーブルの上に置いておいてください。"},{"id":{"d":1019,"n":8,"t":1},"speaker":"B","text":"分かりました。ここに置いておきます。"}];
INSERT INTO sentence [{"id":{"d":1020,"n":1,"t":1},"speaker":"A","text":"随分厚着をしていますね。"},{"id":{"d":1020,"n":2,"t":1},"speaker":"B","text":"朝から少し寒気がするのです。"},{"id":{"d":1020,"n":3,"t":1},"speaker":"A","text":"それはいけませんね。熱を測りましたか？"},{"id":{"d":1020,"n":4,"t":1},"speaker":"B","text":"はい、測りました。38度ありました。"},{"id":{"d":1020,"n":5,"t":1},"speaker":"A","text":"風邪をひいたのではありませんか？何か心当たりがありますか？"},{"id":{"d":1020,"n":6,"t":1},"speaker":"B","text":"昨夜、お風呂から上がったあと、しばらくシャツ1枚で過ごしていました。"},{"id":{"d":1020,"n":7,"t":1},"speaker":"A","text":"きっと、そのせいですよ。医者に診てもらった方がいいですよ。"},{"id":{"d":1020,"n":8,"t":1},"speaker":"B","text":"まずは風邪薬を飲んでしばらく様子を見てみます。ひどくなったら病院へいきます。"},{"id":{"d":1020,"n":9,"t":1},"speaker":"A","text":"お大事になさってくださいね。"}];
INSERT INTO sentence [{"id":{"d":1021,"n":1,"t":1},"speaker":"A","text":"夕食は何にしましょうか？何が食べたいですか？"},{"id":{"d":1021,"n":2,"t":1},"speaker":"B","text":"何が食べたいかと言われても、すぐには思いつきません。何があるのですか？"},{"id":{"d":1021,"n":3,"t":1},"speaker":"A","text":"魚ではサンマとサバとサケとイワシがあります。肉では鶏のもも肉と牛肉の切り落としと豚肉のスペアリブがあります。"},{"id":{"d":1021,"n":4,"t":1},"speaker":"B","text":"豚肉のスペアリブが食べたいです。ワインと一緒にどうですか？"},{"id":{"d":1021,"n":5,"t":1},"speaker":"A","text":"それでは豚肉のスペアリブにしましょう。トマト缶とタマネギ、それにニンニクを入れて、圧力鍋で煮ればすぐにできます。"},{"id":{"d":1021,"n":6,"t":1},"speaker":"B","text":"おいしそうですね。それでは、私はサラダを作りましょう。何か材料がありますか？"},{"id":{"d":1021,"n":7,"t":1},"speaker":"A","text":"レタスとパプリカとブロッコリーとアボカドと生ハムがあります。"},{"id":{"d":1021,"n":8,"t":1},"speaker":"B","text":"いいですね。それを全部使って作りましょう。"}];
INSERT INTO sentence [{"id":{"d":1022,"n":1,"t":1},"speaker":"A","text":"この辺りに駐車場はありますか？スマートフォンで調べてもらえますか？"},{"id":{"d":1022,"n":2,"t":1},"speaker":"B","text":"ちょっと待ってくださいね。今、調べます。"},{"id":{"d":1022,"n":3,"t":1},"speaker":"A","text":"もしあっても、日曜日だからどこも満車かもしれませんね。"},{"id":{"d":1022,"n":4,"t":1},"speaker":"B","text":"ありました。由比ケ浜駅の近くに1つあります。"},{"id":{"d":1022,"n":5,"t":1},"speaker":"A","text":"どう行けばよいのですか？"},{"id":{"d":1022,"n":6,"t":1},"speaker":"B","text":"この道路を真っすぐ行って、3つ目の信号を左に曲がります。"},{"id":{"d":1022,"n":7,"t":1},"speaker":"A","text":"この信号を左ですね。"},{"id":{"d":1022,"n":8,"t":1},"speaker":"B","text":"それから、2つ目の信号を右に曲がると、駐車場があります。"},{"id":{"d":1022,"n":9,"t":1},"speaker":"A","text":"この駐車場ですね。やはり満車のようですね。"},{"id":{"d":1022,"n":10,"t":1},"speaker":"B","text":"そうですか。他に駐車できる場があるか調べてみます。"}];
INSERT INTO sentence [{"id":{"d":1023,"n":1,"t":1},"speaker":"A","text":"サイレンの音がします。"},{"id":{"d":1023,"n":2,"t":1},"speaker":"B","text":"あれは消防車のサイレンですね。"},{"id":{"d":1023,"n":3,"t":1},"speaker":"A","text":"こちらに近づいてくるようですよ。"},{"id":{"d":1023,"n":4,"t":1},"speaker":"B","text":"近くで火事が起こったのでしょうか？ちょっと窓から見てください。"},{"id":{"d":1023,"n":5,"t":1},"speaker":"A","text":"煙が昇っているのが見えます。"},{"id":{"d":1023,"n":6,"t":1},"speaker":"B","text":"近いですか？"},{"id":{"d":1023,"n":7,"t":1},"speaker":"A","text":"はい、とても近いです。100メートルほど先でしょうか？"},{"id":{"d":1023,"n":8,"t":1},"speaker":"B","text":"それは大変です。逃げなくてよいでしょうか？"},{"id":{"d":1023,"n":9,"t":1},"speaker":"A","text":"こちらが風上なので大丈夫だと思います。消防車が続々と何台も来ているようです。"},{"id":{"d":1023,"n":10,"t":1},"speaker":"B","text":"それほどひどいのでしょうか。早く消し止められるといいですね。"}];
INSERT INTO sentence [{"id":{"d":1024,"n":1,"t":1},"speaker":"A","text":"スニーカーが擦り切れてしまいました。"},{"id":{"d":1024,"n":2,"t":1},"speaker":"B","text":"同じスニーカーばかり履くからですよ。"},{"id":{"d":1024,"n":3,"t":1},"speaker":"A","text":"気に入っているスニーカーなので、こればかり履いていたのです。"},{"id":{"d":1024,"n":4,"t":1},"speaker":"B","text":"他にないのですか？"},{"id":{"d":1024,"n":5,"t":1},"speaker":"A","text":"あることはありますが、あまり気に入らないのです。新しいスニーカーが欲しいです。"},{"id":{"d":1024,"n":6,"t":1},"speaker":"B","text":"そのようなぜいたくを言ってはいけません。お金は出してあげませんよ。"},{"id":{"d":1024,"n":7,"t":1},"speaker":"A","text":"結構です。自分でアルバイトをしてお金をためます。"},{"id":{"d":1024,"n":8,"t":1},"speaker":"B","text":"勉強はどうするのですか？"},{"id":{"d":1024,"n":9,"t":1},"speaker":"A","text":"勉強もちゃんとします。"},{"id":{"d":1024,"n":10,"t":1},"speaker":"B","text":"約束ですよ。"}];
INSERT INTO sentence [{"id":{"d":1025,"n":1,"t":1},"speaker":"A","text":"昼飯はどこにしましょうか？"},{"id":{"d":1025,"n":2,"t":1},"speaker":"B","text":"立ち食いのすしというのはどうですか？"},{"id":{"d":1025,"n":3,"t":1},"speaker":"A","text":"私は行ったことがありませんが、どのような所ですか？"},{"id":{"d":1025,"n":4,"t":1},"speaker":"B","text":"文字通り、立って食うすし屋です。カウンターの前に立って、注文して食べるのです。"},{"id":{"d":1025,"n":5,"t":1},"speaker":"A","text":"おいしいのですか？"},{"id":{"d":1025,"n":6,"t":1},"speaker":"B","text":"材料も新鮮で、おいしいですよ。ただし、窮屈ですけれどね。"},{"id":{"d":1025,"n":7,"t":1},"speaker":"A","text":"混んでいるのですか？"},{"id":{"d":1025,"n":8,"t":1},"speaker":"B","text":"狭い所に客がひしめいているので、みんなカウンターに斜めに立っているのです。"},{"id":{"d":1025,"n":9,"t":1},"speaker":"A","text":"何だか満員電車のような光景が目に浮かびますね。"}];
INSERT INTO sentence [{"id":{"d":1026,"n":1,"t":1},"speaker":"A","text":"先週の日曜日にアウトレットモールへ買い物に行ってきました。"},{"id":{"d":1026,"n":2,"t":1},"speaker":"B","text":"何か買いたいものがあったのですか？"},{"id":{"d":1026,"n":3,"t":1},"speaker":"A","text":"はい。仕事に行く時に履く靴を買いたいと思い、行ってきました。"},{"id":{"d":1026,"n":4,"t":1},"speaker":"B","text":"良い靴は見つかりましたか？"},{"id":{"d":1026,"n":5,"t":1},"speaker":"A","text":"いいえ、残念ながら気に入った靴は見つかりませんでした。靴は買いませんでしたが、ジャケットを購入しました。以前から気になっていたのですが、値段が高くて買えなかったのです。それが4割引になっていたので買いました。"},{"id":{"d":1026,"n":6,"t":1},"speaker":"B","text":"それは良いお買い物をしましたね。"},{"id":{"d":1026,"n":7,"t":1},"speaker":"A","text":"はい、アウトレット価格からさらに安い値段で購入できたので、とても満足しています。"}];
INSERT INTO sentence [{"id":{"d":1027,"n":1,"t":1},"speaker":"A","text":"桜井さん、お仕事の区切りがよければ食堂にお昼ご飯を食べに行きませんか？"},{"id":{"d":1027,"n":2,"t":1},"speaker":"B","text":"ちょうど良いタイミングです。行きましょう。"},{"id":{"d":1027,"n":3,"t":1},"speaker":"A","text":"今日の日替わり定食は何でしょうね。昨日がアジフライ定食で揚げ物だったから、今日は炒め物かしら。"},{"id":{"d":1027,"n":4,"t":1},"speaker":"B","text":"正解です。今日は野菜炒め定食です。佐藤さんは日替わり定食にしますか？"},{"id":{"d":1027,"n":5,"t":1},"speaker":"A","text":"はい、最近野菜不足気味なので野菜炒めはありがたい献立です。桜井さんは何にしますか？"},{"id":{"d":1027,"n":6,"t":1},"speaker":"B","text":"私は麺類を食べたい気分です。ぶっかけうどんと山菜の天ぷらにしようと思います。"},{"id":{"d":1027,"n":7,"t":1},"speaker":"A","text":"今日はデザートも充実しているみたいです。私は杏仁豆腐を付けようかしら。桜井さんもどうですか？"},{"id":{"d":1027,"n":8,"t":1},"speaker":"B","text":"最近、体重が増加気味なのですが、大好物のあんみつがあるので、私はあんみつを食べます。"}];
INSERT INTO sentence [{"id":{"d":1028,"n":1,"t":1},"speaker":"A","text":"今日の新聞の折り込み広告の不動産情報に、なかなか魅力的な物件がありました。"},{"id":{"d":1028,"n":2,"t":1},"speaker":"B","text":"僕たちもそろそろ持ち家が欲しいですね。どのような物件か詳しく聞かせてください。"},{"id":{"d":1028,"n":3,"t":1},"speaker":"A","text":"駅から徒歩8分の4LDKの一戸建てで、新築のオール電化の物件です。小学校・中学校も徒歩10分以内にあるようです。スーパーマーケットも近くにあるようです。"},{"id":{"d":1028,"n":4,"t":1},"speaker":"B","text":"それはいいですね。ただ、通勤時間が気になります。何線の沿線ですか？"},{"id":{"d":1028,"n":5,"t":1},"speaker":"A","text":"京浜急行線の金沢八景駅が最寄り駅です。金沢八景駅には日中は快速特急が止まりませんが、朝のラッシュの時間帯には快速特急の停車駅になるようです。"},{"id":{"d":1028,"n":6,"t":1},"speaker":"B","text":"いいですね。でも、家は一生の買い物ですから、慎重に選びましょう。地盤も気になります。"},{"id":{"d":1028,"n":7,"t":1},"speaker":"A","text":"そうですね。ハザードマップで調べますね。"},{"id":{"d":1028,"n":8,"t":1},"speaker":"B","text":"今週の日曜日にドライブがてら見に行きませんか？じっくり検討して十分納得できるものを選びましょう。"}];
INSERT INTO sentence [{"id":{"d":1029,"n":1,"t":1},"speaker":"A","text":"今日は救急車がよく通りますね。"},{"id":{"d":1029,"n":2,"t":1},"speaker":"B","text":"1週間ほど前から、そこの大学病院の救命救急センターにひっきりなしに患者さんが運ばれているそうです。その大学病院に勤めている看護師さんから聞きました。"},{"id":{"d":1029,"n":3,"t":1},"speaker":"A","text":"この異常な暑さですから、熱中症になる人が多いのかもしれませんね。もったいながらずに、エアコンを使わなくてはなりません。"},{"id":{"d":1029,"n":4,"t":1},"speaker":"B","text":"最近のエアコンは省エネの性能が高いですものね。うちは昨年エアコンを買い替えたのですが、1カ月の電気代が1,000円以上安くなりました。"},{"id":{"d":1029,"n":5,"t":1},"speaker":"A","text":"それは大きいですね。わが家のエアコンは20年ほど前のものなのですが、壊れないものですから、買い替えられずにいます。"},{"id":{"d":1029,"n":6,"t":1},"speaker":"B","text":"そうですよね。壊れないと買い替えにくいですよね。まだ使えるのに捨ててしまうなんてエアコンがかわいそうだと思ってしまいますから。"},{"id":{"d":1029,"n":7,"t":1},"speaker":"A","text":"でも、このままでは地球がかわいそうですものね。地球に優しい、省エネのエアコンの購入を検討しようと思います。"},{"id":{"d":1029,"n":8,"t":1},"speaker":"B","text":"新しい機種は、お手入れが不要だったりして、家事のエネルギーのカットにも役立ちます。各社の製品カタログを見比べてみてください。"}];
INSERT INTO sentence [{"id":{"d":1030,"n":1,"t":1},"speaker":"A","text":"もう6月の半ばだというのに、うちはまだ衣替えを済ませていません。"},{"id":{"d":1030,"n":2,"t":1},"speaker":"B","text":"うちもそうです。毎日肌寒くて、衣替えをする気になれませんものね。でも、7月が目前だというのに、このような厚地のパーカーを着ているのは恥ずかしいような気もします。"},{"id":{"d":1030,"n":3,"t":1},"speaker":"A","text":"気温が低いので仕方がありません。無理をして風邪をひいては元も子もありません。"},{"id":{"d":1030,"n":4,"t":1},"speaker":"B","text":"中学生たちは校則で衣替えの日が6月1日と決まっていますから、気候がどうあれ夏服を着なくてはなりませんね。気の毒に思います。"},{"id":{"d":1030,"n":5,"t":1},"speaker":"A","text":"秋の衣替えは10月1日ですよね。最近は温暖化の影響で、10月1日といってもまだまだ暑くて、大汗をかきながら学校に行く中学生を見かけます。"},{"id":{"d":1030,"n":6,"t":1},"speaker":"B","text":"中学校の衣替えについては、もう少し柔軟なルールに変更してもよさそうですね。"},{"id":{"d":1030,"n":7,"t":1},"speaker":"A","text":"同感です。それはそれとしてわが家もなんとか6月中には衣替えを済ませたいものです。できればお天気の良い日に冬服に風を通してからしまいたいのですが。"},{"id":{"d":1030,"n":8,"t":1},"speaker":"B","text":"そうですね。晴れた日が良いですね。天気予報では明後日に晴れマークが出ていました。うちもそろそろ衣替えをしようと思います。"}];
INSERT INTO sentence [{"id":{"d":1031,"n":1,"t":1},"speaker":"A","text":"もしもし、リストランテ・トモさんですか？席の予約をしたいのですが。"},{"id":{"d":1031,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。日にちと時間をお知らせください。"},{"id":{"d":1031,"n":3,"t":1},"speaker":"A","text":"7月11日の17時以降でお願いします。"},{"id":{"d":1031,"n":4,"t":1},"speaker":"B","text":"何名さまでご来店でしょうか？"},{"id":{"d":1031,"n":5,"t":1},"speaker":"A","text":"8名です。"},{"id":{"d":1031,"n":6,"t":1},"speaker":"B","text":"予約状況を調べますので少々お待ちください。7月11日は18時でしたら8名さまのお席をお取りできます。"},{"id":{"d":1031,"n":7,"t":1},"speaker":"A","text":"では、18時に伺います。ディナーのBコースを頂きたいと思います。"},{"id":{"d":1031,"n":8,"t":1},"speaker":"B","text":"承知いたしました。お飲み物はいかがいたしましょう？当日お選びいただくこともできますが。"},{"id":{"d":1031,"n":9,"t":1},"speaker":"A","text":"飲み物は当日決めたいと思います。"},{"id":{"d":1031,"n":10,"t":1},"speaker":"B","text":"かしこまりました。では、お名前とご連絡用のお電話番号をお願いします。"},{"id":{"d":1031,"n":11,"t":1},"speaker":"A","text":"はい。中嶋と申します。電話番号は12-3456-7890です。"},{"id":{"d":1031,"n":12,"t":1},"speaker":"B","text":"ありがとうございます。中嶋さま、7月11日18時のご来店をお待ちしております。"}];
INSERT INTO sentence [{"id":{"d":1032,"n":1,"t":1},"speaker":"A","text":"今日は少し疲れたので、晩ご飯を作らずに外食したいと思います。久しぶりに喫茶ランプに行きませんか？"},{"id":{"d":1032,"n":2,"t":1},"speaker":"B","text":"いいですね。喫茶ランプに行くと言ったら、子どもたちも喜ぶと思いますよ。"},{"id":{"d":1032,"n":3,"t":1},"speaker":"A","text":"喫茶ランプは洋食も和食も中華もあって、ファミリーレストランのようですものね。皆が自分の食べたいものを選ぶことができて良いですよね。"},{"id":{"d":1032,"n":4,"t":1},"speaker":"B","text":"僕はかつ丼にしようと思います。ミックスピザも捨てがたいですね。喫茶ランプに行くとなかなか注文が決められなくて困ってしまいます。"},{"id":{"d":1032,"n":5,"t":1},"speaker":"A","text":"私はカニクリームコロッケが食べたい気分です。食後にはチョコレートパフェを頼んでしまおうかしら。"},{"id":{"d":1032,"n":6,"t":1},"speaker":"B","text":"喫茶ランプはチョコレートケーキも絶品なのを知っていますか？"},{"id":{"d":1032,"n":7,"t":1},"speaker":"A","text":"それは知りませんでした。チョコレートケーキもぜひ食べたいです。チョコレートパフェも食べたいので、あなたがチョコレートケーキを頼んで、私に1口食べさせてください。"},{"id":{"d":1032,"n":8,"t":1},"speaker":"B","text":"僕たち大人でもこれほど迷うのだから、子どもたちはもっと決められないでしょうね。"},{"id":{"d":1032,"n":9,"t":1},"speaker":"A","text":"お食事選びで迷うのも楽しいことですから、たくさん迷わせてあげましょう。"}];
INSERT INTO sentence [{"id":{"d":1033,"n":1,"t":1},"speaker":"A","text":"進学先が決まりましたので、早速下宿先の物件を探し始めましょう。防犯のため、オートロックとテレビ付きインターホンのある物件が良いですね。"},{"id":{"d":1033,"n":2,"t":1},"speaker":"B","text":"お風呂とトイレは別々になっていてほしいと思います。エアコンも設置されていると助かります。"},{"id":{"d":1033,"n":3,"t":1},"speaker":"A","text":"近くにスーパーマーケットがあると文句なしですが、それが無理ならコンビニエンスストアがあることも条件にしましょう。"},{"id":{"d":1033,"n":4,"t":1},"speaker":"B","text":"建物にエレベーターはなくても構いません。トレーニングだと思って階段の上り下りをします。"},{"id":{"d":1033,"n":5,"t":1},"speaker":"A","text":"そうですね、若いのですから頑張って階段を使ってくださいね。まずは大学が扱っている不動産情報に当たってみればよいのでしょうか？"},{"id":{"d":1033,"n":6,"t":1},"speaker":"B","text":"大学が紹介する下宿は古い物件が多いと聞いています。水回りが清潔でないと嫌なので、築年数が10年以内の物件に絞りたいと思いますが、それでよいですか？"},{"id":{"d":1033,"n":7,"t":1},"speaker":"A","text":"耐震性の面からも新しい物件が良いですね。築年数は7年以内のものにしましょう。良い下宿が見つかるといいですね。"},{"id":{"d":1033,"n":8,"t":1},"speaker":"B","text":"はい、新生活の一歩ですから、とてもワクワクします。"}];
INSERT INTO sentence [{"id":{"d":1034,"n":1,"t":1},"speaker":"A","text":"買い物へ出るついでに、いろいろな用事を済ませてこようと思います。留守番をお願いします。"},{"id":{"d":1034,"n":2,"t":1},"speaker":"B","text":"はい、分かりました。洗濯物は取り込みますから、時間を気にしないで出かけてきてください。"},{"id":{"d":1034,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。よろしくお願いします。"},{"id":{"d":1034,"n":4,"t":1},"speaker":"B","text":"どちらまで行く予定ですか？"},{"id":{"d":1034,"n":5,"t":1},"speaker":"A","text":"まず、宅配センターに行って、宅配便を出してきます。その次に郵便局で、振り込みを済ませます。それからJAの直売所で野菜を買って、最後にスーパーマーケットでその他の食材を買って帰ってきます。"},{"id":{"d":1034,"n":6,"t":1},"speaker":"B","text":"あちこちに行かなくてはなりませんので大変ですね。さらに用事を増やして申し訳ありませんが、郵便局に行くのであれば、このはがきを出してきてもらえないでしょうか？"},{"id":{"d":1034,"n":7,"t":1},"speaker":"A","text":"分かりました。他に用事はありませんか？本屋の前も通りますから、ついでにいつもの雑誌を買ってきましょうか？"},{"id":{"d":1034,"n":8,"t":1},"speaker":"B","text":"月刊トマトですね。今月号は買わなくてよいです。今日は暑いので、スーパーマーケットでアイスを買ってきてくれるとうれしいです。"},{"id":{"d":1034,"n":9,"t":1},"speaker":"A","text":"私もアイスを買おうと思っていました。箱入りのアイスにしましょう。"},{"id":{"d":1034,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。今は練乳味のものが食べたい気分です。何かおいしそうなのがあったらお願いします。"},{"id":{"d":1034,"n":11,"t":1},"speaker":"A","text":"分かりました。行ってきます。"},{"id":{"d":1034,"n":12,"t":1},"speaker":"B","text":"行ってらっしゃい。"}];
INSERT INTO sentence [{"id":{"d":1035,"n":1,"t":1},"speaker":"A","text":"お客さま、サイズ違いをお探しでしたら、お手伝いいたします。"},{"id":{"d":1035,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。このワンピースの9号が見当たりませんので、探していただけますか？それから、これは色が濃いので、もっと薄い色があるとうれしいのですが。"},{"id":{"d":1035,"n":3,"t":1},"speaker":"A","text":"9号はバックヤードにありますので、持ってまいります。ネービーの他にサーモンピンクもあり、あちらにディスプレーしてあります。サーモンピンクの9号もお持ちしましょうか？"},{"id":{"d":1035,"n":4,"t":1},"speaker":"B","text":"はい。両方持ってきてください。"},{"id":{"d":1035,"n":5,"t":1},"speaker":"A","text":"お客さま、お持ちしました。ちょうど試着室が空いていますので、どうぞお使いください。"},{"id":{"d":1035,"n":6,"t":1},"speaker":"B","text":"実際に合わせてみると、色が濃くてもネービーは暑苦しく見えませんね。"},{"id":{"d":1035,"n":7,"t":1},"speaker":"A","text":"はい、寒色ですので、むしろ爽やかです。ネービーは知的な色ですし、お客さまのお顔の印象にも合って、よくお似合いですよ。"},{"id":{"d":1035,"n":8,"t":1},"speaker":"B","text":"私もとても気に入りました。こちらを頂きます。ノースリーブのブラウスも1枚欲しいと思っています。首回りの開きが大きくないものを見せていただけますか？"}];
INSERT INTO sentence [{"id":{"d":1036,"n":1,"t":1},"speaker":"A","text":"来週の火曜日は外に食べに出ることができませんので、おにぎりを作ってもらえると助かります。"},{"id":{"d":1036,"n":2,"t":1},"speaker":"B","text":"おにぎりでは栄養が足りませんからお弁当を作りますよ。バッグにお弁当箱が入るスペースはありますか？"},{"id":{"d":1036,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。細長いタイプのお弁当箱なら入ります。簡単なお弁当でよいですよ。"},{"id":{"d":1036,"n":4,"t":1},"speaker":"B","text":"何を入れようかしら。卵焼きとブロッコリーとミニトマトはお弁当の彩りのために必ず入れましょう。メインは鶏の照り焼きか唐揚げにしましょう。どちらがいいですか？"},{"id":{"d":1036,"n":5,"t":1},"speaker":"A","text":"焼き物か揚げ物かの選択ですね。どちらも好きなので迷いますが、揚げ物の方が手がかかりますよね。照り焼きにしてください。"},{"id":{"d":1036,"n":6,"t":1},"speaker":"B","text":"前日の晩ご飯を鶏の唐揚げにして、残りをお弁当に詰めるという手もあります。そうすると手間はかかりません。"},{"id":{"d":1036,"n":7,"t":1},"speaker":"A","text":"それでしたら、鶏の唐揚げがいいです。ポテトサラダも入っていると気分が上がります。"},{"id":{"d":1036,"n":8,"t":1},"speaker":"B","text":"分かりました。ポテトサラダも作りましょう。ポテトサラダにマカロニは入れますか？"},{"id":{"d":1036,"n":9,"t":1},"speaker":"A","text":"入れてください。わが家のポテトサラダはかなりおいしいと思いますが、ポテマカサラダはそのポテトサラダよりもっともっと好きです。"}];
INSERT INTO sentence [{"id":{"d":1037,"n":1,"t":1},"speaker":"A","text":"良い香りがしてきました。牛肉の香りですね。今日は肉じゃがですか？"},{"id":{"d":1037,"n":2,"t":1},"speaker":"B","text":"正解です。スーパーマーケットで和牛が特売になっていたので、今晩は牛肉を使ったお料理にすることにしました。カレーライスにするか肉じゃがにするか迷いましたが、先週の水曜日にチキンカレーを食べたので、今日は肉じゃがにすることにしました。"},{"id":{"d":1037,"n":3,"t":1},"speaker":"A","text":"肉じゃがは大好きです。たくさん作ってください。"},{"id":{"d":1037,"n":4,"t":1},"speaker":"B","text":"たっぷり作りますね。食べ切れなかったら、肉じゃがコロッケにリメークしましょう。"},{"id":{"d":1037,"n":5,"t":1},"speaker":"A","text":"肉じゃがコロッケも僕の好物です。肉じゃがコロッケにする分を残さなくてはなりませんから、食べ過ぎないよう気を付けます。今晩の副菜は何ですか？"},{"id":{"d":1037,"n":6,"t":1},"speaker":"B","text":"ニンジンとコマツナの白和え、それからタコとキュウリの酢の物にしようと思います。"},{"id":{"d":1037,"n":7,"t":1},"speaker":"A","text":"おなかが鳴り始めました。早く食べたいのでお手伝いをします。僕にできることはありますか？"},{"id":{"d":1037,"n":8,"t":1},"speaker":"B","text":"それでは、キュウリを輪切りにして塩もみしてください。"}];
INSERT INTO sentence [{"id":{"d":1038,"n":1,"t":1},"speaker":"A","text":"今、防災無線が流れていたようなのですが、聞こえましたか？"},{"id":{"d":1038,"n":2,"t":1},"speaker":"B","text":"防災無線が流れていましたか。気が付きませんでした。テレビの音が大き過ぎたかもしれません。テレビのボリュームを少し下げますね。防災無線の内容はメールで届くので確認しますね。今、流れていた防災無線の内容は、熱中症注意喚起でした。"},{"id":{"d":1038,"n":3,"t":1},"speaker":"A","text":"メールで防災無線の内容が提供されているのですね。後で設定の方法を教えてください。"},{"id":{"d":1038,"n":4,"t":1},"speaker":"B","text":"はい。設定方法は簡単です。後でやりましょう。以前は地震や暴風雨の時に防災無線の内容が聞き取れず、不安な思いをしていましたが、今はその不安は払拭されました。"},{"id":{"d":1038,"n":5,"t":1},"speaker":"A","text":"市外に出ている時に、防災無線の内容を知ることができるのはいいですね。"},{"id":{"d":1038,"n":6,"t":1},"speaker":"B","text":"その通りです。警察からの不審者情報や行方不明者情報は、市外にいる時も入手したい情報です。"},{"id":{"d":1038,"n":7,"t":1},"speaker":"A","text":"行政のサービスも以前に比べて随分と良くなってきているのですね。"},{"id":{"d":1038,"n":8,"t":1},"speaker":"B","text":"そうですね。ただ、インターネット弱者の方々が取り残されることのないよう、工夫する必要はあると思います。"}];
INSERT INTO sentence [{"id":{"d":1039,"n":1,"t":1},"speaker":"A","text":"今日はパトカーを何台も見かけるような気がします。"},{"id":{"d":1039,"n":2,"t":1},"speaker":"B","text":"そうですね。先ほどは白バイともすれ違いました。海外の要人が来日しているのでしょうか？"},{"id":{"d":1039,"n":3,"t":1},"speaker":"A","text":"ニュースではそのようなことは言っていなかったと思います。何かあったのでしょうか？気になります。"},{"id":{"d":1039,"n":4,"t":1},"speaker":"B","text":"警察の人が忙しくしている理由が分かりました。秋の交通安全運動が昨日から始まっていたようです。それでパトカーや白バイが注意喚起のために走っているのですね。そういえば、サイレンは鳴らしていませんでしたものね。"},{"id":{"d":1039,"n":5,"t":1},"speaker":"A","text":"救急車もいつもより多く見かけるような気がします。これはなぜでしょう。"},{"id":{"d":1039,"n":6,"t":1},"speaker":"B","text":"9月の中旬とはいえ今日は猛暑日ですから、熱中症の人が搬送されているのかもしれませんよ。"},{"id":{"d":1039,"n":7,"t":1},"speaker":"A","text":"そういえば、今朝の天気予報で、今日は熱中症警戒アラートが発令されそうだと言っていました。気を付けなくてはなりませんね。"},{"id":{"d":1039,"n":8,"t":1},"speaker":"B","text":"そうですね。水分を補給しましょう。冷蔵庫にジンジャーエールが冷えていますから家に帰ったら飲みましょう。"}];
INSERT INTO sentence [{"id":{"d":1040,"n":1,"t":1},"speaker":"A","text":"今日はアイスクリームが全品40パーセント引きになっています。買いましょう。"},{"id":{"d":1040,"n":2,"t":1},"speaker":"B","text":"冷凍庫が空いていたかしら。今日のお昼ご飯にうどんを食べれば、冷凍うどんが入っているスペースが空きますね。"},{"id":{"d":1040,"n":3,"t":1},"speaker":"A","text":"お昼ご飯はぶっかけうどんにしましょう。それではアイスを選びますね。"},{"id":{"d":1040,"n":4,"t":1},"speaker":"B","text":"私はバニラにします。お父さんにもバニラを選びましょう。それから、竜太はイチゴ味が好きですよね。友子は何味にしますか？"},{"id":{"d":1040,"n":5,"t":1},"speaker":"A","text":"私は抹茶にしようかしら。でもチョコも捨てがたいです。決められなくて困ってしまいます。"},{"id":{"d":1040,"n":6,"t":1},"speaker":"B","text":"では、お父さんのバニラをやめて、抹茶とチョコを買いましょう。お父さんに分けてもらったらいいのではないかしら。"},{"id":{"d":1040,"n":7,"t":1},"speaker":"A","text":"両方の味が楽しめるのですね。うれしいです。お兄ちゃんのイチゴも少し分けてもらおうかしら。お母さんのバニラも食べさせてください。"},{"id":{"d":1040,"n":8,"t":1},"speaker":"B","text":"よいですよ。全員違うフレーバーにして少しずつ分け合いましょう。"}];
INSERT INTO sentence [{"id":{"d":1041,"n":1,"t":1},"speaker":"A","text":"今日は天気が悪いので、外で遊ぶことができません。児童館に行ってきます。"},{"id":{"d":1041,"n":2,"t":1},"speaker":"B","text":"誰かお友達と一緒に行くのですか？"},{"id":{"d":1041,"n":3,"t":1},"speaker":"A","text":"恭一君と約束しました。児童館でボードゲームをして遊ぼうと思っています。"},{"id":{"d":1041,"n":4,"t":1},"speaker":"B","text":"私は児童館の隣の図書館に本を返しに行かなくてはなりません。雨が降っていることですし、児童館まで車で送ってあげましょうか？"},{"id":{"d":1041,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。うちの車に恭一君も一緒に乗せてあげることはできますか？"},{"id":{"d":1041,"n":6,"t":1},"speaker":"B","text":"もちろんです。恭一君のお母さんに電話を入れましょう。遊ぶと喉が渇くでしょう。水筒を用意しておきなさい。"},{"id":{"d":1041,"n":7,"t":1},"speaker":"A","text":"分かりました。そういえば、僕の借りていた本も返却日が近づいていたと思います。もう読み終わっているので、お母さんの本と一緒に返却しておいていただけますか？"},{"id":{"d":1041,"n":8,"t":1},"speaker":"B","text":"よいですよ。本を持ってきてこの袋に入れておいてください。"}];
INSERT INTO sentence [{"id":{"d":1042,"n":1,"t":1},"speaker":"A","text":"最近気に入っているのが、和菓子のバイヤーさんのツイッターです。美しい和菓子の写真に魅了されています。"},{"id":{"d":1042,"n":2,"t":1},"speaker":"B","text":"和菓子は季節感がありますから、見ていて飽きることがありませんね。最近はどのようなお菓子が紹介されていましたか？"},{"id":{"d":1042,"n":3,"t":1},"speaker":"A","text":"東京の老舗和菓子屋さんの錦玉羹が紹介されていました。透明な寒天の中に金魚が泳いでいて、夏の暑さを忘れる涼しげなお菓子でした。"},{"id":{"d":1042,"n":4,"t":1},"speaker":"B","text":"ようかんの中に水中の景色を表しているのですね。素晴らしい職人技ですね。"},{"id":{"d":1042,"n":5,"t":1},"speaker":"A","text":"はい。食べるのがもったいないような和菓子です。そのような繊細なお菓子が紹介されている一方、串団子のような素朴な和菓子の写真も掲載されており、甘い物好きにはたまりません。"},{"id":{"d":1042,"n":6,"t":1},"speaker":"B","text":"その方は、デパートのバイヤーさんなのですか？"},{"id":{"d":1042,"n":7,"t":1},"speaker":"A","text":"デパートの催事の案内もありますので、恐らくそうだと思います。この方のツイッターを見ているとデパートの地下食料品売り場に行きたくなります。"},{"id":{"d":1042,"n":8,"t":1},"speaker":"B","text":"デパートの地下は本当に魅力的な空間ですよね。デパートに行った時には、どのフロアよりも地下食料品売り場で過ごす時間が長くなってしまいます。"}];
INSERT INTO sentence [{"id":{"d":1043,"n":1,"t":1},"speaker":"A","text":"スターバックスの新作が発表されましたね。もうチェックしましたか？"},{"id":{"d":1043,"n":2,"t":1},"speaker":"B","text":"もちろんです。ツイッターで見ましたよ。イチゴのフラペチーノでしたよね？"},{"id":{"d":1043,"n":3,"t":1},"speaker":"A","text":"そうです。イチゴのフラペチーノは毎年人気ですよね。"},{"id":{"d":1043,"n":4,"t":1},"speaker":"B","text":"しかも、今年は去年の2倍のイチゴソースが入っているそうですよ。"},{"id":{"d":1043,"n":5,"t":1},"speaker":"A","text":"そうですか。それでは、すぐさま売り切れてしまうかもしれませんね。"},{"id":{"d":1043,"n":6,"t":1},"speaker":"B","text":"それはなんとしてでも避けたいです。発売日に一緒に行きませんか？"},{"id":{"d":1043,"n":7,"t":1},"speaker":"A","text":"いいですね。当日の開店前から並びたいくらいの心持ちでいます。"},{"id":{"d":1043,"n":8,"t":1},"speaker":"B","text":"その熱量でフラペチーノを溶かさないようにしてくださいね。"},{"id":{"d":1043,"n":9,"t":1},"speaker":"A","text":"気を付けますよ。ところで、今日はこの後空いてますか？"},{"id":{"d":1043,"n":10,"t":1},"speaker":"B","text":"空いていますよ。どうしましたか？"},{"id":{"d":1043,"n":11,"t":1},"speaker":"A","text":"スターバックスの話をしていたら、行きたくなってきました。一緒に行きませんか？"},{"id":{"d":1043,"n":12,"t":1},"speaker":"B","text":"名案ですね。行きましょう。"}];
INSERT INTO sentence [{"id":{"d":1044,"n":1,"t":1},"speaker":"A","text":"そのトップスはとてもすてきですね。目をひかれました。"},{"id":{"d":1044,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。なんとこれ、1,500円もしないで手に入れられたのですよ。"},{"id":{"d":1044,"n":3,"t":1},"speaker":"A","text":"かなり安いですね。そのような珍しいデザインの服が安価で手に入れられるなんて素晴らしいです。"},{"id":{"d":1044,"n":4,"t":1},"speaker":"B","text":"これは、自分で少し手を加えているのですよ。ここのタックは自分でいれました。"},{"id":{"d":1044,"n":5,"t":1},"speaker":"A","text":"そのようなことができるのですね。お裁縫を習っていたのですか？"},{"id":{"d":1044,"n":6,"t":1},"speaker":"B","text":"いいえ、全く経験はありません。家庭科の授業で少しミシンに触ったくらいです。"},{"id":{"d":1044,"n":7,"t":1},"speaker":"A","text":"それにしては、きれいに仕上がっていますね。"},{"id":{"d":1044,"n":8,"t":1},"speaker":"B","text":"実は、100円ショップの便利グッズを使用したのです。"},{"id":{"d":1044,"n":9,"t":1},"speaker":"A","text":"どのような商品ですか？"},{"id":{"d":1044,"n":10,"t":1},"speaker":"B","text":"洗濯に強い布用のテープで、両面テープのように使用できます。"},{"id":{"d":1044,"n":11,"t":1},"speaker":"A","text":"なるほど。つまり、ミシンがなくても布同士を接着できるというわけですね。"},{"id":{"d":1044,"n":12,"t":1},"speaker":"B","text":"その通りです。とても便利ですのでぜひ一度使ってみてください。"}];
INSERT INTO sentence [{"id":{"d":1045,"n":1,"t":1},"speaker":"A","text":"お久しぶりです。東京での暮らしには慣れましたか？"},{"id":{"d":1045,"n":2,"t":1},"speaker":"B","text":"はい、もう警視庁に勤めて5年目になります。生活も落ち着いてきましたよ。"},{"id":{"d":1045,"n":3,"t":1},"speaker":"A","text":"そうですよね。高校卒業とともに就職されましたものね。"},{"id":{"d":1045,"n":4,"t":1},"speaker":"B","text":"はい、皆さんが大学生活を送っているのをうらやましく見ていましたよ。"},{"id":{"d":1045,"n":5,"t":1},"speaker":"A","text":"私は、早いうちから社会人として働くあなたの姿に刺激を受けていましたよ。"},{"id":{"d":1045,"n":6,"t":1},"speaker":"B","text":"そうですか。お互いさまなのかもしれませんね。"},{"id":{"d":1045,"n":7,"t":1},"speaker":"A","text":"隣の庭は青い、と言いますしね。"},{"id":{"d":1045,"n":8,"t":1},"speaker":"B","text":"ええ、まさにその通りだと思います。"}];
INSERT INTO sentence [{"id":{"d":1046,"n":1,"t":1},"speaker":"A","text":"私は結婚相手の条件の一つに、「1人暮らしの経験があること」を置いています。"},{"id":{"d":1046,"n":2,"t":1},"speaker":"B","text":"それはどうしてですか？"},{"id":{"d":1046,"n":3,"t":1},"speaker":"A","text":"一緒に生活する上で、家事の大変さを理解している者同士でないとうまくいく気がしません。"},{"id":{"d":1046,"n":4,"t":1},"speaker":"B","text":"なるほど。家事は小さなストレスの種になりますからね。"},{"id":{"d":1046,"n":5,"t":1},"speaker":"A","text":"ええ。いくら小さいストレスでも、ちりも積もれば山となると言いますから。"},{"id":{"d":1046,"n":6,"t":1},"speaker":"B","text":"例えば、作るのが簡単と言われているスパゲティ1皿をとっても、洗い物が発生します。"},{"id":{"d":1046,"n":7,"t":1},"speaker":"A","text":"ミートソースが付いたお皿を洗う手間もばかになりませんよね。"},{"id":{"d":1046,"n":8,"t":1},"speaker":"B","text":"はい。それを理解していることが私にとっては重要なのです。"},{"id":{"d":1046,"n":9,"t":1},"speaker":"A","text":"確かに、大切な条件かもしれませんね。"}];
INSERT INTO sentence [{"id":{"d":1047,"n":1,"t":1},"speaker":"A","text":"久しぶりにここのモーニングを食べにきました。"},{"id":{"d":1047,"n":2,"t":1},"speaker":"B","text":"ええ、半年ぶりですね。モーニングを楽しめるほど時間の余裕はありませんでしたからね。"},{"id":{"d":1047,"n":3,"t":1},"speaker":"A","text":"そうですね。やはり学生時代とは違いますね。"},{"id":{"d":1047,"n":4,"t":1},"speaker":"B","text":"当時は、本当に楽しかったです。また戻りたいといつも思っていますよ。"},{"id":{"d":1047,"n":5,"t":1},"speaker":"A","text":"それはかなわない願いですね。メニューを見てください。この半年で大きく変わっています。"},{"id":{"d":1047,"n":6,"t":1},"speaker":"B","text":"本当ですね。やはり、何事も日々変化していくのですね。"},{"id":{"d":1047,"n":7,"t":1},"speaker":"A","text":"そうですよ。冬にアイスクリーム、夏にクラムチャウダーを食べてもおいしくないということです。"},{"id":{"d":1047,"n":8,"t":1},"speaker":"B","text":"なるほど。当時は学生だったから、あの自由な生活を楽しめていたのかもしれませんね。"},{"id":{"d":1047,"n":9,"t":1},"speaker":"A","text":"そうだと思います。社会人は社会人としての楽しみ方があるはずですよ。"}];
INSERT INTO sentence [{"id":{"d":1048,"n":1,"t":1},"speaker":"A","text":"あなたの最近のマイブームは何ですか？"},{"id":{"d":1048,"n":2,"t":1},"speaker":"B","text":"パン屋さん巡りです。一昨日なんて、1日で4軒回りました。"},{"id":{"d":1048,"n":3,"t":1},"speaker":"A","text":"4軒も回ったのですか。合計でいくつ購入しましたか？"},{"id":{"d":1048,"n":4,"t":1},"speaker":"B","text":"1軒につき2つずつ買ったので、8個買いました。"},{"id":{"d":1048,"n":5,"t":1},"speaker":"A","text":"たくさん買ったのですね。全てその日に食べたのですか？"},{"id":{"d":1048,"n":6,"t":1},"speaker":"B","text":"いいえ、少しずつ食べて、残りは冷凍しました。毎日少しずつ食べるのが幸せなのです。"},{"id":{"d":1048,"n":7,"t":1},"speaker":"A","text":"グルテンには依存物質が含まれていると聞きましたが、そういった実感はありますか？"},{"id":{"d":1048,"n":8,"t":1},"speaker":"B","text":"はい。パン屋巡りを始めた当初は、1日1つまでとしていたのですが、今となっては際限なく買ってしまいます。"}];
INSERT INTO sentence [{"id":{"d":1049,"n":1,"t":1},"speaker":"A","text":"いつもご飯を作ってくれてありがとう。来週からは僕が作りますね。"},{"id":{"d":1049,"n":2,"t":1},"speaker":"B","text":"仕事で家に着くのが遅くなるのに、それから食事の準備をするなんて、負担になりませんか？"},{"id":{"d":1049,"n":3,"t":1},"speaker":"A","text":"実は、来週から在宅勤務になるのです。通勤時間がなくなるので、家事に充てる時間が確保できると考えています。"},{"id":{"d":1049,"n":4,"t":1},"speaker":"B","text":"そうですか。とても助かります。"},{"id":{"d":1049,"n":5,"t":1},"speaker":"A","text":"何かメニューのリクエストはありますか？"},{"id":{"d":1049,"n":6,"t":1},"speaker":"B","text":"オムライスを食べたいです。ケチャップでもよいですが、デミグラスソースだとうれしいです。"},{"id":{"d":1049,"n":7,"t":1},"speaker":"A","text":"分かりました、挑戦してみます。"}];
INSERT INTO sentence [{"id":{"d":1050,"n":1,"t":1},"speaker":"A","text":"コストコで販売しているチキンの丸焼きを食べてみたいです。1人ではなかなか挑戦できませんので。"},{"id":{"d":1050,"n":2,"t":1},"speaker":"B","text":"いいですね。僕も食べてみたかったので、今度ぜひやってみましょうよ。"},{"id":{"d":1050,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。2人でも食べ切れないと思うので、メンバーを集めましょう。"},{"id":{"d":1050,"n":4,"t":1},"speaker":"B","text":"そうですね。まずは青木さんに声を掛けてみましょうか？"},{"id":{"d":1050,"n":5,"t":1},"speaker":"A","text":"いいですね。彼女は何でもおいしそうに食べるので、見ていると幸せになります。"},{"id":{"d":1050,"n":6,"t":1},"speaker":"B","text":"はい。青木さんが来られるのなら、コストコで彼女の好物であるマフィンも買いましょう。"}];
INSERT INTO sentence [{"id":{"d":1051,"n":1,"t":1},"speaker":"A","text":"今から一緒にご飯に行きませんか？"},{"id":{"d":1051,"n":2,"t":1},"speaker":"B","text":"ぜひ行きましょう。どこか行きたい所はありますか？"},{"id":{"d":1051,"n":3,"t":1},"speaker":"A","text":"はい。国道沿いに新しくできた回転ずし屋さんに行ってみたいです。"},{"id":{"d":1051,"n":4,"t":1},"speaker":"B","text":"おすしですか。実は僕、生ものが苦手なのです。"},{"id":{"d":1051,"n":5,"t":1},"speaker":"A","text":"そうですか。残念です。では、イタリアンにしましょうか？"},{"id":{"d":1051,"n":6,"t":1},"speaker":"B","text":"いえ、行き先は回転ずしで大丈夫です。"},{"id":{"d":1051,"n":7,"t":1},"speaker":"A","text":"よいのですか？苦手なものを食べることになってしまいますよ。"},{"id":{"d":1051,"n":8,"t":1},"speaker":"B","text":"問題ありません。回転ずしにはうどんやフライドポテトなど生もの以外のメニューがたくさんあるので、僕はそれらを食べます。"},{"id":{"d":1051,"n":9,"t":1},"speaker":"A","text":"なるほど。では回転ずし屋さんに向かいましょう。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":1052,"n":1,"t":1},"speaker":"A","text":"お帰りなさい。私もちょうど今帰ってきたところで、夕ご飯の準備がまだできていません。"},{"id":{"d":1052,"n":2,"t":1},"speaker":"B","text":"ただいま。そうですか。では今日は出前にしましょうか？"},{"id":{"d":1052,"n":3,"t":1},"speaker":"A","text":"いいですね。久しぶりの出前にテンションが上がっています。"},{"id":{"d":1052,"n":4,"t":1},"speaker":"B","text":"やはり、出前といえばすしでしょうか？"},{"id":{"d":1052,"n":5,"t":1},"speaker":"A","text":"最近はウーバーイーツが勢力を拡大しているので、すしに限らずなんでも注文できますよ。"},{"id":{"d":1052,"n":6,"t":1},"speaker":"B","text":"そうでしたね。大通り沿いの韓国料理屋に出前対応のポスターが貼ってありました。あそこのトッポギを注文してもよいですか？"},{"id":{"d":1052,"n":7,"t":1},"speaker":"A","text":"残念ですが、あのお店は今日が定休日です。"},{"id":{"d":1052,"n":8,"t":1},"speaker":"B","text":"そうでしたか。"},{"id":{"d":1052,"n":9,"t":1},"speaker":"A","text":"今日は3丁目の韓国料理屋に注文しましょう。"}];
INSERT INTO sentence [{"id":{"d":1053,"n":1,"t":1},"speaker":"A","text":"バレンタインデーが近づいてきましたね。"},{"id":{"d":1053,"n":2,"t":1},"speaker":"B","text":"そうですね。木村さんは誰にチョコレートを贈る予定ですか？"},{"id":{"d":1053,"n":3,"t":1},"speaker":"A","text":"4組の小山君に渡したくて、先月から何度か試作しています。"},{"id":{"d":1053,"n":4,"t":1},"speaker":"B","text":"それは相当おいしいものが渡せそうですね。小山君もきっと喜んでくれますよ。"},{"id":{"d":1053,"n":5,"t":1},"speaker":"A","text":"去年は義理チョコとして渡したのですが、今年は違うのです。"},{"id":{"d":1053,"n":6,"t":1},"speaker":"B","text":"つまり、本命チョコですか。すてきですね。"},{"id":{"d":1053,"n":7,"t":1},"speaker":"A","text":"そうです。ラブレターを一緒に入れようと思っています。"},{"id":{"d":1053,"n":8,"t":1},"speaker":"B","text":"聞いているこちらが緊張してきました。うまくいきますように。"},{"id":{"d":1053,"n":9,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":1054,"n":1,"t":1},"speaker":"A","text":"小山君、伝えたいことがあるのですが。"},{"id":{"d":1054,"n":2,"t":1},"speaker":"B","text":"どうしましたか？"},{"id":{"d":1054,"n":3,"t":1},"speaker":"A","text":"実は今日、僕のロッカーにチョコレートが入っていました。"},{"id":{"d":1054,"n":4,"t":1},"speaker":"B","text":"それはきっと、バレンタインのチョコレートですね。杉山君のことが好きな人がいるということですね。"},{"id":{"d":1054,"n":5,"t":1},"speaker":"A","text":"いいえ、違います。"},{"id":{"d":1054,"n":6,"t":1},"speaker":"B","text":"どうしてそう思うのですか？"},{"id":{"d":1054,"n":7,"t":1},"speaker":"A","text":"チョコレートに、「小山君へ」と書かれていました。"},{"id":{"d":1054,"n":8,"t":1},"speaker":"B","text":"僕宛てだったのですか。ロッカーが隣ですから、間違えてしまったのかもしれませんね。"},{"id":{"d":1054,"n":9,"t":1},"speaker":"A","text":"そうだと思います。おいしく食べてあげてください。"},{"id":{"d":1054,"n":10,"t":1},"speaker":"B","text":"ありがとうございます。杉山君は優しいですね。"}];
INSERT INTO sentence [{"id":{"d":1055,"n":1,"t":1},"speaker":"A","text":"お兄ちゃん、目覚ましの音量を下げてください。"},{"id":{"d":1055,"n":2,"t":1},"speaker":"B","text":"どうしてですか？今でも目覚ましに気付かず寝坊するのに、余計起きられなくなってしまいます。"},{"id":{"d":1055,"n":3,"t":1},"speaker":"A","text":"お兄ちゃんの目覚ましの音で、お兄ちゃん以外の家族みんなが起きてしまっているのです。"},{"id":{"d":1055,"n":4,"t":1},"speaker":"B","text":"そうなのですか。ごめんなさい。"},{"id":{"d":1055,"n":5,"t":1},"speaker":"A","text":"音で起きようとするのではなく、別の方法を試してみませんか？"},{"id":{"d":1055,"n":6,"t":1},"speaker":"B","text":"どういうことでしょうか？"},{"id":{"d":1055,"n":7,"t":1},"speaker":"A","text":"光や振動で起こしてくれる目覚ましがあるのです。"},{"id":{"d":1055,"n":8,"t":1},"speaker":"B","text":"それはとてもいいですね。早速電気屋さんに行ってこようと思います。"}];
INSERT INTO sentence [{"id":{"d":1056,"n":1,"t":1},"speaker":"A","text":"あなたは、いつもどこで洋服を買っていますか？"},{"id":{"d":1056,"n":2,"t":1},"speaker":"B","text":"天王寺に行くことが多いです。はやりのブランドから古着屋さんまでそろっているので、いろいろな系統の服を入手できます。"},{"id":{"d":1056,"n":3,"t":1},"speaker":"A","text":"天王寺に古着屋があるのは初耳です。"},{"id":{"d":1056,"n":4,"t":1},"speaker":"B","text":"天王寺MIOの6階に何軒かあります。有名ブランドもいいですが、古着屋さんは宝探しのようでワクワクします。"},{"id":{"d":1056,"n":5,"t":1},"speaker":"A","text":"同感です。気に入ったものが、自分の体型にピッタリだった時の快感はたまりません。"},{"id":{"d":1056,"n":6,"t":1},"speaker":"B","text":"もしよろしければ、今度の日曜に、古着屋巡りをしませんか？"},{"id":{"d":1056,"n":7,"t":1},"speaker":"A","text":"いいですね。ちょうど秋服が欲しくなってきたところです。"},{"id":{"d":1056,"n":8,"t":1},"speaker":"B","text":"お互い、気に入ったものが見つかるといいですね。"}];
INSERT INTO sentence [{"id":{"d":1057,"n":1,"t":1},"speaker":"A","text":"今晩のメニューは何がいいですか？"},{"id":{"d":1057,"n":2,"t":1},"speaker":"B","text":"ギョーザが食べたいです。"},{"id":{"d":1057,"n":3,"t":1},"speaker":"A","text":"ギョーザは先週末にも食べたではありませんか？"},{"id":{"d":1057,"n":4,"t":1},"speaker":"B","text":"今日は市販のものではなく、手作りのものが食べたいです。"},{"id":{"d":1057,"n":5,"t":1},"speaker":"A","text":"分かりました。ただし、条件があります。"},{"id":{"d":1057,"n":6,"t":1},"speaker":"B","text":"なんでしょうか？"},{"id":{"d":1057,"n":7,"t":1},"speaker":"A","text":"家族みんなが協力して包んでくれることです。包むのにかなりの労力を要するので、私1人では作る気になれません。"},{"id":{"d":1057,"n":8,"t":1},"speaker":"B","text":"分かりました。みんなに協力を要請してきます。"}];
INSERT INTO sentence [{"id":{"d":1058,"n":1,"t":1},"speaker":"A","text":"おなかがすいてしまいました。何か食べるものはありますか？"},{"id":{"d":1058,"n":2,"t":1},"speaker":"B","text":"机の上にミックスナッツがあったかと思います。いくつか食べてよいですよ。"},{"id":{"d":1058,"n":3,"t":1},"speaker":"A","text":"もう少しおなかにたまるものはありませんか？ナッツでは満たされません。"},{"id":{"d":1058,"n":4,"t":1},"speaker":"B","text":"冷蔵庫にシナモンロールがありますが、あと1時間で夜ご飯ですよ。我慢できませんか？"},{"id":{"d":1058,"n":5,"t":1},"speaker":"A","text":"どうしても耐えられません。シナモンロールを食べますが、夕飯に響くといけませんので半分にします。"},{"id":{"d":1058,"n":6,"t":1},"speaker":"B","text":"それがいいですね。明日はもう少し昼ご飯の量を増やしましょう。"},{"id":{"d":1058,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。おにぎりを2つ多く付けてください。"},{"id":{"d":1058,"n":8,"t":1},"speaker":"B","text":"食べ盛りですね。分かりました、用意します。"}];
INSERT INTO sentence [{"id":{"d":1059,"n":1,"t":1},"speaker":"A","text":"最近、焼き芋がブームになっていますね。"},{"id":{"d":1059,"n":2,"t":1},"speaker":"B","text":"はい。街中で、焼き芋屋さんをよく見るようになりました。"},{"id":{"d":1059,"n":3,"t":1},"speaker":"A","text":"とてもおしゃれな外装で、一目見ただけでは焼き芋屋さんだと気付きにくい店舗が多いです。"},{"id":{"d":1059,"n":4,"t":1},"speaker":"B","text":"そうですね。先日は駅前の「イモニスト」というお店に行ってきました。"},{"id":{"d":1059,"n":5,"t":1},"speaker":"A","text":"1カ月前にオープンしたばかりのお店ですね。どうでしたか？"},{"id":{"d":1059,"n":6,"t":1},"speaker":"B","text":"焼き芋だけでなく、芋パフェや芋けんぴ、芋プリンといったバラエティー豊かなラインアップでした。"},{"id":{"d":1059,"n":7,"t":1},"speaker":"A","text":"芋プリンがとても気になります。今度行ってみようと思います。"},{"id":{"d":1059,"n":8,"t":1},"speaker":"B","text":"ぜひ。私は紫芋アイスをいただきましたが、こちらも絶品でした。"},{"id":{"d":1059,"n":9,"t":1},"speaker":"A","text":"どちらも試してみたくなってきました。"}];
INSERT INTO sentence [{"id":{"d":1060,"n":1,"t":1},"speaker":"A","text":"先日、いろいろなメーカーのサラダチキンを比較してみたのです。"},{"id":{"d":1060,"n":2,"t":1},"speaker":"B","text":"結果はどうでしたか？おいしいチキンを販売しているメーカーを知りたいです。"},{"id":{"d":1060,"n":3,"t":1},"speaker":"A","text":"AKUTAのチキンは、味付けが秀逸です。身の柔らかさは、リボディーがピカイチです。"},{"id":{"d":1060,"n":4,"t":1},"speaker":"B","text":"ありがたいです。筋トレをしているので、サラダチキンを買う機会が多いです。"},{"id":{"d":1060,"n":5,"t":1},"speaker":"A","text":"体作りに胸肉は欠かせませんからね。"},{"id":{"d":1060,"n":6,"t":1},"speaker":"B","text":"時間に余裕があるときは、鶏ハムを手作りしています。"},{"id":{"d":1060,"n":7,"t":1},"speaker":"A","text":"素晴らしいですね。1個作るのに、どれくらいの時間を要しますか？"},{"id":{"d":1060,"n":8,"t":1},"speaker":"B","text":"それが、塩と砂糖をすり込んだら炊飯器に入れて50分保温するだけで完成です。"},{"id":{"d":1060,"n":9,"t":1},"speaker":"A","text":"なんて簡単なのでしょう。早速まねしてみます。"}];
INSERT INTO sentence [{"id":{"d":1061,"n":1,"t":1},"speaker":"A","text":"今日は、時間がなくて夜ご飯を作れませんでした。"},{"id":{"d":1061,"n":2,"t":1},"speaker":"B","text":"では、出前でも取りましょう。めったにない機会なので、ジャンクフードが食べたいです。"},{"id":{"d":1061,"n":3,"t":1},"speaker":"A","text":"もう23時ですから、残念ですが、どこも出前対応を終了していると思いますよ。"},{"id":{"d":1061,"n":4,"t":1},"speaker":"B","text":"そうですか。では、カップ麺でも食べましょう。横のコンビニエンスストアに、気になる商品があるのです。"},{"id":{"d":1061,"n":5,"t":1},"speaker":"A","text":"そうしましょう。気になっているものの商品名を教えてください。"},{"id":{"d":1061,"n":6,"t":1},"speaker":"B","text":"「旨辛花椒汁なし坦々麺」です。"},{"id":{"d":1061,"n":7,"t":1},"speaker":"A","text":"やはりそれでしたか。悟さんが好きだろうと思って、昨日買ってきました。電子レンジの横に置いてありますよ。"},{"id":{"d":1061,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。しかし、幸子さんの食べる分がありません。"},{"id":{"d":1061,"n":9,"t":1},"speaker":"A","text":"私は昨日の夜ご飯の残りを食べるので、気にしないでください。"}];
INSERT INTO sentence [{"id":{"d":1062,"n":1,"t":1},"speaker":"A","text":"毎日のお風呂掃除が面倒で参っています。"},{"id":{"d":1062,"n":2,"t":1},"speaker":"B","text":"それでしたら、吹き付けるだけでこすり洗いの要らないスプレータイプの洗浄液を使ってみてはいかがですか？"},{"id":{"d":1062,"n":3,"t":1},"speaker":"A","text":"そのような便利グッズがあるのですね。実際に使っていらっしゃいますか？"},{"id":{"d":1062,"n":4,"t":1},"speaker":"B","text":"はい。半年前から毎日使用しています。もう手放せません。"},{"id":{"d":1062,"n":5,"t":1},"speaker":"A","text":"累積の汚れも気になりませんか？"},{"id":{"d":1062,"n":6,"t":1},"speaker":"B","text":"ええ。ピカピカなままです。もっと早くから使っておけば良かったと後悔しています。"},{"id":{"d":1062,"n":7,"t":1},"speaker":"A","text":"それほどの効果なのですね。今日買って帰ります。"},{"id":{"d":1062,"n":8,"t":1},"speaker":"B","text":"薬局よりも通販の方が安く買えますよ。"},{"id":{"d":1062,"n":9,"t":1},"speaker":"A","text":"有益な情報をいくつもありがとうございます。インターネットで購入しようと思います。"}];
INSERT INTO sentence [{"id":{"d":1063,"n":1,"t":1},"speaker":"A","text":"すみません、お巡りさんいらっしゃいますか？"},{"id":{"d":1063,"n":2,"t":1},"speaker":"B","text":"はい、福岡駅前交番です。どうされましたか？"},{"id":{"d":1063,"n":3,"t":1},"speaker":"A","text":"先ほど電車内で財布を拾いました。"},{"id":{"d":1063,"n":4,"t":1},"speaker":"B","text":"お持ちいただきありがとうございます。中身を確認させていただきますので、こちらの椅子に掛けてお待ちください。"},{"id":{"d":1063,"n":5,"t":1},"speaker":"A","text":"はい。"},{"id":{"d":1063,"n":6,"t":1},"speaker":"B","text":"お待ちいただいている間に、お届け主さまの情報をこちらに書いていただけますか？"},{"id":{"d":1063,"n":7,"t":1},"speaker":"A","text":"これは必ず記入しなければいけませんか？"},{"id":{"d":1063,"n":8,"t":1},"speaker":"B","text":"これを記入していただくことで、届けたことに対するお礼を頂く権利が発生します。"},{"id":{"d":1063,"n":9,"t":1},"speaker":"A","text":"分かりました。記入いたします。"}];
INSERT INTO sentence [{"id":{"d":1064,"n":1,"t":1},"speaker":"A","text":"最近、100円均一ショップの便利グッズの収集に熱中しています。"},{"id":{"d":1064,"n":2,"t":1},"speaker":"B","text":"そのコレクションの中で、最も優秀だと思うものは何ですか？"},{"id":{"d":1064,"n":3,"t":1},"speaker":"A","text":"蛇口延長ガイドです。"},{"id":{"d":1064,"n":4,"t":1},"speaker":"B","text":"それは一体どのようなものなのでしょうか？"},{"id":{"d":1064,"n":5,"t":1},"speaker":"A","text":"蛇口の口元に装着することで、水をより手前まで運んでくれるグッズです。流しそうめんの竹から着想したそうです。"},{"id":{"d":1064,"n":6,"t":1},"speaker":"B","text":"なるほど。それを付けることでどのようなメリットがあるのですか？"},{"id":{"d":1064,"n":7,"t":1},"speaker":"A","text":"子どもでも、手が洗いやすくなります。腕が短い子どもは、洗面台部分が邪魔で、蛇口まで手が届かないことがあるのです。"},{"id":{"d":1064,"n":8,"t":1},"speaker":"B","text":"それはとても便利そうです。"}];
INSERT INTO sentence [{"id":{"d":1065,"n":1,"t":1},"speaker":"A","text":"はい、ビューティーサロン雅です。"},{"id":{"d":1065,"n":2,"t":1},"speaker":"B","text":"カットとカラーの予約を入れたいのですが、12日の土曜に空いている時間はありますか？"},{"id":{"d":1065,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。12日でしたら12時、15時、17時が空いております。"},{"id":{"d":1065,"n":4,"t":1},"speaker":"B","text":"では、15時からでお願いします。"},{"id":{"d":1065,"n":5,"t":1},"speaker":"A","text":"はい、カットとカラーのご予約を15時からですね。お名前とお電話番号を頂戴できますか？"},{"id":{"d":1065,"n":6,"t":1},"speaker":"B","text":"岡田優美です。電話番号は、090-1234-5678です。"},{"id":{"d":1065,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。では、12日の15時にお待ちしております。"},{"id":{"d":1065,"n":8,"t":1},"speaker":"B","text":"よろしくお願いします。失礼いたします。"}];
INSERT INTO sentence [{"id":{"d":1066,"n":1,"t":1},"speaker":"A","text":"ジョンが、日本に来て驚いたことを教えてください。"},{"id":{"d":1066,"n":2,"t":1},"speaker":"B","text":"食文化のギャップにはいつも驚かされます。まず、アメリカと日本では、食事の鮮やかさが違います。"},{"id":{"d":1066,"n":3,"t":1},"speaker":"A","text":"アメリカのお菓子は、目を見張るほど鮮やかなものが多いですね。"},{"id":{"d":1066,"n":4,"t":1},"speaker":"B","text":"はい。日本の知育菓子なども色とりどりなお菓子がありますが、海外のようなビビッドカラーではありません。"},{"id":{"d":1066,"n":5,"t":1},"speaker":"A","text":"言われてみれば、確かにそうですね。他にもカルチャーショックを受けたものはありますか？"},{"id":{"d":1066,"n":6,"t":1},"speaker":"B","text":"料理のサイズです。アメリカでは、豪快で満足感のある量が提供されますが、日本のものは繊細で、少量であるイメージです。"},{"id":{"d":1066,"n":7,"t":1},"speaker":"A","text":"例えば、どこのお店でその違いを感じましたか？"},{"id":{"d":1066,"n":8,"t":1},"speaker":"B","text":"おすし屋さんです。アメリカのすし屋は、豪快にさばいた魚を、豪快に巻いて、豪快に切り分けたロール系のお寿司が大量に提供されます。一方、日本では一つ一つ丁寧に握った寿司が少量提供されます。"},{"id":{"d":1066,"n":9,"t":1},"speaker":"A","text":"そうですか。文化の違いを知るのは面白いです。"}];
INSERT INTO sentence [{"id":{"d":1067,"n":1,"t":1},"speaker":"A","text":"今日、帰りがけにみりんを買ってきてほしいとお願いしましたが、みりんは、どこに置きましたか？"},{"id":{"d":1067,"n":2,"t":1},"speaker":"B","text":"すみません。買ってくるのをすっかり忘れていました。"},{"id":{"d":1067,"n":3,"t":1},"speaker":"A","text":"そうですか。今から買いに行ってもらうことはできますか？"},{"id":{"d":1067,"n":4,"t":1},"speaker":"B","text":"お風呂を済ませてしまったので、買いに出るのはまた明日でもよいですか？"},{"id":{"d":1067,"n":5,"t":1},"speaker":"A","text":"分かりました。お砂糖とお酒で代用できると思うので、それぞれの分量を調べてもらえますか？"},{"id":{"d":1067,"n":6,"t":1},"speaker":"B","text":"日本酒とお砂糖を、3対1の割合で配合するようです。"},{"id":{"d":1067,"n":7,"t":1},"speaker":"A","text":"大変です。お酒も切らしていました。今晩はメニューを変更してみりんとお酒を使わないものを作ります。"},{"id":{"d":1067,"n":8,"t":1},"speaker":"B","text":"では、明日、みりんとお酒を買って帰りますね。"}];
INSERT INTO sentence [{"id":{"d":1068,"n":1,"t":1},"speaker":"A","text":"今年も一緒に花火大会に行きませんか？"},{"id":{"d":1068,"n":2,"t":1},"speaker":"B","text":"いいですね。8月11日から13日までの3日間開催されるようですが、何日に行きましょうか？"},{"id":{"d":1068,"n":3,"t":1},"speaker":"A","text":"12日に行きましょう。あじき花火大会は、例年中日が比較的すいていると聞いたことがあります。"},{"id":{"d":1068,"n":4,"t":1},"speaker":"B","text":"そうですか。では、12日の土曜は空けておきますね。"},{"id":{"d":1068,"n":5,"t":1},"speaker":"A","text":"もし、ゆかちゃんが大変でなかったら、今年は浴衣で行きませんか？"},{"id":{"d":1068,"n":6,"t":1},"speaker":"B","text":"とてもすてきですね。しかし、私は浴衣を持っていません。"},{"id":{"d":1068,"n":7,"t":1},"speaker":"A","text":"僕も持っていませんので、一緒にレンタルしませんか？ペア割でとても安く借りられるそうです。"},{"id":{"d":1068,"n":8,"t":1},"speaker":"B","text":"そこまで調べてくれてありがとうございます。では、浴衣で花火大会を楽しみましょう。"}];
INSERT INTO sentence [{"id":{"d":1069,"n":1,"t":1},"speaker":"A","text":"もしもし。浴衣レンタルの予約をしたいのですが。"},{"id":{"d":1069,"n":2,"t":1},"speaker":"B","text":"お電話ありがとうございます。はい、承知いたしました。お日にちをお伺いいたします。"},{"id":{"d":1069,"n":3,"t":1},"speaker":"A","text":"8月15日の土曜日です。"},{"id":{"d":1069,"n":4,"t":1},"speaker":"B","text":"ご予約される方の人数と性別をお伺いいたします。"},{"id":{"d":1069,"n":5,"t":1},"speaker":"A","text":"彼女とペアでお借りしたいと考えていますので、女性1名、男性1名です。"},{"id":{"d":1069,"n":6,"t":1},"speaker":"B","text":"承知いたしました。小物はお付けしますか？バッグや扇子、髪飾りが、1,000円追加で選び放題です。"},{"id":{"d":1069,"n":7,"t":1},"speaker":"A","text":"では、小物も付けていただけますか？そうすると、合計でいくらになりますか？"},{"id":{"d":1069,"n":8,"t":1},"speaker":"B","text":"2名さま分の浴衣レンタルに小物代を合わせて、5,200円です。"},{"id":{"d":1069,"n":9,"t":1},"speaker":"A","text":"ペア割は適用されていますか？"},{"id":{"d":1069,"n":10,"t":1},"speaker":"B","text":"はい。適用後のお値段です。"},{"id":{"d":1069,"n":11,"t":1},"speaker":"A","text":"ありがとうございます。では、その内容で予約をお願いします。"},{"id":{"d":1069,"n":12,"t":1},"speaker":"B","text":"かしこまりました。"}];
