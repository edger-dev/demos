INSERT INTO sentence [{"id":{"d":542,"n":1,"t":1},"speaker":"A","text":"ママ、何かお手伝いをしましょうか？"},{"id":{"d":542,"n":2,"t":1},"speaker":"B","text":"うれしいです。それでは、お風呂を洗ってください。"},{"id":{"d":542,"n":3,"t":1},"speaker":"A","text":"分かりました。洗剤とスポンジはどこにありますか？"},{"id":{"d":542,"n":4,"t":1},"speaker":"B","text":"洗面所にあります。"},{"id":{"d":542,"n":5,"t":1},"speaker":"A","text":"それでは洗ってきます。"},{"id":{"d":542,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。洗い終わったら、お湯をためてくれますか？"},{"id":{"d":542,"n":7,"t":1},"speaker":"A","text":"分かりました。お湯の温度は40度でよいですか？"},{"id":{"d":542,"n":8,"t":1},"speaker":"B","text":"はい。それでお願いします。"}];
