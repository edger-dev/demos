INSERT INTO sentence [{"id":{"d":1,"n":1,"t":1},"speaker":"A","text":"おはようございます。今日はとても良いお天気ですね。"},{"id":{"d":1,"n":2,"t":1},"speaker":"B","text":"はい。雲一つなくとても良い天気です。"},{"id":{"d":1,"n":3,"t":1},"speaker":"A","text":"この様子だと昼から暑くなりそうですね。"},{"id":{"d":1,"n":4,"t":1},"speaker":"B","text":"そうですね。お昼ご飯にはさっぱりした物が食べたくなります。"},{"id":{"d":1,"n":5,"t":1},"speaker":"A","text":"冷やし中華などでしょうか？"},{"id":{"d":1,"n":6,"t":1},"speaker":"B","text":"いいですね。よかったら一緒に食べに行きませんか？"},{"id":{"d":1,"n":7,"t":1},"speaker":"A","text":"ぜひ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":2,"n":1,"t":1},"speaker":"A","text":"お昼休憩ですね。今日は何を食べますか？"},{"id":{"d":2,"n":2,"t":1},"speaker":"B","text":"今日はステーキが食べたい気分です。加藤さんは何が食べたいですか？"},{"id":{"d":2,"n":3,"t":1},"speaker":"A","text":"私はハンバーグが食べたいです。"},{"id":{"d":2,"n":4,"t":1},"speaker":"B","text":"では、ステーキとハンバーグの両方のメニューがあるお店を探しましょうか？"},{"id":{"d":2,"n":5,"t":1},"speaker":"A","text":"そうですね。この先にレストランのガストがあるので、ガストへ行きましょうか？"},{"id":{"d":2,"n":6,"t":1},"speaker":"B","text":"では、ガストへ行きましょう。私はドリンクバーの割引クーポンを持っているので、ドリンクバーもつけましょう。"}];
INSERT INTO sentence [{"id":{"d":3,"n":1,"t":1},"speaker":"A","text":"休憩時間になりましたが、何か飲みますか？"},{"id":{"d":3,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。何を飲むか考えますね。"},{"id":{"d":3,"n":3,"t":1},"speaker":"A","text":"コーヒーはいかがでしょうか？"},{"id":{"d":3,"n":4,"t":1},"speaker":"B","text":"コーヒーは苦手です。他の飲み物はありますか？"},{"id":{"d":3,"n":5,"t":1},"speaker":"A","text":"紅茶もありますよ。"},{"id":{"d":3,"n":6,"t":1},"speaker":"B","text":"紅茶は大好きなので、紅茶にします。"},{"id":{"d":3,"n":7,"t":1},"speaker":"A","text":"では、紅茶を入れます。"},{"id":{"d":3,"n":8,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":4,"n":1,"t":1},"speaker":"A","text":"最近、体が引き締まっていますが、運動の他に食事などにも気を使っているのですか？"},{"id":{"d":4,"n":2,"t":1},"speaker":"B","text":"はい、低カロリーかつ栄養バランスの良い食事を心掛けています。最近は、完全栄養食のパンをよく食べています。"},{"id":{"d":4,"n":3,"t":1},"speaker":"A","text":"なるほど。そのパンはおいしいのですか？"},{"id":{"d":4,"n":4,"t":1},"speaker":"B","text":"好みによると思いますが、おいしいですよ。"},{"id":{"d":4,"n":5,"t":1},"speaker":"A","text":"どのような味がするのですか？"},{"id":{"d":4,"n":6,"t":1},"speaker":"B","text":"アボカドやトマト、キュウリ、パプリカなどが入っています。さっぱりした味です。"},{"id":{"d":4,"n":7,"t":1},"speaker":"A","text":"それはおいしそうですね。"}];
INSERT INTO sentence [{"id":{"d":5,"n":1,"t":1},"speaker":"A","text":"高橋さんは、どのようなテレビ番組をご覧になりますか？"},{"id":{"d":5,"n":2,"t":1},"speaker":"B","text":"ニュースやドラマをよく見ています。"},{"id":{"d":5,"n":3,"t":1},"speaker":"A","text":"ドラマがお好きなのですね。どのようなドラマをご覧になりますか？"},{"id":{"d":5,"n":4,"t":1},"speaker":"B","text":"推理ものや恋愛系が好きです。"},{"id":{"d":5,"n":5,"t":1},"speaker":"A","text":"良いですね。僕はアクションものをよく見ます。"},{"id":{"d":5,"n":6,"t":1},"speaker":"B","text":"アクションものも面白いですよね。"}];
INSERT INTO sentence [{"id":{"d":6,"n":1,"t":1},"speaker":"A","text":"佐藤君、今日の夕方に花火大会が開催されますが、一緒に行きませんか？"},{"id":{"d":6,"n":2,"t":1},"speaker":"B","text":"すみません。今日は塾へ行く予定があるから花火大会へは行けません。"},{"id":{"d":6,"n":3,"t":1},"speaker":"A","text":"そうですか。残念ですが、塾へ行く予定があるなら仕方ありませんね。"},{"id":{"d":6,"n":4,"t":1},"speaker":"B","text":"花火大会は明日まで開催していましたよね？明日なら行けます。"},{"id":{"d":6,"n":5,"t":1},"speaker":"A","text":"では、明日一緒に行きましょう。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":7,"n":1,"t":1},"speaker":"A","text":"最近何かおいしいものを食べましたか？"},{"id":{"d":7,"n":2,"t":1},"speaker":"B","text":"ええ、私はいつもおいしいものを食べています。"},{"id":{"d":7,"n":3,"t":1},"speaker":"A","text":"それはすごい。一体どのようなものを食べているのですか？"},{"id":{"d":7,"n":4,"t":1},"speaker":"B","text":"妻の手料理です。毎日家へ帰ることが楽しみですよ。"},{"id":{"d":7,"n":5,"t":1},"speaker":"A","text":"なるほど、そういうことですか。うらやましいです。"}];
INSERT INTO sentence [{"id":{"d":8,"n":1,"t":1},"speaker":"A","text":"山田さん、これとこれをコピーしてくれますか？"},{"id":{"d":8,"n":2,"t":1},"speaker":"B","text":"それぞれ何枚ずつコピーしますか？"},{"id":{"d":8,"n":3,"t":1},"speaker":"A","text":"10枚ずつ、どちらも白黒でお願いします。"},{"id":{"d":8,"n":4,"t":1},"speaker":"B","text":"はい、分かりました。裏紙をコピー用紙として使ってもよいですか？"},{"id":{"d":8,"n":5,"t":1},"speaker":"A","text":"うちの課で使うだけなので、それでお願いします。"}];
INSERT INTO sentence [{"id":{"d":9,"n":1,"t":1},"speaker":"A","text":"私は、昔ボクシングをしていたのですが、あなたは何かスポーツをされていましたか？"},{"id":{"d":9,"n":2,"t":1},"speaker":"B","text":"私は、野球をしていて、ポジションはレフトでした。"},{"id":{"d":9,"n":3,"t":1},"speaker":"A","text":"今でもプレーをしているのですか？"},{"id":{"d":9,"n":4,"t":1},"speaker":"B","text":"今は、休日に息子とキャッチボールをする程度です。"},{"id":{"d":9,"n":5,"t":1},"speaker":"A","text":"息子さんも野球をしているのですか？"},{"id":{"d":9,"n":6,"t":1},"speaker":"B","text":"リトルリーグのチームに入っています。"},{"id":{"d":9,"n":7,"t":1},"speaker":"A","text":"それは素晴らしいことです。将来はメジャーリーグの選手ですね。"},{"id":{"d":9,"n":8,"t":1},"speaker":"B","text":"そうなってくれると良いですね。"}];
INSERT INTO sentence [{"id":{"d":10,"n":1,"t":1},"speaker":"A","text":"田上さん、お茶でも飲んで、一息入れませんか？"},{"id":{"d":10,"n":2,"t":1},"speaker":"B","text":"そうしましょう。僕は、コーヒーにします。高山君は、何を飲みますか？"},{"id":{"d":10,"n":3,"t":1},"speaker":"A","text":"私は、紅茶にします。"},{"id":{"d":10,"n":4,"t":1},"speaker":"B","text":"紅茶が好きなのですね。ところで緑茶も紅茶も元は同じ茶葉だという事は知っていますか？"},{"id":{"d":10,"n":5,"t":1},"speaker":"A","text":"そうなのですか？"},{"id":{"d":10,"n":6,"t":1},"speaker":"B","text":"発酵度合いの差で変わってくるらしいです。"},{"id":{"d":10,"n":7,"t":1},"speaker":"A","text":"知りませんでした。勉強になりました。"}];
INSERT INTO sentence [{"id":{"d":11,"n":1,"t":1},"speaker":"A","text":"そのお洋服は、とてもすてきですね。どこで買っているのですか？"},{"id":{"d":11,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。いろいろなところで洋服を買うので忘れてしまいました。"},{"id":{"d":11,"n":3,"t":1},"speaker":"A","text":"街歩きしながら直感で服を決めるのですか？"},{"id":{"d":11,"n":4,"t":1},"speaker":"B","text":"そうです。いつも直感的に良いなと思ったものを買っています。"},{"id":{"d":11,"n":5,"t":1},"speaker":"A","text":"センスが良いのですね。すてきです。"},{"id":{"d":11,"n":6,"t":1},"speaker":"B","text":"良かったら今度一緒に買い物へ行きませんか？"},{"id":{"d":11,"n":7,"t":1},"speaker":"A","text":"うれしいです。センスが良い方の服の選び方を、参考にいたします。"}];
INSERT INTO sentence [{"id":{"d":12,"n":1,"t":1},"speaker":"A","text":"お母さん、明日からぐずついた天気になるらしいです。"},{"id":{"d":12,"n":2,"t":1},"speaker":"B","text":"それは大変ですね。急いで洗濯します。"},{"id":{"d":12,"n":3,"t":1},"speaker":"A","text":"それほどに洗濯物がたまっているのですか？"},{"id":{"d":12,"n":4,"t":1},"speaker":"B","text":"干す場所が足りないくらいたまっています。洗濯物は、ほとんどのものがあなたの部活のユニフォームなどですよ。"},{"id":{"d":12,"n":5,"t":1},"speaker":"A","text":"そういえば、部室に忘れていたものをまとめて持ち帰ってきたのでした。"},{"id":{"d":12,"n":6,"t":1},"speaker":"B","text":"毎日少しずつ洗濯したいので、忘れないで持ち帰ってきてくださいね。"},{"id":{"d":12,"n":7,"t":1},"speaker":"A","text":"ごめんなさい、今度からは気を付けます。"}];
INSERT INTO sentence [{"id":{"d":13,"n":1,"t":1},"speaker":"A","text":"天気予報では雨でしたが、見事に晴れましたね。"},{"id":{"d":13,"n":2,"t":1},"speaker":"B","text":"そうですね。最近、天気予報があまり当たらない気がします。"},{"id":{"d":13,"n":3,"t":1},"speaker":"A","text":"この時期は天気が変わりやすく、予報が難しいのかもしれませんね。明日も予報は雨ですが、傘を持って行きますか？"},{"id":{"d":13,"n":4,"t":1},"speaker":"B","text":"はい。念のため折り畳み傘を持って行こうと思います。"},{"id":{"d":13,"n":5,"t":1},"speaker":"A","text":"それなら邪魔になりませんね。僕も折り畳み傘を持って行くことにします。"}];
INSERT INTO sentence [{"id":{"d":14,"n":1,"t":1},"speaker":"A","text":"大分県のお薦めの観光地はありますか？"},{"id":{"d":14,"n":2,"t":1},"speaker":"B","text":"湯布院や久住高原は自然が豊かで良いですよ。"},{"id":{"d":14,"n":3,"t":1},"speaker":"A","text":"なるほど、大分県内には川釣りができる場所はありますか？"},{"id":{"d":14,"n":4,"t":1},"speaker":"B","text":"由布川でできます。湯布院は温泉も素晴らしいですよ。"},{"id":{"d":14,"n":5,"t":1},"speaker":"A","text":"温泉とはいいですね。釣りの後に温泉に入ると気持ちよさそうです。"},{"id":{"d":14,"n":6,"t":1},"speaker":"B","text":"いいですね。何だか僕も行きたくなってきました。"},{"id":{"d":14,"n":7,"t":1},"speaker":"A","text":"予定が合えば一緒に行きませんか？"},{"id":{"d":14,"n":8,"t":1},"speaker":"B","text":"ぜひ、行きましょう。"}];
INSERT INTO sentence [{"id":{"d":15,"n":1,"t":1},"speaker":"A","text":"村山さんは、温泉は好きですか？"},{"id":{"d":15,"n":2,"t":1},"speaker":"B","text":"好きですよ。栗田さんも温泉が好きなのですか？"},{"id":{"d":15,"n":3,"t":1},"speaker":"A","text":"私も大好きです。函館駅の近くにある温泉によく通っています。"},{"id":{"d":15,"n":4,"t":1},"speaker":"B","text":"駅の近くにあるのですか。それは便利そうですね。僕も今度行ってみたいです。"},{"id":{"d":15,"n":5,"t":1},"speaker":"A","text":"後でその温泉の場所を教えますね。ぜひ行ってみてください。"}];
INSERT INTO sentence [{"id":{"d":16,"n":1,"t":1},"speaker":"A","text":"最近は雨続きで外出ができず、時間を持て余しています。何か良い時間の使い方を知っていますか？"},{"id":{"d":16,"n":2,"t":1},"speaker":"B","text":"インターネット将棋とかはどうですか？インターネットだと室内で離れた場所にいる人と遊べますよ。"},{"id":{"d":16,"n":3,"t":1},"speaker":"A","text":"それは面白そうですね。これを機に将棋のルールを覚えるのも良さそうです。"},{"id":{"d":16,"n":4,"t":1},"speaker":"B","text":"ルールは簡単なので、僕が教えますよ。"},{"id":{"d":16,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。将棋は強いのですか？"},{"id":{"d":16,"n":6,"t":1},"speaker":"B","text":"アマチュア1級です。"},{"id":{"d":16,"n":7,"t":1},"speaker":"A","text":"それはすごいですね。ぜひ、教えてください。"}];
INSERT INTO sentence [{"id":{"d":17,"n":1,"t":1},"speaker":"A","text":"急に、てんぷらが食べたくなりました。"},{"id":{"d":17,"n":2,"t":1},"speaker":"B","text":"冷蔵庫にエビがあったはずです。"},{"id":{"d":17,"n":3,"t":1},"speaker":"A","text":"エビの他に何か具材はありますか？"},{"id":{"d":17,"n":4,"t":1},"speaker":"B","text":"海鮮だとイカがあって、野菜だとあなたの好きなナスとかぼちゃがあります。"},{"id":{"d":17,"n":5,"t":1},"speaker":"A","text":"シイタケのてんぷらも食べたいので、買い物に行きましょう。"},{"id":{"d":17,"n":6,"t":1},"speaker":"B","text":"ちょうど隣町のスーパーマーケットがキノコの安売りをしていますよ。"},{"id":{"d":17,"n":7,"t":1},"speaker":"A","text":"では、隣町のスーパーマーケットに行きましょう。"}];
INSERT INTO sentence [{"id":{"d":18,"n":1,"t":1},"speaker":"A","text":"今日も疲れましたね。中西さんは自宅に帰ってから何をしますか？"},{"id":{"d":18,"n":2,"t":1},"speaker":"B","text":"犬の散歩をしてから夕食の買い出しに行きます。岡さんは？"},{"id":{"d":18,"n":3,"t":1},"speaker":"A","text":"まずシャワーを浴びてから、ドラマを見る予定です。"},{"id":{"d":18,"n":4,"t":1},"speaker":"B","text":"何のドラマですか？"},{"id":{"d":18,"n":5,"t":1},"speaker":"A","text":"刑事ドラマです。毎週楽しみにしています。"},{"id":{"d":18,"n":6,"t":1},"speaker":"B","text":"面白そうですね。私も見てみようかしら。"},{"id":{"d":18,"n":7,"t":1},"speaker":"A","text":"刑事ドラマは、主人公に正義感があるので見ているとストレス発散になりますよ。ぜひ、見てほしいです。"}];
INSERT INTO sentence [{"id":{"d":19,"n":1,"t":1},"speaker":"A","text":"最近、楽曲の制作を始めました。"},{"id":{"d":19,"n":2,"t":1},"speaker":"B","text":"すごいですね。どのような曲を作るのですか？"},{"id":{"d":19,"n":3,"t":1},"speaker":"A","text":"ポップな曲を作ることが多いです。"},{"id":{"d":19,"n":4,"t":1},"speaker":"B","text":"ポップとは、Jポップのような曲ですか？"},{"id":{"d":19,"n":5,"t":1},"speaker":"A","text":"そのような感じです。作曲は問題ないのですが、作詞が難しくていつも困っています。"},{"id":{"d":19,"n":6,"t":1},"speaker":"B","text":"僕はポエムを書くことが好きなので、もし、よろしければ相談に乗りますよ。"},{"id":{"d":19,"n":7,"t":1},"speaker":"A","text":"それは頼もしいです。お願いしてもよいですか？"},{"id":{"d":19,"n":8,"t":1},"speaker":"B","text":"もちろんです。任せてください。"}];
INSERT INTO sentence [{"id":{"d":20,"n":1,"t":1},"speaker":"A","text":"先日、川で遊んでいたら、たくさんの大きな魚を見かけました。"},{"id":{"d":20,"n":2,"t":1},"speaker":"B","text":"それはすごいですね。今度その魚を釣りに行きませんか？"},{"id":{"d":20,"n":3,"t":1},"speaker":"A","text":"いいですね。来週は空いていますか？"},{"id":{"d":20,"n":4,"t":1},"speaker":"B","text":"では、来週の日曜日に行きましょう。"},{"id":{"d":20,"n":5,"t":1},"speaker":"A","text":"そうしましょう。仕掛けを作るのは私に任せてください。"},{"id":{"d":20,"n":6,"t":1},"speaker":"B","text":"仕掛けを作るのは任せました。僕は、大きなクーラーボックスを用意しておきます。"}];
INSERT INTO sentence [{"id":{"d":21,"n":1,"t":1},"speaker":"A","text":"今年のクリスマスの予定は決まりましたか？"},{"id":{"d":21,"n":2,"t":1},"speaker":"B","text":"まだ決まっていません。秋山さんはどうされますか？"},{"id":{"d":21,"n":3,"t":1},"speaker":"A","text":"私もまだ決まっていません。ただ、今年はひとりぼっちで過ごすクリスマスになりそうです。"},{"id":{"d":21,"n":4,"t":1},"speaker":"B","text":"彼氏がいるのに会えないのですか？"},{"id":{"d":21,"n":5,"t":1},"speaker":"A","text":"今年は、仕事が忙しいようです。"},{"id":{"d":21,"n":6,"t":1},"speaker":"B","text":"それは残念ですね。そうしたら私とパーティーしませんか？"},{"id":{"d":21,"n":7,"t":1},"speaker":"A","text":"いいですね。プレゼント交換もしましょう。"},{"id":{"d":21,"n":8,"t":1},"speaker":"B","text":"プレゼント交換ですか、懐かしいです。当日を、楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":22,"n":1,"t":1},"speaker":"A","text":"クリスマスプレゼントは、何がいいですか？"},{"id":{"d":22,"n":2,"t":1},"speaker":"B","text":"くまのぬいぐるみかかわいいお洋服が欲しいです。"},{"id":{"d":22,"n":3,"t":1},"speaker":"A","text":"一番欲しいのはどちらですか？"},{"id":{"d":22,"n":4,"t":1},"speaker":"B","text":"決められないので、ママが決めてください。"},{"id":{"d":22,"n":5,"t":1},"speaker":"A","text":"では、あなたがずっと欲しがっていたくまのぬいぐるみにしましょう。"},{"id":{"d":22,"n":6,"t":1},"speaker":"B","text":"はい、そうします。"}];
INSERT INTO sentence [{"id":{"d":23,"n":1,"t":1},"speaker":"A","text":"今年のお正月は、どう過ごすか決めましたか？"},{"id":{"d":23,"n":2,"t":1},"speaker":"B","text":"まだ9月ですよ。気が早すぎませんか？"},{"id":{"d":23,"n":3,"t":1},"speaker":"A","text":"のんびりしていると、あっという間にお正月になりますよ。"},{"id":{"d":23,"n":4,"t":1},"speaker":"B","text":"そのように急ぐ理由は、あるのでしょうか？"},{"id":{"d":23,"n":5,"t":1},"speaker":"A","text":"今年こそ、人気店の限定販売のおせちを予約したいと思っています。毎年、早い時期に売り切れてしまいます。"},{"id":{"d":23,"n":6,"t":1},"speaker":"B","text":"なるほど、そういう理由でしたか。今年は無事予約ができるといいですね。"}];
INSERT INTO sentence [{"id":{"d":24,"n":1,"t":1},"speaker":"A","text":"何か熱中している事は、ありますか？"},{"id":{"d":24,"n":2,"t":1},"speaker":"B","text":"最近は、パソコンの勉強に熱中しています。"},{"id":{"d":24,"n":3,"t":1},"speaker":"A","text":"プログラムとかを作るのですか？"},{"id":{"d":24,"n":4,"t":1},"speaker":"B","text":"いえ、今はExcelの勉強をしています。"},{"id":{"d":24,"n":5,"t":1},"speaker":"A","text":"Excelはマクロや関数が難しそうですね。"},{"id":{"d":24,"n":6,"t":1},"speaker":"B","text":"難しいですが、楽しいですよ。"},{"id":{"d":24,"n":7,"t":1},"speaker":"A","text":"なぜパソコンの勉強を始めたのですか？"},{"id":{"d":24,"n":8,"t":1},"speaker":"B","text":"家計簿を自分で作ってみたくて。勉強しながら少しずつ作っています。"}];
INSERT INTO sentence [{"id":{"d":25,"n":1,"t":1},"speaker":"A","text":"信号機の故障により、ただいま電車が遅れております。"},{"id":{"d":25,"n":2,"t":1},"speaker":"B","text":"すみません、時間の遅れはどれくらい出そうですか？"},{"id":{"d":25,"n":3,"t":1},"speaker":"A","text":"申し訳ございません、運行再開は未定となっております。"},{"id":{"d":25,"n":4,"t":1},"speaker":"B","text":"そうですか。遅延証明書は発行してもらえますか？"},{"id":{"d":25,"n":5,"t":1},"speaker":"A","text":"はい、あちらの窓口で発行しております。"},{"id":{"d":25,"n":6,"t":1},"speaker":"B","text":"多くの人が並んでいる所ですね。ありがとうございます。"},{"id":{"d":25,"n":7,"t":1},"speaker":"A","text":"恐れ入ります。ご迷惑をおかけいたしまして、大変申し訳ございません。"}];
INSERT INTO sentence [{"id":{"d":26,"n":1,"t":1},"speaker":"A","text":"お父さん、おなかがすきました。"},{"id":{"d":26,"n":2,"t":1},"speaker":"B","text":"いっぱい遊んだからですね。何かおいしいものを食べに行きましょう。"},{"id":{"d":26,"n":3,"t":1},"speaker":"A","text":"ステーキが食べたいです。"},{"id":{"d":26,"n":4,"t":1},"speaker":"B","text":"いいですね。お肉だけでなく野菜もしっかり食べてくださいね。"},{"id":{"d":26,"n":5,"t":1},"speaker":"A","text":"野菜は嫌いです。"},{"id":{"d":26,"n":6,"t":1},"speaker":"B","text":"君が大好きなスポーツ選手はみんな野菜も食べていますよ。"},{"id":{"d":26,"n":7,"t":1},"speaker":"A","text":"じゃあ僕も頑張って野菜を食べます。"},{"id":{"d":26,"n":8,"t":1},"speaker":"B","text":"約束ですよ。では、洋服を着替えて出かけましょう。"}];
INSERT INTO sentence [{"id":{"d":27,"n":1,"t":1},"speaker":"A","text":"顔色が悪いようですが、体調が悪いのですか？"},{"id":{"d":27,"n":2,"t":1},"speaker":"B","text":"昨日、夜更かししたせいかもしれません。それほどひどいですか？"},{"id":{"d":27,"n":3,"t":1},"speaker":"A","text":"普段とはかなり違います。病院で診てもらってはどうでしょうか？早めに診てもらえば安心しますよ。"},{"id":{"d":27,"n":4,"t":1},"speaker":"B","text":"そうですね。実は朝から具合も悪いのでこのあと病院で診てもらおうと思います。"},{"id":{"d":27,"n":5,"t":1},"speaker":"A","text":"一人で行けますか？心配なので一緒に行きましょうか？"},{"id":{"d":27,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。一緒に行ってくださると心強いです。"}];
INSERT INTO sentence [{"id":{"d":28,"n":1,"t":1},"speaker":"A","text":"お財布を買い替えたいのですが、長財布かがまぐち財布かで悩んでいます。"},{"id":{"d":28,"n":2,"t":1},"speaker":"B","text":"私はがまぐち財布を使っています。"},{"id":{"d":28,"n":3,"t":1},"speaker":"A","text":"使い心地はどうですか？小銭入れの部分がすぐに開いて便利そうですよね。"},{"id":{"d":28,"n":4,"t":1},"speaker":"B","text":"小銭を入れるには便利です。お札は入らないから別のお財布が必要ですけどね。"},{"id":{"d":28,"n":5,"t":1},"speaker":"A","text":"なるほど、2つのお財布を持ち歩くことになるのですね。"}];
INSERT INTO sentence [{"id":{"d":29,"n":1,"t":1},"speaker":"A","text":"この音は何ですか？"},{"id":{"d":29,"n":2,"t":1},"speaker":"B","text":"緊急地震速報の報知音です。すぐに机の下に隠れましょう。"},{"id":{"d":29,"n":3,"t":1},"speaker":"A","text":"突然のチャイムでとても驚きました。少し揺れていますね。"},{"id":{"d":29,"n":4,"t":1},"speaker":"B","text":"やっと地震が収まったようです。今回は揺れが小さくて良かったですが、心構えは大切ですね。"},{"id":{"d":29,"n":5,"t":1},"speaker":"A","text":"いつ地震がきても落ち着いて行動できるように、避難訓練や防災グッズを用意しておこうと思います。"}];
INSERT INTO sentence [{"id":{"d":30,"n":1,"t":1},"speaker":"A","text":"旅行に行きたいのですが、どこかお薦めの場所はありますか？"},{"id":{"d":30,"n":2,"t":1},"speaker":"B","text":"広島の嚴島神社がお薦めです。鳥居が海の中に立っています。"},{"id":{"d":30,"n":3,"t":1},"speaker":"A","text":"神秘的ですてきですね。"},{"id":{"d":30,"n":4,"t":1},"speaker":"B","text":"他にも海辺で鹿が寝ている姿が見られますよ。"},{"id":{"d":30,"n":5,"t":1},"speaker":"A","text":"その鹿は、自然に生息しているのですか？"},{"id":{"d":30,"n":6,"t":1},"speaker":"B","text":"自力で泳いできたともいわれているので野生だと思います。癒されるので、ぜひ、行ってみてください。"},{"id":{"d":30,"n":7,"t":1},"speaker":"A","text":"興味が湧いてきました。旅程を調べてみます。"}];
INSERT INTO sentence [{"id":{"d":31,"n":1,"t":1},"speaker":"A","text":"明日の算数のテストは、高得点の自信があるので結果を楽しみにしていてください。"},{"id":{"d":31,"n":2,"t":1},"speaker":"B","text":"テストで90点以上取れたら夕飯はあなたの好物にしましょうか？"},{"id":{"d":31,"n":3,"t":1},"speaker":"A","text":"ますますやる気になってきました。"},{"id":{"d":31,"n":4,"t":1},"speaker":"B","text":"あなたは、唐揚げとパスタが好きですよね。パスタの味付けは、ナポリタンでよいですか？"},{"id":{"d":31,"n":5,"t":1},"speaker":"A","text":"はい。ナポリタンは、お母さんの得意料理ですね。"},{"id":{"d":31,"n":6,"t":1},"speaker":"B","text":"ナポリタンは、ピーマンとタマネギとウインナーを入れて、しっかりケチャップで炒めることがコツです。"},{"id":{"d":31,"n":7,"t":1},"speaker":"A","text":"ナポリタンを楽しみに勉強してきます。"},{"id":{"d":31,"n":8,"t":1},"speaker":"B","text":"あまり遅くまで勉強して寝不足にならないよう、ほどほどにしてくださいね。"},{"id":{"d":31,"n":9,"t":1},"speaker":"A","text":"分かりました。心配してくれてありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":32,"n":1,"t":1},"speaker":"A","text":"荻原君は、ブラウン管テレビを見たことありますか？"},{"id":{"d":32,"n":2,"t":1},"speaker":"B","text":"あります。昔、祖父の家に置いてありました。"},{"id":{"d":32,"n":3,"t":1},"speaker":"A","text":"若いのに珍しいですね。見たことがないと思いました。"},{"id":{"d":32,"n":4,"t":1},"speaker":"B","text":"今は薄型カラーテレビなので、ブラウン管テレビは全く見かけなくなりましたね。"},{"id":{"d":32,"n":5,"t":1},"speaker":"A","text":"そうですね。あの砂嵐の画面が懐かしいです。"},{"id":{"d":32,"n":6,"t":1},"speaker":"B","text":"あの画面の映像は、砂嵐と呼ぶのですね。"},{"id":{"d":32,"n":7,"t":1},"speaker":"A","text":"はい。白と黒の画面になることを砂嵐と言っていました。"},{"id":{"d":32,"n":8,"t":1},"speaker":"B","text":"それは初めて知りました。"}];
INSERT INTO sentence [{"id":{"d":33,"n":1,"t":1},"speaker":"A","text":"何かスポーツしていましたか？"},{"id":{"d":33,"n":2,"t":1},"speaker":"B","text":"小学生の時に剣道をしていました。地方大会で優勝したこともあります。あなたはどうですか？"},{"id":{"d":33,"n":3,"t":1},"speaker":"A","text":"それはすごいですね。私は、水泳をしていました。"},{"id":{"d":33,"n":4,"t":1},"speaker":"B","text":"水泳ですか。泳ぎはからっきし駄目なので、ぜひ教えてほしいです。"},{"id":{"d":33,"n":5,"t":1},"speaker":"A","text":"もちろんです。今度、一緒にプールへ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":34,"n":1,"t":1},"speaker":"A","text":"新しいスマートフォンのケースを買おうと考えていますが、手帳タイプのケースは使いやすいですか？"},{"id":{"d":34,"n":2,"t":1},"speaker":"B","text":"スマートフォンの画面も保護できるのでいいですよ。"},{"id":{"d":34,"n":3,"t":1},"speaker":"A","text":"手帳タイプは、カードも収納できますよね？"},{"id":{"d":34,"n":4,"t":1},"speaker":"B","text":"それもできますが、僕は使っていません。"},{"id":{"d":34,"n":5,"t":1},"speaker":"A","text":"なぜですか？"},{"id":{"d":34,"n":6,"t":1},"speaker":"B","text":"カードを入れるとケースが膨らんで、使いにくくなるからです。"},{"id":{"d":34,"n":7,"t":1},"speaker":"A","text":"使いにくくなるのですね。参考になりました。"}];
INSERT INTO sentence [{"id":{"d":35,"n":1,"t":1},"speaker":"A","text":"長谷川さん、明日のお昼の時間帯は空いていますか？"},{"id":{"d":35,"n":2,"t":1},"speaker":"B","text":"すみません。デートの予定があります。"},{"id":{"d":35,"n":3,"t":1},"speaker":"A","text":"そうなのですね。楽しんできてください。"},{"id":{"d":35,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。来週の同じ時間は空いていますよ。"},{"id":{"d":35,"n":5,"t":1},"speaker":"A","text":"では、来週、映画館へ行きませんか？"},{"id":{"d":35,"n":6,"t":1},"speaker":"B","text":"映画を見に行くことはいいですね。ぜひ、行きましょう。"}];
INSERT INTO sentence [{"id":{"d":36,"n":1,"t":1},"speaker":"A","text":"マフラーを編むのは大変ですか？"},{"id":{"d":36,"n":2,"t":1},"speaker":"B","text":"大変です。長く編もうと思えばその分時間がかかります。"},{"id":{"d":36,"n":3,"t":1},"speaker":"A","text":"やはり大変ですか。でも、手編みのマフラーをプレゼントにしたいから挑戦してみようと思います。"},{"id":{"d":36,"n":4,"t":1},"speaker":"B","text":"すてきなプレゼントですね。きっと喜ばれますよ。"},{"id":{"d":36,"n":5,"t":1},"speaker":"A","text":"そうですよね。喜んでもらえるように頑張ってみます。"}];
INSERT INTO sentence [{"id":{"d":37,"n":1,"t":1},"speaker":"A","text":"キャラメルは好きですか？"},{"id":{"d":37,"n":2,"t":1},"speaker":"B","text":"大好きです。"},{"id":{"d":37,"n":3,"t":1},"speaker":"A","text":"おいしいですよね。国内で大正3年に発売されたキャラメルは、禁煙のためのお菓子という主旨のキャッチフレーズで売り出されたという事は知っていましたか？"},{"id":{"d":37,"n":4,"t":1},"speaker":"B","text":"それは初めて聞きました。"},{"id":{"d":37,"n":5,"t":1},"speaker":"A","text":"僕も知ったときは驚きました。お菓子はその起源が薬のものがいくつかあります。調べると楽しいですよ。"},{"id":{"d":37,"n":6,"t":1},"speaker":"B","text":"面白いですね。時間がある時に調べてみます。"}];
INSERT INTO sentence [{"id":{"d":38,"n":1,"t":1},"speaker":"A","text":"昨夜テレビで放送していたドッキリ番組を見ましたか？"},{"id":{"d":38,"n":2,"t":1},"speaker":"B","text":"はい、見ました。ドッキリを仕掛けられた人のリアクションはすごかったですね。"},{"id":{"d":38,"n":3,"t":1},"speaker":"A","text":"差し入れのお弁当を開けたら、空っぽだった時は凄い顔をしていましたね。"},{"id":{"d":38,"n":4,"t":1},"speaker":"B","text":"お弁当が空っぽだったら、驚きますよね。"},{"id":{"d":38,"n":5,"t":1},"speaker":"A","text":"明日もドッキリ番組を放送するようですよ。"},{"id":{"d":38,"n":6,"t":1},"speaker":"B","text":"明日は、塾へ行く予定があるからテレビ番組を録画します。"},{"id":{"d":38,"n":7,"t":1},"speaker":"A","text":"録画したテレビ番組は、いつ見ていますか？"},{"id":{"d":38,"n":8,"t":1},"speaker":"B","text":"勉強の合間など、息抜きしたい時に見ています。"}];
INSERT INTO sentence [{"id":{"d":39,"n":1,"t":1},"speaker":"A","text":"沙織、期末試験があるって言っていましたが、試験勉強を頑張っていますか？"},{"id":{"d":39,"n":2,"t":1},"speaker":"B","text":"はい。国語は大丈夫そうですが、数学は得点する自信がなくて困っています。"},{"id":{"d":39,"n":3,"t":1},"speaker":"A","text":"数学は基礎ができていないと、応用問題ができないので、復習が大事ですよ。"},{"id":{"d":39,"n":4,"t":1},"speaker":"B","text":"アドバイスをいただき、ありがとうございます。頑張ります。"}];
INSERT INTO sentence [{"id":{"d":40,"n":1,"t":1},"speaker":"A","text":"今度の土曜日、何か用事はありますか？"},{"id":{"d":40,"n":2,"t":1},"speaker":"B","text":"特にありませんが、どうかしましたか？"},{"id":{"d":40,"n":3,"t":1},"speaker":"A","text":"実は、ニューヨークから僕の彼女が帰って来るのです。一緒に食事でもと思いまして。"},{"id":{"d":40,"n":4,"t":1},"speaker":"B","text":"せっかくニューヨークから帰ってくるのに僕もご一緒してよいのですか？"},{"id":{"d":40,"n":5,"t":1},"speaker":"A","text":"加藤さんに僕の彼女を紹介したいのです。"},{"id":{"d":40,"n":6,"t":1},"speaker":"B","text":"それはうれしいです。では、東京駅の近くにお薦めの和食のお店があるので、そこで食事をするのはいかがでしょうか？"},{"id":{"d":40,"n":7,"t":1},"speaker":"A","text":"いいですね。きっと彼女も喜ぶと思います。"},{"id":{"d":40,"n":8,"t":1},"speaker":"B","text":"それでは、予約しておきます。"}];
INSERT INTO sentence [{"id":{"d":41,"n":1,"t":1},"speaker":"A","text":"最近、寒くて夜に目が覚めてしまいます。"},{"id":{"d":41,"n":2,"t":1},"speaker":"B","text":"確かに、最近はよく冷えますね。電気毛布を試してみるのはいかがですか？"},{"id":{"d":41,"n":3,"t":1},"speaker":"A","text":"電気毛布は温かそうですが、低温やけどが心配です。"},{"id":{"d":41,"n":4,"t":1},"speaker":"B","text":"タイマーをかけておけば大丈夫ですよ。"},{"id":{"d":41,"n":5,"t":1},"speaker":"A","text":"なるほど。では今度、電気毛布を試してみます。"}];
INSERT INTO sentence [{"id":{"d":42,"n":1,"t":1},"speaker":"A","text":"あの大きな山は、火山ですか？"},{"id":{"d":42,"n":2,"t":1},"speaker":"B","text":"前に登った時に火口のようなものがあったので、火山だと思います。"},{"id":{"d":42,"n":3,"t":1},"speaker":"A","text":"火口に行ってみたいです。マグマは見えるでしょうか？"},{"id":{"d":42,"n":4,"t":1},"speaker":"B","text":"マグマが見えるような活火山ではありませんが、すごい迫力ですよ。"},{"id":{"d":42,"n":5,"t":1},"speaker":"A","text":"眺めも良さそうですね。登るのは大変ですか？"},{"id":{"d":42,"n":6,"t":1},"speaker":"B","text":"途中までは車で行けます。その先は、徒歩で登るのですが大変です。火山の写真がありますが見ますか？"},{"id":{"d":42,"n":7,"t":1},"speaker":"A","text":"はい、ぜひ写真を見せてください。"}];
INSERT INTO sentence [{"id":{"d":43,"n":1,"t":1},"speaker":"A","text":"先生、おはようございます。"},{"id":{"d":43,"n":2,"t":1},"speaker":"B","text":"おはようございます。加山君、今日も元気ですね。"},{"id":{"d":43,"n":3,"t":1},"speaker":"A","text":"はい。昨日のテストで良い点を取れたのがうれしいのです。"},{"id":{"d":43,"n":4,"t":1},"speaker":"B","text":"確かに昨日のテストは良い点でしたものね。親御さんも喜んでいたのではないでしょうか？"},{"id":{"d":43,"n":5,"t":1},"speaker":"A","text":"はい。とても喜んでくれて、欲しかったゲームを買ってもらえそうです。"},{"id":{"d":43,"n":6,"t":1},"speaker":"B","text":"それは良かったですね。しかし、ゲームのやりすぎで次のテストの点数が悪くならないようにしましょう。"},{"id":{"d":43,"n":7,"t":1},"speaker":"A","text":"はい。そうならないように頑張ります。"}];
INSERT INTO sentence [{"id":{"d":44,"n":1,"t":1},"speaker":"A","text":"明日の算数のテストは、良い点数を取る自信がありません。"},{"id":{"d":44,"n":2,"t":1},"speaker":"B","text":"この前も点数が低かったでしょう？もう少し頑張って勉強したらいかがですか？"},{"id":{"d":44,"n":3,"t":1},"speaker":"A","text":"今回算数のテストは九九ですが、どうしたらよいか分かりますか？"},{"id":{"d":44,"n":4,"t":1},"speaker":"B","text":"九九は暗記がポイントです。"},{"id":{"d":44,"n":5,"t":1},"speaker":"A","text":"いつも、8の段と9の段が混乱してしまいます。"},{"id":{"d":44,"n":6,"t":1},"speaker":"B","text":"混乱してしまうのであれば、九九の歌があるので一緒に歌で覚えてみませんか？"},{"id":{"d":44,"n":7,"t":1},"speaker":"A","text":"九九の歌があるなんて知りませんでした。"},{"id":{"d":44,"n":8,"t":1},"speaker":"B","text":"お母さんは昔歌で九九を覚えました。歌で覚えると楽しいので、お風呂でよく歌っていました。"}];
INSERT INTO sentence [{"id":{"d":45,"n":1,"t":1},"speaker":"A","text":"筋肉をつけて、他の人から「たくましい」と言われてみたいです。"},{"id":{"d":45,"n":2,"t":1},"speaker":"B","text":"インターネットで筋肉トレーニングの動画を見つけることができますよ。参考になると思います。"},{"id":{"d":45,"n":3,"t":1},"speaker":"A","text":"早速、探してみます。こちらの動画でしょうか？"},{"id":{"d":45,"n":4,"t":1},"speaker":"B","text":"そうです。スクワットなどを行い、足の筋肉を鍛えると良いそうですね。"},{"id":{"d":45,"n":5,"t":1},"speaker":"A","text":"下半身の強化が大切なのですね。"},{"id":{"d":45,"n":6,"t":1},"speaker":"B","text":"あとは、体幹を鍛えるのも良いらしいですよ。"},{"id":{"d":45,"n":7,"t":1},"speaker":"A","text":"早速、今日から実行してみます。"}];
INSERT INTO sentence [{"id":{"d":46,"n":1,"t":1},"speaker":"A","text":"最近、地震が多いですね。"},{"id":{"d":46,"n":2,"t":1},"speaker":"B","text":"そうですね。何か大きな地震の前触れでしょうか？"},{"id":{"d":46,"n":3,"t":1},"speaker":"A","text":"防災グッズとか、そろえた方がいいですね。"},{"id":{"d":46,"n":4,"t":1},"speaker":"B","text":"そろえていても、いざという時に使えないと困りますね。"},{"id":{"d":46,"n":5,"t":1},"speaker":"A","text":"そうしたら、防災訓練として、防災グッズを使ったキャンプをしてみませんか？"},{"id":{"d":46,"n":6,"t":1},"speaker":"B","text":"それは良いアイデアですね。"},{"id":{"d":46,"n":7,"t":1},"speaker":"A","text":"では、今度の日曜日に一緒にキャンプへ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":47,"n":1,"t":1},"speaker":"A","text":"沖縄の軽石について取り上げたニュース番組を見ましたか？"},{"id":{"d":47,"n":2,"t":1},"speaker":"B","text":"見ました。漂流した軽石で港が真っ白になっていて、とても驚きました。"},{"id":{"d":47,"n":3,"t":1},"speaker":"A","text":"海中の映像も、昼間なのに真っ暗でしたね。"},{"id":{"d":47,"n":4,"t":1},"speaker":"B","text":"はい、衝撃的でした。軽石によって、エンジンが故障するので船も出せないらしいです。"},{"id":{"d":47,"n":5,"t":1},"speaker":"A","text":"火山の噴火の影響は、陸地だけでなく海でも怖いものなのですね。"}];
INSERT INTO sentence [{"id":{"d":48,"n":1,"t":1},"speaker":"A","text":"生ごみは、いつ収集場に出せばよいですか？"},{"id":{"d":48,"n":2,"t":1},"speaker":"B","text":"生ごみの収集日は、毎週火曜日です。"},{"id":{"d":48,"n":3,"t":1},"speaker":"A","text":"何時までに、収集場にごみを出せばよいですか？"},{"id":{"d":48,"n":4,"t":1},"speaker":"B","text":"7時までに出してください。収集場は分かりますか？"},{"id":{"d":48,"n":5,"t":1},"speaker":"A","text":"分からないので、教えていただけますか？"},{"id":{"d":48,"n":6,"t":1},"speaker":"B","text":"家から出て、右に曲がった角のところです。"},{"id":{"d":48,"n":7,"t":1},"speaker":"A","text":"分かりました。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":49,"n":1,"t":1},"speaker":"A","text":"今度のゴールデンウィークの予定は、決まっていますか？"},{"id":{"d":49,"n":2,"t":1},"speaker":"B","text":"前から気になっていた、映画を見に行く予定です。"},{"id":{"d":49,"n":3,"t":1},"speaker":"A","text":"どのような内容の映画なのですか？"},{"id":{"d":49,"n":4,"t":1},"speaker":"B","text":"今度公開される大作SF映画です。"},{"id":{"d":49,"n":5,"t":1},"speaker":"A","text":"それは僕も興味があります。"},{"id":{"d":49,"n":6,"t":1},"speaker":"B","text":"良かったら一緒に見に行きませんか？火曜日の10時に品川駅の東口に集合しましょう。映画の後は好楽というお店のラーメンとチャーハンを食べようと思っています。"},{"id":{"d":49,"n":7,"t":1},"speaker":"A","text":"いいですね。僕は好楽のギョウザが好きです。好楽のギョウザは、野菜が多めで、野菜の甘味と肉汁のバランスが絶妙だからおいしいです。"}];
INSERT INTO sentence [{"id":{"d":50,"n":1,"t":1},"speaker":"A","text":"もしもし。西中学校の大橋と申します。佐藤ハナさんのお母さまでしょうか？"},{"id":{"d":50,"n":2,"t":1},"speaker":"B","text":"はい。そうです。ハナがいつもお世話になっております。"},{"id":{"d":50,"n":3,"t":1},"speaker":"A","text":"ハナさんは発熱の症状があるようですので、今からお迎えに来ていただけますか？"},{"id":{"d":50,"n":4,"t":1},"speaker":"B","text":"発熱の症状がありますか。今朝は元気に登校したのですが、最近急に寒くなりましたから、具合が悪くなったのでしょうかね。保健室にお伺いすればよろしいでしょうか？"},{"id":{"d":50,"n":5,"t":1},"speaker":"A","text":"はい。ハナさんは保健室で休んでいるので保健室にお越しください。"},{"id":{"d":50,"n":6,"t":1},"speaker":"B","text":"分かりました。今から、すぐに向かいます。"},{"id":{"d":50,"n":7,"t":1},"speaker":"A","text":"どうぞよろしくお願いします。"}];
INSERT INTO sentence [{"id":{"d":51,"n":1,"t":1},"speaker":"A","text":"このシャープペンシルは、すごく使いやすくてお薦めです。"},{"id":{"d":51,"n":2,"t":1},"speaker":"B","text":"どこで買ったのですか？"},{"id":{"d":51,"n":3,"t":1},"speaker":"A","text":"近くの文房具屋さんで買いました。"},{"id":{"d":51,"n":4,"t":1},"speaker":"B","text":"値段はいくらでしたか？"},{"id":{"d":51,"n":5,"t":1},"speaker":"A","text":"500円でした。少し高いと思いましたが、使いやすいのでとても良い買い物でした。"},{"id":{"d":51,"n":6,"t":1},"speaker":"B","text":"勉強がはかどりそうですね。"}];
INSERT INTO sentence [{"id":{"d":52,"n":1,"t":1},"speaker":"A","text":"ホラー映画を見た後、トイレに一人で行くことはどうしても怖いですよね。"},{"id":{"d":52,"n":2,"t":1},"speaker":"B","text":"子どもの頃は怖かったです。一人でお風呂に入ることも怖かった記憶があります。"},{"id":{"d":52,"n":3,"t":1},"speaker":"A","text":"僕は、今でもホラー映画を見た後は、一人でトイレに行ったり、お風呂に入ったりすることが怖いです。"},{"id":{"d":52,"n":4,"t":1},"speaker":"B","text":"でも、ホラー映画を見ることは好きですよね？"},{"id":{"d":52,"n":5,"t":1},"speaker":"A","text":"はい。怖いと思いながらも、なぜか見てしまいます。あのスリルが好きですから。"}];
INSERT INTO sentence [{"id":{"d":53,"n":1,"t":1},"speaker":"A","text":"寒いですね。もう、すっかり冬ですね。"},{"id":{"d":53,"n":2,"t":1},"speaker":"B","text":"そうですね。雪も降ってきましたよ。"},{"id":{"d":53,"n":3,"t":1},"speaker":"A","text":"本当ですね。とても雪景色がきれいです。明日には積もるでしょうか？"},{"id":{"d":53,"n":4,"t":1},"speaker":"B","text":"この雪質ですと3センチメートルくらい積もるかもしれませんね。もし雪が積もったら、雪だるまを作りませんか？"},{"id":{"d":53,"n":5,"t":1},"speaker":"A","text":"いいですね。雪合戦もしたいです。"},{"id":{"d":53,"n":6,"t":1},"speaker":"B","text":"はい。どちらもしましょう。"},{"id":{"d":53,"n":7,"t":1},"speaker":"A","text":"楽しみです。"}];
INSERT INTO sentence [{"id":{"d":54,"n":1,"t":1},"speaker":"A","text":"今日は、特別寒いですね。"},{"id":{"d":54,"n":2,"t":1},"speaker":"B","text":"そうですか？私は過ごしやすい気温だと思います。"},{"id":{"d":54,"n":3,"t":1},"speaker":"A","text":"本当ですか？私はセーターの下にシャツを4枚も着てきました。"},{"id":{"d":54,"n":4,"t":1},"speaker":"B","text":"それはすごく暖かそうですが、お昼になって気温が上がったら暑く感じるのではないでしょうか？"},{"id":{"d":54,"n":5,"t":1},"speaker":"A","text":"そう感じたら、服を1枚ずつ脱げばよいと思います。"},{"id":{"d":54,"n":6,"t":1},"speaker":"B","text":"確かにそうですね。寒くて風邪をひいてしまうよりは厚着の方がいいです。"}];
INSERT INTO sentence [{"id":{"d":55,"n":1,"t":1},"speaker":"A","text":"ここの公園の湖は、インターネットで検索したら観光地として出てきました。"},{"id":{"d":55,"n":2,"t":1},"speaker":"B","text":"そうですか。地元なのに知りませんでした。何か名物はあるのですか？"},{"id":{"d":55,"n":3,"t":1},"speaker":"A","text":"湖にいる白鳥です。"},{"id":{"d":55,"n":4,"t":1},"speaker":"B","text":"言われてみれば確かに珍しいですね。人が歩いていると、白鳥が寄ってきます。"},{"id":{"d":55,"n":5,"t":1},"speaker":"A","text":"人懐こくてかわいい白鳥です。近所に住んでいる老人会がお世話していますよ。"},{"id":{"d":55,"n":6,"t":1},"speaker":"B","text":"それも知りませんでした。きっと、まだまだ私たちの知らない地元の観光地がありますね。"},{"id":{"d":55,"n":7,"t":1},"speaker":"A","text":"そうですね。調べてみたら、もっと地元が好きになりそうです。"}];
INSERT INTO sentence [{"id":{"d":56,"n":1,"t":1},"speaker":"A","text":"もう年末ですね。新年を気持ちよく迎えるために、身なりを整えたいので、そろそろ美容室を予約しようと思います。"},{"id":{"d":56,"n":2,"t":1},"speaker":"B","text":"どのような髪型にするのですか？"},{"id":{"d":56,"n":3,"t":1},"speaker":"A","text":"少しイメージチェンジを行い、パーマをかけようかと思います。"},{"id":{"d":56,"n":4,"t":1},"speaker":"B","text":"良いですね。似合うと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":57,"n":1,"t":1},"speaker":"A","text":"明日、近所のショッピングモール内の映画館へ映画を見に行きませんか？"},{"id":{"d":57,"n":2,"t":1},"speaker":"B","text":"いいですね。どのような映画を見に行くのですか？"},{"id":{"d":57,"n":3,"t":1},"speaker":"A","text":"カーアクションものです。以前、帰宅途中に寄ったおもちゃ屋さんで、あなたがミニカーに夢中になっていらっしゃったので、車がお好きなのかなと思いまして。"},{"id":{"d":57,"n":4,"t":1},"speaker":"B","text":"よく見ていましたね。確かに車は大好きなので、楽しみです。"}];
INSERT INTO sentence [{"id":{"d":58,"n":1,"t":1},"speaker":"A","text":"東京はすてきな所ですね。都会暮らしに憧れます。"},{"id":{"d":58,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。そう言っていただいてうれしいです。"},{"id":{"d":58,"n":3,"t":1},"speaker":"A","text":"あなたが、東京で好きな場所はどこですか？"},{"id":{"d":58,"n":4,"t":1},"speaker":"B","text":"私は代官山と原宿が好きです。どちらも買い物にお薦めです。"},{"id":{"d":58,"n":5,"t":1},"speaker":"A","text":"話を聞いていたら、代官山と原宿へ行きたくなってきました。ゴールデンウィークに訪れたいと思います。"}];
INSERT INTO sentence [{"id":{"d":59,"n":1,"t":1},"speaker":"A","text":"今日は、いろいろな車を見ることができて楽しかったです。"},{"id":{"d":59,"n":2,"t":1},"speaker":"B","text":"そうですね。昔のレーシングカーは武骨で格好良いです。"},{"id":{"d":59,"n":3,"t":1},"speaker":"A","text":"とてもよく分かります。扇風機のようなものがついているものがありましたね。"},{"id":{"d":59,"n":4,"t":1},"speaker":"B","text":"それは、私が生で見たかった車です。イギリスに行ったかいがありました。"}];
INSERT INTO sentence [{"id":{"d":60,"n":1,"t":1},"speaker":"A","text":"田中さんは洋楽を聴きますか？"},{"id":{"d":60,"n":2,"t":1},"speaker":"B","text":"洋楽は大好きです。キャロル・キングが好きですね。聴いていてまったりします。佐藤さんは、洋楽を聴きますか？"},{"id":{"d":60,"n":3,"t":1},"speaker":"A","text":"私は、邦楽を聴きます。洋楽はザ・ビートルズを聴きます。"},{"id":{"d":60,"n":4,"t":1},"speaker":"B","text":"ザ・ビートルズも良いですね。"}];
INSERT INTO sentence [{"id":{"d":61,"n":1,"t":1},"speaker":"A","text":"もうすぐお誕生日ですね。あなたは何歳になるのですか？"},{"id":{"d":61,"n":2,"t":1},"speaker":"B","text":"それは内緒です。ある程度の年齢ですよ。"},{"id":{"d":61,"n":3,"t":1},"speaker":"A","text":"何はともあれ、おめでとうございます。お祝いは何がいいですか？"},{"id":{"d":61,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。お気持ちだけで十分です。"}];
INSERT INTO sentence [{"id":{"d":62,"n":1,"t":1},"speaker":"A","text":"今日のあなたのお弁当箱は小さいですね。食事量は足りますか？"},{"id":{"d":62,"n":2,"t":1},"speaker":"B","text":"今、ダイエット中なので食事量を控えています。"},{"id":{"d":62,"n":3,"t":1},"speaker":"A","text":"体重の減量の目標は何キログラムですか？"},{"id":{"d":62,"n":4,"t":1},"speaker":"B","text":"冬休みで少し体重が増えてしまったので、3キロ減量が目標です。"}];
INSERT INTO sentence [{"id":{"d":63,"n":1,"t":1},"speaker":"A","text":"新しいミニカーを買いました。良かったら、今日私のコレクションを見に来ませんか？"},{"id":{"d":63,"n":2,"t":1},"speaker":"B","text":"良いですね。どのようなミニカーを買ったのですか？"},{"id":{"d":63,"n":3,"t":1},"speaker":"A","text":"最近発売されたばかりのレーシングカーです。"},{"id":{"d":63,"n":4,"t":1},"speaker":"B","text":"ぜひ、見たいです。その他のコレクションも見たいです。"},{"id":{"d":63,"n":5,"t":1},"speaker":"A","text":"では、学校が終わったら一緒に家に帰りましょう。"},{"id":{"d":63,"n":6,"t":1},"speaker":"B","text":"分りました。校門前で待ちますね。"}];
INSERT INTO sentence [{"id":{"d":64,"n":1,"t":1},"speaker":"A","text":"もしもし、園長先生ですか？おはようございます。渡辺です。"},{"id":{"d":64,"n":2,"t":1},"speaker":"B","text":"おはようございます。渡辺さん、どうされましたか？"},{"id":{"d":64,"n":3,"t":1},"speaker":"A","text":"送迎バスが時間になっても来ないので連絡しました。もう、20分も待っています。"},{"id":{"d":64,"n":4,"t":1},"speaker":"B","text":"それは、申し訳ございません。運転手に連絡してみますので、いったんご自宅へ戻って、お待ちいただいてもよろしいですか？また、こちらから連絡いたします。"},{"id":{"d":64,"n":5,"t":1},"speaker":"A","text":"分かりました。お待ちしております。"}];
INSERT INTO sentence [{"id":{"d":65,"n":1,"t":1},"speaker":"A","text":"このカーペット、すごく手触りが良いですね。デザインもすてきです。"},{"id":{"d":65,"n":2,"t":1},"speaker":"B","text":"昨日、銀座のデパートで購入しました。私は、このカーペットのデザインに一目惚れしました。"},{"id":{"d":65,"n":3,"t":1},"speaker":"A","text":"本当にすてきですね。この色使いはなかなか見ませんね。"},{"id":{"d":65,"n":4,"t":1},"speaker":"B","text":"そう言っていただいて、うれしいです。"}];
INSERT INTO sentence [{"id":{"d":66,"n":1,"t":1},"speaker":"A","text":"いろいろなケーキがあって、どれにするか迷います。"},{"id":{"d":66,"n":2,"t":1},"speaker":"B","text":"私は、チーズケーキにしようと思います。あなたはどのようなケーキが好きですか？"},{"id":{"d":66,"n":3,"t":1},"speaker":"A","text":"私はイチゴのショートケーキが大好きです。"},{"id":{"d":66,"n":4,"t":1},"speaker":"B","text":"それでは、まずはイチゴのショートケーキを2つと、チーズケーキを2つずつ注文しましょう。"},{"id":{"d":66,"n":5,"t":1},"speaker":"A","text":"そうしましょう。"}];
INSERT INTO sentence [{"id":{"d":67,"n":1,"t":1},"speaker":"A","text":"少しおなかがすいてきました。おやつを食べませんか？"},{"id":{"d":67,"n":2,"t":1},"speaker":"B","text":"そうですね。おやつは、何が良いですか？"},{"id":{"d":67,"n":3,"t":1},"speaker":"A","text":"台所にドーナツがあったはずです。今、持ってきますね。"},{"id":{"d":67,"n":4,"t":1},"speaker":"B","text":"もしかして、このドーナツは、アーモンドが入っていますか？"},{"id":{"d":67,"n":5,"t":1},"speaker":"A","text":"入っていますよ。苦手でしたか？"},{"id":{"d":67,"n":6,"t":1},"speaker":"B","text":"私はアーモンドアレルギーなので、このドーナツは食べられません。"},{"id":{"d":67,"n":7,"t":1},"speaker":"A","text":"そうでしたか。それでは徒歩10分くらいの場所にカステラ屋さんがあるので、そこへカステラを買いに行きましょう。"},{"id":{"d":67,"n":8,"t":1},"speaker":"B","text":"カステラは大好きでよく食べます。お気遣いいただきありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":68,"n":1,"t":1},"speaker":"A","text":"そろそろ、コーヒータイムにしましょう。"},{"id":{"d":68,"n":2,"t":1},"speaker":"B","text":"いいですね。"},{"id":{"d":68,"n":3,"t":1},"speaker":"A","text":"コーヒーのメニューは、何が良いですか？"},{"id":{"d":68,"n":4,"t":1},"speaker":"B","text":"私はカフェオレが飲みたいです。"},{"id":{"d":68,"n":5,"t":1},"speaker":"A","text":"私もカフェオレが飲みたい気分です。2人分のカフェオレを入れてきますね。"}];
INSERT INTO sentence [{"id":{"d":69,"n":1,"t":1},"speaker":"A","text":"注文はどうしますか？"},{"id":{"d":69,"n":2,"t":1},"speaker":"B","text":"私は、ラーメンギョウザ定食を頼もうと思います。"},{"id":{"d":69,"n":3,"t":1},"speaker":"A","text":"良いですね。僕はうどん定食にしようと思います。それと鉄火丼を頼みます。"},{"id":{"d":69,"n":4,"t":1},"speaker":"B","text":"少し食べすぎではありませんか？"},{"id":{"d":69,"n":5,"t":1},"speaker":"A","text":"そうかもしれません。まずは片方だけ頼もうと思います。"},{"id":{"d":69,"n":6,"t":1},"speaker":"B","text":"どちらにしますか？"},{"id":{"d":69,"n":7,"t":1},"speaker":"A","text":"昨日うどんを食べたので、鉄火丼にしようと思います。"},{"id":{"d":69,"n":8,"t":1},"speaker":"B","text":"了解です。では、店員さんを呼びます。"}];
INSERT INTO sentence [{"id":{"d":70,"n":1,"t":1},"speaker":"A","text":"佐藤さん、好きな食べ物は何ですか？"},{"id":{"d":70,"n":2,"t":1},"speaker":"B","text":"私は卵料理が好きです。"},{"id":{"d":70,"n":3,"t":1},"speaker":"A","text":"卵料理の中で、佐藤さんが特に好きなものは何ですか？"},{"id":{"d":70,"n":4,"t":1},"speaker":"B","text":"私は、卵焼きやスクランブルエッグが好きです。"},{"id":{"d":70,"n":5,"t":1},"speaker":"A","text":"そうなのですね。私は茶碗蒸しが好きです。"}];
INSERT INTO sentence [{"id":{"d":71,"n":1,"t":1},"speaker":"A","text":"佐藤さんは、サンドイッチの中身は何が好きですか？"},{"id":{"d":71,"n":2,"t":1},"speaker":"B","text":"卵サンドです。鈴木さんは、サンドイッチの中身は何が好きですか？"},{"id":{"d":71,"n":3,"t":1},"speaker":"A","text":"私は、カツサンドが好きです。"},{"id":{"d":71,"n":4,"t":1},"speaker":"B","text":"そうなのですね。カツサンドは、ボリュームがあって食べ応えもありますものね。"},{"id":{"d":71,"n":5,"t":1},"speaker":"A","text":"はい。サンドイッチは、いつもカツサンドにしています。"}];
INSERT INTO sentence [{"id":{"d":72,"n":1,"t":1},"speaker":"A","text":"今日のステーキの焼き加減は、どうしましょうか？"},{"id":{"d":72,"n":2,"t":1},"speaker":"B","text":"僕はレアが好きです。あなたの好みの焼き加減は何ですか？"},{"id":{"d":72,"n":3,"t":1},"speaker":"A","text":"私も、レアが好きですが、おなかが弱いのでウェルダンにしようと思います。"},{"id":{"d":72,"n":4,"t":1},"speaker":"B","text":"なるほど。良い選択ですね。"}];
INSERT INTO sentence [{"id":{"d":73,"n":1,"t":1},"speaker":"A","text":"月曜に見ているドラマが今度映画化するらしいですね。"},{"id":{"d":73,"n":2,"t":1},"speaker":"B","text":"本当ですか？"},{"id":{"d":73,"n":3,"t":1},"speaker":"A","text":"公式サイトで見たので、間違いないと思います。"},{"id":{"d":73,"n":4,"t":1},"speaker":"B","text":"映画の主題歌を、誰が歌うのか気になります。"},{"id":{"d":73,"n":5,"t":1},"speaker":"A","text":"人気のドラマだから、誰が歌ってもヒットしそうですね。"},{"id":{"d":73,"n":6,"t":1},"speaker":"B","text":"そうですね。楽しみです。"}];
INSERT INTO sentence [{"id":{"d":74,"n":1,"t":1},"speaker":"A","text":"今度の修学旅行で、初めて新幹線に乗るのでとても楽しみです。"},{"id":{"d":74,"n":2,"t":1},"speaker":"B","text":"私も子どもの頃以来で、とても久しぶりなのでワクワクしています。"},{"id":{"d":74,"n":3,"t":1},"speaker":"A","text":"帰りは飛行機ですね。飛行機も初めて乗るので本当に楽しみです。"},{"id":{"d":74,"n":4,"t":1},"speaker":"B","text":"そうですね。帰りは飛行機機を貸し切るようです。みんなでワイワイ騒げますね。"},{"id":{"d":74,"n":5,"t":1},"speaker":"A","text":"修学旅行当日が待ち遠しいです。"}];
INSERT INTO sentence [{"id":{"d":75,"n":1,"t":1},"speaker":"A","text":"今回の修学旅行は、行き先を3カ所から選択するそうです。東京、大阪、沖縄から選べます。"},{"id":{"d":75,"n":2,"t":1},"speaker":"B","text":"実は、私は九州から出た事がありません。大阪へ行ってみたいです。"},{"id":{"d":75,"n":3,"t":1},"speaker":"A","text":"私も、同じく大阪へ行きたいです。関西弁を聞いてみたいです。"},{"id":{"d":75,"n":4,"t":1},"speaker":"B","text":"では、一緒に大阪を選びましょう。楽しみですね。"}];
INSERT INTO sentence [{"id":{"d":76,"n":1,"t":1},"speaker":"A","text":"今度の休みは、どこかへ出かけますか？"},{"id":{"d":76,"n":2,"t":1},"speaker":"B","text":"家族でドライブに行きます。紅葉を見に行く予定です。"},{"id":{"d":76,"n":3,"t":1},"speaker":"A","text":"良いですね。うちは、父が仕事で忙しいので遠出は無理です。"},{"id":{"d":76,"n":4,"t":1},"speaker":"B","text":"どこにも、出かけないのですか？"},{"id":{"d":76,"n":5,"t":1},"speaker":"A","text":"近場の映画館に、映画を見に行こうと思っています。"},{"id":{"d":76,"n":6,"t":1},"speaker":"B","text":"映画も良いですね。"}];
INSERT INTO sentence [{"id":{"d":77,"n":1,"t":1},"speaker":"A","text":"田中さんの趣味は、何ですか？"},{"id":{"d":77,"n":2,"t":1},"speaker":"B","text":"趣味は、絵を描く事です。"},{"id":{"d":77,"n":3,"t":1},"speaker":"A","text":"どのような絵を書いているのですか？"},{"id":{"d":77,"n":4,"t":1},"speaker":"B","text":"私は、イラストや漫画を描いています。"},{"id":{"d":77,"n":5,"t":1},"speaker":"A","text":"そうなのですね。今度、絵の描き方を教えてください。"},{"id":{"d":77,"n":6,"t":1},"speaker":"B","text":"良いですよ。今度、遊ぶ時にお教えしますね。"}];
INSERT INTO sentence [{"id":{"d":78,"n":1,"t":1},"speaker":"A","text":"こういう寒い日には、やっぱり鍋料理が良いですね。夕飯は鍋料理にしましょう。"},{"id":{"d":78,"n":2,"t":1},"speaker":"B","text":"まずは、食材の買い出しですね。旬の食材をたくさん使った鍋料理が良いと思います。"},{"id":{"d":78,"n":3,"t":1},"speaker":"A","text":"何を買えばよいか迷います。"},{"id":{"d":78,"n":4,"t":1},"speaker":"B","text":"今が旬の食材を、母に聞いてみましょう。"},{"id":{"d":78,"n":5,"t":1},"speaker":"A","text":"お願いします。"}];
INSERT INTO sentence [{"id":{"d":79,"n":1,"t":1},"speaker":"A","text":"シチューに合うのは、ご飯とパンのどちらだと思いますか？"},{"id":{"d":79,"n":2,"t":1},"speaker":"B","text":"私は、パンの方が合うと思います。シチューにパンを浸けて食べることが好きです。"},{"id":{"d":79,"n":3,"t":1},"speaker":"A","text":"シチューにご飯もおいしいです。ドリアのようにも見えますが。"},{"id":{"d":79,"n":4,"t":1},"speaker":"B","text":"そうですね。私はご飯にかけるなら、ハヤシライスが好きです。"},{"id":{"d":79,"n":5,"t":1},"speaker":"A","text":"ハヤシライスもいいですね。デミグラスソースがご飯によく合います。"},{"id":{"d":79,"n":6,"t":1},"speaker":"B","text":"はい、そう思います。"}];
INSERT INTO sentence [{"id":{"d":80,"n":1,"t":1},"speaker":"A","text":"最近何か運動をしていますか？"},{"id":{"d":80,"n":2,"t":1},"speaker":"B","text":"私は、登下校時にジョギングをしています。"},{"id":{"d":80,"n":3,"t":1},"speaker":"A","text":"学校まで遠いから、良い運動になりますね。"},{"id":{"d":80,"n":4,"t":1},"speaker":"B","text":"まさにそうなのです。あなたは、何か運動をしていますか？"},{"id":{"d":80,"n":5,"t":1},"speaker":"A","text":"毎朝、私は腹筋のトレーニングをしています。"},{"id":{"d":80,"n":6,"t":1},"speaker":"B","text":"毎朝とは、すごいですね。"}];
INSERT INTO sentence [{"id":{"d":81,"n":1,"t":1},"speaker":"A","text":"慰安旅行で宿泊するホテルの予約は取れましたか？"},{"id":{"d":81,"n":2,"t":1},"speaker":"B","text":"はい、取れました。希望通りの日程になりました。"},{"id":{"d":81,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。社員のみんなにも日程を伝えてくださいね。"},{"id":{"d":81,"n":4,"t":1},"speaker":"B","text":"はい、分かりました。"}];
INSERT INTO sentence [{"id":{"d":82,"n":1,"t":1},"speaker":"A","text":"私の職場の知り合いから登山に誘われたのですが、一緒に行きませんか？"},{"id":{"d":82,"n":2,"t":1},"speaker":"B","text":"初対面で初心者の私が行って大丈夫ですか？"},{"id":{"d":82,"n":3,"t":1},"speaker":"A","text":"はい。初心者向けの山である上に、誰か誘っても良いと言われていますから大丈夫です。"},{"id":{"d":82,"n":4,"t":1},"speaker":"B","text":"では、安心ですね。登山には何を準備すれば良いですか？"},{"id":{"d":82,"n":5,"t":1},"speaker":"A","text":"取りあえず、飲み物があれば安心です。それから、もしもの時の為にレインコートとタオルも多めにあった方がいいですよ。"},{"id":{"d":82,"n":6,"t":1},"speaker":"B","text":"さすが慣れていますね。ご飯はお弁当ですか？"},{"id":{"d":82,"n":7,"t":1},"speaker":"A","text":"クッカーという簡単な料理を温められる調理器具を持って行くので、パスタとかレトルトご飯など食べたいものを持ってきてください。"},{"id":{"d":82,"n":8,"t":1},"speaker":"B","text":"登山でパスタを食べられるとは思いませんでした。とても楽しみです。お誘いいただきありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":83,"n":1,"t":1},"speaker":"A","text":"今年も終わりですね。田中さんは、大みそかは何をして過ごしますか？"},{"id":{"d":83,"n":2,"t":1},"speaker":"B","text":"私は、今年の大みそかは忙しいです。実家で年始に向けて門松や餅つきの準備をしなければなりません。"},{"id":{"d":83,"n":3,"t":1},"speaker":"A","text":"そうなのですね。私は、近所のお寺に除夜の鐘を聞きに行く予定です。"},{"id":{"d":83,"n":4,"t":1},"speaker":"B","text":"良いですね。いつか、私も生で除夜の鐘を打ってみたいものです。"}];
INSERT INTO sentence [{"id":{"d":84,"n":1,"t":1},"speaker":"A","text":"あの話題のギャグマンガの本、明日発売ですね。"},{"id":{"d":84,"n":2,"t":1},"speaker":"B","text":"ギャグマンガなのに、SF的要素やミステリーな内容があってとても面白いです。"},{"id":{"d":84,"n":3,"t":1},"speaker":"A","text":"即日完売になるくらいのマンガですから、明日、急いで書店に行かないといけません。"},{"id":{"d":84,"n":4,"t":1},"speaker":"B","text":"そうですね。明日、大阪駅前の書店に、開店前に集合です。"}];
INSERT INTO sentence [{"id":{"d":85,"n":1,"t":1},"speaker":"A","text":"次の動画は、楽器の演奏に合わせて、ダンスを踊る動画を撮ります。"},{"id":{"d":85,"n":2,"t":1},"speaker":"B","text":"ダンスの動画を撮るなら、練習をしないといけませんね。"},{"id":{"d":85,"n":3,"t":1},"speaker":"A","text":"はい。ですがダンスの練習をしすぎて勉強することを忘れてしまってはいけませんよ。"},{"id":{"d":85,"n":4,"t":1},"speaker":"B","text":"気を付けます。何事にも、メリハリは大事ですからね。"}];
INSERT INTO sentence [{"id":{"d":86,"n":1,"t":1},"speaker":"A","text":"9月の第3月曜日は、敬老の日です。あなたは、何をする予定ですか？"},{"id":{"d":86,"n":2,"t":1},"speaker":"B","text":"私は実家が離れているので、週末から帰省して両親と近くのレストランで食事をしようと思っています。先輩はどうするのですか？"},{"id":{"d":86,"n":3,"t":1},"speaker":"A","text":"私は、息子がおじいちゃんにマッサージをしてあげると、張り切っています。"},{"id":{"d":86,"n":4,"t":1},"speaker":"B","text":"それは良いですね。私の子にも「おばあちゃんにマッサージをしてあげない？」と聞いてみます。"},{"id":{"d":86,"n":5,"t":1},"speaker":"A","text":"良いですね。あなたのお母さまも、喜ぶかと思いますよ。"}];
INSERT INTO sentence [{"id":{"d":87,"n":1,"t":1},"speaker":"A","text":"私は、バイクの免許を取りたいと思います。"},{"id":{"d":87,"n":2,"t":1},"speaker":"B","text":"田中さんは、運転免許を持っていませんでしたか？"},{"id":{"d":87,"n":3,"t":1},"speaker":"A","text":"車の運転免許は持っていますが、二輪車の運転免許は持っていません。"},{"id":{"d":87,"n":4,"t":1},"speaker":"B","text":"そうでしたか。バイクは風を切って走るから、とても気持ちが良いですよ。"},{"id":{"d":87,"n":5,"t":1},"speaker":"A","text":"そうなのですね。早く免許を取ってバイクでツーリングに行きたいです。"}];
INSERT INTO sentence [{"id":{"d":88,"n":1,"t":1},"speaker":"A","text":"明日の運動会ですが、天気がとても心配です。"},{"id":{"d":88,"n":2,"t":1},"speaker":"B","text":"雲行きが怪しいですね。天気予報を見てみましょう。"},{"id":{"d":88,"n":3,"t":1},"speaker":"A","text":"テレビの天気予報番組では、「明日の空模様は、曇りのち所によって雨」と言っています。"},{"id":{"d":88,"n":4,"t":1},"speaker":"B","text":"雨が降らないように、てるてる坊主にお願いするといいですね。"},{"id":{"d":88,"n":5,"t":1},"speaker":"A","text":"ずっと楽しみにしていた運動会ですし、雨が降らないといいですね。"}];
INSERT INTO sentence [{"id":{"d":89,"n":1,"t":1},"speaker":"A","text":"秋は気候が良いので遠出したくなりますね。"},{"id":{"d":89,"n":2,"t":1},"speaker":"B","text":"そうですね。サイクリングなど良さそうです。"},{"id":{"d":89,"n":3,"t":1},"speaker":"A","text":"私は山登りがしたい気分ですね。"},{"id":{"d":89,"n":4,"t":1},"speaker":"B","text":"では、景色の良い山を調べて登りましょう。"},{"id":{"d":89,"n":5,"t":1},"speaker":"A","text":"筑波山は登りやすくて景色も良いですよ。ぜひ一緒に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":90,"n":1,"t":1},"speaker":"A","text":"今度、みんなでぶどう狩りに行きましょうよ。"},{"id":{"d":90,"n":2,"t":1},"speaker":"B","text":"スケジュールを確認しますね。ツアーですか？"},{"id":{"d":90,"n":3,"t":1},"speaker":"A","text":"はい、観光バスで行くツアーです。"},{"id":{"d":90,"n":4,"t":1},"speaker":"B","text":"観光バスで行くツアーは初めてです。私のスケジュールは空いていました。ぶどう狩りへ出かけることを、楽しみにしていますね。"}];
INSERT INTO sentence [{"id":{"d":91,"n":1,"t":1},"speaker":"A","text":"高校卒業後の進路で、目指したい進学先と職種は大体決まっていますか？"},{"id":{"d":91,"n":2,"t":1},"speaker":"B","text":"私はテクノロジー系の専門学校に進学したいです。もっと勉強を頑張りたいですし、将来はロケットを開発するチームの一員になりたいです。"},{"id":{"d":91,"n":3,"t":1},"speaker":"A","text":"そうなのですね。そういえばこの専門学校からオープンキャンパスのお知らせがありましたが、興味はありますか？"},{"id":{"d":91,"n":4,"t":1},"speaker":"B","text":"ちょうど、その学校に興味があったのです。内容や応募期間を教えてください。"},{"id":{"d":91,"n":5,"t":1},"speaker":"A","text":"分かりました。では、このお知らせを転送しますね。"},{"id":{"d":91,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":92,"n":1,"t":1},"speaker":"A","text":"富竹さんは、釣りが好きですか？"},{"id":{"d":92,"n":2,"t":1},"speaker":"B","text":"好きですよ。この前も、夜釣りに行きました。"},{"id":{"d":92,"n":3,"t":1},"speaker":"A","text":"そうなのですか。私は今度初めて釣り船に乗って船釣をします。"},{"id":{"d":92,"n":4,"t":1},"speaker":"B","text":"ぜひ、楽しんできてください。船酔いには気を付けてくださいね。"},{"id":{"d":92,"n":5,"t":1},"speaker":"A","text":"はい、酔い止め薬をかばんに入れて行きます。"}];
INSERT INTO sentence [{"id":{"d":93,"n":1,"t":1},"speaker":"A","text":"昨日、露天風呂で夜空を眺めて、天の川を探したのですが見つけられませんでした。"},{"id":{"d":93,"n":2,"t":1},"speaker":"B","text":"それは、今が冬だからですね。"},{"id":{"d":93,"n":3,"t":1},"speaker":"A","text":"そうなのですか。冬は見られないのですか？"},{"id":{"d":93,"n":4,"t":1},"speaker":"B","text":"見られなくはありませんが、分かりにくいです。"},{"id":{"d":93,"n":5,"t":1},"speaker":"A","text":"知りませんでした。"},{"id":{"d":93,"n":6,"t":1},"speaker":"B","text":"天の川を見るなら夏がお勧めです。"}];
INSERT INTO sentence [{"id":{"d":94,"n":1,"t":1},"speaker":"A","text":"相手チームの10番、なかなかうまいですね。"},{"id":{"d":94,"n":2,"t":1},"speaker":"B","text":"そうですね。パス、フェイント、ドリブルどれをとってもうまいです。"},{"id":{"d":94,"n":3,"t":1},"speaker":"A","text":"左サイドバックが上がって、10番がパスを出しますよ。"},{"id":{"d":94,"n":4,"t":1},"speaker":"B","text":"サイドバックがサイドをドリブルで駆け上がっていって、コーナー付近からクロスを上げました。"},{"id":{"d":94,"n":5,"t":1},"speaker":"A","text":"10番のヘディングシュートです。"},{"id":{"d":94,"n":6,"t":1},"speaker":"B","text":"あっという間にゴールを取られました。さすがですね。"}];
INSERT INTO sentence [{"id":{"d":95,"n":1,"t":1},"speaker":"A","text":"何を握りましょうか？今日はブリがお薦めです。"},{"id":{"d":95,"n":2,"t":1},"speaker":"B","text":"では、まずブリとトロ、それとイカの握りをお願いします。"},{"id":{"d":95,"n":3,"t":1},"speaker":"A","text":"かしこまりました。"},{"id":{"d":95,"n":4,"t":1},"speaker":"B","text":"あと汁物もお願いします。このお店のネタは、いつも新鮮でおいしいですね。"},{"id":{"d":95,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":96,"n":1,"t":1},"speaker":"A","text":"今度のデートは、水族館へ行きませんか？"},{"id":{"d":96,"n":2,"t":1},"speaker":"B","text":"良いですね。私はクラゲが好きです。"},{"id":{"d":96,"n":3,"t":1},"speaker":"A","text":"クラゲが泳いでいる姿は、フワフワしていて幻想的ですよね。"},{"id":{"d":96,"n":4,"t":1},"speaker":"B","text":"そうなのです。暗い水槽の中で、光に当たって泳いでいる姿は、まさに幻想的で癒やされます。"}];
INSERT INTO sentence [{"id":{"d":97,"n":1,"t":1},"speaker":"A","text":"昼休みは、体育館で何をしますか？"},{"id":{"d":97,"n":2,"t":1},"speaker":"B","text":"バトミントンはどうですか？"},{"id":{"d":97,"n":3,"t":1},"speaker":"A","text":"いいですね。今度の大会に向けてトレーニングをしておきたいですし。"},{"id":{"d":97,"n":4,"t":1},"speaker":"B","text":"そうですね。私も、頑張らないといけません。"}];
INSERT INTO sentence [{"id":{"d":98,"n":1,"t":1},"speaker":"A","text":"この写真、夜景がきれいですね。どこで撮ったのですか？"},{"id":{"d":98,"n":2,"t":1},"speaker":"B","text":"山の奥にある空港です。そちらの写真もきれいですね。どちらで撮ったのですか？"},{"id":{"d":98,"n":3,"t":1},"speaker":"A","text":"これは鉄道用の橋に近い河川敷です。夕焼けに照らされている感じが良いですよね。"},{"id":{"d":98,"n":4,"t":1},"speaker":"B","text":"確かに、これは美しいです。"},{"id":{"d":98,"n":5,"t":1},"speaker":"A","text":"列車が通るタイミングが良くて、カメラがぶれることなく、きれいに撮れました。"}];
INSERT INTO sentence [{"id":{"d":99,"n":1,"t":1},"speaker":"A","text":"日本の国技を知っていますか？"},{"id":{"d":99,"n":2,"t":1},"speaker":"B","text":"もちろん、相撲ですよね？"},{"id":{"d":99,"n":3,"t":1},"speaker":"A","text":"正解です。では、なぜ横綱はモンゴルの人が多いのでしょうか？"},{"id":{"d":99,"n":4,"t":1},"speaker":"B","text":"理由は分かりません。しかし、いろいろな国の人が相撲に興味を持ってくれる事はうれしいですね。"},{"id":{"d":99,"n":5,"t":1},"speaker":"A","text":"本当にそうですね。さらに相撲を応援したくなります。"}];
INSERT INTO sentence [{"id":{"d":100,"n":1,"t":1},"speaker":"A","text":"友美さん、小さな子が着物を着て手に袋を下げて、うれしそうに鳥居の下にいますが、あれは何ですか？"},{"id":{"d":100,"n":2,"t":1},"speaker":"B","text":"あれは、七五三というお祝いです。"},{"id":{"d":100,"n":3,"t":1},"speaker":"A","text":"どのようなお祝いなのですか？"},{"id":{"d":100,"n":4,"t":1},"speaker":"B","text":"子どもの健やかな成長を祝います。日本の伝統的な行事の一つです。"},{"id":{"d":100,"n":5,"t":1},"speaker":"A","text":"成長を祝うとは、どういうことですか？"},{"id":{"d":100,"n":6,"t":1},"speaker":"B","text":"昔は医療の発達も未熟で、幼いうちに亡くなる子も多かったので、3歳と5歳と7歳の区切りに神様に子の成長を感謝して御参りをしていたそうです。"},{"id":{"d":100,"n":7,"t":1},"speaker":"A","text":"そうなのですね。七五三では、他にどのようなことをするのですか？"},{"id":{"d":100,"n":8,"t":1},"speaker":"B","text":"これからも長生きできるようにと、願いを込めて縁起物の千歳あめを食べます。地域によっては、いなりずしを食べる所もあるようです。"}];
INSERT INTO sentence [{"id":{"d":101,"n":1,"t":1},"speaker":"A","text":"海外へ出かけるなら、どこへ行きたいですか？"},{"id":{"d":101,"n":2,"t":1},"speaker":"B","text":"グアムに行きたいです。姉の結婚式で初めてグアムに行ったのですが、気候も良く海もきれいだったので、今度は1週間くらい滞在したいです。"},{"id":{"d":101,"n":3,"t":1},"speaker":"A","text":"いいですね。私も行った事がありますが、最高でした。プライベートビーチで思い切り遊んで、夜はショーを見ながらバーベキューをしました。店員さんもみんなフレンドリーで楽しかった事を覚えています。"},{"id":{"d":101,"n":4,"t":1},"speaker":"B","text":"またグアムに行けるように、少し前から毎月貯金をしています。来年には目標額に達するので、長期休暇を取って旅行する予定です。"},{"id":{"d":101,"n":5,"t":1},"speaker":"A","text":"それは素晴らしいですね。土産話をぜひ聞かせてくださいね。"}];
INSERT INTO sentence [{"id":{"d":102,"n":1,"t":1},"speaker":"A","text":"おすしが大好きで、一人で100貫を食べた事もあります。"},{"id":{"d":102,"n":2,"t":1},"speaker":"B","text":"それはすごいですね。特に好きなネタは何ですか？"},{"id":{"d":102,"n":3,"t":1},"speaker":"A","text":"一番に好きなネタは、マグロです。"},{"id":{"d":102,"n":4,"t":1},"speaker":"B","text":"マグロは私も好きです。最近は、ホタテも好きです。"},{"id":{"d":102,"n":5,"t":1},"speaker":"A","text":"ホタテは、食感と甘味が良いですよね。"},{"id":{"d":102,"n":6,"t":1},"speaker":"B","text":"おすしの話をしていたら、おすしが食べたくなってきましたね。"},{"id":{"d":102,"n":7,"t":1},"speaker":"A","text":"そうですね。少し早いですが、昼食にすし屋へ行きましょう。"}];
INSERT INTO sentence [{"id":{"d":103,"n":1,"t":1},"speaker":"A","text":"パパ、見て。あそこにゾウさんがいます。"},{"id":{"d":103,"n":2,"t":1},"speaker":"B","text":"そうですね。動物園には他にもいろいろな動物がいますよ。向こうには、ライオンがいますね。"},{"id":{"d":103,"n":3,"t":1},"speaker":"A","text":"ライオンは、怖いです。このように近づいて、大丈夫なのですか？"},{"id":{"d":103,"n":4,"t":1},"speaker":"B","text":"柵があるから安全ですし、ここにいる動物は飼育員さんがきちんとしつけをしていますから大丈夫ですよ。"}];
INSERT INTO sentence [{"id":{"d":104,"n":1,"t":1},"speaker":"A","text":"斉藤さん、今回の中間テストの手応えはどうでしたか？"},{"id":{"d":104,"n":2,"t":1},"speaker":"B","text":"英語のテストが難しかったですね。佐藤さんは、どうでした？"},{"id":{"d":104,"n":3,"t":1},"speaker":"A","text":"私は、数学のテストが心配です。"},{"id":{"d":104,"n":4,"t":1},"speaker":"B","text":"今回のテストは、いつもより難しかった気がします。お互い良い点数だといいですね。"}];
INSERT INTO sentence [{"id":{"d":105,"n":1,"t":1},"speaker":"A","text":"年賀状の準備が、昨日やっと終わりました。"},{"id":{"d":105,"n":2,"t":1},"speaker":"B","text":"それは、お疲れさまでした。私もあと少しで、年賀状を書き終わります。"},{"id":{"d":105,"n":3,"t":1},"speaker":"A","text":"年賀状は、12月20日までにポストへ投函しないと、確実に元旦に届かないようなので、気を付けてくださいね。"},{"id":{"d":105,"n":4,"t":1},"speaker":"B","text":"はい、気を付けます。来年は、寅年なので、今年は、寅の絵の入った年賀状を選びました。"},{"id":{"d":105,"n":5,"t":1},"speaker":"A","text":"私は干支よりもお正月らしい絵柄が好きなので、今年は、羽子板の絵柄の入った年賀状を選びました。"},{"id":{"d":105,"n":6,"t":1},"speaker":"B","text":"すてきですね。年賀状が、届くことを楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":106,"n":1,"t":1},"speaker":"A","text":"近所で買い物をするときは、どのお店を利用しますか？"},{"id":{"d":106,"n":2,"t":1},"speaker":"B","text":"よく使うお店は、駅前の東急ストアです。このお店で買い物をした後、帰り道にパン屋さんやお花屋さんに寄ることが多いです。"},{"id":{"d":106,"n":3,"t":1},"speaker":"A","text":"それはすてきです。あえて東急ストアの外にあるパン屋さんやお花屋さんに行くのですね。"},{"id":{"d":106,"n":4,"t":1},"speaker":"B","text":"はい。パンやお花は好みが分かれやすいかと思いますので、自分の好みにより合ったお店に行くようにしています。"},{"id":{"d":106,"n":5,"t":1},"speaker":"A","text":"私も同じです。特にお花は、以前、フラワーアレンジメントを習っていたこともありこだわりが強いです。自然の造形を活かした花束のアレンジをしてくれるお店が好きです。"},{"id":{"d":106,"n":6,"t":1},"speaker":"B","text":"普段はどこのお店を利用しているのですか？"},{"id":{"d":106,"n":7,"t":1},"speaker":"A","text":"少し自宅からは遠いのですが、私は駅前のお花屋さんが好きです。お祝いの花束はいつもここで作ってもらいます。"},{"id":{"d":106,"n":8,"t":1},"speaker":"B","text":"駅前にお花屋さんがあるとは知りませんでした。今度行ってみます。"},{"id":{"d":106,"n":9,"t":1},"speaker":"A","text":"とてもすてきなお店なのでぜひ足を運んでみてください。"}];
INSERT INTO sentence [{"id":{"d":107,"n":1,"t":1},"speaker":"A","text":"あなたの好きな食べ物は、何ですか？"},{"id":{"d":107,"n":2,"t":1},"speaker":"B","text":"私の好きな食べ物は、ラーメンです。昨日もおとといも近所のお店で、しょうゆラーメンを食べました。"},{"id":{"d":107,"n":3,"t":1},"speaker":"A","text":"本当に、ラーメンが大好きなのですね。"},{"id":{"d":107,"n":4,"t":1},"speaker":"B","text":"毎日でも食べたいくらい大好きです。学生時代には日本全国のラーメン食べ歩きをしていたくらいです。"},{"id":{"d":107,"n":5,"t":1},"speaker":"A","text":"すごい行動力ですね。"},{"id":{"d":107,"n":6,"t":1},"speaker":"B","text":"いろいろな場所に行って、好きな食べ物を食べることができて、本当に楽しかったです。"},{"id":{"d":107,"n":7,"t":1},"speaker":"A","text":"日本全国のラーメンの中で、一番印象に残っているラーメンは何ですか？"},{"id":{"d":107,"n":8,"t":1},"speaker":"B","text":"北海道のみそバターラーメンです。雪の降る中で食べたあの味は、ずっと忘れられません。"}];
INSERT INTO sentence [{"id":{"d":108,"n":1,"t":1},"speaker":"A","text":"田島さんは、左利きなのですか？"},{"id":{"d":108,"n":2,"t":1},"speaker":"B","text":"はい、そうです。"},{"id":{"d":108,"n":3,"t":1},"speaker":"A","text":"初めて知りました。左利きの人は手先が器用と聞きますが、田島さんも器用ですか？"},{"id":{"d":108,"n":4,"t":1},"speaker":"B","text":"どちらかといえば、器用だと思います。編み物や細かい作業は得意ですよ。"},{"id":{"d":108,"n":5,"t":1},"speaker":"A","text":"そうなのですね。左利きで困ったことはありますか？"},{"id":{"d":108,"n":6,"t":1},"speaker":"B","text":"世間一般では右利きの人が多いので、世の中には右利き用に作られている物が多く、そういう物はしばしば使いづらさを感じます。例えば、お玉やハサミなどです。"},{"id":{"d":108,"n":7,"t":1},"speaker":"A","text":"そういう困り事があるのですね。"}];
INSERT INTO sentence [{"id":{"d":109,"n":1,"t":1},"speaker":"A","text":"あなたは大きくなったら、どのような仕事をしたいですか？"},{"id":{"d":109,"n":2,"t":1},"speaker":"B","text":"学校の先生になりたいです。"},{"id":{"d":109,"n":3,"t":1},"speaker":"A","text":"すてきですね。小学校の先生ですか？"},{"id":{"d":109,"n":4,"t":1},"speaker":"B","text":"はい、小学校の保健室の先生です。"},{"id":{"d":109,"n":5,"t":1},"speaker":"A","text":"優しい先生になってください。応援しています。"}];
INSERT INTO sentence [{"id":{"d":110,"n":1,"t":1},"speaker":"A","text":"本日は、ホームパーティーにお招きいただきまして、本当にありがとうございます。"},{"id":{"d":110,"n":2,"t":1},"speaker":"B","text":"恐れ入ります。こちらこそお忙しい中お越しいただきまして、本当にありがとうございます。"},{"id":{"d":110,"n":3,"t":1},"speaker":"A","text":"すてきなおうちに、おいしいお料理とお酒と、楽しい時間をどうもありがとうございます。"},{"id":{"d":110,"n":4,"t":1},"speaker":"B","text":"今回は準備をいつも以上に頑張ったので、そのように言っていただけてうれしいです。"},{"id":{"d":110,"n":5,"t":1},"speaker":"A","text":"それは改めまして感謝いたします。いつか私もこのようなパーティーを開けるようになりたいと思いました。"},{"id":{"d":110,"n":6,"t":1},"speaker":"B","text":"そのときはぜひ呼んでくださいね。ホームパーティーは慣れると本当に楽しいですよ。"},{"id":{"d":110,"n":7,"t":1},"speaker":"A","text":"はい、その時はいろいろとご教示ください。"},{"id":{"d":110,"n":8,"t":1},"speaker":"B","text":"もちろんです。いつでもお声掛けください。"}];
INSERT INTO sentence [{"id":{"d":111,"n":1,"t":1},"speaker":"A","text":"黒田さんは、先の休日は何をされていましたか？"},{"id":{"d":111,"n":2,"t":1},"speaker":"B","text":"休日は、家族とお台場へお買い物に行きました。"},{"id":{"d":111,"n":3,"t":1},"speaker":"A","text":"それは楽しそうですね。ご主人とお子さんと行かれたのですか？"},{"id":{"d":111,"n":4,"t":1},"speaker":"B","text":"はい、そうです。久しぶりの家族水入らずで、楽しかったです。あなたは何をされていましたか？"},{"id":{"d":111,"n":5,"t":1},"speaker":"A","text":"私は、友達と表参道に遊びに行きました。"},{"id":{"d":111,"n":6,"t":1},"speaker":"B","text":"それはまた楽しそうです。何をされたのですか？"},{"id":{"d":111,"n":7,"t":1},"speaker":"A","text":"大通り沿いのオープンエアカフェで飲食を楽しみながら、おしゃべりをしました。"}];
INSERT INTO sentence [{"id":{"d":112,"n":1,"t":1},"speaker":"A","text":"若林さんの趣味は、何ですか？"},{"id":{"d":112,"n":2,"t":1},"speaker":"B","text":"私の趣味は、テレビを見ることです。"},{"id":{"d":112,"n":3,"t":1},"speaker":"A","text":"それは奇遇です。私の趣味も、テレビを見ることです。"},{"id":{"d":112,"n":4,"t":1},"speaker":"B","text":"そうですか。好きなテレビ番組のジャンルは何ですか？私が好きなのはバラエティ番組です。"},{"id":{"d":112,"n":5,"t":1},"speaker":"A","text":"良いですね。私もバラエティ番組が好きです。あと、音楽番組も好きです。"},{"id":{"d":112,"n":6,"t":1},"speaker":"B","text":"音楽番組ですか。例えば、どのような番組を見られますか？"},{"id":{"d":112,"n":7,"t":1},"speaker":"A","text":"よく「ららら♪クラシック」を見ます。"}];
INSERT INTO sentence [{"id":{"d":113,"n":1,"t":1},"speaker":"A","text":"あなたは、どのような服装が好きですか？"},{"id":{"d":113,"n":2,"t":1},"speaker":"B","text":"私は、カジュアルな服が好きです。"},{"id":{"d":113,"n":3,"t":1},"speaker":"A","text":"今日の服もすてきですね。"},{"id":{"d":113,"n":4,"t":1},"speaker":"B","text":"私は、いつも行きつけのお店で買うので、お店の人が私に合う服を選んでくれます。"},{"id":{"d":113,"n":5,"t":1},"speaker":"A","text":"行きつけのお店があると良いですね。"},{"id":{"d":113,"n":6,"t":1},"speaker":"B","text":"ええ、とても助かっています。今度一緒に行ってみますか？"},{"id":{"d":113,"n":7,"t":1},"speaker":"A","text":"はい、ぜひ行きたいです。"}];
INSERT INTO sentence [{"id":{"d":114,"n":1,"t":1},"speaker":"A","text":"先月から、私は料理教室に通い始めました。"},{"id":{"d":114,"n":2,"t":1},"speaker":"B","text":"それは、素晴らしいです。どちらのお料理教室ですか？"},{"id":{"d":114,"n":3,"t":1},"speaker":"A","text":"銀座にある「田中伶子クッキングスクール」です。"},{"id":{"d":114,"n":4,"t":1},"speaker":"B","text":"そのお料理教室の名前を、私も聞いたことがあります。田中伶子先生は、料理の本も出している方ですよね。和洋中全てのお料理を教えてくれるので、上達が早い教室だと聞いたことがあります。"},{"id":{"d":114,"n":5,"t":1},"speaker":"A","text":"はい、そうなのです。先週は、中華料理のマーボー豆腐とサンラータンを教えてもらったのですが、本当においしかったです。こちらの教室に通うようになってから、中華料理が大好きになりました。"},{"id":{"d":114,"n":6,"t":1},"speaker":"B","text":"それは、良いですね。私も今度見学してみたいです。実は、前から料理教室に興味がありました。"},{"id":{"d":114,"n":7,"t":1},"speaker":"A","text":"ぜひ、見学なさってください。体験レッスンにお申し込みされるといいと思います。"},{"id":{"d":114,"n":8,"t":1},"speaker":"B","text":"はい、申し込んでみます。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":115,"n":1,"t":1},"speaker":"A","text":"あなたは、どのようなジャンルの映画が好きですか？"},{"id":{"d":115,"n":2,"t":1},"speaker":"B","text":"私は、アニメ映画が好きです。特に戦隊アニメが好きです。"},{"id":{"d":115,"n":3,"t":1},"speaker":"A","text":"戦隊アニメですか。ちょうど今いろいろ上映されていますよね。私の周りにも見に行ったという方が多いです。私はラブコメディーの方が好きなので戦隊アニメにはあまり詳しくありません。"},{"id":{"d":115,"n":4,"t":1},"speaker":"B","text":"はい、私も見に行きました。そういえば、今週末から話題のラブコメディー映画が公開されますよね。"},{"id":{"d":115,"n":5,"t":1},"speaker":"A","text":"そうなのです、絶対に見に行きます。"}];
INSERT INTO sentence [{"id":{"d":116,"n":1,"t":1},"speaker":"A","text":"先日、夫と銀座にフランス料理を食べに行きましたが、とてもおいしかったです。"},{"id":{"d":116,"n":2,"t":1},"speaker":"B","text":"それは、すてきです。ちなみに、どちらのお店ですか？"},{"id":{"d":116,"n":3,"t":1},"speaker":"A","text":"ありがとうございます。「ドミニク・ブシェ トーキョー」というお店です。"},{"id":{"d":116,"n":4,"t":1},"speaker":"B","text":"おお、今話題のお店ですね。この間、雑誌で特集しているのを見ました。"},{"id":{"d":116,"n":5,"t":1},"speaker":"A","text":"はい。素晴らしいお料理とインテリアが堪能できる趣味の良いお店でした。"},{"id":{"d":116,"n":6,"t":1},"speaker":"B","text":"それは、良いことを聞きました。私も今度、夫と行ってみようと思います。"},{"id":{"d":116,"n":7,"t":1},"speaker":"A","text":"はい。ぜひ、今度行ってみてください。ご満足いただけるかと思います。"}];
INSERT INTO sentence [{"id":{"d":117,"n":1,"t":1},"speaker":"A","text":"勉強は、難しいですね。"},{"id":{"d":117,"n":2,"t":1},"speaker":"B","text":"そうですね。ですが、得意な科目だけは勉強がはかどりますよ。"},{"id":{"d":117,"n":3,"t":1},"speaker":"A","text":"あなたは、何の科目が得意ですか？"},{"id":{"d":117,"n":4,"t":1},"speaker":"B","text":"私の得意科目は、歴史です。知らないことを調べるのが楽しいです。"},{"id":{"d":117,"n":5,"t":1},"speaker":"A","text":"では、社会の勉強は得意なのですね。"},{"id":{"d":117,"n":6,"t":1},"speaker":"B","text":"はい、そうです。"}];
INSERT INTO sentence [{"id":{"d":118,"n":1,"t":1},"speaker":"A","text":"うちの畑は、私の代で終わりになるかもしれません。"},{"id":{"d":118,"n":2,"t":1},"speaker":"B","text":"鈴木さんの畑のお野菜は、甘味があってとてもおいしいのに残念です。なぜ終わってしまうのですか？"},{"id":{"d":118,"n":3,"t":1},"speaker":"A","text":"跡を継いでくれる人がいないのです。"},{"id":{"d":118,"n":4,"t":1},"speaker":"B","text":"そうですか。跡継ぎがいないことについては、役場に相談してみましたか？"},{"id":{"d":118,"n":5,"t":1},"speaker":"A","text":"いいえ。相談していません。"},{"id":{"d":118,"n":6,"t":1},"speaker":"B","text":"まずは、役場に相談してみてください。農業をしたい若い世代や海外の人を派遣してくれるようですよ。"},{"id":{"d":118,"n":7,"t":1},"speaker":"A","text":"それは知りませんでした。ありがとうございます。役場に相談してみます。"}];
INSERT INTO sentence [{"id":{"d":119,"n":1,"t":1},"speaker":"A","text":"あなたは夏休みの宿題を、どのように進めていますか？"},{"id":{"d":119,"n":2,"t":1},"speaker":"B","text":"私は、夏休み終了日までに余裕を持って、宿題を終わらせられるよう、1日分ずつの学習計画を立てて毎日こつこつと進めています。"},{"id":{"d":119,"n":3,"t":1},"speaker":"A","text":"計画的にできるのは素晴らしいですね。私は、毎年夏休み最後の日まで宿題が終わらなくて、大変な事になってしまいます。"},{"id":{"d":119,"n":4,"t":1},"speaker":"B","text":"それは大変そうです。先に学習計画を立ててから、その通りに進めると、余裕をもって夏休みの宿題を終わらせる事ができますよ。"},{"id":{"d":119,"n":5,"t":1},"speaker":"A","text":"そうですよね。今年の夏休みはそのようにしてみます。"},{"id":{"d":119,"n":6,"t":1},"speaker":"B","text":"頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":120,"n":1,"t":1},"speaker":"A","text":"先週、私は映画を見に行ってきました。"},{"id":{"d":120,"n":2,"t":1},"speaker":"B","text":"そうなのですね。どのような映画を見たのですか？"},{"id":{"d":120,"n":3,"t":1},"speaker":"A","text":"飛行機の墜落で無人島に漂着した男性が3年間のサバイバルの末、無事に家に帰るというヒューマンドラマでした。勇気と希望をもらいました。"},{"id":{"d":120,"n":4,"t":1},"speaker":"B","text":"とても面白そうな映画ですね。僕も見てみたいです。まだ、上映していますか？"},{"id":{"d":120,"n":5,"t":1},"speaker":"A","text":"上映期間は今月末までなので、まだ間に合いますよ。映画を見たら、ぜひ感想を聞かせてくださいね。"},{"id":{"d":120,"n":6,"t":1},"speaker":"B","text":"はい。あとで感想を語り合いましょう。他にも、お薦めの作品があったら教えてほしいです。"}];
INSERT INTO sentence [{"id":{"d":121,"n":1,"t":1},"speaker":"A","text":"田島さんの好きな授業は、何ですか？"},{"id":{"d":121,"n":2,"t":1},"speaker":"B","text":"私の好きな授業は、家庭科です。調理実習がとても楽しいです。"},{"id":{"d":121,"n":3,"t":1},"speaker":"A","text":"そうなのですね。"},{"id":{"d":121,"n":4,"t":1},"speaker":"B","text":"はい。田中さんは、何の授業が好きですか？"},{"id":{"d":121,"n":5,"t":1},"speaker":"A","text":"私は、体育の授業が好きです。"},{"id":{"d":121,"n":6,"t":1},"speaker":"B","text":"体育は、体を動かせるので良いですよね。私も好きです。"}];
INSERT INTO sentence [{"id":{"d":122,"n":1,"t":1},"speaker":"A","text":"今日中には、片付けが終わりそうにありませんね。"},{"id":{"d":122,"n":2,"t":1},"speaker":"B","text":"後藤さん、これを見てください。"},{"id":{"d":122,"n":3,"t":1},"speaker":"A","text":"これは、アルバムですか？"},{"id":{"d":122,"n":4,"t":1},"speaker":"B","text":"はい。押入れでこのアルバムを見つけました。今から一緒に見ませんか？"},{"id":{"d":122,"n":5,"t":1},"speaker":"A","text":"ぜひ、見たいです。でも、まずは片付けを進めなければいけません。片付けが終わってからにしましょう。"},{"id":{"d":122,"n":6,"t":1},"speaker":"B","text":"そうですね。"}];
INSERT INTO sentence [{"id":{"d":123,"n":1,"t":1},"speaker":"A","text":"私は、高校生になってから、登下校が大変になりました。"},{"id":{"d":123,"n":2,"t":1},"speaker":"B","text":"そうなのですか？"},{"id":{"d":123,"n":3,"t":1},"speaker":"A","text":"はい。バスと電車を乗り継いでいく必要があるので、毎日とても疲れます。"},{"id":{"d":123,"n":4,"t":1},"speaker":"B","text":"それは、確かに大変そうですね。"}];
INSERT INTO sentence [{"id":{"d":124,"n":1,"t":1},"speaker":"A","text":"あなたは、小野先生が今度、転任されるという話を聞きましたか？"},{"id":{"d":124,"n":2,"t":1},"speaker":"B","text":"はい、聞きました。小野先生には、私も小学校の時にとてもお世話になりました。"},{"id":{"d":124,"n":3,"t":1},"speaker":"A","text":"私もお世話になりました。転任のあいさつの日に花束を持って行こうと思っていますが、一緒に行きますか？"},{"id":{"d":124,"n":4,"t":1},"speaker":"B","text":"もちろん行きます。確か小野先生は、ガーベラが好きでしたよね？ガーベラを入れた花束にするのはどうでしょうか？"},{"id":{"d":124,"n":5,"t":1},"speaker":"A","text":"いいですね。先生も喜ばれると思います。"},{"id":{"d":124,"n":6,"t":1},"speaker":"B","text":"では当日、花屋の前で待ち合わせしてから行きましょう。"},{"id":{"d":124,"n":7,"t":1},"speaker":"A","text":"分かりました。"}];
INSERT INTO sentence [{"id":{"d":125,"n":1,"t":1},"speaker":"A","text":"野菜は、何が好きですか？"},{"id":{"d":125,"n":2,"t":1},"speaker":"B","text":"好きな野菜がたくさんあって、迷います。"},{"id":{"d":125,"n":3,"t":1},"speaker":"A","text":"では、冬の野菜では何が好きですか？"},{"id":{"d":125,"n":4,"t":1},"speaker":"B","text":"私は、カボチャが好きです。"},{"id":{"d":125,"n":5,"t":1},"speaker":"A","text":"カボチャは甘くておいしいですね。"},{"id":{"d":125,"n":6,"t":1},"speaker":"B","text":"はい。煮物やデザートに使うと甘みが増してすごくおいしいです。"},{"id":{"d":125,"n":7,"t":1},"speaker":"A","text":"私も、カボチャのプリンを以前食べましたが、とてもおいしかったです。"},{"id":{"d":125,"n":8,"t":1},"speaker":"B","text":"私も大好きです。"}];
INSERT INTO sentence [{"id":{"d":126,"n":1,"t":1},"speaker":"A","text":"膝にひどい痛みを感じます。"},{"id":{"d":126,"n":2,"t":1},"speaker":"B","text":"どうしたのですか？"},{"id":{"d":126,"n":3,"t":1},"speaker":"A","text":"さっき、手を滑らせて鉄棒から落ちてしまいました。"},{"id":{"d":126,"n":4,"t":1},"speaker":"B","text":"膝から血が出ていますね。保健室で処置してもらいましょう。立てますか？"},{"id":{"d":126,"n":5,"t":1},"speaker":"A","text":"はい、なんとか立てそうです。すみません。"},{"id":{"d":126,"n":6,"t":1},"speaker":"B","text":"困っていたら助け合うのは当然です。気にしないでください。"},{"id":{"d":126,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":127,"n":1,"t":1},"speaker":"A","text":"最近、寒いですね。"},{"id":{"d":127,"n":2,"t":1},"speaker":"B","text":"そうですね。もう12月ですしね。"},{"id":{"d":127,"n":3,"t":1},"speaker":"A","text":"でも、寒いと良いこともありますよね。"},{"id":{"d":127,"n":4,"t":1},"speaker":"B","text":"例えば、どのようなことですか？"},{"id":{"d":127,"n":5,"t":1},"speaker":"A","text":"虫が少なくなります。"},{"id":{"d":127,"n":6,"t":1},"speaker":"B","text":"確かにそうですね。私も虫が苦手なのでそれはうれしいことです。"}];
INSERT INTO sentence [{"id":{"d":128,"n":1,"t":1},"speaker":"A","text":"あなたは、昨日のF1レースを見ましたか？"},{"id":{"d":128,"n":2,"t":1},"speaker":"B","text":"見ました。大きなレースで有名なレーサーたちも、たくさん出場していましたね。"},{"id":{"d":128,"n":3,"t":1},"speaker":"A","text":"そうですね。とても盛り上がった、良い試合でしたね。"},{"id":{"d":128,"n":4,"t":1},"speaker":"B","text":"はい。激しいバトルでしたが、最後のゴールシーンには感動しました。"}];
INSERT INTO sentence [{"id":{"d":129,"n":1,"t":1},"speaker":"A","text":"あなたは、何色が好きですか？"},{"id":{"d":129,"n":2,"t":1},"speaker":"B","text":"私の好きな色は青ですが、洋服なら黒か茶をよく着ます。"},{"id":{"d":129,"n":3,"t":1},"speaker":"A","text":"そうなのですね。私も青が好きです。"},{"id":{"d":129,"n":4,"t":1},"speaker":"B","text":"私たちは、気が合いますね。"},{"id":{"d":129,"n":5,"t":1},"speaker":"A","text":"本当ですね。うれしいです。"}];
INSERT INTO sentence [{"id":{"d":130,"n":1,"t":1},"speaker":"A","text":"最近、私は久しぶりにジョギングをしました。"},{"id":{"d":130,"n":2,"t":1},"speaker":"B","text":"健康的で良いですね。どこからどこまでジョギングをしたのですか？"},{"id":{"d":130,"n":3,"t":1},"speaker":"A","text":"自宅から小学校までです。"},{"id":{"d":130,"n":4,"t":1},"speaker":"B","text":"それは、結構な距離がありますね。"},{"id":{"d":130,"n":5,"t":1},"speaker":"A","text":"はい。久しぶりだったのもあり、さすがに疲れました。"}];
INSERT INTO sentence [{"id":{"d":131,"n":1,"t":1},"speaker":"A","text":"本屋の横にある水路に、渡り鳥の親子がいるのを見ました。"},{"id":{"d":131,"n":2,"t":1},"speaker":"B","text":"渡り鳥が、水路にいたのですか？"},{"id":{"d":131,"n":3,"t":1},"speaker":"A","text":"そうです。きれいな中洲ですからね。"},{"id":{"d":131,"n":4,"t":1},"speaker":"B","text":"来年はもっとたくさんの渡り鳥が来てくれると良いですね。"}];
INSERT INTO sentence [{"id":{"d":132,"n":1,"t":1},"speaker":"A","text":"高橋さん、一緒にジェットコースターに乗りませんか？"},{"id":{"d":132,"n":2,"t":1},"speaker":"B","text":"僕は、ジェットコースターが苦手です。"},{"id":{"d":132,"n":3,"t":1},"speaker":"A","text":"どうして苦手なのですか？"},{"id":{"d":132,"n":4,"t":1},"speaker":"B","text":"高いところから、ゆっくり上った後の急降下をする瞬間が一番怖いからです。"},{"id":{"d":132,"n":5,"t":1},"speaker":"A","text":"確かに、その瞬間はドキドキしますね。"},{"id":{"d":132,"n":6,"t":1},"speaker":"B","text":"清水さんは、怖くないのですか？"},{"id":{"d":132,"n":7,"t":1},"speaker":"A","text":"もちろん怖いです。しかし、ジェットコースターが上っている時の緊迫感と、急降下した時の爽快感が、私は好きです。"},{"id":{"d":132,"n":8,"t":1},"speaker":"B","text":"それはすごい。僕だったら、怖すぎて目も開けられませんよ。"}];
INSERT INTO sentence [{"id":{"d":133,"n":1,"t":1},"speaker":"A","text":"ロールケーキを買ってきたので、一緒に食べましょう。"},{"id":{"d":133,"n":2,"t":1},"speaker":"B","text":"ありがとうございます。では、私はお茶を入れてきます。"},{"id":{"d":133,"n":3,"t":1},"speaker":"A","text":"では、いつもの緑茶でお願いします。"},{"id":{"d":133,"n":4,"t":1},"speaker":"B","text":"お母さん、最近いつも緑茶を飲んでいますね。"},{"id":{"d":133,"n":5,"t":1},"speaker":"A","text":"茶殻で緑茶のパウンドケーキを作ろうと思っていて、そのために、たくさんの緑茶を飲んでいるのですよ。"},{"id":{"d":133,"n":6,"t":1},"speaker":"B","text":"茶殻でパウンドケーキを作るのですか？私も作りたいのでレシピを教えてください。"},{"id":{"d":133,"n":7,"t":1},"speaker":"A","text":"もちろんです。一緒に作りましょう。作るときは声を掛けますね。"},{"id":{"d":133,"n":8,"t":1},"speaker":"B","text":"はい。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":134,"n":1,"t":1},"speaker":"A","text":"冬なのに、あなたは随分と薄着ですね。寒くないのですか？"},{"id":{"d":134,"n":2,"t":1},"speaker":"B","text":"私は、トレーニングで走って来たので暑いくらいです。"},{"id":{"d":134,"n":3,"t":1},"speaker":"A","text":"最近頑張っていますが、何か大会に出るのですか？"},{"id":{"d":134,"n":4,"t":1},"speaker":"B","text":"いえ、健康のためです。健康診断で悪い数値が出てしまい、このままでは、入院することになると医者から言われました。"},{"id":{"d":134,"n":5,"t":1},"speaker":"A","text":"それは大変ですね。僕も良い数値ではなかったので、他人事ではありません。"},{"id":{"d":134,"n":6,"t":1},"speaker":"B","text":"健康診断の検査結果の数値を基準値にするためには、無理なく続けられる運動をすることが良いらしいですよ。"},{"id":{"d":134,"n":7,"t":1},"speaker":"A","text":"分かりました。僕も軽い運動から始めてみます。"}];
INSERT INTO sentence [{"id":{"d":135,"n":1,"t":1},"speaker":"A","text":"私は、勉強を続けると疲れます。"},{"id":{"d":135,"n":2,"t":1},"speaker":"B","text":"そのような時は、リフレッシュが必要です。"},{"id":{"d":135,"n":3,"t":1},"speaker":"A","text":"例えば、どのようなことをしますか？"},{"id":{"d":135,"n":4,"t":1},"speaker":"B","text":"私は、腕を上げて背伸びをしたり大きく息を吸って深呼吸をしたりします。"},{"id":{"d":135,"n":5,"t":1},"speaker":"A","text":"簡単にできて良いですね。早速、試してみます。"}];
INSERT INTO sentence [{"id":{"d":136,"n":1,"t":1},"speaker":"A","text":"座間君、今度のオープンキャンパスはどのようにして行きますか？"},{"id":{"d":136,"n":2,"t":1},"speaker":"B","text":"僕は、電車で大阪駅まで行き、そこから歩きます。山賀さんはどうしますか？"},{"id":{"d":136,"n":3,"t":1},"speaker":"A","text":"私は、バスで行こうと思っています。大阪駅からだと、山を登る事になりますが、大丈夫ですか？"},{"id":{"d":136,"n":4,"t":1},"speaker":"B","text":"ええ、それを試してみるつもりです。"},{"id":{"d":136,"n":5,"t":1},"speaker":"A","text":"なるほど。では、キャンパスに着いたら連絡をしてください。一緒に教室まで行きましょう。"}];
INSERT INTO sentence [{"id":{"d":137,"n":1,"t":1},"speaker":"A","text":"あなたは、何か好きな食べ物はありますか？"},{"id":{"d":137,"n":2,"t":1},"speaker":"B","text":"そうですね、私は炭水化物が好きです。"},{"id":{"d":137,"n":3,"t":1},"speaker":"A","text":"炭水化物ですか？白米が好きなのですか？"},{"id":{"d":137,"n":4,"t":1},"speaker":"B","text":"白米も好きですが、特に、麵類が好きです。うどんやラーメンが好きです。"},{"id":{"d":137,"n":5,"t":1},"speaker":"A","text":"麵類は、僕も好きです。パスタも良いですね。"},{"id":{"d":137,"n":6,"t":1},"speaker":"B","text":"はい、パスタもおいしいですね。"}];
INSERT INTO sentence [{"id":{"d":138,"n":1,"t":1},"speaker":"A","text":"12月に入ったら、急に寒くなりました。"},{"id":{"d":138,"n":2,"t":1},"speaker":"B","text":"一日中寒いですが、朝起きた時が一番寒いです。"},{"id":{"d":138,"n":3,"t":1},"speaker":"A","text":"そうですね。私は、寒くて起きるのがおっくうになります。"},{"id":{"d":138,"n":4,"t":1},"speaker":"B","text":"私もそうです。毎日一生懸命、自分を奮い立たせて起きています。"},{"id":{"d":138,"n":5,"t":1},"speaker":"A","text":"偉いですね。春が来るまで、頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":139,"n":1,"t":1},"speaker":"A","text":"なぜか、私は最近よく眠れない気がしています。"},{"id":{"d":139,"n":2,"t":1},"speaker":"B","text":"なぜでしょうか？昼寝はしていますか？"},{"id":{"d":139,"n":3,"t":1},"speaker":"A","text":"はい。結構、長時間の昼寝をしています。"},{"id":{"d":139,"n":4,"t":1},"speaker":"B","text":"あまり昼寝の時間が長いと、夜は眠れなくなってしまうそうですよ。"},{"id":{"d":139,"n":5,"t":1},"speaker":"A","text":"そうでしたか。早速、今日から昼寝を控えてみようと思います。"},{"id":{"d":139,"n":6,"t":1},"speaker":"B","text":"夜によく眠るためには、ベッドに入る時間を決めると良いとも聞いたことがあります。"},{"id":{"d":139,"n":7,"t":1},"speaker":"A","text":"そうなのですね。では私は、22時を目標にベッドに入るようにしようと思います。"}];
INSERT INTO sentence [{"id":{"d":140,"n":1,"t":1},"speaker":"A","text":"すみません。道を尋ねたいのですが、よろしいでしょうか？"},{"id":{"d":140,"n":2,"t":1},"speaker":"B","text":"はい。僕で分かる道だと良いのですが。目的地はどこですか？"},{"id":{"d":140,"n":3,"t":1},"speaker":"A","text":"最寄りの駅を教えてください。"},{"id":{"d":140,"n":4,"t":1},"speaker":"B","text":"最寄り駅は神戸駅です。この道を真っ直ぐに行って、1つ目のコンビニエンスストアを右に曲がったら駅が見えますよ。"},{"id":{"d":140,"n":5,"t":1},"speaker":"A","text":"ありがとうございます。助かりました。スマートフォンの充電が切れていたので、道が分からず、困っていました。"},{"id":{"d":140,"n":6,"t":1},"speaker":"B","text":"そうだったのですね。お役に立てて幸いです。"}];
INSERT INTO sentence [{"id":{"d":141,"n":1,"t":1},"speaker":"A","text":"校長先生、子犬が校庭に捨てられていたので連れてきました。"},{"id":{"d":141,"n":2,"t":1},"speaker":"B","text":"2匹いますね。校庭のどこにいたのですか？"},{"id":{"d":141,"n":3,"t":1},"speaker":"A","text":"すべり台の下です。"},{"id":{"d":141,"n":4,"t":1},"speaker":"B","text":"それは、かわいそうに。"},{"id":{"d":141,"n":5,"t":1},"speaker":"A","text":"どのようにしたら良いでしょうか。僕の家では、犬を飼うことができませんが、学校で飼うことはできませんか？"},{"id":{"d":141,"n":6,"t":1},"speaker":"B","text":"分かりました。では、一時的に子犬たちを学校で預かりましょう。"},{"id":{"d":141,"n":7,"t":1},"speaker":"A","text":"ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":142,"n":1,"t":1},"speaker":"A","text":"木村さんは、英語の成績がすごく良いですが、どのように勉強しているのですか？"},{"id":{"d":142,"n":2,"t":1},"speaker":"B","text":"私は、授業の予習、復習とは別に英語の小説を読んでいます。"},{"id":{"d":142,"n":3,"t":1},"speaker":"A","text":"それはすごいです。木村さんは、英語の小説をすらすらと読めるのですか？"},{"id":{"d":142,"n":4,"t":1},"speaker":"B","text":"私は辞書を引きながら英語の小説を読んでいますが、教科書にはない表現などの勉強になりますよ。"},{"id":{"d":142,"n":5,"t":1},"speaker":"A","text":"なるほど、確かに良い勉強になりそうですね。"}];
INSERT INTO sentence [{"id":{"d":143,"n":1,"t":1},"speaker":"A","text":"もうすぐ、クリスマスですね。"},{"id":{"d":143,"n":2,"t":1},"speaker":"B","text":"そうですね。私は、ケーキももちろんですが、何よりもプレゼントが楽しみです。"},{"id":{"d":143,"n":3,"t":1},"speaker":"A","text":"プレゼントは、何が欲しいのですか？"},{"id":{"d":143,"n":4,"t":1},"speaker":"B","text":"今年は、ネックレスが欲しいです。"},{"id":{"d":143,"n":5,"t":1},"speaker":"A","text":"サンタクロースがネックレスを届けてくれるといいですね。"},{"id":{"d":143,"n":6,"t":1},"speaker":"B","text":"はい。良い子にしていたので、きっとサンタクロースが来てくれると思います。"}];
INSERT INTO sentence [{"id":{"d":144,"n":1,"t":1},"speaker":"A","text":"最近、私はお料理に凝っています。"},{"id":{"d":144,"n":2,"t":1},"speaker":"B","text":"おお、それは素晴らしいです。具体的には、どのようなお料理を作られるのですか？"},{"id":{"d":144,"n":3,"t":1},"speaker":"A","text":"そうですね、私は洋食をよく作ります。この間の週末には、オムライスとミネストローネを作りました。"},{"id":{"d":144,"n":4,"t":1},"speaker":"B","text":"本格的ですね。もしよければ、今度のホームパーティーに何か差し入れをしていただけませんか？"},{"id":{"d":144,"n":5,"t":1},"speaker":"A","text":"もちろんです。何かご希望はありますか？"},{"id":{"d":144,"n":6,"t":1},"speaker":"B","text":"ありがとうございます。サラダとメインのお料理をお願いできるとうれしいです。"},{"id":{"d":144,"n":7,"t":1},"speaker":"A","text":"分かりました。私は、ニース風サラダとコロッケを作って持ってホームパーティーへ行きますね。"},{"id":{"d":144,"n":8,"t":1},"speaker":"B","text":"よろしくお願いいたします。楽しみにしています。"}];
INSERT INTO sentence [{"id":{"d":145,"n":1,"t":1},"speaker":"A","text":"冬になると温泉に入りたくなりませんか？"},{"id":{"d":145,"n":2,"t":1},"speaker":"B","text":"入りたくなります。今年の冬は、温泉地へ遠出をしても良いかもしれませんね。"},{"id":{"d":145,"n":3,"t":1},"speaker":"A","text":"いいですね。箱根は、いかがですか？"},{"id":{"d":145,"n":4,"t":1},"speaker":"B","text":"箱根も良いですが、この際、九州まで行くのはどうでしょう？"},{"id":{"d":145,"n":5,"t":1},"speaker":"A","text":"楽しそうです。では、霧島か別府の温泉はいかがでしょうか？"},{"id":{"d":145,"n":6,"t":1},"speaker":"B","text":"私は別府が良いですね、湯布院に行きましょう。"}];
INSERT INTO sentence [{"id":{"d":146,"n":1,"t":1},"speaker":"A","text":"最近、あなたがよく食べているものは何ですか？"},{"id":{"d":146,"n":2,"t":1},"speaker":"B","text":"私は、昼食にカレーライスを食べることが多いです。木村さんは、何をよく食べますか？"},{"id":{"d":146,"n":3,"t":1},"speaker":"A","text":"私は、唐揚げとサラダです。唐揚げは昔からの好物で、最近は健康のためにサラダも食べるようにしています。"},{"id":{"d":146,"n":4,"t":1},"speaker":"B","text":"健康は大事ですね。私もあなたを見習って、カレーライスにサラダをつけるようにします。"}];
INSERT INTO sentence [{"id":{"d":147,"n":1,"t":1},"speaker":"A","text":"先輩の壮行会は、どこで開催しますか？"},{"id":{"d":147,"n":2,"t":1},"speaker":"B","text":"今回の壮行会は、参加人数が多いので、近所の焼き肉屋さんにしようかと思っています。"},{"id":{"d":147,"n":3,"t":1},"speaker":"A","text":"去年は隣町のおすし屋さんでしたか？"},{"id":{"d":147,"n":4,"t":1},"speaker":"B","text":"はい。あのおすし屋さんはお店が少し小さいので、今回の人数が入ると窮屈かもしれません。"},{"id":{"d":147,"n":5,"t":1},"speaker":"A","text":"そうですね。焼き肉屋さんが、壮行会の会場として良さそうです。"}];
INSERT INTO sentence [{"id":{"d":148,"n":1,"t":1},"speaker":"A","text":"最近、あなたは、よくジーンズをはいていらっしゃいますね。"},{"id":{"d":148,"n":2,"t":1},"speaker":"B","text":"はい。私はジーンズがすごく好きなのです。"},{"id":{"d":148,"n":3,"t":1},"speaker":"A","text":"良いですね。とてもお似合いです。"},{"id":{"d":148,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。ひとえにジーンズといっても多くのバリエーションがあり、その中から好みの物を選ぶことがとても楽しいのです。"},{"id":{"d":148,"n":5,"t":1},"speaker":"A","text":"知っています。いつもいろいろな種類のジーンズをはいていらっしゃると思っていました。"},{"id":{"d":148,"n":6,"t":1},"speaker":"B","text":"お気付きだったのですね。そうなのです。ジーンズには色も丈もさまざまなものがあるのです。"},{"id":{"d":148,"n":7,"t":1},"speaker":"A","text":"良いですね。今度、ぜひ詳しく教えてください。"},{"id":{"d":148,"n":8,"t":1},"speaker":"B","text":"もちろんです。いつでもお声掛けください。"}];
INSERT INTO sentence [{"id":{"d":149,"n":1,"t":1},"speaker":"A","text":"この前、私は県立美術館に行きました。"},{"id":{"d":149,"n":2,"t":1},"speaker":"B","text":"どのような作品を見ましたか？"},{"id":{"d":149,"n":3,"t":1},"speaker":"A","text":"絵画では抽象画、さらには彫刻など、たくさんの作品を見ました。"},{"id":{"d":149,"n":4,"t":1},"speaker":"B","text":"やはり市立美術館よりもたくさんの作品がありましたか？"},{"id":{"d":149,"n":5,"t":1},"speaker":"A","text":"はい。市立美術館とは比べものにならないほど、県立美術館にはたくさんの作品が展示されていました。"}];
INSERT INTO sentence [{"id":{"d":150,"n":1,"t":1},"speaker":"A","text":"高橋先輩、「ロボットアイデア甲子園」に出す機体はタイヤ式のものでは駄目ですか？"},{"id":{"d":150,"n":2,"t":1},"speaker":"B","text":"そうですね。タイヤ式は安定感がありますが、全国大会を目指すには斬新さが足りません。"},{"id":{"d":150,"n":3,"t":1},"speaker":"A","text":"なるほど。うちの技術をアピールするためには、この4本足の方が適切なのですね。"},{"id":{"d":150,"n":4,"t":1},"speaker":"B","text":"はい。中村君のアイデアのおかげで、以前よりも信頼性が高くなったのも大きいですよ。"},{"id":{"d":150,"n":5,"t":1},"speaker":"A","text":"光栄です。全国大会に、出られるように頑張ります。"},{"id":{"d":150,"n":6,"t":1},"speaker":"B","text":"はい。ぜひ、一緒に頑張りましょう。"}];
INSERT INTO sentence [{"id":{"d":151,"n":1,"t":1},"speaker":"A","text":"今日の昼食は、何を食べたら良いと思いますか？"},{"id":{"d":151,"n":2,"t":1},"speaker":"B","text":"そうですね、カレーライスはいかがでしょう？この近くにすごくおいしいカレー屋さんがあります。"},{"id":{"d":151,"n":3,"t":1},"speaker":"A","text":"それはぜひ行ってみたいです。カレーが大好きなので。"},{"id":{"d":151,"n":4,"t":1},"speaker":"B","text":"カレーがお好きなら、ご満足いただけること間違いなしかと思います。"},{"id":{"d":151,"n":5,"t":1},"speaker":"A","text":"ちなみに、インドカレーですか？辛さはいかほどですか？"},{"id":{"d":151,"n":6,"t":1},"speaker":"B","text":"はい、インドカレーです。辛さは、スパイスが強めの甘辛です。"},{"id":{"d":151,"n":7,"t":1},"speaker":"A","text":"それは通好みそうなカレーですね。ますます楽しみになってきました。"}];
INSERT INTO sentence [{"id":{"d":152,"n":1,"t":1},"speaker":"A","text":"私は、ゴールデンウィークに友達の結婚式に行きましたが、式場がとてもすてきでしたよ。"},{"id":{"d":152,"n":2,"t":1},"speaker":"B","text":"どのような式場でしたか？"},{"id":{"d":152,"n":3,"t":1},"speaker":"A","text":"窓の全面から見える景色が、見渡す限りの空と海でした。"},{"id":{"d":152,"n":4,"t":1},"speaker":"B","text":"それは素晴らしい景色ですね。"},{"id":{"d":152,"n":5,"t":1},"speaker":"A","text":"結婚式の演出も素晴らしかったです。花嫁が入ってくると同時に白い羽が舞って、花嫁がまるで天使のように見えました。白い羽が天使の羽のようで、ロマンティックな演出でした。"},{"id":{"d":152,"n":6,"t":1},"speaker":"B","text":"すてきな光景が目に浮かびます。"}];
INSERT INTO sentence [{"id":{"d":153,"n":1,"t":1},"speaker":"A","text":"私は、ざるそばを注文します。佐藤さんは、何を注文しますか？"},{"id":{"d":153,"n":2,"t":1},"speaker":"B","text":"私は、きつねそばにします。黒島さんは、そば湯を注文しますか？"},{"id":{"d":153,"n":3,"t":1},"speaker":"A","text":"いえ、結構です。"},{"id":{"d":153,"n":4,"t":1},"speaker":"B","text":"分かりました。では、店員さんを呼びますね。"},{"id":{"d":153,"n":5,"t":1},"speaker":"A","text":"はい、お願いします。"}];
INSERT INTO sentence [{"id":{"d":154,"n":1,"t":1},"speaker":"A","text":"私は、新しい趣味としてDIYを始めました。"},{"id":{"d":154,"n":2,"t":1},"speaker":"B","text":"それは、良い趣味ですね。具体的には、何を作っているのですか？"},{"id":{"d":154,"n":3,"t":1},"speaker":"A","text":"今は、棚を作っています。"},{"id":{"d":154,"n":4,"t":1},"speaker":"B","text":"棚ですか。それはまた、大変そうですね。"},{"id":{"d":154,"n":5,"t":1},"speaker":"A","text":"そうでもありませんよ。インパクトドライバーなどの便利な工具を使っていますから。"},{"id":{"d":154,"n":6,"t":1},"speaker":"B","text":"インパクトドライバーとはどういうものですか？"},{"id":{"d":154,"n":7,"t":1},"speaker":"A","text":"電動で、ネジ締め作業を行なう工具です。手動でドライバーを回す必要がないので、とても楽です。"},{"id":{"d":154,"n":8,"t":1},"speaker":"B","text":"それは、便利そうですね。"}];
INSERT INTO sentence [{"id":{"d":155,"n":1,"t":1},"speaker":"A","text":"あなたが、今まで旅をしたところで、一番良かった場所はどこですか？"},{"id":{"d":155,"n":2,"t":1},"speaker":"B","text":"京都です。風情がありすてきでした。"},{"id":{"d":155,"n":3,"t":1},"speaker":"A","text":"私は、京都へは行ったことがないので、ぜひ、行ってみたいと思っています。"},{"id":{"d":155,"n":4,"t":1},"speaker":"B","text":"京都は、観光地がたくさんあって楽しいですよ。私のお薦めの場所は清水寺です。"},{"id":{"d":155,"n":5,"t":1},"speaker":"A","text":"教えていただきありがとうございます。ぜひ、行ってみたいです。"}];
INSERT INTO sentence [{"id":{"d":156,"n":1,"t":1},"speaker":"A","text":"真っ黒な雲が、向こうに見えますよ。"},{"id":{"d":156,"n":2,"t":1},"speaker":"B","text":"もしかすると、これから天気が荒れるかもしれませんね。"},{"id":{"d":156,"n":3,"t":1},"speaker":"A","text":"ちょうど今、スマートフォンに暴風警報の通知が来ました。"},{"id":{"d":156,"n":4,"t":1},"speaker":"B","text":"嵐が来るようですね。午後の外出はやめておきましょう。"}];
INSERT INTO sentence [{"id":{"d":157,"n":1,"t":1},"speaker":"A","text":"鈴木さんは、お肌がきれいですね。何か特別なことをしているのですか？"},{"id":{"d":157,"n":2,"t":1},"speaker":"B","text":"特別なことは、何もしていません。洗顔をして化粧水と乳液をつけているだけです。"},{"id":{"d":157,"n":3,"t":1},"speaker":"A","text":"化粧水と乳液は、どこのお店で買っているのですか？"},{"id":{"d":157,"n":4,"t":1},"speaker":"B","text":"マツモトキヨシなど普通のドラッグストアで買っています。あとは、ハトムギ化粧水を使っています。"},{"id":{"d":157,"n":5,"t":1},"speaker":"A","text":"そうなのですね。私もそれを使ってみようかしら。"}];
INSERT INTO sentence [{"id":{"d":158,"n":1,"t":1},"speaker":"A","text":"来月のミュージカル公演に向けて、ダンスレッスンを始めます。"},{"id":{"d":158,"n":2,"t":1},"speaker":"B","text":"はい、お願いします。"},{"id":{"d":158,"n":3,"t":1},"speaker":"A","text":"江藤さんは、特にリズムにのってダンスをすることを心掛けてくださいね。"},{"id":{"d":158,"n":4,"t":1},"speaker":"B","text":"分かりました。ミュージカル公演も近いので、より一層気を引き締めて頑張ります。"},{"id":{"d":158,"n":5,"t":1},"speaker":"A","text":"はい、しっかり頑張ってください。"}];
INSERT INTO sentence [{"id":{"d":159,"n":1,"t":1},"speaker":"A","text":"相手のチーム、すごく強いですね。"},{"id":{"d":159,"n":2,"t":1},"speaker":"B","text":"そうですね。特に5番の壁のようなブロックを抜けられる気がしません。"},{"id":{"d":159,"n":3,"t":1},"speaker":"A","text":"5番の子は、なんとバレーボールを始めてまだ半年だそうです。"},{"id":{"d":159,"n":4,"t":1},"speaker":"B","text":"それは驚きです。とてもそうは見えません。"},{"id":{"d":159,"n":5,"t":1},"speaker":"A","text":"同感です。次こそ、あのブロックを打ち抜いて見せます。次のトスはとにかく高くしてください。"},{"id":{"d":159,"n":6,"t":1},"speaker":"B","text":"分かりました。あなたのアタックに期待していますね。"}];
INSERT INTO sentence [{"id":{"d":160,"n":1,"t":1},"speaker":"A","text":"最近は、テレビよりも動画サイトを見ることが多いです。"},{"id":{"d":160,"n":2,"t":1},"speaker":"B","text":"そうなのですね。僕は、テレビも動画も見ないから、よく分かりません。"},{"id":{"d":160,"n":3,"t":1},"speaker":"A","text":"珍しいですね。インターネットも利用しないのですか？"},{"id":{"d":160,"n":4,"t":1},"speaker":"B","text":"ゲームをするためにインターネットは使っています。"}];
INSERT INTO sentence [{"id":{"d":161,"n":1,"t":1},"speaker":"A","text":"よし、今日こそはホームランを打ちますよ。"},{"id":{"d":161,"n":2,"t":1},"speaker":"B","text":"気合が入っていますね。ヒットでも十分ですよ。"},{"id":{"d":161,"n":3,"t":1},"speaker":"A","text":"昨日はフライや凡打だけでしたから、今日こそは活躍したいのです。"},{"id":{"d":161,"n":4,"t":1},"speaker":"B","text":"力んで空振りしないようにしてくださいね。"},{"id":{"d":161,"n":5,"t":1},"speaker":"A","text":"私に、任せてください。"}];
INSERT INTO sentence [{"id":{"d":162,"n":1,"t":1},"speaker":"A","text":"あのチームの投手は、カーブが得意なのですよね。"},{"id":{"d":162,"n":2,"t":1},"speaker":"B","text":"そうですね。でも、最後は必ず高速のストレートで三振を取るのですよね。格好良いです。"},{"id":{"d":162,"n":3,"t":1},"speaker":"A","text":"その緩急つけたボールを投げられるところが、あの投手の良いところですね。"},{"id":{"d":162,"n":4,"t":1},"speaker":"B","text":"はい。今日もそれが見られるかと思うと楽しみです。"},{"id":{"d":162,"n":5,"t":1},"speaker":"A","text":"私も楽しみです。もうすぐ試合が始まりますね。"}];
INSERT INTO sentence [{"id":{"d":163,"n":1,"t":1},"speaker":"A","text":"安藤課長、来月のお花見はどうしましょうか？"},{"id":{"d":163,"n":2,"t":1},"speaker":"B","text":"幹事は岸田君でしたか？例年通り、記念公園でいいと思いますよ。"},{"id":{"d":163,"n":3,"t":1},"speaker":"A","text":"分かりました。桜の開花時期などを調べて、日程を調整しておきます。"},{"id":{"d":163,"n":4,"t":1},"speaker":"B","text":"ありがとうございます。ところで岸田君は、プライベートだとどのようなお花見が好きですか？"},{"id":{"d":163,"n":5,"t":1},"speaker":"A","text":"僕が好きなお花見の状況ですか？そうですね、夜桜は風情があって好きです。"},{"id":{"d":163,"n":6,"t":1},"speaker":"B","text":"風流ですね。てっきり、花より団子かと思っていましたよ。"},{"id":{"d":163,"n":7,"t":1},"speaker":"A","text":"そのように思われていたなんて、心外です。"}];
INSERT INTO sentence [{"id":{"d":164,"n":1,"t":1},"speaker":"A","text":"お父さん、フォーマルな服というのは、どのようなものですか？"},{"id":{"d":164,"n":2,"t":1},"speaker":"B","text":"冠婚葬祭や式典などに着ていく正装の事です。"},{"id":{"d":164,"n":3,"t":1},"speaker":"A","text":"普通のスーツなどとは、違うのですか？"},{"id":{"d":164,"n":4,"t":1},"speaker":"B","text":"厳密には少し違います。礼服といって、それぞれの用途に合わせたものがあるのです。"},{"id":{"d":164,"n":5,"t":1},"speaker":"A","text":"僕も、そろそろ礼服を用意した方がいいのでしょうか？"},{"id":{"d":164,"n":6,"t":1},"speaker":"B","text":"あなたは学生なので、学校の制服が立派な礼服になりますよ。"},{"id":{"d":164,"n":7,"t":1},"speaker":"A","text":"そうなのですね。安心しました。"}];
INSERT INTO sentence [{"id":{"d":165,"n":1,"t":1},"speaker":"A","text":"車の運転は、難しいですね。"},{"id":{"d":165,"n":2,"t":1},"speaker":"B","text":"心配しなくても良いですよ。私も初めはなかなか上手にできませんでした。"},{"id":{"d":165,"n":3,"t":1},"speaker":"A","text":"どうしたら、運転が上手になれますか？"},{"id":{"d":165,"n":4,"t":1},"speaker":"B","text":"少しずつ運転をする機会を増やしていくと上達していくので、焦らないことが大切ですね。"},{"id":{"d":165,"n":5,"t":1},"speaker":"A","text":"繰り返して行うことが、上手になる近道なのですね。ありがとうございます。"}];
INSERT INTO sentence [{"id":{"d":166,"n":1,"t":1},"speaker":"A","text":"あなたは、郵便局で年賀状を出したことはありますか？"},{"id":{"d":166,"n":2,"t":1},"speaker":"B","text":"ありません。そもそも、郵便局に行く機会自体がほとんどありません。"},{"id":{"d":166,"n":3,"t":1},"speaker":"A","text":"確かにそうかもしれませんね。僕も普段はメールやSMSで済ませてしまいます。"},{"id":{"d":166,"n":4,"t":1},"speaker":"B","text":"そうですよね。最近は手紙やはがきを使うことはほとんどなくなってしまいました。"}];
INSERT INTO sentence [{"id":{"d":167,"n":1,"t":1},"speaker":"A","text":"暖かくなってきたので、ツーリングに行きたくなります。"},{"id":{"d":167,"n":2,"t":1},"speaker":"B","text":"はい。この時期になるとツーリングで風を受けて、全身に春を感じたくなりますね。"},{"id":{"d":167,"n":3,"t":1},"speaker":"A","text":"今週末いつものメンバーで、一緒に行きましょう。お薦めの場所はありますか？"},{"id":{"d":167,"n":4,"t":1},"speaker":"B","text":"海岸沿いか峠道が王道でしょうか？"},{"id":{"d":167,"n":5,"t":1},"speaker":"A","text":"海岸沿いはまだ少し寒いので、山の方にしませんか？"},{"id":{"d":167,"n":6,"t":1},"speaker":"B","text":"そうですね。他のメンバーにも、連絡を入れておきます。"}];
