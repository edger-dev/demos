INSERT INTO sentence [{"id":{"d":624,"n":1,"t":1},"speaker":"A","text":"スマートフォンの料金プランについて教えてください。"},{"id":{"d":624,"n":2,"t":1},"speaker":"B","text":"かしこまりました。どのようなことにお困りですか？"},{"id":{"d":624,"n":3,"t":1},"speaker":"A","text":"毎月の料金が高いので、プランを見直したいです。"},{"id":{"d":624,"n":4,"t":1},"speaker":"B","text":"現在はどのプランにご加入でしょうか？"},{"id":{"d":624,"n":5,"t":1},"speaker":"A","text":"ベーシックプランです。"},{"id":{"d":624,"n":6,"t":1},"speaker":"B","text":"スマートフォンで通話することは多いですか？"},{"id":{"d":624,"n":7,"t":1},"speaker":"A","text":"いいえ。電話をかけたり受けたりすることはほとんどありません。"},{"id":{"d":624,"n":8,"t":1},"speaker":"B","text":"それでは、通話よりインターネットの利用に向いたプランの方がいいですね。"},{"id":{"d":624,"n":9,"t":1},"speaker":"A","text":"そうですね。適したプランはありますか？"},{"id":{"d":624,"n":10,"t":1},"speaker":"B","text":"ライトプランがいいと思います。"}];
INSERT INTO sentence [{"id":{"d":625,"n":1,"t":1},"speaker":"A","text":"小銭が必要なのですが、お金を崩してもらえますか？"},{"id":{"d":625,"n":2,"t":1},"speaker":"B","text":"必要なのは100円玉ですか？"},{"id":{"d":625,"n":3,"t":1},"speaker":"A","text":"はい。100円玉が4枚必要です。"},{"id":{"d":625,"n":4,"t":1},"speaker":"B","text":"それならあります。"},{"id":{"d":625,"n":5,"t":1},"speaker":"A","text":"助かります。私は1,000円札しかないのですが、どう両替しましょうか？"},{"id":{"d":625,"n":6,"t":1},"speaker":"B","text":"私は500円玉が1枚あって、他の小銭が400円分あります。1,000円には100円たりませんが、とりあえずあるものをお渡しして1,000円札と交換することならできます。"},{"id":{"d":625,"n":7,"t":1},"speaker":"A","text":"それでお願いします。"},{"id":{"d":625,"n":8,"t":1},"speaker":"B","text":"分かりました。残りの100円は手に入り次第すぐにお渡ししますね。では、交換しましよう。"}];
INSERT INTO sentence [{"id":{"d":626,"n":1,"t":1},"speaker":"A","text":"ベッド派ですか？それとも布団派ですか？"},{"id":{"d":626,"n":2,"t":1},"speaker":"B","text":"ベッド派ですが、布団で寝ています。"},{"id":{"d":626,"n":3,"t":1},"speaker":"A","text":"それはどうしてですか？"},{"id":{"d":626,"n":4,"t":1},"speaker":"B","text":"部屋が狭くて、ベッドを置いたらスペースがなくなってしまうのです。"},{"id":{"d":626,"n":5,"t":1},"speaker":"A","text":"それは仕方ありませんね。私は布団の方が好きです。"},{"id":{"d":626,"n":6,"t":1},"speaker":"B","text":"布団を毎日敷くのは面倒ではありませんか？"},{"id":{"d":626,"n":7,"t":1},"speaker":"A","text":"多少面倒ですが、布団は日に当てて干せるので気持ちが良いです。"},{"id":{"d":626,"n":8,"t":1},"speaker":"B","text":"それは分かります。お日さまで干した布団は最高ですよね。"}];
INSERT INTO sentence [{"id":{"d":627,"n":1,"t":1},"speaker":"A","text":"転勤の内示を受けました。"},{"id":{"d":627,"n":2,"t":1},"speaker":"B","text":"転勤先はどこですか？"},{"id":{"d":627,"n":3,"t":1},"speaker":"A","text":"福岡です。"},{"id":{"d":627,"n":4,"t":1},"speaker":"B","text":"それは遠いですね。"},{"id":{"d":627,"n":5,"t":1},"speaker":"A","text":"はい。引っ越しをすることになります。"},{"id":{"d":627,"n":6,"t":1},"speaker":"B","text":"転勤はいつですか？"},{"id":{"d":627,"n":7,"t":1},"speaker":"A","text":"11月です。"},{"id":{"d":627,"n":8,"t":1},"speaker":"B","text":"あまり日にちがありませんね。引っ越し先を探す必要もありますし、忙しくなりそうですね。"}];
INSERT INTO sentence [{"id":{"d":628,"n":1,"t":1},"speaker":"A","text":"運転免許証の更新はこちらでよいですか？"},{"id":{"d":628,"n":2,"t":1},"speaker":"B","text":"はい。申請用紙への記入は済んでいますか？"},{"id":{"d":628,"n":3,"t":1},"speaker":"A","text":"はい。これで大丈夫でしょうか？"},{"id":{"d":628,"n":4,"t":1},"speaker":"B","text":"大丈夫です。更新手数料が3,000円になります。"},{"id":{"d":628,"n":5,"t":1},"speaker":"A","text":"これでお願いします。"},{"id":{"d":628,"n":6,"t":1},"speaker":"B","text":"5,000円をお預かりしましたので、2,000円のお返しになります。"},{"id":{"d":628,"n":7,"t":1},"speaker":"A","text":"次はどうすればよいですか？"},{"id":{"d":628,"n":8,"t":1},"speaker":"B","text":"視力検査をしますので、左側へお進みください。"}];
INSERT INTO sentence [{"id":{"d":629,"n":1,"t":1},"speaker":"A","text":"もうすぐ自動車保険の更新が必要です。"},{"id":{"d":629,"n":2,"t":1},"speaker":"B","text":"同じ保険会社で更新するのですか？"},{"id":{"d":629,"n":3,"t":1},"speaker":"A","text":"そのつもりです。"},{"id":{"d":629,"n":4,"t":1},"speaker":"B","text":"いろいろな保険会社で見積もりをすると、保険料が安く抑えられるかもしれませんよ。"},{"id":{"d":629,"n":5,"t":1},"speaker":"A","text":"あなたもそうしているのですか？"},{"id":{"d":629,"n":6,"t":1},"speaker":"B","text":"はい。6社くらいに見積もり依頼をして、補償内容と保険料が希望に合う保険会社で契約しました。"},{"id":{"d":629,"n":7,"t":1},"speaker":"A","text":"私も何社かに見積もりを依頼してみます。"},{"id":{"d":629,"n":8,"t":1},"speaker":"B","text":"何か分からないことがあれば、質問してください。"}];
INSERT INTO sentence [{"id":{"d":630,"n":1,"t":1},"speaker":"A","text":"再来週の日曜日に参議院選挙がありますね。"},{"id":{"d":630,"n":2,"t":1},"speaker":"B","text":"当日は投票に行きますか？"},{"id":{"d":630,"n":3,"t":1},"speaker":"A","text":"はい。行きます。"},{"id":{"d":630,"n":4,"t":1},"speaker":"B","text":"私は出張の予定が入ってしまったので、期日前投票をします。"},{"id":{"d":630,"n":5,"t":1},"speaker":"A","text":"期日前投票はどこでできるのですか？"},{"id":{"d":630,"n":6,"t":1},"speaker":"B","text":"市役所でできます。"},{"id":{"d":630,"n":7,"t":1},"speaker":"A","text":"そうですか。期日前投票はいつからできるのですか？"},{"id":{"d":630,"n":8,"t":1},"speaker":"B","text":"昨日からです。私は、来週にでも期日前投票に行ってきます。"}];
INSERT INTO sentence [{"id":{"d":631,"n":1,"t":1},"speaker":"A","text":"これは、うちの畑で採れたキュウリです。よろしかったらどうぞ。"},{"id":{"d":631,"n":2,"t":1},"speaker":"B","text":"これほどたくさん頂いてよいのですか？"},{"id":{"d":631,"n":3,"t":1},"speaker":"A","text":"たくさん採れて食べ切れませんので、もらっていただけると助かります。"},{"id":{"d":631,"n":4,"t":1},"speaker":"B","text":"それでは、遠慮なく頂きます。立派なキュウリですね。"},{"id":{"d":631,"n":5,"t":1},"speaker":"A","text":"今年は例年よりできが良かったです。"},{"id":{"d":631,"n":6,"t":1},"speaker":"B","text":"うちの家族はみんなキュウリが好きなので、うれしいです。"},{"id":{"d":631,"n":7,"t":1},"speaker":"A","text":"それはよかったです。それでは、これで失礼します。"},{"id":{"d":631,"n":8,"t":1},"speaker":"B","text":"本当にありがとうございます。"}];
