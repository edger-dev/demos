INSERT INTO sentence [{"id":{"d":1,"n":1,"t":5},"speaker":"A","text":"どのような歌をよく聴きますか？"},{"id":{"d":1,"n":2,"t":5},"speaker":"B","text":"ポップスをよく聴きます。"},{"id":{"d":1,"n":3,"t":5},"speaker":"A","text":"どのようなポップスですか？"},{"id":{"d":1,"n":4,"t":5},"speaker":"B","text":"私がよく聴くのは西野カナの曲です。彼女の歌の歌詞がとても好きなのです。"}];
INSERT INTO sentence [{"id":{"d":2,"n":1,"t":5},"speaker":"A","text":"どの季節が好きですか？"},{"id":{"d":2,"n":2,"t":5},"speaker":"B","text":"紅葉が綺麗なので秋が好きです。"},{"id":{"d":2,"n":3,"t":5},"speaker":"A","text":"モミジの色がとても綺麗ですものね。"},{"id":{"d":2,"n":4,"t":5},"speaker":"B","text":"はい。自然の木々にはいろいろな色があって面白いです。"},{"id":{"d":2,"n":5,"t":5},"speaker":"A","text":"深い色合いもあって綺麗ですよね。"}];
INSERT INTO sentence [{"id":{"d":3,"n":1,"t":5},"speaker":"A","text":"今日の天気を教えてください。"},{"id":{"d":3,"n":2,"t":5},"speaker":"B","text":"今日は晴天です。気温は22度くらいになります。"},{"id":{"d":3,"n":3,"t":5},"speaker":"A","text":"とても気持ちの良い天気ですね。"},{"id":{"d":3,"n":4,"t":5},"speaker":"B","text":"暖かくて、気持ちが良いので眠くなりますね。"},{"id":{"d":3,"n":5,"t":5},"speaker":"A","text":"そうですね。このような日は昼寝をするのも良いですね。"},{"id":{"d":3,"n":6,"t":5},"speaker":"B","text":"昼寝をすると体がすっきりしますね。"}];
INSERT INTO sentence [{"id":{"d":4,"n":1,"t":5},"speaker":"A","text":"休みの日は何をしていますか？"},{"id":{"d":4,"n":2,"t":5},"speaker":"B","text":"掃除ですかね。"},{"id":{"d":4,"n":3,"t":5},"speaker":"A","text":"一日中ですか？"},{"id":{"d":4,"n":4,"t":5},"speaker":"B","text":"一日中する日もありますよ。時間をかけて隅々まで綺麗にしています。"}];
INSERT INTO sentence [{"id":{"d":5,"n":1,"t":5},"speaker":"A","text":"お笑い芸人で好きな人は誰ですか？"},{"id":{"d":5,"n":2,"t":5},"speaker":"B","text":"物まね芸人のコロッケが好きです。"},{"id":{"d":5,"n":3,"t":5},"speaker":"A","text":"どのようなところが好きですか？"},{"id":{"d":5,"n":4,"t":5},"speaker":"B","text":"ロボットのネタがとても面白くて好きです。"},{"id":{"d":5,"n":5,"t":5},"speaker":"A","text":"他に面白いネタはありますか？"},{"id":{"d":5,"n":6,"t":5},"speaker":"B","text":"河村隆一の物まねはとても似ていてすごいと思いました。"},{"id":{"d":5,"n":7,"t":5},"speaker":"A","text":"今度見てみようと思います。"},{"id":{"d":5,"n":8,"t":5},"speaker":"B","text":"ぜひ見てみてください。"}];
INSERT INTO sentence [{"id":{"d":6,"n":1,"t":5},"speaker":"A","text":"ゲームは何が好きですか？"},{"id":{"d":6,"n":2,"t":5},"speaker":"B","text":"『ドラゴンクエスト』のようなRPGが好きです。"},{"id":{"d":6,"n":3,"t":5},"speaker":"A","text":"どのようなゲームですか？"},{"id":{"d":6,"n":4,"t":5},"speaker":"B","text":"何作もシリーズが出ている不朽の名作です。"},{"id":{"d":6,"n":5,"t":5},"speaker":"A","text":"そうなのですね。それは、難しいですか？"},{"id":{"d":6,"n":6,"t":5},"speaker":"B","text":"最後のボスを倒すまでとても時間がかかります。"}];
INSERT INTO sentence [{"id":{"d":7,"n":1,"t":5},"speaker":"A","text":"スポーツの観戦は好きですか？"},{"id":{"d":7,"n":2,"t":5},"speaker":"B","text":"Kｰ1のような格闘技が好きです。"},{"id":{"d":7,"n":3,"t":5},"speaker":"A","text":"どのような所が好きですか？"},{"id":{"d":7,"n":4,"t":5},"speaker":"B","text":"パンチやキックを見ているとドキドキします。"},{"id":{"d":7,"n":5,"t":5},"speaker":"A","text":"確かにみんなすごいパンチやキックをしますよね。"},{"id":{"d":7,"n":6,"t":5},"speaker":"B","text":"はい。私もあのようになりたいです。"}];
INSERT INTO sentence [{"id":{"d":8,"n":1,"t":5},"speaker":"A","text":"野球はどこのチームが好きですか？"},{"id":{"d":8,"n":2,"t":5},"speaker":"B","text":"野球にはあまり興味がありませんが、大谷翔平はすごいと思います。"},{"id":{"d":8,"n":3,"t":5},"speaker":"A","text":"すごく有名ですよね。"},{"id":{"d":8,"n":4,"t":5},"speaker":"B","text":"はい。バッターとしてもピッチャーとしてもすごいです。"}];
INSERT INTO sentence [{"id":{"d":9,"n":1,"t":5},"speaker":"A","text":"学生時代は何のスポーツをしていましたか？"},{"id":{"d":9,"n":2,"t":5},"speaker":"B","text":"柔道をしていました。"},{"id":{"d":9,"n":3,"t":5},"speaker":"A","text":"柔道ですか。段は何段を持っているのですか？"},{"id":{"d":9,"n":4,"t":5},"speaker":"B","text":"初段です。小学生の時から柔道を始めて国士舘高校の柔道部に所属していました。"},{"id":{"d":9,"n":5,"t":5},"speaker":"A","text":"強いと思う人はいましたか？"},{"id":{"d":9,"n":6,"t":5},"speaker":"B","text":"先輩の鈴木桂治さんです。オリンピックで金メダルを取りましたが、彼は学生の時から強かったです。"}];
INSERT INTO sentence [{"id":{"d":10,"n":1,"t":5},"speaker":"A","text":"DVDで好きな映画はありますか？"},{"id":{"d":10,"n":2,"t":5},"speaker":"B","text":"『13日の金曜日』という映画が好きです。"},{"id":{"d":10,"n":3,"t":5},"speaker":"A","text":"どのような映画ですか？"},{"id":{"d":10,"n":4,"t":5},"speaker":"B","text":"すごくハラハラするホラー映画です。"},{"id":{"d":10,"n":5,"t":5},"speaker":"A","text":"どのような人が出てきますか？"},{"id":{"d":10,"n":6,"t":5},"speaker":"B","text":"ジェイソンという仮面をかぶった人が出てきます。"},{"id":{"d":10,"n":7,"t":5},"speaker":"A","text":"仮面ですか？それは怖いですね。"},{"id":{"d":10,"n":8,"t":5},"speaker":"B","text":"マスクの中の素顔が知りたくなります。"}];